{"textgrid.poem.42784": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich allein bin, werden meine Ohren lang,", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich allein bin, werden meine Ohren lang,", "tokens": ["Wenn", "ich", "al\u00b7lein", "bin", ",", "wer\u00b7den", "mei\u00b7ne", "Oh\u00b7ren", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Meine, meine Pulse horchen bang", "tokens": ["Mei\u00b7ne", ",", "mei\u00b7ne", "Pul\u00b7se", "hor\u00b7chen", "bang"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "$,", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf queres Kreischen, sterbenden Gesang", "tokens": ["Auf", "que\u00b7res", "Krei\u00b7schen", ",", "ster\u00b7ben\u00b7den", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und all die Stimmen scheeler Leere.", "tokens": ["Und", "all", "die", "Stim\u00b7men", "schee\u00b7ler", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich allein bin, leck ich meine Tr\u00e4ne.", "tokens": ["Wenn", "ich", "al\u00b7lein", "bin", ",", "leck", "ich", "mei\u00b7ne", "Tr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn ich allein bin, bohrt sich meine Schere,", "tokens": ["Wenn", "ich", "al\u00b7lein", "bin", ",", "bohrt", "sich", "mei\u00b7ne", "Sche\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nagelschere in die Z\u00e4hne;", "tokens": ["Die", "Na\u00b7gel\u00b7sche\u00b7re", "in", "die", "Z\u00e4h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sielt h\u00f6hnisch tr\u00e4ge sich herum die Zeit. \u2013", "tokens": ["Sielt", "h\u00f6h\u00b7nisch", "tr\u00e4\u00b7ge", "sich", "he\u00b7rum", "die", "Zeit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "VVFIN", "PRF", "APZR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Tropfen h\u00e4ngt. \u2013 Der Zeiger steht. \u2013", "tokens": ["Der", "Trop\u00b7fen", "h\u00e4ngt", ".", "\u2013", "Der", "Zei\u00b7ger", "steht", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Einmal des Monats steigt ein Postpaket", "tokens": ["Ein\u00b7mal", "des", "Mo\u00b7nats", "steigt", "ein", "Post\u00b7pa\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aufr\u00fchrerisch in meine Einsamkeit.", "tokens": ["Auf\u00b7r\u00fch\u00b7re\u00b7risch", "in", "mei\u00b7ne", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sendet aus Meran die Tante Liese", "tokens": ["So", "sen\u00b7det", "aus", "Me\u00b7ran", "die", "Tan\u00b7te", "Lie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir tausend fromme, aufmerksame Gr\u00fc\u00dfe;", "tokens": ["Mir", "tau\u00b7send", "from\u00b7me", ",", "auf\u00b7merk\u00b7sa\u00b7me", "Gr\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "CARD", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein jeden einzeln sauber einpapiert,", "tokens": ["Ein", "je\u00b7den", "ein\u00b7zeln", "sau\u00b7ber", "ein\u00b7pa\u00b7piert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Schleifchen und mit Fichtengr\u00fcn garniert,", "tokens": ["Mit", "Schleif\u00b7chen", "und", "mit", "Fich\u00b7ten\u00b7gr\u00fcn", "gar\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vierbl\u00e4tterklee und anderm Blumenschmuck \u2013", "tokens": ["Vier\u00b7bl\u00e4t\u00b7ter\u00b7klee", "und", "an\u00b7derm", "Blu\u00b7men\u00b7schmuck", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich aber rupfe das Gem\u00fcse", "tokens": ["Ich", "a\u00b7ber", "rup\u00b7fe", "das", "Ge\u00b7m\u00fc\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Heraus mit einem scharfen Ruck,", "tokens": ["He\u00b7raus", "mit", "ei\u00b7nem", "schar\u00b7fen", "Ruck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerkn\u00fclle fl\u00fcchtig \u00fcberf\u00fchlend", "tokens": ["Zer\u00b7kn\u00fcl\u00b7le", "fl\u00fcch\u00b7tig", "\u00fc\u00b7berf\u00b7\u00fch\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Alles-Gute-W\u00fcnsche-Brief", "tokens": ["Den", "Al\u00b7les\u00b7Gu\u00b7te\u00b7W\u00fcn\u00b7sche\u00b7Brief"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und fische giftig tauchend, w\u00fchlend,", "tokens": ["Und", "fi\u00b7sche", "gif\u00b7tig", "tau\u00b7chend", ",", "w\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus all den Knittern und Rosetten", "tokens": ["Aus", "all", "den", "Knit\u00b7tern", "und", "Ro\u00b7set\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das einzige, was positiv:", "tokens": ["Das", "ein\u00b7zi\u00b7ge", ",", "was", "po\u00b7si\u00b7tiv", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zwei Mark f\u00fcr Zigaretten.", "tokens": ["Zwei", "Mark", "f\u00fcr", "Zi\u00b7ga\u00b7ret\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Bilder meiner Stube h\u00e4ngen schief.", "tokens": ["Die", "Bil\u00b7der", "mei\u00b7ner", "Stu\u00b7be", "h\u00e4n\u00b7gen", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In meiner Stube d\u00fcnsten kalte Betten.", "tokens": ["In", "mei\u00b7ner", "Stu\u00b7be", "d\u00fcns\u00b7ten", "kal\u00b7te", "Bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und meine Hoffart kuscht sich. Wie ein Falter", "tokens": ["Und", "mei\u00b7ne", "Hof\u00b7fart", "kuscht", "sich", ".", "Wie", "ein", "Fal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "$.", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich \u00e4ngstlich einzw\u00e4ngt in die Borkenrinde.", "tokens": ["Sich", "\u00e4ngst\u00b7lich", "ein\u00b7zw\u00e4ngt", "in", "die", "Bor\u00b7ken\u00b7rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Wenn ich allein bin, dreht mein Federhalter", "tokens": ["Wenn", "ich", "al\u00b7lein", "bin", ",", "dreht", "mein", "Fe\u00b7der\u00b7hal\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwarzbraunen Honig aus dem Ohrgewinde.", "tokens": ["Schwarz\u00b7brau\u00b7nen", "Ho\u00b7nig", "aus", "dem", "Ohr\u00b7ge\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Bin ich allein: Starb, wie ein Hund verreckt,", "tokens": ["Bin", "ich", "al\u00b7lein", ":", "Starb", ",", "wie", "ein", "Hund", "ver\u00b7reckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$.", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hat mich ein fremdes Weib mit ihren Schleiern", "tokens": ["Hat", "mich", "ein", "frem\u00b7des", "Weib", "mit", "ih\u00b7ren", "Schlei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Mitleid oder Ekel zugedeckt.", "tokens": ["Aus", "Mit\u00b7leid", "o\u00b7der", "E\u00b7kel", "zu\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch durch die Maschen seh ich Feste feiern,", "tokens": ["Doch", "durch", "die", "Ma\u00b7schen", "seh", "ich", "Fes\u00b7te", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die mich verga\u00dfen \u00fcber junger Lust. \u2013", "tokens": ["Die", "mich", "ver\u00b7ga\u00b7\u00dfen", "\u00fc\u00b7ber", "jun\u00b7ger", "Lust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich rei\u00dfe auseinander meine Brust", "tokens": ["Ich", "rei\u00b7\u00dfe", "aus\u00b7ein\u00b7an\u00b7der", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lasse steigen all die V\u00f6gel, die", "tokens": ["Und", "las\u00b7se", "stei\u00b7gen", "all", "die", "V\u00f6\u00b7gel", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "VVFIN", "PIAT", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich eingekerkert, grausam dort gefangen,", "tokens": ["Ich", "ein\u00b7ge\u00b7ker\u00b7kert", ",", "grau\u00b7sam", "dort", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Leben lang gefangen hielt, und nie", "tokens": ["Ein", "Le\u00b7ben", "lang", "ge\u00b7fan\u00b7gen", "hielt", ",", "und", "nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJD", "VVFIN", "$,", 