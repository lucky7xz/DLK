{"textgrid.poem.26712": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnehmt hin die Welt!\u00ab rief Zeus von seinen H\u00f6hen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnehmt hin die Welt!\u00ab rief Zeus von seinen H\u00f6hen", "tokens": ["\u00bb", "nehmt", "hin", "die", "Welt", "!", "\u00ab", "rief", "Zeus", "von", "sei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "VVFIN", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Menschen zu. \u00bbNehmt, sie soll euer sein!", "tokens": ["Den", "Men\u00b7schen", "zu", ".", "\u00bb", "Nehmt", ",", "sie", "soll", "eu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "VVFIN", "$,", "PPER", "VMFIN", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Euch schenk ich sie zum Erb und ewgen Lehen,", "tokens": ["Euch", "schenk", "ich", "sie", "zum", "Erb", "und", "ew\u00b7gen", "Le\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch teilt euch br\u00fcderlich darein.\u00ab", "tokens": ["Doch", "teilt", "euch", "br\u00fc\u00b7der\u00b7lich", "da\u00b7rein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da eilt, was H\u00e4nde hat, sich einzurichten,", "tokens": ["Da", "eilt", ",", "was", "H\u00e4n\u00b7de", "hat", ",", "sich", "ein\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$,", "PRF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es regte sich gesch\u00e4ftig jung und alt.", "tokens": ["Es", "reg\u00b7te", "sich", "ge\u00b7sch\u00e4f\u00b7tig", "jung", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ackermann griff nach des Feldes Fr\u00fcchten,", "tokens": ["Der", "A\u00b7cker\u00b7mann", "griff", "nach", "des", "Fel\u00b7des", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Junker birschte durch den Wald.", "tokens": ["Der", "Jun\u00b7ker", "birschte", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Kaufmann nimmt, was seine Speicher fassen,", "tokens": ["Der", "Kauf\u00b7mann", "nimmt", ",", "was", "sei\u00b7ne", "Spei\u00b7cher", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Abt w\u00e4hlt sich den edeln Firnewein,", "tokens": ["Der", "Abt", "w\u00e4hlt", "sich", "den", "e\u00b7deln", "Fir\u00b7ne\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der K\u00f6nig sperrt die Br\u00fccken und die Stra\u00dfen", "tokens": ["Der", "K\u00f6\u00b7nig", "sperrt", "die", "Br\u00fc\u00b7cken", "und", "die", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sprach: \u00bbDer Zehente ist mein.\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Der", "Ze\u00b7hen\u00b7te", "ist", "mein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ART", "NN", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ganz sp\u00e4t, nachdem die Teilung l\u00e4ngst geschehen,", "tokens": ["Ganz", "sp\u00e4t", ",", "nach\u00b7dem", "die", "Tei\u00b7lung", "l\u00e4ngst", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Naht der Poet, er kam aus weiter Fern;", "tokens": ["Naht", "der", "Po\u00b7et", ",", "er", "kam", "aus", "wei\u00b7ter", "Fern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! da war \u00fcberall nichts mehr zu sehen,", "tokens": ["Ach", "!", "da", "war", "\u00fc\u00b7be\u00b7rall", "nichts", "mehr", "zu", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VAFIN", "ADV", "PIS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alles hatte seinen Herrn!", "tokens": ["Und", "al\u00b7les", "hat\u00b7te", "sei\u00b7nen", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbweh mir! so soll ich denn allein von allen", "tokens": ["\u00bb", "weh", "mir", "!", "so", "soll", "ich", "denn", "al\u00b7lein", "von", "al\u00b7len"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "$.", "ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergessen sein, ich, dein getreuster Sohn?\u00ab", "tokens": ["Ver\u00b7ges\u00b7sen", "sein", ",", "ich", ",", "dein", "ge\u00b7treus\u00b7ter", "Sohn", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAINF", "$,", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So lie\u00df er laut der Klage Ruf erschallen", "tokens": ["So", "lie\u00df", "er", "laut", "der", "Kla\u00b7ge", "Ruf", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und warf sich hin vor Jovis Thron.", "tokens": ["Und", "warf", "sich", "hin", "vor", "Jo\u00b7vis", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwenn du im Land der Tr\u00e4ume dich verweilet\u00ab,", "tokens": ["\u00bb", "wenn", "du", "im", "Land", "der", "Tr\u00e4u\u00b7me", "dich", "ver\u00b7wei\u00b7let", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versetzt der Gott, \u00bbso hadre nicht mit mir.", "tokens": ["Ver\u00b7setzt", "der", "Gott", ",", "\u00bb", "so", "had\u00b7re", "nicht", "mit", "mir", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "ADV", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo warst du denn, als man die Welt geteilet?\u00ab-", "tokens": ["Wo", "warst", "du", "denn", ",", "als", "man", "die", "Welt", "ge\u00b7tei\u00b7let", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbich war\u00ab, sprach der Poet, \u00bbbei dir.", "tokens": ["\u00bb", "ich", "war", "\u00ab", ",", "sprach", "der", "Po\u00b7et", ",", "\u00bb", "bei", "dir", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$(", "$,", "VVFIN", "ART", "NN", "$,", "$(", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Auge hing an deinem Angesichte,", "tokens": ["Mein", "Au\u00b7ge", "hing", "an", "dei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An deines Himmels Harmonie mein Ohr-", "tokens": ["An", "dei\u00b7nes", "Him\u00b7mels", "Har\u00b7mo\u00b7nie", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzeih dem Geiste, der, von deinem Lichte", "tokens": ["Ver\u00b7zeih", "dem", "Geis\u00b7te", ",", "der", ",", "von", "dei\u00b7nem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Berauscht, das Irdische verlor!\u00ab", "tokens": ["Be\u00b7rauscht", ",", "das", "Ir\u00b7di\u00b7sche", "ver\u00b7lor", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbwas tun?\u00ab spricht Zeus. \u00bbDie Welt ist weggegeben,", "tokens": ["\u00bb", "was", "tun", "?", "\u00ab", "spricht", "Zeus", ".", "\u00bb", "Die", "Welt", "ist", "weg\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVINF", "$.", "$(", "VVFIN", "NE", "$.", "$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herbst, die Jagd, der Markt ist nicht mehr mein.", "tokens": ["Der", "Herbst", ",", "die", "Jagd", ",", "der", "Markt", "ist", "nicht", "mehr", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du in meinem Himmel mit mir leben:", "tokens": ["Willst", "du", "in", "mei\u00b7nem", "Him\u00b7mel", "mit", "mir", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So oft du kommst, er soll dir offen sein.\u00ab", "tokens": ["So", "oft", "du", "kommst", ",", "er", "soll", "dir", "of\u00b7fen", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbnehmt hin die Welt!\u00ab rief Zeus von seinen H\u00f6hen", "tokens": ["\u00bb", "nehmt", "hin", "die", "Welt", "!", "\u00ab", "rief", "Zeus", "von", "sei\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "VVFIN", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Menschen zu. \u00bbNehmt, sie soll euer sein!", "tokens": ["Den", "Men\u00b7schen", "zu", ".", "\u00bb", "Nehmt", ",", "sie", "soll", "eu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punc