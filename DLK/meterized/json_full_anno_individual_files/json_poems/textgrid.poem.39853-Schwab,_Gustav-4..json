{"textgrid.poem.39853": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu guter Letzt': \u2013 O wolltest du mich h\u00f6ren!", "tokens": ["Zu", "gu\u00b7ter", "Letzt'", ":", "\u2013", "O", "woll\u00b7test", "du", "mich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$(", "NE", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6nnt' ich mit meinem Ernst, mit meinen Scherzen", "tokens": ["K\u00f6nnt'", "ich", "mit", "mei\u00b7nem", "Ernst", ",", "mit", "mei\u00b7nen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die beide quellen aus gleich warmem Herzen,", "tokens": ["Die", "bei\u00b7de", "quel\u00b7len", "aus", "gleich", "war\u00b7mem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich in dem Thun, dem unheilvollen, st\u00f6ren!", "tokens": ["Dich", "in", "dem", "Thun", ",", "dem", "un\u00b7heil\u00b7vol\u00b7len", ",", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O h\u00e4tt' ich Einen doch von jenen Ch\u00f6ren,", "tokens": ["O", "h\u00e4tt'", "ich", "Ei\u00b7nen", "doch", "von", "je\u00b7nen", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Geister bei der Auferstehung Kerzen", "tokens": ["Die", "Geis\u00b7ter", "bei", "der", "Auf\u00b7er\u00b7ste\u00b7hung", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hineingesungen in die tiefsten Schw\u00e4rzen", "tokens": ["Hin\u00b7ein\u00b7ge\u00b7sun\u00b7gen", "in", "die", "tiefs\u00b7ten", "Schw\u00e4r\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Seele ", "tokens": ["Der", "See\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Schon setzt er an die giftgef\u00fcllte Schale,", "tokens": ["Schon", "setzt", "er", "an", "die", "gift\u00b7ge\u00b7f\u00fcll\u00b7te", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da klingt es leise mit den Engelszungen,", "tokens": ["Da", "klingt", "es", "lei\u00b7se", "mit", "den", "En\u00b7gels\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er h\u00e4lt, er horcht, und seine Thr\u00e4nen flie\u00dfen.",