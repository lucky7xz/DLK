{"textgrid.poem.36290": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lessings Worte", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn kalte Zweifler selbst prophetisch sprechen,", "tokens": ["Wenn", "kal\u00b7te", "Zweif\u00b7ler", "selbst", "pro\u00b7phe\u00b7tisch", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die klaren Augen nicht das Licht mehr scheuen,", "tokens": ["Die", "kla\u00b7ren", "Au\u00b7gen", "nicht", "das", "Licht", "mehr", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seltsam der Wahrheit Kraft in ihren Treuen", "tokens": ["Selt\u00b7sam", "der", "Wahr\u00b7heit", "Kraft", "in", "ih\u00b7ren", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sich zeigt, den Blitz umsonst die Wolken schw\u00e4chen;", "tokens": ["Sich", "zeigt", ",", "den", "Blitz", "um\u00b7sonst", "die", "Wol\u00b7ken", "schw\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann wahrlich mu\u00df die neue Zeit anbrechen,", "tokens": ["Dann", "wahr\u00b7lich", "mu\u00df", "die", "neu\u00b7e", "Zeit", "an\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann soll das Morgenrot uns doch erfreuen,", "tokens": ["Dann", "soll", "das", "Mor\u00b7gen\u00b7rot", "uns", "doch", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann d\u00fcrfen auch die K\u00fcnste sich erneuen,", "tokens": ["Dann", "d\u00fcr\u00b7fen", "auch", "die", "K\u00fcns\u00b7te", "sich", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Mensch die kleinen Fesseln all' zerbrechen.", "tokens": ["Der", "Mensch", "die", "klei\u00b7nen", "Fes\u00b7seln", "all'", "zer\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbes wird das neue Evangelium kommen!\u00ab", "tokens": ["\u00bb", "es", "wird", "das", "neu\u00b7e", "E\u00b7van\u00b7ge\u00b7li\u00b7um", "kom\u00b7men", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sagte Lessing, doch die bl\u00f6de Rotte", "tokens": ["So", "sag\u00b7te", "Les\u00b7sing", ",", "doch", "die", "bl\u00f6\u00b7de", "Rot\u00b7te"], "token_info": ["word", "word",