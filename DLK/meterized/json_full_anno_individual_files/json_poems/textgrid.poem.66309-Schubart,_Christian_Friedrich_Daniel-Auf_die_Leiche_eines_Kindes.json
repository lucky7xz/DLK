{"textgrid.poem.66309": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Leiche eines Kindes", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund der Herzen,", "tokens": ["Freund", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sieh die Schmerzen,", "tokens": ["Sieh", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die am Grabe bluten.", "tokens": ["Die", "am", "Gra\u00b7be", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schau', Erbarmer, wie die Deinen", "tokens": ["Schau'", ",", "Er\u00b7bar\u00b7mer", ",", "wie", "die", "Dei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter modernden Gebeinen", "tokens": ["Un\u00b7ter", "mo\u00b7dern\u00b7den", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber ihre Todten weinen.", "tokens": ["Ue\u00b7ber", "ih\u00b7re", "Tod\u00b7ten", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nen flie\u00dfen,", "tokens": ["Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Weil wir m\u00fcssen", "tokens": ["Weil", "wir", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Zweige dorren sehen,", "tokens": ["Zwei\u00b7ge", "dor\u00b7ren", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die, wie jugendliche Rosen,", "tokens": ["Die", ",", "wie", "ju\u00b7gend\u00b7li\u00b7che", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Morgenthau begossen,", "tokens": ["Von", "dem", "Mor\u00b7gent\u00b7hau", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem Garten Gottes sprossen.", "tokens": ["Aus", "dem", "Gar\u00b7ten", "Got\u00b7tes", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00fctter stehen", "tokens": ["M\u00fct\u00b7ter", "ste\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stumm und sehen", "tokens": ["Stumm", "und", "se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf die kleine Leiche.", "tokens": ["Auf", "die", "klei\u00b7ne", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "V\u00e4ter schwanken vor dem Kinde,", "tokens": ["V\u00e4\u00b7ter", "schwan\u00b7ken", "vor", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die halb gewachsne Linde", "tokens": ["Wie", "die", "halb", "ge\u00b7wachs\u00b7ne", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor der Wuth emp\u00f6rter Winde.", "tokens": ["Vor", "der", "Wuth", "em\u00b7p\u00f6r\u00b7ter", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Kleinen", "tokens": ["Und", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Stehn und weinen", "tokens": ["Stehn", "und", "wei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Laut um den Gespielen.", "tokens": ["Laut", "um", "den", "Ge\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In des Todtengr\u00e4bers Mienen", "tokens": ["In", "des", "Tod\u00b7ten\u00b7gr\u00e4\u00b7bers", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauen sie durch ihre Thr\u00e4nen", "tokens": ["Schau\u00b7en", "sie", "durch", "ih\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wuth und Grausamkeit in ihnen.", "tokens": ["Wuth", "und", "Grau\u00b7sam\u00b7keit", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So verwelken", "tokens": ["So", "ver\u00b7wel\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Denn die Nelken", "tokens": ["Denn", "die", "Nel\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch in ihrer Knospe?", "tokens": ["Noch", "in", "ih\u00b7rer", "Knos\u00b7pe", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blumen, die wie Sterne stehen,", "tokens": ["Blu\u00b7men", ",", "die", "wie", "Ster\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen, wann die Winde wehen,", "tokens": ["M\u00fcs\u00b7sen", ",", "wann", "die", "Win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halb emporgebl\u00fcht vergehen.", "tokens": ["Halb", "em\u00b7por\u00b7ge\u00b7bl\u00fcht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch nur stille!", "tokens": ["Doch", "nur", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gottes Wille", "tokens": ["Got\u00b7tes", "Wil\u00b7le"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist allein der beste.", "tokens": ["Ist", "al\u00b7lein", "der", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der die Kinder uns gegeben,", "tokens": ["Der", "die", "Kin\u00b7der", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat die Macht, zu jenem Leben", "tokens": ["Hat", "die", "Macht", ",", "zu", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Kinder zu erheben.", "tokens": ["Die\u00b7se", "Kin\u00b7der", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Seelen werden", "tokens": ["See\u00b7len", "wer\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VAINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Auf der Erden", "tokens": ["Auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gr\u00f6\u00dftentheils vergiftet.", "tokens": ["Gr\u00f6\u00df\u00b7ten\u00b7theils", "ver\u00b7gif\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Darum nimmt, nach seinem Willen,", "tokens": ["Da\u00b7rum", "nimmt", ",", "nach", "sei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott die Kinder hin im Stillen,", "tokens": ["Gott", "die", "Kin\u00b7der", "hin", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen Himmel auszuf\u00fcllen.", "tokens": ["Sei\u00b7nen", "Him\u00b7mel", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus dem Bade", "tokens": ["Aus", "dem", "Ba\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Seiner Gnade", "tokens": ["Sei\u00b7ner", "Gna\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Str\u00f6mt des Christen Leben.", "tokens": ["Str\u00f6mt", "des", "Chris\u00b7ten", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist ein S\u00e4ugling \u2013 sprecht, ihr Thoren!", "tokens": ["Ist", "ein", "S\u00e4ug\u00b7ling", "\u2013", "sprecht", ",", "ihr", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den die Taufe neugeboren,", "tokens": ["Den", "die", "Tau\u00b7fe", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon im fr\u00fchen Tod verloren?", "tokens": ["Schon", "im", "fr\u00fc\u00b7hen", "Tod", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weinet minder", "tokens": ["Wei\u00b7net", "min\u00b7der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "F\u00fcr die Kinder,", "tokens": ["F\u00fcr", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Eltern! spart die Z\u00e4hren.", "tokens": ["El\u00b7tern", "!", "spart", "die", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kann es ihren kleinen Seelen", "tokens": ["Kann", "es", "ih\u00b7ren", "klei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da, wo keine Zweifel qu\u00e4len,", "tokens": ["Da", ",", "wo", "kei\u00b7ne", "Zwei\u00b7fel", "qu\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl an der Erziehung fehlen?", "tokens": ["Wohl", "an", "der", "Er\u00b7zie\u00b7hung", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Sehet, dorten", "tokens": ["Se\u00b7het", ",", "dor\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "An den Pforten", "tokens": ["An", "den", "Pfor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Seines Himmels winket,", "tokens": ["Sei\u00b7nes", "Him\u00b7mels", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jesus winkt mit holden Mienen,", "tokens": ["Je\u00b7sus", "winkt", "mit", "hol\u00b7den", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zu Lehrern gibt Er ihnen", "tokens": ["Und", "zu", "Leh\u00b7rern", "gibt", "Er", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Engel, die dem Vater dienen.", "tokens": ["En\u00b7gel", ",", "die", "dem", "Va\u00b7ter", "die\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Darum schweigen", "tokens": ["Da\u00b7rum", "schwei\u00b7gen"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wir und beugen", "tokens": ["Wir", "und", "beu\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Unsern Nacken nieder.", "tokens": ["Un\u00b7sern", "Na\u00b7cken", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wann die Erde wird vergehen,", "tokens": ["Wann", "die", "Er\u00b7de", "wird", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden wir in jenen H\u00f6hen", "tokens": ["Wer\u00b7den", "wir", "in", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsre Todten wieder sehen.", "tokens": ["Uns\u00b7re", "Tod\u00b7ten", "wie\u00b7der", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Freund der Herzen,", "tokens": ["Freund", "der", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Sieh die Schmerzen,", "tokens": ["Sieh", "die", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die am Grabe bluten.", "tokens": ["Die", "am", "Gra\u00b7be", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schau', Erbarmer, wie die Deinen", "tokens": ["Schau'", ",", "Er\u00b7bar\u00b7mer", ",", "wie", "die", "Dei\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PWAV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter modernden Gebeinen", "tokens": ["Un\u00b7ter", "mo\u00b7dern\u00b7den", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber ihre Todten weinen.", "tokens": ["Ue\u00b7ber", "ih\u00b7re", "Tod\u00b7ten", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Thr\u00e4nen flie\u00dfen,", "tokens": ["Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Weil wir m\u00fcssen", "tokens": ["Weil", "wir", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Zweige dorren sehen,", "tokens": ["Zwei\u00b7ge", "dor\u00b7ren", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die, wie jugendliche Rosen,", "tokens": ["Die", ",", "wie", "ju\u00b7gend\u00b7li\u00b7che", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Morgenthau begossen,", "tokens": ["Von", "dem", "Mor\u00b7gent\u00b7hau", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem Garten Gottes sprossen.", "tokens": ["Aus", "dem", "Gar\u00b7ten", "Got\u00b7tes", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "M\u00fctter stehen", "tokens": ["M\u00fct\u00b7ter", "ste\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Stumm und sehen", "tokens": ["Stumm", "und", "se\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf die kleine Leiche.", "tokens": ["Auf", "die", "klei\u00b7ne", "Lei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "V\u00e4ter schwanken vor dem Kinde,", "tokens": ["V\u00e4\u00b7ter", "schwan\u00b7ken", "vor", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die halb gewachsne Linde", "tokens": ["Wie", "die", "halb", "ge\u00b7wachs\u00b7ne", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor der Wuth emp\u00f6rter Winde.", "tokens": ["Vor", "der", "Wuth", "em\u00b7p\u00f6r\u00b7ter", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und die Kleinen", "tokens": ["Und", "die", "Klei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Stehn und weinen", "tokens": ["Stehn", "und", "wei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Laut um den Gespielen.", "tokens": ["Laut", "um", "den", "Ge\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In des Todtengr\u00e4bers Mienen", "tokens": ["In", "des", "Tod\u00b7ten\u00b7gr\u00e4\u00b7bers", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schauen sie durch ihre Thr\u00e4nen", "tokens": ["Schau\u00b7en", "sie", "durch", "ih\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wuth und Grausamkeit in ihnen.", "tokens": ["Wuth", "und", "Grau\u00b7sam\u00b7keit", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "So verwelken", "tokens": ["So", "ver\u00b7wel\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Denn die Nelken", "tokens": ["Denn", "die", "Nel\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch in ihrer Knospe?", "tokens": ["Noch", "in", "ih\u00b7rer", "Knos\u00b7pe", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blumen, die wie Sterne stehen,", "tokens": ["Blu\u00b7men", ",", "die", "wie", "Ster\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen, wann die Winde wehen,", "tokens": ["M\u00fcs\u00b7sen", ",", "wann", "die", "Win\u00b7de", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halb emporgebl\u00fcht vergehen.", "tokens": ["Halb", "em\u00b7por\u00b7ge\u00b7bl\u00fcht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Doch nur stille!", "tokens": ["Doch", "nur", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Gottes Wille", "tokens": ["Got\u00b7tes", "Wil\u00b7le"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ist allein der beste.", "tokens": ["Ist", "al\u00b7lein", "der", "bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der die Kinder uns gegeben,", "tokens": ["Der", "die", "Kin\u00b7der", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat die Macht, zu jenem Leben", "tokens": ["Hat", "die", "Macht", ",", "zu", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Kinder zu erheben.", "tokens": ["Die\u00b7se", "Kin\u00b7der", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Seelen werden", "tokens": ["See\u00b7len", "wer\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VAINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Auf der Erden", "tokens": ["Auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Gr\u00f6\u00dftentheils vergiftet.", "tokens": ["Gr\u00f6\u00df\u00b7ten\u00b7theils", "ver\u00b7gif\u00b7tet", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Darum nimmt, nach seinem Willen,", "tokens": ["Da\u00b7rum", "nimmt", ",", "nach", "sei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott die Kinder hin im Stillen,", "tokens": ["Gott", "die", "Kin\u00b7der", "hin", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seinen Himmel auszuf\u00fcllen.", "tokens": ["Sei\u00b7nen", "Him\u00b7mel", "aus\u00b7zu\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Aus dem Bade", "tokens": ["Aus", "dem", "Ba\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Seiner Gnade", "tokens": ["Sei\u00b7ner", "Gna\u00b7de"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Str\u00f6mt des Christen Leben.", "tokens": ["Str\u00f6mt", "des", "Chris\u00b7ten", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist ein S\u00e4ugling \u2013 sprecht, ihr Thoren!", "tokens": ["Ist", "ein", "S\u00e4ug\u00b7ling", "\u2013", "sprecht", ",", "ihr", "Tho\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den die Taufe neugeboren,", "tokens": ["Den", "die", "Tau\u00b7fe", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schon im fr\u00fchen Tod verloren?", "tokens": ["Schon", "im", "fr\u00fc\u00b7hen", "Tod", "ver\u00b7lo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Weinet minder", "tokens": ["Wei\u00b7net", "min\u00b7der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "F\u00fcr die Kinder,", "tokens": ["F\u00fcr", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Eltern! spart die Z\u00e4hren.", "tokens": ["El\u00b7tern", "!", "spart", "die", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kann es ihren kleinen Seelen", "tokens": ["Kann", "es", "ih\u00b7ren", "klei\u00b7nen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da, wo keine Zweifel qu\u00e4len,", "tokens": ["Da", ",", "wo", "kei\u00b7ne", "Zwei\u00b7fel", "qu\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl an der Erziehung fehlen?", "tokens": ["Wohl", "an", "der", "Er\u00b7zie\u00b7hung", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Sehet, dorten", "tokens": ["Se\u00b7het", ",", "dor\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "An den Pforten", "tokens": ["