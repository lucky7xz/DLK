{"textgrid.poem.51035": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ans Fenster schl\u00e4gt ein unersch\u00f6pfter Regen,", "tokens": ["Ans", "Fens\u00b7ter", "schl\u00e4gt", "ein", "un\u00b7er\u00b7sch\u00f6pf\u00b7ter", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Her rauscht die Mitternacht auf feuchten Schwingen,", "tokens": ["Her", "rauscht", "die", "Mit\u00b7ter\u00b7nacht", "auf", "feuch\u00b7ten", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dem Dunkel mu\u00df das L\u00e4mplein ringen \u2013", "tokens": ["Und", "mit", "dem", "Dun\u00b7kel", "mu\u00df", "das", "L\u00e4m\u00b7plein", "rin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie bin ich m\u00fcd, ich will zu Bett mich legen!", "tokens": ["Wie", "bin", "ich", "m\u00fcd", ",", "ich", "will", "zu", "Bett", "mich", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was sinn ich noch zu meinem Abendsegen? \u2013", "tokens": ["Was", "sinn", "ich", "noch", "zu", "mei\u00b7nem", "A\u00b7bend\u00b7se\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meinem Ohre summt ein leises Klingen", "tokens": ["In", "mei\u00b7nem", "Oh\u00b7re", "summt", "ein", "lei\u00b7ses", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und widerhallet ein verschollnes Singen:", "tokens": ["Und", "wi\u00b7der\u00b7hal\u00b7let", "ein", "ver\u00b7scholl\u00b7nes", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein denket einer auf entfernten Wegen.", "tokens": ["Mein", "den\u00b7ket", "ei\u00b7ner", "auf", "ent\u00b7fern\u00b7ten", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bist du's, o Freund? Auch ich gedenke dein!", "tokens": ["Bist", "du's", ",", "o", "Freund", "?", "Auch", "ich", "ge\u00b7den\u00b7ke", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "FM", "NN", "$.", "ADV", "PPER", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei mir gegr\u00fc\u00dft im unsichtbaren Raume", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", "im", "un\u00b7sicht\u00b7ba\u00b7ren", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPRART", "ADJA", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nach Jahren voll Vergessenheit und Leiden!", "tokens": ["Nach", "Jah\u00b7ren", "voll", "Ver\u00b7ges\u00b7sen\u00b7heit", "und", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bei unsrer Jugend bleichem Sternenschein", "tokens": ["Bei", "uns\u00b7rer", "Ju\u00b7gend", "blei\u00b7chem", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sehn wir uns fl\u00fcchtig fragend an im Traume,", "tokens": ["Sehn", "wir", "uns", "fl\u00fcch\u00b7tig", "fra\u00b7gend", "an", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um wieder lang, auf immer wohl zu scheiden.", "tokens": ["Um", "wie\u00b7der", "lang", ",", "auf", "im\u00b7mer", "wohl", "zu", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "$,", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ans Fenster schl\u00e4gt ein unersch\u00f6pfter Regen,", "tokens": ["Ans", "Fens\u00b7ter", "schl\u00e4gt", "ein", "un\u00b7er\u00b7sch\u00f6pf\u00b7ter", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+