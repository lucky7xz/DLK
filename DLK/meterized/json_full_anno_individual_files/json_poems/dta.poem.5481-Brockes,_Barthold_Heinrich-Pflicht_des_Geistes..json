{"dta.poem.5481": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Pflicht des Geistes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sieht nicht einst das helle Sonnen-Licht,", "tokens": ["Man", "sieht", "nicht", "einst", "das", "hel\u00b7le", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es leuchtet nicht, es w\u00e4rmet nicht,", "tokens": ["Es", "leuch\u00b7tet", "nicht", ",", "es", "w\u00e4r\u00b7met", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo es sich nicht an C\u00f6rpern bricht,", "tokens": ["Wo", "es", "sich", "nicht", "an", "C\u00f6r\u00b7pern", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht r\u00fcckwerts f\u00e4llt, nicht reflectiret.", "tokens": ["Nicht", "r\u00fcck\u00b7werts", "f\u00e4llt", ",", "nicht", "re\u00b7flec\u00b7ti\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So wird von unserm Geist auch nichts gesp\u00fchret,", "tokens": ["So", "wird", "von", "un\u00b7serm", "Geist", "auch", "nichts", "ge\u00b7sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo er sich nicht auf C\u00f6rper senckt,", "tokens": ["Wo", "er", "sich", "nicht", "auf", "C\u00f6r\u00b7per", "senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "PTKNEG", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von ihnen sich nicht r\u00fcckw\u00e4rts lenckt,", "tokens": ["Von", "ih\u00b7nen", "sich", "nicht", "r\u00fcck\u00b7w\u00e4rts", "lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, reflectirend, wirckt und denckt.", "tokens": ["Und", ",", "re\u00b7flec\u00b7ti\u00b7rend", ",", "wirckt", "und", "denckt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}