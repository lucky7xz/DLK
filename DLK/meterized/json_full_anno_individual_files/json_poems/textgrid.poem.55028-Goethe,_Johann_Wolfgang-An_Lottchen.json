": {"text": "Lottchen, wer kennt unsre Sinnen?", "tokens": ["Lott\u00b7chen", ",", "wer", "kennt", "uns\u00b7re", "Sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Lottchen, wer kennt unser Herz?", "tokens": ["Lott\u00b7chen", ",", "wer", "kennt", "un\u00b7ser", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, es machte gern gekannt sein, \u00fcberflie\u00dfen", "tokens": ["Ach", ",", "es", "mach\u00b7te", "gern", "ge\u00b7kannt", "sein", ",", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "VVPP", "VAINF", "$,", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "In das Mitempfinden einer Kreatur", "tokens": ["In", "das", "Mi\u00b7temp\u00b7fin\u00b7den", "ei\u00b7ner", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und vertrauend zwiefach neu genie\u00dfen", "tokens": ["Und", "ver\u00b7trau\u00b7end", "zwie\u00b7fach", "neu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Alles Leid und Freude der Natur.", "tokens": ["Al\u00b7les", "Leid", "und", "Freu\u00b7de", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.pe