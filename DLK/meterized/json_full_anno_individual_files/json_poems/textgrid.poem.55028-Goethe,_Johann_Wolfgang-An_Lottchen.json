{"textgrid.poem.55028": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Lottchen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitten im Get\u00fcmmel mancher Freuden,", "tokens": ["Mit\u00b7ten", "im", "Ge\u00b7t\u00fcm\u00b7mel", "man\u00b7cher", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mancher Sorgen, mancher Herzensnot", "tokens": ["Man\u00b7cher", "Sor\u00b7gen", ",", "man\u00b7cher", "Her\u00b7zens\u00b7not"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denk ich dein, o Lottchen, denken dein die beiden,", "tokens": ["Denk", "ich", "dein", ",", "o", "Lott\u00b7chen", ",", "den\u00b7ken", "dein", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "ART", "PIAT", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie beim stillen Abendrot", "tokens": ["Wie", "beim", "stil\u00b7len", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du die Hand uns freundlich reichtest,", "tokens": ["Du", "die", "Hand", "uns", "freund\u00b7lich", "reich\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da du uns auf reich bebauter Flur,", "tokens": ["Da", "du", "uns", "auf", "reich", "be\u00b7bau\u00b7ter", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In dem Scho\u00dfe herrlicher Natur,", "tokens": ["In", "dem", "Scho\u00b7\u00dfe", "herr\u00b7li\u00b7cher", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Manche leicht verh\u00fcllte Spur", "tokens": ["Man\u00b7che", "leicht", "ver\u00b7h\u00fcll\u00b7te", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Einer lieben Seele zeigtest.", "tokens": ["Ei\u00b7ner", "lie\u00b7ben", "See\u00b7le", "zeig\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl ist mir's, da\u00df ich dich nicht verkannt,", "tokens": ["Wohl", "ist", "mir's", ",", "da\u00df", "ich", "dich", "nicht", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da\u00df ich gleich dich in der ersten Stunde,", "tokens": ["Da\u00df", "ich", "gleich", "dich", "in", "der", "ers\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ganz den Herzensausdruck in dem Munde,", "tokens": ["Ganz", "den", "Her\u00b7zen\u00b7saus\u00b7druck", "in", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Dich ein wahres gutes Kind genannt.", "tokens": ["Dich", "ein", "wah\u00b7res", "gu\u00b7tes", "Kind", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Still und eng und ruhig auferzogen,", "tokens": ["Still", "und", "eng", "und", "ru\u00b7hig", "auf\u00b7er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wirft man uns auf einmal in die Welt;", "tokens": ["Wirft", "man", "uns", "auf", "ein\u00b7mal", "in", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Uns umsp\u00fclen hunderttausend Wogen,", "tokens": ["Uns", "um\u00b7sp\u00fc\u00b7len", "hun\u00b7dert\u00b7tau\u00b7send", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles reizt uns, mancherlei gef\u00e4llt,", "tokens": ["Al\u00b7les", "reizt", "uns", ",", "man\u00b7cher\u00b7lei", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mancherlei verdrie\u00dft uns, und von Stund zu Stunden", "tokens": ["Man\u00b7cher\u00b7lei", "ver\u00b7drie\u00dft", "uns", ",", "und", "von", "Stund", "zu", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KON", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Schwankt das leichtunruhige Gef\u00fchl;", "tokens": ["Schwankt", "das", "leich\u00b7tun\u00b7ru\u00b7hi\u00b7ge", "Ge\u00b7f\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wir empfinden, und was wir empfunden,", "tokens": ["Wir", "emp\u00b7fin\u00b7den", ",", "und", "was", "wir", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sp\u00fclt hinweg das bunte Weltgew\u00fchl.", "tokens": ["Sp\u00fclt", "hin\u00b7weg", "das", "bun\u00b7te", "Welt\u00b7ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl, ich wei\u00df es, da durchschleicht uns innen", "tokens": ["Wohl", ",", "ich", "wei\u00df", "es", ",", "da", "durch\u00b7schleicht", "uns", "in\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Manche Hoffnung, mancher Schmerz.", "tokens": ["Man\u00b7che", "Hoff\u00b7nung", ",", "man\u00b7cher", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lottchen, wer kennt unsre Sinnen?", "tokens": ["Lott\u00b7chen", ",", "wer", "kennt", "uns\u00b7re", "Sin\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Lottchen, wer kennt unser Herz?", "tokens": ["Lott\u00b7chen", ",", "wer", "kennt", "un\u00b7ser", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, es machte gern gekannt sein, \u00fcberflie\u00dfen", "tokens": ["Ach", ",", "es", "mach\u00b7te", "gern", "ge\u00b7kannt", "sein", ",", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "VVPP", "VAINF", "$,", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "In das Mitempfinden einer Kreatur", "tokens": ["In", "das", "Mi\u00b7temp\u00b7fin\u00b7den", "ei\u00b7ner", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Und vertrauend zwiefach neu genie\u00dfen", "tokens": ["Und", "ver\u00b7trau\u00b7end", "zwie\u00b7fach", "neu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Alles Leid und Freude der Natur.", "tokens": ["Al\u00b7les", "Leid", "und", "Freu\u00b7de", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Und da sucht das Aug so oft vergebens", "tokens": ["Und", "da", "sucht", "das", "Aug", "so", "oft", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ringsumher und findet alles zu;", "tokens": ["Rings\u00b7um\u00b7her", "und", "fin\u00b7det", "al\u00b7les", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "So vertaumelt sich der sch\u00f6nste Teil des Lebens", "tokens": ["So", "ver\u00b7tau\u00b7melt", "sich", "der", "sch\u00f6ns\u00b7te", "Teil", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ohne Sturm und ohne Ruh;", "tokens": ["Oh\u00b7ne", "Sturm", "und", "oh\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zu deinem ew'gen Unbehagen", "tokens": ["Und", "zu", "dei\u00b7nem", "ew'\u00b7gen", "Un\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "St\u00f6\u00dft dich heute, was dich gestern zog.", "tokens": ["St\u00f6\u00dft", "dich", "heu\u00b7te", ",", "was", "dich", "ge\u00b7stern", "zog", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Kannst du zu der Welt nur Neigung tragen,", "tokens": ["Kannst", "du", "zu", "der", "Welt", "nur", "Nei\u00b7gung", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die so oft dich trog", "tokens": ["Die", "so", "oft", "dich", "trog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und bei deinem Weh, bei deinem Gl\u00fccke", "tokens": ["Und", "bei", "dei\u00b7nem", "Weh", ",", "bei", "dei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Blieb in eigenwill'ger, starrer Ruh?", "tokens": ["Blieb", "in", "ei\u00b7gen\u00b7will'\u00b7ger", ",", "star\u00b7rer", "Ruh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Sieh, da tritt der Geist in sich zur\u00fccke,", "tokens": ["Sieh", ",", "da", "tritt", "der", "Geist", "in", "sich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und das Herz \u2013 es schlie\u00dft sich zu.", "tokens": ["Und", "das", "Herz", "\u2013", "es", "schlie\u00dft", "sich", "zu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So fand ich dich und ging dir frei entgegen.", "tokens": ["So", "fand", "ich", "dich", "und", "ging", "dir", "frei", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbo sie ist wert, zu sein geliebt!\u00ab", "tokens": ["\u00bb", "o", "sie", "ist", "wert", ",", "zu", "sein", "ge\u00b7liebt", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PPER", "VAFIN", "ADJD", "$,", "APPR", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rief ich, erflehte dir des Himmels reinsten Segen,", "tokens": ["Rief", "ich", ",", "er\u00b7fleh\u00b7te", "dir", "des", "Him\u00b7mels", "reins\u00b7ten", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den er dir nun in deiner Freundin gibt.", "tokens": ["Den", "er", "dir", "nun", "in", "dei\u00b7ner", "Freun\u00b7din", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mitten im Get\u00fcmmel mancher Freuden,", "tokens": ["Mit\u00b7ten", "im", "Ge\u00b7t\u00fcm\u00b7mel", "man\u00b7cher", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mancher Sorgen, mancher Herzensnot", "tokens": ["Man\u00b7cher", "Sor\u00b7gen", ",", "man\u00b7cher", "Her\u00b7zens\u00b7not"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denk ich dein, o Lottchen, denken dein die beiden,", "tokens": ["Denk", "ich", "dein", ",", "o", "Lott\u00b7chen", ",", "den\u00b7ken", "dein", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "ART", "PIAT", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie beim stillen Abendrot", "tokens": ["Wie", "beim", "stil\u00b7len", "A\u00b7ben\u00b7drot"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du die Hand uns freundlich reichtest,", "tokens": ["Du", "die", "Hand", "uns", "freund\u00b7lich", "reich\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da du uns auf reich bebauter Flur,", "tokens": ["Da", "du", "uns", "auf", "reich", "be\u00b7bau\u00b7ter", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ADJD", 