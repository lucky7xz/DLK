{"dta.poem.15656": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welt ist Bewegung. Was bleibt unbeweglich wol?", "tokens": ["Welt", "ist", "Be\u00b7we\u00b7gung", ".", "Was", "bleibt", "un\u00b7be\u00b7weg\u00b7lich", "wol", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "PWS", "VVFIN", "ADJD", "ADV", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Vor tausend Jahren wies ein andrer Stern den Pol.", "tokens": ["Vor", "tau\u00b7send", "Jah\u00b7ren", "wies", "ein", "an\u00b7drer", "Stern", "den", "Pol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach tausend Jahren wird ein anderer ihn weisen,", "tokens": ["Nach", "tau\u00b7send", "Jah\u00b7ren", "wird", "ein", "an\u00b7de\u00b7rer", "ihn", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wonach man steuern wird bei Land- und Meeresreisen.", "tokens": ["Wo\u00b7nach", "man", "steu\u00b7ern", "wird", "bei", "Lan\u00b7d", "und", "Mee\u00b7res\u00b7rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VAFIN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Warum steht selbst nicht fest der fixen Sterne Chor?", "tokens": ["Wa\u00b7rum", "steht", "selbst", "nicht", "fest", "der", "fi\u00b7xen", "Ster\u00b7ne", "Chor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nach unserm Sonnenkreis, so scheint es, r\u00fcckt er vor.", "tokens": ["Nach", "un\u00b7serm", "Son\u00b7nen\u00b7kreis", ",", "so", "scheint", "es", ",", "r\u00fcckt", "er", "vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn unsre Sonn' in f\u00fcnfundzwanzigtausend Jahren", "tokens": ["Denn", "uns\u00b7re", "Sonn'", "in", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig\u00b7tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will ihre Sonn' einmal, s\u00fcd\u00f6stlich scheints, umfahren.", "tokens": ["Will", "ih\u00b7re", "Sonn'", "ein\u00b7mal", ",", "s\u00fcd\u00b7\u00f6st\u00b7lich", "scheints", ",", "um\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "$,", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Wo ist der Sonne Sonn' im S\u00fcdost? Unerkannt", "tokens": ["Wo", "ist", "der", "Son\u00b7ne", "Sonn'", "im", "S\u00fcd\u00b7ost", "?", "Un\u00b7er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "APPRART", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Sternheer, doch vielleicht der Sirius genannt.", "tokens": ["Im", "Stern\u00b7heer", ",", "doch", "viel\u00b7leicht", "der", "Si\u00b7rius", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Und steht nun diese fest? Auch sie wird, Gott zu preisen,", "tokens": ["Und", "steht", "nun", "die\u00b7se", "fest", "?", "Auch", "sie", "wird", ",", "Gott", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDS", "PTKVZ", "$.", "ADV", "PPER", "VAFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch sie um eine Sonn', und die um eine kreisen.", "tokens": ["Auch", "sie", "um", "ei\u00b7ne", "Sonn'", ",", "und", "die", "um", "ei\u00b7ne", "krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "$,", "KON", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}