{"dta.poem.7418": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungen und die Alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu bist jung, Du sollst nicht sprechen!", "tokens": ["\u201e", "du", "bist", "jung", ",", "Du", "sollst", "nicht", "spre\u00b7chen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist jung, wir sind die Alten!", "tokens": ["Du", "bist", "jung", ",", "wir", "sind", "die", "Al\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df die Wogen erst sich brechen", "tokens": ["La\u00df", "die", "Wo\u00b7gen", "erst", "sich", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Gluten erst erkalten!", "tokens": ["Und", "die", "Glu\u00b7ten", "erst", "er\u00b7kal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Du bist jung, Dein Thun ist eitel!", "tokens": ["Du", "bist", "jung", ",", "Dein", "Thun", "ist", "ei\u00b7tel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du bist jung und unerfahren!", "tokens": ["Du", "bist", "jung", "und", "un\u00b7er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist jung, kr\u00e4nz' Deinen Scheitel", "tokens": ["Du", "bist", "jung", ",", "kr\u00e4nz'", "Dei\u00b7nen", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst mit unsern weissen Haaren!", "tokens": ["Erst", "mit", "un\u00b7sern", "weis\u00b7sen", "Haa\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lern', mein Lieber, erst entsagen,", "tokens": ["Lern'", ",", "mein", "Lie\u00b7ber", ",", "erst", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Flammen erst verrauchen,", "tokens": ["La\u00df", "die", "Flam\u00b7men", "erst", "ver\u00b7rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df Dich erst in Ketten schlagen,", "tokens": ["La\u00df", "Dich", "erst", "in", "Ket\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann vielleicht kann man Dich brauchen!\u201c", "tokens": ["Dann", "viel\u00b7leicht", "kann", "man", "Dich", "brau\u00b7chen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Kluge Herren! Die Gefangnen", "tokens": ["Klu\u00b7ge", "Her\u00b7ren", "!", "Die", "Ge\u00b7fang\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chten ihres Gleichen schauen;", "tokens": ["M\u00f6ch\u00b7ten", "ih\u00b7res", "Glei\u00b7chen", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, ihr H\u00fcter des Vergangnen,", "tokens": ["Doch", ",", "ihr", "H\u00fc\u00b7ter", "des", "Ver\u00b7gang\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer soll denn die Zukunft bauen?", "tokens": ["Wer", "soll", "denn", "die", "Zu\u00b7kunft", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sprecht, was sind euch denn verblieben,", "tokens": ["Sprecht", ",", "was", "sind", "euch", "denn", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Au\u00dfer uns, f\u00fcr wackre St\u00fctzen?", "tokens": ["Au\u00b7\u00dfer", "uns", ",", "f\u00fcr", "wack\u00b7re", "St\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer soll eure T\u00f6chter lieben?", "tokens": ["Wer", "soll", "eu\u00b7re", "T\u00f6ch\u00b7ter", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer soll eure H\u00e4user sch\u00fctzen?", "tokens": ["Wer", "soll", "eu\u00b7re", "H\u00e4u\u00b7ser", "sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schm\u00e4ht mir nicht die blonden Locken,", "tokens": ["Schm\u00e4ht", "mir", "nicht", "die", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die st\u00fcrmische Geberde!", "tokens": ["Nicht", "die", "st\u00fcr\u00b7mi\u00b7sche", "Ge\u00b7ber\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n sind eure Silberflocken,", "tokens": ["Sch\u00f6n", "sind", "eu\u00b7re", "Sil\u00b7ber\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch dem ", "tokens": ["Doch", "dem"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Schm\u00e4het, schm\u00e4ht mir nicht die Jugend,", "tokens": ["Schm\u00e4\u00b7het", ",", "schm\u00e4ht", "mir", "nicht", "die", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie auch sich laut verk\u00fcndigt!", "tokens": ["Wie", "sie", "auch", "sich", "laut", "ver\u00b7k\u00fcn\u00b7digt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie oft hat eure Tugend", "tokens": ["O", "wie", "oft", "hat", "eu\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Menschheit still ges\u00fcndigt!", "tokens": ["An", "der", "Menschheit", "still", "ge\u00b7s\u00fcn\u00b7digt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}