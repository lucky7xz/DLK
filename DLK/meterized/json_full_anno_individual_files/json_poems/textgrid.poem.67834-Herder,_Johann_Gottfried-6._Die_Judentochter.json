{"textgrid.poem.67834": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6. Die Judentochter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen, er rinnt durch Mirrilandstadt,", "tokens": ["Der", "Re\u00b7gen", ",", "er", "rinnt", "durch", "Mir\u00b7ri\u00b7land\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rinnt ab und nieden den Po!", "tokens": ["Rinnt", "ab", "und", "nie\u00b7den", "den", "Po", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PIS", "ART", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So thun die Knaben in Mirrilandstadt,", "tokens": ["So", "thun", "die", "Kna\u00b7ben", "in", "Mir\u00b7ri\u00b7land\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Ballspiel rennen sie so.", "tokens": ["Zum", "Ball\u00b7spiel", "ren\u00b7nen", "sie", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da 'naus und kam die Judentochter,", "tokens": ["Da", "'naus", "und", "kam", "die", "Ju\u00b7den\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach: willt du nicht kommen hinein?", "tokens": ["Sprach", ":", "willt", "du", "nicht", "kom\u00b7men", "hin\u00b7ein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbich will nicht kommen, ich kann nicht kommen", "tokens": ["\u00bb", "ich", "will", "nicht", "kom\u00b7men", ",", "ich", "kann", "nicht", "kom\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Von allen Gespielen mein.\u00ab", "tokens": ["Von", "al\u00b7len", "Ge\u00b7spie\u00b7len", "mein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie sch\u00e4lt einen Apfel, war roth (und) wei\u00df,", "tokens": ["Sie", "sch\u00e4lt", "ei\u00b7nen", "Ap\u00b7fel", ",", "war", "roth", "(", "und", ")", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "$(", "KON", "$(", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Zu locken den Knaben hinan.", "tokens": ["Zu", "lo\u00b7cken", "den", "Kna\u00b7ben", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie sch\u00e4lt einen Apfel, war wei\u00df und roth,", "tokens": ["Sie", "sch\u00e4lt", "ei\u00b7nen", "Ap\u00b7fel", ",", "war", "wei\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das s\u00fcsse Kind der gewann.", "tokens": ["Das", "s\u00fcs\u00b7se", "Kind", "der", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und aus und zog sie ein spizig Mess'r,", "tokens": ["Und", "aus", "und", "zog", "sie", "ein", "spi\u00b7zig", "Mess'r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie hatt's versteckt beiher;", "tokens": ["Sie", "hatt's", "ver\u00b7steckt", "bei\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stachs dem jungen Knaben ins Herz,", "tokens": ["Sie", "stachs", "dem", "jun\u00b7gen", "Kna\u00b7ben", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kein Wort sprach nimmer er mehr.", "tokens": ["Kein", "Wort", "sprach", "nim\u00b7mer", "er", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Und aus und kam das dick dick Blut,", "tokens": ["Und", "aus", "und", "kam", "das", "dick", "dick", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "VVFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aus und kam es so d\u00fcnn,", "tokens": ["Und", "aus", "und", "kam", "es", "so", "d\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und aus und kam 's Kinds Herzensblut;", "tokens": ["Und", "aus", "und", "kam", "'s", "Kinds", "Her\u00b7zens\u00b7blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da war kein Leben mehr in.", "tokens": ["Da", "war", "kein", "Le\u00b7ben", "mehr", "in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "APPR", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Sie legt' ihn auf ein Schlachtbrett hin,", "tokens": ["Sie", "legt'", "ihn", "auf", "ein", "Schlacht\u00b7brett", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlacht't ihn ein Christenschwein,", "tokens": ["Schlacht't", "ihn", "ein", "Chris\u00b7ten\u00b7schwein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprach lachend: \u00bbgeh und spiele nun da", "tokens": ["Sprach", "la\u00b7chend", ":", "\u00bb", "geh", "und", "spie\u00b7le", "nun", "da"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "$(", "VVFIN", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit allen Gespielen dein!\u00ab", "tokens": ["Mit", "al\u00b7len", "Ge\u00b7spie\u00b7len", "dein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie rollt ihn in ein'n Kasten Blei;", "tokens": ["Sie", "rollt", "ihn", "in", "ein'n", "Kas\u00b7ten", "Blei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnun schlaf da!\u00ab lachend sie rief;", "tokens": ["\u00bb", "nun", "schlaf", "da", "!", "\u00ab", "la\u00b7chend", "sie", "rief", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "$.", "$(", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie warf ihn in ein'n tiefen Brunn,", "tokens": ["Sie", "warf", "ihn", "in", "ein'n", "tie\u00b7fen", "Brunn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War funfzig Faden tief.", "tokens": ["War", "funf\u00b7zig", "Fa\u00b7den", "tief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als Betglock klang und die Nacht eindrang,", "tokens": ["Als", "Bet\u00b7glock", "klang", "und", "die", "Nacht", "ein\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jede Mutter nun kam daheim;", "tokens": ["Je\u00b7de", "Mut\u00b7ter", "nun", "kam", "da\u00b7heim", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Jede Mutter hatt' ihren herzlieben Sohn,", "tokens": ["Je\u00b7de", "Mut\u00b7ter", "hatt'", "ih\u00b7ren", "herz\u00b7lie\u00b7ben", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Nur Mutter Anne hatt kein'n.", "tokens": ["Nur", "Mut\u00b7ter", "An\u00b7ne", "hatt", "kein'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "NN", "NE", "VAFIN", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Sie rollt ihren Mantel um sich her,", "tokens": ["Sie", "rollt", "ih\u00b7ren", "Man\u00b7tel", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fing an zu weinen sehr,", "tokens": ["Fing", "an", "zu", "wei\u00b7nen", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PTKZU", "VVINF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie rann so schnell ins Juden Castell,", "tokens": ["Sie", "rann", "so", "schnell", "ins", "Ju\u00b7den", "Cas\u00b7tell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo keiner, ach! wachte mehr:", "tokens": ["Wo", "kei\u00b7ner", ",", "ach", "!", "wach\u00b7te", "mehr", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "ITJ", "$.", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbmein liebster H\u00f6nne, mein guter H\u00f6nne,", "tokens": ["\u00bb", "mein", "liebs\u00b7ter", "H\u00f6n\u00b7ne", ",", "mein", "gu\u00b7ter", "H\u00f6n\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo bist du? antwort mir!\u00ab", "tokens": ["Wo", "bist", "du", "?", "ant\u00b7wort", "mir", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo Mutter, o rennt zum Ziehbrunn tief,", "tokens": ["\u00bb", "o", "Mut\u00b7ter", ",", "o", "rennt", "zum", "Zieh\u00b7brunn", "tief", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Euren Sohn da findet ihr!\u00ab", "tokens": ["Eu\u00b7ren", "Sohn", "da", "fin\u00b7det", "ihr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Mutter Anne rann zum tiefen Brunn,", "tokens": ["Mut\u00b7ter", "An\u00b7ne", "rann", "zum", "tie\u00b7fen", "Brunn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie fiel danieder aufs Knie!", "tokens": ["Sie", "fiel", "da\u00b7nie\u00b7der", "aufs", "Knie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u00bbmein liebster H\u00f6nne, mein guter H\u00f6nne,", "tokens": ["\u00bb", "mein", "liebs\u00b7ter", "H\u00f6n\u00b7ne", ",", "mein", "gu\u00b7ter", "H\u00f6n\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O antwort, bist du hier?\u00ab", "tokens": ["O", "ant\u00b7wort", ",", "bist", "du", "hier", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbder Brunn ist wunder tief, o Mutter,", "tokens": ["\u00bb", "der", "Brunn", "ist", "wun\u00b7der", "tief", ",", "o", "Mut\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "NN", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bleikast wunder schwer;", "tokens": ["Der", "Blei\u00b7kast", "wun\u00b7der", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein scharf, spiz Messer geht durch mein Herz;", "tokens": ["Ein", "scharf", ",", "spiz", "Mes\u00b7ser", "geht", "durch", "mein", "Herz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Kein Wort sprech nimmer ich mehr.", "tokens": ["Kein", "Wort", "sprech", "nim\u00b7mer", "ich", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Geh heim, geh heim, mein' Mutter theur,", "tokens": ["Geh", "heim", ",", "geh", "heim", ",", "mein'", "Mut\u00b7ter", "theur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mach' mir mein Leichenkleid,", "tokens": ["Mach'", "mir", "mein", "Lei\u00b7chen\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Daheim da hinter Mirrilandstadt", "tokens": ["Da\u00b7heim", "da", "hin\u00b7ter", "Mir\u00b7ri\u00b7land\u00b7stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Komm' ich an eure Seit'.\u00ab", "tokens": ["Komm'", "ich", "an", "eu\u00b7re", "Seit'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Regen, er rinnt durch Mirrilandstadt,", "tokens": ["Der", "Re\u00b7gen", ",", "er", "rinnt", "durch", "Mir\u00b7ri\u00b7land\u00b7stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rinnt ab und nieden den Po!", "tokens": ["Rinnt", "ab", "und", "nie\u00b7den", "den", "Po", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PIS", "ART", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So thun die Knaben in Mirrilandstadt,", "tokens": ["So", "thun", "die", "Kna\u00b7ben", "in", "Mir\u00b7ri\u00b7land\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Ballspiel rennen sie so.", "tokens": ["Zum", "Ball\u00b7spiel", "ren\u00b7nen", "sie", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Da 'naus und kam die Judentochter,", "tokens": ["Da", "'naus", "und", "kam", "die", "Ju\u00b7den\u00b7toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach: willt du nicht kommen hinein?", "tokens": ["Sprach", ":", "willt", "du", "nicht", "kom\u00b7men", "hin\u00b7ein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$."