{"textgrid.poem.32983": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lebwohl, Kind! ... die Fahrt, die du wagst,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebwohl, Kind! ... die Fahrt, die du wagst,", "tokens": ["Leb\u00b7wohl", ",", "Kind", "!", "...", "die", "Fahrt", ",", "die", "du", "wagst", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "$(", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "ist weit!", "tokens": ["ist", "weit", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Mein Wunsch, da\u00df es gut dir gehe,", "tokens": ["Mein", "Wunsch", ",", "da\u00df", "es", "gut", "dir", "ge\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "geb dir getreulich Geleit!", "tokens": ["geb", "dir", "ge\u00b7treu\u00b7lich", "Ge\u00b7leit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Leb wohl! den Kopf immer hoch", "tokens": ["Leb", "wohl", "!", "den", "Kopf", "im\u00b7mer", "hoch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "und fr\u00f6hlich und unverzagt,", "tokens": ["und", "fr\u00f6h\u00b7lich", "und", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "und nie zuviel auch bei andern", "tokens": ["und", "nie", "zu\u00b7viel", "auch", "bei", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIS", "ADV", "APPR", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "um Rat und Meinung gefragt!", "tokens": ["um", "Rat", "und", "Mei\u00b7nung", "ge\u00b7fragt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Raten ist leicht, doch es geht schon", "tokens": ["Ra\u00b7ten", "ist", "leicht", ",", "doch", "es", "geht", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "nicht alles im rechten Gleis,", "tokens": ["nicht", "al\u00b7les", "im", "rech\u00b7ten", "Gleis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "wenn man Rat braucht, Kind, und sich", "tokens": ["wenn", "man", "Rat", "braucht", ",", "Kind", ",", "und", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "$,", "NN", "$,", "KON", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "nicht selbst zu helfen wei\u00df!", "tokens": ["nicht", "selbst", "zu", "hel\u00b7fen", "wei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Es tr\u00e4gt ein jeder zudem schon", "tokens": ["Es", "tr\u00e4gt", "ein", "je\u00b7der", "zu\u00b7dem", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "so viel an eigener Last,", "tokens": ["so", "viel", "an", "ei\u00b7ge\u00b7ner", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "da\u00df er sich meist nur ungern", "tokens": ["da\u00df", "er", "sich", "meist", "nur", "un\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "mit fremden Sorgen befa\u00dft!", "tokens": ["mit", "frem\u00b7den", "Sor\u00b7gen", "be\u00b7fa\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Es kommt auch selten etwas", "tokens": ["Es", "kommt", "auch", "sel\u00b7ten", "et\u00b7was"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "dabei heraus und ich mein:", "tokens": ["da\u00b7bei", "he\u00b7raus", "und", "ich", "mein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "KON", "PPER", "PPOSAT", "$."], "meter": "-+----+", "measure": "dactylic.init"}, "line.19": {"text": "man m\u00fcsse f\u00fcr Gl\u00fcck und Ungl\u00fcck", "tokens": ["man", "m\u00fcs\u00b7se", "f\u00fcr", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "immer selbst verantwortlich sein.", "tokens": ["im\u00b7mer", "selbst", "ver\u00b7ant\u00b7wort\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Wer seines Zieles klar ist,", "tokens": ["Wer", "sei\u00b7nes", "Zie\u00b7les", "klar", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "erreicht, was er erstrebt,", "tokens": ["er\u00b7reicht", ",", "was", "er", "er\u00b7strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "und wer ein Ziel errungen,", "tokens": ["und", "wer", "ein", "Ziel", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "hat nie vergebens gelebt!", "tokens": ["hat", "nie", "ver\u00b7ge\u00b7bens", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Lebwohl, Kind! und wenn es wettert", "tokens": ["Leb\u00b7wohl", ",", "Kind", "!", "und", "wenn", "es", "wet\u00b7tert"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "KON", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und Blitze und Wolken dr\u00e4un,", "tokens": ["und", "Blit\u00b7ze", "und", "Wol\u00b7ken", "dr\u00e4un", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "es kommen auch Tage wieder,", "tokens": ["es", "kom\u00b7men", "auch", "Ta\u00b7ge", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die Bl\u00fcten und Rosen streun.", "tokens": ["die", "Bl\u00fc\u00b7ten", "und", "Ro\u00b7sen", "streun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es ging ja uns beiden im Leben", "tokens": ["Es", "ging", "ja", "uns", "bei\u00b7den", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PIAT", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "nie noch besonders gut,", "tokens": ["nie", "noch", "be\u00b7son\u00b7ders", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "wir erfuhren niemals, wie sch\u00f6n es", "tokens": ["wir", "er\u00b7fuh\u00b7ren", "nie\u00b7mals", ",", "wie", "sch\u00f6n", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "PPER"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "ohne Sorge sich ruht;", "tokens": ["oh\u00b7ne", "Sor\u00b7ge", "sich", "ruht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "wir haben von fr\u00fch an in fremde", "tokens": ["wir", "ha\u00b7ben", "von", "fr\u00fch", "an", "in", "frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Launen uns schicken gemu\u00dft", "tokens": ["Lau\u00b7nen", "uns", "schi\u00b7cken", "ge\u00b7mu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "VVINF", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "und hatten niemand, zu teilen,", "tokens": ["und", "hat\u00b7ten", "nie\u00b7mand", ",", "zu", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "weder bei Leid noch bei Lust;", "tokens": ["we\u00b7der", "bei", "Leid", "noch", "bei", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "und gerade in Jugendtagen", "tokens": ["und", "ge\u00b7ra\u00b7de", "in", "Ju\u00b7gend\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "ist das wohl der herbste Schmerz:", "tokens": ["ist", "das", "wohl", "der", "herbs\u00b7te", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "man tr\u00e4umt da von Wunderdingen", "tokens": ["man", "tr\u00e4umt", "da", "von", "Wun\u00b7der\u00b7din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "und hat so voll das Herz,", "tokens": ["und", "hat", "so", "voll", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "man m\u00f6chte jubeln und jauchzen", "tokens": ["man", "m\u00f6ch\u00b7te", "ju\u00b7beln", "und", "jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "und m\u00f6chte gl\u00fccklich sein", "tokens": ["und", "m\u00f6ch\u00b7te", "gl\u00fcck\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "und denkt, das Leben best\u00fcnde", "tokens": ["und", "denkt", ",", "das", "Le\u00b7ben", "be\u00b7st\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "aus lauter Sonnenschein.", "tokens": ["aus", "lau\u00b7ter", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es kann ja nun alles sich \u00e4ndern,", "tokens": ["Es", "kann", "ja", "nun", "al\u00b7les", "sich", "\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PIS", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "ich glaubte f\u00fcr dich es so gern:", "tokens": ["ich", "glaub\u00b7te", "f\u00fcr", "dich", "es", "so", "gern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "es kann vom Himmel fallen", "tokens": ["es", "kann", "vom", "Him\u00b7mel", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wie ein rotblitzender Stern,", "tokens": ["wie", "ein", "rot\u00b7blit\u00b7zen\u00b7der", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "es kann auf schimmerndem Fl\u00fcgel", "tokens": ["es", "kann", "auf", "schim\u00b7mern\u00b7dem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "herrauschen im Windeswehn,", "tokens": ["her\u00b7rau\u00b7schen", "im", "Win\u00b7des\u00b7wehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "es kann mit jauchzendem Liede", "tokens": ["es", "kann", "mit", "jauch\u00b7zen\u00b7dem", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "urpl\u00f6tzlich vor dir stehn! ...", "tokens": ["ur\u00b7pl\u00f6tz\u00b7lich", "vor", "dir", "stehn", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Dichter sind's, die das sagen,", "tokens": ["Dich\u00b7ter", "sin\u00b7d's", ",", "die", "das", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "auch h\u00f6rt man es sonst dann und wann,", "tokens": ["auch", "h\u00f6rt", "man", "es", "sonst", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "ADV", "KON", "PWAV", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "im wirklichen Leben aber ...", "tokens": ["im", "wirk\u00b7li\u00b7chen", "Le\u00b7ben", "a\u00b7ber", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "ich glaube nicht recht daran!", "tokens": ["ich", "glau\u00b7be", "nicht", "recht", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ich glaube viel eher, es wird", "tokens": ["Ich", "glau\u00b7be", "viel", "e\u00b7her", ",", "es", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "so sein, wie es bisher war:", "tokens": ["so", "sein", ",", "wie", "es", "bis\u00b7her", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "von allem, was man sich w\u00fcnscht,", "tokens": ["von", "al\u00b7lem", ",", "was", "man", "sich", "w\u00fcnscht", ","], "token_info": ["word", "w