{"dta.poem.19005": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.85", "af:0.14"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Warumb jhr Frawen vnd Jungfrawen", "tokens": ["Wa\u00b7rumb", "jhr", "Fra\u00b7wen", "vnd", "Jung\u00b7fra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Spieglet jhr euch so williglich/", "tokens": ["Spieg\u00b7let", "jhr", "euch", "so", "wil\u00b7lig\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wan die Sonn jhren glantz zu schawen", "tokens": ["Wan", "die", "Sonn", "jhren", "glantz", "zu", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Pfleget in euch zu spieglen sich;", "tokens": ["Pfle\u00b7get", "in", "euch", "zu", "spieg\u00b7len", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKZU", "VVFIN", "PRF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Vnd wan die klarheit ewrer augen", "tokens": ["Vnd", "wan", "die", "klar\u00b7heit", "ew\u00b7rer", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan wol f\u00fcr einen spiegel taugen?", "tokens": ["Kan", "wol", "f\u00fcr", "ei\u00b7nen", "spie\u00b7gel", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr wahre Spiegel aller ehren", "tokens": ["Ihr", "wah\u00b7re", "Spie\u00b7gel", "al\u00b7ler", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pfleget mit durchdringendem schein", "tokens": ["Pfle\u00b7get", "mit", "durch\u00b7drin\u00b7gen\u00b7dem", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die G\u00f6tter vnd menschen zu lehren", "tokens": ["Die", "G\u00f6t\u00b7ter", "vnd", "men\u00b7schen", "zu", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie tugenthafft sie sollen sein/", "tokens": ["Wie", "tu\u00b7gent\u00b7hafft", "sie", "sol\u00b7len", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil die klarheit ewrer augen", "tokens": ["Die\u00b7weil", "die", "klar\u00b7heit", "ew\u00b7rer", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kan besser dan kein spiegel taugen.", "tokens": ["Kan", "bes\u00b7ser", "dan", "kein", "spie\u00b7gel", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Darumb die G\u00f6tter mit verlangen", "tokens": ["Da\u00b7rumb", "die", "G\u00f6t\u00b7ter", "mit", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Spiegelmacher gar voll glantz/", "tokens": ["Als", "Spie\u00b7gel\u00b7ma\u00b7cher", "gar", "voll", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ehren Euch kommen gegangen/", "tokens": ["Zu", "eh\u00b7ren", "Euch", "kom\u00b7men", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VVINF", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu spieglen sich in jhrem dantz/", "tokens": ["Zu", "spieg\u00b7len", "sich", "in", "jhrem", "dantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "APPR", "PPOSAT", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dieweil die striemen ewrer augen", "tokens": ["Die\u00b7weil", "die", "strie\u00b7men", "ew\u00b7rer", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr spiegel jhren spiegeln taugen.", "tokens": ["F\u00fcr", "spie\u00b7gel", "jhren", "spie\u00b7geln", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}