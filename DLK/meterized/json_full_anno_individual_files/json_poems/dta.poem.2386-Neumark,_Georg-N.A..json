{"dta.poem.2386": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.68", "no:0.24"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Dort jener Friedensheld/ der R\u00f6mer frommer Keiser", "tokens": ["Dort", "je\u00b7ner", "Frie\u00b7dens\u00b7held", "/", "der", "R\u00f6\u00b7mer", "from\u00b7mer", "Kei\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der l\u00f6blich\u2019 Antonin/ der Helden Tugendweiser/", "tokens": ["Der", "l\u00f6b\u00b7lich'", "An\u00b7to\u00b7nin", "/", "der", "Hel\u00b7den", "Tu\u00b7gend\u00b7wei\u00b7ser", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des Himmels liebe Lust war offt dahin bedacht", "tokens": ["Des", "Him\u00b7mels", "lie\u00b7be", "Lust", "war", "offt", "da\u00b7hin", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Er durch viel Geschenk\u2019 ihm gute Freunde macht\u2019/", "tokens": ["Wie", "Er", "durch", "viel", "Ge\u00b7schen\u00b7k'", "ihm", "gu\u00b7te", "Freun\u00b7de", "macht'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und br\u00e4chte sie zur Gunst; ietzt d\u00e4nk\u2019 ich auch dergleichen/", "tokens": ["Und", "br\u00e4ch\u00b7te", "sie", "zur", "Gunst", ";", "ietzt", "d\u00e4nk'", "ich", "auch", "derg\u00b7lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ich mein Freundes Hertz euch satsam \u00fcberreichen", "tokens": ["Wie", "ich", "mein", "Freun\u00b7des", "Hertz", "euch", "sat\u00b7sam", "\u00fc\u00b7berr\u00b7ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und offenbaren soll. Herr Meyrmann  werther Frund,", "tokens": ["Und", "of\u00b7fen\u00b7ba\u00b7ren", "soll", ".", "Herr", "Meyr\u00b7mann", "wert\u00b7her", "Frund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$.", "NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den mein unfalsches Hertz recht/ treu und redlich meint.", "tokens": ["Den", "mein", "un\u00b7fal\u00b7sches", "Hertz", "recht", "/", "treu", "und", "red\u00b7lich", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADJD", "$(", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wol! es ist schon bedacht. Nehmt die\u00df von meinen H\u00e4nde\u0303/", "tokens": ["Wol", "!", "es", "ist", "schon", "be\u00b7dacht", ".", "Nehmt", "die\u00df", "von", "mei\u00b7nen", "H\u00e4n\u00b7d\u1ebd", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$.", "VVFIN", "PDS", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Mit dem ich unverf\u00e4lscht Euch wil mein Hertz verpf\u00e4nden/", "tokens": ["Mit", "dem", "ich", "un\u00b7ver\u00b7f\u00e4lscht", "Euch", "wil", "mein", "Hertz", "ver\u00b7pf\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und meine Sch\u00fcldigkeit. Ein treuer Freund der liebt/", "tokens": ["Und", "mei\u00b7ne", "Sch\u00fcl\u00b7dig\u00b7keit", ".", "Ein", "treu\u00b7er", "Freund", "der", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was ihm sein Gegenfreund aus treuem Hertzen giebt.", "tokens": ["Was", "ihm", "sein", "Ge\u00b7gen\u00b7freund", "aus", "treu\u00b7em", "Hert\u00b7zen", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zwar ich bekenn\u2019 es gern da\u00df dieses Buch geringe/", "tokens": ["Zwar", "ich", "be\u00b7kenn'", "es", "gern", "da\u00df", "die\u00b7ses", "Buch", "ge\u00b7rin\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "KOUS", "PDAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nach eusserlichem Wehrt/ doch zeigt es solche Dinge", "tokens": ["Nach", "eus\u00b7ser\u00b7li\u00b7chem", "Wehrt", "/", "doch", "zeigt", "es", "sol\u00b7che", "Din\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die theur und kostbar sind: Wir finden da den Weg/", "tokens": ["Die", "theur", "und", "kost\u00b7bar", "sind", ":", "Wir", "fin\u00b7den", "da", "den", "Weg", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zum rechten Christenthum/ der rechte Himmelssteg", "tokens": ["Zum", "rech\u00b7ten", "Chris\u00b7ten\u00b7thum", "/", "der", "rech\u00b7te", "Him\u00b7melss\u00b7teg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ist auch darinn gebaant/ die sch\u00f6ne Melodeyen/", "tokens": ["Ist", "auch", "da\u00b7rinn", "ge\u00b7baant", "/", "die", "sch\u00f6\u00b7ne", "Me\u00b7lo\u00b7de\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVPP", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Sind so gesetzt/ da\u00df sie Sinn/ Hertz und Seel erfreuen.", "tokens": ["Sind", "so", "ge\u00b7setzt", "/", "da\u00df", "sie", "Sinn", "/", "Hertz", "und", "Seel", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$(", "KOUS", "PPER", "NN", "$(", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wolan/ so nehmt es an/ und lernet diese zwey:", "tokens": ["Wo\u00b7lan", "/", "so", "nehmt", "es", "an", "/", "und", "ler\u00b7net", "die\u00b7se", "zwey", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "PDAT", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Baut euer Christenthum/ und d\u00e4nket", "tokens": ["Baut", "eu\u00b7er", "Chris\u00b7ten\u00b7thum", "/", "und", "d\u00e4n\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "mein dabey.", "tokens": ["mein", "da\u00b7bey", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "PAV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}