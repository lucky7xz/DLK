{"dta.poem.23840": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt l\u00e4\u00dft ihr Tadeln nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Merck auf/ ich bitte dich/ wies jenem Alten gieng/", "tokens": ["Merck", "auf", "/", "ich", "bit\u00b7te", "dich", "/", "wies", "je\u00b7nem", "Al\u00b7ten", "gieng", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "$(", "PPER", "VVFIN", "PPER", "$(", "VVFIN", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der/ um die Welt zu sehn/ noch an zu wandern fieng/", "tokens": ["Der", "/", "um", "die", "Welt", "zu", "sehn", "/", "noch", "an", "zu", "wan\u00b7dern", "fi\u00b7eng", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "APPR", "ART", "NN", "PTKZU", "VVINF", "$(", "ADV", "APPR", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ein Esel trug ihn fort/ sein Sohn war sein Gef\u00e4hrte.", "tokens": ["Ein", "E\u00b7sel", "trug", "ihn", "fort", "/", "sein", "Sohn", "war", "sein", "Ge\u00b7f\u00e4hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als nun der sanffte Ritt kaum eine Stunde w\u00e4hrte/", "tokens": ["Als", "nun", "der", "sanff\u00b7te", "Ritt", "kaum", "ei\u00b7ne", "Stun\u00b7de", "w\u00e4hr\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da rief ein Reisender ihm unterwegens an:", "tokens": ["Da", "rief", "ein", "Rei\u00b7sen\u00b7der", "ihm", "un\u00b7ter\u00b7we\u00b7gens", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was hat euch immermehr das arme Kind gethan/", "tokens": ["Was", "hat", "euch", "im\u00b7mer\u00b7mehr", "das", "ar\u00b7me", "Kind", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ihrs la\u00dft neben euch auf schwachen F\u00fcssen traben/", "tokens": ["Da\u00df", "ihrs", "la\u00dft", "ne\u00b7ben", "euch", "auf", "schwa\u00b7chen", "F\u00fcs\u00b7sen", "tra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drum stieg der Vater ab und wich dem m\u00fcden Knaben.", "tokens": ["Drum", "stieg", "der", "Va\u00b7ter", "ab", "und", "wich", "dem", "m\u00fc\u00b7den", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch als er dergestalt die Liebe walten lie\u00df/", "tokens": ["Doch", "als", "er", "der\u00b7ge\u00b7stalt", "die", "Lie\u00b7be", "wal\u00b7ten", "lie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sah er da\u00df man hernach mit Fingern auf ihn wie\u00df.", "tokens": ["Sah", "er", "da\u00df", "man", "her\u00b7nach", "mit", "Fin\u00b7gern", "auf", "ihn", "wie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PIS", "ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr k\u00f6ntet ja mit Recht/ h\u00f6rt er von andern Leuten/", "tokens": ["Ihr", "k\u00f6n\u00b7tet", "ja", "mit", "Recht", "/", "h\u00f6rt", "er", "von", "an\u00b7dern", "Leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zum wenigsten zugleich mit eurem Buben reiten.", "tokens": ["Zum", "we\u00b7nigs\u00b7ten", "zu\u00b7gleich", "mit", "eu\u00b7rem", "Bu\u00b7ben", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Er folgte diesem Raht/ und als er weiter kam/", "tokens": ["Er", "folg\u00b7te", "die\u00b7sem", "Raht", "/", "und", "als", "er", "wei\u00b7ter", "kam", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erfuhr er da\u00df man ihm auch dis f\u00fcr \u00fcbel nahm/", "tokens": ["Er\u00b7fuhr", "er", "da\u00df", "man", "ihm", "auch", "dis", "f\u00fcr", "\u00fc\u00b7bel", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PIS", "PPER", "ADV", "PDS", "APPR", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es schry ein gantzer Marck: ihr thut dem Thiere Scha-", "tokens": ["Es", "schry", "ein", "gant\u00b7zer", "Marck", ":", "ihr", "thut", "dem", "Thie\u00b7re", "Scha"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man pflegt nicht/ so wie ihr/ sein Vieh zu \u00fcberladen.", "tokens": ["Man", "pflegt", "nicht", "/", "so", "wie", "ihr", "/", "sein", "Vieh", "zu", "\u00fc\u00b7berl\u00b7a\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$(", "ADV", "KOKOM", "PPOSAT", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Alte der noch nie die Welt so wohl gekant/", "tokens": ["Der", "Al\u00b7te", "der", "noch", "nie", "die", "Welt", "so", "wohl", "ge\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der kehrte wiederum/ wie ers am besten fand/", "tokens": ["Der", "kehr\u00b7te", "wie\u00b7de\u00b7rum", "/", "wie", "ers", "am", "bes\u00b7ten", "fand", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$(", "PWAV", "PIS", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und sagte/ solt ich mich in alle Menschen schicken/", "tokens": ["Und", "sag\u00b7te", "/", "solt", "ich", "mich", "in", "al\u00b7le", "Men\u00b7schen", "schi\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So packten sie mir gar den Esel auf den R\u00fccken.", "tokens": ["So", "pack\u00b7ten", "sie", "mir", "gar", "den", "E\u00b7sel", "auf", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}