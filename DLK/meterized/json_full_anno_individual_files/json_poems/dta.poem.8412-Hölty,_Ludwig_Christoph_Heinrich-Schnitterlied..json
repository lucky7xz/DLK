{"dta.poem.8412": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schnitterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Von gr\u00fcnen Str\u00e4uchen und B\u00e4umen", "tokens": ["Von", "gr\u00fc\u00b7nen", "Str\u00e4u\u00b7chen", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Floss Abendk\u00fchlung herab,", "tokens": ["Floss", "A\u00b7bend\u00b7k\u00fch\u00b7lung", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als, hinter Garben von Weizen,", "tokens": ["Als", ",", "hin\u00b7ter", "Gar\u00b7ben", "von", "Wei\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein wahrer Engel an Reizen", "tokens": ["Ein", "wah\u00b7rer", "En\u00b7gel", "an", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dies Pfand der Liebe mir gab.", "tokens": ["Dies", "Pfand", "der", "Lie\u00b7be", "mir", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie sprach mit fr\u00f6lichem Mute:", "tokens": ["Sie", "sprach", "mit", "fr\u00f6\u00b7li\u00b7chem", "Mu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trag diese Blumen am Hute", "tokens": ["Trag", "die\u00b7se", "Blu\u00b7men", "am", "Hu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "APPRART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und dieses goldene Band!", "tokens": ["Und", "die\u00b7ses", "gol\u00b7de\u00b7ne", "Band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und gab die Blumen und Flittern,", "tokens": ["Und", "gab", "die", "Blu\u00b7men", "und", "Flit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "An meinem Hute zu zittern,", "tokens": ["An", "mei\u00b7nem", "Hu\u00b7te", "zu", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mir in die wartende Hand.", "tokens": ["Mir", "in", "die", "war\u00b7ten\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die Blumen hab' ich getragen,", "tokens": ["Die", "Blu\u00b7men", "hab'", "ich", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seit vierzehn gl\u00fccklichen Tagen,", "tokens": ["Seit", "vier\u00b7zehn", "gl\u00fcck\u00b7li\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und diese schwanden so schnell!", "tokens": ["Und", "die\u00b7se", "schwan\u00b7den", "so", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ihr B\u00e4nder, sah ich euch schweben,", "tokens": ["Ihr", "B\u00e4n\u00b7der", ",", "sah", "ich", "euch", "schwe\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Begann das Herz mir zu beben,", "tokens": ["Be\u00b7gann", "das", "Herz", "mir", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ward meine Seele so hell!", "tokens": ["Ward", "mei\u00b7ne", "See\u00b7le", "so", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ha! morgen bringen wir Leute,", "tokens": ["Ha", "!", "mor\u00b7gen", "brin\u00b7gen", "wir", "Leu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Geschm\u00fcckt wie Freier und Br\u00e4ute,", "tokens": ["Ge\u00b7schm\u00fcckt", "wie", "Frei\u00b7er", "und", "Br\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Ernte flitternden Kranz:", "tokens": ["Der", "Ern\u00b7te", "flit\u00b7tern\u00b7den", "Kranz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dann t\u00f6nen helle Schalmeien", "tokens": ["Dann", "t\u00f6\u00b7nen", "hel\u00b7le", "Schal\u00b7mei\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch unsre l\u00e4ndlichen Reihen,", "tokens": ["Durch", "uns\u00b7re", "l\u00e4nd\u00b7li\u00b7chen", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dann schwing' ich Liebchen im Tanz !", "tokens": ["Dann", "schwing'", "ich", "Lieb\u00b7chen", "im", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}