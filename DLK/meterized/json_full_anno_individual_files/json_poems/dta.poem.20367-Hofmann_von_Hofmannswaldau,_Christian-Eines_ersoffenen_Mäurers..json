{"dta.poem.20367": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Eines ersoffenen M\u00e4urers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchrte h\u00e4user auff/ bewahrte sarg und bein/", "tokens": ["Ich", "f\u00fchr\u00b7te", "h\u00e4u\u00b7ser", "auff", "/", "be\u00b7wahr\u00b7te", "sarg", "und", "bein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "$(", "VVFIN", "ADJD", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die st\u00e4dte schlo\u00df ich auch in feste mauern ein;", "tokens": ["Die", "st\u00e4d\u00b7te", "schlo\u00df", "ich", "auch", "in", "fes\u00b7te", "mau\u00b7ern", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kan ich meinen rest in keine mauer fassen/", "tokens": ["Doch", "kan", "ich", "mei\u00b7nen", "rest", "in", "kei\u00b7ne", "mau\u00b7er", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ich das rechte maa\u00df zum grunde hinterlassen.", "tokens": ["Weil", "ich", "das", "rech\u00b7te", "maa\u00df", "zum", "grun\u00b7de", "hin\u00b7ter\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}