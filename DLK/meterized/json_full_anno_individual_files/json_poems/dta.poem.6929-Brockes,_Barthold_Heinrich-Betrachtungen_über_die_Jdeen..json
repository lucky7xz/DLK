{"dta.poem.6929": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie aber! haben wir nicht klar genug gemacht", "tokens": ["Wie", "a\u00b7ber", "!", "ha\u00b7ben", "wir", "nicht", "klar", "ge\u00b7nug", "ge\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "VAFIN", "PPER", "PTKNEG", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den SEHOEPFFER, Dessen Wort dies ALL hervorge-", "tokens": ["Den", "Se\u00b7HOE\u00b7PF\u00b7FER", ",", "Des\u00b7sen", "Wort", "dies", "AlL", "her\u00b7vor\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PDS", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bracht?", "tokens": ["bracht", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und glaubt mit Recht ein menschlicher Verstand,", "tokens": ["Und", "glaubt", "mit", "Recht", "ein", "menschli\u00b7cher", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ob sey sein eigenes Gesetz Jhm unbekannt?", "tokens": ["Ob", "sey", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7setz", "Jhm", "un\u00b7be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Meister der Natur hat die Materie", "tokens": ["Der", "Meis\u00b7ter", "der", "Na\u00b7tur", "hat", "die", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Vom Anfang allbereit, nach Maa\u00df und nach Gewicht", "tokens": ["Vom", "An\u00b7fang", "all\u00b7be\u00b7reit", ",", "nach", "Maa\u00df", "und", "nach", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sehr weislich zugericht\u2019t.", "tokens": ["Sehr", "weis\u00b7lich", "zu\u00b7ge\u00b7richt'", "t."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "VVPP", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Es hat durch ein Gesetz, das allgemein,", "tokens": ["Es", "hat", "durch", "ein", "Ge\u00b7setz", ",", "das", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sein\u2019 Allmacht in der Welt", "tokens": ["Sein'", "All\u00b7macht", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Die W\u00fcrckungen, die gantz verschiedlich seyn,", "tokens": ["Die", "W\u00fcr\u00b7ckun\u00b7gen", ",", "die", "gantz", "ver\u00b7schied\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Doch in ein gleich Gewicht gestellt.", "tokens": ["Doch", "in", "ein", "gleich", "Ge\u00b7wicht", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er hat gemacht, da\u00df Wasser, Lufft und Erde,", "tokens": ["Er", "hat", "ge\u00b7macht", ",", "da\u00df", "Was\u00b7ser", ",", "Lufft", "und", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Durch ein unendliches Gemisch von Fruchtbarkeit", "tokens": ["Durch", "ein", "un\u00b7end\u00b7li\u00b7ches", "Ge\u00b7misch", "von", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Saamen angef\u00fcllet werde.", "tokens": ["Und", "Saa\u00b7men", "an\u00b7ge\u00b7f\u00fcl\u00b7let", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Es \u00e4ndert alles sich,", "tokens": ["Es", "\u00e4n\u00b7dert", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und kehret allezeit", "tokens": ["Und", "keh\u00b7ret", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Durch steten Wechsel wieder her,", "tokens": ["Durch", "ste\u00b7ten", "Wech\u00b7sel", "wie\u00b7der", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Sein ewigs Ur-Gesetz bleibt unver\u00e4nderlich.", "tokens": ["Sein", "e\u00b7wigs", "Ur\u00b7Ge\u00b7setz", "bleibt", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Des SCHOEPFFERS Herrlichkeit erscheinet destomehr", "tokens": ["Des", "ScHOE\u00b7PF\u00b7FERS", "Herr\u00b7lich\u00b7keit", "er\u00b7schei\u00b7net", "des\u00b7to\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "Jm Stoff, der einfach ist, und nur in dem Bewegen,", "tokens": ["Jm", "Stoff", ",", "der", "ein\u00b7fach", "ist", ",", "und", "nur", "in", "dem", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Durch dessen Aendrung blo\u00df in unsern Sinnen sich", "tokens": ["Durch", "des\u00b7sen", "A\u00b7en\u00b7drung", "blo\u00df", "in", "un\u00b7sern", "Sin\u00b7nen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "PRF"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Verschiedne Aendrungen erregen.", "tokens": ["Ver\u00b7schied\u00b7ne", "A\u00b7en\u00b7drun\u00b7gen", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}