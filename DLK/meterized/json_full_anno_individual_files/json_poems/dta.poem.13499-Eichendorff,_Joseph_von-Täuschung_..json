{"dta.poem.13499": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "T\u00e4uschung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ruhte aus vom Wandern,", "tokens": ["Ich", "ruh\u00b7te", "aus", "vom", "Wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond ging eben auf,", "tokens": ["Der", "Mond", "ging", "e\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sah ich fern im Lande", "tokens": ["Da", "sah", "ich", "fern", "im", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alten Tiber Lauf,", "tokens": ["Der", "al\u00b7ten", "Ti\u00b7ber", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Walde lagen Tr\u00fcmmer,", "tokens": ["Im", "Wal\u00b7de", "la\u00b7gen", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Pal\u00e4ste auf stillen H\u00f6h'n", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", "auf", "stil\u00b7len", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und G\u00e4rten im Mondesschimmer \u2014", "tokens": ["Und", "G\u00e4r\u00b7ten", "im", "Mon\u00b7des\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "O Welschland, wie bist du sch\u00f6n!", "tokens": ["O", "Wel\u00b7schland", ",", "wie", "bist", "du", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als die Nacht vergangen,", "tokens": ["Und", "als", "die", "Nacht", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde blitzte so weit,", "tokens": ["Die", "Er\u00b7de", "blitz\u00b7te", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Einen Hirten sah ich hangen", "tokens": ["Ei\u00b7nen", "Hir\u00b7ten", "sah", "ich", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Am Fels in der Einsamkeit.", "tokens": ["Am", "Fels", "in", "der", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Den fragt' ich ganz geblendet:", "tokens": ["Den", "fragt'", "ich", "ganz", "ge\u00b7blen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Komm' ich nach Rom noch heut?", "tokens": ["Komm'", "ich", "nach", "Rom", "noch", "heut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er dehnt' sich halbgewendet:", "tokens": ["Er", "dehnt'", "sich", "halb\u00b7ge\u00b7wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr seyd nicht recht gescheut!", "tokens": ["Ihr", "seyd", "nicht", "recht", "ge\u00b7scheut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Eine Winzerin lacht' her\u00fcber,", "tokens": ["Ei\u00b7ne", "Win\u00b7ze\u00b7rin", "lacht'", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Man sah sie vor Weinlaub kaum,", "tokens": ["Man", "sah", "sie", "vor", "Wein\u00b7laub", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Mir aber ging's Herze \u00fcber \u2014", "tokens": ["Mir", "a\u00b7ber", "ging's", "Her\u00b7ze", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVFIN", "APPR", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Es war ja alles nur Traum.", "tokens": ["Es", "war", "ja", "al\u00b7les", "nur", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ADV", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}