{"dta.poem.10297": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Es ebbt. Gemach dem Schlamm und Schlick umher               ", "tokens": ["Es", "ebbt", ".", "Ge\u00b7mach", "dem", "Schlamm", "und", "Schlick", "um\u00b7her"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ART", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entragen alte Wracks und Besenbaken,", "tokens": ["Ent\u00b7ra\u00b7gen", "al\u00b7te", "Wracks", "und", "Be\u00b7sen\u00b7ba\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und traurig h\u00fcllt ein graues Nebellaken", "tokens": ["Und", "trau\u00b7rig", "h\u00fcllt", "ein", "grau\u00b7es", "Ne\u00b7bel\u00b7la\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Hallig ein, die Watten und das Meer.", "tokens": ["Die", "Hal\u00b7lig", "ein", ",", "die", "Wat\u00b7ten", "und", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKVZ", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Himmel schweigt, die Welt ist freudenleer.", "tokens": ["Der", "Him\u00b7mel", "schweigt", ",", "die", "Welt", "ist", "freu\u00b7den\u00b7leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nachrichten, Teufel, die mich oft erschraken,", "tokens": ["Nach\u00b7rich\u00b7ten", ",", "Teu\u00b7fel", ",", "die", "mich", "oft", "er\u00b7schra\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sind Engel gegen solchen Widerhaken,", "tokens": ["Sind", "En\u00b7gel", "ge\u00b7gen", "sol\u00b7chen", "Wi\u00b7der\u00b7ha\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den heut im Herzen w\u00fchlt ein rauher Speer.", "tokens": ["Den", "heut", "im", "Her\u00b7zen", "w\u00fchlt", "ein", "rau\u00b7her", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sonderbar! Ich wollte schon verzagen", "tokens": ["Wie", "son\u00b7der\u00b7bar", "!", "Ich", "woll\u00b7te", "schon", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mich ergeben, ohne Mannesw\u00fcrde,", "tokens": ["Und", "mich", "er\u00b7ge\u00b7ben", ",", "oh\u00b7ne", "Man\u00b7nes\u00b7w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da blitzt ein Bild hervor aus fernen Tagen:", "tokens": ["Da", "blitzt", "ein", "Bild", "her\u00b7vor", "aus", "fer\u00b7nen", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf meiner Stute \u00fcber Heck und H\u00fcrde", "tokens": ["Auf", "mei\u00b7ner", "Stu\u00b7te", "\u00fc\u00b7ber", "Heck", "und", "H\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weit der Schwadron voran seh\u2019 ich mich jagen", "tokens": ["Weit", "der", "Schwad\u00b7ron", "vo\u00b7ran", "seh'", "ich", "mich", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKVZ", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "In Schlacht und Sieg, entlastet aller B\u00fcrde.", "tokens": ["In", "Schlacht", "und", "Sieg", ",", "ent\u00b7las\u00b7tet", "al\u00b7ler", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}