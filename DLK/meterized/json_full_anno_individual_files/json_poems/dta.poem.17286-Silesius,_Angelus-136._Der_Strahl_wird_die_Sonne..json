{"dta.poem.17286": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "136.  Der Strahl wird die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist/ kombt er in GOtt/ wird selbst die ewge", "tokens": ["Mein", "Geist", "/", "kombt", "er", "in", "Gott", "/", "wird", "selbst", "die", "ew\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "APPR", "NN", "$(", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wonne:", "tokens": ["Won\u00b7ne", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Gleich wie der Strahl nichts ist als Sonn\u2019 in seiner", "tokens": ["Gleich", "wie", "der", "Strahl", "nichts", "ist", "als", "Sonn'", "in", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "PIS", "VAFIN", "KOKOM", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonne.", "tokens": ["Son\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}