{"dta.poem.292": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der h\u00f6ffliche Liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze/ und das Auge spricht", "tokens": ["Mein", "Hert\u00b7ze", "/", "und", "das", "Au\u00b7ge", "spricht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "$(", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob schon die Lippen schweigen/", "tokens": ["Ob", "schon", "die", "Lip\u00b7pen", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nennt gleich der Mund mein Lieben nicht/", "tokens": ["Nennt", "gleich", "der", "Mund", "mein", "Lie\u00b7ben", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "PTKNEG", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Dennochs die Seuffzers zeigen.", "tokens": ["Den\u00b7nochs", "die", "Seuff\u00b7zers", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und deckt mein Blut", "tokens": ["Und", "deckt", "mein", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Gleich seine Gluth", "tokens": ["Gleich", "sei\u00b7ne", "Gluth"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.7": {"text": "Die Funcken doch aus meinen Adern steigen.", "tokens": ["Die", "Fun\u00b7cken", "doch", "aus", "mei\u00b7nen", "A\u00b7dern", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Wesen l\u00e4st mein Feuer sehn/", "tokens": ["Mein", "We\u00b7sen", "l\u00e4st", "mein", "Feu\u00b7er", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob ich es schon verh\u00e4hle;", "tokens": ["Ob", "ich", "es", "schon", "ver\u00b7h\u00e4h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4st gleich mein Mund kein Klagen gehn/", "tokens": ["L\u00e4st", "gleich", "mein", "Mund", "kein", "Kla\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So thut es doch die Seele/", "tokens": ["So", "thut", "es", "doch", "die", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zeigt die Brust", "tokens": ["Und", "zeigt", "die", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Gleich aussen Lust/", "tokens": ["Gleich", "aus\u00b7sen", "Lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Doch ich mich sehr durch innre Flammen qu\u00e4hle.", "tokens": ["Doch", "ich", "mich", "sehr", "durch", "inn\u00b7re", "Flam\u00b7men", "qu\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So lieb\u2019 ich nun/ doch scheut der Mund", "tokens": ["So", "lieb'", "ich", "nun", "/", "doch", "scheut", "der", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Liebes-Feur zu nennen/", "tokens": ["Sein", "Lie\u00b7bes\u00b7Feur", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Hertze aber macht es kund/", "tokens": ["Das", "Hert\u00b7ze", "a\u00b7ber", "macht", "es", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeiget an mein Brennen/", "tokens": ["Und", "zei\u00b7get", "an", "mein", "Bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch wei\u00df ich nicht?", "tokens": ["Doch", "wei\u00df", "ich", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wenns Hertze spricht", "tokens": ["Wenns", "Hert\u00b7ze", "spricht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ob meine Gluth mein Engel wird erkennen.", "tokens": ["Ob", "mei\u00b7ne", "Gluth", "mein", "En\u00b7gel", "wird", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}