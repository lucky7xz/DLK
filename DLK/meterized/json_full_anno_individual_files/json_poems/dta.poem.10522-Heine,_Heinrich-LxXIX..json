{"dta.poem.10522": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.85", "af:0.14"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Doch die Kastraten klagten", "tokens": ["Doch", "die", "Kas\u00b7tra\u00b7ten", "klag\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--++-+-", "measure": "anapaest.init"}, "line.2": {"text": "Als ich meine Stimm' erhob;", "tokens": ["Als", "ich", "mei\u00b7ne", "Stimm'", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie klagten und sie sagten:", "tokens": ["Sie", "klag\u00b7ten", "und", "sie", "sag\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich s\u00e4nge viel zu grob.", "tokens": ["Ich", "s\u00e4n\u00b7ge", "viel", "zu", "grob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und lieblich erhoben sie alle", "tokens": ["Und", "lieb\u00b7lich", "er\u00b7ho\u00b7ben", "sie", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIAT"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die kleinen Stimmelein,", "tokens": ["Die", "klei\u00b7nen", "Stim\u00b7mel\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Trillerchen, wie Kristalle,", "tokens": ["Die", "Tril\u00b7ler\u00b7chen", ",", "wie", "Kris\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+--+++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie klangen so fein und rein.", "tokens": ["Sie", "klan\u00b7gen", "so", "fein", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie sangen von Liebessehnen,", "tokens": ["Sie", "san\u00b7gen", "von", "Lie\u00b7bes\u00b7seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von Lieb' und Liebesergu\u00df;", "tokens": ["Von", "Lieb'", "und", "Lie\u00b7be\u00b7ser\u00b7gu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Damen schwammen in Thr\u00e4nen,", "tokens": ["Die", "Da\u00b7men", "schwam\u00b7men", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bei solchem Kunstgenu\u00df.", "tokens": ["Bei", "sol\u00b7chem", "Kunst\u00b7ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}