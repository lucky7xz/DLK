{"textgrid.poem.66006": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du da und du \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du da und du \u2013", "tokens": ["Du", "da", "und", "du", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPER", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ihr d\u00fcnktet euch immer mehr als ich.", "tokens": ["Ihr", "d\u00fcnk\u00b7tet", "euch", "im\u00b7mer", "mehr", "als", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "In deinem strammschenkligen Kraftprotzentum,", "tokens": ["In", "dei\u00b7nem", "stramm\u00b7schen\u00b7kli\u00b7gen", "Kraft\u00b7prot\u00b7zen\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Ein sogenannter \u00bbfamoser Kerl\u00ab", "tokens": ["Ein", "so\u00b7ge\u00b7nann\u00b7ter", "\u00bb", "fa\u00b7mo\u00b7ser", "Kerl", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADJA", "NN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Bei Weibern und Pferden.", "tokens": ["Bei", "Wei\u00b7bern", "und", "Pfer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und du,", "tokens": ["Und", "du", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Hundertmal plumper,", "tokens": ["Hun\u00b7dert\u00b7mal", "plum\u00b7per", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ver\u00e4chtlicher,", "tokens": ["Ver\u00b7\u00e4cht\u00b7li\u00b7cher", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.10": {"text": "Geldprotz du!", "tokens": ["Geld\u00b7protz", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Wenn ihr jene feinen Ohren h\u00e4ttet,", "tokens": ["Wenn", "ihr", "je\u00b7ne", "fei\u00b7nen", "Oh\u00b7ren", "h\u00e4t\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Mit welchen wir Dichter alles belauschen,", "tokens": ["Mit", "wel\u00b7chen", "wir", "Dich\u00b7ter", "al\u00b7les", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "NN", "PIS", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Welch silberstimmiges Lachen w\u00fcrdet ihr h\u00f6ren,", "tokens": ["Welch", "sil\u00b7ber\u00b7stim\u00b7mi\u00b7ges", "La\u00b7chen", "w\u00fcr\u00b7det", "ihr", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Ein Lachen so leicht, fr\u00f6hlich, obenhin,", "tokens": ["Ein", "La\u00b7chen", "so", "leicht", ",", "fr\u00f6h\u00b7lich", ",", "o\u00b7ben\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Als Antwort auf eure dreisten", "tokens": ["Als", "Ant\u00b7wort", "auf", "eu\u00b7re", "dreis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Ellbogenfragen:", "tokens": ["Ell\u00b7bo\u00b7gen\u00b7fra\u00b7gen", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "\u00bbsiehst du, was f\u00fcr Kerle wir sind?\u00ab", "tokens": ["\u00bb", "siehst", "du", ",", "was", "f\u00fcr", "Ker\u00b7le", "wir", "sind", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Ich sehe es!", "tokens": ["Ich", "se\u00b7he", "es", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Aber jener da,", "tokens": ["A\u00b7ber", "je\u00b7ner", "da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der mit dem \u00fcberlegenen L\u00e4cheln,", "tokens": ["Der", "mit", "dem", "\u00fc\u00b7berl\u00b7e\u00b7ge\u00b7nen", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Schulmeister,", "tokens": ["Der", "Schul\u00b7meis\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Er thut mir leid.", "tokens": ["Er", "thut", "mir", "leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Was ist euch Kunst, Wissenschaft,", "tokens": ["Was", "ist", "euch", "Kunst", ",", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und jenes unw\u00e4gbare Geschenk der G\u00f6tter:", "tokens": ["Und", "je\u00b7nes", "un\u00b7w\u00e4g\u00b7ba\u00b7re", "Ge\u00b7schenk", "der", "G\u00f6t\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "\u00bbgeist!\u00ab", "tokens": ["\u00bb", "geist", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Ein Nichts!", "tokens": ["Ein", "Nichts", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "PIS", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "(doch! Geist liebt ihr:", "tokens": ["(", "doch", "!", "Geist", "liebt", "ihr", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Klapphornverse!)", "tokens": ["Klapp\u00b7horn\u00b7ver\u00b7se", "!", ")"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Aber ihn,", "tokens": ["A\u00b7ber", "ihn", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Ihn narrten die G\u00f6tter.", "tokens": ["Ihn", "narr\u00b7ten", "die", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Sie gaben ihm Flei\u00df, Verstand, Ged\u00e4chtnis,", "tokens": ["Sie", "ga\u00b7ben", "ihm", "Flei\u00df", ",", "Ver\u00b7stand", ",", "Ge\u00b7d\u00e4cht\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Liebe zum Guten,", "tokens": ["Lie\u00b7be", "zum", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Und einen feinf\u00fchlenden Finger.", "tokens": ["Und", "ei\u00b7nen", "fein\u00b7f\u00fch\u00b7len\u00b7den", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Aber sie schlugen ihn mit Kurzsichtigkeit", "tokens": ["A\u00b7ber", "sie", "schlu\u00b7gen", "ihn", "mit", "Kurz\u00b7sich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Und gaben ihm nicht", "tokens": ["Und", "ga\u00b7ben", "ihm", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Ihr H\u00f6chstes:", "tokens": ["Ihr", "H\u00f6chs\u00b7tes", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Phantasie.", "tokens": ["Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Nun tappt er umher", "tokens": ["Nun", "tappt", "er", "um\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und freut sich,", "tokens": ["Und", "freut", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wessen er habhaft wird mit tastendem Finger.", "tokens": ["Wes\u00b7sen", "er", "hab\u00b7haft", "wird", "mit", "tas\u00b7ten\u00b7dem", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Aber drau\u00dfen,", "tokens": ["A\u00b7ber", "drau\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wo Schwingen sich weiten,", "tokens": ["Wo", "Schwin\u00b7gen", "sich", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auftragenden Fluges", "tokens": ["Auf\u00b7tra\u00b7gen\u00b7den", "Flu\u00b7ges"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Phantasiebegabte, leichtere Geister", "tokens": ["Phan\u00b7ta\u00b7sie\u00b7be\u00b7gab\u00b7te", ",", "leich\u00b7te\u00b7re", "Geis\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Mit Sonnenkindern Frage und Antwort spielen:", "tokens": ["Mit", "Son\u00b7nen\u00b7kin\u00b7dern", "Fra\u00b7ge", "und", "Ant\u00b7wort", "spie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Hier ist er nicht heimisch.", "tokens": ["Hier", "ist", "er", "nicht", "hei\u00b7misch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Hier f\u00fchlt er seinen Mangel", "tokens": ["Hier", "f\u00fchlt", "er", "sei\u00b7nen", "Man\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und r\u00fcmpft die Nase,", "tokens": ["Und", "r\u00fcmpft", "die", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.12": {"text": "Wie h\u00e4ssliche M\u00e4dchen", "tokens": ["Wie", "h\u00e4ss\u00b7li\u00b7che", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Unter sch\u00f6neren Schwestern", "tokens": ["Un\u00b7ter", "sch\u00f6\u00b7ne\u00b7ren", "Schwes\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Sich gern auf die \u00dcberlegenen hinausspielen,", "tokens": ["Sich", "gern", "auf", "die", "\u00dc\u00b7berl\u00b7e\u00b7ge\u00b7nen", "hin\u00b7aus\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die Gesetzteren,", "tokens": ["Die", "Ge\u00b7setz\u00b7te\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Innerlicheren.", "tokens": ["In\u00b7ner\u00b7li\u00b7che\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Der Arme!", "tokens": ["Der", "Ar\u00b7me", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ihn narrten die G\u00f6tter,", "tokens": ["Ihn", "narr\u00b7ten", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Mitleid weckt mir", "tokens": ["Und", "Mit\u00b7leid", "weckt", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sein \u00fcberlegenes L\u00e4cheln,", "tokens": ["Sein", "\u00fc\u00b7berl\u00b7e\u00b7ge\u00b7nes", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Tiefes Mitleid.", "tokens": ["Tie\u00b7fes", "Mit\u00b7leid", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1"