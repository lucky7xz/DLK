{"dta.poem.2351": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Was schallt aus diesem Wald'? Ich horche/ da\u00df ich h\u00f6re?", "tokens": ["Was", "schallt", "aus", "die\u00b7sem", "Wald'", "?", "Ich", "hor\u00b7che", "/", "da\u00df", "ich", "h\u00f6\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "$.", "PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "E. Ehre.", "tokens": ["E.", "Eh\u00b7re", "."], "token_info": ["abbreviation", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Was mehr? ich mag sehr gern von neuen Dingen wissen?", "tokens": ["Was", "mehr", "?", "ich", "mag", "sehr", "gern", "von", "neu\u00b7en", "Din\u00b7gen", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "PPER", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "E. Wissen.", "tokens": ["E.", "Wis\u00b7sen", "."], "token_info": ["abbreviation", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Mich d\u00fcnkt/ ich h\u00f6r\u2019 hierbei ein reingemeintes K\u00fcssen/", "tokens": ["Mich", "d\u00fcnkt", "/", "ich", "h\u00f6r'", "hier\u00b7bei", "ein", "rein\u00b7ge\u00b7mein\u00b7tes", "K\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "E. K\u00fcssen.", "tokens": ["E.", "K\u00fcs\u00b7sen", "."], "token_info": ["abbreviation", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Wie? schallet es nicht auch von einer feinen Lehre.", "tokens": ["Wie", "?", "schal\u00b7let", "es", "nicht", "auch", "von", "ei\u00b7ner", "fei\u00b7nen", "Leh\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "E. Lehre.", "tokens": ["E.", "Leh\u00b7re", "."], "token_info": ["abbreviation", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "So handelt dieser Wald/ und schallet wie ich h\u00f6re/", "tokens": ["So", "han\u00b7delt", "die\u00b7ser", "Wald", "/", "und", "schal\u00b7let", "wie", "ich", "h\u00f6\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$(", "KON", "VVFIN", "KOKOM", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Und lehrt der Sitten Zier. Orpheus war der Krafft", "tokens": ["Und", "lehrt", "der", "Sit\u00b7ten", "Zier", ".", "Or\u00b7pheus", "war", "der", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$.", "NE", "VAFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Den Wald an sich zu ziehn. Seht/ wie es sich verkehre/", "tokens": ["Den", "Wald", "an", "sich", "zu", "ziehn", ".", "Seht", "/", "wie", "es", "sich", "ver\u00b7keh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$.", "VVFIN", "$(", "PWAV", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nu ziehet ihn der Wald/ den unser Neumark pflantzet/", "tokens": ["Nu", "zie\u00b7het", "ihn", "der", "Wald", "/", "den", "un\u00b7ser", "Neu\u00b7mark", "pflant\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wo selbst der Delius mit seinen Musen dantzet.", "tokens": ["Wo", "selbst", "der", "De\u00b7lius", "mit", "sei\u00b7nen", "Mu\u00b7sen", "dant\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sag\u2019 es ins Gesicht\u2019 und sag\u2019 es hinderwerts/", "tokens": ["Ich", "sag'", "es", "ins", "Ge\u00b7sicht'", "und", "sag'", "es", "hin\u00b7der\u00b7werts", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr Neumark schreibet rein/ und hat ein reines Hertz.", "tokens": ["Herr", "Neu\u00b7mark", "schrei\u00b7bet", "rein", "/", "und", "hat", "ein", "rei\u00b7nes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADJD", "$(", "KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel als ich das Deutsch? und reines Hertz mag kennen", "tokens": ["So", "viel", "als", "ich", "das", "Deutsch", "?", "und", "rei\u00b7nes", "Hertz", "mag", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ART", "NN", "$.", "KON", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan ich ihn wol Rein von Sprach und Hertzen nennen.", "tokens": ["So", "kan", "ich", "ihn", "wol", "Rein", "von", "Sprach", "und", "Hert\u00b7zen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}