{"dta.poem.8522": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen verehrten Freund, den Freiherrn von  \n Madroux, bei Uebersendung der \u201eGedichte.\u201c", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als diese Lieder ich vereint", "tokens": ["Als", "die\u00b7se", "Lie\u00b7der", "ich", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Kranz in ferner Heimath paarte,", "tokens": ["Zum", "Kranz", "in", "fer\u00b7ner", "Hei\u00b7math", "paar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da kannt\u2019 ich freilich nicht den Freund,", "tokens": ["Da", "kannt'", "ich", "frei\u00b7lich", "nicht", "den", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den mir die Zukunft aufbewahrte;", "tokens": ["Den", "mir", "die", "Zu\u00b7kunft", "auf\u00b7be\u00b7wahr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wu\u00dft\u2019 es nicht, da\u00df manches Wort,", "tokens": ["Ich", "wu\u00dft'", "es", "nicht", ",", "da\u00df", "man\u00b7ches", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ich aus tiefer Brust gesungen,", "tokens": ["Das", "ich", "aus", "tie\u00b7fer", "Brust", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Fand in der seinen den Akkord,", "tokens": ["Fand", "in", "der", "sei\u00b7nen", "den", "Ak\u00b7kord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der es harmonisch nachgeklungen.", "tokens": ["Der", "es", "har\u00b7mo\u00b7nisch", "nach\u00b7ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nun in ernster Gegenwart,", "tokens": ["Doch", "nun", "in", "erns\u00b7ter", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In freundlicher, doch fremder Zone", "tokens": ["In", "freund\u00b7li\u00b7cher", ",", "doch", "frem\u00b7der", "Zo\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir seines Beifalls Freude ward", "tokens": ["Mir", "sei\u00b7nes", "Bei\u00b7falls", "Freu\u00b7de", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seiner Freundschaft Ehrenkrone;", "tokens": ["Und", "sei\u00b7ner", "Freund\u00b7schaft", "Eh\u00b7ren\u00b7kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun reich\u2019 ich gern die Lieder dar,", "tokens": ["Nun", "reich'", "ich", "gern", "die", "Lie\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Fl\u00fccht\u2019ges drin, das sei vernichtet,", "tokens": ["Was", "Fl\u00fccht'\u00b7ges", "drin", ",", "das", "sei", "ver\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was ritterlich, was gut und wahr,", "tokens": ["Was", "rit\u00b7ter\u00b7lich", ",", "was", "gut", "und", "wahr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das sei, als hab\u2019 ich\u2019s dir gedichtet.", "tokens": ["Das", "sei", ",", "als", "hab'", "ich's", "dir", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOKOM", "VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}