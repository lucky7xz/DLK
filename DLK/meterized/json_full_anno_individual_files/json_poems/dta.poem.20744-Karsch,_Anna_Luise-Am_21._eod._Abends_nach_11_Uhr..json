{"dta.poem.20744": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Am 21.  eod.  Abends nach 11 Uhr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin Empfindung und Gesang,                 ", "tokens": ["Ich", "bin", "Emp\u00b7fin\u00b7dung", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesungen hab ich euch, mir ward der Tag nicht lang.", "tokens": ["Ge\u00b7sun\u00b7gen", "hab", "ich", "euch", ",", "mir", "ward", "der", "Tag", "nicht", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr folgt der M\u00fcdigkeit, gefangen nimmt der Schlaf", "tokens": ["Ihr", "folgt", "der", "M\u00fc\u00b7dig\u00b7keit", ",", "ge\u00b7fan\u00b7gen", "nimmt", "der", "Schlaf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit s\u00fc\u00dfen Fesseln unsern Graf.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Fes\u00b7seln", "un\u00b7sern", "Graf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schlummert alle sanft, und du, den ich ge dacht,", "tokens": ["Nun", "schlum\u00b7mert", "al\u00b7le", "sanft", ",", "und", "du", ",", "den", "ich", "ge", "dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "$,", "KON", "PPER", "$,", "PRELS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was ich auch immer schrieb, mein Thyrsis, gute Nacht.", "tokens": ["Was", "ich", "auch", "im\u00b7mer", "schrieb", ",", "mein", "Thyr\u00b7sis", ",", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}