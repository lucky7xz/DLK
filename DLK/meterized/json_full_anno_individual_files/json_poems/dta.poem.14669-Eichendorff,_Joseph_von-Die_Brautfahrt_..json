{"dta.poem.14669": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber'm Rauschen dunkler Wogen", "tokens": ["Ue\u00b7ber'm", "Rau\u00b7schen", "dunk\u00b7ler", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der wilden Einsamkeit,", "tokens": ["In", "der", "wil\u00b7den", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hausend auf dem Felsenbogen,", "tokens": ["Hau\u00b7send", "auf", "dem", "Fel\u00b7sen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ringt er fromm mit seinem Leid,", "tokens": ["Ringt", "er", "fromm", "mit", "sei\u00b7nem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat, da manches Jahr entschwunden,", "tokens": ["Hat", ",", "da", "man\u00b7ches", "Jahr", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimath, Braut und Ruh' gefunden. \u2014", "tokens": ["Hei\u00b7math", ",", "Braut", "und", "Ruh'", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}