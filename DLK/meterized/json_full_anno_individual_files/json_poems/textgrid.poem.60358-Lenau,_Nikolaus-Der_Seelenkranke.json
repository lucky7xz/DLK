{"textgrid.poem.60358": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Seelenkranke", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich trag im Herzen eine tiefe Wunde", "tokens": ["Ich", "trag", "im", "Her\u00b7zen", "ei\u00b7ne", "tie\u00b7fe", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und will sie stumm bis an mein Ende tragen;", "tokens": ["Und", "will", "sie", "stumm", "bis", "an", "mein", "En\u00b7de", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchl ihr rastlos immer tiefres Nagen,", "tokens": ["Ich", "f\u00fchl", "ihr", "rast\u00b7los", "im\u00b7mer", "tief\u00b7res", "Na\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie das Leben bricht von Stund zu Stunde.", "tokens": ["Und", "wie", "das", "Le\u00b7ben", "bricht", "von", "Stund", "zu", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur eine wei\u00df ich, der ich meine Kunde", "tokens": ["Nur", "ei\u00b7ne", "wei\u00df", "ich", ",", "der", "ich", "mei\u00b7ne", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vertrauen m\u00f6chte und ihr alles sagen;", "tokens": ["Ver\u00b7trau\u00b7en", "m\u00f6ch\u00b7te", "und", "ihr", "al\u00b7les", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "K\u00f6nnt ich an ihrem Halse schluchzen, klagen!", "tokens": ["K\u00f6nnt", "ich", "an", "ih\u00b7rem", "Hal\u00b7se", "schluch\u00b7zen", ",", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eine aber liegt verscharrt im Grunde.", "tokens": ["Die", "ei\u00b7ne", "a\u00b7ber", "liegt", "ver\u00b7scharrt", "im", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "VVFIN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Mutter, komm, la\u00df dich mein Flehn bewegen!", "tokens": ["O", "Mut\u00b7ter", ",", "komm", ",", "la\u00df", "dich", "mein", "Flehn", "be\u00b7we\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn deine Liebe noch im Tode wacht,", "tokens": ["Wenn", "dei\u00b7ne", "Lie\u00b7be", "noch", "im", "To\u00b7de", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn du darfst, wie einst, dein Kind noch pflegen,", "tokens": ["Und", "wenn", "du", "darfst", ",", "wie", "einst", ",", "dein", "Kind", "noch", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PAV", "$,", "PWAV", "ADV", "$,", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So la\u00df mich bald aus diesem Leben scheiden.", "tokens": ["So", "la\u00df", "mich", "bald", "aus", "die\u00b7sem", "Le\u00b7ben", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sehne mich nach einer stillen Nacht,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "ei\u00b7ner", "stil\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O hilf dem Schmerz, dein m\u00fcdes Kind entkleiden.", "tokens": ["O", "hilf", "dem", "Schmerz", ",", "dein", "m\u00fc\u00b7des", "Kind", "ent\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich trag im Herzen eine tiefe Wunde", "tokens": ["Ich", "trag", "im", "Her\u00b7zen", "ei\u00b7ne", "tie\u00b7fe", "Wun\u00b7de"], "token_info": ["wo