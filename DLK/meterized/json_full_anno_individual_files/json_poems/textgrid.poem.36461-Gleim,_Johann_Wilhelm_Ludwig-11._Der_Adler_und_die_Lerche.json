{"textgrid.poem.36461": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "11. Der Adler und die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adler traf, auf seiner Bahn", "tokens": ["Ein", "Ad\u00b7ler", "traf", ",", "auf", "sei\u00b7ner", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Sonn', einst eine Lerche an,", "tokens": ["Zur", "Sonn'", ",", "einst", "ei\u00b7ne", "Ler\u00b7che", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00f6rte sie", "tokens": ["Und", "h\u00f6r\u00b7te", "sie"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die sch\u00f6nste Melodie", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem stillen Himmel singen.", "tokens": ["Dem", "stil\u00b7len", "Him\u00b7mel", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die ausgebreiteten und eilgewohnten Schwingen", "tokens": ["Die", "aus\u00b7ge\u00b7brei\u00b7te\u00b7ten", "und", "eil\u00b7ge\u00b7wohn\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "m