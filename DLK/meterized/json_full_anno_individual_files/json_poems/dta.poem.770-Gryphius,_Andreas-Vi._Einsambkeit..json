{"dta.poem.770": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  Einsambkeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Jn dieser Einsamkeit/ der mehr denn \u00f6den w\u00fcsten/ ", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "/", "der", "mehr", "denn", "\u00f6\u00b7den", "w\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$(", "ART", "ADV", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestreckt auff wildes Kraut/ an die bem\u00f6\u00dfte See:", "tokens": ["Ge\u00b7streckt", "auff", "wil\u00b7des", "Kraut", "/", "an", "die", "be\u00b7m\u00f6\u00df\u00b7te", "See", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beschaw\u2019ich jenes Thal vnd dieser Felsen h\u00f6h\u2019", "tokens": ["Be\u00b7scha\u00b7w'\u00b7ich", "je\u00b7nes", "Thal", "vnd", "die\u00b7ser", "Fel\u00b7sen", "h\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PDAT", "NN", "KON", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Auff welchem Eulen nur vnd stille V\u00f6gel nisten.", "tokens": ["Auff", "wel\u00b7chem", "Eu\u00b7len", "nur", "vnd", "stil\u00b7le", "V\u00f6\u00b7gel", "nis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier/ fern von dem Pallast; weit von de\u00df P\u00f6vels l\u00fcsten/", "tokens": ["Hier", "/", "fern", "von", "dem", "Pal\u00b7last", ";", "weit", "von", "de\u00df", "P\u00f6\u00b7vels", "l\u00fcs\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "APPR", "ART", "NN", "$.", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Betracht ich: wie der Mensch in Eitelkeit vergeh\u2019", "tokens": ["Be\u00b7tracht", "ich", ":", "wie", "der", "Mensch", "in", "Ei\u00b7tel\u00b7keit", "ver\u00b7geh'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie auff nicht festem grund\u2019 all vnser hoffen steh\u2019", "tokens": ["Wie", "auff", "nicht", "fes\u00b7tem", "grund'", "all", "vn\u00b7ser", "hof\u00b7fen", "steh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PTKNEG", "ADJA", "NN", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie die vor abend schm\u00e4hn/ die vor dem tag vn\u00df gr\u00fc\u00dften.", "tokens": ["Wie", "die", "vor", "a\u00b7bend", "schm\u00e4hn", "/", "die", "vor", "dem", "tag", "vn\u00df", "gr\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "ADJD", "VVINF", "$(", "ART", "APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die H\u00f6ell/ der rawe wald/ der Todtenkopff/ der Stein/", "tokens": ["Die", "H\u00f6ell", "/", "der", "ra\u00b7we", "wald", "/", "der", "Tod\u00b7ten\u00b7kopff", "/", "der", "Stein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den auch die zeit auff frist/ die abgezehrten bein.", "tokens": ["Den", "auch", "die", "zeit", "auff", "frist", "/", "die", "ab\u00b7ge\u00b7zehr\u00b7ten", "bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NN", "$(", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entwerffen in dem Mut vnzehliche gedancken.", "tokens": ["Ent\u00b7werf\u00b7fen", "in", "dem", "Mut", "vn\u00b7zeh\u00b7li\u00b7che", "ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Mauren alter grau\u00df/ di\u00df vngebaw\u2019te Land", "tokens": ["Der", "Mau\u00b7ren", "al\u00b7ter", "grau\u00df", "/", "di\u00df", "vn\u00b7ge\u00b7ba\u00b7w'\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ist sch\u00f6n vnd fruchtbar mir/ der eigentlich erkant/", "tokens": ["Ist", "sch\u00f6n", "vnd", "frucht\u00b7bar", "mir", "/", "der", "ei\u00b7gent\u00b7lich", "er\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "PPER", "$(", "ART", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das alles/ ohn ein Geist/ den GOt selst h\u00e4lt/ mu\u00df wancken. ", "tokens": ["Das", "al\u00b7les", "/", "ohn", "ein", "Geist", "/", "den", "Got", "selst", "h\u00e4lt", "/", "mu\u00df", "wan\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "APPR", "ART", "NN", "$(", "ART", "NN", "ADV", "VVFIN", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}