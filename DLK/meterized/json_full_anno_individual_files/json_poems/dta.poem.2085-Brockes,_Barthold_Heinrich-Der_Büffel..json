{"dta.poem.2085": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der B\u00fcffel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der auch ist ein' Art von Ochsen, doch in vielem unter-", "tokens": ["Der", "auch", "ist", "ein'", "Art", "von", "Och\u00b7sen", ",", "doch", "in", "vie\u00b7lem", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "APPR", "PIS", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "schieden,", "tokens": ["schie\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So an Art als an Gestalt. Es ist seine Haut so fest", "tokens": ["So", "an", "Art", "als", "an", "Ge\u00b7stalt", ".", "Es", "ist", "sei\u00b7ne", "Haut", "so", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und so dicke, da\u00df sie sich kaum durch Kugeln trennen", "tokens": ["Und", "so", "di\u00b7cke", ",", "da\u00df", "sie", "sich", "kaum", "durch", "Ku\u00b7geln", "tren\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "l\u00e4\u00dft.", "tokens": ["l\u00e4\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "T\u00fcchtig ist die\u00df Thier zur Arbeit und nicht leichtlich", "tokens": ["T\u00fcch\u00b7tig", "ist", "die\u00df", "Thier", "zur", "Ar\u00b7beit", "und", "nicht", "leicht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PDS", "NN", "APPRART", "NN", "KON", "PTKNEG", "ADJD"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "zu erm\u00fcden.", "tokens": ["zu", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Seine ringelreiche H\u00f6rner sind stets hinterwerts gekr\u00fcmmt,", "tokens": ["Sei\u00b7ne", "rin\u00b7gel\u00b7rei\u00b7che", "H\u00f6r\u00b7ner", "sind", "stets", "hin\u00b7ter\u00b7werts", "ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Die sind als ein h\u00fclfreich Mittel gegen allen Krampf be-", "tokens": ["Die", "sind", "als", "ein", "h\u00fclf\u00b7reich", "Mit\u00b7tel", "ge\u00b7gen", "al\u00b7len", "Krampf", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "KOKOM", "ART", "ADJD", "NN", "APPR", "PIAT", "NN", "TRUNC"], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "stimmt,", "tokens": ["stimmt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Auch zu mancherley Gef\u00e4\u00dfen, Trinkgeschirren und zu Bo-", "tokens": ["Auch", "zu", "man\u00b7cher\u00b7ley", "Ge\u00b7f\u00e4\u00b7\u00dfen", ",", "Trink\u00b7ge\u00b7schir\u00b7ren", "und", "zu", "Bo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,", "NN", "KON", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Es zu z\u00e4hmen, wird ihm stets durch die Nas\u2019 ein Ring", "tokens": ["Es", "zu", "z\u00e4h\u00b7men", ",", "wird", "ihm", "stets", "durch", "die", "Nas'", "ein", "Ring"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "gezogen.", "tokens": ["ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Hieb- und schu\u00dffrey sind die K\u00f6ller, womit der Soldat", "tokens": ["Hie\u00b7b", "und", "schu\u00df\u00b7frey", "sind", "die", "K\u00f6l\u00b7ler", ",", "wo\u00b7mit", "der", "Sol\u00b7dat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "sich deckt", "tokens": ["sich", "deckt"], "token_info": ["word", "word"], "pos": ["PRF", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Aus der B\u00fcffel dicken Haut. Da ihr Fleisch auch nied-", "tokens": ["Aus", "der", "B\u00fcf\u00b7fel", "di\u00b7cken", "Haut", ".", "Da", "ihr", "Fleisch", "auch", "nie\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "KOUS", "PPOSAT", "NN", "ADV", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "lich schmeckt", "tokens": ["lich", "schmeckt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Und besonders nahrsam ist, wird es ebenfalls gegessen.", "tokens": ["Und", "be\u00b7son\u00b7ders", "nahr\u00b7sam", "ist", ",", "wird", "es", "e\u00b7ben\u00b7falls", "ge\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Soll man denn nicht dessen Huld, auch bey diesem Thier,", "tokens": ["Soll", "man", "denn", "nicht", "des\u00b7sen", "Huld", ",", "auch", "bey", "die\u00b7sem", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PTKNEG", "PRELAT", "NN", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.21": {"text": "ermessen,", "tokens": ["er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Welcher es, f\u00fcr uns, formirt? uns nicht durch sein Fleisch", "tokens": ["Wel\u00b7cher", "es", ",", "f\u00fcr", "uns", ",", "for\u00b7mirt", "?", "uns", "nicht", "durch", "sein", "Fleisch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "$,", "APPR", "PPER", "$,", "VVPP", "$.", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "nur n\u00e4hrt,", "tokens": ["nur", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.24": {"text": "Uns nicht nur zur sichren Kleidung die so feste Haut beschert,", "tokens": ["Uns", "nicht", "nur", "zur", "sich\u00b7ren", "Klei\u00b7dung", "die", "so", "fes\u00b7te", "Haut", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Sondern auch bey unsrer Arbeit es zur H\u00fclfe uns ge-", "tokens": ["Son\u00b7dern", "auch", "bey", "uns\u00b7rer", "Ar\u00b7beit", "es", "zur", "H\u00fcl\u00b7fe", "uns", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PPER", "APPRART", "NN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "wehrt?", "tokens": ["wehrt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}