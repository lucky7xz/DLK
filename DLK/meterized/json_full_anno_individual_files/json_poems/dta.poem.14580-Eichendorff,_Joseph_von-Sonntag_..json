{"dta.poem.14580": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht war kaum verbl\u00fchet,", "tokens": ["Die", "Nacht", "war", "kaum", "ver\u00b7bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur eine Lerche sang", "tokens": ["Nur", "ei\u00b7ne", "Ler\u00b7che", "sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die stille Luft entlang.", "tokens": ["Die", "stil\u00b7le", "Luft", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wen gr\u00fc\u00dft sie schon so fr\u00fche?", "tokens": ["Wen", "gr\u00fc\u00dft", "sie", "schon", "so", "fr\u00fc\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}