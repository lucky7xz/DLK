{"textgrid.poem.61717": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von langer Reise kam ich heim, so tr\u00e4umte mir,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von langer Reise kam ich heim, so tr\u00e4umte mir,", "tokens": ["Von", "lan\u00b7ger", "Rei\u00b7se", "kam", "ich", "heim", ",", "so", "tr\u00e4um\u00b7te", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und trat ins Haus, mein s\u00fc\u00dfes Weib \u2013 ich wu\u00dfte nicht", "tokens": ["Und", "trat", "ins", "Haus", ",", "mein", "s\u00fc\u00b7\u00dfes", "Weib", "\u2013", "ich", "wu\u00df\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Spiel des Traumes, da\u00df sie mir gestorben war \u2013", "tokens": ["Im", "Spiel", "des", "Trau\u00b7mes", ",", "da\u00df", "sie", "mir", "ge\u00b7stor\u00b7ben", "war", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ans Herz zu dr\u00fccken nach so manchem \u00f6den Tag,", "tokens": ["Ans", "Herz", "zu", "dr\u00fc\u00b7cken", "nach", "so", "man\u00b7chem", "\u00f6\u00b7den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und fast verging in Ungeduld die Seele mir.", "tokens": ["Und", "fast", "ver\u00b7ging", "in", "Un\u00b7ge\u00b7duld", "die", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch wie ich fragte, hie\u00df es, da\u00df sie droben sei", "tokens": ["Doch", "wie", "ich", "frag\u00b7te", ",", "hie\u00df", "es", ",", "da\u00df", "sie", "dro\u00b7ben", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Im obern Stockwerk: raschen Fu\u00dfes st\u00fcrmt' ich denn", "tokens": ["Im", "o\u00b7bern", "Stock\u00b7werk", ":", "ra\u00b7schen", "Fu\u00b7\u00dfes", "st\u00fcrmt'", "ich", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hinan die Treppen, aber nirgends fand ich sie.", "tokens": ["Hi\u00b7nan", "die", "Trep\u00b7pen", ",", "a\u00b7ber", "nir\u00b7gends", "fand", "ich", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wieder h\u00f6her wies man mich, und wiederum", "tokens": ["Und", "wie\u00b7der", "h\u00f6\u00b7her", "wies", "man", "mich", ",", "und", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PIS", "PPER", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von dort hinaufw\u00e4rts \u00fcber Stufen ohne Zahl", "tokens": ["Von", "dort", "hin\u00b7auf\u00b7w\u00e4rts", "\u00fc\u00b7ber", "Stu\u00b7fen", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zu klimmen hatt' ich, bis zuletzt im obersten", "tokens": ["Zu", "klim\u00b7men", "hatt'", "ich", ",", "bis", "zu\u00b7letzt", "im", "o\u00b7bers\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "$,", "KOUS", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Gescho\u00df ein gl\u00e4nzend heller Saal sich \u00f6ffnete.", "tokens": ["Ge\u00b7scho\u00df", "ein", "gl\u00e4n\u00b7zend", "hel\u00b7ler", "Saal", "sich", "\u00f6ff\u00b7ne\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da sa\u00df sie zwischen fremden Blumen, stillvertieft,", "tokens": ["Da", "sa\u00df", "sie", "zwi\u00b7schen", "frem\u00b7den", "Blu\u00b7men", ",", "still\u00b7ver\u00b7tieft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Haupt gelind zur Seite neigend, ganz wie sonst,", "tokens": ["Das", "Haupt", "ge\u00b7lind", "zur", "Sei\u00b7te", "nei\u00b7gend", ",", "ganz", "wie", "sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn sich in ernstes Sinnen ihr Gem\u00fct verlor,", "tokens": ["Wenn", "sich", "in", "erns\u00b7tes", "Sin\u00b7nen", "ihr", "Ge\u00b7m\u00fct", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nur himmlisch sch\u00f6ner. S\u00fc\u00dfe D\u00fcfte wallten rings,", "tokens": ["Nur", "himm\u00b7lisch", "sch\u00f6\u00b7ner", ".", "S\u00fc\u00b7\u00dfe", "D\u00fcf\u00b7te", "wall\u00b7ten", "rings", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "$.", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und solche Klarheit war umher, da\u00df ich verstummt,", "tokens": ["Und", "sol\u00b7che", "Klar\u00b7heit", "war", "um\u00b7her", ",", "da\u00df", "ich", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vom Glanz geblendet auf der Schwelle zauderte.", "tokens": ["Vom", "Glanz", "ge\u00b7blen\u00b7det", "auf", "der", "Schwel\u00b7le", "zau\u00b7der\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie aber wandte, wie den Kelch im Sommerhauch", "tokens": ["Sie", "a\u00b7ber", "wand\u00b7te", ",", "wie", "den", "Kelch", "im", "Som\u00b7mer\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Lilie wendet, sanft zu mir das Antlitz her", "tokens": ["Die", "Li\u00b7lie", "wen\u00b7det", ",", "sanft", "zu", "mir", "das", "Ant\u00b7litz", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "APPR", "PPER", "ART", "NN", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und sah mich an voll Liebe, da\u00df das treue Licht", "tokens": ["Und", "sah", "mich", "an", "voll", "Lie\u00b7be", ",", "da\u00df", "das", "treu\u00b7e", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJD", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der braunen Augen tief mir in die Seele drang,", "tokens": ["Der", "brau\u00b7nen", "Au\u00b7gen", "tief", "mir", "in", "die", "See\u00b7le", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie ganz erf\u00fcllend. Aber als ich nun nach ihr", "tokens": ["Sie", "ganz", "er\u00b7f\u00fcl\u00b7lend", ".", "A\u00b7ber", "als", "ich", "nun", "nach", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$.", "KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Arme breitet', ach, da war das holde Bild", "tokens": ["Die", "Ar\u00b7me", "brei\u00b7tet'", ",", "ach", ",", "da", "war", "das", "hol\u00b7de", "Bild"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ITJ", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "In Duft zerronnen pl\u00f6tzlich dem Erwachenden.", "tokens": ["In", "Duft", "zer\u00b7ron\u00b7nen", "pl\u00f6tz\u00b7lich", "dem", "Er\u00b7wa\u00b7chen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "K\u00fchl flo\u00df der Mondschein \u00fcber mein verwitwet Bett,", "tokens": ["K\u00fchl", "flo\u00df", "der", "Mond\u00b7schein", "\u00fc\u00b7ber", "mein", "ver\u00b7wit\u00b7wet", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und hei\u00dfe Tr\u00e4nen weint' ich in den Scho\u00df der Nacht.", "tokens": ["Und", "hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", "weint'", "ich", "in", "den", "Scho\u00df", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART