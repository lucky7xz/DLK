{"dta.poem.2479": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Non laudandus est, qui plus credit  \n qu\u00e6 audit quam qu\u00e6videt.  Plaut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gesetze die sind gleich den schwachen Spinneweben/", "tokens": ["Ge\u00b7set\u00b7ze", "die", "sind", "gleich", "den", "schwa\u00b7chen", "Spin\u00b7ne\u00b7we\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00f6rnsen brechen durch/ die M\u00fckken bleiben kleben.", "tokens": ["Die", "H\u00f6rn\u00b7sen", "bre\u00b7chen", "durch", "/", "die", "M\u00fck\u00b7ken", "blei\u00b7ben", "kle\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}