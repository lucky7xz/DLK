{"textgrid.poem.43942": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So sollt und must es seyn: Die Strafe folgt der S\u00fcnde,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So sollt und must es seyn: Die Strafe folgt der S\u00fcnde,", "tokens": ["So", "sollt", "und", "must", "es", "seyn", ":", "Die", "Stra\u00b7fe", "folgt", "der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KON", "VMFIN", "PPER", "VAINF", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so, verf\u00fchrter Geist, geschieht dir eben recht;", "tokens": ["Und", "so", ",", "ver\u00b7f\u00fchr\u00b7ter", "Geist", ",", "ge\u00b7schieht", "dir", "e\u00b7ben", "recht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es l\u00e4st dich endlich auch die nette Philirinde,", "tokens": ["Es", "l\u00e4st", "dich", "end\u00b7lich", "auch", "die", "net\u00b7te", "Phi\u00b7li\u00b7rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist es, was dein Herz mit neuem Kummer schw\u00e4cht,", "tokens": ["Dies", "ist", "es", ",", "was", "dein", "Herz", "mit", "neu\u00b7em", "Kum\u00b7mer", "schw\u00e4cht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dies ist auch, was dich jezt mit Nachdruck lehren kan,", "tokens": ["Dies", "ist", "auch", ",", "was", "dich", "jezt", "mit", "Nach\u00b7druck", "leh\u00b7ren", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie weh du Lenchens Brust durch Flucht und Bruch gethan.", "tokens": ["Wie", "weh", "du", "Len\u00b7chens", "Brust", "durch", "Flucht", "und", "Bruch", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NE", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach, freylich thut es weh, wenn solche Ketten springen;", "tokens": ["Ach", ",", "frey\u00b7lich", "thut", "es", "weh", ",", "wenn", "sol\u00b7che", "Ket\u00b7ten", "sprin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Brecht, s\u00fc\u00dfe Fe\u00dfel, brecht! Ich bin genug gedr\u00fcckt,", "tokens": ["Brecht", ",", "s\u00fc\u00b7\u00dfe", "Fe\u00b7\u00dfel", ",", "brecht", "!", "Ich", "bin", "ge\u00b7nug", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich soll kein frischer Ku\u00df in neue Bande zwingen;", "tokens": ["Mich", "soll", "kein", "fri\u00b7scher", "Ku\u00df", "in", "neu\u00b7e", "Ban\u00b7de", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Philirindens Zorn die lezte Glut erstickt,", "tokens": ["Da", "Phi\u00b7li\u00b7rin\u00b7dens", "Zorn", "die", "lez\u00b7te", "Glut", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und da mich ihre Flucht auf Erden elend macht,", "tokens": ["Und", "da", "mich", "ih\u00b7re", "Flucht", "auf", "Er\u00b7den", "e\u00b7lend", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So sag ich auf einmahl der Liebe gute Nacht.", "tokens": ["So", "sag", "ich", "auf", "ein\u00b7mahl", "der", "Lie\u00b7be", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Liebe gute Nacht und auf einmahl zu sagen,", "tokens": ["Der", "Lie\u00b7be", "gu\u00b7te", "Nacht", "und", "auf", "ein\u00b7mahl", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "APPR", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Herz besinne dich und sch\u00e4ze diesen Schlu\u00df", "tokens": ["Mein", "Herz", "be\u00b7sin\u00b7ne", "dich", "und", "sch\u00e4\u00b7ze", "die\u00b7sen", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wi\u00dfe, da\u00df ein Mensch bey allen Ungl\u00fccksplagen", "tokens": ["Und", "wi\u00b7\u00dfe", ",", "da\u00df", "ein", "Mensch", "bey", "al\u00b7len", "Un\u00b7gl\u00fccks\u00b7pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch wahre Lieb allein den Gram vers\u00fc\u00dfen mu\u00df;", "tokens": ["Durch", "wah\u00b7re", "Lieb", "al\u00b7lein", "den", "Gram", "ver\u00b7s\u00fc\u00b7\u00dfen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Las seyn, da\u00df dieses Kind den treuen Wuntsch betriegt,", "tokens": ["Las", "seyn", ",", "da\u00df", "die\u00b7ses", "Kind", "den", "treu\u00b7en", "Wunt\u00b7sch", "be\u00b7triegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAINF", "$,", "KOUS", "PDAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Wer weis, wie bald dich noch was Artigers vergn\u00fcgt.", "tokens": ["Wer", "weis", ",", "wie", "bald", "dich", "noch", "was", "Ar\u00b7ti\u00b7gers", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADV", "PPER", "ADV", "PWS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vergn\u00fcgt mich diese nicht, so darf mich nichts vergn\u00fcgen:", "tokens": ["Ver\u00b7gn\u00fcgt", "mich", "die\u00b7se", "nicht", ",", "so", "darf", "mich", "nichts", "ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "PTKNEG", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies ist ein blinder Wahn beth\u00f6rter Weichligkeit.", "tokens": ["Dies", "ist", "ein", "blin\u00b7der", "Wahn", "be\u00b7th\u00f6r\u00b7ter", "Weich\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwey Mittel geben Rath, den Kummer zu besiegen:", "tokens": ["Zwey", "Mit\u00b7tel", "ge\u00b7ben", "Rath", ",", "den", "Kum\u00b7mer", "zu", "be\u00b7sie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebrauche der Vernunft, vertrau den Schmerz der Zeit,", "tokens": ["Ge\u00b7brau\u00b7che", "der", "Ver\u00b7nunft", ",", "ver\u00b7trau", "den", "Schmerz", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wiltu ja noch mehr und bald getr\u00f6stet seyn,", "tokens": ["Und", "wil\u00b7tu", "ja", "noch", "mehr", "und", "bald", "ge\u00b7tr\u00f6s\u00b7tet", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "ADV", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So nimm mit Bu\u00df und Reu die alten Flammen ein.", "tokens": ["So", "nimm", "mit", "Bu\u00df", "und", "Reu", "die", "al\u00b7ten", "Flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "NN", "KON", "NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja, ja, ich f\u00fchle schon die R\u00fcckkunft erster Triebe,", "tokens": ["Ja", ",", "ja", ",", "ich", "f\u00fch\u00b7le", "schon", "die", "R\u00fcck\u00b7kunft", "ers\u00b7ter", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Blut erinnert sich der damahls reinen Treu,", "tokens": ["Mein", "Blut", "e\u00b7rin\u00b7nert", "sich", "der", "da\u00b7mahls", "rei\u00b7nen", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wallt und jauchzt vor Lust und wehlt die alte Liebe,", "tokens": ["Es", "wallt", "und", "jauchzt", "vor", "Lust", "und", "wehlt", "die", "al\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit sie dermahleins des Ehstands Himmel sey.", "tokens": ["Da\u00b7mit", "sie", "der\u00b7mah\u00b7leins", "des", "Eh\u00b7stands", "Him\u00b7mel", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was denckstu dir, mein Herz? O gieb dir selbst Geh\u00f6r:", "tokens": ["Was", "dencks\u00b7tu", "dir", ",", "mein", "Herz", "?", "O", "gieb", "dir", "selbst", "Ge\u00b7h\u00f6r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "NE", "VVIMP", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du suchest Lenchens Gunst, sie liebt dich ja nicht mehr.", "tokens": ["Du", "su\u00b7chest", "Len\u00b7chens", "Gunst", ",", "sie", "liebt", "dich", "ja", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich weis, sie liebt mich noch und kan mich nicht verla\u00dfen;", "tokens": ["Ich", "weis", ",", "sie", "liebt", "mich", "noch", "und", "kan", "mich", "nicht", "ver\u00b7la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADV", "KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Neigung gleicher Art verband uns gar zu scharf.", "tokens": ["Die", "Nei\u00b7gung", "glei\u00b7cher", "Art", "ver\u00b7band", "uns", "gar", "zu", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm wieder, liebster Schaz, nun will ich dich umfa\u00dfen,", "tokens": ["Komm", "wie\u00b7der", ",", "liebs\u00b7ter", "Schaz", ",", "nun", "will", "ich", "dich", "um\u00b7fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lang ich nur noch hier der Luft genie\u00dfen darf;", "tokens": ["So", "lang", "ich", "nur", "noch", "hier", "der", "Luft", "ge\u00b7nie\u00b7\u00dfen", "darf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist etwas, das uns trennt, so ist's der Leichenstein;", "tokens": ["Ist", "et\u00b7was", ",", "das", "uns", "trennt", ",", "so", "ist's", "der", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So st\u00e4rckt der Ri\u00df das Band: so sollt und must es seyn.", "tokens": ["So", "st\u00e4rckt", "der", "Ri\u00df", "das", "Band", ":", "so", "sollt", "und", "must", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "VMFIN", "KON", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So sollt und must es seyn: Die Strafe folgt der S\u00fcnde,", "tokens": ["So", "sollt", "und", "must", "es", "seyn", ":", "Die", "Stra\u00b7fe", "folgt", "der", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KON", "VMFIN", "PPER", "VAINF", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so, verf\u00fchrter Geist, geschieht dir eben recht;", "tokens": ["Und", "so", ",", "ver\u00b7f\u00fchr\u00b7ter", "Geist", ",", "ge\u00b7schieht", "dir", "e\u00b7ben", "recht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es l\u00e4st dich endlich auch die nette Philirinde,", "tokens": ["Es", "l\u00e4st", "dich", "end\u00b7lich", "auch", "die", "net\u00b7te", "Phi\u00b7li\u00b7rin\u