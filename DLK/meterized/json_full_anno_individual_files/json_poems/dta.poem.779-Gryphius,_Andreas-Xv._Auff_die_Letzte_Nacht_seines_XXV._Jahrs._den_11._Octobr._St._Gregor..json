{"dta.poem.779": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  \n Auff die Letzte Nacht seines  XXV.  Jahrs.  \n den 11. Octobr.  St. Gregor.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Kom Mitternacht vnd schleu\u00df di\u00df Threnen reiche Jahr? ", "tokens": ["Kom", "Mit\u00b7ter\u00b7nacht", "vnd", "schleu\u00df", "di\u00df", "Thre\u00b7nen", "rei\u00b7che", "Jahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "PDS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die schmertzen volle zeit/ die mich so tief verletzet/", "tokens": ["Die", "schmert\u00b7zen", "vol\u00b7le", "zeit", "/", "die", "mich", "so", "tief", "ver\u00b7let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die dich mein Bruder hat in jenes Reich versetzet/", "tokens": ["Die", "dich", "mein", "Bru\u00b7der", "hat", "in", "je\u00b7nes", "Reich", "ver\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VAFIN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd Schwester deine Leich gestellet auff die Baar.", "tokens": ["Vnd", "Schwes\u00b7ter", "dei\u00b7ne", "Leich", "ge\u00b7stel\u00b7let", "auff", "die", "Baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die zeit/ die auff mich angst/ vnd grimmer seuchen schaar/", "tokens": ["Die", "zeit", "/", "die", "auff", "mich", "angst", "/", "vnd", "grim\u00b7mer", "seu\u00b7chen", "schaar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "PPER", "VVPP", "$(", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd trawren/ vnd verdru\u00df vnd schrecken hat verhetzet.", "tokens": ["Vnd", "traw\u00b7ren", "/", "vnd", "ver\u00b7dru\u00df", "vnd", "schre\u00b7cken", "hat", "ver\u00b7het\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "NN", "KON", "VVFIN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer hat noch newlich mich nicht schon vor tod gesch\u00e4tzet/", "tokens": ["Wer", "hat", "noch", "new\u00b7lich", "mich", "nicht", "schon", "vor", "tod", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da/ als ich mir nicht mehr im Siechbett \u00e4hnlich w\u00e4r;", "tokens": ["Da", "/", "als", "ich", "mir", "nicht", "mehr", "im", "Siech\u00b7bett", "\u00e4hn\u00b7lich", "w\u00e4r", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn deine Trew\u2019 O Gott mich nicht mit trost erquicket/", "tokens": ["Wenn", "dei\u00b7ne", "Tre\u00b7w'", "O", "Gott", "mich", "nicht", "mit", "trost", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "NN", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als so viel grause Noth den bl\u00f6den Geist verstricket/", "tokens": ["Als", "so", "viel", "grau\u00b7se", "Noth", "den", "bl\u00f6\u00b7den", "Geist", "ver\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00e4r ich gantz in angst ertruncken vnd verschmacht.", "tokens": ["So", "w\u00e4r", "ich", "gantz", "in", "angst", "er\u00b7trun\u00b7cken", "vnd", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "HeRR/ dessen linde Faust wischt die bethr\u00e4nten wangen.", "tokens": ["HeRR", "/", "des\u00b7sen", "lin\u00b7de", "Faust", "wischt", "die", "be\u00b7thr\u00e4n\u00b7ten", "wan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "La\u00df doch nach so viel sturm mich linder zeit anfangen", "tokens": ["La\u00df", "doch", "nach", "so", "viel", "sturm", "mich", "lin\u00b7der", "zeit", "an\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "APPR", "ADV", "ADV", "ADJD", "PPER", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Vn hei\u00df die herben Jahr vergehn mit dieser Nacht.", "tokens": ["Vn", "hei\u00df", "die", "her\u00b7ben", "Jahr", "ver\u00b7gehn", "mit", "die\u00b7ser", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "VVINF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}