, "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wer einem Sieger widerspricht, der widerspricht mit Unbedacht.", "tokens": ["Wer", "ei\u00b7nem", "Sie\u00b7ger", "wi\u00b7der\u00b7spricht", ",", "der", "wi\u00b7der\u00b7spricht", "mit", "Un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Nur eine Bitte wag ich noch, abw\u00e4gend dein Geschick und meins:", "tokens": ["Nur", "ei\u00b7ne", "Bit\u00b7te", "wag", "ich", "noch", ",", "ab\u00b7w\u00e4\u00b7gend", "dein", "Ge\u00b7schick", "und", "meins", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJD", "PPOSAT", "NN", "KON", "NE", "$."], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Drei Tage focht ich ohne Trunk, la\u00df reichen einen Becher Weins!", "tokens": ["Drei", "Ta\u00b7ge", "focht", "ich", "oh\u00b7ne", "Trunk", ",", "la\u00df", "rei\u00b7chen", "ei\u00b7nen", "Be\u00b7cher", "Weins", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "VVIMP", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und auf des Feldherrn leisen Wink steht ihm sogleich ein Trunk bereit;", "tokens": ["Und", "auf", "des", "Feld\u00b7herrn", "lei\u00b7sen", "Wink", "steht", "ihm", "sog\u00b7leich", "ein", "Trunk", "be\u00b7reit", ";"], "token_