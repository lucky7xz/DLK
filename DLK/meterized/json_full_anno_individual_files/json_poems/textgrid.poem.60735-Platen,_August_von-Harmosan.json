{"textgrid.poem.60735": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Harmosan", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon war gesunken in den Staub der Sassaniden alter Thron,", "tokens": ["Schon", "war", "ge\u00b7sun\u00b7ken", "in", "den", "Staub", "der", "Sas\u00b7sa\u00b7ni\u00b7den", "al\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es pl\u00fcndert Mosleminenhand das sch\u00e4tzereiche Ktesiphon:", "tokens": ["Es", "pl\u00fcn\u00b7dert", "Mos\u00b7le\u00b7mi\u00b7nen\u00b7hand", "das", "sch\u00e4t\u00b7ze\u00b7rei\u00b7che", "Kte\u00b7si\u00b7phon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Schon langt am Oxus Omar an, nach manchem durchgek\u00e4mpften Tag,", "tokens": ["Schon", "langt", "am", "O\u00b7xus", "O\u00b7mar", "an", ",", "nach", "man\u00b7chem", "durch\u00b7ge\u00b7k\u00e4mpf\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NE", "NE", "PTKVZ", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wo Chosrus Enkel Jesdegerd auf Leichen eine Leiche lag.", "tokens": ["Wo", "Chos\u00b7rus", "En\u00b7kel", "Jes\u00b7de\u00b7gerd", "auf", "Lei\u00b7chen", "ei\u00b7ne", "Lei\u00b7che", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "NE", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und als die Beute mustern ging Medinas F\u00fcrst auf weitem Plan,", "tokens": ["Und", "als", "die", "Beu\u00b7te", "mus\u00b7tern", "ging", "Me\u00b7di\u00b7nas", "F\u00fcrst", "auf", "wei\u00b7tem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "VVFIN", "NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ward ein Satrap vor ihn gef\u00fchrt, er hie\u00df mit Namen Harmosan;", "tokens": ["Ward", "ein", "Sat\u00b7rap", "vor", "ihn", "ge\u00b7f\u00fchrt", ",", "er", "hie\u00df", "mit", "Na\u00b7men", "Har\u00b7mo\u00b7san", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,", "PPER", "VVFIN", "APPR", "NN", "NE", "$."], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der letzte, der im Hochgebirg dem k\u00fchnen Feind sich widersetzt;", "tokens": ["Der", "letz\u00b7te", ",", "der", "im", "Hoch\u00b7ge\u00b7birg", "dem", "k\u00fch\u00b7nen", "Feind", "sich", "wi\u00b7der\u00b7setzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPRART", "NN", "ART", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Doch ach, die sonst so tapfre Hand trug eine schwere Kette jetzt!", "tokens": ["Doch", "ach", ",", "die", "sonst", "so", "tapf\u00b7re", "Hand", "trug", "ei\u00b7ne", "schwe\u00b7re", "Ket\u00b7te", "jetzt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Und Omar blickt ihn finster an und spricht: Erkennst du nun, wie sehr", "tokens": ["Und", "O\u00b7mar", "blickt", "ihn", "fins\u00b7ter", "an", "und", "spricht", ":", "Er\u00b7kennst", "du", "nun", ",", "wie", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Vergeblich ist vor unserm Gott der G\u00f6tzendiener Gegenwehr?", "tokens": ["Ver\u00b7geb\u00b7lich", "ist", "vor", "un\u00b7serm", "Gott", "der", "G\u00f6t\u00b7zen\u00b7die\u00b7ner", "Ge\u00b7gen\u00b7wehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und Harmosan erwidert ihm: In deinen H\u00e4nden ist die Macht,", "tokens": ["Und", "Har\u00b7mo\u00b7san", "er\u00b7wi\u00b7dert", "ihm", ":", "In", "dei\u00b7nen", "H\u00e4n\u00b7den", "ist", "die", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$.", "APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wer einem Sieger widerspricht, der widerspricht mit Unbedacht.", "tokens": ["Wer", "ei\u00b7nem", "Sie\u00b7ger", "wi\u00b7der\u00b7spricht", ",", "der", "wi\u00b7der\u00b7spricht", "mit", "Un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Nur eine Bitte wag ich noch, abw\u00e4gend dein Geschick und meins:", "tokens": ["Nur", "ei\u00b7ne", "Bit\u00b7te", "wag", "ich", "noch", ",", "ab\u00b7w\u00e4\u00b7gend", "dein", "Ge\u00b7schick", "und", "meins", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADJD", "PPOSAT", "NN", "KON", "NE", "$."], "meter": "-+-+-+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Drei Tage focht ich ohne Trunk, la\u00df reichen einen Becher Weins!", "tokens": ["Drei", "Ta\u00b7ge", "focht", "ich", "oh\u00b7ne", "Trunk", ",", "la\u00df", "rei\u00b7chen", "ei\u00b7nen", "Be\u00b7cher", "Weins", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "NN", "$,", "VVIMP", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und auf des Feldherrn leisen Wink steht ihm sogleich ein Trunk bereit;", "tokens": ["Und", "auf", "des", "Feld\u00b7herrn", "lei\u00b7sen", "Wink", "steht", "ihm", "sog\u00b7leich", "ein", "Trunk", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Doch Harmosan bef\u00fcrchtet Gift, und zaudert eine kleine Zeit.", "tokens": ["Doch", "Har\u00b7mo\u00b7san", "be\u00b7f\u00fcrch\u00b7tet", "Gift", ",", "und", "zau\u00b7dert", "ei\u00b7ne", "klei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Was zagst du, ruft der Sarazen, nie t\u00e4uscht ein Moslem seinen Gast,", "tokens": ["Was", "zagst", "du", ",", "ruft", "der", "Sa\u00b7ra\u00b7zen", ",", "nie", "t\u00e4uscht", "ein", "Mos\u00b7lem", "sei\u00b7nen", "Gast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Nicht eher sollst du sterben, Freund, als bis du dies getrunken hast!", "tokens": ["Nicht", "e\u00b7her", "sollst", "du", "ster\u00b7ben", ",", "Freund", ",", "als", "bis", "du", "dies", "ge\u00b7trun\u00b7ken", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "VVINF", "$,", "NN", "$,", "KOUS", "APPR", "PPER", "PDS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Da greift der Perser nach dem Glas, und statt zu trinken, schleudert hart", "tokens": ["Da", "greift", "der", "Per\u00b7ser", "nach", "dem", "Glas", ",", "und", "statt", "zu", "trin\u00b7ken", ",", "schleu\u00b7dert", "hart"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "KON", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Zu Boden er's auf einen Stein mit rascher Geistesgegenwart.", "tokens": ["Zu", "Bo\u00b7den", "er's", "auf", "ei\u00b7nen", "Stein", "mit", "ra\u00b7scher", "Geis\u00b7tes\u00b7ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Und Omars Mannen st\u00fcrzen schon mit blankem Schwert auf ihn heran,", "tokens": ["Und", "O\u00b7mars", "Man\u00b7nen", "st\u00fcr\u00b7zen", "schon", "mit", "blan\u00b7kem", "Schwert", "auf", "ihn", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Zu strafen ob der Hinterlist den allzuschlauen Harmosan;", "tokens": ["Zu", "stra\u00b7fen", "ob", "der", "Hin\u00b7ter\u00b7list", "den", "all\u00b7zu\u00b7schlau\u00b7en", "Har\u00b7mo\u00b7san", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Doch wehrt der Feldherr ihnen ab und spricht sodann: Er lebe fort!", "tokens": ["Doch", "wehrt", "der", "Feld\u00b7herr", "ih\u00b7nen", "ab", "und", "spricht", "so\u00b7dann", ":", "Er", "le\u00b7be", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wenn was auf Erden heilig ist, so ist es eines Helden Wort.", "tokens": ["Wenn", "was", "auf", "Er\u00b7den", "hei\u00b7lig", "ist", ",", "so", "ist", "es", "ei\u00b7nes", "Hel\u00b7den", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADJD", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Schon war gesunken in den Staub der Sassaniden alter Thron,", "tokens": ["Schon", "war", "ge\u00b7sun\u00b7ken", "in", "den", "Staub", "der", "Sas\u00b7sa\u00b7ni\u00b7den", "al\u00b7ter", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es pl\u00fcndert Mosleminenhand das sch\u00e4tzereiche Ktesiphon:", "tokens": ["Es", "pl\u00fcn\u00b7dert", "Mos\u00b7le\u00b7mi\u00b7nen\u00b7hand", "das", "sch\u00e4t\u00b7ze\u00b7rei\u00b7che", "Kte\u00b7si\u00b7phon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Schon langt am Oxus Omar an, nach manchem durchgek\u00e4mpften Tag,", "tokens": ["Schon", "langt", "am", "O\u00b7xus", "O\u00b7mar", "an", ",", "nach", "man\u00b7chem", "durch\u00b7ge\u00b7k\u00e4mpf\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NE", "NE", "PTKVZ", "$,", "APPR", 