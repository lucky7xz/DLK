{"textgrid.poem.41410": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Doris", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Doris, die freundliche Sch\u00f6ne,", "tokens": ["Als", "Do\u00b7ris", ",", "die", "freund\u00b7li\u00b7che", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Vorzug der Freiheit verlor,", "tokens": ["Den", "Vor\u00b7zug", "der", "Frei\u00b7heit", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und man ihr, nach langem Geh\u00f6hne,", "tokens": ["Und", "man", "ihr", ",", "nach", "lan\u00b7gem", "Ge\u00b7h\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Den h\u00e4\u00dflichsten Eh'schatz erkor;", "tokens": ["Den", "h\u00e4\u00df\u00b7lichs\u00b7ten", "Eh'\u00b7schatz", "er\u00b7kor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Da flohen die gaukelnde Freude,", "tokens": ["Da", "flo\u00b7hen", "die", "gau\u00b7keln\u00b7de", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Das Scherzen, der Liebreiz, die Huld;", "tokens": ["Das", "Scher\u00b7zen", ",", "der", "Lieb\u00b7reiz", ",", "die", "Huld", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Doch kamen im Hochzeitgeschmeide", "tokens": ["Doch", "ka\u00b7men", "im", "Hoch\u00b7zeit\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Die Treue, die Pflicht, die Geduld.", "tokens": ["Die", "Treu\u00b7e", ",", "die", "Pflicht", ",", "die", "Ge\u00b7duld", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ihr Mann, den die Eifersucht nagte,", "tokens": ["Ihr", "Mann", ",", "den", "die", "Ei\u00b7fer\u00b7sucht", "nag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Erwies sich so grausam und hart,", "tokens": ["Er\u00b7wies", "sich", "so", "grau\u00b7sam", "und", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df, was sie nur machte, nur sagte,", "tokens": ["Da\u00df", ",", "was", "sie", "nur", "mach\u00b7te", ",", "nur", "sag\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ihm gleich zur Beleidigung ward.", "tokens": ["Ihm", "gleich", "zur", "Be\u00b7lei\u00b7di\u00b7gung", "ward", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Es glichen den Tagen die N\u00e4chte;", "tokens": ["Es", "gli\u00b7chen", "den", "Ta\u00b7gen", "die", "N\u00e4ch\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Auch dann nahm sein Argwohn nicht ab,", "tokens": ["Auch", "dann", "nahm", "sein", "Arg\u00b7wohn", "nicht", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.7": {"text": "Noch, wann er die frostige Rechte", "tokens": ["Noch", ",", "wann", "er", "die", "fros\u00b7ti\u00b7ge", "Rech\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Zum Anwunsch des Schlafes ihr gab.", "tokens": ["Zum", "An\u00b7wunsch", "des", "Schla\u00b7fes", "ihr", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ihr Eifer benetzte die Wangen;", "tokens": ["Ihr", "Ei\u00b7fer", "be\u00b7netz\u00b7te", "die", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie klagte dem Himmel ihr Leid:", "tokens": ["Sie", "klag\u00b7te", "dem", "Him\u00b7mel", "ihr", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Soll Treue nur Undank empfangen;", "tokens": ["Soll", "Treu\u00b7e", "nur", "Un\u00b7dank", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was steht denn der Untreu' bereit?", "tokens": ["Was", "steht", "denn", "der", "Un\u00b7treu'", "be\u00b7reit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf! r\u00e4chender Himmel, erwache,", "tokens": ["Auf", "!", "r\u00e4\u00b7chen\u00b7der", "Him\u00b7mel", ",", "er\u00b7wa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$.", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Erm\u00fcde, mein Elend zu sehn!", "tokens": ["Er\u00b7m\u00fc\u00b7de", ",", "mein", "E\u00b7lend", "zu", "sehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Du z\u00f6gerst? So mu\u00df denn die Rache", "tokens": ["Du", "z\u00f6\u00b7gerst", "?", "So", "mu\u00df", "denn", "die", "Ra\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VMFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Vielleicht durch mich selber geschehn.", "tokens": ["Viel\u00b7leicht", "durch", "mich", "sel\u00b7ber", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Gesetze der Ehre, der Tugend,", "tokens": ["Ge\u00b7set\u00b7ze", "der", "Eh\u00b7re", ",", "der", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Euch leb' ich mit Seufzen jetzt nach;", "tokens": ["Euch", "leb'", "ich", "mit", "Seuf\u00b7zen", "jetzt", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch ist die empfindliche Jugend", "tokens": ["Doch", "ist", "die", "emp\u00b7find\u00b7li\u00b7che", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nicht dieser Versuchung zu schwach?", "tokens": ["Nicht", "die\u00b7ser", "Ver\u00b7su\u00b7chung", "zu", "schwach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Es drohet Verzweiflung dem Herzen,", "tokens": ["Es", "dro\u00b7het", "Ver\u00b7zwei\u00b7flung", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Kummer verzehret den Leib;", "tokens": ["Der", "Kum\u00b7mer", "ver\u00b7zeh\u00b7ret", "den", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Soll Unschuld denn alles verschmerzen,", "tokens": ["Soll", "Un\u00b7schuld", "denn", "al\u00b7les", "ver\u00b7schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "PIS", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und bin ich nicht sch\u00f6n, und ein Weib?", "tokens": ["Und", "bin", "ich", "nicht", "sch\u00f6n", ",", "und", "ein", "Weib", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KON", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Was Doris aus Rache vollstrecket,", "tokens": ["Was", "Do\u00b7ris", "aus", "Ra\u00b7che", "voll\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das hat mir noch niemand erz\u00e4hlt.", "tokens": ["Das", "hat", "mir", "noch", "nie\u00b7mand", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ihr l\u00e4chelnden Sch\u00f6nen, entdecket:", "tokens": ["Ihr", "l\u00e4\u00b7cheln\u00b7den", "Sch\u00f6\u00b7nen", ",", "ent\u00b7de\u00b7cket", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was h\u00e4ttet ihr selber gew\u00e4hlt?", "tokens": ["Was", "h\u00e4t\u00b7tet", "ihr", "sel\u00b7ber", "ge\u00b7w\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ihr M\u00e4dchen, befraget die Frauen;", "tokens": ["Ihr", "M\u00e4d\u00b7chen", ",", "be\u00b7fra\u00b7get", "die", "Frau\u00b7en", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zwar sind sie geheim und gescheit:", "tokens": ["Zwar", "sind", "sie", "ge\u00b7heim", "und", "ge\u00b7scheit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Doch manche verr\u00e4th im Vertrauen", "tokens": ["Doch", "man\u00b7che", "ver\u00b7r\u00e4\u00b7th", "im", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Rache, die Weiber erfreut.", "tokens": ["Die", "Ra\u00b7che", ",", "die", "Wei\u00b7ber", "er\u00b7freut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Als Doris, die freundliche Sch\u00f6ne,", "tokens": ["Als", "Do\u00b7ris", ",", "die", "freund\u00b7li\u00b7che", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den Vorzug der Freiheit verlor,", "tokens": ["Den", "Vor\u00b7zug", "der", "Frei\u00b7heit", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "pun