{"textgrid.poem.66333": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettelsoldat", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit jammervollem Blicke,", "tokens": ["Mit", "jam\u00b7mer\u00b7vol\u00b7lem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von tausend Sorgen schwer,", "tokens": ["Von", "tau\u00b7send", "Sor\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hink' ich an meiner Kr\u00fccke", "tokens": ["Hin\u00b7k'", "ich", "an", "mei\u00b7ner", "Kr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In weiter Welt umher.", "tokens": ["In", "wei\u00b7ter", "Welt", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott wei\u00df, hab' viel gelitten,", "tokens": ["Gott", "wei\u00df", ",", "hab'", "viel", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab' so manchen Kampf", "tokens": ["Ich", "hab'", "so", "man\u00b7chen", "Kampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In mancher Schlacht gestritten,", "tokens": ["In", "man\u00b7cher", "Schlacht", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geh\u00fcllt in Pulverdampf.", "tokens": ["Ge\u00b7h\u00fcllt", "in", "Pul\u00b7ver\u00b7dampf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sah manchen Kameraden", "tokens": ["Sah", "man\u00b7chen", "Ka\u00b7me\u00b7ra\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner Seite todt,", "tokens": ["An", "mei\u00b7ner", "Sei\u00b7te", "todt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mu\u00dft' im Blute waten,", "tokens": ["Und", "mu\u00dft'", "im", "Blu\u00b7te", "wa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn es mein Herr gebot.", "tokens": ["Wenn", "es", "mein", "Herr", "ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mir drohten oft Gesch\u00fctze", "tokens": ["Mir", "droh\u00b7ten", "oft", "Ge\u00b7sch\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den f\u00fcrchterlichsten Tod,", "tokens": ["Den", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7ten", "Tod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oft trank ich aus der Pf\u00fctze,", "tokens": ["Oft", "trank", "ich", "aus", "der", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oft a\u00df ich schimmlicht Brod.", "tokens": ["Oft", "a\u00df", "ich", "schimm\u00b7licht", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich stand in Sturm und Regen", "tokens": ["Ich", "stand", "in", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In grauser Mitternacht,", "tokens": ["In", "grau\u00b7ser", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei Blitz und Donnerschl\u00e4gen", "tokens": ["Bei", "Blitz", "und", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oft einsam auf der Wacht.", "tokens": ["Oft", "ein\u00b7sam", "auf", "der", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und nun nach mancher Schonung,", "tokens": ["Und", "nun", "nach", "man\u00b7cher", "Scho\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch fern von meinem Grab,", "tokens": ["Noch", "fern", "von", "mei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Empfang' ich die Belohnung", "tokens": ["Emp\u00b7fang'", "ich", "die", "Be\u00b7loh\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit diesem Bettelstab.", "tokens": ["Mit", "die\u00b7sem", "Bet\u00b7tel\u00b7stab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bedeckt mit dreizehn Wunden,", "tokens": ["Be\u00b7deckt", "mit", "drei\u00b7zehn", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner Kr\u00fcck' gelehnt,", "tokens": ["An", "mei\u00b7ner", "Kr\u00fcck", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hab' ich in manchen Stunden", "tokens": ["Hab'", "ich", "in", "man\u00b7chen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich nach dem Tod gesehnt.", "tokens": ["Mich", "nach", "dem", "Tod", "ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich bettle vor den Th\u00fcren,", "tokens": ["Ich", "bett\u00b7le", "vor", "den", "Th\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich armer lahmer Mann!", "tokens": ["Ich", "ar\u00b7mer", "lah\u00b7mer", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ach! wen kann ich r\u00fchren?", "tokens": ["Doch", "ach", "!", "wen", "kann", "ich", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer nimmt sich meiner an?", "tokens": ["Wer", "nimmt", "sich", "mei\u00b7ner", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "War einst ein braver Krieger,", "tokens": ["War", "einst", "ein", "bra\u00b7ver", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sang manch Soldatenlied", "tokens": ["Sang", "manch", "Sol\u00b7da\u00b7ten\u00b7lied"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Reihen froher Sieger;", "tokens": ["Im", "Rei\u00b7hen", "fro\u00b7her", "Sie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun bin ich Invalid.", "tokens": ["Nun", "bin", "ich", "In\u00b7va\u00b7lid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr S\u00f6hne, bei der Kr\u00fccke,", "tokens": ["Ihr", "S\u00f6h\u00b7ne", ",", "bei", "der", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An der mein Leib sich beugt,", "tokens": ["An", "der", "mein", "Leib", "sich", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei diesem Thr\u00e4nenblicke,", "tokens": ["Bei", "die\u00b7sem", "Thr\u00e4\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sich zum Grabe neigt,", "tokens": ["Der", "sich", "zum", "Gra\u00b7be", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Beschw\u00f6r' ich euch \u2013 ihr S\u00f6hne!", "tokens": ["Be\u00b7schw\u00f6r'", "ich", "euch", "\u2013", "ihr", "S\u00f6h\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O flieht der Trommel Ton", "tokens": ["O", "flieht", "der", "Trom\u00b7mel", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Kriegstrommetent\u00f6ne!", "tokens": ["Un