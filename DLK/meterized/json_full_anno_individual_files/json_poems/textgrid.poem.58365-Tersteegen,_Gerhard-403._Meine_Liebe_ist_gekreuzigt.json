{"textgrid.poem.58365": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "403. Meine Liebe ist gekreuzigt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ", "tokens": ["Ja", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Was n\u00fctzt mir alle Welt samt Ehre, Lust und Sch\u00e4tzen?", "tokens": ["Was", "n\u00fctzt", "mir", "al\u00b7le", "Welt", "samt", "Eh\u00b7re", ",", "Lust", "und", "Sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die H\u00e4nde sind mir fest, ich kann mich nicht erg\u00f6tzen,", "tokens": ["Die", "H\u00e4n\u00b7de", "sind", "mir", "fest", ",", "ich", "kann", "mich", "nicht", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist kein Feu'r in mir, als das zum Vater geht.", "tokens": ["Es", "ist", "kein", "Feu'r", "in", "mir", ",", "als", "das", "zum", "Va\u00b7ter", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PPER", "$,", "KOUS", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, ", "tokens": ["Ja", ","], "token_inf