{"textgrid.poem.34273": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit \u00e4chterm Ruhm, als unbesiegte Sieger", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit \u00e4chterm Ruhm, als unbesiegte Sieger", "tokens": ["Mit", "\u00e4ch\u00b7term", "Ruhm", ",", "als", "un\u00b7be\u00b7sieg\u00b7te", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur gro\u00df an Gl\u00fcck, am Herzen wild als Tiger,", "tokens": ["Nur", "gro\u00df", "an", "Gl\u00fcck", ",", "am", "Her\u00b7zen", "wild", "als", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "APPRART", "NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch H\u00e4rt und Wuth und unerh\u00f6rtes Schlachten", "tokens": ["Durch", "H\u00e4rt", "und", "Wuth", "und", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Schlach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu haschen trachten;", "tokens": ["Zu", "ha\u00b7schen", "trach\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mit \u00e4chterm Ruhm, als mancher Filz bezahlet,", "tokens": ["Mit", "\u00e4ch\u00b7term", "Ruhm", ",", "als", "man\u00b7cher", "Filz", "be\u00b7zah\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mit des Reimers feiler Demuth prahlet,", "tokens": ["Der", "mit", "des", "Rei\u00b7mers", "fei\u00b7ler", "De\u00b7muth", "prah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Strohmann gleich, den man mit Lappen decket", "tokens": ["Dem", "Stroh\u00b7mann", "gleich", ",", "den", "man", "mit", "Lap\u00b7pen", "de\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PIS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Kinder schr\u00f6cket;", "tokens": ["Und", "Kin\u00b7der", "schr\u00f6\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mit \u00e4chterm Ruhme wird der Mann belohnet,", "tokens": ["Mit", "\u00e4ch\u00b7term", "Ruh\u00b7me", "wird", "der", "Mann", "be\u00b7loh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welchem Tugend bey der Wei\u00dfheit wohnet,", "tokens": ["In", "wel\u00b7chem", "Tu\u00b7gend", "bey", "der", "Wei\u00df\u00b7heit", "woh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschheit Lehrer, der, was er sie lehret,", "tokens": ["Der", "Menschheit", "Leh\u00b7rer", ",", "der", ",", "was", "er", "sie", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "$,", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Selbst \u00fcbt und ehret:", "tokens": ["Selbst", "\u00fcbt", "und", "eh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Des richtig Auge nie ein Schimmer blendte,", "tokens": ["Des", "rich\u00b7tig", "Au\u00b7ge", "nie", "ein", "Schim\u00b7mer", "blend\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der nie die Thorheit kriechend Wei\u00dfheit nennte,", "tokens": ["Der", "nie", "die", "Thor\u00b7heit", "krie\u00b7chend", "Wei\u00df\u00b7heit", "nenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der oft die Maske die wir scheuen m\u00fcssen", "tokens": ["Der", "oft", "die", "Mas\u00b7ke", "die", "wir", "scheu\u00b7en", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr abgerissen.", "tokens": ["Ihr", "ab\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Da lag der Orden und des Hofes Waare,", "tokens": ["Da", "lag", "der", "Or\u00b7den", "und", "des", "Ho\u00b7fes", "Waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Kriegeszeichen, Turban und Tiare,", "tokens": ["Und", "Krie\u00b7ges\u00b7zei\u00b7chen", ",", "Tur\u00b7ban", "und", "Ti\u00b7a\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Priestermantel, Schleyer, Kutten, Decken,", "tokens": ["Der", "Pries\u00b7ter\u00b7man\u00b7tel", ",", "Schle\u00b7yer", ",", "Kut\u00b7ten", ",", "De\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sie verstecken", "tokens": ["Die", "sie", "ver\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und sie stand nackend. Abscheu und Gel\u00e4chter", "tokens": ["Und", "sie", "stand", "na\u00b7ckend", ".", "Ab\u00b7scheu", "und", "Ge\u00b7l\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "VVPP", "$.", "NN", "KON", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ward ihr zu Theile. Aber die Ver\u00e4chter", "tokens": ["Ward", "ihr", "zu", "Thei\u00b7le", ".", "A\u00b7ber", "die", "Ver\u00b7\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des schlechten Kittels und berauchter H\u00fctten", "tokens": ["Des", "schlech\u00b7ten", "Kit\u00b7tels", "und", "be\u00b7rauch\u00b7ter", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Samt ihren Sitten", "tokens": ["Samt", "ih\u00b7ren", "Sit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Sahn staunend dort, sie, die den Glanz der Thronen", "tokens": ["Sahn", "stau\u00b7nend", "dort", ",", "sie", ",", "die", "den", "Glanz", "der", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4het, dort die hohe Wei\u00dfheit wohnen,", "tokens": ["Ver\u00b7schm\u00e4\u00b7het", ",", "dort", "die", "ho\u00b7he", "Wei\u00df\u00b7heit", "woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die an Verstand und Herzen ungekr\u00e4nket,", "tokens": ["Die", "an", "Ver\u00b7stand", "und", "Her\u00b7zen", "un\u00b7ge\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort lebt und denket.", "tokens": ["Dort", "lebt", "und", "den\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Schon vielen Augen hat er Licht gegeben,", "tokens": ["Schon", "vie\u00b7len", "Au\u00b7gen", "hat", "er", "Licht", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einfalt im Denken und Natur im Leben", "tokens": ["Ein\u00b7falt", "im", "Den\u00b7ken", "und", "Na\u00b7tur", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Wei\u00dfheit Sch\u00fclern, die er unterwiesen,", "tokens": ["Der", "Wei\u00df\u00b7heit", "Sch\u00fc\u00b7lern", ",", "die", "er", "un\u00b7ter\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Ernst gepriesen:", "tokens": ["Mit", "Ernst", "ge\u00b7prie\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Mit reiner Lust ihr Leben angef\u00fcllet,", "tokens": ["Mit", "rei\u00b7ner", "Lust", "ihr", "Le\u00b7ben", "an\u00b7ge\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie den Durst nach Wei\u00dfheit, den er stillet,", "tokens": ["Weil", "sie", "den", "Durst", "nach", "Wei\u00df\u00b7heit", ",", "den", "er", "stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch nimmer l\u00f6schet, gl\u00fccklicher als F\u00fcrsten,", "tokens": ["Doch", "nim\u00b7mer", "l\u00f6\u00b7schet", ",", "gl\u00fcck\u00b7li\u00b7cher", "als", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeitlebens d\u00fcrsten:", "tokens": ["Zeit\u00b7le\u00b7bens", "d\u00fcrs\u00b7ten", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Den Tod mit Rosen und Jesmin gezieret,", "tokens": ["Den", "Tod", "mit", "Ro\u00b7sen", "und", "Jes\u00b7min", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NE", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Voll neuer Reitze ihnen zugef\u00fchret,", "tokens": ["Voll", "neu\u00b7er", "Reit\u00b7ze", "ih\u00b7nen", "zu\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie den Retter aus des Lebens Schlingen,", "tokens": ["Da\u00df", "sie", "den", "Ret\u00b7ter", "aus", "des", "Le\u00b7bens", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vertraut umfiengen.", "tokens": ["Ver\u00b7traut", "um\u00b7fi\u00b7en\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Stets wollen wir durch Wei\u00dfheit Ihn erheben,", "tokens": ["Stets", "wol\u00b7len", "wir", "durch", "Wei\u00df\u00b7heit", "Ihn", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn unsern Lehrer, wie er lehrte, leben", "tokens": ["Ihn", "un\u00b7sern", "Leh\u00b7rer", ",", "wie", "er", "lehr\u00b7te", ",", "le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und andre lehren: unsre Kinder sollen", "tokens": ["Und", "and\u00b7re", "leh\u00b7ren", ":", "uns\u00b7re", "Kin\u00b7der", "sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "$.", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch also wollen.", "tokens": ["Auch", "al\u00b7so", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Ihr S\u00f6hne Frankreichs! schm\u00e4ht denn unser Norden,", "tokens": ["Ihr", "S\u00f6h\u00b7ne", "Fran\u00b7kreichs", "!", "schm\u00e4ht", "denn", "un\u00b7ser", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fragt ob Genies je hier erzeuget worden:", "tokens": ["Fragt", "ob", "Ge\u00b7nies", "je", "hier", "er\u00b7zeu\u00b7get", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "ADV", "ADV", "VVFIN", "VAPP", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Wenn Kant noch lebet, werdt ihr diese Fragen", "tokens": ["Wenn", "Kant", "noch", "le\u00b7bet", ",", "werdt", "ihr", "die\u00b7se", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht wieder wagen.", "tokens": ["Nicht", "wie\u00b7der", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Mit \u00e4chterm Ruhm, als unbesiegte Sieger", "tokens": ["Mit", "\u00e4ch\u00b7term", "Ruhm", ",", "als", "un\u00b7be\u00b7sieg\u00b7te", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur gro\u00df an Gl\u00fcck, am Herzen wild als Tiger,", "tokens": ["Nur", "gro\u00df", "an", "Gl\u00fcck", ",", "am", "Her\u00b7zen", "wild", "als", "Ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "APPRART", "NN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch H\u00e4rt und Wuth und unerh\u00f6rtes Schlachten", "tokens": ["Durch", "H\u00e4rt", "und", "Wuth", "und", "un\u00b7er\u00b7h\u00f6r\u