{"dta.poem.17598": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "220.  Wie Gott gefunden wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott recht finden wil/ mu\u00df sich zuvor verliehrn/", "tokens": ["Wer", "Gott", "recht", "fin\u00b7den", "wil", "/", "mu\u00df", "sich", "zu\u00b7vor", "ver\u00b7liehrn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVINF", "VMFIN", "$(", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bi\u00df in Ewigkeit nicht wieder sehn noch sp\u00fcrn.", "tokens": ["Und", "bi\u00df", "in", "E\u00b7wig\u00b7keit", "nicht", "wie\u00b7der", "sehn", "noch", "sp\u00fcrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PTKNEG", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}