{"textgrid.poem.57939": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Auch ein Defizit", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Maschpark ist 'ne sch\u00f6ne Gegend,", "tokens": ["Der", "Maschpark", "ist", "'ne", "sch\u00f6\u00b7ne", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie man sobald noch keine sah,", "tokens": ["Wie", "man", "so\u00b7bald", "noch", "kei\u00b7ne", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ist's nicht, wie bei armen Leuten,", "tokens": ["Da", "ist's", "nicht", ",", "wie", "bei", "ar\u00b7men", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Gegenteil, 's ist alles da.", "tokens": ["Im", "Ge\u00b7gen\u00b7teil", ",", "'s", "ist", "al\u00b7les", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPER", "VAFIN", "PIS", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn da sind B\u00e4ume und Gestr\u00e4uche,", "tokens": ["Denn", "da", "sind", "B\u00e4u\u00b7me", "und", "Ge\u00b7str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch einen See hat man gemacht,", "tokens": ["Auch", "ei\u00b7nen", "See", "hat", "man", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an dem Ufer von demselben", "tokens": ["Und", "an", "dem", "U\u00b7fer", "von", "dem\u00b7sel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist ein Gebirge angebracht.", "tokens": ["Ist", "ein", "Ge\u00b7bir\u00b7ge", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und B\u00e4nke stehen da in Massen,", "tokens": ["Und", "B\u00e4n\u00b7ke", "ste\u00b7hen", "da", "in", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf denen kann man sitzen gehn,", "tokens": ["Auf", "de\u00b7nen", "kann", "man", "sit\u00b7zen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von der Br\u00fccke kann man Fische", "tokens": ["Und", "von", "der", "Br\u00fc\u00b7cke", "kann", "man", "Fi\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Teils f\u00fcttern, teils blo\u00df so besehn.", "tokens": ["Teils", "f\u00fct\u00b7tern", ",", "teils", "blo\u00df", "so", "be\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und Blumen gibt's von allen Sorten,", "tokens": ["Und", "Blu\u00b7men", "gibt's", "von", "al\u00b7len", "Sor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja wohl noch mehr, es ist ein Staat,", "tokens": ["Ja", "wohl", "noch", "mehr", ",", "es", "ist", "ein", "Staat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch etwas fehlt, was man zuweilen", "tokens": ["Doch", "et\u00b7was", "fehlt", ",", "was", "man", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch n\u00f6tiger als diese hat.", "tokens": ["Noch", "n\u00f6\u00b7ti\u00b7ger", "als", "die\u00b7se", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und fehlt es, kann es leicht passieren,", "tokens": ["Und", "fehlt", "es", ",", "kann", "es", "leicht", "pas\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df auf die Gartenkunst man pfeift", "tokens": ["Da\u00df", "auf", "die", "Gar\u00b7ten\u00b7kunst", "man", "pfeift"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und furchtbar pl\u00f6tzlich in die Gegend,", "tokens": ["Und", "furcht\u00b7bar", "pl\u00f6tz\u00b7lich", "in", "die", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo noch Natur ist, w\u00e4rtser l\u00e4uft.", "tokens": ["Wo", "noch", "Na\u00b7tur", "ist", ",", "w\u00e4rt\u00b7ser", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch diese die ist hier vernagelt,", "tokens": ["Doch", "die\u00b7se", "die", "ist", "hier", "ver\u00b7na\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es stehn da Warnungstafeln 'rum,", "tokens": ["Es", "stehn", "da", "War\u00b7nung\u00b7sta\u00b7feln", "'rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und es wird einem immer schlechter,", "tokens": ["Und", "es", "wird", "ei\u00b7nem", "im\u00b7mer", "schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und es wird einem mehr als dumm.", "tokens": ["Und", "es", "wird", "ei\u00b7nem", "mehr", "als", "dumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und man rennt hierhin und rennt dahin,", "tokens": ["Und", "man", "rennt", "hier\u00b7hin", "und", "rennt", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PAV", "KON", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Stirn voll Schwitz, das Auge stier,", "tokens": ["Die", "Stirn", "voll", "Schwitz", ",", "das", "Au\u00b7ge", "stier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nirgendswo kann man erblicken", "tokens": ["Und", "nir\u00b7gends\u00b7wo", "kann", "man", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das hei\u00dfersehnte W\u00f6rtchen: Hier!", "tokens": ["Das", "hei\u00b7\u00dfer\u00b7sehn\u00b7te", "W\u00f6rt\u00b7chen", ":", "Hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Maschpark ist 'ne sch\u00f6ne Gegend,", "tokens": ["Der", "Maschpark", "ist", "'ne", "sch\u00f6\u00b7ne", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iamb