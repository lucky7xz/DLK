{"textgrid.poem.64062": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling ohne Wiederkehr", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich ist des Lenzes erstes L\u00e4cheln,", "tokens": ["Lieb\u00b7lich", "ist", "des", "Len\u00b7zes", "ers\u00b7tes", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn in Bl\u00fcthenb\u00e4umen laue Luft sich wieget,", "tokens": ["Wenn", "in", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7men", "lau\u00b7e", "Luft", "sich", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und des Baches eisbefreite Welle", "tokens": ["Und", "des", "Ba\u00b7ches", "eis\u00b7be\u00b7frei\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht mehr stockend, durch die Fluren rinnt.", "tokens": ["Nicht", "mehr", "sto\u00b7ckend", ",", "durch", "die", "Flu\u00b7ren", "rinnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dann ermuntern sich zu neuem Leben", "tokens": ["Dann", "er\u00b7mun\u00b7tern", "sich", "zu", "neu\u00b7em", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die verblichnen Wiesen aus dem Winterschlafe,", "tokens": ["Die", "ver\u00b7blich\u00b7nen", "Wie\u00b7sen", "aus", "dem", "Win\u00b7ter\u00b7schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und das Gras wacht auf, und decket tr\u00e4umend", "tokens": ["Und", "das", "Gras", "wacht", "auf", ",", "und", "de\u00b7cket", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wiederum den Schooss der Mutter Erde.", "tokens": ["Wie\u00b7de\u00b7rum", "den", "Scho\u00b7oss", "der", "Mut\u00b7ter", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und die Blumen \u00f6ffnen ihre Kelche \u2013", "tokens": ["Und", "die", "Blu\u00b7men", "\u00f6ff\u00b7nen", "ih\u00b7re", "Kel\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle die im sp\u00e4ten Herbste starben", "tokens": ["Al\u00b7le", "die", "im", "sp\u00e4\u00b7ten", "Herbs\u00b7te", "star\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Richten sich aus ihrem dunklen Grabe", "tokens": ["Rich\u00b7ten", "sich", "aus", "ih\u00b7rem", "dunk\u00b7len", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Neu empor im Glanz der Auferstehung.", "tokens": ["Neu", "em\u00b7por", "im", "Glanz", "der", "Auf\u00b7er\u00b7ste\u00b7hung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "O Natur \u2013 wie milde giebst Du wieder", "tokens": ["O", "Na\u00b7tur", "\u2013", "wie", "mil\u00b7de", "giebst", "Du", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "PWAV", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was Dein feierlicher Gang zert\u00f6ret.", "tokens": ["Was", "Dein", "fei\u00b7er\u00b7li\u00b7cher", "Gang", "zer\u00b7t\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fest im stillen, ewig gleichen Kreislauf,", "tokens": ["Fest", "im", "stil\u00b7len", ",", "e\u00b7wig", "glei\u00b7chen", "Kreis\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Folgt auf Deinen Ernst ein mildes L\u00e4cheln.", "tokens": ["Folgt", "auf", "Dei\u00b7nen", "Ernst", "ein", "mil\u00b7des", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht Vernichtung, nur ein leiser Schlummer", "tokens": ["Nicht", "Ver\u00b7nich\u00b7tung", ",", "nur", "ein", "lei\u00b7ser", "Schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "H\u00e4lt des Fr\u00fchlings holde Lust gefangen;", "tokens": ["H\u00e4lt", "des", "Fr\u00fch\u00b7lings", "hol\u00b7de", "Lust", "ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bald, bekr\u00e4nzt mit Veilchen, kehrt er wieder", "tokens": ["Bald", ",", "be\u00b7kr\u00e4nzt", "mit", "Veil\u00b7chen", ",", "kehrt", "er", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "S\u00fcss umhallt von Nachtigallent\u00f6nen.", "tokens": ["S\u00fcss", "um\u00b7hallt", "von", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7t\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Doch wann kehrt ", "tokens": ["Doch", "wann", "kehrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ach, verscheucht hat ihn die Nacht der Trennung", "tokens": ["Ach", ",", "ver\u00b7scheucht", "hat", "ihn", "die", "Nacht", "der", "Tren\u00b7nung"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVPP", "VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Winterschauer einer ew'gen Ferne", "tokens": ["Und", "der", "Win\u00b7ter\u00b7schau\u00b7er", "ei\u00b7ner", "ew'\u00b7gen", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "T\u00f6dtet rauh das zarte Gr\u00fcn der Hoffnung.", "tokens": ["T\u00f6d\u00b7tet", "rauh", "das", "zar\u00b7te", "Gr\u00fcn", "der", "Hoff\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Des Beisammenlebens Stundenblumen", "tokens": ["Des", "Bei\u00b7sam\u00b7men\u00b7le\u00b7bens", "Stun\u00b7den\u00b7blu\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Starben hin im Seufzerhauch des Abschieds.", "tokens": ["Star\u00b7ben", "hin", "im", "Seuf\u00b7zer\u00b7hauch", "des", "Ab\u00b7schieds", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kummervoll benetzt von heissen Thr\u00e4nen,", "tokens": ["Kum\u00b7mer\u00b7voll", "be\u00b7netzt", "von", "heis\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sind der Freude Rosen l\u00e4ngst verblichen.", "tokens": ["Sind", "der", "Freu\u00b7de", "Ro\u00b7sen", "l\u00e4ngst", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Keine Sonne wird sie neu erwecken \u2013", "tokens": ["Kei\u00b7ne", "Son\u00b7ne", "wird", "sie", "neu", "er\u00b7we\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Keines Wiedersehens goldner Schimmer", "tokens": ["Kei\u00b7nes", "Wie\u00b7der\u00b7se\u00b7hens", "gold\u00b7ner", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Winkt des Gl\u00fcckes lichterf\u00fcllte Tage", "tokens": ["Winkt", "des", "Gl\u00fc\u00b7ckes", "lich\u00b7ter\u00b7f\u00fcll\u00b7te", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus dem Grabe der Vergangenheit hervor.", "tokens": ["Aus", "dem", "Gra\u00b7be", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.9": {"line.1": {"text": "Traurig zieht der Jahreszeiten Wechsel", "tokens": ["Trau\u00b7rig", "zieht", "der", "Jah\u00b7res\u00b7zei\u00b7ten", "Wech\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinem still umw\u00f6lkten Blick vor\u00fcber.", "tokens": ["Mei\u00b7nem", "still", "um\u00b7w\u00f6lk\u00b7ten", "Blick", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach es folgt der Fr\u00fchling auf den Winter,", "tokens": ["Ach", "es", "folgt", "der", "Fr\u00fch\u00b7ling", "auf", "den", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aber nimmer kehrt der Liebe Fr\u00fchling wieder!", "tokens": ["A\u00b7ber", "nim\u00b7mer", "kehrt", "der", "Lie\u00b7be", "Fr\u00fch\u00b7ling", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NE", "ADV", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.10": {"line.1": {"text": "Lieblich ist des Lenzes erstes L\u00e4cheln,", "tokens": ["Lieb\u00b7lich", "ist", "des", "Len\u00b7zes", "ers\u00b7tes", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn in Bl\u00fcthenb\u00e4umen laue Luft sich wieget,", "tokens": ["Wenn", "in", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7men", "lau\u00b7e", "Luft", "sich", "wie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und des Baches eisbefreite Welle", "tokens": ["Und", "des", "Ba\u00b7ches", "eis\u00b7be\u00b7frei\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text