{"textgrid.poem.64537": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom ", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Der auf Apollens Pfeil die halbe Welt durchflogen,", "tokens": ["Der", "auf", "A\u00b7pol\u00b7lens", "Pfeil", "die", "hal\u00b7be", "Welt", "durch\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat man vordem so viel gelogen;", "tokens": ["Hat", "man", "vor\u00b7dem", "so", "viel", "ge\u00b7lo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sey auch mir von ihm ein M\u00e4hrchen jetzt verg\u00f6nnt.", "tokens": ["So", "sey", "auch", "mir", "von", "ihm", "ein", "M\u00e4hr\u00b7chen", "jetzt", "ver\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Athen h\u00f6rt' ihn bewundernd an,", "tokens": ["A\u00b7then", "h\u00f6rt'", "ihn", "be\u00b7wun\u00b7dernd", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Denn attisch war an ihm selbst Ausdruck, Witz und Sitten,", "tokens": ["Denn", "at\u00b7tisch", "war", "an", "ihm", "selbst", "Aus\u00b7druck", ",", "Witz", "und", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PPER", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "In Versen sprach sogar der Mann!", "tokens": ["In", "Ver\u00b7sen", "sprach", "so\u00b7gar", "der", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist's m\u00f6glich? k\u00f6nnen das denn Scythen?", "tokens": ["Ist's", "m\u00f6g\u00b7lich", "?", "k\u00f6n\u00b7nen", "das", "denn", "Scyt\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VMFIN", "PDS", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O ja! dem Scythen auch ist muntrer Witz verliehn:", "tokens": ["O", "ja", "!", "dem", "Scyt\u00b7hen", "auch", "ist", "mun\u00b7trer", "Witz", "ver\u00b7liehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "ADV", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch Wahrheit ohne Schmuck hat immer Reiz f\u00fcr ihn;", "tokens": ["Doch", "Wahr\u00b7heit", "oh\u00b7ne", "Schmuck", "hat", "im\u00b7mer", "Reiz", "f\u00fcr", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VAFIN", "ADV", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "li