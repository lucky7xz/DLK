{"dta.poem.8023": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Traum .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.85", "sv:0.14"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat mir j\u00fcngst getr\u00e4umet,", "tokens": ["Es", "hat", "mir", "j\u00fcngst", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich l\u00e4g\u2019 auf steiler H\u00f6h\u2019;", "tokens": ["Ich", "l\u00e4g'", "auf", "stei\u00b7ler", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es war am Meeresstrande,", "tokens": ["Es", "war", "am", "Mee\u00b7res\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sah wohl in die Lande", "tokens": ["Ich", "sah", "wohl", "in", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcber die weite See.", "tokens": ["Und", "\u00fc\u00b7ber", "die", "wei\u00b7te", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es lag am Ufer drunten", "tokens": ["Es", "lag", "am", "U\u00b7fer", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schmuckes Schiff bereit,", "tokens": ["Ein", "schmu\u00b7ckes", "Schiff", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit bunten Wimpeln wehend,", "tokens": ["Mit", "bun\u00b7ten", "Wim\u00b7peln", "we\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Ferg\u2019 am Ufer stehend,", "tokens": ["Der", "Fer\u00b7g'", "am", "U\u00b7fer", "ste\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als w\u00e4r\u2019 ihm lang die Zeit.", "tokens": ["Als", "w\u00e4r'", "ihm", "lang", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da kam von fernen Bergen", "tokens": ["Da", "kam", "von", "fer\u00b7nen", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein lust\u2019ger Zug daher.", "tokens": ["Ein", "lust'\u00b7ger", "Zug", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Engel th\u00e4ten sie gl\u00e4nzen,", "tokens": ["Wie", "En\u00b7gel", "th\u00e4\u00b7ten", "sie", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Geschm\u00fcckt mit Blumenkr\u00e4nzen,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zogen nach dem Meer.", "tokens": ["Und", "zo\u00b7gen", "nach", "dem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Voran dem Zuge schw\u00e4rmten", "tokens": ["Vo\u00b7ran", "dem", "Zu\u00b7ge", "schw\u00e4rm\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der muntern Kinder viel.", "tokens": ["Der", "mun\u00b7tern", "Kin\u00b7der", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Andern Becher schwangen,", "tokens": ["Die", "An\u00b7dern", "Be\u00b7cher", "schwan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Musizirten, sangen,", "tokens": ["Mu\u00b7si\u00b7zir\u00b7ten", ",", "san\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schwebten in Tanz und Spiel.", "tokens": ["Schweb\u00b7ten", "in", "Tanz", "und", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Sie sprachen zu dem Schiffer:", "tokens": ["Sie", "spra\u00b7chen", "zu", "dem", "Schif\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ewillt du uns f\u00fchren gern?", "tokens": ["\u201e", "willt", "du", "uns", "f\u00fch\u00b7ren", "gern", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sind die Wonnen und Freuden,", "tokens": ["Wir", "sind", "die", "Won\u00b7nen", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wollen von der Erde scheiden,", "tokens": ["Wol\u00b7len", "von", "der", "Er\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "All von der Erde fern.\u201c", "tokens": ["All", "von", "der", "Er\u00b7de", "fern", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Er hie\u00df in\u2019s Schiff sie treten,", "tokens": ["Er", "hie\u00df", "in's", "Schiff", "sie", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freuden allzumal,", "tokens": ["Die", "Freu\u00b7den", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach: \u201eSagt an, ihr Lieben!", "tokens": ["Er", "sprach", ":", "\u201e", "Sagt", "an", ",", "ihr", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist Keins zur\u00fcckgeblieben", "tokens": ["Ist", "Keins", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf Bergen, noch im Thal?\u201c", "tokens": ["Auf", "Ber\u00b7gen", ",", "noch", "im", "Thal", "?", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie riefen: \u201eWir sind Alle!", "tokens": ["Sie", "rie\u00b7fen", ":", "\u201e", "Wir", "sind", "Al\u00b7le", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fahr zu, wir haben Eil\u2019!\u201c", "tokens": ["Fahr", "zu", ",", "wir", "ha\u00b7ben", "Eil'", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fuhren mit frischen Winden,", "tokens": ["Sie", "fuh\u00b7ren", "mit", "fri\u00b7schen", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Fern, ferne sah ich schwinden", "tokens": ["Fern", ",", "fer\u00b7ne", "sah", "ich", "schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Erde Lust und Heil.", "tokens": ["Der", "Er\u00b7de", "Lust", "und", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}