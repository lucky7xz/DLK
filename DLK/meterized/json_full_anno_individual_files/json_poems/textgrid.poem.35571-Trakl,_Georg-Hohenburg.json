{"textgrid.poem.35571": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Hohenburg", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist niemand im Haus. Herbst in Zimmern;", "tokens": ["Es", "ist", "nie\u00b7mand", "im", "Haus", ".", "Herbst", "in", "Zim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPRART", "NN", "$.", "NN", "APPR", "NN", "$."], "meter": "--+--++-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mondeshelle Sonate", "tokens": ["Mon\u00b7des\u00b7hel\u00b7le", "So\u00b7na\u00b7te"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und das Erwachen am Saum des d\u00e4mmernden Walds.", "tokens": ["Und", "das", "Er\u00b7wa\u00b7chen", "am", "Saum", "des", "d\u00e4m\u00b7mern\u00b7den", "Walds", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "---+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Immer denkst du das wei\u00dfe Antlitz des Menschen", "tokens": ["Im\u00b7mer", "denkst", "du", "das", "wei\u00b7\u00dfe", "Ant\u00b7litz", "des", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ferne dem Get\u00fcmmel der Zeit;", "tokens": ["Fer\u00b7ne", "dem", "Ge\u00b7t\u00fcm\u00b7mel", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00dcber ein Tr\u00e4umendes neigt sich gerne gr\u00fcnes Gezweig,", "tokens": ["\u00dc\u00b7ber", "ein", "Tr\u00e4u\u00b7men\u00b7des", "neigt", "sich", "ger\u00b7ne", "gr\u00fc\u00b7nes", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Kreuz und Abend;", "tokens": ["Kreuz", "und", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Umf\u00e4ngt den T\u00f6nenden mit purpurnen Armen sein Stern,", "tokens": ["Um\u00b7f\u00e4ngt", "den", "T\u00f6\u00b7nen\u00b7den", "mit", "pur\u00b7pur\u00b7nen", "Ar\u00b7men", "sein", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der zu unbewohnten Fenstern hinaufsteigt.", "tokens": ["Der", "zu", "un\u00b7be\u00b7wohn\u00b7ten", "Fens\u00b7tern", "hin\u00b7auf\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Also zittert im Dunkel der Fremdling,", "tokens": ["Al\u00b7so", "zit\u00b7tert", "im", "Dun\u00b7kel", "der", "Fremd\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da er leise die Lider \u00fcber ein Menschliches aufhebt,", "tokens": ["Da", "er", "lei\u00b7se", "die", "Li\u00b7der"