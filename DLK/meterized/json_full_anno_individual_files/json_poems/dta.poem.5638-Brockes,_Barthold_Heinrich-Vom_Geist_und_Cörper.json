{"dta.poem.5638": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch eh' wir C\u00f6rper selbst betrachten und besehen,", "tokens": ["Noch", "eh'", "wir", "C\u00f6r\u00b7per", "selbst", "be\u00b7trach\u00b7ten", "und", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "ADV", "VVINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sp\u00fcrt der Mensch in sich ein innerliches Dencken,", "tokens": ["So", "sp\u00fcrt", "der", "Mensch", "in", "sich", "ein", "in\u00b7ner\u00b7li\u00b7ches", "Den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn von den Sinnlichen, wie sie sich etwa lencken,", "tokens": ["Denn", "von", "den", "Sinn\u00b7li\u00b7chen", ",", "wie", "sie", "sich", "et\u00b7wa", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Empfinden wir in uns noch Spuren, noch Jdeen", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "in", "uns", "noch", "Spu\u00b7ren", ",", "noch", "Jdeen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPER", "ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Die \u00e4lter sind, als das. Wenn man die Wissenschafft", "tokens": ["Die", "\u00e4l\u00b7ter", "sind", ",", "als", "das", ".", "Wenn", "man", "die", "Wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PDS", "$.", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ohn uns recht anzusehn, mit diesem Satz verwirrte:", "tokens": ["Ohn", "uns", "recht", "an\u00b7zu\u00b7sehn", ",", "mit", "die\u00b7sem", "Satz", "ver\u00b7wirr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "VVIZU", "$,", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Sinne W\u00fcrckungen und der Gedancken Krafft", "tokens": ["Der", "Sin\u00b7ne", "W\u00fcr\u00b7ckun\u00b7gen", "und", "der", "Ge\u00b7dan\u00b7cken", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN"], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sey ein gelinder Wind, ein leichtes Feu\u2019r; so irr\u2019te", "tokens": ["Sey", "ein", "ge\u00b7lin\u00b7der", "Wind", ",", "ein", "leich\u00b7tes", "Feu'r", ";", "so", "irr'\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man grob und freventlich. Die\u00df ist der erste Schlu\u00df,", "tokens": ["Man", "grob", "und", "fre\u00b7vent\u00b7lich", ".", "Die\u00df", "ist", "der", "ers\u00b7te", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJD", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den die Vernunfft uns machen mu\u00df,", "tokens": ["Den", "die", "Ver\u00b7nunfft", "uns", "ma\u00b7chen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Will sie sich anders nicht vom Pfad der Warheit lencken:", "tokens": ["Will", "sie", "sich", "an\u00b7ders", "nicht", "vom", "Pfad", "der", "War\u00b7heit", "len\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Menschen ", "tokens": ["Des", "Men\u00b7schen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "dencken.", "tokens": ["den\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}