", "measure": "iambic.tetra"}, "line.4": {"text": "Mein M\u00e4dchen mir durch manchen Ku\u00df.", "tokens": ["Mein", "M\u00e4d\u00b7chen", "mir", "durch", "man\u00b7chen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Oftmal, mir selber unbekannt,", "tokens": ["Oft\u00b7mal", ",", "mir", "sel\u00b7ber", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcckt meine Hand dann ihre Hand;", "tokens": ["Dr\u00fcckt", "mei\u00b7ne", "Hand", "dann", "ih\u00b7re", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl's, und seufze, da\u00df ihr Bild", "tokens": ["Ich", "f\u00fchl's", ",", "und", "seuf\u00b7ze", ",", "da\u00df", "ihr", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den hei\u00dfen Wunsch so schwach erf\u00fcllt.", "tokens": ["Den", "hei\u00b7\u00dfen", "Wunsch", "so", "schwach", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So sehnlich sucht' ich, und so lang'!", "tokens": ["So", "sehn\u00b7lich", "sucht'", "ich", ",", "und", "so", "lang'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "po