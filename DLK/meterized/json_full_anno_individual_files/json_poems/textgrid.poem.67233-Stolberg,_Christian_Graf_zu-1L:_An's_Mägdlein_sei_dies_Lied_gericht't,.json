ns": ["Wie", "ich", "ihm", "na\u00b7ch\u00b7emp\u00b7fin\u00b7den", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "So glaub', da\u00df du mein M\u00e4dchen bist,", "tokens": ["So", "glaub'", ",", "da\u00df", "du", "mein", "M\u00e4d\u00b7chen", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nur f\u00fcr mich geboren ist,", "tokens": ["Das", "nur", "f\u00fcr", "mich", "ge\u00b7bo\u00b7ren", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und liebe mich, und sag' es mir,", "tokens": ["Und", "lie\u00b7be", "mich", ",", "und", "sag'", "es", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So eil' ich, Beste, froh zu dir!", "tokens": ["So", "eil'", "ich", ",", "Bes\u00b7te", ",", "froh", "zu", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}