{"dta.poem.3620": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach triumphir nicht vor dem Siege,               ", "tokens": ["Ach", "tri\u00b7um\u00b7phir", "nicht", "vor", "dem", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Seel wo willt du fliehen hin;", "tokens": ["O", "Seel", "wo", "willt", "du", "flie\u00b7hen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PWAV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dein verblendter Eigensinn", "tokens": ["Da", "dein", "ver\u00b7blend\u00b7ter", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Feinden frey und sicher liege.", "tokens": ["Vor", "Fein\u00b7den", "frey", "und", "si\u00b7cher", "lie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Suchst du noch Ruh in \u00e4ussern Dingen,", "tokens": ["Suchst", "du", "noch", "Ruh", "in", "\u00e4us\u00b7sern", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach glaube mir, du findst sie nicht;", "tokens": ["Ach", "glau\u00b7be", "mir", ",", "du", "findst", "sie", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wirst du nicht nach dem Innern ringen,", "tokens": ["Wirst", "du", "nicht", "nach", "dem", "In\u00b7nern", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ists mit dem nicht ausgericht.", "tokens": ["So", "ists", "mit", "dem", "nicht", "aus\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PRELS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drum bleib nun im Gehorsam stehen,", "tokens": ["Drum", "bleib", "nun", "im", "Ge\u00b7hor\u00b7sam", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Kriegsmann weicht von seinem Post;", "tokens": ["Kein", "Kriegs\u00b7mann", "weicht", "von", "sei\u00b7nem", "Post", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenns auch schon Blut und Leben kost,", "tokens": ["Wenns", "auch", "schon", "Blut", "und", "Le\u00b7ben", "kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn sein Herr dahin hei\u00dft gehen.", "tokens": ["Wenn", "ihn", "sein", "Herr", "da\u00b7hin", "hei\u00dft", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PAV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Glaub wei\u00df nichts von eignem Willen,", "tokens": ["Der", "Glaub", "wei\u00df", "nichts", "von", "eig\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er sieht sich selbst den Weg nicht aus,", "tokens": ["Er", "sieht", "sich", "selbst", "den", "Weg", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dadurch er Gottes Will erf\u00fcllen,", "tokens": ["Da\u00b7durch", "er", "Got\u00b7tes", "Will", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und aus dem Streit will kommen raus.", "tokens": ["Und", "aus", "dem", "Streit", "will", "kom\u00b7men", "raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist dir selbst die gr\u00f6\u00dfte Plage,", "tokens": ["Du", "bist", "dir", "selbst", "die", "gr\u00f6\u00df\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du tr\u00e4gst noch Babel stets in dir;", "tokens": ["Du", "tr\u00e4gst", "noch", "Ba\u00b7bel", "stets", "in", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willt du noch Ruh genie\u00dfen hier,", "tokens": ["Willt", "du", "noch", "Ruh", "ge\u00b7nie\u00b7\u00dfen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df dir keine s\u00fc\u00dfe Tage", "tokens": ["So", "la\u00df", "dir", "kei\u00b7ne", "s\u00fc\u00b7\u00dfe", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch s\u00fc\u00dfe Tr\u00e4ume hier verlegen,", "tokens": ["Durch", "s\u00fc\u00b7\u00dfe", "Tr\u00e4u\u00b7me", "hier", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du machst dich nur mehr misvergn\u00fcgt;", "tokens": ["Du", "machst", "dich", "nur", "mehr", "mis\u00b7ver\u00b7gn\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der liebe Jesu wird dich hegen,", "tokens": ["Der", "lie\u00b7be", "Je\u00b7su", "wird", "dich", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der alles Wissen \u00fcberwiegt.", "tokens": ["Der", "al\u00b7les", "Wis\u00b7sen", "\u00fc\u00b7berw\u00b7iegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du kannst auch mitten im Get\u00fcmmel", "tokens": ["Du", "kannst", "auch", "mit\u00b7ten", "im", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Welt, den Vater beten an;", "tokens": ["Der", "Welt", ",", "den", "Va\u00b7ter", "be\u00b7ten", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der dich ja bald erl\u00f6sen kann,", "tokens": ["Der", "dich", "ja", "bald", "er\u00b7l\u00f6\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn dir erst n\u00fctze jener Himmel", "tokens": ["Wenn", "dir", "erst", "n\u00fct\u00b7ze", "je\u00b7ner", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dich Egypten nicht sollt \u00fcben,", "tokens": ["Und", "dich", "E\u00b7gyp\u00b7ten", "nicht", "sollt", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df deiner Treiber schweres Joch", "tokens": ["Da\u00df", "dei\u00b7ner", "Trei\u00b7ber", "schwe\u00b7res", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich lehrte recht den Himmel lieben,", "tokens": ["Dich", "lehr\u00b7te", "recht", "den", "Him\u00b7mel", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dein Verlangen stillte noch.", "tokens": ["Und", "dein", "Ver\u00b7lan\u00b7gen", "still\u00b7te", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier ist kein Canaan zu hoffen,", "tokens": ["Hier", "ist", "kein", "Ca\u00b7naan", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Kein Paradies ist mehr allhier;", "tokens": ["Kein", "Pa\u00b7ra\u00b7dies", "ist", "mehr", "all\u00b7hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat noch niemand der mit dir", "tokens": ["Es", "hat", "noch", "nie\u00b7mand", "der", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ART", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entfliehen will, den Zweck getroffen.", "tokens": ["Ent\u00b7flie\u00b7hen", "will", ",", "den", "Zweck", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Hoffnung n\u00e4hrt sich mit den Dingen,", "tokens": ["Die", "Hoff\u00b7nung", "n\u00e4hrt", "sich", "mit", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die s\u00fc\u00df und doch unsichtbar sind;", "tokens": ["Die", "s\u00fc\u00df", "und", "doch", "un\u00b7sicht\u00b7bar", "sind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Es mu\u00df uns doch zulezt gelingen,", "tokens": ["Es", "mu\u00df", "uns", "doch", "zu\u00b7lezt", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bleib nur in Einfalt Gottes Kind.", "tokens": ["Bleib", "nur", "in", "Ein\u00b7falt", "Got\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur freue dich auf jene Kammer", "tokens": ["Nur", "freu\u00b7e", "dich", "auf", "je\u00b7ne", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Friedens, da du wohnen wirst,", "tokens": ["Des", "Frie\u00b7dens", ",", "da", "du", "woh\u00b7nen", "wirst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich nicht mehr nach Ruhe d\u00fcrst,", "tokens": ["Wenn", "dich", "nicht", "mehr", "nach", "Ru\u00b7he", "d\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bist befreyt von allem Jammer,", "tokens": ["Und", "bist", "be\u00b7freyt", "von", "al\u00b7lem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den hier noch St\u00e4dt und W\u00fcsten haben,", "tokens": ["Den", "hier", "noch", "St\u00e4dt", "und", "W\u00fcs\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo du nur willt fliehen hin;", "tokens": ["Und", "wo", "du", "nur", "willt", "flie\u00b7hen", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Einsamkeit kann dich nicht laben,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "kann", "dich", "nicht", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn mit dir zieht dein Eigensinn.", "tokens": ["Wenn", "mit", "dir", "zieht", "dein", "Ei\u00b7gen\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}