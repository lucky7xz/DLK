{"textgrid.poem.64333": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "54. Die Meere", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schmeichelst mein Ohr,", "tokens": ["Du", "schmei\u00b7chelst", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich kenne dein Rauschen,", "tokens": ["Ich", "ken\u00b7ne", "dein", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Deiner Wogen Sirenen-Gesang!", "tokens": ["Dei\u00b7ner", "Wo\u00b7gen", "Si\u00b7re\u00b7nen\u00b7Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ost-See, du nahmst mich", "tokens": ["Ost\u00b7See", ",", "du", "nahmst", "mich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Oft mit kosenden Armen", "tokens": ["Oft", "mit", "ko\u00b7sen\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "In den k\u00fchlenden Scho\u00df!", "tokens": ["In", "den", "k\u00fch\u00b7len\u00b7den", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Du bist sch\u00f6n!", "tokens": ["Du", "bist", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nymphe, sch\u00f6n!", "tokens": ["Nym\u00b7phe", ",", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vertraute des waldichten Ufers,", "tokens": ["Ver\u00b7trau\u00b7te", "des", "wal\u00b7dich\u00b7ten", "U\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Oft entschl\u00fcpfet der West den Wipfeln des Hains,", "tokens": ["Oft", "ent\u00b7schl\u00fcp\u00b7fet", "der", "West", "den", "Wip\u00b7feln", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und schwebet \u00fcber dir hin mit gleitendem Flug", "tokens": ["Und", "schwe\u00b7bet", "\u00fc\u00b7ber", "dir", "hin", "mit", "glei\u00b7ten\u00b7dem", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Du bist sch\u00f6n,", "tokens": ["Du", "bist", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nymphe, sch\u00f6n,", "tokens": ["Nym\u00b7phe", ",", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Aber die G\u00f6ttin sch\u00f6ner als du!", "tokens": ["A\u00b7ber", "die", "G\u00f6t\u00b7tin", "sch\u00f6\u00b7ner", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KOKOM", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Lauter, als du,", "tokens": ["Lau\u00b7ter", ",", "als", "du", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Donnert die Nord-See,", "tokens": ["Don\u00b7nert", "die", "Nord\u00b7See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+--++", "measure": "adoneus"}, "line.6": {"text": "Steigend erhebt sich und wei\u00df und gestadersch\u00fctternd ihr Fu\u00df!", "tokens": ["Stei\u00b7gend", "er\u00b7hebt", "sich", "und", "wei\u00df", "und", "ge\u00b7sta\u00b7der\u00b7sch\u00fct\u00b7ternd", "ihr", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "KON", "VVFIN", "KON", "VVPP", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "St\u00e4rker und freier, als du,", "tokens": ["St\u00e4r\u00b7ker", "und", "frei\u00b7er", ",", "als", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Tanzet sie eignen Tanz,", "tokens": ["Tan\u00b7zet", "sie", "eig\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Lauschet nicht dienstbar der Stimme", "tokens": ["Lau\u00b7schet", "nicht", "dienst\u00b7bar", "der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJD", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Herrschender Winde;", "tokens": ["Herr\u00b7schen\u00b7der", "Win\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Steiget und sinkt,", "tokens": ["Stei\u00b7get", "und", "sinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wenn, mit Wolken umschleiert,", "tokens": ["Wenn", ",", "mit", "Wol\u00b7ken", "um\u00b7schlei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "In geheimer Halle schlummert des Sturmes Haupt!", "tokens": ["In", "ge\u00b7hei\u00b7mer", "Hal\u00b7le", "schlum\u00b7mert", "des", "Stur\u00b7mes", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich sah die Kiele", "tokens": ["Ich", "sah", "die", "Kie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Blitzgewaffneter Schiffe", "tokens": ["Blitz\u00b7ge\u00b7waff\u00b7ne\u00b7ter", "Schif\u00b7fe"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Eilen \u00fcber ihr hin,", "tokens": ["Ei\u00b7len", "\u00fc\u00b7ber", "ihr", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wenn die Flagge sank,", "tokens": ["Wenn", "die", "Flag\u00b7ge", "sank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und der z\u00fcngelnde Wimpel sank", "tokens": ["Und", "der", "z\u00fcn\u00b7geln\u00b7de", "Wim\u00b7pel", "sank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und das S\u00e4useln in Hellebeks Buchen schwieg.", "tokens": ["Und", "das", "S\u00e4u\u00b7seln", "in", "Hel\u00b7le\u00b7beks", "Bu\u00b7chen", "schwieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wie nennet dich mein Gesang!", "tokens": ["Wie", "nen\u00b7net", "dich", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Nord-Meer, Welt-Meer, G\u00f6ttin, Unendliche,", "tokens": ["Nord\u00b7Meer", ",", "Welt\u00b7Meer", ",", "G\u00f6t\u00b7tin", ",", "Un\u00b7end\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "++-++--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Erdumg\u00fcrtende, Wiege der allerleuchtenden", "tokens": ["Er\u00b7dum\u00b7g\u00fcr\u00b7ten\u00b7de", ",", "Wie\u00b7ge", "der", "al\u00b7ler\u00b7leuch\u00b7ten\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ART", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sonne, des himmelwandelnden", "tokens": ["Son\u00b7ne", ",", "des", "him\u00b7mel\u00b7wan\u00b7deln\u00b7den"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ART", "NN"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.5": {"text": "Mondes und zahlloser", "tokens": ["Mon\u00b7des", "und", "zahl\u00b7lo\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-++--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Sterne, die in melodischem", "tokens": ["Ster\u00b7ne", ",", "die", "in", "me\u00b7lo\u00b7di\u00b7schem"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tanze sich spiegeln, wenn steiget die Well' und hinab sich senkt.", "tokens": ["Tan\u00b7ze", "sich", "spie\u00b7geln", ",", "wenn", "stei\u00b7get", "die", "Well'", "und", "hin\u00b7ab", "sich", "senkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$,", "KOUS", "VVFIN", "ART", "NN", "KON", "ADV", "PRF", "VVFIN", "$."], "meter": "+--+--+--+--+-+", "measure": "diphilius"}}, "stanza.7": {"line.1": {"text": "Auf deinen Wassern", "tokens": ["Auf", "dei\u00b7nen", "Was\u00b7sern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schwebete Gottes Geist,", "tokens": ["Schwe\u00b7be\u00b7te", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Als noch die Erde", "tokens": ["Als", "noch", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lag in traurender Stille,", "tokens": ["Lag", "in", "trau\u00b7ren\u00b7der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Mutterfreuden kannte noch nicht!", "tokens": ["Mut\u00b7ter\u00b7freu\u00b7den", "kann\u00b7te", "noch", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "\u00dcber dir wehet,", "tokens": ["\u00dc\u00b7ber", "dir", "we\u00b7het", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Hehr und geheimnisvoll,", "tokens": ["Hehr", "und", "ge\u00b7heim\u00b7nis\u00b7voll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Flutend und ebbend,", "tokens": ["Flu\u00b7tend", "und", "eb\u00b7bend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Sichtbar noch des Allm\u00e4chtigen Hauch!", "tokens": ["Sicht\u00b7bar", "noch", "des", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Hauch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Auf hoher Entz\u00fcckung", "tokens": ["Auf", "ho\u00b7her", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steigendem Fl\u00fcgel", "tokens": ["Stei\u00b7gen\u00b7dem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Flog dir entgegen mein Geist;", "tokens": ["Flog", "dir", "ent\u00b7ge\u00b7gen", "mein", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "G\u00f6ttin, ich flehte:", "tokens": ["G\u00f6t\u00b7tin", ",", "ich", "fleh\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Nimm mich, o G\u00f6ttin,", "tokens": ["Nimm", "mich", ",", "o", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nimm mich in deinen m\u00e4chtigen Scho\u00df!", "tokens": ["Nimm", "mich", "in", "dei\u00b7nen", "m\u00e4ch\u00b7ti\u00b7gen", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Aber du eiltest", "tokens": ["A\u00b7ber", "du", "eil\u00b7test"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Stolz mir und donnernd vorbei,", "tokens": ["Stolz", "mir", "und", "don\u00b7nernd", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Da spannt' ich die Fl\u00fcgel", "tokens": ["Da", "spannt'", "ich", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Des Wogendurchwallers,", "tokens": ["Des", "Wo\u00b7gen\u00b7durch\u00b7wal\u00b7lers", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und schwebte zum ferneren Ufer hin.", "tokens": ["Und", "schweb\u00b7te", "zum", "fer\u00b7ne\u00b7ren", "U\u00b7fer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Du donnertest lauter", "tokens": ["Du", "don\u00b7ner\u00b7test", "lau\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Am Felsen-Gestade,", "tokens": ["Am", "Fel\u00b7sen\u00b7Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Ich eilte hinan", "tokens": ["Ich", "eil\u00b7te", "hi\u00b7nan"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Das Felsen-Gestade,", "tokens": ["Das", "Fel\u00b7sen\u00b7Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Und eilte hinab,", "tokens": ["Und", "eil\u00b7te", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Da fa\u00dft' ich dich, G\u00f6ttin,", "tokens": ["Da", "fa\u00dft'", "ich", "dich", ",", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Mit nervichtem Arm,", "tokens": ["Mit", "ner\u00b7vich\u00b7tem", "Arm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "In der Felsen-Halle!", "tokens": ["In", "der", "Fel\u00b7sen\u00b7Hal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "\u00dcber mir hingen", "tokens": ["\u00dc\u00b7ber", "mir", "hin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Dr\u00e4uende Gipfel;", "tokens": ["Dr\u00e4u\u00b7en\u00b7de", "Gip\u00b7fel", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Strudelnde Fluten", "tokens": ["Stru\u00b7deln\u00b7de", "Flu\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Dr\u00e4ngten durch Kl\u00fcfte der Felsen sich durch!", "tokens": ["Dr\u00e4ng\u00b7ten", "durch", "Kl\u00fcf\u00b7te", "der", "Fel\u00b7sen", "sich", "durch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "PRF", "APPR", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.24": {"text": "Und wohl mir ward", "tokens": ["Und", "wohl", "mir", "ward"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "In der G\u00f6ttin Scho\u00df,", "tokens": ["In", "der", "G\u00f6t\u00b7tin", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.26": {"text": "An der Unsterblichen", "tokens": ["An", "der", "U\u00b7nsterb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+---", "measure": "anapaest.init"}, "line.27": {"text": "Wallenden Busen!", "tokens": ["Wal\u00b7len\u00b7den", "Bu\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Heil dir, Heil,", "tokens": ["Heil", "dir", ",", "Heil", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "G\u00f6ttin, und Dank,", "tokens": ["G\u00f6t\u00b7tin", ",", "und", "Dank", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "F\u00fcr den seligen Genu\u00df", "tokens": ["F\u00fcr", "den", "se\u00b7li\u00b7gen", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "In der Felsen-Halle!", "tokens": ["In", "der", "Fel\u00b7sen\u00b7Hal\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Du schmeichelst mein Ohr,", "tokens": ["Du", "schmei\u00b7chelst", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ich kenne dein Rauschen,", "tokens": ["Ich", "ken\u00b7ne", "dein", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Deiner Wogen Sirenen-Gesang!", "tokens": ["Dei\u00b7ner", "Wo\u00b7gen", "Si\u00b7re\u00b7nen\u00b7Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ost-See, du nahmst mich", "tokens": ["Ost\u00b7See", ",", "du", "nahmst", "mich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Oft mit kosenden Armen", "tokens": ["Oft", "mit", "ko\u00b7sen\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "In den k\u00fchlenden Scho\u00df!", "tokens": ["In", "den", "k\u00fch\u00b7len\u00b7den", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Du bist sch\u00f6n!", "tokens": ["Du", "bist", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nymphe, sch\u00f6n!", "tokens": ["Nym\u00b7phe", ",", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vertraute des waldichten Ufers,", "tokens": ["Ver\u00b7trau\u00b7te", "des", "wal\u00b7dich\u00b7ten", "U\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Oft entschl\u00fcpfet der West den Wipfeln des Hains,", "tokens": ["Oft", "ent\u00b7schl\u00fcp\u00b7fet", "der", "West", "den", "Wip\u00b7feln", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und schwebet \u00fcber dir hin mit gleitendem Flug", "tokens": ["Und", "schwe\u00b7bet", "\u00fc\u00b7ber", "dir", "hin", "mit", "glei\u00b7ten\u00b7dem", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Du bist sch\u00f6n,", "tokens": ["Du", "bist", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Nymphe, sch\u00f6n,", "tokens": ["Nym\u00b7phe", ",", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Aber die G\u00f6ttin sch\u00f6ner als du!", "tokens": ["A\u00b7ber", "die", "G\u00f6t\u00b7tin", "sch\u00f6\u00b7ner", "als", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KOKOM", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Lauter, als du,", "tokens": ["Lau\u00b7ter", ",", "als", "du", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Donnert die Nord-See,", "tokens": ["Don\u00b7nert", "die", "Nord\u00b7See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+--++", "measure": "adoneus"}, "line.6": {"text": "Steigend erhebt sich und wei\u00df und gestadersch\u00fctternd ihr Fu\u00df!", "tokens": ["Stei\u00b7gend", "er\u00b7hebt", "sich", "und", "wei\u00df", "und", "ge\u00b7sta\u00b7der\u00b7sch\u00fct\u00b7ternd", "ihr", "Fu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "KON", "VVFIN", "KON", "VVPP", "PPOSAT", "NN", "$."], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}}, "stanza.12": {"line.1": {"text": "St\u00e4rker und freier, als du,", "tokens": ["St\u00e4r\u00b7ker", "und", "frei\u00b7er", ",", "als", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Tanzet sie eignen Tanz,", "tokens": ["Tan\u00b7zet", "sie", "eig\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Lauschet nicht dienstbar der Stimme", "tokens": ["Lau\u00b7schet", "nicht", "dienst\u00b7bar", "der", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJD", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Herrschender Winde;", "tokens": ["Herr\u00b7schen\u00b7der", "Win\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Steiget und sinkt,", "tokens": ["Stei\u00b7get", "und", "sinkt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wenn, mit Wolken umschleiert,", "tokens": ["Wenn", ",", "mit", "Wol\u00b7ken", "um\u00b7schlei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "VV