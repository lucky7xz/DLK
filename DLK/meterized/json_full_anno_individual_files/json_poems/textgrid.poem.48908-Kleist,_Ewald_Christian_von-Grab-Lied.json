{"textgrid.poem.48908": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Grab-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1757, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh dir! da\u00df du gestorben bist.", "tokens": ["Weh", "dir", "!", "da\u00df", "du", "ge\u00b7stor\u00b7ben", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst nicht mehr Auroren sehn", "tokens": ["Du", "wirst", "nicht", "mehr", "Au\u00b7ro\u00b7ren", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie vom Morgen Himmel blickt", "tokens": ["Wenn", "sie", "vom", "Mor\u00b7gen", "Him\u00b7mel", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In rother Tracht, mit g\u00fcldnem Haar;", "tokens": ["In", "ro\u00b7ther", "Tracht", ",", "mit", "g\u00fcld\u00b7nem", "Haar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die bethauten Wiesen nicht,", "tokens": ["Und", "die", "be\u00b7thau\u00b7ten", "Wie\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch nicht im melancholschen Hayn", "tokens": ["Auch", "nicht", "im", "me\u00b7lan