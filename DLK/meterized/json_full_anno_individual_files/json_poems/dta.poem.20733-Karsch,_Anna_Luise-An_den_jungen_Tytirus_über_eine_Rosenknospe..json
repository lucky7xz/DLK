{"dta.poem.20733": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den jungen Tytirus  \n \u00fcber eine Rosenknospe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Busen ist sie mir gestorben               ", "tokens": ["Am", "Bu\u00b7sen", "ist", "sie", "mir", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Rosenknospe, die von dir", "tokens": ["Die", "Ro\u00b7sen\u00b7knos\u00b7pe", ",", "die", "von", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Freund durch seinen Blick erworben,", "tokens": ["Mein", "Freund", "durch", "sei\u00b7nen", "Blick", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kannst du glauben, da\u00df man ihr", "tokens": ["Und", "kannst", "du", "glau\u00b7ben", ",", "da\u00df", "man", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein be\u00dfres Schicksal w\u00fcnschen konnte,", "tokens": ["Kein", "be\u00df\u00b7res", "Schick\u00b7sal", "w\u00fcn\u00b7schen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als diesen Platz, den ich ihr gonnte,", "tokens": ["Als", "die\u00b7sen", "Platz", ",", "den", "ich", "ihr", "gonn\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der bald vielleicht verdorren mu\u00df,", "tokens": ["Der", "bald", "viel\u00b7leicht", "ver\u00b7dor\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dem manch M\u00e4dchenherz entglommen.", "tokens": ["Von", "dem", "manch", "M\u00e4d\u00b7chen\u00b7herz", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wei\u00dft du wo diese Rose nun", "tokens": ["Wei\u00dft", "du", "wo", "die\u00b7se", "Ro\u00b7se", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWAV", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von meinem Busen hingekommen?", "tokens": ["Von", "mei\u00b7nem", "Bu\u00b7sen", "hin\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie hat schon ihren Plaz genommen,", "tokens": ["Sie", "hat", "schon", "ih\u00b7ren", "Plaz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wo mehr gestorbne Blumen ruhn,", "tokens": ["Wo", "mehr", "ge\u00b7storb\u00b7ne", "Blu\u00b7men", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Milon alle mir gegeben.", "tokens": ["Die", "Mi\u00b7lon", "al\u00b7le", "mir", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In einer Kiste liegt sie still", "tokens": ["In", "ei\u00b7ner", "Kis\u00b7te", "liegt", "sie", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Bis auf den Tag, da sich mein Leben", "tokens": ["Bis", "auf", "den", "Tag", ",", "da", "sich", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Beschlie\u00dfen soll, und mu\u00df, und will.", "tokens": ["Be\u00b7schlie\u00b7\u00dfen", "soll", ",", "und", "mu\u00df", ",", "und", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KON", "VMFIN", "$,", "KON", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Da hei\u00df ich mir die Blumen geben,", "tokens": ["Da", "hei\u00df", "ich", "mir", "die", "Blu\u00b7men", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "K\u00fc\u00df eine nach der andern noch,", "tokens": ["K\u00fc\u00df", "ei\u00b7ne", "nach", "der", "an\u00b7dern", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Und spreche dann zu meinen Kindern:", "tokens": ["Und", "spre\u00b7che", "dann", "zu", "mei\u00b7nen", "Kin\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "\u201each Kinder, h\u00f6rt, ihr wisset doch,", "tokens": ["\u201e", "ach", "Kin\u00b7der", ",", "h\u00f6rt", ",", "ihr", "wis\u00b7set", "doch", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u201ewas meine Ruhe kann vermindern,", "tokens": ["\u201e", "was", "mei\u00b7ne", "Ru\u00b7he", "kann", "ver\u00b7min\u00b7dern", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "\u201ewenn ihr mit Thr\u00e4nen mich begrabt,", "tokens": ["\u201e", "wenn", "ihr", "mit", "Thr\u00e4\u00b7nen", "mich", "be\u00b7grabt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u201emit Ausruf banger Klagelieder,", "tokens": ["\u201e", "mit", "Aus\u00b7ruf", "ban\u00b7ger", "Kla\u00b7ge\u00b7lie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "\u201eund diese Blumen mir nicht mitgegeben habt:", "tokens": ["\u201e", "und", "die\u00b7se", "Blu\u00b7men", "mir", "nicht", "mit\u00b7ge\u00b7ge\u00b7ben", "habt", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ealsdann kommt meine Seele wieder", "tokens": ["\u201e", "als\u00b7dann", "kommt", "mei\u00b7ne", "See\u00b7le", "wie\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "\u201eund rasselt um den Nachttisch her,", "tokens": ["\u201e", "und", "ras\u00b7selt", "um", "den", "Nacht\u00b7tisch", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "\u201ewird \u00fcber dieser Kiste schweben,", "tokens": ["\u201e", "wird", "\u00fc\u00b7ber", "die\u00b7ser", "Kis\u00b7te", "schwe\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "\u201eals wenn es eine Taube w\u00e4r,", "tokens": ["\u201e", "als", "wenn", "es", "ei\u00b7ne", "Tau\u00b7be", "w\u00e4r", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "\u201ewird Achtung auf die Blumen geben,", "tokens": ["\u201e", "wird", "Ach\u00b7tung", "auf", "die", "Blu\u00b7men", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "\u201ewie eines Geitzigen schwerabgeschiedner Geist", "tokens": ["\u201e", "wie", "ei\u00b7nes", "Geit\u00b7zi\u00b7gen", "schwer\u00b7ab\u00b7ge\u00b7schied\u00b7ner", "Geist"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "\u201eauf sein vergrabnes Gold im Keller Achtung giebet,", "tokens": ["\u201e", "auf", "sein", "ver\u00b7grab\u00b7nes", "Gold", "im", "Kel\u00b7ler", "Ach\u00b7tung", "gie\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201edarum erf\u00fcllet gern, was euch die Mutter hei\u00dft,", "tokens": ["\u201e", "da\u00b7rum", "er\u00b7f\u00fcl\u00b7let", "gern", ",", "was", "euch", "die", "Mut\u00b7ter", "hei\u00dft", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u201edie noch im Tode liebet \u2014 \u2014", "tokens": ["\u201e", "die", "noch", "im", "To\u00b7de", "lie\u00b7bet"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADV", "APPRART", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}