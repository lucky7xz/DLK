{"dta.poem.19019": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.   Auf den Fall und Errettung eines gros-  \n sen Herrn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hieher, ihr Potentaten!", "tokens": ["Hie\u00b7her", ",", "ihr", "Po\u00b7ten\u00b7ta\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut einen Printzen an,", "tokens": ["Schaut", "ei\u00b7nen", "Print\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der seinem Heil gerathen;", "tokens": ["Der", "sei\u00b7nem", "Heil", "ge\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er eilt zur Gnaden-Bahn,", "tokens": ["Er", "eilt", "zur", "Gna\u00b7den\u00b7Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu denen Lebens-St\u00e4dten,", "tokens": ["Zu", "de\u00b7nen", "Le\u00b7bens\u00b7St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wovon er ausgetreten.", "tokens": ["Wo\u00b7von", "er", "aus\u00b7ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es hatte der Verfluchte,", "tokens": ["Es", "hat\u00b7te", "der", "Ver\u00b7fluch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als er zur Mitternacht", "tokens": ["Als", "er", "zur", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auch diese Seele suchte,", "tokens": ["Auch", "die\u00b7se", "See\u00b7le", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sie fast davon gebracht;", "tokens": ["Sie", "fast", "da\u00b7von", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nur solte das Verschlingen", "tokens": ["Nur", "sol\u00b7te", "das", "Ver\u00b7schlin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Jhm di\u00dfmahl mi\u00dfgelingen.", "tokens": ["Jhm", "di\u00df\u00b7mahl", "mi\u00df\u00b7ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Der W\u00e4chter seiner Heerden,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sei\u00b7ner", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der treulich f\u00fcr sie k\u00e4mpfft,", "tokens": ["Der", "treu\u00b7lich", "f\u00fcr", "sie", "k\u00e4mpfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und mit so viel Beschwerden", "tokens": ["Und", "mit", "so", "viel", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die Wuth des Wolffes d\u00e4mpfft,", "tokens": ["Die", "Wuth", "des", "Wolf\u00b7fes", "d\u00e4mpfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Ri\u00df, o ein gro\u00dfes Gl\u00fccke!", "tokens": ["Ri\u00df", ",", "o", "ein", "gro\u00b7\u00dfes", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.18": {"text": "Di\u00df arme Schaf zur\u00fccke.", "tokens": ["Di\u00df", "ar\u00b7me", "Schaf", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die Christliche Gemeine", "tokens": ["Die", "Christ\u00b7li\u00b7che", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ist warrlich \u00fcbel dran,", "tokens": ["Ist", "warr\u00b7lich", "\u00fc\u00b7bel", "dran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Des Satans Zauber-Hayne", "tokens": ["Des", "Sa\u00b7tans", "Zau\u00b7ber\u00b7Hay\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Sieht man vor Kirchen an;", "tokens": ["Sieht", "man", "vor", "Kir\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Viel Cantzeln und Alt\u00e4re", "tokens": ["Viel", "Cant\u00b7zeln", "und", "Al\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Sind Thronen falscher Lehre.", "tokens": ["Sind", "Thro\u00b7nen", "fal\u00b7scher", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Die F\u00fcrsten solten Hirten", "tokens": ["Die", "F\u00fcrs\u00b7ten", "sol\u00b7ten", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Der Kirche Christi seyn,", "tokens": ["Der", "Kir\u00b7che", "Chris\u00b7ti", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und, wenn sie seitw\u00e4rts irrten,", "tokens": ["Und", ",", "wenn", "sie", "seit\u00b7w\u00e4rts", "irr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Es \u00f6ffentlich bereu\u2019n", "tokens": ["Es", "\u00f6f\u00b7fent\u00b7lich", "be\u00b7reu'n"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Vor dem, der ihr Verbrechen", "tokens": ["Vor", "dem", ",", "der", "ihr", "Ver\u00b7bre\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Kan mit dem Tode r\u00e4chen.", "tokens": ["Kan", "mit", "dem", "To\u00b7de", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Begl\u00fcckte GOttes-H\u00e4user,", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", "Got\u00b7tes\u00b7H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Da Theodosius,", "tokens": ["Da", "Theo\u00b7do\u00b7si\u00b7us", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Der Schuld beladne K\u00e4yser,", "tokens": ["Der", "Schuld", "be\u00b7lad\u00b7ne", "K\u00e4y\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Den Bann-Spruch h\u00f6ren mu\u00df,", "tokens": ["Den", "Bann\u00b7Spruch", "h\u00f6\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Und, bey der Christen Hauffen,", "tokens": ["Und", ",", "bey", "der", "Chris\u00b7ten", "Hauf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Mit Thr\u00e4nen Einla\u00df kauffen.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "Ein\u00b7la\u00df", "kauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "So r\u00fchmen Ahabs Zeiten", "tokens": ["So", "r\u00fch\u00b7men", "A\u00b7habs", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Eli\u00e4 Helden-Muth,", "tokens": ["E\u00b7li\u00e4", "Hel\u00b7den\u00b7Muth", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.39": {"text": "Dem frey zu wiederstreiten,", "tokens": ["Dem", "frey", "zu", "wie\u00b7der\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Der freye S\u00fcnde thut;", "tokens": ["Der", "frey\u00b7e", "S\u00fcn\u00b7de", "thut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Wer hat den stummen Hunden", "tokens": ["Wer", "hat", "den", "stum\u00b7men", "Hun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Bey uns das Maul verbunden?", "tokens": ["Bey", "uns", "das", "Maul", "ver\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Drum h\u00f6ret mich, ihr Gro\u00dfen,", "tokens": ["Drum", "h\u00f6\u00b7ret", "mich", ",", "ihr", "Gro\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Sonst wird des K\u00f6nigs Grimm", "tokens": ["Sonst", "wird", "des", "K\u00f6\u00b7nigs", "Grimm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Euch von dem Stule stossen;", "tokens": ["Euch", "von", "dem", "Stu\u00b7le", "stos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.46": {"text": "Euch wird die Donner-Stimm:", "tokens": ["Euch", "wird", "die", "Don\u00b7ner\u00b7Stimm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "Verfluchte weicht von dannen!", "tokens": ["Ver\u00b7fluch\u00b7te", "weicht", "von", "dan\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "In Ewigkeit verbannen.", "tokens": ["In", "E\u00b7wig\u00b7keit", "ver\u00b7ban\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Jhr seyd so arme S\u00fcnder,", "tokens": ["Ihr", "seyd", "so", "ar\u00b7me", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Als and\u2019re Sterbliche;", "tokens": ["Als", "an\u00b7d'\u00b7re", "Sterb\u00b7li\u00b7che", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.51": {"text": "Jhr wachst, wie andre Kinder,", "tokens": ["Ihr", "wachst", ",", "wie", "and\u00b7re", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Mit Sorgen in die H\u00f6h,", "tokens": ["Mit", "Sor\u00b7gen", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Und euer erstes Stehnen", "tokens": ["Und", "eu\u00b7er", "ers\u00b7tes", "Steh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Vermischet sich mit Thr\u00e4nen.", "tokens": ["Ver\u00b7mi\u00b7schet", "sich", "mit", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Wie wolt ihr den Gewittern", "tokens": ["Wie", "wolt", "ihr", "den", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Der letzten Stund entgehn,", "tokens": ["Der", "letz\u00b7ten", "Stund", "ent\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Daf\u00fcr die Himmel zittern,", "tokens": ["Da\u00b7f\u00fcr", "die", "Him\u00b7mel", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Die durch ihr Schreck-Geth\u00f6n,", "tokens": ["Die", "durch", "ihr", "Schreck\u00b7Get\u00b7h\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Wie z\u00e4he Weitzen-Halmen,", "tokens": ["Wie", "z\u00e4\u00b7he", "Weit\u00b7zen\u00b7Hal\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Den Erden-Crei\u00df zermalmen?", "tokens": ["Den", "Er\u00b7den\u00b7Crei\u00df", "zer\u00b7mal\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "Geht oder kriecht zum Creutze,", "tokens": ["Geht", "o\u00b7der", "kriecht", "zum", "Creut\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Und k\u00fc\u00dft den grossen Sohn,", "tokens": ["Und", "k\u00fc\u00dft", "den", "gros\u00b7sen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.63": {"text": "Da\u00df ers Erbarmen reitze;", "tokens": ["Da\u00df", "ers", "Er\u00b7bar\u00b7men", "reit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Sonst habt ihr euren Lohn", "tokens": ["Sonst", "habt", "ihr", "eu\u00b7ren", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {"text": "Mit den verjagten F\u00fcrsten,", "tokens": ["Mit", "den", "ver\u00b7jag\u00b7ten", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Die nur nach Ungl\u00fcck d\u00fcrsten.", "tokens": ["Die", "nur", "nach", "Un\u00b7gl\u00fcck", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.67": {"text": "Die GOtt-geweyhte Printzen,", "tokens": ["Die", "Got\u00b7tge\u00b7wey\u00b7hte", "Print\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Die in sich selber klein,", "tokens": ["Die", "in", "sich", "sel\u00b7ber", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Vor dem die Augen blintzen,", "tokens": ["Vor", "dem", "die", "Au\u00b7gen", "blint\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Ders Haupt der Creutz-Gemein,", "tokens": ["Ders", "Haupt", "der", "Creutz\u00b7Ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.71": {"text": "Und ihm zu Fusse liegen;", "tokens": ["Und", "ihm", "zu", "Fus\u00b7se", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Die werden Gnade kriegen.", "tokens": ["Die", "wer\u00b7den", "Gna\u00b7de", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "VVINF", "$."], "meter