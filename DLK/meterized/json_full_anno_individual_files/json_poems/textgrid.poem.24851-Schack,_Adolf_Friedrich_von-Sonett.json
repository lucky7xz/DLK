{"textgrid.poem.24851": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich ruht es sich in Sommern\u00e4chten,", "tokens": ["Wie", "lieb\u00b7lich", "ruht", "es", "sich", "in", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn durch das Laub, wo tr\u00e4umend V\u00f6gel singen,", "tokens": ["Wenn", "durch", "das", "Laub", ",", "wo", "tr\u00e4u\u00b7mend", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PWAV", "VVPP", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Westwind rauscht, als ob auf Mondlichtschwingen", "tokens": ["Der", "West\u00b7wind", "rauscht", ",", "als", "ob", "auf", "Mond\u00b7licht\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von fernen Welten Geister Gr\u00fc\u00dfe br\u00e4chten.", "tokens": ["Von", "fer\u00b7nen", "Wel\u00b7ten", "Geis\u00b7ter", "Gr\u00fc\u00b7\u00dfe", "br\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Adele wiegt mich sanft mit ihrer Rechten,", "tokens": ["A\u00b7de\u00b7le", "wiegt", "mich", "sanft", "mit", "ih\u00b7rer", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und, wie wir fest uns aneinander schlingen,", "tokens": ["Und", ",", "wie", "wir", "fest", "uns", "an\u00b7ein\u00b7an\u00b7der", "schlin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umwallen uns mit schwarzen Lockenringen", "tokens": ["Um\u00b7wal\u00b7len", "uns", "mit", "schwar\u00b7zen", "Lo\u00b7cken\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Langflie\u00dfend ihres Haars gel\u00f6ste Flechten.", "tokens": ["Lang\u00b7flie\u00b7\u00dfend", "ih\u00b7res", "Haars", "ge\u00b7l\u00f6s\u00b7te", "Flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schlaf, heil'ger Schlaf! la\u00df deine Murmelquellen", "tokens": ["Schlaf", ",", "heil'\u00b7ger", "Schlaf", "!", "la\u00df", "dei\u00b7ne", "Mur\u00b7mel\u00b7quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "VVIMP", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Melodisch rauschend unser Haupt umsp\u00fclen,", "tokens": ["Me\u00b7lo\u00b7disch", "rau\u00b7schend", "un\u00b7ser", "Haupt", "um\u00b7sp\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und trag uns fort auf ihren Schaukelwellen", "tokens": ["Und", "trag", "uns", "fort", "auf", "ih\u00b7ren", "Schau\u00b7kel\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ins Meer des Traums, da\u00df nach dem Tag, dem schw\u00fclen,", "tokens": ["Ins", "Meer", "des", "Traums", ",", "da\u00df", "nach", "dem", "Tag", ",", "dem", "schw\u00fc\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir uns in seinen frischen, d\u00e4mmerhellen,", "tokens": ["Wir", "uns", "in", "sei\u00b7nen", "fri\u00b7schen", ",", "d\u00e4m\u00b7mer\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Mondenglanz erf\u00fcllten Grotten k\u00fchlen.", "tokens": ["Von", "Mon\u00b7den\u00b7glanz", "er\u00b7f\u00fcll\u00b7ten", "Grot\u00b7ten", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie lieblich ruht es sich in Sommern\u00e4chten,", "tokens": ["Wie", "lieb\u00b7lich", "ruht", "es", "sich", "in", "Som\u00b7mer\u00b7n\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"]