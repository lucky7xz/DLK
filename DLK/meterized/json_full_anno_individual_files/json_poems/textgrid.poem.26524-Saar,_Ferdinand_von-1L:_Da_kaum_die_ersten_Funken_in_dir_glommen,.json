{"textgrid.poem.26524": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da kaum die ersten Funken in dir glommen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da kaum die ersten Funken in dir glommen,", "tokens": ["Da", "kaum", "die", "ers\u00b7ten", "Fun\u00b7ken", "in", "dir", "glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Knabe schon begannst du still zu leiden;", "tokens": ["Als", "Kna\u00b7be", "schon", "be\u00b7gannst", "du", "still", "zu", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sagtest dir: mich wird das Gl\u00fcck vermeiden,", "tokens": ["Du", "sag\u00b7test", "dir", ":", "mich", "wird", "das", "Gl\u00fcck", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ahntest leise, was da w\u00fcrde kommen.", "tokens": ["Und", "ahn\u00b7test", "lei\u00b7se", ",", "was", "da", "w\u00fcr\u00b7de", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PRELS", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und als du dann das Kreuz auf dich genommen", "tokens": ["Und", "als", "du", "dann", "das", "Kreuz", "auf", "dich", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Pfade einschlugst, die von And'ren scheiden,", "tokens": ["Und", "Pfa\u00b7de", "ein\u00b7schlugst", ",", "die", "von", "An\u00b7d'\u00b7ren", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da schienst du dich an deinem Loos zu weiden \u2013", "tokens": ["Da", "schienst", "du", "dich", "an", "dei\u00b7nem", "Loos", "zu", "wei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sangst mit Lust: mir kann der Schmerz nur frommen!", "tokens": ["Und", "sangst", "mit", "Lust", ":", "mir", "kann", "der", "Schmerz", "nur", "from\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und doch wie th\u00f6richt! Deine Klagen schallen,", "tokens": ["Und", "doch", "wie", "th\u00f6\u00b7richt", "!", "Dei\u00b7ne", "Kla\u00b7gen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "$.", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So oft ein Pfeil dich in die Brust getroffen,", "tokens": ["So", "oft", "ein", "Pfeil", "dich", "in", "die", "Brust", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deiner Sehnsucht hei\u00dfe Thr\u00e4nen fallen.", "tokens": ["Und", "dei\u00b7ner", "Sehn\u00b7sucht", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch ist dein Auge holder Sch\u00f6nheit offen,", "tokens": ["Noch", "ist", "dein", "Au\u00b7ge", "hol\u00b7der", "Sch\u00f6n\u00b7heit", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Geist dem Ruhme \u2013 und was bei dem Allen", "tokens": ["Dein", "Geist", "dem", "Ruh\u00b7me", "\u2013", "und", "was", "bei", "dem", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "KON", "PWS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Schlimmste ist: du h\u00f6rst nicht auf zu hoffen.", "tokens": ["Das", "Schlimms\u00b7te", "ist", ":", "du", "h\u00f6rst", "nicht", "auf", "zu", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+