{"dta.poem.5870": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuerst mu\u00df der Begriff so einfach seyn, als klar,", "tokens": ["Zu\u00b7erst", "mu\u00df", "der", "Be\u00b7griff", "so", "ein\u00b7fach", "seyn", ",", "als", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADJD", "VAINF", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So da\u00df der Geist, wenn er daran gedencket,", "tokens": ["So", "da\u00df", "der", "Geist", ",", "wenn", "er", "da\u00b7ran", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Augenblick sich in die Sachen sencket,", "tokens": ["Den", "Au\u00b7gen\u00b7blick", "sich", "in", "die", "Sa\u00b7chen", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und findet, da\u00df sie w\u00fcrcklich wahr.", "tokens": ["Und", "fin\u00b7det", ",", "da\u00df", "sie", "w\u00fcrck\u00b7lich", "wahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}