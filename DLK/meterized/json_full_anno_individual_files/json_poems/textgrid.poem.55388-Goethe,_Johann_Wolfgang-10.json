{"textgrid.poem.55388": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer Teich war zugefroren,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Teich", "war", "zu\u00b7ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fr\u00f6schlein, in der Tiefe verloren,", "tokens": ["Die", "Fr\u00f6schlein", ",", "in", "der", "Tie\u00b7fe", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durften nicht ferner quaken noch springen,", "tokens": ["Durf\u00b7ten", "nicht", "fer\u00b7ner", "qua\u00b7ken", "noch", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Versprachen sich aber im halben Traum,", "tokens": ["Ver\u00b7spra\u00b7chen", "sich", "a\u00b7ber", "im", "hal\u00b7ben", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "F\u00e4nden sie nur da oben Raum,", "tokens": ["F\u00e4n\u00b7den", "sie", "nur", "da", "o\u00b7ben", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "lin