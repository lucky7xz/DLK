{"dta.poem.3342": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Secret\u00e4r G*.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich dr\u00e4ng im Geiste mich zum Tempel der Cythere,", "tokens": ["Ich", "dr\u00e4ng", "im", "Geis\u00b7te", "mich", "zum", "Tem\u00b7pel", "der", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch schw\u00e4rmender Verliebten Heere,", "tokens": ["Durch", "schw\u00e4r\u00b7men\u00b7der", "Ver\u00b7lieb\u00b7ten", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch den geweihten Myrthenhayn.", "tokens": ["Durch", "den", "ge\u00b7weih\u00b7ten", "Myr\u00b7then\u00b7hayn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Freude reichet mir die H\u00e4nde;", "tokens": ["Die", "Freu\u00b7de", "rei\u00b7chet", "mir", "die", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie f\u00fchrt mich schalkhaft l\u00e4chelnd ein:", "tokens": ["Sie", "f\u00fchrt", "mich", "schalk\u00b7haft", "l\u00e4\u00b7chelnd", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! wenn sie nicht so schnell verschw\u00e4nde,", "tokens": ["Ach", "!", "wenn", "sie", "nicht", "so", "schnell", "ver\u00b7schw\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn unser Herz sie rein empf\u00e4nde;", "tokens": ["Wenn", "un\u00b7ser", "Herz", "sie", "rein", "em\u00b7pf\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie g\u00f6ttlich w\u00fcrde sie nicht seyn!", "tokens": ["Wie", "g\u00f6tt\u00b7lich", "w\u00fcr\u00b7de", "sie", "nicht", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Ueberwinderinn der Herzen", "tokens": ["Die", "Ue\u00b7ber\u00b7win\u00b7de\u00b7rinn", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ruht unter gauckelhaften Scherzen:", "tokens": ["Ruht", "un\u00b7ter", "gau\u00b7ckel\u00b7haf\u00b7ten", "Scher\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr Auge flammt voll schlauer Lust,", "tokens": ["Ihr", "Au\u00b7ge", "flammt", "voll", "schlau\u00b7er", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und W\u00fcnsche schwellen ihre Brust.", "tokens": ["Und", "W\u00fcn\u00b7sche", "schwel\u00b7len", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es dampft, mit Seufzern untermischet,", "tokens": ["Es", "dampft", ",", "mit", "Seuf\u00b7zern", "un\u00b7ter\u00b7mi\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der Weirauch wolkicht vom Altar;", "tokens": ["Der", "Wei\u00b7rauch", "wol\u00b7kicht", "vom", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und ihres Zephyrs Hauch erfrischet", "tokens": ["Und", "ih\u00b7res", "Ze\u00b7phyrs", "Hauch", "er\u00b7fri\u00b7schet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sie, ach! die manch verlohrnes Jahr.", "tokens": ["Sie", ",", "ach", "!", "die", "manch", "ver\u00b7lohr\u00b7nes", "Jahr", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ITJ", "$.", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mir fremde war.", "tokens": ["Mir", "frem\u00b7de", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Nun klopft mein Herz ihr wild entgegen;", "tokens": ["Nun", "klopft", "mein", "Herz", "ihr", "wild", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und Bluhmen d\u00fcften auf den Wegen", "tokens": ["Und", "Bluh\u00b7men", "d\u00fcf\u00b7ten", "auf", "den", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Zum Sitz der gro\u00dfen K\u00f6niginn,", "tokens": ["Zum", "Sitz", "der", "gro\u00b7\u00dfen", "K\u00f6\u00b7ni\u00b7ginn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Zum innern Tempel hin,", "tokens": ["Zum", "in\u00b7nern", "Tem\u00b7pel", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Wohin Chlorinde mich begleitet,", "tokens": ["Wo\u00b7hin", "Chlo\u00b7rin\u00b7de", "mich", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Die, wenn ich ihr zu z\u00e4rtlich bin,", "tokens": ["Die", ",", "wenn", "ich", "ihr", "zu", "z\u00e4rt\u00b7lich", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Sich scherzend str\u00e4ubt und lockend streitet.", "tokens": ["Sich", "scher\u00b7zend", "str\u00e4ubt", "und", "lo\u00b7ckend", "strei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die G\u00f6ttinn l\u00e4chelt sanft, und ihr entw\u00f6lkter Blick", "tokens": ["Die", "G\u00f6t\u00b7tinn", "l\u00e4\u00b7chelt", "sanft", ",", "und", "ihr", "ent\u00b7w\u00f6lk\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Weissaget meiner Liebe Gl\u00fcck:", "tokens": ["Weis\u00b7sa\u00b7get", "mei\u00b7ner", "Lie\u00b7be", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie wird mein Feuer angefachet!", "tokens": ["Wie", "wird", "mein", "Feu\u00b7er", "an\u00b7ge\u00b7fa\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Doch wie? was Cypris mir verspricht,", "tokens": ["Doch", "wie", "?", "was", "Cyp\u00b7ris", "mir", "ver\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PWS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Vollzieht sie selber nicht?", "tokens": ["Voll\u00b7zieht", "sie", "sel\u00b7ber", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Sie winkt! und wem? verdr\u00fc\u00dfliches Gesicht,", "tokens": ["Sie", "winkt", "!", "und", "wem", "?", "ver\u00b7dr\u00fc\u00df\u00b7li\u00b7ches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PWS", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Auf dem die magre Sorge wachet,", "tokens": ["Auf", "dem", "die", "mag\u00b7re", "Sor\u00b7ge", "wa\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Das niemals, oder frostig lachet!", "tokens": ["Das", "nie\u00b7mals", ",", "o\u00b7der", "fros\u00b7tig", "la\u00b7chet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ach! Hymen ists, und ihn verlangt ich nicht!", "tokens": ["Ach", "!", "Hy\u00b7men", "ists", ",", "und", "ihn", "ver\u00b7langt", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "VAFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Wie? Amor und sein Chor verschwand,", "tokens": ["Wie", "?", "A\u00b7mor", "und", "sein", "Chor", "ver\u00b7schwand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NE", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Sobald er neben sich den tr\u00e4gen Hymen fand,", "tokens": ["So\u00b7bald", "er", "ne\u00b7ben", "sich", "den", "tr\u00e4\u00b7gen", "Hy\u00b7men", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Den ekelhaft Gepr\u00e4ng noch ekelhafter machet?", "tokens": ["Den", "e\u00b7kel\u00b7haft", "Ge\u00b7pr\u00e4ng", "noch", "e\u00b7kel\u00b7haf\u00b7ter", "ma\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "O schrecklich Wort! o Ehestand!", "tokens": ["O", "schreck\u00b7lich", "Wort", "!", "o", "E\u00b7hes\u00b7tand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mein Saitenspiel entschl\u00e4ft, und schl\u00fcpft mir aus der", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "ent\u00b7schl\u00e4ft", ",", "und", "schl\u00fcpft", "mir", "aus", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Hand.", "tokens": ["Hand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}