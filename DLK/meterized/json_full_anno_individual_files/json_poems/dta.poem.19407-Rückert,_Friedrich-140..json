{"dta.poem.19407": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "140.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur die Beschr\u00e4nktheit wird an dem, den sie will ehren,", "tokens": ["Nur", "die", "Be\u00b7schr\u00e4nk\u00b7theit", "wird", "an", "dem", ",", "den", "sie", "will", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "ART", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fehler leugnen und f\u00fcr Tugenden erkl\u00e4ren.", "tokens": ["Die", "Feh\u00b7ler", "leug\u00b7nen", "und", "f\u00fcr", "Tu\u00b7gen\u00b7den", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Mannes Gr\u00f6\u00df' ist mir, nicht da\u00df er fehlerfrei,", "tokens": ["Des", "Man\u00b7nes", "Gr\u00f6\u00df'", "ist", "mir", ",", "nicht", "da\u00df", "er", "feh\u00b7ler\u00b7frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "$,", "PTKNEG", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch \u00fcber Fehler, die er hat, erhaben sei.", "tokens": ["Doch", "\u00fc\u00b7ber", "Feh\u00b7ler", ",", "die", "er", "hat", ",", "er\u00b7ha\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"],