{"textgrid.poem.43329": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Bragas Lied im Sternenklang,", "tokens": ["Ist", "Bra\u00b7gas", "Lied", "im", "Ster\u00b7nen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ists, Tochter Dvals, dein Weihgesang,", "tokens": ["Ists", ",", "Toch\u00b7ter", "Dvals", ",", "dein", "Weih\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was rings die alte Nacht verj\u00fcngt?", "tokens": ["Was", "rings", "die", "al\u00b7te", "Nacht", "ver\u00b7j\u00fcngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch mich \u2013 ach! meinen Staub durchdringt,", "tokens": ["Auch", "mich", "\u2013", "ach", "!", "mei\u00b7nen", "Staub", "durch\u00b7dringt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "ITJ", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Blitze Thors, die Gruft enth\u00f6lt,", "tokens": ["Wie", "Blit\u00b7ze", "Thors", ",", "die", "Gruft", "ent\u00b7h\u00f6lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Wonne! mich \u2013 mich neu beseelt?", "tokens": ["O", "Won\u00b7ne", "!", "mich", "\u2013", "mich", "neu", "be\u00b7seelt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "$(", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus rothen Wellen str\u00f6hmt das Licht;", "tokens": ["Aus", "ro\u00b7then", "Wel\u00b7len", "str\u00f6hmt", "das", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich aber, Heil mir! schlummre nicht,", "tokens": ["Ich", "a\u00b7ber", ",", "Heil", "mir", "!", "schlumm\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "PPER", "$.", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Heil mir Erwachten! bade ganz", "tokens": ["Heil", "mir", "Er\u00b7wach\u00b7ten", "!", "ba\u00b7de", "ganz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "NN", "$.", "VVFIN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Den neuen Leib in Sonnenglanz,", "tokens": ["Den", "neu\u00b7en", "Leib", "in", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Schwimm in die leichtre Luft empor,", "tokens": ["Schwimm", "in", "die", "leicht\u00b7re", "Luft", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bin ganz Entz\u00fcckung, bin ganz Ohr,", "tokens": ["Bin", "ganz", "Ent\u00b7z\u00fc\u00b7ckung", ",", "bin", "ganz", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und walle trunken in der Fluth", "tokens": ["Und", "wal\u00b7le", "trun\u00b7ken", "in", "der", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der hohen Harmonie? \u2013", "tokens": ["Der", "ho\u00b7hen", "Har\u00b7mo\u00b7nie", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wo ruht", "tokens": ["Wo", "ruht"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Mein schwebender Geist auf luftiger H\u00f6h?", "tokens": ["Mein", "schwe\u00b7ben\u00b7der", "Geist", "auf", "luf\u00b7ti\u00b7ger", "H\u00f6h", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wo \u00fcber Berg und Thal und See", "tokens": ["Wo", "\u00fc\u00b7ber", "Berg", "und", "Thal", "und", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Flattr ich und gl\u00fch im Silberton?", "tokens": ["Flattr", "ich", "und", "gl\u00fch", "im", "Sil\u00b7ber\u00b7ton", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wohin, mein Geist, bist du entflohn?", "tokens": ["Wo\u00b7hin", ",", "mein", "Geist", ",", "bist", "du", "ent\u00b7flohn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wo badest du den Schwung so fr\u00fch", "tokens": ["Wo", "ba\u00b7dest", "du", "den", "Schwung", "so", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Im Urquell s\u00fc\u00dfer Harmonie?", "tokens": ["Im", "Ur\u00b7quell", "s\u00fc\u00b7\u00dfer", "Har\u00b7mo\u00b7nie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nicht so entfesselte einst Njord", "tokens": ["Nicht", "so", "ent\u00b7fes\u00b7sel\u00b7te", "einst", "Njord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVFIN", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Den blanken Eisberg durch Accord:", "tokens": ["Den", "blan\u00b7ken", "Eis\u00b7berg", "durch", "Ac\u00b7cord", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Der Fels, wo er die Hymne ergo\u00df,", "tokens": ["Der", "Fels", ",", "wo", "er", "die", "Hym\u00b7ne", "er\u00b7go\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Da\u00df Nord-Sturm tonvoll ihn umflo\u00df,", "tokens": ["Da\u00df", "Nord\u00b7Sturm", "ton\u00b7voll", "ihn", "um\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Bebt' unter ihm, die Tiefe klang,", "tokens": ["Bebt'", "un\u00b7ter", "ihm", ",", "die", "Tie\u00b7fe", "klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und Geister seufzten in seinen Gesang.", "tokens": ["Und", "Geis\u00b7ter", "seufz\u00b7ten", "in", "sei\u00b7nen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo Mimers Haupt vom H\u00fcgel quoll,", "tokens": ["Wo", "Mi\u00b7mers", "Haupt", "vom", "H\u00fc\u00b7gel", "quoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier ist Sigtuna, hier Valholl,", "tokens": ["Hier", "ist", "Sig\u00b7tu\u00b7na", ",", "hier", "Val\u00b7holl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "$,", "ADV", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Hier Glasurs Dunkel, hier der Kranz,", "tokens": ["Hier", "Gla\u00b7surs", "Dun\u00b7kel", ",", "hier", "der", "Kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mit der Wipfel heilgem Glanz", "tokens": ["Der", "mit", "der", "Wip\u00b7fel", "heil\u00b7gem", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herab aus Wolken, die er st\u00fctzt,", "tokens": ["Her\u00b7ab", "aus", "Wol\u00b7ken", ",", "die", "er", "st\u00fctzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die goldnen Schilder \u00fcberblitzt.", "tokens": ["Die", "gold\u00b7nen", "Schil\u00b7der", "\u00fc\u00b7berb\u00b7litzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sehe Fansal \u2013 Schaur umfa\u00dft", "tokens": ["Ich", "se\u00b7he", "Fan\u00b7sal", "\u2013", "Schaur", "um\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$(", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stille W\u00fcrde den Palast;", "tokens": ["Und", "stil\u00b7le", "W\u00fcr\u00b7de", "den", "Pa\u00b7last", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich sehe Gladheims Freuden-Saal,", "tokens": ["Ich", "se\u00b7he", "Glad\u00b7heims", "Freu\u00b7den\u00b7Saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geh\u00fcllt in seines Goldes Strahl:", "tokens": ["Ge\u00b7h\u00fcllt", "in", "sei\u00b7nes", "Gol\u00b7des", "Strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von seiner Zinne bebt der Blick,", "tokens": ["Von", "sei\u00b7ner", "Zin\u00b7ne", "bebt", "der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Zu stumpf, ihn anzuschaun, zur\u00fcck.", "tokens": ["Zu", "stumpf", ",", "ihn", "an\u00b7zu\u00b7schaun", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "PPER", "VVIZU", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wer schreitet k\u00f6niglich daher", "tokens": ["Wer", "schrei\u00b7tet", "k\u00f6\u00b7nig\u00b7lich", "da\u00b7her"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In Vingolfs Hayn, am sanftern Meer?", "tokens": ["In", "Vin\u00b7golfs", "Hayn", ",", "am", "sanf\u00b7tern", "Meer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "La\u00df mich, du Majest\u00e4t im Hayn,", "tokens": ["La\u00df", "mich", ",", "du", "Ma\u00b7jes\u00b7t\u00e4t", "im", "Hayn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Auf deinen Fu\u00dftritt Blumen streun!", "tokens": ["Auf", "dei\u00b7nen", "Fu\u00df\u00b7tritt", "Blu\u00b7men", "streun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du K\u00f6nig, Vater, Friedensheld,", "tokens": ["Du", "K\u00f6\u00b7nig", ",", "Va\u00b7ter", ",", "Frie\u00b7dens\u00b7held", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Du Lust des Himmels und der Welt!", "tokens": ["Du", "Lust", "des", "Him\u00b7mels", "und", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "La\u00df mich die Stunde weihen, da", "tokens": ["La\u00df", "mich", "die", "Stun\u00b7de", "wei\u00b7hen", ",", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ich deinen Tritt, Alfadur, sah!", "tokens": ["Ich", "dei\u00b7nen", "Tritt", ",", "Al\u00b7fa\u00b7dur", ",", "sah", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Hier, wie in Asgaards Valaskialf", "tokens": ["Hier", ",", "wie", "in", "As\u00b7ga\u00b7ards", "Va\u00b7las\u00b7ki\u00b7alf"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Umringt von Disen oder Alf,", "tokens": ["Um\u00b7ringt", "von", "Di\u00b7sen", "o\u00b7der", "Alf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Den Zepter Hlidskialfs in der Hand,", "tokens": ["Den", "Zep\u00b7ter", "Hlids\u00b7ki\u00b7alfs", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Im Helm, im purpurnen Gewand,", "tokens": ["Im", "Helm", ",", "im", "pur\u00b7pur\u00b7nen", "Ge\u00b7wand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.25": {"text": "Stets Freund der Menschen, dort wie hier,", "tokens": ["Stets", "Freund", "der", "Men\u00b7schen", ",", "dort", "wie", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Bist du geweiht, und glorreich, mir!", "tokens": ["Bist", "du", "ge\u00b7weiht", ",", "und", "glor\u00b7reich", ",", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KON", "ADJD", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist Bragas Lied im Sternenklang,", "tokens": ["Ist", "Bra\u00b7gas", "Lied", "im", "Ster\u00b7nen\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ists, Tochter Dvals, dein Weihgesang,", "tokens": ["Ists", ",", "Toch\u00b7ter", "Dvals", ",", "dein", "Weih\u00b7ge\u00b7sang", ","], "token_