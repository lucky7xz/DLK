{"dta.poem.13236": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eo deutsches Vaterland! ich rede nur von dir!", "tokens": ["\u201e", "o", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!", "ich", "re\u00b7de", "nur", "von", "dir", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewarum er\u00f6ffnest du dem Untergang die Th\u00fcr?", "tokens": ["\u201e", "wa\u00b7rum", "er\u00b7\u00f6ff\u00b7nest", "du", "dem", "Un\u00b7ter\u00b7gang", "die", "Th\u00fcr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ebetrachte diesen Krieg und manche Niederlage!", "tokens": ["\u201e", "be\u00b7trach\u00b7te", "die\u00b7sen", "Krieg", "und", "man\u00b7che", "Nie\u00b7der\u00b7la\u00b7ge", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "100\u201dBetrachte, was der Feind von deinen Waffen sage!", "tokens": ["\"", "Be\u00b7trach\u00b7te", ",", "was", "der", "Feind", "von", "dei\u00b7nen", "Waf\u00b7fen", "sa\u00b7ge", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edein eigner Lands-Geno\u00df, wann er soll streiten, schl\u00e4ft;", "tokens": ["\u201e", "dein", "eig\u00b7ner", "Lands\u00b7Ge\u00b7no\u00df", ",", "wann", "er", "soll", "strei\u00b7ten", ",", "schl\u00e4ft", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie Treu belohnen ist ein strafbares Gesch\u00e4fft.", "tokens": ["\u201e", "die", "Treu", "be\u00b7loh\u00b7nen", "ist", "ein", "straf\u00b7ba\u00b7res", "Ge\u00b7sch\u00e4fft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eder Feind verlachet dich, wann du dich so besch\u00fczest,", "tokens": ["\u201e", "der", "Feind", "ver\u00b7la\u00b7chet", "dich", ",", "wann", "du", "dich", "so", "be\u00b7sch\u00fc\u00b7zest", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df du des eignen Volcks getreues Bluth versprizest.", "tokens": ["\u201e", "da\u00df", "du", "des", "eig\u00b7nen", "Volcks", "ge\u00b7treu\u00b7es", "Bluth", "ver\u00b7spri\u00b7zest", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "105\u201dErwach! erhohle dich! begreiff des Feinds Bem\u00fchn!", "tokens": ["\"", "Er\u00b7wach", "!", "er\u00b7hoh\u00b7le", "dich", "!", "be\u00b7greiff", "des", "Feinds", "Be\u00b7m\u00fchn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ez\u00e4hl, wieg, mi\u00df den Gewinn den du vermeinst zu ziehn!", "tokens": ["\u201e", "z\u00e4hl", ",", "wieg", ",", "mi\u00df", "den", "Ge\u00b7winn", "den", "du", "ver\u00b7meinst", "zu", "ziehn", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ART", "NN", "ART", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "\u201esonst gehest du zu Grund und eilest zum Verderben;", "tokens": ["\u201e", "sonst", "ge\u00b7hest", "du", "zu", "Grund", "und", "ei\u00b7lest", "zum", "Ver\u00b7der\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eerinnre dich des Leids, des Unheils deiner Erben!", "tokens": ["\u201e", "e\u00b7rinn\u00b7re", "dich", "des", "Leids", ",", "des", "Un\u00b7heils", "dei\u00b7ner", "Er\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "ART", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}