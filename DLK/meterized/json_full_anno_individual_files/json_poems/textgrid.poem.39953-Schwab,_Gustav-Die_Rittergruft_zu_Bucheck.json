{"textgrid.poem.39953": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Rittergruft zu Bucheck", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem kahlen H\u00fcgel dort", "tokens": ["Auf", "dem", "kah\u00b7len", "H\u00fc\u00b7gel", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den wenig Mauern", "tokens": ["Mit", "den", "we\u00b7nig", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wohnten gute M\u00e4nner einst", "tokens": ["Wohn\u00b7ten", "gu\u00b7te", "M\u00e4n\u00b7ner", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wert, beim Volk zu dauern.", "tokens": ["Wert", ",", "beim", "Volk", "zu", "dau\u00b7ern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "In Gew\u00f6lben eng und schwarz", "tokens": ["In", "Ge\u00b7w\u00f6l\u00b7ben", "eng", "und", "schwarz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegen jetzt die Braven,", "tokens": ["Lie\u00b7gen", "jetzt", "die", "Bra\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00f6nnen in dem dunkeln Haus", "tokens": ["K\u00f6n\u00b7nen", "in", "dem", "dun\u00b7keln", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne That nicht schlafen;", "tokens": ["Oh\u00b7ne", "That", "nicht", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lauschen aus dem stillen Grab", "tokens": ["Lau\u00b7schen", "aus", "dem", "stil\u00b7len", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den schweren Waffen:", "tokens": ["In", "den", "schwe\u00b7ren", "Waf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie es droben lebe, macht", "tokens": ["Wie", "es", "dro\u00b7ben", "le\u00b7be", ",", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihnen stets zu schaffen.", "tokens": ["Ih\u00b7nen", "stets", "zu", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn's \u00fcbel will ergehn,", "tokens": ["Und", "wenn's", "\u00fc\u00b7bel", "will", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchrt sich's in dem Berge,", "tokens": ["R\u00fchrt", "sich's", "in", "dem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leise t\u00f6nen, lauter dann", "tokens": ["Lei\u00b7se", "t\u00f6\u00b7nen", ",", "lau\u00b7ter", "dann"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVINF", "$,", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die metallnen S\u00e4rge.", "tokens": ["Die", "me\u00b7tall\u00b7nen", "S\u00e4r\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wie wenn einer lagerauf", "tokens": ["Wie", "wenn", "ei\u00b7ner", "la\u00b7ge\u00b7rauf"], "token_info": ["word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Springet, hallen Tritte,", "tokens": ["Sprin\u00b7get", ",", "hal\u00b7len", "Trit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie wenn wer in Waffen geht,", "tokens": ["Wie", "wenn", "wer", "in", "Waf\u00b7fen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schallen dumpfe Schritte.", "tokens": ["Schal\u00b7len", "dump\u00b7fe", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn es solche Zeichen h\u00f6rt,", "tokens": ["Wenn", "es", "sol\u00b7che", "Zei\u00b7chen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht das Volk zusammen,", "tokens": ["Steht", "das", "Volk", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wehret sich mit Rat und That", "tokens": ["Weh\u00b7ret", "sich", "mit", "Rat", "und", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor den Kriegesflammen.", "tokens": ["Vor", "den", "Krie\u00b7ges\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Als der wilde Nachbar j\u00fcngst", "tokens": ["Als", "der", "wil\u00b7de", "Nach\u00b7bar", "j\u00fcngst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder von den H\u00fcgeln", "tokens": ["Nie\u00b7der", "von", "den", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seine fremde Freiheit bracht'", "tokens": ["Sei\u00b7ne", "frem\u00b7de", "Frei\u00b7heit", "bracht'"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf Gewitterfl\u00fcgeln:", "tokens": ["Auf", "Ge\u00b7wit\u00b7ter\u00b7fl\u00fc\u00b7geln", ":"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "O wie \u00e4ngstlich regten sich", "tokens": ["O", "wie", "\u00e4ngst\u00b7lich", "reg\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Gruft die Toten,", "tokens": ["In", "der", "Gruft", "die", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hatten klirrend lang gewarnt,", "tokens": ["Hat\u00b7ten", "klir\u00b7rend", "lang", "ge\u00b7warnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Widerstand geboten.", "tokens": ["Wi\u00b7der\u00b7stand", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und wie nun das Volk sich hob,", "tokens": ["Und", "wie", "nun", "das", "Volk", "sich", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Ruhe zu erstreiten;", "tokens": ["Ru\u00b7he", "zu", "er\u00b7strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ja, da klang's wie Hieb und Sto\u00df", "tokens": ["Ja", ",", "da", "klang's", "wie", "Hieb", "und", "Sto\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Berges Seiten;", "tokens": ["In", "des", "Ber\u00b7ges", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "T\u00f6nt', ein unterirdisch Heer,", "tokens": ["T\u00f6nt'", ",", "ein", "un\u00b7ter\u00b7ir\u00b7disch", "Heer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In den \u00f6den Gr\u00fcften,", "tokens": ["In", "den", "\u00f6\u00b7den", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4r' gestiegen gar zu gern", "tokens": ["W\u00e4r'", "ge\u00b7stie\u00b7gen", "gar", "zu", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ADV", "PTKA", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilfreich zu den L\u00fcften.", "tokens": ["Hilf\u00b7reich", "zu", "den", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.11": {"line.1": {"text": "Denn die Zwietracht l\u00f6s'te bald", "tokens": ["Denn", "die", "Zwiet\u00b7racht", "l\u00f6s'\u00b7te", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Reihen droben;", "tokens": ["Al\u00b7le", "Rei\u00b7hen", "dro\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O wie bange fingen sie", "tokens": ["O", "wie", "ban\u00b7ge", "fin\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drunten an zu toben!", "tokens": ["Drun\u00b7ten", "an", "zu", "to\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Und als Alles unterlag,", "tokens": ["Und", "als", "Al\u00b7les", "un\u00b7ter\u00b7lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Als der Fremdling siegte:", "tokens": ["Als", "der", "Fremd\u00b7ling", "sieg\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie sich's dort, erstickten Halls", "tokens": ["Wie", "sich's", "dort", ",", "er\u00b7stick\u00b7ten", "Halls"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Gr\u00e4ber schmiegte!", "tokens": ["In", "die", "Gr\u00e4\u00b7ber", "schmieg\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Wie man Waffen von sich legt,", "tokens": ["Wie", "man", "Waf\u00b7fen", "von", "sich", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweres Erz und Eisen,", "tokens": ["Schwe\u00b7res", "Erz", "und", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter":