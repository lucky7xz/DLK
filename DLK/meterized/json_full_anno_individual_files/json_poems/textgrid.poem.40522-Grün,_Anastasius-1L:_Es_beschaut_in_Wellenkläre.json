{"textgrid.poem.40522": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es beschaut in Wellenkl\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es beschaut in Wellenkl\u00e4re", "tokens": ["Es", "be\u00b7schaut", "in", "Wel\u00b7len\u00b7kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Fels, ein sch\u00f6ner Greis,", "tokens": ["Sich", "der", "Fels", ",", "ein", "sch\u00f6\u00b7ner", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den See zieht meine F\u00e4hre", "tokens": ["Durch", "den", "See", "zieht", "mei\u00b7ne", "F\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise ihr kristallnes Gleis.", "tokens": ["Lei\u00b7se", "ihr", "kris\u00b7tall\u00b7nes", "Gleis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vorn im Schiff, das Ruder r\u00fchrend,", "tokens": ["Vorn", "im", "Schiff", ",", "das", "Ru\u00b7der", "r\u00fch\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scherzt die schlanke Schifferin!", "tokens": ["Scherzt", "die", "schlan\u00b7ke", "Schif\u00b7fe\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinten, fest das Steuer f\u00fchrend,", "tokens": ["Hin\u00b7ten", ",", "fest", "das", "Steu\u00b7er", "f\u00fch\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Starrt ihr Vater ernst dahin.", "tokens": ["Starrt", "ihr", "Va\u00b7ter", "ernst", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vorn am Schiffe scheint zu glimmen", "tokens": ["Vorn", "am", "Schif\u00b7fe", "scheint", "zu", "glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Fluth ein rother Schein;", "tokens": ["In", "der", "Fluth", "ein", "ro\u00b7ther", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind es Rosen, die da schwimmen?", "tokens": ["Sind", "es", "Ro\u00b7sen", ",", "die", "da", "schwim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, sind's die Wangen dein?", "tokens": ["M\u00e4d\u00b7chen", ",", "sin\u00b7d's", "die", "Wan\u00b7gen", "dein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Hinten an dem Steuer blinken", "tokens": ["Hin\u00b7ten", "an", "dem", "Steu\u00b7er", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings die Wellen silberwei\u00df;", "tokens": ["Rings", "die", "Wel\u00b7len", "sil\u00b7ber\u00b7wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spiegeln sich der Gletscher Zinken?", "tokens": ["Spie\u00b7geln", "sich", "der", "Glet\u00b7scher", "Zin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's dein Lockenschnee, o Greis?", "tokens": ["Ist's", "dein", "Lo\u00b7cken\u00b7schnee", ",", "o", "Greis", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch die Wellen werden rege,", "tokens": ["Doch", "die", "Wel\u00b7len", "wer\u00b7den", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es verschwinden Ros' und Schnee,", "tokens": ["Es", "ver\u00b7schwin\u00b7den", "Ros'", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob Geisterhand sie z\u00f6ge", "tokens": ["Als", "ob", "Geis\u00b7ter\u00b7hand", "sie", "z\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder in den tiefen See.", "tokens": ["Nie\u00b7der", "in", "den", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh, sturmlust'ge Winde fallen", "tokens": ["Weh", ",", "sturm\u00b7lust'\u00b7ge", "Win\u00b7de", "fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Felsen Hinterhalt.", "tokens": ["Aus", "der", "Fel\u00b7sen", "Hin\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "See, dein schlummernd Kindeslallen", "tokens": ["See", ",", "dein", "schlum\u00b7mernd", "Kin\u00b7des\u00b7lal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als Gigantenfeldschrei hallt!", "tokens": ["Als", "Gi\u00b7gan\u00b7ten\u00b7feld\u00b7schrei", "hallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ungeth\u00fcme sind die Wellen,", "tokens": ["Un\u00b7ge\u00b7th\u00fc\u00b7me", "sind", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4umend hoch den Leib empor,", "tokens": ["B\u00e4u\u00b7mend", "hoch", "den", "Leib", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Zottenm\u00e4hnen schwellen,", "tokens": ["Ih\u00b7re", "Zot\u00b7ten\u00b7m\u00e4h\u00b7nen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihr Rachen heult im Chor.", "tokens": ["Und", "ihr", "Ra\u00b7chen", "heult", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ungest\u00fcm in tollem Satze", "tokens": ["Un\u00b7ge\u00b7st\u00fcm", "in", "tol\u00b7lem", "Sat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Springen schnaubend sie heran,", "tokens": ["Sprin\u00b7gen", "schnau\u00b7bend", "sie", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Haun die grimme wei\u00dfe Tatze", "tokens": ["Haun", "die", "grim\u00b7me", "wei\u00b7\u00dfe", "Tat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den morschen, schwanken Kahn.", "tokens": ["In", "den", "mor\u00b7schen", ",", "schwan\u00b7ken", "Kahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber peitschend ihre Flanken", "tokens": ["A\u00b7ber", "peit\u00b7schend", "ih\u00b7re", "Flan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild der Greis sein Ruder schwingt,", "tokens": ["Wild", "der", "Greis", "sein", "Ru\u00b7der", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die Bestienhord' im Schwanken", "tokens": ["Bis", "die", "Bes\u00b7ti\u00b7en\u00b7hord'", "im", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Knirschend, heulend, ihm entspringt.", "tokens": ["Knir\u00b7schend", ",", "heu\u00b7lend", ",", "ihm", "ent\u00b7springt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Leis die krausen Sch\u00e4del streichelnd", "tokens": ["Leis", "die", "krau\u00b7sen", "Sch\u00e4\u00b7del", "strei\u00b7chelnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fchrt die Maid ihr Ruder nun,", "tokens": ["R\u00fchrt", "die", "Maid", "ihr", "Ru\u00b7der", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis, wie H\u00fcndchen, wedelnd, schmeichelnd", "tokens": ["Bis", ",", "wie", "H\u00fcnd\u00b7chen", ",", "we\u00b7delnd", ",", "schmei\u00b7chelnd"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "$,", "PWAV", "NN", "$,", "VVPP", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle ihr zu F\u00fc\u00dfen ruhn.", "tokens": ["Al\u00b7le", "ihr", "zu", "F\u00fc\u00b7\u00dfen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Nimmer sind die Wellen rege,", "tokens": ["Nim\u00b7mer", "sind", "die", "Wel\u00b7len", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder schimmern Ros' und Schnee,", "tokens": ["Wie\u00b7der", "schim\u00b7mern", "Ros'", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob Geisterhand sie lege", "tokens": ["Als", "ob", "Geis\u00b7ter\u00b7hand", "sie", "le\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPER", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Auf den hellen, stillen See.", "tokens": ["Auf", "den", "hel\u00b7len", ",", "stil\u00b7len", "See", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "War ein K\u00e4mpfen das und Kosen,", "tokens": ["War", "ein", "K\u00e4mp\u00b7fen", "das", "und", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abzuringen von dem See,", "tokens": ["Ab\u00b7zu\u00b7rin\u00b7gen", "von", "dem", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4dchen, du die Handvoll Rosen,", "tokens": ["M\u00e4d\u00b7chen", ",", "du", "die", "Hand\u00b7voll", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alter, du die Handvoll Schnee!", "tokens": ["Al\u00b7ter", ",", "du", "die", "Hand\u00b7voll", "Schnee", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Es beschaut in Wellenkl\u00e4re", "tokens": ["Es", "be\u00b7schaut", "in", "Wel\u00b7len\u00b7kl\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich der Fels, ein sch\u00f6ner Greis,", "tokens": ["Sich", "der", "Fels", ",", "ein", "sch\u00f6\u00b7ner", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den See zieht meine F\u00e4hre", "tokens": ["Durch", "den", "See", "zieht", "mei\u00b7ne", "F\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise ihr kristallnes Gleis.", "tokens": ["Lei\u00b7se", "ihr", "kris\u00b7tall\u00b7nes", "Gleis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Vorn im Schiff, das Ruder r\u00fchrend,", "tokens": ["Vorn", "im", "Schiff", ",", "das", "Ru\u00b7der", "r\u00fch\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scherzt die schlanke Schifferin!", "tokens": ["Scherzt", "die", "schlan\u00b7ke", "Schif\u00b7fe\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinten, fest das Steuer f\u00fchrend,", "tokens": ["Hin\u00b7ten", ",", "fest", "das", "Steu\u00b7er", "f\u00fch\u00b7rend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Starrt ihr Vater ernst dahin.", "tokens": ["Starrt", "ihr", "Va\u00b7ter", "ernst", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Vorn am Schiffe scheint zu glimmen", "tokens": ["Vorn", "am", "Schif\u00b7fe", "scheint", "zu", "glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Fluth ein rother Schein;", "tokens": ["In", "der", "Fluth", "ein", "ro\u00b7ther", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind es Rosen, die da schwimmen?", "tokens": ["Sind", "es", "Ro\u00b7sen", ",", "die", "da", "schwim\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, sind's die Wangen dein?", "tokens": ["M\u00e4d\u00b7chen", ",", "sin\u00b7d's", "die", "Wan\u00b7gen", "dein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.16": {"line.1": {"text": "Hinten an dem Steuer blinken", "tokens": ["Hin\u00b7ten", "an", "dem", "Steu\u00b7er", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings die Wellen silberwei\u00df;", "tokens": ["Rings", "die", "Wel\u00b7len", "sil\u00b7ber\u00b7wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spiegeln sich der Gletscher Zinken?", "tokens": ["Spie\u00b7geln", "sich", "der", "Glet\u00b7scher", "Zin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's dein Lockenschnee, o Greis?", "tokens": ["Ist's", "dein", "Lo\u00b7cken\u00b7schnee", ",", "o", "Greis", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Doch die Wellen werden rege,", "tokens": ["Doch", "die", "Wel\u00b7len", "wer\u00b7den", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Es verschwinden Ros' und Schnee,", "tokens": ["Es", "ver\u00b7schwin\u00b7den", "Ros'", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob Geisterhand sie z\u00f6ge", "tokens": ["Als", "ob", "Geis\u00b7ter\u00b7hand", "sie", "z\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder in den tiefen See.", "tokens": ["Nie\u00b7der", "in", "den", "tie\u00b7fen", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Weh, sturmlust'ge Winde fallen", "tokens": ["Weh", ",", "sturm\u00b7lust'\u00b7ge", "Win\u00b7de", "fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Felsen Hinterhalt.", "tokens": ["Aus", "der", "Fel\u00b7sen", "Hin\u00b7ter\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "See, dein schlummernd Kindeslallen", "tokens": ["See", ",", "dein", "schlum\u00b7mernd", "Kin\u00b7des\u00b7lal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "li