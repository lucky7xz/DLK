{"dta.poem.18786": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Volk und Knecht und Ueberwinder", "tokens": ["Volk", "und", "Knecht", "und", "Ue\u00b7ber\u00b7win\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie gestehn, zu jeder Zeit,", "tokens": ["Sie", "ge\u00b7stehn", ",", "zu", "je\u00b7der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6chstes Gl\u00fcck der Erdenkinder", "tokens": ["H\u00f6chs\u00b7tes", "Gl\u00fcck", "der", "Er\u00b7den\u00b7kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey nur die Pers\u00f6nlichkeit.", "tokens": ["Sey", "nur", "die", "Per\u00b7s\u00f6n\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jedes Leben sey zu f\u00fchren,", "tokens": ["Je\u00b7des", "Le\u00b7ben", "sey", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man sich nicht selbst vermisst;", "tokens": ["Wenn", "man", "sich", "nicht", "selbst", "ver\u00b7misst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles k\u00f6nne man verlieren,", "tokens": ["Al\u00b7les", "k\u00f6n\u00b7ne", "man", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man bliebe was man ist.", "tokens": ["Wenn", "man", "blie\u00b7be", "was", "man", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PWS", "PIS", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kann wohl seyn! so wird gemeynet;", "tokens": ["Kann", "wohl", "seyn", "!", "so", "wird", "ge\u00b7mey\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VAINF", "$.", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch ich bin auf andrer Spur,", "tokens": ["Doch", "ich", "bin", "auf", "an\u00b7drer", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles Erdengl\u00fcck vereinet", "tokens": ["Al\u00b7les", "Er\u00b7den\u00b7gl\u00fcck", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Find\u2019 ich in Suleika nur.", "tokens": ["Find'", "ich", "in", "Sul\u00b7ei\u00b7ka", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sie sich an mich verschwendet,", "tokens": ["Wie", "sie", "sich", "an", "mich", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Bin ich mir ein werthes Ich;", "tokens": ["Bin", "ich", "mir", "ein", "wert\u00b7hes", "Ich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ART", "ADJA", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte sie sich weggewendet,", "tokens": ["H\u00e4t\u00b7te", "sie", "sich", "weg\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Augenblicks verl\u00f6r ich mich.", "tokens": ["Au\u00b7gen\u00b7blicks", "ver\u00b7l\u00f6r", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun, mit Hatem w\u00e4r\u2019s zu Ende;", "tokens": ["Nun", ",", "mit", "Ha\u00b7tem", "w\u00e4r's", "zu", "En\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch schon hab\u2019 ich umgelost,", "tokens": ["Doch", "schon", "hab'", "ich", "um\u00b7ge\u00b7lost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verk\u00f6rpre mich behende", "tokens": ["Ich", "ver\u00b7k\u00f6r\u00b7pre", "mich", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Holden den sie kost.", "tokens": ["In", "den", "Hol\u00b7den", "den", "sie", "kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wollte, wo nicht gar ein Rabbi,", "tokens": ["Woll\u00b7te", ",", "wo", "nicht", "gar", "ein", "Rab\u00b7bi", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWAV", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das will mir so recht nicht ein;", "tokens": ["Das", "will", "mir", "so", "recht", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch Ferdusi, Motanabbi,", "tokens": ["Doch", "Fer\u00b7du\u00b7si", ",", "Mo\u00b7ta\u00b7nab\u00b7bi", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Allenfalls der Kaiser seyn.", "tokens": ["Al\u00b7len\u00b7falls", "der", "Kai\u00b7ser", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}