{"dta.poem.10723": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit fleicht hinweg al\u00df wer sie nie gewesen/", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "fleicht", "hin\u00b7weg", "al\u00df", "wer", "sie", "nie", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PWS", "PPER", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "reliquis fa-", "tokens": ["re\u00b7li\u00b7quis", "fa"], "token_info": ["word", "word"], "pos": ["NE", "TRUNC"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "oilior est, &amp;", "tokens": ["o\u00b7i\u00b7li\u00b7or", "est", ",", "&amp;"], "token_info": ["word", "word", "punct", "XML_entity"], "pos": ["FM", "FM", "$,", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "ut mihi vi-", "tokens": ["ut", "mi\u00b7hi", "vi"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "----", "measure": "unknown.measure.zero"}, "line.5": {"text": "ditur, elega\u0303-", "tokens": ["di\u00b7tur", ",", "e\u00b7leg\u00e3"], "token_info": ["word", "punct", "word"], "pos": ["FM.la", "$,", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "tsor quoque.", "tokens": ["tsor", "quo\u00b7que", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Wer sie mit tugent schm\u00fcckt/ ist selig vnd genesen:", "tokens": ["Wer", "sie", "mit", "tu\u00b7gent", "schm\u00fcckt", "/", "ist", "se\u00b7lig", "vnd", "ge\u00b7ne\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$(", "VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Al\u00df den steht alles wohl/ vnd sihet hurtig au\u00df/", "tokens": ["Al\u00df", "den", "steht", "al\u00b7les", "wohl", "/", "vnd", "si\u00b7het", "hur\u00b7tig", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "PIS", "ADV", "$(", "KON", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Al\u00df den wohnt ein sch\u00f6n wirth in einem sch\u00f6nen Hau\u00df.", "tokens": ["Al\u00df", "den", "wohnt", "ein", "sch\u00f6n", "wirth", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "VVFIN", "ART", "ADJD", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-++-+-+-+", "measure": "unknown.measure.septa"}}}}}