{"textgrid.poem.65815": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "89", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du flohest mich. Nenn' einen schlimmen Streich,", "tokens": ["Du", "flo\u00b7hest", "mich", ".", "Nenn'", "ei\u00b7nen", "schlim\u00b7men", "Streich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum? und ich beweise meine S\u00fcnde.", "tokens": ["Wa\u00b7rum", "?", "und", "ich", "be\u00b7wei\u00b7se", "mei\u00b7ne", "S\u00fcn\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprich, ich sei lahm, und sieh! ich hinke gleich:", "tokens": ["Sprich", ",", "ich", "sei", "lahm", ",", "und", "sieh", "!", "ich", "hin\u00b7ke", "gleich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VAFIN", "PTKVZ", "$,", "KON", "VVIMP", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit keinem Wort bestreit' ich deine Gr\u00fcnde.", "tokens": ["Mit", "kei\u00b7nem", "Wort", "be\u00b7streit'", "ich", "dei\u00b7ne", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du kannst mir, Herz, nicht halb so kalt begegnen,", "tokens": ["Du", "kannst", "mir", ",", "Herz", ",", "nicht", "halb", "so", "kalt", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "ADJD", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ersehnte Trennung zu besch\u00f6nigen,", "tokens": ["Er\u00b7sehn\u00b7te", "Tren\u00b7nung", "zu", "be\u00b7sch\u00f6\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als ich mir selber, deines Friedens wegen,", "tokens": ["Als", "ich", "mir", "sel\u00b7ber", ",", "dei\u00b7nes", "Frie\u00b7dens", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "$,", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Will sein, Bekanntschaft w\u00fcrgen, fremde sehn,", "tokens": ["Will", "sein", ",", "Be\u00b7kannt\u00b7schaft", "w\u00fcr\u00b7gen", ",", "frem\u00b7de", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "NN", "VVINF", "$,", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fern deinen Wegen sein will. \u2013 Lippen, schweiget,", "tokens": ["Fern", "dei\u00b7nen", "We\u00b7gen", "sein", "will", ".", "\u2013", "Lip\u00b7pen", ",", "schwei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAINF", "VMFIN", "$.", "$(", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nennt k\u00fcnftig nur den teuern Namen nicht!", "toke