{"textgrid.poem.52126": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drey Theil, als Seel und Geist und Leib, Mensch sind in dir,", "tokens": ["Drey", "Theil", ",", "als", "Seel", "und", "Geist", "und", "Leib", ",", "Mensch", "sind", "in", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "KOUS", "NN", "KON", "NN", "KON", "NN", "$,", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir sehn, so iedes bricht, aus seinem Mittel f\u00fcr:", "tokens": ["Wir", "sehn", ",", "so", "ie\u00b7des", "bricht", ",", "aus", "sei\u00b7nem", "Mit\u00b7tel", "f\u00fcr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PIS", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gesundheit und Vernunfft und Heiligkeit sich f\u00e4rben,", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "Ver\u00b7nunfft", "und", "Hei\u00b7lig\u00b7keit", "sich", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo nicht, so mu\u00df der Mensch dran krancken oder sterben.", "tokens": ["Wo", "nicht", ",", "so", "mu\u00df", "der", "Mensch", "dran", "kran\u00b7cken", "o\u00b7der", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VMFIN", "ART", "NN", "PAV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Geist ist frisch, wenn er Gott nach dem Wesen kennt,", "tokens": ["Der", "Geist", "ist", "frisch", ",", "wenn", "er", "Gott", "nach", "dem", "We\u00b7sen", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gott nach dem Willen ehrt: Gott nachzufolgen brennt:", "tokens": ["Gott", "nach", "dem", "Wil\u00b7len", "ehrt", ":", "Gott", "nach\u00b7zu\u00b7fol\u00b7gen", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$.", "NN", "VVIZU", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und dieses hat er blo\u00df im Glauben auserkohren,", "tokens": ["Und", "die\u00b7ses", "hat", "er", "blo\u00df", "im", "Glau\u00b7ben", "au\u00b7ser\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn ihn ist kennen, ehrn und brennen gantz verlohren.", "tokens": ["Ohn", "ihn", "ist", "ken\u00b7nen", ",", "ehrn", "und", "bren\u00b7nen", "gantz", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "VVINF", "$,", "ADJD", "KON", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Seel ist frisch: wann sie rein als die Engel liebt,", "tokens": ["Die", "Seel", "ist", "frisch", ":", "wann", "sie", "rein", "als", "die", "En\u00b7gel", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PWAV", "PPER", "ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fromm, als die Geister ist, frey, als die J\u00fcnger, giebt:", "tokens": ["Fromm", ",", "als", "die", "Geis\u00b7ter", "ist", ",", "frey", ",", "als", "die", "J\u00fcn\u00b7ger", ",", "giebt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ART", "NN", "VAFIN", "$,", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dieses kan allein die Wei\u00dfheit in ihr machen,", "tokens": ["Und", "die\u00b7ses", "kan", "al\u00b7lein", "die", "Wei\u00df\u00b7heit", "in", "ihr", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn Sie sind lieben, seyn und geben todte Sachen.", "tokens": ["Ohn", "Sie", "sind", "lie\u00b7ben", ",", "seyn", "und", "ge\u00b7ben", "tod\u00b7te", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "VVINF", "$,", "VAINF", "KON", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Leib ist frisch: wann er an Sinnen Kr\u00e4ffte sp\u00fcrt,", "tokens": ["Der", "Leib", "ist", "frisch", ":", "wann", "er", "an", "Sin\u00b7nen", "Kr\u00e4ff\u00b7te", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PWAV", "PPER", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Gliedern St\u00e4rcke f\u00fchlt, an Adern Speise f\u00fchrt:", "tokens": ["An", "Glie\u00b7dern", "St\u00e4r\u00b7cke", "f\u00fchlt", ",", "an", "A\u00b7dern", "Spei\u00b7se", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dieses kan in Ihm die M\u00e4\u00dfigkeit erhalten,", "tokens": ["Und", "die\u00b7ses", "kan", "in", "Ihm", "die", "M\u00e4\u00b7\u00dfig\u00b7keit", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohn Sie sehn wir die Kr\u00e4fft' und St\u00e4rck' und Speis' erkalten.", "tokens": ["Ohn", "Sie", "sehn", "wir", "die", "Kr\u00e4fft'", "und", "St\u00e4rck", "und", "Speis'", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ohn Glauben seyn, das ist Ohn Gotts Erk\u00e4nntn\u00fc\u00df seyn:", "tokens": ["Ohn", "Glau\u00b7ben", "seyn", ",", "das", "ist", "Ohn", "Gotts", "Er\u00b7k\u00e4nnt\u00b7n\u00fc\u00df", "seyn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$,", "PDS", "VAFIN", "APPR", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn Wei\u00dfheit seyn, das ist entb\u00e4rn der Tugend Schein:", "tokens": ["Ohn", "Wei\u00df\u00b7heit", "seyn", ",", "das", "ist", "ent\u00b7b\u00e4rn", "der", "Tu\u00b7gend", "Schein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$,", "PDS", "VAFIN", "NE", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ohn Maa\u00dfe seyn, das ist sich t\u00f6dten vor dem Sterben,", "tokens": ["Ohn", "Maa\u00b7\u00dfe", "seyn", ",", "das", "ist", "sich", "t\u00f6d\u00b7ten", "vor", "dem", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$,", "PDS", "VAFIN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mensch, auff den Schlag mu\u00df Geist und Seel u. Leib verterben.", "tokens": ["Mensch", ",", "auff", "den", "Schlag", "mu\u00df", "Geist", "und", "Seel", "u.", "Leib", "ver\u00b7ter\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "VMFIN", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-++-+-", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Auff da\u00df du nun nicht gantz verterbest, suche Heil,", "tokens": ["Auff", "da\u00df", "du", "nun", "nicht", "gantz", "ver\u00b7ter\u00b7best", ",", "su\u00b7che", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schau der Raphael gibt iedem Theil sein Theil:", "tokens": ["Und", "schau", "der", "Ra\u00b7phael", "gibt", "ie\u00b7dem", "Theil", "sein", "Theil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NE", "VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der treue Raph\u00e4el, der als ein Artzt voll Ehren", "tokens": ["Der", "treu\u00b7e", "Ra\u00b7ph\u00e4\u00b7el", ",", "der", "als", "ein", "Artzt", "voll", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "KOUS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gesundheit und Vernunfft und Heiligkeit kan lehren.", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "Ver\u00b7nunfft", "und", "Hei\u00b7lig\u00b7keit", "kan", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mensch, ist der Geist ie kranck: Er wird ergeistern sich,", "tokens": ["Mensch", ",", "ist", "der", "Geist", "ie", "kranck", ":", "Er", "wird", "er\u00b7geis\u00b7tern", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in das Hertze Gott's versenken ihn und dich:", "tokens": ["Und", "in", "das", "Hert\u00b7ze", "Gott's", "ver\u00b7sen\u00b7ken", "ihn", "und", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "VVFIN", "NE", "VVFIN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und durch die neue Krafft des Wesens \u00fcberfeuchten,", "tokens": ["Und", "durch", "die", "neu\u00b7e", "Krafft", "des", "We\u00b7sens", "\u00fc\u00b7ber\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df nichts im Glauben wird als JESUS wieder leuchten.", "tokens": ["Bi\u00df", "nichts", "im", "Glau\u00b7ben", "wird", "als", "Je\u00b7SUS", "wie\u00b7der", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPRART", "NN", "VAFIN", "KOKOM", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mensch, ist die Seel ie kranck: Er wird sie f\u00fchrn zur Zucht,", "tokens": ["Mensch", ",", "ist", "die", "Seel", "ie", "kranck", ":", "Er", "wird", "sie", "f\u00fchrn", "zur", "Zucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die er in JESUS Hertz und seiner Sanfft Muth sucht:", "tokens": ["Die", "er", "in", "Je\u00b7SUS", "Hertz", "und", "sei\u00b7ner", "Sanfft", "Muth", "sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "NN", "KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird deine Seele da mit Blut und Wa\u00dfer reinen,", "tokens": ["Wird", "dei\u00b7ne", "See\u00b7le", "da", "mit", "Blut", "und", "Wa\u00b7\u00dfer", "rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df nichts als Wei\u00dfheit wird durch deinen Willen scheinen.", "tokens": ["Bi\u00df", "nichts", "als", "Wei\u00df\u00b7heit", "wird", "durch", "dei\u00b7nen", "Wil\u00b7len", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mensch, ist der Leib ie kranck: Er wird ein Artzney gl\u00fchn,", "tokens": ["Mensch", ",", "ist", "der", "Leib", "ie", "kranck", ":", "Er", "wird", "ein", "Artz\u00b7ney", "gl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Natur ihr Hertz und de\u00dfen Dunst abziehn:", "tokens": ["Und", "der", "Na\u00b7tur", "ihr", "Hertz", "und", "de\u00b7\u00dfen", "Dunst", "ab\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Grund Feucht' in dir u. die Grund Hitz' erg\u00e4ntzen,", "tokens": ["Und", "die", "Grund", "Feucht'", "in", "dir", "u.", "die", "Grund", "Hitz'", "er\u00b7g\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df nichts als M\u00e4\u00dfigkeit durch Sinn und Leib wird gl\u00e4ntzen.", "tokens": ["Da\u00df", "nichts", "als", "M\u00e4\u00b7\u00dfig\u00b7keit", "durch", "Sinn", "und", "Leib", "wird", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wer nun, als wie es Leib und Seel und Geist ist wehrt,", "tokens": ["Wer", "nun", ",", "als", "wie", "es", "Leib", "und", "Seel", "und", "Geist", "ist", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PWAV", "PPER", "NN", "KON", "NN", "KON", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesundheit und Vernunfft und Heiligkeit begehrt:", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "Ver\u00b7nunfft", "und", "Hei\u00b7lig\u00b7keit", "be\u00b7gehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "pu