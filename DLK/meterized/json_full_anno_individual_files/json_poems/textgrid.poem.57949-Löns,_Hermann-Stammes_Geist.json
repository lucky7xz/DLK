{"textgrid.poem.57949": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Stammes Geist", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr den \u00e4gidischen Torplatz", "tokens": ["F\u00fcr", "den", "\u00e4\u00b7gi\u00b7di\u00b7schen", "Tor\u00b7platz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ich das Geld bestimmt;", "tokens": ["Hatt'", "ich", "das", "Geld", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie kommt es, das man es pl\u00f6tzlich", "tokens": ["Wie", "kommt", "es", ",", "das", "man", "es", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "PPER", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum Rathausbaue nimmt?", "tokens": ["Zum", "Rat\u00b7haus\u00b7bau\u00b7e", "nimmt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Brunnen sollte es werden,", "tokens": ["Ein", "Brun\u00b7nen", "soll\u00b7te", "es", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf einem freien Platz;", "tokens": ["Auf", "ei\u00b7nem", "frei\u00b7en", "Platz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Dings f\u00fcr eine Treppe,", "tokens": ["Kein", "Dings", "f\u00fcr", "ei\u00b7ne", "Trep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn das ist f\u00fcr die Katz.", "tokens": ["Denn", "das", "ist", "f\u00fcr", "die", "Katz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und bin ich auch tot und begraben:", "tokens": ["Und", "bin", "ich", "auch", "tot", "und", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hier ist mein Testament!", "tokens": ["Hier", "ist", "mein", "Tes\u00b7ta\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Herren, nehmt eure Brillen,", "tokens": ["Ihr", "Her\u00b7ren", ",", "nehmt", "eu\u00b7re", "Bril\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn ihr's nicht lesen k\u00f6nnt!", "tokens": ["Wenn", "ih\u00b7r's", "nicht", "le\u00b7sen", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier hei\u00dft's: ent- oder -weder!", "tokens": ["Hier", "hei\u00dft's", ":", "ent", "o\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn nicht: Das Geld gebt her!", "tokens": ["Wenn", "nicht", ":", "Das", "Geld", "gebt", "her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich nach dem Tode bemogeln,", "tokens": ["Mich", "nach", "dem", "To\u00b7de", "be\u00b7mo\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Das eggert mich doch zu sehr!", "tokens": ["Das", "eg\u00b7gert", "mich", "doch", "zu", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "F\u00fcr den \u00e4gidischen Torplatz", "tokens": ["F\u00fcr", "den", "\u00e4\u00b7gi\u00b7di\u00b7schen", "Tor\u00b7platz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt' ich das Geld bestimmt;", "tokens": ["Hatt'", "ich", "das", "Geld", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie kommt es, das man es pl\u00f6tzlich", "tokens": ["Wie", "kommt", "es", ",", "das", "man", "es", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "PPER", "ADJD"], "meter"