{"textgrid.poem.33957": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein schlummerndes Kind", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, o Kindlein, vor dir stehe,", "tokens": ["Wenn", "ich", ",", "o", "Kin\u00b7dlein", ",", "vor", "dir", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich im Traum dich l\u00e4cheln sehe,", "tokens": ["Wenn", "ich", "im", "Traum", "dich", "l\u00e4\u00b7cheln", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn du ergl\u00fchst so wunderbar,", "tokens": ["Wenn", "du", "er\u00b7gl\u00fchst", "so", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ahne ich mit s\u00fc\u00dfem Grauen:", "tokens": ["Da", "ah\u00b7ne", "ich", "mit", "s\u00fc\u00b7\u00dfem", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "D\u00fcrft' ich in deine Tr\u00e4ume schauen,", "tokens": ["D\u00fcrft'", "ich", "in", "dei\u00b7ne", "Tr\u00e4u\u00b7me", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], 