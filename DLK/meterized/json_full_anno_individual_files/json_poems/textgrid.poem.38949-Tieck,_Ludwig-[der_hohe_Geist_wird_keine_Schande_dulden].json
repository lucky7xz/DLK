{"textgrid.poem.38949": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[der hohe Geist wird keine Schande dulden]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der hohe Geist wird keine Schande dulden,", "tokens": ["Der", "ho\u00b7he", "Geist", "wird", "kei\u00b7ne", "Schan\u00b7de", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein k\u00fchner Sinn erkennet keine Schranken,", "tokens": ["Ein", "k\u00fch\u00b7ner", "Sinn", "er\u00b7ken\u00b7net", "kei\u00b7ne", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer feste steht wird nicht so leichte wanken,", "tokens": ["Wer", "fes\u00b7te", "steht", "wird", "nicht", "so", "leich\u00b7te", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "VVFIN", "VAFIN", "PTKNEG", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch junges Blut macht gar zu gerne Schulden.", "tokens": ["Doch", "jun\u00b7ges", "Blut", "macht", "gar", "zu", "ger\u00b7ne", "Schul\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denkt, sechszehn Groschen machen einen Gulden;", "tokens": ["Denkt", ",", "sechs\u00b7zehn", "Gro\u00b7schen", "ma\u00b7chen", "ei\u00b7nen", "Gul\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Brutus einst die besten R\u00f6mer sanken,", "tokens": ["Mit", "Bru\u00b7tus", "einst", "die", "bes\u00b7ten", "R\u00f6\u00b7mer", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer Ruhe liebt, wird nur ungerne zanken,", "tokens": ["Wer", "Ru\u00b7he", "liebt", ",", "wird", "nur", "un\u00b7ger\u00b7ne", "zan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Man sagt vergolden, aber auch vergulden. \u2013", "tokens": ["Man", "sagt", "ver\u00b7gol\u00b7den", ",", "a\u00b7ber", "auch", "ver\u00b7gul\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "VVINF", "$,", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du Eremit in deiner stillen Klause", "tokens": ["Du", "E\u00b7re\u00b7mit", "in", "dei\u00b7ner", "stil\u00b7len", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bel\u00e4chelst wohl den warmen Sonnenschein,", "tokens": ["Be\u00b7l\u00e4\u00b7chelst", "wohl", "den", "war\u00b7men", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "AD