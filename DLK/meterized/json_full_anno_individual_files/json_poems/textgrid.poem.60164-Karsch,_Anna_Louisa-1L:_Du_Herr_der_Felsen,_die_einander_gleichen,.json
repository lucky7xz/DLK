{"textgrid.poem.60164": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Herr der Felsen, die einander gleichen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Herr der Felsen, die einander gleichen,", "tokens": ["Du", "Herr", "der", "Fel\u00b7sen", ",", "die", "ein\u00b7an\u00b7der", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie S\u00f6hne die Ein Weib gebahr;", "tokens": ["Wie", "S\u00f6h\u00b7ne", "die", "Ein", "Weib", "ge\u00b7bahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stolz wuchsen sie empor, den Himmel zu erreichen:", "tokens": ["Stolz", "wuch\u00b7sen", "sie", "em\u00b7por", ",", "den", "Him\u00b7mel", "zu", "er\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf ihren Gipfel floh ein Paar", "tokens": ["Auf", "ih\u00b7ren", "Gip\u00b7fel", "floh", "ein", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verliebte, als f\u00fcr ihre schwarze S\u00fcnde", "tokens": ["Ver\u00b7lieb\u00b7te", ",", "als", "f\u00fcr", "ih\u00b7re", "schwar\u00b7ze", "S\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die erste Welt in Wasser schwomm!", "tokens": ["Die", "ers\u00b7te", "Welt", "in", "Was\u00b7ser", "schwomm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da forschte Gott, ob er sie schonens-werth befinde", "tokens": ["Da", "forschte", "Gott", ",", "ob", "er", "sie", "scho\u00b7nens\u00b7werth", "be\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und ihrer beyder Liebe fromm.", "tokens": ["Und", "ih\u00b7rer", "bey\u00b7der", "Lie\u00b7be", "fromm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Blick in sie aus seines Dunkels H\u00fclle", "tokens": ["Ein", "Blick", "in", "sie", "aus", "sei\u00b7nes", "Dun\u00b7kels", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fand ihre Seelen ganz verderbt;", "tokens": ["Fand", "ih\u00b7re", "See\u00b7len", "ganz", "ver\u00b7derbt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, da\u00df ihr Leben nicht die Erde neu erf\u00fclle", "tokens": ["Und", ",", "da\u00df", "ihr", "Le\u00b7ben", "nicht", "die", "Er\u00b7de", "neu", "er\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Bosheit, sprach der R\u00e4cher: sterbt!", "tokens": ["Mit", "Bos\u00b7heit", ",", "sprach", "der", "R\u00e4\u00b7cher", ":", "sterbt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Fluth vernahm es, die Orcane h\u00f6rten", "tokens": ["Die", "Fluth", "ver\u00b7nahm", "es", ",", "die", "Or\u00b7ca\u00b7ne", "h\u00f6r\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und st\u00fcrzten auf die Felsen los,", "tokens": ["Und", "st\u00fcrz\u00b7ten", "auf", "die", "Fel\u00b7sen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Kriegesheere die Jerusalem zerst\u00f6rten:", "tokens": ["Wie", "Krie\u00b7ges\u00b7hee\u00b7re", "die", "Je\u00b7ru\u00b7sa\u00b7lem", "zer\u00b7st\u00f6r\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NE", "VVFIN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da bebten von der Wellen Sto\u00df", "tokens": ["Da", "beb\u00b7ten", "von", "der", "Wel\u00b7len", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der m\u00fctterlichen Erde Zwillings S\u00f6hne", "tokens": ["Der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Er\u00b7de", "Zwil\u00b7lings", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dreymahl; und die Gewalt zertrieb", "tokens": ["Drey\u00b7mahl", ";", "und", "die", "Ge\u00b7walt", "zer\u00b7trieb"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie also reissend da\u00df auf einem Fels die Sch\u00f6ne,", "tokens": ["Sie", "al\u00b7so", "reis\u00b7send", "da\u00df", "auf", "ei\u00b7nem", "Fels", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der J\u00fcngling auf dem andern blieb.", "tokens": ["Der", "J\u00fcng\u00b7ling", "auf", "dem", "an\u00b7dern", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Wolken-Welt die \u00fcber ihren K\u00f6pfen", "tokens": ["Die", "Wol\u00b7ken\u00b7Welt", "die", "\u00fc\u00b7ber", "ih\u00b7ren", "K\u00f6p\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ganzen Meeren Wasser hing", "tokens": ["Mit", "gan\u00b7zen", "Mee\u00b7ren", "Was\u00b7ser", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward finstrer, schien sich selber zu ersch\u00f6pfen", "tokens": ["Ward", "finst\u00b7rer", ",", "schien", "sich", "sel\u00b7ber", "zu", "er\u00b7sch\u00f6p\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indem das M\u00e4dchen untergieng.", "tokens": ["In\u00b7dem", "das", "M\u00e4d\u00b7chen", "un\u00b7ter\u00b7gieng", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die dicke Luft erscholl von dem Geheule", "tokens": ["Die", "di\u00b7cke", "Luft", "er\u00b7scholl", "von", "dem", "Ge\u00b7heu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des J\u00fcnglings der zum Wasser sprach:", "tokens": ["Des", "J\u00fcng\u00b7lings", "der", "zum", "Was\u00b7ser", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm schnell herauf gestiegen Wasser! eile!", "tokens": ["Komm", "schnell", "her\u00b7auf", "ge\u00b7stie\u00b7gen", "Was\u00b7ser", "!", "ei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es kam, er schwomm dem M\u00e4dchen nach.", "tokens": ["Es", "kam", ",", "er", "schwomm", "dem", "M\u00e4d\u00b7chen", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie k\u00e4mpfte noch mit ihren Untersinken,", "tokens": ["Sie", "k\u00e4mpf\u00b7te", "noch", "mit", "ih\u00b7ren", "Un\u00b7ter\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ihr Geliebter sie umfing", "tokens": ["Als", "ihr", "Ge\u00b7lieb\u00b7ter", "sie", "um\u00b7fing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und geizig war, den Geist in sich zu trinken,", "tokens": ["Und", "gei\u00b7zig", "war", ",", "den", "Geist", "in", "sich", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ART", "NN", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der an den kalten Lippen hing.", "tokens": ["Der", "an", "den", "kal\u00b7ten", "Lip\u00b7pen", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Muse sagt, sie l\u00e4gen an dem Fusse", "tokens": ["Die", "Mu\u00b7se", "sagt", ",", "sie", "l\u00e4\u00b7gen", "an", "dem", "Fus\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des einen Felsen, w\u00e4ren Stein", "tokens": ["Des", "ei\u00b7nen", "Fel\u00b7sen", ",", "w\u00e4\u00b7ren", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "$,", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unkennbar durch die Zeit, wie ein vom Regengusse", "tokens": ["Un\u00b7kenn\u00b7bar", "durch", "die", "Zeit", ",", "wie", "ein", "vom", "Re\u00b7gen\u00b7gus\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "PWAV", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwaschnes Bildni\u00df pflegt zu seyn.", "tokens": ["Ver\u00b7waschnes", "Bild\u00b7ni\u00df", "pflegt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Felsen aber mit erhabnem Haupte", "tokens": ["Die", "Fel\u00b7sen", "a\u00b7ber", "mit", "er\u00b7hab\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verk\u00fcndigen des H\u00f6chsten Hand,", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gen", "des", "H\u00f6chs\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00fcber eine Welt, die keine Gottheit glaubte,", "tokens": ["Der", "\u00fc\u00b7ber", "ei\u00b7ne", "Welt", ",", "die", "kei\u00b7ne", "Got\u00b7theit", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Tod in Wolken abgesandt.", "tokens": ["Den", "Tod", "in", "Wol\u00b7ken", "ab\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Sie sehen sich, troz allen Ungewittern", "tokens": ["Sie", "se\u00b7hen", "sich", ",", "troz", "al\u00b7len", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unumgest\u00fcrzt einander an:", "tokens": ["Un\u00b7um\u00b7ge\u00b7st\u00fcrzt", "ein\u00b7an\u00b7der", "an", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steht, wenn Schlag auf Schlag die Erde wird ersch\u00fcttern", "tokens": ["So", "steht", ",", "wenn", "Schlag", "auf", "Schlag", "die", "Er\u00b7de", "wird", "er\u00b7sch\u00fct\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "NN", "APPR", "NN", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Christ, und der rechtschafne Mann!", "tokens": ["Der", "Christ", ",", "und", "der", "recht\u00b7schaf\u00b7ne", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du H