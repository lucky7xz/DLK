{"textgrid.poem.36373": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der tr\u00fcbe Winter ist vorbei,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der tr\u00fcbe Winter ist vorbei,", "tokens": ["Der", "tr\u00fc\u00b7be", "Win\u00b7ter", "ist", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kranich' wieder kehren.", "tokens": ["Die", "Kra\u00b7nich'", "wie\u00b7der", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun reget sich der V\u00f6gel Schrei,", "tokens": ["Nun", "re\u00b7get", "sich", "der", "V\u00f6\u00b7gel", "Schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Neste sich vermehren.", "tokens": ["Die", "Nes\u00b7te", "sich", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Laub allgemach", "tokens": ["Laub", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Nun schleicht an Tag,", "tokens": ["Nun", "schleicht", "an", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Bl\u00fcmlein hold sich melden,", "tokens": ["Die", "Bl\u00fcm\u00b7lein", "hold", "sich", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Schl\u00e4nglein krumm", "tokens": ["Wie", "Schl\u00e4ng\u00b7lein", "krumm"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gehn spielend um", "tokens": ["Gehn", "spie\u00b7lend", "um"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Die B\u00e4chlein k\u00fchl in W\u00e4lden.", "tokens": ["Die", "B\u00e4ch\u00b7lein", "k\u00fchl", "in", "W\u00e4l\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Br\u00fcnnlein klar und Quellen rein", "tokens": ["Der", "Br\u00fcnn\u00b7lein", "klar", "und", "Quel\u00b7len", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel hie, viel dort erscheinen,", "tokens": ["Viel", "hie", ",", "viel", "dort", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "All' silberwei\u00dfe T\u00f6chterlein", "tokens": ["All'", "sil\u00b7ber\u00b7wei\u00b7\u00dfe", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hohen Berg' und Steinen.", "tokens": ["Der", "ho\u00b7hen", "Ber\u00b7g'", "und", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In gro\u00dfer Meng'", "tokens": ["In", "gro\u00b7\u00dfer", "Meng'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sie mit Gedr\u00e4ng,", "tokens": ["Sie", "mit", "Ge\u00b7dr\u00e4ng", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wie Pfeil' von Felsen zielen.", "tokens": ["Wie", "Pfeil'", "von", "Fel\u00b7sen", "zie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie flie\u00dfen her,", "tokens": ["Sie", "flie\u00b7\u00dfen", "her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und rauschen sehr", "tokens": ["Und", "rau\u00b7schen", "sehr"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und mit den Steinlein spielen.", "tokens": ["Und", "mit", "den", "Stein\u00b7lein", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die J\u00e4gerin Diana stolz,", "tokens": ["Die", "J\u00e4\u00b7ge\u00b7rin", "Di\u00b7a\u00b7na", "stolz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wald- und Wassernymphen", "tokens": ["Die", "Wald", "und", "Was\u00b7ser\u00b7nym\u00b7phen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gehn wieder froh im gr\u00fcnen Holz,", "tokens": ["Gehn", "wie\u00b7der", "froh", "im", "gr\u00fc\u00b7nen", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Spielen, Scherz und Schimpfen.", "tokens": ["Mit", "Spie\u00b7len", ",", "Scherz", "und", "Schimp\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die goldne Sonn'", "tokens": ["Die", "gold\u00b7ne", "Sonn'"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Schm\u00fcckt ihre Kron,", "tokens": ["Schm\u00fcckt", "ih\u00b7re", "Kron", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Den K\u00f6cher f\u00fcllt mit Pfeilen;", "tokens": ["Den", "K\u00f6\u00b7cher", "f\u00fcllt", "mit", "Pfei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die muntern Ross'", "tokens": ["Die", "mun\u00b7tern", "Ross'"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "L\u00e4\u00dft laufen los", "tokens": ["L\u00e4\u00dft", "lau\u00b7fen", "los"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "An himmelblauen Meilen.", "tokens": ["An", "him\u00b7mel\u00b7blau\u00b7en", "Mei\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit ihr die k\u00fchlen Abendwind',", "tokens": ["Mit", "ihr", "die", "k\u00fch\u00b7len", "A\u00b7bend\u00b7wind'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Knaben sanft von Sitten,", "tokens": ["Wie", "Kna\u00b7ben", "sanft", "von", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Spielen in der Luft gesinnt", "tokens": ["Zum", "Spie\u00b7len", "in", "der", "Luft", "ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf kleinen Wolken ritten.", "tokens": ["Auf", "klei\u00b7nen", "Wol\u00b7ken", "rit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die B\u00e4um' und \u00c4st'", "tokens": ["Die", "B\u00e4um'", "und", "\u00c4st'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Auch tun ihr Best',", "tokens": ["Auch", "tun", "ihr", "Best'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bereichern sich mit Schatten,", "tokens": ["Be\u00b7rei\u00b7chern", "sich", "mit", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da sich das Wild", "tokens": ["Da", "sich", "das", "Wild"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Verbirgt und h\u00fcllt,", "tokens": ["Ver\u00b7birgt", "und", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Wann's mu\u00df vor Hitz' ermatten.", "tokens": ["Wann's", "mu\u00df", "vor", "Hitz'", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Meng' der V\u00f6glein h\u00f6ren l\u00e4\u00dft", "tokens": ["Die", "Meng'", "der", "V\u00f6\u00b7glein", "h\u00f6\u00b7ren", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Lied von Tirelieren,", "tokens": ["Ihr", "Lied", "von", "Ti\u00b7re\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da klinget aus so manchem Nest", "tokens": ["Da", "klin\u00b7get", "aus", "so", "man\u00b7chem", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vereintes Musizieren.", "tokens": ["Ver\u00b7ein\u00b7tes", "Mu\u00b7si\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Zweiglein schwank", "tokens": ["Die", "Zwei\u00b7glein", "schwank"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zum Vogelsang", "tokens": ["Zum", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Sich auf, sich nieder neigen;", "tokens": ["Sich", "auf", ",", "sich", "nie\u00b7der", "nei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$,", "PRF", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im Gr\u00fcnen gehn", "tokens": ["Im", "Gr\u00fc\u00b7nen", "gehn"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "H\u00f6rt man ein Wehn,", "tokens": ["H\u00f6rt", "man", "ein", "Wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Wie Lautenspiel und Geigen.", "tokens": ["Wie", "Lau\u00b7ten\u00b7spiel", "und", "Gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo man nur schaut, die ganze Welt", "tokens": ["Wo", "man", "nur", "schaut", ",", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Freuden sich tut r\u00fcsten;", "tokens": ["Zu", "Freu\u00b7den", "sich", "tut", "r\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Scherzen alles ist gestellt.", "tokens": ["Zum", "Scher\u00b7zen", "al\u00b7les", "ist", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt alles froh in L\u00fcsten.", "tokens": ["Schwebt", "al\u00b7les", "froh", "in", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich nur allein,", "tokens": ["Ich", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich leide Pein,", "tokens": ["Ich", "lei\u00b7de", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ohn' End' ich werd' gequ\u00e4let,", "tokens": ["Ohn'", "End'", "ich", "werd'", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seit ich mit dir,", "tokens": ["Seit", "ich", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und du mit mir,", "tokens": ["Und", "du", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "O Jesu dich verm\u00e4hlet.", "tokens": ["O", "Je\u00b7su", "dich", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nur ich, o Jesu bin allein", "tokens": ["Nur", "ich", ",", "o", "Je\u00b7su", "bin", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "NE", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stetem Leid umgeben,", "tokens": ["Mit", "ste\u00b7tem", "Leid", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur ich mu\u00df tief in Schmerzen sein,", "tokens": ["Nur", "ich", "mu\u00df", "tief", "in", "Schmer\u00b7zen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "ADJD", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil nicht bei dir darf leben.", "tokens": ["Weil", "nicht", "bei", "dir", "darf", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ewige Klag',", "tokens": ["E\u00b7wi\u00b7ge", "Klag'", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dauernde Plag',", "tokens": ["Dau\u00b7ern\u00b7de", "Plag'", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "So lang von dir zu scheiden;", "tokens": ["So", "lang", "von", "dir", "zu", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-