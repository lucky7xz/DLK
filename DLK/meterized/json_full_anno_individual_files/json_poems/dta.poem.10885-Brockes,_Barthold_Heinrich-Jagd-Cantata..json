{"dta.poem.10885": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jagd-Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da Luft und Erde wundersch\u00f6n:", "tokens": ["Da", "Luft", "und", "Er\u00b7de", "wun\u00b7der\u00b7sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00dft uns den, der alle Pracht", "tokens": ["So", "la\u00dft", "uns", "den", ",", "der", "al\u00b7le", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ART", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu unsrer Lust, und seiner Ehre macht,", "tokens": ["Zu", "uns\u00b7rer", "Lust", ",", "und", "sei\u00b7ner", "Eh\u00b7re", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch bey der Jagd,", "tokens": ["Auch", "bey", "der", "Jagd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Zu unsrer Lust, zu seinen Ehren, sehn!", "tokens": ["Zu", "uns\u00b7rer", "Lust", ",", "zu", "sei\u00b7nen", "Eh\u00b7ren", ",", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}