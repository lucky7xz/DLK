{"textgrid.poem.66479": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "Meine Kunst", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, meine Kunst, hast durch das Leben", "tokens": ["Du", ",", "mei\u00b7ne", "Kunst", ",", "hast", "durch", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als holde Gottheit mich gef\u00fchrt!", "tokens": ["Als", "hol\u00b7de", "Got\u00b7theit", "mich", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir allein ward mir gegeben,", "tokens": ["Von", "dir", "al\u00b7lein", "ward", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was mich als Freud' und Gl\u00fcck ber\u00fchrt.", "tokens": ["Was", "mich", "als", "Freud'", "und", "Gl\u00fcck", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch dich empfing ich Kraft und Willen", "tokens": ["Durch", "dich", "emp\u00b7fing", "ich", "Kraft", "und", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir selbst zu bilden meine Welt,", "tokens": ["Mir", "selbst", "zu", "bil\u00b7den", "mei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die ich mir erschuf im Stillen,", "tokens": ["Und", "die", "ich", "mir", "er\u00b7schuf", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hast du mit heil'gem Strahl erhellt.", "tokens": ["Hast", "du", "mit", "heil'\u00b7gem", "Strahl", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom ersten Stammeln, da der Knabe", "tokens": ["Vom", "ers\u00b7ten", "Stam\u00b7meln", ",", "da", "der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die fl\u00fcch'gen Reime kaum erhascht,", "tokens": ["Die", "fl\u00fcch'\u00b7gen", "Rei\u00b7me", "kaum", "er\u00b7hascht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Empfand er, da\u00df der Dichtung Gabe", "tokens": ["Emp\u00b7fand", "er", ",", "da\u00df", "der", "Dich\u00b7tung", "Ga\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Seligkeit ihn \u00fcberrascht.", "tokens": ["Wie", "Se\u00b7lig\u00b7keit", "ihn", "\u00fc\u00b7berr\u00b7ascht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie war's, die auch ein hartes Ringen", "tokens": ["Sie", "wa\u00b7r's", ",", "die", "auch", "ein", "har\u00b7tes", "Rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mit m\u00e4chtigem Gef\u00fchl verwob,", "tokens": ["Mit", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7f\u00fchl", "ver\u00b7wob", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie, wenn im Hoffen auf Gelingen", "tokens": ["Wie", ",", "wenn", "im", "Hof\u00b7fen", "auf", "Ge\u00b7lin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aufathmend sich die Seel' erhob.", "tokens": ["Auf\u00b7ath\u00b7mend", "sich", "die", "Seel'", "er\u00b7hob", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo Menschen mich und Welt bedrohten,", "tokens": ["Wo", "Men\u00b7schen", "mich", "und", "Welt", "be\u00b7droh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wu\u00dft' in unverr\u00fccktem Gang", "tokens": ["Ich", "wu\u00dft'", "in", "un\u00