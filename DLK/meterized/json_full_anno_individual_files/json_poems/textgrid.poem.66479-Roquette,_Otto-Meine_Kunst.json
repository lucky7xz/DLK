etra"}, "line.7": {"text": "So durft' ich dich als H\u00f6chstes fassen,", "tokens": ["So", "durft'", "ich", "dich", "als", "H\u00f6chs\u00b7tes", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als Eigenstes in meiner Brust.", "tokens": ["Als", "Ei\u00b7gens\u00b7tes", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Von einem einz'gen Wunsch umfangen", "tokens": ["Von", "ei\u00b7nem", "einz'\u00b7gen", "Wunsch", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist so mir alles Daseins Werth:", "tokens": ["Ist", "so", "mir", "al\u00b7les", "Da\u00b7seins", "Werth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unverl\u00f6scht f\u00fcr mein Verlangen", "tokens": ["Da\u00df", "un\u00b7ver\u00b7l\u00f6scht", "f\u00fcr", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du leuchtend bleibest meinem Herd!", "tokens": ["Du", "leuch\u00b7tend", "blei\u00b7best", "mei\u00b7nem", "Herd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wie von heil'gem Opferrauche", "tokens": ["Da\u00df", ",", "wie", "von", "heil'\u00b7gem", "Op\u00b7fer\u00b7rau\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verh\u00fcllt, ich noch in deiner Gunst", "tokens": ["Ver\u00b7h\u00fcllt", ",", "ich", "noch", "in", "dei\u00b7ner", "Gunst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den letzten Athemzug verhauche,", "tokens": ["Den", "letz\u00b7ten", "A\u00b7them\u00b7zug", "ver\u00b7hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von dir beseelt, du meine Kunst!", "tokens": ["Von", "dir", "be\u00b7seelt", ",", "du", "mei\u00b7ne", "Kunst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}