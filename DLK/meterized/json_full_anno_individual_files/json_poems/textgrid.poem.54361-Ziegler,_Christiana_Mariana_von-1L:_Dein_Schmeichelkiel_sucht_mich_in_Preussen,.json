{"textgrid.poem.54361": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Schmeichelkiel sucht mich in Preussen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Schmeichelkiel sucht mich in Preussen,", "tokens": ["Dein", "Schmei\u00b7chel\u00b7kiel", "sucht", "mich", "in", "Preus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und mu\u00df ein Dichter heissen,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Dich\u00b7ter", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wundersch\u00f6n klingt dieses nicht;", "tokens": ["Wie", "wun\u00b7der\u00b7sch\u00f6n", "klingt", "die\u00b7ses", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, bin ich gleich kein Hauptpoete,", "tokens": ["Doch", ",", "bin", "ich", "gleich", "kein", "Haupt\u00b7po\u00b7e\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So greif ich doch nach meiner Fl\u00f6te,", "tokens": ["So", "greif", "ich", "doch", "nach", "mei\u00b7ner", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da dieser frohe Tag anbricht.", "tokens": ["Da", "die\u00b7ser", "fro\u00b7he", "Tag", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tag ermuntert meine Sinnen;", "tokens": ["Der", "Tag", "er\u00b7mun\u00b7tert", "mei\u00b7ne", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was k\u00f6nnt ich edlers wohl beginnen,", "tokens": ["Was", "k\u00f6nnt", "ich", "ed\u00b7lers", "wohl", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als dies bey deinem Namensfest?", "tokens": ["Als", "dies", "bey", "dei\u00b7nem", "Na\u00b7mens\u00b7fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch liebste Musen! helft mich retten,", "tokens": ["Doch", "liebs\u00b7te", "Mu\u00b7sen", "!", "helft", "mich", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hand, Geist und Einfall liegt an Ketten,", "tokens": ["Hand", ",", "Geist", "und", "Ein\u00b7fall", "liegt", "an", "Ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da mich mein Pegasus verl\u00e4\u00dft.", "tokens": ["Da", "mich", "mein", "Pe\u00b7ga\u00b7sus", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald sangst du aus recht hohem Thone,", "tokens": ["Bald", "sangst", "du", "aus", "recht", "ho\u00b7hem", "Tho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00e4mpftest um die Dichterkrone,", "tokens": ["Und", "k\u00e4mpf\u00b7test", "um", "die", "Dich\u00b7ter\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Phoebus dir l\u00e4ngst zugedacht:", "tokens": ["Die", "Phoe\u00b7bus", "dir", "l\u00e4ngst", "zu\u00b7ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald war dein kauderwelsch Gedichte", "tokens": ["Bald", "war", "dein", "kau\u00b7der\u00b7welsch", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz voller sch\u00f6nen Mordgeschichte:", "tokens": ["Ganz", "vol\u00b7ler", "sch\u00f6\u00b7nen", "Mord\u00b7ge\u00b7schich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das hat den Klepper scheu gemacht.", "tokens": ["Das", "hat", "den", "Klep\u00b7per", "scheu", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch alles dies soll mich nicht schrecken,", "tokens": ["Doch", "al\u00b7les", "dies", "soll", "mich", "nicht", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich in ein lustig Kleid zu stecken,", "tokens": ["Mich", "in", "ein", "lus\u00b7tig", "Kleid", "zu", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu singen, wie dort Canitz sang.", "tokens": ["Zu", "sin\u00b7gen", ",", "wie", "dort", "Ca\u00b7nitz", "sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sch\u00f6ne Fest will solches haben,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Fest", "will", "sol\u00b7ches", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum la\u00df ich meine Feder traben,", "tokens": ["Drum", "la\u00df", "ich", "mei\u00b7ne", "Fe\u00b7der", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sonder allen Putz und Zwang.", "tokens": ["Doch", "son\u00b7der", "al\u00b7len", "Putz", "und", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was w\u00fcnsch ich denn, Herr Namenstr\u00e4ger", "tokens": ["Was", "w\u00fcnsch", "ich", "denn", ",", "Herr", "Na\u00b7mens\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, als der Musen besten Pfleger,", "tokens": ["Dir", ",", "als", "der", "Mu\u00b7sen", "bes\u00b7ten", "Pfle\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das dir recht sanft und kirre thut?", "tokens": ["Das", "dir", "recht", "sanft", "und", "kir\u00b7re", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft Schwestern, mir daf\u00fcr nicht grauen,", "tokens": ["La\u00dft", "Schwes\u00b7tern", ",", "mir", "da\u00b7f\u00fcr", "nicht", "grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "PPER", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und lang an meinen N\u00e4geln kauen,", "tokens": ["Und", "lang", "an", "mei\u00b7nen", "N\u00e4\u00b7geln", "kau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seyd mir nur dies mal hold und gut.", "tokens": ["Seyd", "mir", "nur", "dies", "mal", "hold", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "PDS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr seyd es auch, wie ich schon merke,", "tokens": ["Ihr", "seyd", "es", "auch", ",", "wie", "ich", "schon", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Drum schreit ich auch nunmehr zum Werke,", "tokens": ["Drum", "schreit", "ich", "auch", "nun\u00b7mehr", "zum", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und fange mit den W\u00fcnschen an;", "tokens": ["Und", "fan\u00b7ge", "mit", "den", "W\u00fcn\u00b7schen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nymphen, so das Rund der Erden,", "tokens": ["Die", "Nym\u00b7phen", ",", "so", "das", "Rund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schon hegt, und noch gebohren werden,", "tokens": ["Schon", "hegt", ",", "und", "noch", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die bleiben dir stets zugethan.", "tokens": ["Die", "blei\u00b7ben", "dir", "stets", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es schmecke dir der Saft der Reben", "tokens": ["Es", "schme\u00b7cke", "dir", "der", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch funfzig Jahr in deinem Leben,", "tokens": ["Noch", "funf\u00b7zig", "Jahr", "in", "dei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So trinkst du lange, werther Freund.", "tokens": ["So", "trinkst", "du", "lan\u00b7ge", ",", "wert\u00b7her", "Freund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Parcen Faden halte feste,", "tokens": ["Der", "Par\u00b7cen", "Fa\u00b7den", "hal\u00b7te", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Damit der Tod vor deinem Neste", "tokens": ["Da\u00b7mit", "der", "Tod", "vor", "dei\u00b7nem", "Nes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In vieler Zeit noch nicht erscheint.", "tokens": ["In", "vie\u00b7ler", "Zeit", "noch", "nicht", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nimm hier mit meinem Wunsch vor Willen,", "tokens": ["Nimm", "hier", "mit", "mei\u00b7nem", "Wunsch", "vor", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gl\u00fcck und Vorsicht wird erf\u00fcllen,", "tokens": ["Den", "Gl\u00fcck", "und", "Vor\u00b7sicht", "wird", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie m\u00fcssen wohl, du bist es werth.", "tokens": ["Sie", "m\u00fcs\u00b7sen", "wohl", ",", "du", "bist", "es", "werth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich weis dir weiter nichts zu schenken,", "tokens": ["Ich", "weis", "dir", "wei\u00b7ter", "nichts", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als mein best\u00e4ndig Angedenken,", "tokens": ["Als", "mein", "be\u00b7st\u00e4n\u00b7dig", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schelm, der mehr von mir begehrt.", "tokens": ["Ein", "Schelm", ",", "der", "mehr", "von", "mir", "be\u00b7gehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dein Schmeichelkiel sucht mich in Preussen,", "tokens": ["Dein", "Schmei\u00b7chel\u00b7kiel", "sucht", "mich", "in", "Preus\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und mu\u00df ein Dichter heissen,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Dich\u00b7ter", "heis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wundersch\u00f6n klingt dieses nicht;", "tokens": ["Wie", "wun\u00b7der\u00b7sch\u00f6n", "klingt", "die\u00b7ses", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, bin ich gleich kein Hauptpoete,", "tokens": ["Doch", ",", "bin", "ich", "gleich", "kein", "Haupt\u00b7po\u00b7e\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So greif ich doch nach meiner Fl\u00f6te,", "tokens": ["So", "greif", "ich", "doch", "nach", "mei\u00b7ner", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da dieser frohe Tag anbricht.", "tokens": ["Da", "die\u00b7ser", "fro\u00b7he", "Tag", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der Tag ermuntert meine Sinnen;", "tokens": ["Der", "Tag", "er\u00b7mun\u00b7tert", "mei\u00b7ne", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was k\u00f6nnt ich edlers wohl beginnen,", "tokens": ["Was", "k\u00f6nnt", "ich", "ed\u00b7lers", "wohl", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als dies bey deinem Namensfest?", "tokens": ["Als", "dies", "bey", "dei\u00b7nem", "Na\u00b7mens\u00b7fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch liebste Musen! helft mich retten,", "tokens": ["Doch", "liebs\u00b7te", "Mu\u00b7sen", "!", "helft", "mich", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hand, Geist und Einfall liegt an Ketten,", "tokens": ["Hand", ",", "Geist", "und", "Ein\u00b7fall", "liegt", "an", "Ket\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da mich mein Pegasus verl\u00e4\u00dft.", "tokens": ["Da", "mich", "mein", "Pe\u00b7ga\u00b7sus", "ver\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Bald sangst du aus recht hohem Thone,", "tokens": ["Bald", "sangst", "du", "aus", "recht", "ho\u00b7hem", "Tho\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00e4mpftest um die Dichterkrone,", "tokens": ["Und", "k\u00e4mpf\u00b7test", "um", "die", "Dich\u00b7ter\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Phoebus dir l\u00e4ngst zugedacht:", "tokens": ["Die", "Phoe\u00b7bus", "dir", "l\u00e4ngst", "zu\u00b7ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald war dein kauderwelsch Gedichte", "tokens": ["Bald", "war", "dein", "kau\u00b7der\u00b7welsch", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ganz voller sch\u