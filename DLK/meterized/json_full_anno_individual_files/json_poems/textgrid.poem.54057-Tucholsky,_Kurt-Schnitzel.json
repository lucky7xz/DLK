{"textgrid.poem.54057": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnitzel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt Schriftsteller, die rasen sehr exakt. Sie dichten aus dem Reinen ins Unreine.", "tokens": ["Es", "gibt", "Schrift\u00b7stel\u00b7ler", ",", "die", "ra\u00b7sen", "sehr", "ex\u00b7akt", ".", "Sie", "dich\u00b7ten", "aus", "dem", "Rei\u00b7nen", "ins", "Un\u00b7rei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "$.", "PPER", "ADJA", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-+-+-+---+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Manche Zeitschriften halten sich nur durch die Freiabonnenten.", "tokens": ["Man\u00b7che", "Zeit\u00b7schrif\u00b7ten", "hal\u00b7ten", "sich", "nur", "durch", "die", "Frei\u00b7a\u00b7bon\u00b7nen\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-++-+-+-+-+-+--", "measure": "unknown.measure.octa.plus"}}, "stanza.2": {"line.1": {"text": "(aus den Spr\u00fcchen des Pfarrers Otto): \u00bbDie Frauen sind die Holzwolle in der Glaskiste des Lebens.\u00ab", "tokens": ["(", "aus", "den", "Spr\u00fc\u00b7chen", "des", "Pfar\u00b7rers", "Ot\u00b7to", ")", ":", "\u00bb", "Die", "Frau\u00b7en", "sind", "die", "Holz\u00b7wol\u00b7le", "in", "der", "Glas\u00b7kis\u00b7te", "des", "Le\u00b7bens", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word",