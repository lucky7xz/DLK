{"textgrid.poem.65450": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "23. Der April", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der spr\u00f6den Rosenknospe gleich erscheinst du mir, April,", "tokens": ["Der", "spr\u00f6\u00b7den", "Ro\u00b7sen\u00b7knos\u00b7pe", "gleich", "er\u00b7scheinst", "du", "mir", ",", "Ap\u00b7ril", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die, wie auch Luft und Licht sie kost, sich noch nicht \u00f6ffnen will.", "tokens": ["Die", ",", "wie", "auch", "Luft", "und", "Licht", "sie", "kost", ",", "sich", "noch", "nicht", "\u00f6ff\u00b7nen", "will", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADV", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Doch aus den rothen Ritzen schielt sie dann und wann hervor,", "tokens": ["Doch", "aus", "den", "ro\u00b7then", "Rit\u00b7zen", "schielt", "sie", "dann", "und", "wann", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "KON", "PWAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und zittert sch\u00fcchtern gleich zur\u00fcck, wenn's tr\u00fcb' ist vor dem Thor.", "tokens": ["Und", "zit\u00b7tert", "sch\u00fcch\u00b7tern", "gleich", "zu\u00b7r\u0