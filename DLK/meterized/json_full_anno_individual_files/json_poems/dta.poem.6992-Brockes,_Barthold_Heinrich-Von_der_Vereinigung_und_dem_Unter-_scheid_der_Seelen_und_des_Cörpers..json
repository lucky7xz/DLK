{"dta.poem.6992": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' er vermocht, die Regeln zu erdencken,", "tokens": ["H\u00e4tt'", "er", "ver\u00b7mocht", ",", "die", "Re\u00b7geln", "zu", "er\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wodurch er f\u00e4hig ist, sich selbst zur\u00fcck zu treiben,", "tokens": ["Wo\u00b7durch", "er", "f\u00e4\u00b7hig", "ist", ",", "sich", "selbst", "zu\u00b7r\u00fcck", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VAFIN", "$,", "PRF", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich vom Pfad der Wollust abzulencken?", "tokens": ["Und", "sich", "vom", "Pfad", "der", "Wol\u00b7lust", "ab\u00b7zu\u00b7len\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4tt\u2019 er vermocht ihm Regeln vorzuschreiben,", "tokens": ["H\u00e4tt'", "er", "ver\u00b7mocht", "ihm", "Re\u00b7geln", "vor\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf welche Weis\u2019 es anzufangen,", "tokens": ["Auf", "wel\u00b7che", "Weis'", "es", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So wolauf lange Zeit gesund zu bleiben,", "tokens": ["So", "wo\u00b7lauf", "lan\u00b7ge", "Zeit", "ge\u00b7sund", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als wenn Gesundheit fort, sie wieder zu erlangen?", "tokens": ["Als", "wenn", "Ge\u00b7sund\u00b7heit", "fort", ",", "sie", "wie\u00b7der", "zu", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist es der Trieb, der, wenn die Hefftigkeiten", "tokens": ["Ist", "es", "der", "Trieb", ",", "der", ",", "wenn", "die", "Heff\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Des Durstes in der Hitz\u2019 uns plaget,", "tokens": ["Des", "Durs\u00b7tes", "in", "der", "Hitz'", "uns", "pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Uns kaltes Wasser untersaget,", "tokens": ["Uns", "kal\u00b7tes", "Was\u00b7ser", "un\u00b7ter\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und einen starcken Trunck verbeut?", "tokens": ["Und", "ei\u00b7nen", "star\u00b7cken", "Trunck", "ver\u00b7beut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch welch ein kalt und gar zu j\u00e4hes Trincken,", "tokens": ["Durch", "welch", "ein", "kalt", "und", "gar", "zu", "j\u00e4\u00b7hes", "Trin\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "ADJD", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein Krancker in der Lust wird in die Grube sincken?", "tokens": ["Ein", "Kran\u00b7cker", "in", "der", "Lust", "wird", "in", "die", "Gru\u00b7be", "sin\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}