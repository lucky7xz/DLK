{"dta.poem.10204": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Jagdst\u00fcck. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin die Zeit, als meine Brust umbrandet", "tokens": ["Wo\u00b7hin", "die", "Zeit", ",", "als", "mei\u00b7ne", "Brust", "um\u00b7bran\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Wettern und von schweren Schicksalschl\u00e4gen.", "tokens": ["Von", "Wet\u00b7tern", "und", "von", "schwe\u00b7ren", "Schick\u00b7sal\u00b7schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Sicherhafen bin ich l\u00e4ngst gelandet,", "tokens": ["Im", "Si\u00b7cher\u00b7ha\u00b7fen", "bin", "ich", "l\u00e4ngst", "ge\u00b7lan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wandle stumpf in ausgetretnen Wegen.", "tokens": ["Und", "wand\u00b7le", "stumpf", "in", "aus\u00b7ge\u00b7tret\u00b7nen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fast w\u00e4r\u2019s mein Wunsch, da\u00df ich im Sturm gestrandet,", "tokens": ["Fast", "w\u00e4r's", "mein", "Wunsch", ",", "da\u00df", "ich", "im", "Sturm", "ge\u00b7stran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Ufernichterreichender, erlegen.", "tokens": ["Ein", "Uf\u00b7er\u00b7nich\u00b7ter\u00b7rei\u00b7chen\u00b7der", ",", "er\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Als da\u00df ich hier, verrostet und versandet,", "tokens": ["Als", "da\u00df", "ich", "hier", ",", "ver\u00b7ros\u00b7tet", "und", "ver\u00b7san\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein altes Wrack, um das die Winde fegen.", "tokens": ["Ein", "al\u00b7tes", "Wrack", ",", "um", "das", "die", "Win\u00b7de", "fe\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}