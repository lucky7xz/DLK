{"textgrid.poem.37046": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der May", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der junge May erscheint, und streuet Gold,", "tokens": ["Der", "jun\u00b7ge", "May", "er\u00b7scheint", ",", "und", "streu\u00b7et", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Azur in die L\u00fcfte,", "tokens": ["Und", "A\u00b7zur", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Thal, bes\u00e4t mit Fr\u00fchlingsblumen, zollt", "tokens": ["Das", "Thal", ",", "be\u00b7s\u00e4t", "mit", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", ",", "zollt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Zephyrn wieder D\u00fcfte.", "tokens": ["Den", "Ze\u00b7phyrn", "wie\u00b7der", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun schlinget sich der Bach, vom kalten Band", "tokens": ["Nun", "schlin\u00b7get", "sich", "der", "Bach", ",", "vom", "kal\u00b7ten", "Band"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Eises lo\u00dfgebunden,", "tokens": ["Des", "Ei\u00b7ses", "lo\u00df\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Flur hinab, den sammetweichen Rand", "tokens": ["Die", "Flur", "hin\u00b7ab", ",", "den", "sam\u00b7met\u00b7wei\u00b7chen", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Kr\u00e4nzen rund umwunden.", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "rund", "um\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In jedem Wellchen schwimmt Aurorens Bild,", "tokens": ["In", "je\u00b7dem", "Well\u00b7chen", "schwimmt", "Au\u00b7ro\u00b7rens", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie den Tag erwecket,", "tokens": ["Wenn", "sie", "den", "Tag", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den ganzen Ost in ihren Purpur h\u00fcllt,", "tokens": ["Den", "gan\u00b7zen", "Ost", "in", "ih\u00b7ren", "Pur\u00b7pur", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Berg mit Gold bedecket.", "tokens": ["Den", "Berg", "mit", "Gold", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun sinket D\u00e4mmerung und gr\u00fcne Nacht", "tokens": ["Nun", "sin\u00b7ket", "D\u00e4m\u00b7me\u00b7rung", "und", "gr\u00fc\u00b7ne", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von jedem Wipfel nieder,", "tokens": ["Von", "je\u00b7dem", "Wip\u00b7fel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wirbeln, wenn der Abendstern erwacht,", "tokens": ["Nun", "wir\u00b7beln", ",", "wenn", "der", "A\u00b7bends\u00b7tern", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Nachtigallen Lieder.", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun h\u00fcpft die Ruh, dort, wo das Quellchen schw\u00e4tzt,", "tokens": ["Nun", "h\u00fcpft", "die", "Ruh", ",", "dort", ",", "wo", "das", "Quell\u00b7chen", "schw\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im aetherblauen Kleide,", "tokens": ["Im", "a\u00b7e\u00b7ther\u00b7blau\u00b7en", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit ihrer Schwester, die der Erdball sch\u00e4tzt,", "tokens": ["Mit", "ih\u00b7rer", "Schwes\u00b7ter", ",", "die", "der", "Erd\u00b7ball", "sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Arme, mit der Freude.", "tokens": ["Am", "Ar\u00b7me", ",", "mit", "der", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie fliehn die Stadt, den goldenen Pallast,", "tokens": ["Sie", "fliehn", "die", "Stadt", ",", "den", "gol\u00b7de\u00b7nen", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und seine Marmors\u00e4ale", "tokens": ["Und", "sei\u00b7ne", "Mar\u00b7mor\u00b7s\u00e4\u00b7a\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Tafeln, die der weise Comus ha\u00dft,", "tokens": ["Die", "Ta\u00b7feln", ",", "die", "der", "wei\u00b7se", "Co\u00b7mus", "ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00e4umenden Poc\u00e4le.", "tokens": ["Die", "sch\u00e4u\u00b7men\u00b7den", "Po\u00b7c\u00e4\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie tanzen durch die Bl\u00fcmchen ihren Reihn,", "tokens": ["Sie", "tan\u00b7zen", "durch", "die", "Bl\u00fcm\u00b7chen", "ih\u00b7ren", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Westen sanft gek\u00fchlet,", "tokens": ["Von", "Wes\u00b7ten", "sanft", "ge\u00b7k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und um den Sch\u00e4fer, der im Buchenhayn", "tokens": ["Und", "um", "den", "Sch\u00e4\u00b7fer", ",", "der", "im", "Bu\u00b7chen\u00b7hayn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf seiner Fl\u00f6te spielet.", "tokens": ["Auf", "sei\u00b7ner", "Fl\u00f6\u00b7te", "spie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O dreymahl gl\u00fccklich, wer an ihren Arm", "tokens": ["O", "drey\u00b7mahl", "gl\u00fcck\u00b7lich", ",", "wer", "an", "ih\u00b7ren", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "$,", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschlungen, durch die Fl\u00e4chen,", "tokens": ["Ge\u00b7schlun\u00b7gen", ",", "durch", "die", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll Heerden, irrt, in Th\u00e4lern, die kein Harm", "tokens": ["Voll", "Heer\u00b7den", ",", "irrt", ",", "in", "Th\u00e4\u00b7lern", ",", "die", "kein", "Harm"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN", "$,", "APPR", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beschleichet, an den B\u00e4chen.", "tokens": ["Be\u00b7schlei\u00b7chet", ",", "an", "den", "B\u00e4\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sein Geist ist ruhig, wie der Sommersee,", "tokens": ["Sein", "Geist", "ist", "ru\u00b7hig", ",", "wie", "der", "Som\u00b7mer\u00b7see", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um den ein W\u00e4ldchen nicket,", "tokens": ["Um", "den", "ein", "W\u00e4ld\u00b7chen", "ni\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Luna von des Himmels blauer H\u00f6h", "tokens": ["Wenn", "Lu\u00b7na", "von", "des", "Him\u00b7mels", "blau\u00b7er", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf ihn herunterblicket.", "tokens": ["Auf", "ihn", "her\u00b7un\u00b7ter\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der junge May erscheint, und streuet Gold,", "tokens": ["Der", "jun\u00b7ge", "May", "er\u00b7scheint", ",", "und", "streu\u00b7et", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Azur in die L\u00fcfte,", "tokens": ["Und", "A\u00b7zur", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Thal, bes\u00e4t mit Fr\u00fchlingsblumen, zollt", "tokens": ["Das", "Thal", ",", "be\u00b7s\u00e4t", "mit", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", ",", "zollt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Zephyrn wieder D\u00fcfte.", "tokens": ["Den", "Ze\u00b7phyrn", "wie\u00b7der", "D\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun schlinget sich der Bach, vom kalten Band", "tokens": ["Nun", "schlin\u00b7get", "sich", "der", "Bach", ",", "vom", "kal\u00b7ten", "Band"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Eises lo\u00dfgebunden,", "tokens": ["Des", "Ei\u00b