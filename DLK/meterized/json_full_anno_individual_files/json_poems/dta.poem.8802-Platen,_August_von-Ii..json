{"dta.poem.8802": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Von Liebesgefahr bedroht zu seyn,", "tokens": ["Von", "Lie\u00b7bes\u00b7ge\u00b7fahr", "be\u00b7droht", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf sch\u00e4umendem Meer des Gl\u00fccks best\u00fcrmt", "tokens": ["Auf", "sch\u00e4u\u00b7men\u00b7dem", "Meer", "des", "Gl\u00fccks", "be\u00b7st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein schaukelgewohntes Boot zu seyn!", "tokens": ["Ein", "schau\u00b7kel\u00b7ge\u00b7wohn\u00b7tes", "Boot", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "O scheue dich nicht, da\u00df nicht du bist,", "tokens": ["O", "scheu\u00b7e", "dich", "nicht", ",", "da\u00df", "nicht", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PTKNEG", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Was unser Prophet gebot zu seyn!", "tokens": ["Was", "un\u00b7ser", "Pro\u00b7phet", "ge\u00b7bot", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie sch\u00f6n, in der Wage Mustafa's", "tokens": ["Wie", "sch\u00f6n", ",", "in", "der", "Wa\u00b7ge", "Mu\u00b7sta\u00b7fa's"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "APPR", "ART", "NN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wenn auch nur ein leichtes Loth zu seyn!", "tokens": ["Wenn", "auch", "nur", "ein", "leich\u00b7tes", "Loth", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Schattirungen liebt die Tulpe zwar,", "tokens": ["Schat\u00b7ti\u00b7run\u00b7gen", "liebt", "die", "Tul\u00b7pe", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Doch freut sich die Rose, roth zu seyn:", "tokens": ["Doch", "freut", "sich", "die", "Ro\u00b7se", ",", "roth", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wer sehnte sich nicht, um stets zu bl\u00fchn", "tokens": ["Wer", "sehn\u00b7te", "sich", "nicht", ",", "um", "stets", "zu", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "PTKNEG", "$,", "KOUI", "ADV", "PTKZU", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Im Liede, wie ", "tokens": ["Im", "Lie\u00b7de", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}}}}}