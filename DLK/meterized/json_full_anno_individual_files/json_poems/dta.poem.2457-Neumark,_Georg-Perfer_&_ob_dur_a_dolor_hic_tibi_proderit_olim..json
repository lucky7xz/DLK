{"dta.poem.2457": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Perfer & ob dur a dolor hic tibi proderit olim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Dann dann steh als ein Leu/ la\u00df dann ein Hertze blikken/", "tokens": ["Dann", "dann", "steh", "als", "ein", "Leu", "/", "la\u00df", "dann", "ein", "Hert\u00b7ze", "blik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KOKOM", "ART", "NN", "$(", "VVIMP", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wann dein Verh\u00e4ngn\u00fcs tobt/ es wettre wie es", "tokens": ["Wann", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fcs", "tobt", "/", "es", "wett\u00b7re", "wie", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "PPER", "VVFIN", "KOKOM", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wil/", "tokens": ["wil", "/"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "So halte mit Gedult in solchem Sturme still/", "tokens": ["So", "hal\u00b7te", "mit", "Ge\u00b7dult", "in", "sol\u00b7chem", "Stur\u00b7me", "still", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es wird sich dermaleins zu deinem Besten schikken.", "tokens": ["Es", "wird", "sich", "der\u00b7ma\u00b7leins", "zu", "dei\u00b7nem", "Bes\u00b7ten", "schik\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}