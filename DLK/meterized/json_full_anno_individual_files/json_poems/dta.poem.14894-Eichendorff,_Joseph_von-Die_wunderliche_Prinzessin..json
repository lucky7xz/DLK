{"dta.poem.14894": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderliche Prinzessin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch die alten Helden alle,", "tokens": ["Doch", "die", "al\u00b7ten", "Hel\u00b7den", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drau\u00dfen vor der Burg gelagert,", "tokens": ["Drau\u00b7\u00dfen", "vor", "der", "Burg", "ge\u00b7la\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen dort im Morgenglanze,", "tokens": ["Sa\u00b7\u00dfen", "dort", "im", "Mor\u00b7gen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die das sch\u00f6ne Kind bewachten.", "tokens": ["Die", "das", "sch\u00f6\u00b7ne", "Kind", "be\u00b7wach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An das Thor die Freier kamen", "tokens": ["An", "das", "Thor", "die", "Frei\u00b7er", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun gesprengt, geh\u00fcpft, gelaufen,", "tokens": ["Nun", "ge\u00b7sprengt", ",", "ge\u00b7h\u00fcpft", ",", "ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ritter, J\u00e4ger, Provenzalen,", "tokens": ["Rit\u00b7ter", ",", "J\u00e4\u00b7ger", ",", "Pro\u00b7ven\u00b7za\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bunte, helle, lichte Haufen.", "tokens": ["Bun\u00b7te", ",", "hel\u00b7le", ",", "lich\u00b7te", "Hau\u00b7fen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und vor allen junge Recken", "tokens": ["Und", "vor", "al\u00b7len", "jun\u00b7ge", "Re\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Stolzen Blicks den Berg berannten,", "tokens": ["Stol\u00b7zen", "Blicks", "den", "Berg", "be\u00b7rann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die die alten Helden weckten,", "tokens": ["Die", "die", "al\u00b7ten", "Hel\u00b7den", "weck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sie vertraulich Br\u00fcder nannten.", "tokens": ["Sie", "ver\u00b7trau\u00b7lich", "Br\u00fc\u00b7der", "nann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch wie diese uralt blicken,", "tokens": ["Doch", "wie", "die\u00b7se", "ur\u00b7alt", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PDAT", "NN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.14": {"text": "An die Eisenbrust geschlossen,", "tokens": ["An", "die", "Ei\u00b7sen\u00b7brust", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Br\u00fcderlich die Jungen dr\u00fccken,", "tokens": ["Br\u00fc\u00b7der\u00b7lich", "die", "Jun\u00b7gen", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Fallen die erdr\u00fcckt zu Boden.", "tokens": ["Fal\u00b7len", "die", "er\u00b7dr\u00fcckt", "zu", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Andre lagern sich zum Alten,", "tokens": ["And\u00b7re", "la\u00b7gern", "sich", "zum", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Graust ihn'n gleich bei seinen Mienen,", "tokens": ["Graust", "ihn'n", "gleich", "bei", "sei\u00b7nen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Ordnen sein verworr'nes Walten,", "tokens": ["Ord\u00b7nen", "sein", "ver\u00b7worr'\u00b7nes", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df es jedem wohlgefiele;", "tokens": ["Da\u00df", "es", "je\u00b7dem", "wohl\u00b7ge\u00b7fie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Doch sie f\u00fchlen schauernd balde,", "tokens": ["Doch", "sie", "f\u00fch\u00b7len", "schau\u00b7ernd", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}