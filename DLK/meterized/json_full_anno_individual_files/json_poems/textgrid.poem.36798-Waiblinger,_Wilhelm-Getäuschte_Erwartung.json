{"textgrid.poem.36798": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Get\u00e4uschte Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles dacht' ich mir sch\u00f6ner, eh' ich's mit Augen gesehen,", "tokens": ["Al\u00b7les", "dacht'", "ich", "mir", "sch\u00f6\u00b7ner", ",", "eh'", "ich's", "mit", "Au\u00b7gen", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADJD", "$,", "KOUS", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und erstaunte, wie klein alles in Wirklichkeit ist.", "tokens": ["Und", "er\u00b7staun\u00b7te", ",", "wie", "klein", "al\u00b7les", "in", "Wirk\u00b7lich\u00b7keit", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "APPR", "NN", "VAFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wie hat nur mich St. Peter get\u00e4uscht, nach der Reisebeschreibung", "tokens": ["Wie", "hat", "nur", "mich", "St.", "Pe\u00b7ter", "ge\u00b7t\u00e4uscht", ",", "nach", "der", "Rei\u00b7se\u00b7be\u00b7schrei\u00b7bung"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PPER", "NE", "NE", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "Sollt' er noch einmal so hoch, einmal so pr\u00e4chtig noch sein.", "tokens": ["Sollt'", "er", "noch", "ein\u00b7mal", "so", "hoch", ",", "ein\u00b7mal", "so", "pr\u00e4ch\u00b7tig", "noch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "ADV", "VAINF", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Reden sie vom Capitol, ich erwartet' es hoch in den L\u00fcften,", "tokens": ["Re\u00b7den", "sie", "vom", "Ca\u00b7pi\u00b7tol", ",", "ich", "er\u00b7war\u00b7tet'", "es", "hoch", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--++-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Und noch einmal so sch\u00f6n dacht' ich's Museum mir selbst;", "tokens": ["Und", "noch", "ein\u00b7mal", "so", "sch\u00f6n", "dacht'", "ich's", "Mu\u00b7se\u00b7um", "mir", "selbst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "PIS", "NN", "PPER", "ADV", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und der tarpejische Fels! Doch wenigstens auch wie der Montblanc", "tokens": ["Und", "der", "tar\u00b7pe\u00b7ji\u00b7sche", "Fels", "!", "Doch", "we\u00b7nigs\u00b7tens", "auch", "wie", "der", "Mont\u00b7blanc"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "KON", "ADV", "ADV", "KOKOM", "ART", "NE"], "meter": "+-+-+-+-+-+-+--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.8": {"text": "Glaubt' ich ihn hoch, und er ist doch wie ein H\u00fcgelchen nur.", "tokens": ["Glaubt'", "ich", "ihn", "hoch", ",", "und", "er", "ist", "doch", "wie", "ein", "H\u00fc\u00b7gel\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "KON", "PPER", "VAFIN", "ADV", "KOKOM", "ART", "NN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Auch das Colosseum, ich dacht' es noch einmal so furchtbar,", "tokens": ["Auch", "das", "Co\u00b7los\u00b7se\u00b7um", ",", "ich", "dacht'", "es", "noch", "ein\u00b7mal", "so", "furcht\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "--+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Britten k\u00e4men wohl hier nicht ohne Extrapost durch.", "tokens": ["Brit\u00b7ten", "k\u00e4\u00b7men", "wohl", "hier", "nicht", "oh\u00b7ne", "Ext\u00b7ra\u00b7post", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PTKNEG", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+---", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Wie ist der Corso so eng! Vierhundert Kirchen und dennoch", "tokens": ["Wie", "ist", "der", "Cor\u00b7so", "so", "eng", "!", "Vier\u00b7hun\u00b7dert", "Kir\u00b7chen", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$.", "CARD", "NN", "KON", "ADV"], "meter": "-+-+--+++-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Fast kein Th\u00fcrmchen, und welch Fl\u00fc\u00dfchen der Tiber nur ist!", "tokens": ["Fast", "kein", "Th\u00fcrm\u00b7chen", ",", "und", "welch", "Fl\u00fc\u00df\u00b7chen", "der", "Ti\u00b7ber", "nur", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KON", "PWAT", "NN", "ART", "NE", "ADV", "VAFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Raffaels Stanzen, da hofft' ich doch auch hellschimmernde Farben,", "tokens": ["Raf\u00b7faels", "Stan\u00b7zen", ",", "da", "hofft'", "ich", "doch", "auch", "hell\u00b7schim\u00b7mern\u00b7de", "Far\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Aber welch h\u00e4\u00dflicher Wust, schmutziges Alter und Staub!", "tokens": ["A\u00b7ber", "welch", "h\u00e4\u00df\u00b7li\u00b7cher", "Wust", ",", "schmut\u00b7zi\u00b7ges", "Al\u00b7ter", "und", "Staub", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Dann das j\u00fcngste Gericht ist ein Fleischmarkt, und die Sistina", "tokens": ["Dann", "das", "j\u00fcngs\u00b7te", "Ge\u00b7richt", "ist", "ein", "Fleischmarkt", ",", "und", "die", "Sis\u00b7ti\u00b7na"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,", "KON", "ART", "NE"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "H\u00e4tt' ich mir hundertmal sch\u00f6ner und gr\u00f6\u00dfer gedacht.", "tokens": ["H\u00e4tt'", "ich", "mir", "hun\u00b7dert\u00b7mal", "sch\u00f6\u00b7ner", "und", "gr\u00f6\u00b7\u00dfer", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.17": {"text": "Selbst die Weiber gefallen mir nicht und all' das Gerede,", "tokens": ["Selbst", "die", "Wei\u00b7ber", "ge\u00b7fal\u00b7len", "mir", "nicht", "und", "all'", "das", "Ge\u00b7re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "PPER", "PTKNEG", "KON", "PIS", "ART", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Falsch ist's, ich hab' sie mir traun hundertmal sch\u00f6ner gedacht.", "tokens": ["Falsch", "ist's", ",", "ich", "hab'", "sie", "mir", "traun", "hun\u00b7dert\u00b7mal", "sch\u00f6\u00b7ner", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "PPER", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Auch was sie fabeln zu Hause von itali\u00e4nischem Himmel,", "tokens": ["Auch", "was", "sie", "fa\u00b7beln", "zu", "Hau\u00b7se", "von", "i\u00b7ta\u00b7li\u00b7\u00e4\u00b7ni\u00b7schem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Nein! Ich habe davon nicht auch ein Bi\u00dfchen gemerkt.", "tokens": ["Nein", "!", "Ich", "ha\u00b7be", "da\u00b7von", "nicht", "auch", "ein", "Bi\u00df\u00b7chen", "ge\u00b7merkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "PAV", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Uebrigens kann ich zu Haus mich r\u00fchmen: ich hab' es gesehen,", "tokens": ["Ueb\u00b7ri\u00b7gens", "kann", "ich", "zu", "Haus", "mich", "r\u00fch\u00b7men", ":", "ich", "hab'", "es", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Und nat\u00fcrlich, dann ist's \u2013 sch\u00f6ner noch als ich's gedacht.", "tokens": ["Und", "na\u00b7t\u00fcr\u00b7lich", ",", "dann", "ist's", "\u2013", "sch\u00f6\u00b7ner", "noch", "als", "ich's", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "$(", "ADJD", "ADV", "KOUS", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Alles dacht' ich mir sch\u00f6ner, eh' ich's mit Augen gesehen,", "tokens": ["Al\u00b7les", "dacht'", "ich", "mir", "sch\u00f6\u00b7ner", ",", "eh'", "ich's", "mit", "Au\u00b7gen", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "ADJD", "$,", "KOUS", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und erstaunte, wie klein alles in Wirklichkeit ist.", "tokens": ["Und", "er\u00b7staun\u00b7te", ",", "wie", "klein", "al\u00b7les", "in", "Wirk\u00b7lich\u00b7keit", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "PIS", "APPR", "NN", "VAFIN", "$."], "meter": "--+