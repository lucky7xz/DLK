{"textgrid.poem.47034": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wonneschau, Lustanblick, Augenweide!", "tokens": ["O", "Won\u00b7ne\u00b7schau", ",", "Lus\u00b7tan\u00b7blick", ",", "Au\u00b7gen\u00b7wei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So hab' ich sie, die sch\u00f6nste, denn gesehen", "tokens": ["So", "hab'", "ich", "sie", ",", "die", "sch\u00f6ns\u00b7te", ",", "denn", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ART", "ADJA", "$,", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor meinen Blicken so versch\u00f6nert stehen,", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", "so", "ver\u00b7sch\u00f6\u00b7nert", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie's nur die Sch\u00f6nheit werden kann vom Kleide.", "tokens": ["Wie's", "nur", "die", "Sch\u00f6n\u00b7heit", "wer\u00b7den", "kann", "vom", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VAINF", "VMFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O schmeichelhaftes Kleid! Ich sah die Seide", "tokens": ["O", "schmei\u00b7chel\u00b7haf\u00b7tes", "Kleid", "!", "Ich", "sah", "die", "Sei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihrem Busen mir entgegen wehen,", "tokens": ["Von", "ih\u00b7rem", "Bu\u00b7sen", "mir", "ent\u00b7ge\u00b7gen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sah die Blumen dort nach mir sich drehen,", "tokens": ["Und", "sah", "die", "Blu\u00b7men", "dort", "nach", "mir", "sich", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Seid' und Blumen, meine Gaben beide.", "tokens": ["Die", "Seid'", "und", "Blu\u00b7men", ",", "mei\u00b7ne", "Ga\u00b7ben", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sieht der Fr\u00fchlingstag mit Morgenstrahlen", "tokens": ["So", "sieht", "der", "Fr\u00fch\u00b7lings\u00b7tag", "mit", "Mor\u00b7gen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herab auf der geliebten Erde Glieder,", "tokens": ["Her\u00b7ab", "auf", "der", "ge\u00b7lieb\u00b7ten", "Er\u00b7de", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "m