{"textgrid.poem.33554": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "10. Am Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich im azurnen Schleier,", "tokens": ["Ich", "sah", "dich", "im", "a\u00b7zur\u00b7nen", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "In deinen Rosen, Sommernacht,", "tokens": ["In", "dei\u00b7nen", "Ro\u00b7sen", ",", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hab' gewacht in stiller Feier.", "tokens": ["Und", "hab'", "ge\u00b7wacht", "in", "stil\u00b7ler", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Lichte deiner Sterne w\u00e4hnen", "tokens": ["Im", "Lich\u00b7te", "dei\u00b7ner", "Ster\u00b7ne", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die treuen Blicke wir zu schau'n,", "tokens": ["Die", "treu\u00b7en", "Bli\u00b7cke", "wir", "zu", "schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die uns verstehn und unsre Tr\u00e4nen.", "tokens": ["Die", "uns", "ver\u00b7stehn", "un