{"dta.poem.14180": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das St\u00e4ndchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aber von der stillen Schwelle", "tokens": ["A\u00b7ber", "von", "der", "stil\u00b7len", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trugen sie mein Lieb' zur Ruh \u2014", "tokens": ["Tru\u00b7gen", "sie", "mein", "Lieb'", "zur", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du, fr\u00f6hlicher Geselle,", "tokens": ["Und", "du", ",", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe, sing' nur immer zu!", "tokens": ["Sin\u00b7ge", ",", "sing'", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}