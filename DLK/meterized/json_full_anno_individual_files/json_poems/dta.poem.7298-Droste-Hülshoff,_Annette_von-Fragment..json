{"dta.poem.7298": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Fragment.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Savoyen, Land beschnei'ter H\u00f6h'n,", "tokens": ["Sa\u00b7vo\u00b7yen", ",", "Land", "be\u00b7schnei'\u00b7ter", "H\u00f6h'n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wer hat dein kr\u00e4ftig Bild geseh'n,", "tokens": ["Wer", "hat", "dein", "kr\u00e4f\u00b7tig", "Bild", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer trat in deiner W\u00e4lder Nacht,", "tokens": ["Wer", "trat", "in", "dei\u00b7ner", "W\u00e4l\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sah auf zu deiner Wipfel Pracht,", "tokens": ["Sah", "auf", "zu", "dei\u00b7ner", "Wip\u00b7fel", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer stand an deinem Wasserfall,", "tokens": ["Wer", "stand", "an", "dei\u00b7nem", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer lauschte deiner Str\u00f6me Hall,", "tokens": ["Wer", "lauschte", "dei\u00b7ner", "Str\u00f6\u00b7me", "Hall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und nannte dich nicht sch\u00f6n?", "tokens": ["Und", "nann\u00b7te", "dich", "nicht", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Du Land des Volks, dem Reiche weihen", "tokens": ["Du", "Land", "des", "Volks", ",", "dem", "Rei\u00b7che", "wei\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "ART", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ruhmvoll den Namen des getreuen,", "tokens": ["Ruhm\u00b7voll", "den", "Na\u00b7men", "des", "ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Bist herrlich, wenn der Fr\u00fchlingssturm", "tokens": ["Bist", "herr\u00b7lich", ",", "wenn", "der", "Fr\u00fch\u00b7lings\u00b7sturm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Berggew\u00e4sser sch\u00e4umend f\u00fchrt,", "tokens": ["Die", "Berg\u00b7ge\u00b7w\u00e4s\u00b7ser", "sch\u00e4u\u00b7mend", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und deiner Fichte schlanker Thurm", "tokens": ["Und", "dei\u00b7ner", "Fich\u00b7te", "schlan\u00b7ker", "Thurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sich mit der jungen Nadel ziert;", "tokens": ["Sich", "mit", "der", "jun\u00b7gen", "Na\u00b7del", "ziert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bist reizend, wenn die Sommerglut", "tokens": ["Bist", "rei\u00b7zend", ",", "wenn", "die", "Som\u00b7mer\u00b7glut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Erzittert um den Mandelbaum;", "tokens": ["Er\u00b7zit\u00b7tert", "um", "den", "Man\u00b7del\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Doch in des Herbstes goldner Flut", "tokens": ["Doch", "in", "des", "Herbs\u00b7tes", "gold\u00b7ner", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du ruhst gleich dunkeln Auges Traum.", "tokens": ["Du", "ruhst", "gleich", "dun\u00b7keln", "Au\u00b7ges", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dann treibt der Wind kein rasselnd Laub", "tokens": ["Dann", "treibt", "der", "Wind", "kein", "ras\u00b7selnd", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Durch brauner Haiden Wirbelstaub;", "tokens": ["Durch", "brau\u00b7ner", "Hai\u00b7den", "Wir\u00b7bel\u00b7staub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie halb bezwungne Seufzer wallen,", "tokens": ["Wie", "halb", "be\u00b7zwung\u00b7ne", "Seuf\u00b7zer", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nur leis' die zarten Nadeln fallen,", "tokens": ["Nur", "leis'", "die", "zar\u00b7ten", "Na\u00b7deln", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als wagten sie zu fl\u00fcstern kaum.", "tokens": ["Als", "wag\u00b7ten", "sie", "zu", "fl\u00fcs\u00b7tern", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PTKZU", "VVINF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tag bricht an; noch einsam steht", "tokens": ["Der", "Tag", "bricht", "an", ";", "noch", "ein\u00b7sam", "steht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Sonnenrund am Firmament;", "tokens": ["Das", "Son\u00b7nen\u00b7rund", "am", "Fir\u00b7ma\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Strahl, der auf und nieder streicht,", "tokens": ["Am", "Strahl", ",", "der", "auf", "und", "nie\u00b7der", "streicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemach der Erdbeerbaum entbrennt;", "tokens": ["Ge\u00b7mach", "der", "Erd\u00b7beer\u00b7baum", "ent\u00b7brennt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch will das Genzian nicht wagen", "tokens": ["Noch", "will", "das", "Gen\u00b7zi\u00b7an", "nicht", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dunkeln Wimpern aufzuschlagen;", "tokens": ["Die", "dun\u00b7keln", "Wim\u00b7pern", "auf\u00b7zu\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Noch schl\u00e4ft die Luft im Nebeldicht.", "tokens": ["Noch", "schl\u00e4ft", "die", "Luft", "im", "Ne\u00b7bel\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Welch' greller Schrei die Stille bricht?", "tokens": ["Welch'", "grel\u00b7ler", "Schrei", "die", "Stil\u00b7le", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Auerhahn begr\u00fc\u00dft das Licht;", "tokens": ["Der", "Au\u00b7er\u00b7hahn", "be\u00b7gr\u00fc\u00dft", "das", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er schaukelt, wiegt sich, macht sich breit,", "tokens": ["Er", "schau\u00b7kelt", ",", "wiegt", "sich", ",", "macht", "sich", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er putzt sein stattlich Federkleid,", "tokens": ["Er", "putzt", "sein", "statt\u00b7lich", "Fe\u00b7der\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und langsam streckt ihr stumpf Gesicht", "tokens": ["Und", "lang\u00b7sam", "streckt", "ihr", "stumpf", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Marmotte aus hohlen Baumes Nacht:", "tokens": ["Mar\u00b7mot\u00b7te", "aus", "hoh\u00b7len", "Bau\u00b7mes", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Das Leben, Leben ist erwacht;", "tokens": ["Das", "Le\u00b7ben", ",", "Le\u00b7ben", "ist", "er\u00b7wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die Geier pfeifen, Birkhahn ruft,", "tokens": ["Die", "Gei\u00b7er", "pfei\u00b7fen", ",", "Birk\u00b7hahn", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Schneeh\u00fchner flattern aus der Kluft;", "tokens": ["Schnee\u00b7h\u00fch\u00b7ner", "flat\u00b7tern", "aus", "der", "Kluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Die Fichten selbst, da\u00df keiner s\u00e4ume,", "tokens": ["Die", "Fich\u00b7ten", "selbst", ",", "da\u00df", "kei\u00b7ner", "s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Erz\u00e4hlen fl\u00fcsternd sich die Tr\u00e4ume.", "tokens": ["Er\u00b7z\u00e4h\u00b7len", "fl\u00fcs\u00b7ternd", "sich", "die", "Tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und durch Remi geht \u00fcberall", "tokens": ["Und", "durch", "Re\u00b7mi", "geht", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Ein dumpf Gemurr von Stall zu Stall.", "tokens": ["Ein", "dumpf", "Ge\u00b7murr", "von", "Stall", "zu", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}