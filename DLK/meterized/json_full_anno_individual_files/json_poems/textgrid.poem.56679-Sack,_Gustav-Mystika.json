{"textgrid.poem.56679": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Mystika", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend, viel mal tausend Jahre", "tokens": ["Tau\u00b7send", ",", "viel", "mal", "tau\u00b7send", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "$,", "ADV", "ADV", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "rollten ab gleich einem Blitz,", "tokens": ["roll\u00b7ten", "ab", "gleich", "ei\u00b7nem", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "eh' ich aus verliebtem Paare", "tokens": ["eh'", "ich", "aus", "ver\u00b7lieb\u00b7tem", "Paa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sprang hervor, ein dummer Witz", "tokens": ["sprang", "her\u00b7vor", ",", "ein", "dum\u00b7mer", "Witz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "eines ewig Namenlosen.", "tokens": ["ei\u00b7nes", "e\u00b7wig", "Na\u00b7men\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Armer kleiner Narrensang,", "tokens": ["Ar\u00b7mer", "klei\u00b7ner", "Nar\u00b7ren\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der in wilder Jahre Tosen", "tokens": ["der", "in", "wil\u00b7der", "Jah\u00b7re", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schwindet, eh' er kaum erklang!", "tokens": ["schwin\u00b7det", ",", "eh'", "er", "kaum", "er\u00b7klang", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber dennoch berge ich", "tokens": ["A\u00b7ber", "den\u00b7noch", "ber\u00b7ge", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "in mir aller Welten Qualen", "tokens": ["in", "mir", "al\u00b7ler", "Wel\u00b7ten", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und was sein wird, das mu\u00df sich", "tokens": ["und", "was", "sein", "wird", ",", "das", "mu\u00df", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VAINF", "VAFIN", "$,", "PDS", "VMFIN", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vorher erst in mir abmalen.", "tokens": ["vor\u00b7her", "erst", "in", "mir", "ab\u00b7ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, der Vater, der mich rief,", "tokens": ["Ja", ",", "der", "Va\u00b7ter", ",", "der", "mich", "rief", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "kann in mir erst Dasein werden,", "tokens": ["kann", "in", "mir", "erst", "Da\u00b7sein", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ohne mich sogar verlief,", "tokens": ["oh\u00b7ne", "mich", "so\u00b7gar", "ver\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "sich der wilde Tanz der Erden. \u2013", "tokens": ["sich", "der", "wil\u00b7de", "Tanz", "der", "Er\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie die Narrenkappe schellt!", "tokens": ["Wie", "die", "Nar\u00b7ren\u00b7kap\u00b7pe", "schellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ohne solches Klingen", "tokens": ["A\u00b7ber", "oh\u00b7ne", "sol\u00b7ches", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wirst du und die ganze Welt", "tokens": ["wirst", "du", "und", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "blitzschnell in ein Nichts zerspringen!", "tokens": ["blitz\u00b7schnell", "in", "ein", "Nichts", "zer\u00b7sprin\u00b7gen"