{"textgrid.poem.49157": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, vergn\u00fcge mit der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, vergn\u00fcge mit der Welt,", "tokens": ["Herr", ",", "ver\u00b7gn\u00fc\u00b7ge", "mit", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den, der auf dieselbe h\u00e4lt,", "tokens": ["Den", ",", "der", "auf", "die\u00b7sel\u00b7be", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb ihm allen Schatz der Erden,", "tokens": ["Gieb", "ihm", "al\u00b7len", "Schatz", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoheit, Wollust, Ehr' und Freud',", "tokens": ["Ho\u00b7heit", ",", "Wol\u00b7lust", ",", "Ehr'", "und", "Freud'", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und lass' mir in Schmach und Leid", "tokens": ["Und", "lass'", "mir", "in", "Schmach", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nur daf\u00fcr den Himmel werden;", "tokens": ["Nur", "da\u00b7f\u00fcr", "den", "Him\u00b7mel", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn ich gern hier nichts will sein,", "tokens": ["Denn", "ich", "gern", "hier", "nichts", "will", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "PIS", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Krieg' ich dort nur Alles ein.", "tokens": ["Krieg'", "ich", "dort", "nur", "Al\u00b7les", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer nur dieses Leben sucht,", "tokens": ["Wer", "nur", "die\u00b7ses", "Le\u00b7ben", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist auch werth nur dessen Frucht;", "tokens": ["Ist", "auch", "werth", "nur", "des\u00b7sen", "Frucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat er nur des Bauches Frommen,", "tokens": ["Hat", "er", "nur", "des", "Bau\u00b7ches", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat er seine Leibesh\u00fcll'", "tokens": ["Hat", "er", "sei\u00b7ne", "Lei\u00b7bes\u00b7h\u00fcll'"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihm gleicher Kinder F\u00fcll',", "tokens": ["Und", "ihm", "glei\u00b7cher", "Kin\u00b7der", "F\u00fcll'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die nach ihm sein Gut bekommen,", "tokens": ["Die", "nach", "ihm", "sein", "Gut", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Herr, der l\u00e4\u00dft dir deine Stern'", "tokens": ["Herr", ",", "der", "l\u00e4\u00dft", "dir", "dei\u00b7ne", "Stern'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und derselben Reichthum gern.", "tokens": ["Und", "der\u00b7sel\u00b7ben", "Reicht\u00b7hum", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich dagegen finde nicht,", "tokens": ["Ich", "da\u00b7ge\u00b7gen", "fin\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem mein Herz ohn' dich verpflicht;", "tokens": ["Dem", "mein", "Herz", "ohn'", "dich", "ver\u00b7pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich sterben, ist mein Leben,", "tokens": ["T\u00e4g\u00b7lich", "ster\u00b7ben", ",", "ist", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kummer ist mein Trank und Brod", "tokens": ["Kum\u00b7mer", "ist", "mein", "Trank", "und", "Brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein Gut Beschwer und Noth;", "tokens": ["Und", "mein", "Gut", "Be\u00b7schwer", "und", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses hei\u00dft nach dir mich streben,", "tokens": ["Die\u00b7ses", "hei\u00dft", "nach", "dir", "mich", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn was Andre unterdr\u00fcckt,", "tokens": ["Denn", "was", "And\u00b7re", "un\u00b7ter\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist, was mich hinauf stets r\u00fcckt.", "tokens": ["Ist", ",", "was", "mich", "hin\u00b7auf", "stets", "r\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, wie fr\u00f6hlich wird mein Geist,", "tokens": ["Ach", ",", "wie", "fr\u00f6h\u00b7lich", "wird", "mein", "Geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn er sich der Zeit entrei\u00dft", "tokens": ["Wenn", "er", "sich", "der", "Zeit", "ent\u00b7rei\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in dein unsterblich Leben", "tokens": ["Und", "in", "dein", "uns\u00b7terb\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Blick verrichten mag,", "tokens": ["Ei\u00b7nen", "Blick", "ver\u00b7rich\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn ich denk' an jenen Tag,", "tokens": ["Wenn", "ich", "denk'", "an", "je\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da ich hier soll Abschied geben,", "tokens": ["Da", "ich", "hier", "soll", "Ab\u00b7schied", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Und wie vieler Freuden voll,", "tokens": ["Und", "wie", "vie\u00b7ler", "Freu\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott, dein Antlitz schauen soll!", "tokens": ["Gott", ",", "dein", "Ant\u00b7litz", "schau\u00b7en", "soll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Setzt mich meine S\u00fcnde zwar", "tokens": ["Setzt", "mich", "mei\u00b7ne", "S\u00fcn\u00b7de", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich den Andern auf die Bahr',", "tokens": ["Gleich", "den", "An\u00b7dern", "auf", "die", "Bahr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werd' ich endlich doch erwachen", "tokens": ["Werd'", "ich", "end\u00b7lich", "doch", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in deinem Heilesrock,", "tokens": ["Und", "in", "dei\u00b7nem", "Hei\u00b7les\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesu, wie die sch\u00f6nste Tock'", "tokens": ["Je\u00b7su", ",", "wie", "die", "sch\u00f6ns\u00b7te", "Tock'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Wiederum hervor mich machen,", "tokens": ["Wie\u00b7de\u00b7rum", "her\u00b7vor", "mich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn mich deines Lebens Bild", "tokens": ["Wenn", "mich", "dei\u00b7nes", "Le\u00b7bens", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit verj\u00fcngter Kraft erf\u00fcllt.", "tokens": ["Mit", "ver\u00b7j\u00fcng\u00b7ter", "Kraft", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann wird mir der Unschuld Schein,", "tokens": ["Dann", "wird", "mir", "der", "Un\u00b7schuld", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann, ach, werd' ich satte sein", "tokens": ["Dann", ",", "ach", ",", "werd'", "ich", "sat\u00b7te", "sein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ITJ", "$,", "VAFIN", "PPER", "VVFIN", "PPOSAT"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von dem Manna, von den Str\u00f6men", "tokens": ["Von", "dem", "Man\u00b7na", ",", "von", "den", "Str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Deiner Liebe, Gnad' und G\u00fct'.", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", ",", "Gnad'", "und", "G\u00fct'", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebster Gott, lass' mein Gem\u00fcth", "tokens": ["Liebs\u00b7ter", "Gott", ",", "lass'", "mein", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hierher sein' Erquickung nehmen,", "tokens": ["Hier\u00b7her", "sein'", "Er\u00b7quic\u00b7kung", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Bis ich einmal von der Erd'", "tokens": ["Bis", "ich", "ein\u00b7mal", "von", "der", "Erd'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst dahin gelangen werd'.", "tokens": ["Selbst", "da\u00b7hin", "ge\u00b7lan\u00b7gen", "werd'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr, vergn\u00fcge mit der Welt,", "tokens": ["Herr", ",", "ver\u00b7gn\u00fc\u00b7ge", "mit", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den, der auf dieselbe h\u00e4lt,", "tokens": ["Den", ",", "der", "auf", "die\u00b7sel\u00b7be", "h\u00e4lt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "PDAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb ihm allen Schatz der Erden,", "tokens": ["Gieb", "ihm", "al\u00b7len", "Schatz", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.