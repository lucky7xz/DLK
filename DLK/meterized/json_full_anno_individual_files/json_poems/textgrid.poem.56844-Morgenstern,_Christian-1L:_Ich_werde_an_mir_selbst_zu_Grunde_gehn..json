{"textgrid.poem.56844": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich werde an mir selbst zu Grunde gehn.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werde an mir selbst zu Grunde gehn.", "tokens": ["Ich", "wer\u00b7de", "an", "mir", "selbst", "zu", "Grun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich, das sind zwei, ein M\u00f6chte sein und Bin, \u2013", "tokens": ["Ich", ",", "das", "sind", "zwei", ",", "ein", "M\u00f6ch\u00b7te", "sein", "und", "Bin", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "PDS", "VAFIN", "CARD", "$,", "ART", "NN", "VAINF", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und jenes wird zum Schlusse dies erw\u00fcrgen.", "tokens": ["und", "je\u00b7nes", "wird", "zum", "Schlus\u00b7se", "dies", "er\u00b7w\u00fcr\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPRART", "NN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das M\u00f6chte sein ist wie ein rasend Ro\u00df,", "tokens": ["Das", "M\u00f6ch\u00b7te", "sein", "ist", "wie", "ein", "ra\u00b7send", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "VAFIN", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "an dessen Schweif das Bin gefesselt ward,", "tokens": ["an", "des\u00b7sen", "Schweif", "das", "Bin", "ge\u00b7fes\u00b7selt", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ist wie ein Rad, darauf das Bin geflochten,", "tokens": ["ist", "wie", "ein", "Rad", ",", "da\u00b7rauf", "das", "Bin", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ist wie ein M\u00f6nch, der sich den Leib zerdornt,", "tokens": ["ist", "wie", "ein", "M\u00f6nch", ",", "der", "sich", "den", "Leib", "zer\u00b7dornt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "wie eine Furie, deren Finger sich", "tokens": ["wie", "ei\u00b7ne", "Fu\u00b7rie", ",", "de\u00b7ren", "Fin\u00b7ger", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELAT", "NN", "PRF"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "in ihres Opfers Haar verstricken, wie", "tokens": ["in", "ih\u00b7res", "Op\u00b7fers", "Haar", "ver\u00b7stri\u00b7cken", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,", "PWAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ein Vampyr, der am Herzen sitz und saugt", "tokens": ["ein", "Vam\u00b7pyr", ",", "der", "am", "Her\u00b7zen", "sitz", "und", "saugt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "NE", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "und saugt ...", "tokens": ["und", "saugt", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Wohl wie ein Gott auch, der emporziehn will,", "tokens": ["Wohl", "wie", "ein", "Gott", "auch", ",", "der", "em\u00b7por\u00b7ziehn", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "oder ein Weib, aus dessen Augen es", "tokens": ["o\u00b7der", "ein", "Weib", ",", "aus", "des\u00b7sen", "Au\u00b7gen", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "dem Wanderer entgegenlockt, und das", "tokens": ["dem", "Wan\u00b7de\u00b7rer", "ent\u00b7ge\u00b7gen\u00b7lockt", ",", "und", "das"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "der atemlose Narr doch nie erreicht.", "tokens": ["der", "a\u00b7tem\u00b7lo\u00b7se", "Narr", "doch", "nie", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "So sieht mein Ich von innen aus, von au\u00dfen", "tokens": ["So", "sieht", "mein", "Ich", "von", "in\u00b7nen", "aus", ",", "von", "au\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "PPER", "APPR", "ADV", "PTKVZ", "$,", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "ein Haus wie andre, hell die Fenster manchmal,", "tokens": ["ein", "Haus", "wie", "and\u00b7re", ",", "hell", "die", "Fens\u00b7ter", "manch\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PIS", "$,", "ADJD", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "doch \u00f6fter dunkel. Sto\u00df die T\u00fcr auf! schau", "tokens": ["doch", "\u00f6f\u00b7ter", "dun\u00b7kel", ".", "Sto\u00df", "die", "T\u00fcr", "auf", "!", "schau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADJD", "$.", "NN", "ART", "NN", "PTKVZ", "$.", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "die sch\u00f6ne Eh' von Bin und M\u00f6chte sein!", "tokens": ["die", "sch\u00f6\u00b7ne", "Eh'", "von", "Bin", "und", "M\u00f6ch\u00b7te", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Schaut nur hinein und ruft bedauernd ach!", "tokens": ["Schaut", "nur", "hin\u00b7ein", "und", "ruft", "be\u00b7dau\u00b7ernd", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ADJD", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "und weh! und, wenns euch leichter macht, auch pfui!", "tokens": ["und", "weh", "!", "und", ",", "wenns", "euch", "leich\u00b7ter", "macht", ",", "auch", "pfui", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "KON", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Bin ich nicht Dichter? Hab ich nicht das Vorrecht \u2013", "tokens": ["Bin", "ich", "nicht", "Dich\u00b7ter", "?", "Hab", "ich", "nicht", "das", "Vor\u00b7recht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$.", "NN", "PPER", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "oh welch ein Vorrecht! \u2013 jedem frechen Auge", "tokens": ["oh", "welch", "ein", "Vor\u00b7recht", "!", "\u2013", "je\u00b7dem", "fre\u00b7chen", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "ART", "NN", "$.", "$(", "PIAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "die R\u00e4ume meiner H\u00e4uslichkeit zu zeigen?", "tokens": ["die", "R\u00e4u\u00b7me", "mei\u00b7ner", "H\u00e4us\u00b7lich\u00b7keit", "zu", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "H\u00f6rt doch mein Pathos, das euch jeden Winkel", "tokens": ["H\u00f6rt", "doch", "mein", "Pa\u00b7thos", ",", "das", "euch", "je\u00b7den", "Win\u00b7kel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PIAT", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "beschreibt und tut, als h\u00e4tt es just zum Zweck,", "tokens": ["be\u00b7schreibt", "und", "tut", ",", "als", "h\u00e4tt", "es", "just", "zum", "Zweck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "ihn euch als Sehensw\u00fcrdigkeit zu preisen.", "tokens": ["ihn", "euch", "als", "Se\u00b7hens\u00b7w\u00fcr\u00b7dig\u00b7keit", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ists Eitelkeit, die mich zum Cicerone", "tokens": ["Ists", "Ei\u00b7tel\u00b7keit", ",", "die", "mich", "zum", "Ci\u00b7ce\u00b7ro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "der eignen Seele macht? ists Geiz nach Ehre?", "tokens": ["der", "eig\u00b7nen", "See\u00b7le", "macht", "?", "ists", "Geiz", "nach", "Eh\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Mangel an Scham, an Stolz, an Wert, an Tiefe?", "tokens": ["Man\u00b7gel", "an", "Scham", ",", "an", "Stolz", ",", "an", "Wert", ",", "an", "Tie\u00b7fe", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Das alles ists wohl auch, doch ists noch mehr.", "tokens": ["Das", "al\u00b7les", "ists", "wohl", "auch", ",", "doch", "ists", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADV", "$,", "ADV", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "So etwas noch wie Rachsucht, Grausamkeit,", "tokens": ["So", "et\u00b7was", "noch", "wie", "Rach\u00b7sucht", ",", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KOKOM", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Blutgierde, Ha\u00df, Verachtung wider mich selbst,", "tokens": ["Blut\u00b7gier\u00b7de", ",", "Ha\u00df", ",", "Ver\u00b7ach\u00b7tung", "wi\u00b7der", "mich", "selbst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.34": {"text": "so etwas, das nicht hat, was es erlechzt,", "tokens": ["so", "et\u00b7was", ",", "das", "nicht", "hat", ",", "was", "es", "er\u00b7lechzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PTKNEG", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "ein Durst nach Macht, der, ungestillt, verzehrt,", "tokens": ["ein", "Durst", "nach", "Macht", ",", "der", ",", "un\u00b7ge\u00b7stillt", ",", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "das W\u00e4r' ich! vor der kalten Sphinx Ich bin.", "tokens": ["das", "W\u00e4r'", "ich", "!", "vor", "der", "kal\u00b7ten", "Sphinx", "Ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$.", "APPR", "ART", "ADJA", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ja, darum f\u00fchr ich euch herum in mir,", "tokens": ["Ja", ",", "da\u00b7rum", "f\u00fchr", "ich", "euch", "he\u00b7rum", "in", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PAV", "VVFIN", "PPER", "PRF", "APZR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "weil ich mir selbst damit das Herz zerrei\u00dfe,", "tokens": ["weil", "ich", "mir", "selbst", "da\u00b7mit", "das", "Herz", "zer\u00b7rei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "mich selbst erniedre und zum Schw\u00e4tzer mache;", "tokens": ["mich", "selbst", "er\u00b7nied\u00b7re", "und", "zum", "Schw\u00e4t\u00b7zer", "ma\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "es tut so wohl, wenn man den stumpfen Schmerz", "tokens": ["es", "tut", "so", "wohl", ",", "wenn", "man", "den", "stump\u00b7fen", "Schmerz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "laut bluten l\u00e4\u00dft aus aufgerissnen Wunden.", "tokens": ["laut", "blu\u00b7ten", "l\u00e4\u00dft", "aus", "auf\u00b7ge\u00b7riss\u00b7nen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Und dann: Ihr seht ja nur das Blut und nicht", "tokens": ["Und", "dann", ":", "Ihr", "seht", "ja", "nur", "das", "Blut", "und", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "KON", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "das Herz, daraus es stammt! Es lacht vielleicht,", "tokens": ["das", "Herz", ",", "da\u00b7raus", "es", "stammt", "!", "Es", "lacht", "viel\u00b7leicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "wenn ihr des Blutes F\u00e4rbung d\u00fcster findet,", "tokens": ["wenn", "ihr", "des", "Blu\u00b7tes", "F\u00e4r\u00b7bung", "d\u00fcs\u00b7ter", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "und weint gewi\u00df, indes ihr w\u00e4hnt, es lacht.", "tokens": ["und", "weint", "ge\u00b7wi\u00df", ",", "in\u00b7des", "ihr", "w\u00e4hnt", ",", "es", "lacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Woh