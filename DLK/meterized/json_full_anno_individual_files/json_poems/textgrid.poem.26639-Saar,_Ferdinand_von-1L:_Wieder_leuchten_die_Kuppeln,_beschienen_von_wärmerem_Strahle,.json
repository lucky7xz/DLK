 "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Dort schon schimmert der Strom, schimmern die knospenden Au'n.", "tokens": ["Dort", "schon", "schim\u00b7mert", "der", "Strom", ",", "schim\u00b7mern", "die", "knos\u00b7pen\u00b7den", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Tiefes Schweigen ringsum; nur von noch scholligen Feldern", "tokens": ["Tie\u00b7fes", "Schwei\u00b7gen", "ring\u00b7sum", ";", "nur", "von", "noch", "schol\u00b7li\u00b7gen", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$.", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Schwingt sich mit Jubelgesang einsam die Lerche empor.", "tokens": ["Schwingt", "sich", "mit", "Ju\u00b7bel\u00b7ge\u00b7sang", "ein\u00b7sam", "die", "Ler\u00b7che", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Bl\u00fchende B\u00e4ume umfrieden vereinzelte stille Geh\u00f6fte,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "B\u00e4u\u00b7me", "um\u00b7frie\u00b7den", "ver\u00b7ein\u00b7zel\u00b7te", "stil\u00b7le", "Ge\u00b7h\u00f6f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und in br\u00e4utlichem Schmuck s