{"textgrid.poem.26639": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder leuchten die Kuppeln, beschienen von w\u00e4rmerem Strahle,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder leuchten die Kuppeln, beschienen von w\u00e4rmerem Strahle,", "tokens": ["Wie\u00b7der", "leuch\u00b7ten", "die", "Kup\u00b7peln", ",", "be\u00b7schie\u00b7nen", "von", "w\u00e4r\u00b7me\u00b7rem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und in mildestem Blau breitet der Himmel sich aus.", "tokens": ["Und", "in", "mil\u00b7des\u00b7tem", "Blau", "brei\u00b7tet", "der", "Him\u00b7mel", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sonnige L\u00fcfte umkosen das Antlitz der wandelnden Menschen,", "tokens": ["Son\u00b7ni\u00b7ge", "L\u00fcf\u00b7te", "um\u00b7ko\u00b7sen", "das", "Ant\u00b7litz", "der", "wan\u00b7deln\u00b7den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Fr\u00fchlingsh\u00fctchen zur Schau tragen die Sch\u00f6nen bereits.", "tokens": ["Fr\u00fch\u00b7lings\u00b7h\u00fct\u00b7chen", "zur", "Schau", "tra\u00b7gen", "die", "Sch\u00f6\u00b7nen", "be\u00b7reits", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Duftende Veilchen verkauft man und zarte, goldige Primeln,", "tokens": ["Duf\u00b7ten\u00b7de", "Veil\u00b7chen", "ver\u00b7kauft", "man", "und", "zar\u00b7te", ",", "gol\u00b7di\u00b7ge", "Pri\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "KON", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Mit verlangendem Griff strecken die H\u00e4nde sich aus.", "tokens": ["Mit", "ver\u00b7lan\u00b7gen\u00b7dem", "Griff", "stre\u00b7cken", "die", "H\u00e4n\u00b7de", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Woche vor Ostern, du stillste des Jahres, wie bist du belebt doch!", "tokens": ["Wo\u00b7che", "vor", "Os\u00b7tern", ",", "du", "stills\u00b7te", "des", "Jah\u00b7res", ",", "wie", "bist", "du", "be\u00b7lebt", "doch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Kirchen- und Gr\u00e4berbesuch f\u00fcllen die Stra\u00dfen der Stadt.", "tokens": ["Kir\u00b7chen", "und", "Gr\u00e4\u00b7ber\u00b7be\u00b7such", "f\u00fcl\u00b7len", "die", "Stra\u00b7\u00dfen", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Fernher dr\u00e4ngt sich die Schaulust zum Auferstehungsgepr\u00e4nge:", "tokens": ["Fern\u00b7her", "dr\u00e4ngt", "sich", "die", "Schau\u00b7lust", "zum", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7ge\u00b7pr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Fahnen, Posaunen, Gesang, funkelnder Priesterornat. \u2013", "tokens": ["Fah\u00b7nen", ",", "Po\u00b7sau\u00b7nen", ",", "Ge\u00b7sang", ",", "fun\u00b7keln\u00b7der", "Pries\u00b7te\u00b7ror\u00b7nat", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ich doch wandle hinaus in's Freie und suche die Pfade,", "tokens": ["Ich", "doch", "wand\u00b7le", "hin\u00b7aus", "in's", "Frei\u00b7e", "und", "su\u00b7che", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPRART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die zum Kahlengebirg f\u00fchren allm\u00e4lig hinan.", "tokens": ["Die", "zum", "Kah\u00b7len\u00b7ge\u00b7birg", "f\u00fch\u00b7ren", "all\u00b7m\u00e4\u00b7lig", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "--+--++--+-+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Weiter und weiter erschlie\u00dft sich im Kreise die liebliche Landschaft;", "tokens": ["Wei\u00b7ter", "und", "wei\u00b7ter", "er\u00b7schlie\u00dft", "sich", "im", "Krei\u00b7se", "die", "lieb\u00b7li\u00b7che", "Land\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Dort schon schimmert der Strom, schimmern die knospenden Au'n.", "tokens": ["Dort", "schon", "schim\u00b7mert", "der", "Strom", ",", "schim\u00b7mern", "die", "knos\u00b7pen\u00b7den", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Tiefes Schweigen ringsum; nur von noch scholligen Feldern", "tokens": ["Tie\u00b7fes", "Schwei\u00b7gen", "ring\u00b7sum", ";", "nur", "von", "noch", "schol\u00b7li\u00b7gen", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$.", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Schwingt sich mit Jubelgesang einsam die Lerche empor.", "tokens": ["Schwingt", "sich", "mit", "Ju\u00b7bel\u00b7ge\u00b7sang", "ein\u00b7sam", "die", "Ler\u00b7che", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Bl\u00fchende B\u00e4ume umfrieden vereinzelte stille Geh\u00f6fte,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "B\u00e4u\u00b7me", "um\u00b7frie\u00b7den", "ver\u00b7ein\u00b7zel\u00b7te", "stil\u00b7le", "Ge\u00b7h\u00f6f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Und in br\u00e4utlichem Schmuck stehen die B\u00fcsche am Rain.", "tokens": ["Und", "in", "br\u00e4ut\u00b7li\u00b7chem", "Schmuck", "ste\u00b7hen", "die", "B\u00fc\u00b7sche", "am", "Rain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Endlich ist sie erreicht die Fernen er\u00f6ffnende Stelle,", "tokens": ["End\u00b7lich", "ist", "sie", "er\u00b7reicht", "die", "Fer\u00b7nen", "er\u00b7\u00f6ff\u00b7nen\u00b7de", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Wo ich als Knabe bereits schwelgenden Auges geweilt.", "tokens": ["Wo", "ich", "als", "Kna\u00b7be", "be\u00b7reits", "schwel\u00b7gen\u00b7den", "Au\u00b7ges", "ge\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KOUS", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Dort eine Bank auch \u2013 vielleicht noch dieselbe! Nun ruh' ich im Anblick.", "tokens": ["Dort", "ei\u00b7ne", "Bank", "auch", "\u2013", "viel\u00b7leicht", "noch", "die\u00b7sel\u00b7be", "!", "Nun", "ruh'", "ich", "im", "An\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$(", "ADV", "ADV", "PDAT", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Hehr aufschauert in mir wonniges Heimatgef\u00fchl.", "tokens": ["Hehr", "auf\u00b7schau\u00b7ert", "in", "mir", "won\u00b7ni\u00b7ges", "Hei\u00b7mat\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Ja, da bin ich im Herzen der alten, der herrlichen Ostmark,", "tokens": ["Ja", ",", "da", "bin", "ich", "im", "Her\u00b7zen", "der", "al\u00b7ten", ",", "der", "herr\u00b7li\u00b7chen", "Ost\u00b7mark", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Deren Banner einst stolz flatterte \u00fcber dem Reich \u2013", "tokens": ["De\u00b7ren", "Ban\u00b7ner", "einst", "stolz", "flat\u00b7ter\u00b7te", "\u00fc\u00b7ber", "dem", "Reich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Ueber dem Reich, von dem sie getrennt nun, beinahe ein Fremdling:", "tokens": ["Ue\u00b7ber", "dem", "Reich", ",", "von", "dem", "sie", "ge\u00b7trennt", "nun", ",", "bei\u00b7na\u00b7he", "ein", "Fremd\u00b7ling", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "ADV", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Oestreichs S\u00f6hne, man z\u00e4hlt kaum zu den Deutschen sie mehr.", "tokens": ["O\u00b7e\u00b7streichs", "S\u00f6h\u00b7ne", ",", "man", "z\u00e4hlt", "kaum", "zu", "den", "Deut\u00b7schen", "sie", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Aber nicht deshalb neig' ich die Stirn jetzt in bangender Trauer,", "tokens": ["A\u00b7ber", "nicht", "des\u00b7halb", "neig'", "ich", "die", "Stirn", "jetzt", "in", "ban\u00b7gen\u00b7der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PAV", "VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-++--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Weil du, mein Vaterland, ganz auf dich selber gestellt.", "tokens": ["Weil", "du", ",", "mein", "Va\u00b7ter\u00b7land", ",", "ganz", "auf", "dich", "sel\u00b7ber", "ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.29": {"text": "Proben kannst du die eigenste Kraft, die Kraft des Gerechten \u2013", "tokens": ["Pro\u00b7ben", "kannst", "du", "die", "ei\u00b7gens\u00b7te", "Kraft", ",", "die", "Kraft", "des", "Ge\u00b7rech\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Und es sinkt und es steigt ewig die Woge der Zeit.", "tokens": ["Und", "es", "sinkt", "und", "es", "steigt", "e\u00b7wig", "die", "Wo\u00b7ge", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Aber o Schmerz! Du bist auch getrennt von den eigenen Gliedern,", "tokens": ["A\u00b7ber", "o", "Schmerz", "!", "Du", "bist", "auch", "ge\u00b7trennt", "von", "den", "ei\u00b7ge\u00b7nen", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "In Verblendung, mit Ha\u00df w\u00fcthen sie gegen das Haupt.", "tokens": ["In", "Ver\u00b7blen\u00b7dung", ",", "mit", "Ha\u00df", "w\u00fct\u00b7hen", "sie", "ge\u00b7gen", "das", "Haupt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Doch du ", "tokens": ["Doch", "du"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Uralt m\u00e4chtiges Lied rauscht ihm die Donau hinan.", "tokens": ["Ur\u00b7alt", "m\u00e4ch\u00b7ti\u00b7ges", "Lied", "rauscht", "ihm", "die", "Do\u00b7nau", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PPER", "ART", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.35": {"text": "Und so wirst du besteh'n, was auch die Zukunft dir bringe \u2013", "tokens": ["Und", "so", "wirst", "du", "be\u00b7steh'n", ",", "was", "auch", "die", "Zu\u00b7kunft", "dir", "brin\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Dir und der heimischen Flur, die dich umgr\u00fcnt und umbl\u00fcht.", "tokens": ["Dir", "und", "der", "hei\u00b7mi\u00b7schen", "Flur", ",", "die", "dich", "um\u00b7gr\u00fcnt", "und", "um\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.37": {"text": "Sieh, es d\u00e4mmert der Abend, doch morgen flammt wieder das Fr\u00fchroth \u2013", "tokens": ["Sieh", ",", "es", "d\u00e4m\u00b7mert", "der", "A\u00b7bend", ",", "doch", "mor\u00b7gen", "flammt", "wie\u00b7der", "das", "Fr\u00fch\u00b7roth", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.38": {"text": "Und bei fernem Gel\u00e4ut' segnet dich jetzt dein Poet.", "tokens": ["Und", "bei", "fer\u00b7nem", "Ge\u00b7l\u00e4ut'", "seg\u00b7net", "dich", "jetzt", "dein", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Wieder leuchten die Kuppeln, beschienen von w\u00e4rmerem Strahle,", "tokens": ["Wie\u00b7der", "leuch\u00b7ten", "die", "Kup\u00b7peln", ",", "be\u00b7schie\u00b7nen", "von", "w\u00e4r\u00b7me\u00b7rem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Und in mildestem Blau breitet der Himmel sich aus.", "tokens": ["Und", "in", "mil\u00b7des\u00b7tem", "Blau", "brei\u00b7tet", "der", "Him\u00b7mel", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sonnige L\u00fcfte umkosen das Antlitz der wandelnden Menschen,", "tokens": ["Son\u00b7ni\u00b7ge", "L\u00fcf\u00b7te", "um\u00b7ko\u00b7sen", "das", "Ant\u00b7litz", "der", "wan\u00b7deln\u00b7den", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Fr\u00fchlingsh\u00fctchen zur Schau tragen die Sch\u00f6nen bereits.", "tokens": ["Fr\u00fch\u00b7lings\u00b7h\u00fct\u00b7chen", "zur", "Schau", "tra\u00b7gen", "die", "Sch\u00f6\u00b7nen", "be\u00b7reits", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Duftende Veilchen verkauft man und zarte, goldige Pri