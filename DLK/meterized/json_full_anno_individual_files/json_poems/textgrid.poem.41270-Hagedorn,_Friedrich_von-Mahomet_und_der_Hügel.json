{"textgrid.poem.41270": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mahomet und der H\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Volk sprach der Prophet beth\u00f6rter Muselm\u00e4nner:", "tokens": ["Zum", "Volk", "sprach", "der", "Pro\u00b7phet", "be\u00b7th\u00f6r\u00b7ter", "Mu\u00b7sel\u00b7m\u00e4n\u00b7ner", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wahrheit zum Beweis, ist unsers Allah Schlu\u00df,", "tokens": ["Der", "Wahr\u00b7heit", "zum", "Be\u00b7weis", ",", "ist", "un\u00b7sers", "Al\u00b7lah", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df, wenn ihr w\u00fcrdig glaubt, versammelte Bekenner,", "tokens": ["Da\u00df", ",", "wenn", "ihr", "w\u00fcr\u00b7dig", "glaubt", ",", "ver\u00b7sam\u00b7mel\u00b7te", "Be\u00b7ken\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Der H\u00fcgel, der dort ruht, sich einst uns n\u00e4hern mu\u00df ...", "tokens": ["Der", "H\u00fc\u00b7gel", ",", "der", "dort", "ruht", ",", "sich", "einst", "uns", "n\u00e4\u00b7hern", "mu\u00df", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PRF", "ADV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf, H\u00fcgel, h\u00f6re mich! Vernimm, du Kind der Erde,", "tokens": ["Auf", ",", "H\u00fc\u00b7gel", ",", "h\u00f6\u00b7re", "mich", "!", "Ver\u00b7nimm", ",", "du", "Kind", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "VVFIN", "PPER", "$.", "NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vernimm des Sch\u00f6pfers Ruf! Der Ruf erschallt durch mich:", "tokens": ["Ver\u00b7nimm", "des", "Sch\u00f6p\u00b7fers", "Ruf", "!", "Der", "Ruf", "er\u00b7schallt", "durch", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er will, da\u00df diesem Volk ein Wunder sichtbar werde,", "tokens": ["Er", "will", ",", "da\u00df", "die\u00b7sem", "Volk", "ein", "Wun\u00b7der", "sicht\u00b7bar", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PDAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erscheine hier vor uns! Auf, auf! Erhebe dich! ...", "tokens": ["Er\u00b7schei\u00b7ne", "hier", "vor", "uns", "!", "Auf", ",", "auf", "!", "Er\u00b7he\u00b7be", "dich", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "APPR", "PPER"