{"textgrid.poem.49398": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Haupt der Familie und ein w\u00fcrd'ger B\u00fcrgermeister.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Haupt der Familie und ein w\u00fcrd'ger B\u00fcrgermeister.", "tokens": ["Haupt", "der", "Fa\u00b7mi\u00b7lie", "und", "ein", "w\u00fcrd'\u00b7ger", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der hohe Kragen schluckt sein Ohr, man sieht es kaum,", "tokens": ["Der", "ho\u00b7he", "Kra\u00b7gen", "schluckt", "sein", "Ohr", ",", "man", "sieht", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die \u00c4uglein schwimmen sorglos wie in sel'gem Traum,", "tokens": ["Die", "\u00c4ug\u00b7lein", "schwim\u00b7men", "sorg\u00b7los", "wie", "in", "sel'\u00b7gem", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sein buntgestickter Schuh, wie farbenfreudig gleisst er.", "tokens": ["Sein", "bunt\u00b7ge\u00b7stick\u00b7ter", "Schuh", ",", "wie", "far\u00b7ben\u00b7freu\u00b7dig", "gleisst", "er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht der Gestirne Gold und nicht die Lauben preist er,", "tokens": ["Nicht", "der", "Ge\u00b7stir\u00b7ne", "Gold", "und", "nicht", "die", "Lau\u00b7ben", "preist", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "KON", "PTKNEG", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo s\u00fcss der Vogel singt, und nicht den Himmelsraum.", "tokens": ["Wo", "s\u00fcss", "der", "Vo\u00b7gel", "singt", ",", "und", "nicht", "den", "Him\u00b7mels\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NE", "VVFIN", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was k\u00fcmmert ihn die Flur, der Wiesen gr\u00fcner Saum?", "tokens": ["Was", "k\u00fcm\u00b7mert", "ihn", "die", "Flur", ",", "der", "Wie\u00b7sen", "gr\u00fc\u00b7ner", "Saum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf seiner Tochter Heirat richtet seinen Geist er.", "tokens": ["Auf", "sei\u00b7ner", "Toch\u00b7ter", "Hei\u00b7rat", "rich\u00b7tet", "sei\u00b7nen", "Geist", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr Dingsda ist's, der ihm als Schwiegersohn gef\u00e4llt.", "tokens": ["Herr", "Dings\u00b7da", "ist's", ",", "der", "ihm", "als", "Schwie\u00b7ger\u00b7sohn", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "PRELS", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist Botaniker, ist dick, hat ziemlich Geld \u2013", "tokens": ["Er", "ist", "Bo\u00b7ta\u00b7ni\u00b7ker", ",", "ist", "dick", ",", "hat", "ziem\u00b7lich", "Geld", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "VAFIN", "ADJD", "$,", "VAFIN", "ADV", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "M\u00f6g' ihn der Himmel vor dem Dichterpack beh\u00fcten!", "tokens": ["M\u00f6g'", "ihn", "der", "Him\u00b7mel", "vor", "dem", "Dich\u00b7ter\u00b7pack", "be\u00b7h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So schlecht gek\u00e4mmtes Volk hat ihm noch nie gepasst.", "tokens": ["So", "schlecht", "ge\u00b7k\u00e4mm\u00b7tes", "Volk", "hat", "ihm", "noch", "nie", "ge\u00b7passt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr als sein ew'ger Schnupfen ist es ihm verhasst.", "tokens": ["Mehr", "als", "sein", "ew'\u00b7ger", "Schnup\u00b7fen", "ist", "es", "ihm", "ver\u00b7hasst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word