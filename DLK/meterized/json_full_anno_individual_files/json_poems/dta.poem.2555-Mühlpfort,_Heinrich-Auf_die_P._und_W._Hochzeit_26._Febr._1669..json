{"dta.poem.2555": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die P. und W. Hochzeit 26. Febr. 1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Venus lie\u00df ein Mahl auffs pr\u00e4chtigste bereiten/", "tokens": ["Die", "Ve\u00b7nus", "lie\u00df", "ein", "Mahl", "auffs", "pr\u00e4ch\u00b7tigs\u00b7te", "be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Cupido solte da der Jungfern Marschalck seyn;", "tokens": ["Cu\u00b7pi\u00b7do", "sol\u00b7te", "da", "der", "Jung\u00b7fern", "Mar\u00b7schalck", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bald henckt der kleine Schalck den K\u00f6cher an die Seit\u1ebd/", "tokens": ["Bald", "henckt", "der", "klei\u00b7ne", "Schalck", "den", "K\u00f6\u00b7cher", "an", "die", "Seit\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ladet hier und dort die sch\u00f6nen Nimfen ein:", "tokens": ["Und", "la\u00b7det", "hier", "und", "dort", "die", "sch\u00f6\u00b7nen", "Nim\u00b7fen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er sagte/ werthes Volck/ des Himmels Meister-St\u00fccke/", "tokens": ["Er", "sag\u00b7te", "/", "wert\u00b7hes", "Volck", "/", "des", "Him\u00b7mels", "Meis\u00b7ter\u00b7St\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr Edelsten Gesch\u00f6pff/ und Wunder dieser Welt/", "tokens": ["Ihr", "E\u00b7dels\u00b7ten", "Ge\u00b7sch\u00f6pff", "/", "und", "Wun\u00b7der", "die\u00b7ser", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "NN", "PDAT", "NN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Seht welch ein Freuden-Schein durch milde Gnaden-Blicke/", "tokens": ["Seht", "welch", "ein", "Freu\u00b7den\u00b7Schein", "durch", "mil\u00b7de", "Gna\u00b7den\u00b7Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAT", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von meiner Mutter Gunst auff eure Scheitel f\u00e4llt.", "tokens": ["Von", "mei\u00b7ner", "Mut\u00b7ter", "Gunst", "auff", "eu\u00b7re", "Schei\u00b7tel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie hat ein Fest der Lust h\u00f6chst-feyrlich angesetzet/", "tokens": ["Sie", "hat", "ein", "Fest", "der", "Lust", "h\u00f6chst\u00b7fey\u00b7rlich", "an\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das eure Gegenwart noch angenehmer macht.", "tokens": ["Das", "eu\u00b7re", "Ge\u00b7gen\u00b7wart", "noch", "an\u00b7ge\u00b7neh\u00b7mer", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was nur ersinnlich ist/ das euch durchaus ergetzet/", "tokens": ["Was", "nur", "er\u00b7sinn\u00b7lich", "ist", "/", "das", "euch", "durc\u00b7haus", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird da in einem Kreis als wie zusammen bracht/", "tokens": ["Wird", "da", "in", "ei\u00b7nem", "Kreis", "als", "wie", "zu\u00b7sam\u00b7men", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "KOUS", "KOKOM", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Tafel ist bestellt mit auserlesnen Speisen/", "tokens": ["Die", "Ta\u00b7fel", "ist", "be\u00b7stellt", "mit", "au\u00b7ser\u00b7les\u00b7nen", "Spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jhr habt da G\u00f6tter-Brodt und lauter Nectar-Wein.", "tokens": ["Ihr", "habt", "da", "G\u00f6t\u00b7ter\u00b7Brodt", "und", "lau\u00b7ter", "Nec\u00b7ta\u00b7rWein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was meine Pflicht betrifft/ wil ich mich so erweisen/", "tokens": ["Was", "mei\u00b7ne", "Pflicht", "be\u00b7tr\u00b7ifft", "/", "wil", "ich", "mich", "so", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Da\u00df jeder sagen kan/ Cupido macht es fein.", "tokens": ["Da\u00df", "je\u00b7der", "sa\u00b7gen", "kan", "/", "Cu\u00b7pi\u00b7do", "macht", "es", "fein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$(", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Alleine last euch di\u00df zu einer Warnung dienen/", "tokens": ["Al\u00b7lei\u00b7ne", "last", "euch", "di\u00df", "zu", "ei\u00b7ner", "War\u00b7nung", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Je sch\u00f6ner ihr geziert/ je h\u00f6her man euch r\u00fchmt.", "tokens": ["Je", "sch\u00f6\u00b7ner", "ihr", "ge\u00b7ziert", "/", "je", "h\u00f6\u00b7her", "man", "euch", "r\u00fchmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVPP", "$(", "ADV", "ADJD", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wo ihr/ wie Sonnen/ kom\u0303t in Schmuck und Pracht geschienen/", "tokens": ["Wo", "ihr", "/", "wie", "Son\u00b7nen", "/", "kom\u0303t", "in", "Schmuck", "und", "Pracht", "ge\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "KOKOM", "NN", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und da\u00df der gantze Leib mit Sternen \u00fcberbl\u00fchmt/", "tokens": ["Und", "da\u00df", "der", "gant\u00b7ze", "Leib", "mit", "Ster\u00b7nen", "\u00fc\u00b7berb\u00b7l\u00fchmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So wird der Venus Aug\u2019 euch mit Vergn\u00fcgung schauen.", "tokens": ["So", "wird", "der", "Ve\u00b7nus", "Aug'", "euch", "mit", "Ver\u00b7gn\u00fc\u00b7gung", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ja da\u00df zum Uberflu\u00df an Zierath nichts gebricht/", "tokens": ["Ja", "da\u00df", "zum", "U\u00b7ber\u00b7flu\u00df", "an", "Zie\u00b7rath", "nichts", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "APPRART", "NN", "APPR", "NN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "(wo ihr nur anders wollt auch meinen Worten trauen/)", "tokens": ["(", "wo", "ihr", "nur", "an\u00b7ders", "wollt", "auch", "mei\u00b7nen", "Wor\u00b7ten", "trau\u00b7en", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Urtheilet unter euch der Farben Nacht und Licht.", "tokens": ["Ur\u00b7thei\u00b7let", "un\u00b7ter", "euch", "der", "Far\u00b7ben", "Nacht", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drauff schlo\u00df der Hertzens-Dieb/ als ihm das Frauen-Zimmer", "tokens": ["Drauff", "schlo\u00df", "der", "Hert\u00b7zens\u00b7Dieb", "/", "als", "ihm", "das", "Frau\u00b7en\u00b7Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Einhellig Antwort gab: er/ als der liebste Sohn/", "tokens": ["Ein\u00b7hel\u00b7lig", "Ant\u00b7wort", "gab", ":", "er", "/", "als", "der", "liebs\u00b7te", "Sohn", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$.", "PPER", "$(", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Verst\u00fcnde witziger der Farben Glantz und Schimmer/", "tokens": ["Ver\u00b7st\u00fcn\u00b7de", "wit\u00b7zi\u00b7ger", "der", "Far\u00b7ben", "Glantz", "und", "Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie hofften treuen Rath und Unterricht davon.", "tokens": ["Sie", "hoff\u00b7ten", "treu\u00b7en", "Rath", "und", "Un\u00b7ter\u00b7richt", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Purpur deckte nur der Venus Perlen-Glieder/", "tokens": ["Der", "Pur\u00b7pur", "deck\u00b7te", "nur", "der", "Ve\u00b7nus", "Per\u00b7len\u00b7Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie wolten Nimfen und nicht gleich den G\u00f6ttern seyn.", "tokens": ["Sie", "wol\u00b7ten", "Nim\u00b7fen", "und", "nicht", "gleich", "den", "G\u00f6t\u00b7tern", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "KON", "PTKNEG", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den Seiden-reichen Sammt beliebte nicht ein jeder/", "tokens": ["Den", "Sei\u00b7den\u00b7rei\u00b7chen", "Sammt", "be\u00b7lieb\u00b7te", "nicht", "ein", "je\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Offt geb\u2019 ein andrer Zeug den allerbesten Schein.", "tokens": ["Offt", "geb'", "ein", "an\u00b7drer", "Zeug", "den", "al\u00b7ler\u00b7bes\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wol/ sprach der Fl\u00fcgel-Sch\u00fctz/ ihr Blumen zarter Jahre/", "tokens": ["Wol", "/", "sprach", "der", "Fl\u00fc\u00b7gel\u00b7Sch\u00fctz", "/", "ihr", "Blu\u00b7men", "zar\u00b7ter", "Jah\u00b7re", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "ART", "NN", "$(", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.34": {"text": "So viel als Blumen in dem g\u00fcldnen Lentzen stehn/", "tokens": ["So", "viel", "als", "Blu\u00b7men", "in", "dem", "g\u00fcld\u00b7nen", "Lent\u00b7zen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So viel erwehlt euch von der Farben Art und Wahre/", "tokens": ["So", "viel", "er\u00b7wehlt", "euch", "von", "der", "Far\u00b7ben", "Art", "und", "Wah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jhr solt zu Tantz und Spiel gemahlt wie Bilder gehn.", "tokens": ["Ihr", "solt", "zu", "Tantz", "und", "Spiel", "ge\u00b7mahlt", "wie", "Bil\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVPP", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ach angenehmer Schlu\u00df! jedwede ward erfreuet;", "tokens": ["Ach", "an\u00b7ge\u00b7neh\u00b7mer", "Schlu\u00df", "!", "jed\u00b7we\u00b7de", "ward", "er\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PIS", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Liebens-werthe Volck hebt einen Wett-Streit an.", "tokens": ["Das", "Lie\u00b7bens\u00b7wert\u00b7he", "Volck", "hebt", "ei\u00b7nen", "Wet\u00b7tStreit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die kieset ihr ein Kleid mit Lilgen \u00fcberstreuet/", "tokens": ["Die", "kie\u00b7set", "ihr", "ein", "Kleid", "mit", "Lil\u00b7gen", "\u00fc\u00b7bers\u00b7treu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die einen Sommer-Rock der gleich dem Majoran/", "tokens": ["Die", "ei\u00b7nen", "Som\u00b7mer\u00b7Rock", "der", "gleich", "dem", "Ma\u00b7jo\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der hat das Mohren-Braun und jener Roth beliebet/", "tokens": ["Der", "hat", "das", "Moh\u00b7ren\u00b7Braun", "und", "je\u00b7ner", "Roth", "be\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "PDAT", "NN", "VVFI