{"dta.poem.11509": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von dem menschlichen elende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es schlie\u00dft der arme mensch, wenn er den lebens-lauff", "tokens": ["Es", "schlie\u00dft", "der", "ar\u00b7me", "mensch", ",", "wenn", "er", "den", "le\u00b7bens\u00b7lauff"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser welt beginnt, vielmehr zu bittrem weinen,", "tokens": ["In", "die\u00b7ser", "welt", "be\u00b7ginnt", ",", "viel\u00b7mehr", "zu", "bitt\u00b7rem", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "ADV", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als um das licht zu sehn, die m\u00fcden augen auf:", "tokens": ["Als", "um", "das", "licht", "zu", "sehn", ",", "die", "m\u00fc\u00b7den", "au\u00b7gen", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er f\u00fchlt die sclaverey alsbald an arm und beinen:", "tokens": ["Er", "f\u00fchlt", "die", "scla\u00b7ve\u00b7rey", "als\u00b7bald", "an", "arm", "und", "bei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die windeln fesseln ihn: Und wenn ihn dann die brust", "tokens": ["Die", "win\u00b7deln", "fes\u00b7seln", "ihn", ":", "Und", "wenn", "ihn", "dann", "die", "brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "KON", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der mutter nicht mehr s\u00e4ugt, so wird ihm seine lust", "tokens": ["Der", "mut\u00b7ter", "nicht", "mehr", "s\u00e4ugt", ",", "so", "wird", "ihm", "sei\u00b7ne", "lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit ruthen eingetr\u00e4nckt, durch strenge zucht verg\u00e4llet.", "tokens": ["Mit", "ru\u00b7then", "ein\u00b7ge\u00b7tr\u00e4nckt", ",", "durch", "stren\u00b7ge", "zucht", "ver\u00b7g\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat ihn die mannbarkeit auf freyen fu\u00df gestellet,", "tokens": ["Hat", "ihn", "die", "mann\u00b7bar\u00b7keit", "auf", "frey\u00b7en", "fu\u00df", "ge\u00b7stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So machen lieb und gl\u00fcck aus ihm ein gauckelspiel:", "tokens": ["So", "ma\u00b7chen", "lieb", "und", "gl\u00fcck", "aus", "ihm", "ein", "gau\u00b7ckel\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er mu\u00df sein brod im schwei\u00df und thr\u00e4nen essen:", "tokens": ["Er", "mu\u00df", "sein", "brod", "im", "schwei\u00df", "und", "thr\u00e4\u00b7nen", "es\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und offt erlangt er kaum so viel,", "tokens": ["Und", "offt", "er\u00b7langt", "er", "kaum", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn kranckheit, feind und noth ihn auf das sch\u00e4rffste pressen.", "tokens": ["Wenn", "kran\u00b7ck\u00b7heit", ",", "feind", "und", "noth", "ihn", "auf", "das", "sch\u00e4rffs\u00b7te", "pres\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Bricht dann zuletzt das alter ein,", "tokens": ["Bricht", "dann", "zu\u00b7letzt", "das", "al\u00b7ter", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So st\u00fctzet er mit einem morschen stabe", "tokens": ["So", "st\u00fct\u00b7zet", "er", "mit", "ei\u00b7nem", "mor\u00b7schen", "sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Den abgezehrten leib, und hincket so zu seinem grabe:", "tokens": ["Den", "ab\u00b7ge\u00b7zehr\u00b7ten", "leib", ",", "und", "hin\u00b7cket", "so", "zu", "sei\u00b7nem", "gra\u00b7be", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Drauf deckt den rest ein schmaler stein.", "tokens": ["Drauf", "deckt", "den", "rest", "ein", "schma\u00b7ler", "stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Was ist nun, armer mensch! dein trost und dein vergn\u00fcgen?", "tokens": ["Was", "ist", "nun", ",", "ar\u00b7mer", "mensch", "!", "dein", "trost", "und", "dein", "ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "ADJA", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Nichts, als nur di\u00df allein,", "tokens": ["Nichts", ",", "als", "nur", "di\u00df", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "PDS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Da\u00df wieg und grab nah bey einander liegen.", "tokens": ["Da\u00df", "wieg", "und", "grab", "nah", "bey", "ein\u00b7an\u00b7der", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADV", "ADJD", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}