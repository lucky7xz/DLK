{"dta.poem.4167": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An  \n den Herrn  \n Rahts-Herrn Brockes  \n \u00fcber  \n  sein irdisches Vergn\u00fcgen  \n in GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ogeist! der unsern Geist mit s\u00fcsser Macht be-", "tokens": ["O\u00b7geist", "!", "der", "un\u00b7sern", "Geist", "mit", "s\u00fcs\u00b7ser", "Macht", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zwingt,", "tokens": ["zwingt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Lust und Grauen schafft, vergn\u00fcget und erschrecket;", "tokens": ["Der", "Lust", "und", "Grau\u00b7en", "schafft", ",", "ver\u00b7gn\u00fc\u00b7get", "und", "er\u00b7schre\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der etwas G\u00f6ttliches in unsrer Brust erwecket,", "tokens": ["Der", "et\u00b7was", "G\u00f6tt\u00b7li\u00b7ches", "in", "uns\u00b7rer", "Brust", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sein unsterblich Lied der Gottheit Ruhm besingt.", "tokens": ["Wenn", "sein", "uns\u00b7terb\u00b7lich", "Lied", "der", "Got\u00b7theit", "Ruhm", "be\u00b7singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJD", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du zeigst der Wunder Heer, das unsern Kreis umringt.", "tokens": ["Du", "zeigst", "der", "Wun\u00b7der", "Heer", ",", "das", "un\u00b7sern", "Kreis", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blindheit hatte lang den Sterblichen verstecket,", "tokens": ["Die", "Blind\u00b7heit", "hat\u00b7te", "lang", "den", "Sterb\u00b7li\u00b7chen", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was nun der Sch\u00f6pfer Dir, und Du der Welt, entdecket,", "tokens": ["Was", "nun", "der", "Sch\u00f6p\u00b7fer", "Dir", ",", "und", "Du", "der", "Welt", ",", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PPER", "$,", "KON", "PPER", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was uns alhier bereits des Himmels Vorschmack bringt.", "tokens": ["Was", "uns", "al\u00b7hier", "be\u00b7reits", "des", "Him\u00b7mels", "Vor\u00b7schmack", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohlan, erhabner Brocks, Du hast ein Werk voll-", "tokens": ["Wo\u00b7hlan", ",", "er\u00b7hab\u00b7ner", "Brocks", ",", "Du", "hast", "ein", "Werk", "voll"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "f\u00fchret,", "tokens": ["f\u00fch\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das hier der Erde Kreis und dort die Engel r\u00fchret,", "tokens": ["Das", "hier", "der", "Er\u00b7de", "Kreis", "und", "dort", "die", "En\u00b7gel", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das schon die Seligen an Deinen Schall gew\u00f6hnt.", "tokens": ["Das", "schon", "die", "Se\u00b7li\u00b7gen", "an", "Dei\u00b7nen", "Schall", "ge\u00b7w\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geneu\u00df der Sch\u00e4tze lang, die Gott uns hier geschenket,", "tokens": ["Ge\u00b7neu\u00df", "der", "Sch\u00e4t\u00b7ze", "lang", ",", "die", "Gott", "uns", "hier", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis, wenn Dein greises Haupt zur Ruhe sich gesenket,", "tokens": ["Bis", ",", "wenn", "Dein", "grei\u00b7ses", "Haupt", "zur", "Ru\u00b7he", "sich", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ewigs Lied von Dir in Davids Harfe t\u00f6nt.", "tokens": ["Ein", "e\u00b7wigs", "Lied", "von", "Dir", "in", "Da\u00b7vids", "Har\u00b7fe", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}