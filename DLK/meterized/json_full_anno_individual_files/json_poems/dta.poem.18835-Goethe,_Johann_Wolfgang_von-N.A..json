{"dta.poem.18835": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die Perle die der Muschel entrann,", "tokens": ["Die", "Per\u00b7le", "die", "der", "Mu\u00b7schel", "ent\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sch\u00f6nste, hochgeboren,", "tokens": ["Die", "sch\u00f6ns\u00b7te", ",", "hoch\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zum Juwelier, dem guten Mann,", "tokens": ["Zum", "Ju\u00b7we\u00b7lier", ",", "dem", "gu\u00b7ten", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprach sie: ich bin verloren!", "tokens": ["Sprach", "sie", ":", "ich", "bin", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchbohrst du mich, mein sch\u00f6nes All", "tokens": ["Durch\u00b7bohrst", "du", "mich", ",", "mein", "sch\u00f6\u00b7nes", "All"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist sogleich zerr\u00fcttet,", "tokens": ["Es", "ist", "sog\u00b7leich", "zer\u00b7r\u00fct\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Schwestern muss ich, Fall f\u00fcr Fall,", "tokens": ["Mit", "Schwes\u00b7tern", "muss", "ich", ",", "Fall", "f\u00fcr", "Fall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu schlechten seyn gek\u00fcttet.", "tokens": ["Zu", "schlech\u00b7ten", "seyn", "ge\u00b7k\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eich denke jetzt nur an Gewinn,", "tokens": ["\u201e", "ich", "den\u00b7ke", "jetzt", "nur", "an", "Ge\u00b7winn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du musst es mir verzeihen:", "tokens": ["Du", "musst", "es", "mir", "ver\u00b7zei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn wenn ich hier nicht grausam bin,", "tokens": ["Denn", "wenn", "ich", "hier", "nicht", "grau\u00b7sam", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie soll die Schnur sich reihen?\u201c", "tokens": ["Wie", "soll", "die", "Schnur", "sich", "rei\u00b7hen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}