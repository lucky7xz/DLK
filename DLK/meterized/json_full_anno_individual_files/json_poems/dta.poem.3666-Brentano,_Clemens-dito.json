{"dta.poem.3666": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "dito", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heiderlau!               ", "tokens": ["Hei\u00b7der\u00b7lau", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Stirbt meine Frau,", "tokens": ["Stirbt", "mei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Reis' ich in die Wetterau,", "tokens": ["Reis'", "ich", "in", "die", "Wet\u00b7te\u00b7rau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hol mir eine andre,", "tokens": ["Hol", "mir", "ei\u00b7ne", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die soll seyn,", "tokens": ["Die", "soll", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "H\u00fcbsch und fein,", "tokens": ["H\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sch\u00f6ner als die andre.", "tokens": ["Sch\u00f6\u00b7ner", "als", "die", "and\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "A Herr verscho o o ne mich,", "tokens": ["A", "Herr", "ver\u00b7scho", "o", "o", "ne", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "FM", "FM", "FM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jesus Maria a a,", "tokens": ["Je\u00b7sus", "Ma\u00b7ria", "a", "a", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist dann kein Kavallerie mehr da,", "tokens": ["Ist", "dann", "kein", "Ka\u00b7val\u00b7le\u00b7rie", "mehr", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Jesus Marie,", "tokens": ["Je\u00b7sus", "Ma\u00b7rie", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo bleibt dann die Infanterie,", "tokens": ["Wo", "bleibt", "dann", "die", "In\u00b7fan\u00b7te\u00b7rie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00e4tten wir dies,", "tokens": ["H\u00e4t\u00b7ten", "wir", "dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "H\u00e4tten wir das,", "tokens": ["H\u00e4t\u00b7ten", "wir", "das", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "H\u00e4tten wir Heu,", "tokens": ["H\u00e4t\u00b7ten", "wir", "Heu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "H\u00e4tten wir Gras,", "tokens": ["H\u00e4t\u00b7ten", "wir", "Gras", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "So haben wir aber nichts als diese", "tokens": ["So", "ha\u00b7ben", "wir", "a\u00b7ber", "nichts", "als", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "KOKOM", "PDS"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Alte, alte, alte Schindm\u00e4hrerere.", "tokens": ["Al\u00b7te", ",", "al\u00b7te", ",", "al\u00b7te", "Schind\u00b7m\u00e4h\u00b7re\u00b7re\u00b7re", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}