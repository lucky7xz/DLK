{"textgrid.poem.43204": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im D\u00e4mmerlichte schwamm die Barke fort", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7lich\u00b7te", "schwamm", "die", "Bar\u00b7ke", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch den Canal und hin durch die Lagunen,", "tokens": ["Durch", "den", "Ca\u00b7nal", "und", "hin", "durch", "die", "La\u00b7gu\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der todten Zeit geheimni\u00dfvolle Runen", "tokens": ["Der", "tod\u00b7ten", "Zeit", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "Ru\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erstickten schier das laute Menschenwort.", "tokens": ["Er\u00b7stick\u00b7ten", "schier", "das", "lau\u00b7te", "Men\u00b7schen\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es glitt an Tempeln und Pal\u00e4sten nur", "tokens": ["Es", "glitt", "an", "Tem\u00b7peln", "und", "Pa\u00b7l\u00e4s\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.p