{"textgrid.poem.44557": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Feldmarschall Radetzky", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck auf, mein Feldherr, f\u00fchre den Streich!", "tokens": ["Gl\u00fcck", "auf", ",", "mein", "Feld\u00b7herr", ",", "f\u00fch\u00b7re", "den", "Streich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht blo\u00df um des Ruhmes Schimmer,", "tokens": ["Nicht", "blo\u00df", "um", "des", "Ruh\u00b7mes", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deinem Lager ist \u00d6sterreich,", "tokens": ["In", "dei\u00b7nem", "La\u00b7ger", "ist", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir andern sind einzelne Tr\u00fcmmer.", "tokens": ["Wir", "an\u00b7dern", "sind", "ein\u00b7zel\u00b7ne", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Aus Torheit und aus Eitelkeit", "tokens": ["Aus", "Tor\u00b7heit", "und", "aus", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind wir in uns zerfallen,", "tokens": ["Sind", "wir", "in", "uns", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In denen, die du f\u00fchrst zum Streit,", "tokens": ["In", "de\u00b7nen", ",", "die", "du", "f\u00fchrst", "zum", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lebt noch ", "tokens": ["Lebt", "noch"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Dort ist kein J\u00fcngling, der sich vermi\u00dft,", "tokens": ["Dort", "ist", "kein", "J\u00fcng\u00b7ling", ",", "der", "sich", "ver\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es besser als du zu kennen,", "tokens": ["Es", "bes\u00b7ser", "als", "du", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der, was er tr\u00e4umt und nirgends ist,", "tokens": ["Der", ",", "was", "er", "tr\u00e4umt", "und", "nir\u00b7gends", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VVFIN", "KON", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Weisheit wagt zu benennen.", "tokens": ["Als", "Weis\u00b7heit", "wagt", "zu", "be\u00b7nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Und deine Garde, die nicht nur wacht,", "tokens": ["Und", "dei\u00b7ne", "Gar\u00b7de", ",", "die", "nicht", "nur", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nein auch bewacht und beschirmet,", "tokens": ["Nein", "auch", "be\u00b7wacht", "und", "be\u00b7schir\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVPP", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sie hat nicht der eigenen Sicherheit acht,", "tokens": ["Sie", "hat", "nicht", "der", "ei\u00b7ge\u00b7nen", "Si\u00b7cher\u00b7heit", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "CARD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn nachts die Trommel st\u00fcrmet.", "tokens": ["Wenn", "nachts", "die", "Trom\u00b7mel", "st\u00fcr\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der B\u00fcrger deiner wandernden Stadt,", "tokens": ["Der", "B\u00fcr\u00b7ger", "dei\u00b7ner", "wan\u00b7dern\u00b7den", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er wei\u00df, diese Stadt ist sein alles,", "tokens": ["Er", "wei\u00df", ",", "die\u00b7se", "Stadt", "ist", "sein", "al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDAT", "NN", "VAFIN", "PPOSAT", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die, wenn sie die Flamme ergriffen hat,", "tokens": ["Die", ",", "wenn", "sie", "die", "Flam\u00b7me", "er\u00b7grif\u00b7fen", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ihn mitzieht zum Abgrund des Falles.", "tokens": ["Ihn", "mit\u00b7zieht", "zum", "Ab\u00b7grund", "des", "Fal\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und deine Minister, die F\u00fchrer im Heer,", "tokens": ["Und", "dei\u00b7ne", "Mi\u00b7nis\u00b7ter", ",", "die", "F\u00fch\u00b7rer", "im", "Heer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie f\u00fchren das Schwert an der Seite,", "tokens": ["Sie", "f\u00fch\u00b7ren", "das", "Schwert", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Zu strafen, wenns irgend n\u00f6tig w\u00e4r,", "tokens": ["Zu", "stra\u00b7fen", ",", "wenns", "ir\u00b7gend", "n\u00f6\u00b7tig", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gehorsam ist Frieden im Streite.", "tokens": ["Ge\u00b7hor\u00b7sam", "ist", "Frie\u00b7den", "im", "Strei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Die Gott als Slav und Magyaren schuf,", "tokens": ["Die", "Gott", "als", "Slav", "und", "Ma\u00b7gy\u00b7a\u00b7ren", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie streiten um Worte nicht h\u00e4misch,", "tokens": ["Sie", "strei\u00b7ten", "um", "Wor\u00b7te", "nicht", "h\u00e4\u00b7misch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie folgen, ob deutsch auch der Feldherrnruf,", "tokens": ["Sie", "fol\u00b7gen", ",", "ob", "deutsch", "auch", "der", "Feld\u00b7herrn\u00b7ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn: Vorw\u00e4rts! ist ungrisch und b\u00f6hmisch.", "tokens": ["Denn", ":", "Vor\u00b7w\u00e4rts", "!", "ist", "un\u00b7grisch", "und", "b\u00f6h\u00b7misch", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "ADV", "$.", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Gemeinsame Hilf in gemeinsamer Not", "tokens": ["Ge\u00b7mein\u00b7sa\u00b7me", "Hilf", "in", "ge\u00b7mein\u00b7sa\u00b7mer", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hat Reiche und Staaten gegr\u00fcndet,", "tokens": ["Hat", "Rei\u00b7che", "und", "Staa\u00b7ten", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Mensch ist ein einsamer nur im Tod,", "tokens": ["Der", "Mensch", "ist", "ein", "ein\u00b7sa\u00b7mer", "nur", "im", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch Leben und Streben verb\u00fcndet.", "tokens": ["Doch", "Le\u00b7ben", "und", "Stre\u00b7ben", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "W\u00e4r uns ein Beispiel dein ruhmvoller Krieg,", "tokens": ["W\u00e4r", "uns", "ein", "Bei\u00b7spiel", "dein", "ruhm\u00b7vol\u00b7ler", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir reichten uns freudig die H\u00e4nde.", "tokens": ["Wir", "reich\u00b7ten", "uns", "freu\u00b7dig", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Im Anschlu\u00df von allen liegt der Sieg,", "tokens": ["Im", "An\u00b7schlu\u00df", "von", "al\u00b7len", "liegt", "der", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Gl\u00fcck eines jeden das Ende.", "tokens": ["Im", "Gl\u00fcck", "ei\u00b7nes", "je\u00b7den", "das", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PIAT", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Gl\u00fcck auf, mein Feldherr, f\u00fchre den Streich!", "tokens": ["Gl\u00fcck", "auf", ",", "mein", "Feld\u00b7herr", ",", "f\u00fch\u00b7re", "den", "Streich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nicht blo\u00df um des Ruhmes Schimmer,", "tokens": ["Nicht", "blo\u00df", "um", "des", "Ruh\u00b7mes", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deinem Lager ist \u00d6sterreich,", "tokens": ["In", "dei\u00b7nem", "La\u00b7ger", "ist", "\u00d6s\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir andern sind einzelne Tr\u00fcmmer.", "tokens": ["Wir", "