a"}, "line.6": {"text": "Keiner doch wagt's, mir zu nahen,", "tokens": ["Kei\u00b7ner", "doch", "wagt's", ",", "mir", "zu", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "$,", "PPER", "APPR", "ADJA", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Denn ich bin die Tochter Frankreichs,", "tokens": ["Denn", "ich", "bin", "die", "Toch\u00b7ter", "Fran\u00b7kreichs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und der K\u00f6nig ist mein Vater,", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "ist", "mein", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wer meinen Leib ber\u00fchrte,", "tokens": ["Und", "wer", "mei\u00b7nen", "Leib", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "M\u00fc\u00dft's mit seinem Kopf bezahlen.\u00ab", "tokens": ["M\u00fc\u00dft's", "mit", "sei\u00b7nem", "Kopf", "be\u00b7zah\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}