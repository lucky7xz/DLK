"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Andre Wandrer abzuwarten.", "tokens": ["And\u00b7re", "Wand\u00b7rer", "ab\u00b7zu\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kam ein Ritter da geritten,", "tokens": ["Kam", "ein", "Rit\u00b7ter", "da", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleichfalls gen Paris er trabte.", "tokens": ["Gleich\u00b7falls", "gen", "Pa\u00b7ris", "er", "trab\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwenn es Euch beliebt, Herr Ritter,", "tokens": ["\u00bb", "wenn", "es", "Euch", "be\u00b7liebt", ",", "Herr", "Rit\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADJD", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt mich mit aus diesem Walde. \u2013\u00ab", "tokens": ["Nehmt", "mich", "mit", "aus", "die\u00b7sem", "Wal\u00b7de", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "PDAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbherzlich gerne, sch\u00f6ne Herrin!\u00ab", "tokens": ["\u00bb", "herz\u00b7lich", "ger\u00b7ne", ",", "sch\u00f6\u00b7ne", "Her\u00b7rin", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADV", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und, ihr h\u00f6flich aufzuwarten,", "tokens": ["Und", ",", "ihr", "h\u00f6f\u00b7lich", "auf\u00b7zu\u00b7war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sprang der Ritter von dem Rosse,", "tokens": ["Sprang", "der", "Rit\u00b7ter", "von", "dem", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hob hinauf sie, in den Sattel", "tokens": ["Hob", "hin\u00b7auf", "sie", ",", "in", "den", "Sat\u00b7tel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drauf sich selber zu ihr schwingend.", "tokens": ["Drauf", "sich", "sel\u00b7ber", "zu", "ihr", "schwin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ADV", "APPR", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber als sie so im Walde", "tokens": ["A\u00b7ber", "als", "sie", "so", "im", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsam ritten, da begann er", "tokens": ["Ein\u00b7sam", "rit\u00b7ten", ",", "da", "be\u00b7gann", "er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr verliebt den Hof zu machen.", "tokens": ["Ihr", "ver\u00b7liebt", "den", "Hof", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbh\u00fct dich, Ritter, sei nicht sch\u00e4ndlich,", "tokens": ["\u00bb", "h\u00fct", "dich", ",", "Rit\u00b7ter", ",", "sei", "nicht", "sch\u00e4nd\u00b7lich", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein Todkranker war mein Vater", "tokens": ["Ein", "Tod\u00b7kran\u00b7ker", "war", "mein", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und verpestet meine Mutter,", "tokens": ["Und", "ver\u00b7pes\u00b7tet", "mei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Siech und elend m\u00fc\u00dft verschmachten,", "tokens": ["Siech", "und", "e\u00b7lend", "m\u00fc\u00dft", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wer mich frevelhaft ber\u00fchrte. \u2013\u00ab", "tokens": ["Wer", "mich", "fre\u00b7vel\u00b7haft", "be\u00b7r\u00fchr\u00b7te", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Ritter schwieg erblassend.", "tokens": ["Und", "der", "Rit\u00b7ter", "schwieg", "er\u00b7blas\u00b7send", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber in Paris am Tore", "tokens": ["A\u00b7ber", "in", "Pa\u00b7ris", "am", "To\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Still in sich die Jungfrau lachte.", "tokens": ["Still", "in", "sich", "die", "Jung\u00b7frau", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "\u00bbwarum lacht Ihr, sch\u00f6ne Herrin?\u00ab \u2013", "tokens": ["\u00bb", "wa\u00b7rum", "lacht", "Ihr", ",", "sch\u00f6\u00b7ne", "Her\u00b7rin", "?", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "\u00bb\u00fcber den feigen Ritter lach ich,", "tokens": ["\u00bb", "\u00fc\u00b7ber", "den", "fei\u00b7gen", "Rit\u00b7ter", "lach", "ich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Der sein M\u00e4dchen hat im Freien", "tokens": ["Der", "sein", "M\u00e4d\u00b7chen", "hat", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und nichts macht als 