{"textgrid.poem.37677": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4gersmann", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht schie\u00dfen soll der J\u00e4gersmann,", "tokens": ["Nicht", "schie\u00b7\u00dfen", "soll", "der", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er soll das Wild nun schonen,", "tokens": ["Er", "soll", "das", "Wild", "nun", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er giebt es St\u00fcck f\u00fcr St\u00fcck euch an,", "tokens": ["Er", "giebt", "es", "St\u00fcck", "f\u00fcr", "St\u00fcck", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel im Walde wohnen.", "tokens": ["Wie", "viel", "im", "Wal\u00b7de", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor niemand scheuet sich das Wild,", "tokens": ["Vor", "nie\u00b7mand", "scheu\u00b7et", "sich", "das", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich gegen Bauern wehret,", "tokens": ["Sich", "ge\u00b7gen", "Bau\u00b7ern", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der zahme Bauer leise schilt,", "tokens": ["Der", "zah\u00b7me", "Bau\u00b7er", "lei\u00b7se", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es die Saat verzehret.", "tokens": ["Wenn", "es", "die", "Saat", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verzehrt die Saat kein Hochgewild,", "tokens": ["Ver\u00b7zehrt", "die", "Saat", "kein", "Hoch\u00b7ge\u00b7wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Jagdro\u00df tritt sie nieder,", "tokens": ["Das", "Jag\u00b7dro\u00df", "tritt", "sie", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der F\u00fcrst, der wenn es Hetzen gilt", "tokens": ["Der", "F\u00fcrst", ",", "der", "wenn", "es", "Het\u00b7zen", "gilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht schont die eignen Glieder.", "tokens": ["Nicht", "schont", "die", "eig\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und ist der Hirsch so lendenlahm", "tokens": ["Und", "ist", "der", "Hirsch", "so", "len\u00b7den\u00b7lahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie die armen Bauern,", "tokens": ["Als", "wie", "die", "ar\u00b7men", "Bau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sieht ihn weinen eine Dam'", "tokens": ["Da", "sieht", "ihn", "wei\u00b7nen", "ei\u00b7ne", "Dam'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df ihn sehr bedauern.", "tokens": ["Und", "mu\u00df", "ihn", "sehr", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Leben wird ihm dann geschenkt,", "tokens": ["Das", "Le\u00b7ben", "wird", "ihm", "dann", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die weiche Dam' gepriesen,", "tokens": ["Die", "wei\u00b7che", "Dam'", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Bauer endlich todt ihn denkt,", "tokens": ["Der", "Bau\u00b7er", "end\u00b7lich", "todt", "ihn", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht wieder ihn auf Wiesen.", "tokens": ["Sieht", "wie\u00b7der", "ihn", "auf", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sch\u00f6\u00df' ihn gern, hat kein Gewehr,", "tokens": ["Er", "sch\u00f6\u00df'", "ihn", "gern", ",", "hat", "kein", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der F\u00fcrst hat es genommen,", "tokens": ["Der", "F\u00fcrst", "hat", "es", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf sich genommen aller Ehr',", "tokens": ["Auf", "sich", "ge\u00b7nom\u00b7men", "al\u00b7ler", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so ist sie verkommen.", "tokens": ["Und", "so", "ist", "sie", "ver\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er denkt, er hat ein gro\u00dfes Heer,", "tokens": ["Er", "denkt", ",", "er", "hat", "ein", "gro\u00b7\u00dfes", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wenn einst Feinde kommen,", "tokens": ["Doch", "wenn", "einst", "Fein\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schie\u00dfen in den Wind so leer,", "tokens": ["Sie", "schie\u00b7\u00dfen", "in", "den", "Wind", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schu\u00df macht sie beklommen.", "tokens": ["Der", "Schu\u00df", "macht", "sie", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der J\u00e4ger hat darum nicht Rast,", "tokens": ["Der", "J\u00e4\u00b7ger", "hat", "da\u00b7rum", "nicht", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Nachts den Wald durchkrochen,", "tokens": ["Hat", "Nachts", "den", "Wald", "durch\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er den Dieb gleich find' und fa\u00dft,", "tokens": ["Da\u00df", "er", "den", "Dieb", "gleich", "find'", "und", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Holz im Wald gebrochen.", "tokens": ["Der", "Holz", "im", "Wald", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Luft ist d\u00fcnn und schallet kaum,", "tokens": ["Die", "Luft", "ist", "d\u00fcnn", "und", "schal\u00b7let", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Sternen ist ein Flimmern,", "tokens": ["An", "Ster\u00b7nen", "ist", "ein", "Flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als fielen sie, der Erde Saum", "tokens": ["Als", "fie\u00b7len", "sie", ",", "der", "Er\u00b7de", "Saum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist roth und R\u00e4dlein schimmern.", "tokens": ["Ist", "roth", "und", "R\u00e4d\u00b7lein", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es borst der Baum wohl in dem Forst,", "tokens": ["Es", "borst", "der", "Baum", "wohl", "in", "dem", "Forst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df J\u00e4ger, la\u00df dein Schleichen,", "tokens": ["La\u00df", "J\u00e4\u00b7ger", ",", "la\u00df", "dein", "Schlei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Noth nur suchet da sich Trost,", "tokens": ["Die", "Noth", "nur", "su\u00b7chet", "da", "sich", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Wild mu\u00df heulend weichen.", "tokens": ["Wo", "Wild", "mu\u00df", "heu\u00b7lend", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein armes altes Weib er sah,", "tokens": ["Ein", "ar\u00b7mes", "al\u00b7tes", "Weib", "er", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kriecht mit Reis vor\u00fcber,", "tokens": ["Sie", "kriecht", "mit", "Reis", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Frost ist ihm dem Marke nah,", "tokens": ["Der", "Frost", "ist", "ihm", "dem", "Mar\u00b7ke", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schl\u00e4gt die H\u00e4nd' quer\u00fcber.", "tokens": ["Er", "schl\u00e4gt", "die", "H\u00e4nd'", "quer\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nun nimmt er ab, als er nun warm,", "tokens": ["Nun", "nimmt", "er", "ab", ",", "als", "er", "nun", "warm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das B\u00fcndlein Reis mit T\u00fccken,", "tokens": ["Das", "B\u00fcnd\u00b7lein", "Reis", "mit", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gt sie noch, da\u00df Gott erbarm'", "tokens": ["Und", "schl\u00e4gt", "sie", "noch", ",", "da\u00df", "Gott", "er\u00b7barm'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar schm\u00e4hlig auf den R\u00fccken.", "tokens": ["Gar", "schm\u00e4h\u00b7lig", "auf", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sie flucht: \u00bbSo werde nimmer warm,", "tokens": ["Sie", "flucht", ":", "\u00bb", "So", "wer\u00b7de", "nim\u00b7mer", "warm", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ew'ger Frost dich k\u00e4lte,", "tokens": ["Ein", "ew'\u00b7ger", "Frost", "dich", "k\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nicht alt, ich schein' nur arm,", "tokens": ["Ich", "bin", "nicht", "alt", ",", "ich", "schein'", "nur", "arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so das Feuer gelte.\u00ab", "tokens": ["Und", "so", "das", "Feu\u00b7er", "gel\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Er kehrt nach Hause zum Kamin,", "tokens": ["Er", "kehrt", "nach", "Hau\u00b7se", "zum", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reiches Feuer findet,", "tokens": ["Ein", "rei\u00b7ches", "Feu\u00b7er", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbrennt den B\u00fcndel noch darin,", "tokens": ["Ver\u00b7brennt", "den", "B\u00fcn\u00b7del", "noch", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fluch das Feuer bindet.", "tokens": ["Der", "Fluch", "das", "Feu\u00b7er", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Flammen wenden sich von ihm", "tokens": ["Die", "Flam\u00b7men", "wen\u00b7den", "sich", "von", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie von dem wilden Winde,", "tokens": ["Wie", "von", "dem", "wil\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie brennen ihn, sie schmerzen ihm,", "tokens": ["Sie", "bren\u00b7nen", "ihn", ",", "sie", "schmer\u00b7zen", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00e4lten ihn geschwinde.", "tokens": ["Und", "k\u00e4l\u00b7ten", "ihn", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er starrt, die Sonn' beeiset ihn,", "tokens": ["Er", "starrt", ",", "die", "Sonn'", "be\u00b7ei\u00b7set", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Federn ihn beschneien,", "tokens": ["Die", "Fe\u00b7dern", "ihn", "be\u00b7schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der rasche Tanz durchzittert ihn,", "tokens": ["Der", "ra\u00b7sche", "Tanz", "durch\u00b7zit\u00b7tert", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Weib mu\u00df bei ihm weinen.", "tokens": ["Sein", "Weib", "mu\u00df", "bei", "ihm", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Der Kirche Anblick macht ihn kalt,", "tokens": ["Der", "Kir\u00b7che", "An\u00b7blick", "macht", "ihn", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Pred'gers Wort durchschauert,", "tokens": ["Des", "Pre\u00b7d'\u00b7gers", "Wort", "durch\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "So ward er jung schon wei\u00df und alt,", "tokens": ["So", "ward", "er", "jung", "schon", "wei\u00df", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem dann betrauert.", "tokens": ["Von", "kei\u00b7nem", "dann", "be\u00b7trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Sklaven machen den Tyrann,", "tokens": ["Die", "Skla\u00b7ven", "ma\u00b7chen", "den", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kann nicht Sklaven schaffen,", "tokens": ["Er", "kann", "nicht", "Skla\u00b7ven", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo jeder B\u00fcrger fest als Mann,", "tokens": ["Wo", "je\u00b7der", "B\u00fcr\u00b7ger", "fest", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer soll da einen strafen.", "tokens": ["Wer", "soll", "da", "ei\u00b7nen", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Da ist der Arme nimmermehr", "tokens": ["Da", "ist", "der", "Ar\u00b7me", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie S\u00fcnder eingefangen,", "tokens": ["Wie", "S\u00fcn\u00b7der", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist des reichen B\u00fcrgers Ehr',", "tokens": ["Es", "ist", "des", "rei\u00b7chen", "B\u00fcr\u00b7gers", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gastlich zu empfangen.", "tokens": ["Sie", "gast\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Nicht schie\u00dfen soll der J\u00e4gersmann,", "tokens": ["Nicht", "schie\u00b7\u00dfen", "soll", "der", "J\u00e4\u00b7gers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er soll das Wild nun schonen,", "tokens": ["Er", "soll", "das", "Wild", "nun", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er giebt es St\u00fcck f\u00fcr St\u00fcck euch an,", "tokens": ["Er", "giebt", "es", "St\u00fcck", "f\u00fcr", "St\u00fcck", "euch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel im Walde wohnen.", "tokens": ["Wie", "viel", "im", "Wal\u00b7de", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Vor niemand scheuet sich das Wild,", "tokens": ["Vor", "nie\u00b7mand", "scheu\u00b7et", "sich", "das", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich gegen Bauern wehret,", "tokens": ["Sich", "ge\u00b7gen", "Bau\u00b7ern", "weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der zahme Bauer leise schilt,", "tokens": ["Der", "zah\u00b7me", "Bau\u00b7er", "lei\u00b7se", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es die Saat verzehret.", "tokens": ["Wenn", "es", "die", "Saat", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Verzehrt die Saat kein Hochgewild,", "tokens": ["Ver\u00b7zehrt", "die", "Saat", "kein", "Hoch\u00b7ge\u00b7wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Jagdro\u00df tritt sie nieder,", "tokens": ["Das", "Jag\u00b7dro\u00df", "tritt", "sie", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der F\u00fcrst, der wenn es Hetzen gilt", "tokens": ["Der", "F\u00fcrst", ",", "der", "wenn", "es", "Het\u00b7zen", "gilt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht schont die eignen Glieder.", "tokens": ["Nicht", "schont", "die", "eig\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Und ist der Hirsch so lendenlahm", "tokens": ["Und", "ist", "der", "Hirsch", "so", "len\u00b7den\u00b7lahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wie die armen Bauern,", "tokens": ["Als", "wie", "die", "ar\u00b7men", "Bau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sieht ihn weinen eine Dam'", "tokens": ["Da", "sieht", "ihn", "wei\u00b7nen", "ei\u00b7ne", "Dam'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df ihn sehr bedauern.", "tokens": ["Und", "mu\u00df", "ihn", "sehr", "be\u00b7dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Das Leben wird ihm dann geschenkt,", "tokens": ["Das", "Le\u00b7ben", "wird", "ihm", "dann", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die weiche Dam' gepriesen,", "tokens": ["Die", "wei\u00b7che", "Dam'", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Bauer endlich todt ihn denkt,", "tokens": ["Der", "Bau\u00b7er", "end\u00b7lich", "todt", "ihn", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht wieder ihn auf Wiesen.", "tokens": ["Sieht", "wie\u00b7der", "ihn", "auf", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Er sch\u00f6\u00df' ihn gern, hat kein Gewehr,", "tokens": ["Er", "sch\u00f6\u00df'", "ihn", "gern", ",", "hat", "kein", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der F\u00fcrst hat es genommen,", "tokens": ["Der", "F\u00fcrst", "hat", "es", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf sich genommen aller Ehr',", "tokens": ["Auf", "sich", "ge\u00b7nom\u00b7men", "al\u00b7ler", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so ist sie verkommen.", "tokens": ["Und", "so", "ist", "sie", "ver\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Er denkt, er hat ein gro\u00dfes Heer,", "tokens": ["Er", "denkt", ",", "er", "hat", "ein", "gro\u00b7\u00dfes", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wenn einst Feinde kommen,", "tokens": ["Doch", "wenn", "einst", "Fein\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schie\u00dfen in den Wind so leer,", "tokens": ["Sie", "schie\u00b7\u00dfen", "in", "den", "Wind", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schu\u00df macht sie beklommen.", "tokens": ["Der", "Schu\u00df", "macht", "sie", "be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Der J\u00e4ger hat darum nicht Rast,", "tokens": ["Der", "J\u00e4\u00b7ger", "hat", "da\u00b7rum", "nicht", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat Nachts den Wald durchkrochen,", "tokens": ["Hat", "Nachts", "den", "Wald", "durch\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er den Dieb gleich find' und fa\u00dft,", "tokens": ["Da\u00df", "er", "den", "Dieb", "gleich", "find'", "und", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Holz im Wald gebrochen.", "tokens": ["Der", "Holz", "im", "Wald", "ge\u00b7bro\u0