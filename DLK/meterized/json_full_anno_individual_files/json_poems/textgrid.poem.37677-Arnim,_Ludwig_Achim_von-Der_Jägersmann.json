ct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Noth nur suchet da sich Trost,", "tokens": ["Die", "Noth", "nur", "su\u00b7chet", "da", "sich", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Wild mu\u00df heulend weichen.", "tokens": ["Wo", "Wild", "mu\u00df", "heu\u00b7lend", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Ein armes altes Weib er sah,", "tokens": ["Ein", "ar\u00b7mes", "al\u00b7tes", "Weib", "er", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kriecht mit Reis vor\u00fcber,", "tokens": ["Sie", "kriecht", "mit", "Reis", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Frost ist ihm dem Marke nah,", "tokens": ["Der", "Frost", "ist", "ihm", "dem", "Mar\u00b7ke", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schl\u00e4gt die H\u00e4nd' quer\u00fcber.", "tokens": ["Er", "schl\u00e4gt", "die", "H\u00e4nd'", "quer\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.31": {"line.1": {"text": "Nun nimmt er ab, als er nun warm,", "tokens": ["Nun", "nimmt", "er", "ab", ",", "als", "er", "nun", "warm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das B\u00fcndlein Reis mit T\u00fccken,", "tokens": ["Das", "B\u00fcnd\u00b7lein", "Reis", "mit", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gt sie noch, da\u00df Gott erbarm'", "tokens": ["Und", "schl\u00e4gt", "sie", "noch", ",", "da\u00df", "Gott", "er\u00b7barm'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar schm\u00e4hlig auf den R\u00fccken.", "tokens": ["Gar", "schm\u00e4h\u00b7lig", "auf", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Sie flucht: \u00bbSo werde nimmer warm,", "tokens": ["Sie", "flucht", ":", "\u00bb", "So", "wer\u00b7de", "nim\u00b7mer", "warm", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ew'ger Frost dich k\u00e4lte,", "tokens": ["Ein", "ew'\u00b7ger", "Frost", "dich", "k\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nicht alt, ich schein' nur arm,", "tokens": ["Ich", "bin", "nicht", "alt", ",", "ich", "schein'", "nur", "arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so das Feuer gelte.\u00ab", "tokens": ["Und", "so", "das", "Feu\u00b7er", "gel\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "Er kehrt nach Hause zum Kamin,", "tokens": ["Er", "kehrt", "nach", "Hau\u00b7se", "zum", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reiches Feuer findet,", "tokens": ["Ein", "rei\u00b7ches", "Feu\u00b7er", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbrennt den B\u00fcndel noch darin,", "tokens": ["Ver\u00b7brennt", "den", "B\u00fcn\u00b7del", "noch", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fluch das Feuer bindet.", "tokens": ["Der", "Fluch", "das", "Feu\u00b7er", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.34": {"line.1": {"text": "Die Flammen wenden sich von ihm", "tokens": ["Die", "Flam\u00b7men", "wen\u00b7den", "sich", "von", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie von dem wilden Winde,", "tokens": ["Wie", "von", "dem", "wil\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie brennen ihn, sie schmerzen ihm,", "tokens": ["Sie", "bren\u00b7nen", "ihn", ",", "sie", "schmer\u00b7zen", "ihm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00e4lten ihn geschwinde.", "tokens": ["Und", "k\u00e4l\u00b7ten", "ihn", "ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Er starrt, die Sonn' beeiset ihn,", "tokens": ["Er", "starrt", ",", "die", "Sonn'", "be\u00b7ei\u00b7set", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Federn ihn beschneien,", "tokens": ["Die", "Fe\u00b7dern", "ihn", "be\u00b7schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der rasche Tanz durchzittert ihn,", "tokens": ["Der", "ra\u00b7sche", "Tanz", "durch\u00b7zit\u00b7tert", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Weib mu\u00df bei ihm weinen.", "tokens": ["Sein", "Weib", "mu\u00df", "bei", "ihm", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.36": {"line.1": {"text": "Der Kirche Anblick macht ihn kalt,", "tokens": ["Der", "Kir\u00b7che", "An\u00b7blick", "macht", "ihn", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Pred'gers Wort durchschauert,", "tokens": ["Des", "Pre\u00b7d'\u00b7gers", "Wort", "durch\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "So ward er jung schon wei\u00df und alt,", "tokens": ["So", "ward", "er", "jung", "schon", "wei\u00df", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem dann betrauert.", "tokens": ["Von", "kei\u00b7nem", "dann", "be\u00b7trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.37": {"line.1": {"text": "Die Sklaven machen den Tyrann,", "tokens": ["Die", "Skla\u00b7ven", "ma\u00b7chen", "den", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er kann nicht Sklaven schaffen,", "tokens": ["Er", "kann", "nicht", "Skla\u00b7ven", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo jeder B\u00fcrger fest als Mann,", "tokens": ["Wo", "je\u00b7der", "B\u00fcr\u00b7ger", "fest", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer soll da einen strafen.", "tokens": ["Wer", "soll", "da", "ei\u00b7nen", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.38": {"line.1": {"text": "Da ist der Arme nimmermehr", "tokens": ["Da", "ist", "der", "Ar\u00b7me", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie S\u00fcnder eingefangen,", "tokens": ["Wie", "S\u00fcn\u00b7der", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist des reichen B\u00fcrgers Ehr',", "tokens": ["Es", "ist", "des", "rei\u00b7chen", "B\u00fcr\u00b7gers", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gastlich zu empfangen.", "tokens": ["Sie", "gast\u00b7lich", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}