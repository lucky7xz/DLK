{"textgrid.poem.65860": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "134", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So! Nun ich eingestanden, da\u00df ich dein,", "tokens": ["So", "!", "Nun", "ich", "ein\u00b7ge\u00b7stan\u00b7den", ",", "da\u00df", "ich", "dein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "PPER", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und deinem Willen ganz verfallen bin:", "tokens": ["Und", "dei\u00b7nem", "Wil\u00b7len", "ganz", "ver\u00b7fal\u00b7len", "bin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geb' ich mich dir, wenn du dies zweite Mein", "tokens": ["Geb'", "ich", "mich", "dir", ",", "wenn", "du", "dies", "zwei\u00b7te", "Mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "PPER", "$,", "KOUS", "PPER", "PDS", "VVFIN", "PPOSAT"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Zu ew'gem Troste mir erstattest, hin.", "tokens": ["Zu", "ew'\u00b7gem", "Tros\u00b7te", "mir", "er\u00b7stat\u00b7test", ",", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch du sagst nein, und er will Freiheit nicht;", "tokens": ["Doch", "du", "sagst", "nein", ",", "und", "er", "will", "Frei\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKANT", "$,", "KON", "PPER", "VMFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn du bist l\u00fcstern, und er ist gelind.", "tokens": ["Denn", "du", "bist", "l\u00fcs\u00b7tern", ",", "und", "er", "ist", "ge\u00b7lind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er schreibt f\u00fcr mich, kennt nur des B\u00fcrgen Pflicht,", "tokens": ["Er", "schreibt", "f\u00fcr", "mich", ",", "kennt", "nur", "des", "B\u00fcr\u00b7gen", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nach dem Vertrag, der ihn gleich fest umspinnt:", "tokens": ["Nach", "dem", "Ver\u00b7trag", ",", "der", "ihn", "gleich", "fest", "um\u00b7spinnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Du wirst zum Vorrecht deiner Sch\u00f6nheit greifen,", "tokens": ["Du", "wirst", "zum", "Vor\u00b7recht", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Du Wuch'rer, der in Nutzen alles kehrt;", "tokens": ["Du", "Wuch'\u00b7rer", ",", "der", "in", "Nut\