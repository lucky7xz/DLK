{"textgrid.poem.27639": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat mich j\u00fcngst ein Freund au\u00df Pindus lassen wissen,", "tokens": ["Es", "hat", "mich", "j\u00fcngst", "ein", "Freund", "au\u00df", "Pin\u00b7dus", "las\u00b7sen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NE", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Jupiter und Mars wolt einen Frieden schliessen:", "tokens": ["Da\u00df", "Ju\u00b7pi\u00b7ter", "und", "Mars", "wolt", "ei\u00b7nen", "Frie\u00b7den", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So Mars hinfort nicht mehr bei allen seinen Tagen", "tokens": ["So", "Mars", "hin\u00b7fort", "nicht", "mehr", "bei", "al\u00b7len", "sei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "PTKNEG", "ADV", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Himmel und nach dem, was himmlisch ist, wil fragen,", "tokens": ["Nach", "Him\u00b7mel", "und", "nach", "dem", ",", "was", "himm\u00b7lisch", "ist", ",", "wil", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Wil Jupiter dahin sich bindlich denn erkl\u00e4ren,", "tokens": ["Wil", "Ju\u00b7pi\u00b7ter", "da\u00b7hin", "sich", "bind\u00b7lich", "denn", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PAV", "PRF", "ADJD", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Mars noch nebst der Welt die H\u00f6lle zu gewehren.", "tokens": ["Dem", "Mars", "noch", "nebst", "der", "Welt", "die", "H\u00f6l\u00b7le", "zu", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es hat mich j\u00fcngst ein Freund au\u00df Pindus lassen wissen,", "tokens": ["Es", "hat", "mich", "j\u00fcngst", "ein", "Freund", "au\u00df", "Pin\u00b7dus", "las\u00b7sen", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NE", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Jupiter und Mars wolt einen Frieden schliessen:", "tokens": ["Da\u00df", "Ju\u00b7pi\u00b7ter", "und", "Mars", "wolt", "ei\u00b7nen", "Frie\u00b7den", "schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So Mars hinfort nicht mehr bei allen seinen Tagen", "tokens": ["So", "M