{"textgrid.poem.35766": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "5. Nach einem alten Liede", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, m\u00f6chte mein Liebchen ein Rosenstock sein!", "tokens": ["O", ",", "m\u00f6ch\u00b7te", "mein", "Lieb\u00b7chen", "ein", "Ro\u00b7sen\u00b7stock", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dann n\u00e4hm' ich von drau\u00dfen den Liebling herein,", "tokens": ["Dann", "n\u00e4hm'", "ich", "von", "drau\u00b7\u00dfen", "den", "Lieb\u00b7ling", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und stellt' ihn vors Fenster, im Fr\u00fchlingsweh'n,", "tokens": ["Und", "stellt'", "ihn", "vors", "Fens\u00b7ter", ",", "im", "Fr\u00fch\u00b7lings\u00b7weh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da k\u00f6nnt' ich ihn immer und immerdar seh'n.", "tokens": ["Da", "k\u00f6nnt'", "ich", "ihn", "im\u00b7mer", "und", "im\u00b7mer\u00b7dar", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "KON", "ADV", "VVFIN", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Da sollt'