{"dta.poem.19305": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das B\u00f6se ist nicht aus der Welt hinauszul\u00fcgen,", "tokens": ["Das", "B\u00f6\u00b7se", "ist", "nicht", "aus", "der", "Welt", "hin\u00b7aus\u00b7zu\u00b7l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das Bewustseyn l\u00e4\u00dft sich nicht um es betr\u00fcgen.", "tokens": ["Und", "das", "Be\u00b7wust\u00b7seyn", "l\u00e4\u00dft", "sich", "nicht", "um", "es", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "KOUI", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Erkl\u00e4ren l\u00e4\u00dft es auch sich nicht im Bild als Schatten,", "tokens": ["Er\u00b7kl\u00e4\u00b7ren", "l\u00e4\u00dft", "es", "auch", "sich", "nicht", "im", "Bild", "als", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PRF", "PTKNEG", "APPRART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur zu besserm Glanz dem Lichten kommt zu Statten.", "tokens": ["Der", "nur", "zu", "bes\u00b7serm", "Glanz", "dem", "Lich\u00b7ten", "kommt", "zu", "Stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist zu wenig Tag, und zuviel Schattenschlag,", "tokens": ["Es", "ist", "zu", "we\u00b7nig", "Tag", ",", "und", "zu\u00b7viel", "Schat\u00b7ten\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als da\u00df der Sch\u00f6nheitsinn sich dran erfreuen mag.", "tokens": ["Als", "da\u00df", "der", "Sch\u00f6n\u00b7hei\u00b7tsinn", "sich", "dran", "er\u00b7freu\u00b7en", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "KOUS", "ART", "NN", "PRF", "PAV", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und von den gl\u00e4nzenden Partieen nicht beschwichtigt", "tokens": ["Und", "von", "den", "gl\u00e4n\u00b7zen\u00b7den", "Par\u00b7ti\u00b7een", "nicht", "be\u00b7schwich\u00b7tigt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKNEG", "VVPP"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird der Beschauer, der die schadhaften besichtigt.", "tokens": ["Wird", "der", "Be\u00b7schau\u00b7er", ",", "der", "die", "schad\u00b7haf\u00b7ten", "be\u00b7sich\u00b7tigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "VVPP", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Das Bild ist offenbar nicht in dem rechten Stand,", "tokens": ["Das", "Bild", "ist", "of\u00b7fen\u00b7bar", "nicht", "in", "dem", "rech\u00b7ten", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie es gekommen ist aus seines Meisters Hand.", "tokens": ["Wie", "es", "ge\u00b7kom\u00b7men", "ist", "aus", "sei\u00b7nes", "Meis\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir kommen \u00fcberein hierinnen, und entzwein", "tokens": ["Wir", "kom\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", "hie\u00b7rin\u00b7nen", ",", "und", "ent\u00b7zwein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KON", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Uns darin nur, wie dem mag abzuhelfen seyn;", "tokens": ["Uns", "da\u00b7rin", "nur", ",", "wie", "dem", "mag", "ab\u00b7zu\u00b7hel\u00b7fen", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADV", "$,", "PWAV", "PDS", "VMFIN", "VVIZU", "VAINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Wer \u00fcbernehmen soll und kann beim Bild das Amt,", "tokens": ["Wer", "\u00fc\u00b7ber\u00b7neh\u00b7men", "soll", "und", "kann", "beim", "Bild", "das", "Amt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "KON", "VMFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es herzustellen so, wie es vom Meister stammt.", "tokens": ["Es", "her\u00b7zu\u00b7stel\u00b7len", "so", ",", "wie", "es", "vom", "Meis\u00b7ter", "stammt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}