{"textgrid.poem.39812": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zum Feste der Erinnerung an den russischen Feldzug", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Gegenwart, so still, so thatenlos,", "tokens": ["Du", "Ge\u00b7gen\u00b7wart", ",", "so", "still", ",", "so", "tha\u00b7ten\u00b7los", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist's wahr, da\u00df du an Wunderzeiten gr\u00e4nzest,", "tokens": ["Ist's", "wahr", ",", "da\u00df", "du", "an", "Wun\u00b7der\u00b7zei\u00b7ten", "gr\u00e4n\u00b7zest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du von Bildern, welche riesengro\u00df", "tokens": ["Da\u00df", "du", "von", "Bil\u00b7dern", ",", "wel\u00b7che", "rie\u00b7sen\u00b7gro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entfliehn, in hellem Widerscheine gl\u00e4nzest?", "tokens": ["Ent\u00b7fliehn", ",", "in", "hel\u00b7lem", "Wi\u00b7der\u00b7schei\u00b7ne", "gl\u00e4n\u00b7zest", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist's kein Jahrtausend, da\u00df der Kriegsorkan", "tokens": ["Ist's", "kein", "Jahr\u00b7tau\u00b7send", ",", "da\u00df", "der", "Kriegs\u00b7or\u00b7kan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die halbe Welt mit seinen Donnern f\u00fcllte,", "tokens": ["Die", "hal\u00b7be", "Welt", "mit", "sei\u00b7nen", "Don\u00b7nern", "f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df der Nord ein Heer auf stolzer Bahn,", "tokens": ["Und", "da\u00df", "der", "Nord", "ein", "Heer", "auf", "stol\u00b7zer", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum Tod im Schnee bestimmt, in Flammen h\u00fcllte?", "tokens": ["Zum", "Tod", "im", "Schnee", "be\u00b7stimmt", ",", "in", "Flam\u00b7men", "h\u00fcll\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVPP", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Sage schon fiel jene Zeit anheim,", "tokens": ["Der", "Sa\u00b7ge", "schon", "fiel", "je\u00b7ne", "Zeit", "an\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es t\u00f6net fern, gleich einer alten M\u00e4re;", "tokens": ["Es", "t\u00f6\u00b7net", "fern", ",", "gleich", "ei\u00b7ner", "al\u00b7ten", "M\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ja mit dem Schlachtenhalbgott spielt ein Reim,", "tokens": ["Ja", "mit", "dem", "Schlach\u00b7ten\u00b7halb\u00b7gott", "spielt", "ein", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Dichtung schildert seine Siegesehre.", "tokens": ["Die", "Dich\u00b7tung", "schil\u00b7dert", "sei\u00b7ne", "Sie\u00b7ge\u00b7seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn ein S\u00e4nger lang genug gestrebt,", "tokens": ["Und", "wenn", "ein", "S\u00e4n\u00b7ger", "lang", "ge\u00b7nug", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Leben das Vergangne zu begaben,", "tokens": ["Mit", "Le\u00b7ben", "das", "Ver\u00b7gang\u00b7ne", "zu", "be\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nun sein Werk betrachtet: so erbebt", "tokens": ["Und", "nun", "sein", "Werk", "be\u00b7trach\u00b7tet", ":", "so", "er\u00b7bebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er vor sich selbst \u2013 er glaubt getr\u00e4umt zu haben.", "tokens": ["Er", "vor", "sich", "selbst", "\u2013", "er", "glaubt", "ge\u00b7tr\u00e4umt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PRF", "ADV", "$(", "PPER", "VVFIN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Land zu Land so breite Heldenspur,", "tokens": ["Von", "Land", "zu", "Land", "so", "brei\u00b7te", "Hel\u00b7den\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So rei\u00dfend Gl\u00fcck; alsdann aus heitern L\u00fcften", "tokens": ["So", "rei\u00b7\u00dfend", "Gl\u00fcck", ";", "als\u00b7dann", "aus", "hei\u00b7tern", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$.", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der j\u00e4he Schlag, die Schranken der Natur,", "tokens": ["Der", "j\u00e4\u00b7he", "Schlag", ",", "die", "Schran\u00b7ken", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein ganz Titanenvolk in eis'gen Gr\u00fcften;", "tokens": ["Ein", "ganz", "Ti\u00b7ta\u00b7nen\u00b7volk", "in", "eis'\u00b7gen", "Gr\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf Wandrung geht die Muse zweifelnd aus:", "tokens": ["Auf", "Wand\u00b7rung", "geht", "die", "Mu\u00b7se", "zwei\u00b7felnd", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist es geschehn, ja konnt' es nur geschehen?", "tokens": ["Ist", "es", "ge\u00b7schehn", ",", "ja", "konnt'", "es", "nur", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da h\u00e4lt sie still vor einem Sommerhaus,", "tokens": ["Da", "h\u00e4lt", "sie", "still", "vor", "ei\u00b7nem", "Som\u00b7mer\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lauschet Worten, die wie Thaten wehen.", "tokens": ["Und", "lau\u00b7schet", "Wor\u00b7ten", ",", "die", "wie", "Tha\u00b7ten", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "H\u00e4lt Rat im Kreise hier ein Geisterchor,", "tokens": ["H\u00e4lt", "Rat", "im", "Krei\u00b7se", "hier", "ein", "Geis\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und will den S\u00f6hnen ferne Wunder melden?", "tokens": ["Und", "will", "den", "S\u00f6h\u00b7nen", "fer\u00b7ne", "Wun\u00b7der", "mel\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie \u00f6ffnet scheu das angelehnte Thor,", "tokens": ["Sie", "\u00f6ff\u00b7net", "scheu", "das", "an\u00b7ge\u00b7lehn\u00b7te", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sieht \u2013 ein r\u00fcstig H\u00e4uflein alter Helden.", "tokens": ["Und", "sieht", "\u2013", "ein", "r\u00fcs\u00b7tig", "H\u00e4uf\u00b7lein", "al\u00b7ter", "Hel\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "ART", "ADJD", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ehrw\u00fcrd'ge Reste grausenhafter Not,", "tokens": ["Ehr\u00b7w\u00fcrd'\u00b7ge", "Res\u00b7te", "grau\u00b7sen\u00b7haf\u00b7ter", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Nicht Narben blo\u00df lie\u00df euch der nahe Tod,", "tokens": ["Nicht", "Nar\u00b7ben", "blo\u00df", "lie\u00df", "euch", "der", "na\u00b7he", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er fuhr mit kalter Hand in's Mark der Knochen.", "tokens": ["Er", "fuhr", "mit", "kal\u00b7ter", "Hand", "in's", "Mark", "der", "Kno\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Erz\u00e4hlt, erz\u00e4hlt! die Muse st\u00f6rt euch nicht,", "tokens": ["Er\u00b7z\u00e4hlt", ",", "er\u00b7z\u00e4hlt", "!", "die", "Mu\u00b7se", "st\u00f6rt", "euch", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Amt ist heut zu horchen, nicht zu singen,", "tokens": ["Ihr", "Amt", "ist", "heut", "zu", "hor\u00b7chen", ",", "nicht", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Sie l\u00e4\u00dft den Strom an's Herz sich schaudernd dringen.", "tokens": ["Sie", "l\u00e4\u00dft", "den", "Strom", "an's", "Herz", "sich", "schau\u00b7dernd", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Stellt hin des ungeheuren Mannes Bild,", "tokens": ["Stellt", "hin", "des", "un\u00b7ge\u00b7heu\u00b7ren", "Man\u00b7nes", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den ihr in Gl\u00fcck und Mi\u00dfgeschick begleitet,", "tokens": ["Den", "ihr", "in", "Gl\u00fcck", "und", "Mi\u00df\u00b7ge\u00b7schick", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Beschreibet, wie er \u00fcber Tr\u00fcmmer wild", "tokens": ["Be\u00b7schrei\u00b7bet", ",", "wie", "er", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mer", "wild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach seinem Ziel \u2013 und fern vom Ziele schreitet.", "tokens": ["Nach", "sei\u00b7nem", "Ziel", "\u2013", "und", "fern", "vom", "Zie\u00b7le", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Zeigt durch die Steppe bunten Heeres Pfad,", "tokens": ["Zeigt", "durch", "die", "Step\u00b7pe", "bun\u00b7ten", "Hee\u00b7res", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie es den stolzen Schlangenleib entwickelt,", "tokens": ["Wie", "es", "den", "stol\u00b7zen", "Schlan\u00b7gen\u00b7leib", "ent\u00b7wi\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie es krank der Heimat wieder naht,", "tokens": ["Und", "wie", "es", "krank", "der", "Hei\u00b7mat", "wie\u00b7der", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Frost der Nacht ber\u00fchrt, geschw\u00e4cht, zerst\u00fcckelt.", "tokens": ["Vom", "Frost", "der", "Nacht", "be\u00b7r\u00fchrt", ",", "ge\u00b7schw\u00e4cht", ",", "zer\u00b7st\u00fc\u00b7ckelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Nennt manchen Bruder, dessen Schatten nur", "tokens": ["Nennt", "man\u00b7chen", "Bru\u00b7der", ",", "des\u00b7sen", "Schat\u00b7ten", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$,", "PRELAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tr\u00fcbschwebend naht und flieht aus eurem Kreise;", "tokens": ["Tr\u00fcb\u00b7schwe\u00b7bend", "naht", "und", "flieht", "aus", "eu\u00b7rem", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bezeichnet seiner letzten Thaten Spur,", "tokens": ["Be\u00b7zeich\u00b7net", "sei\u00b7ner", "letz\u00b7ten", "Tha\u00b7ten", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Grab, nicht hell von Marmor, ach, von Eise.", "tokens": ["Sein", "Grab", ",", "nicht", "hell", "von", "Mar\u00b7mor", ",", "ach", ",", "von", "Ei\u00b7se", "."], "token_info": ["wor