{"dta.poem.18294": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Einem \u201eTondichter\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Das Theekind aller alten Vetteln", "tokens": ["Das", "Thee\u00b7kind", "al\u00b7ler", "al\u00b7ten", "Vet\u00b7teln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch, was deine Kunst betrifft,", "tokens": ["Und", "auch", ",", "was", "dei\u00b7ne", "Kunst", "be\u00b7tr\u00b7ifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gerecht in allen Modes\u00e4tteln.", "tokens": ["Ge\u00b7recht", "in", "al\u00b7len", "Mo\u00b7de\u00b7s\u00e4t\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Uns fascinirt nicht nur dein Name,", "tokens": ["Uns", "fas\u00b7ci\u00b7nirt", "nicht", "nur", "dein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Du spielst wahrhaftig mit Talent \u2014", "tokens": ["Du", "spielst", "wahr\u00b7haf\u00b7tig", "mit", "Ta\u00b7lent"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zumal dein Lieblingsinstrument,", "tokens": ["Zu\u00b7mal", "dein", "Lieb\u00b7lings\u00b7inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das goldne Kalbfell der Reclame!", "tokens": ["Das", "gold\u00b7ne", "Kalb\u00b7fell", "der", "Re\u00b7cla\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}}}}