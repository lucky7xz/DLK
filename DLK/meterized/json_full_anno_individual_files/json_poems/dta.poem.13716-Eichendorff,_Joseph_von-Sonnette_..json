{"dta.poem.13716": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df die Nacht die grausen Fl\u00fcgel strecken,", "tokens": ["So", "la\u00df", "die", "Nacht", "die", "grau\u00b7sen", "Fl\u00fc\u00b7gel", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur immerzu, mein tapfres Schiff, gedrungen!", "tokens": ["Nur", "im\u00b7mer\u00b7zu", ",", "mein", "tapf\u00b7res", "Schiff", ",", "ge\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer einmal mit den Wogen hat gerungen,", "tokens": ["Wer", "ein\u00b7mal", "mit", "den", "Wo\u00b7gen", "hat", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchlt sich das Herz gehoben in den Schrecken.", "tokens": ["F\u00fchlt", "sich", "das", "Herz", "ge\u00b7ho\u00b7ben", "in", "den", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}