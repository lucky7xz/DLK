{"dta.poem.10784": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Saurbronnen Liedlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schon bey diesem Bronne", "tokens": ["Ob", "schon", "bey", "die\u00b7sem", "Bron\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die meng der Jungfr\u00e4wlein", "tokens": ["Die", "meng", "der", "Jung\u00b7fr\u00e4w\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd dann die f\u00fcll der wonne", "tokens": ["Vnd", "dann", "die", "f\u00fcll", "der", "won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zusammen kommen sein:", "tokens": ["Zu\u00b7sam\u00b7men", "kom\u00b7men", "sein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob schon di\u00df lieblich Thale", "tokens": ["Ob", "schon", "di\u00df", "lieb\u00b7lich", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDS", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob schon die Berg vnd W\u00e4ldt", "tokens": ["Ob", "schon", "die", "Berg", "vnd", "W\u00e4ldt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n gr\u00fcnen vberale/", "tokens": ["Sch\u00f6n", "gr\u00fc\u00b7nen", "vbe\u00b7ra\u00b7le", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+----", "measure": "dactylic.init"}, "line.4": {"text": "Mir dessen nichts gefellt.", "tokens": ["Mir", "des\u00b7sen", "nichts", "ge\u00b7fellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ob schon die B\u00e4chlein sausen/", "tokens": ["Ob", "schon", "die", "B\u00e4ch\u00b7lein", "sau\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich doch nit schlaffen kan/", "tokens": ["Ich", "doch", "nit", "schlaf\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob schon die B\u00e4ume brausen/", "tokens": ["Ob", "schon", "die", "B\u00e4u\u00b7me", "brau\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ficht mich doch immer an/", "tokens": ["Ficht", "mich", "doch", "im\u00b7mer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Gar ferne zu der frist/", "tokens": ["Gar", "fer\u00b7ne", "zu", "der", "frist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die aller zierd\u2019 ein kerne/", "tokens": ["Die", "al\u00b7ler", "zierd'", "ein", "ker\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja Zierde selber ist/", "tokens": ["Ja", "Zier\u00b7de", "sel\u00b7ber", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die nur mit jhren blicken", "tokens": ["Die", "nur", "mit", "jhren", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "All ding erfrewen thut/", "tokens": ["All", "ding", "er\u00b7fre\u00b7wen", "thut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die mir allein erquicken", "tokens": ["Die", "mir", "al\u00b7lein", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kan meinen Geist vnd Muth.", "tokens": ["Kan", "mei\u00b7nen", "Geist", "vnd", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "F\u00fcrwar wohin nur sehen", "tokens": ["F\u00fcr\u00b7war", "wo\u00b7hin", "nur", "se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr gl\u00e4ntzend\u2019 Aeugelein/", "tokens": ["Ihr", "gl\u00e4nt\u00b7zend'", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df alles tr\u00fcb vergehen/", "tokens": ["Mu\u00df", "al\u00b7les", "tr\u00fcb", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie vor dem Sonnenschein.", "tokens": ["Wie", "vor", "dem", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohin jhr Athem wehet/", "tokens": ["Wo\u00b7hin", "jhr", "A\u00b7them", "we\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Athem also bald/", "tokens": ["Ihr", "A\u00b7them", "al\u00b7so", "bald", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gleich dem Westwindt/ au\u00dfseet", "tokens": ["Gleich", "dem", "West\u00b7windt", "/", "au\u00df\u00b7seet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "$(", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Bl\u00fcmlein mannichfalt.", "tokens": ["Der", "Bl\u00fcm\u00b7lein", "man\u00b7nich\u00b7falt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch sie allein verbleibet", "tokens": ["Doch", "sie", "al\u00b7lein", "ver\u00b7blei\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Blum der Bl\u00fcmelein/", "tokens": ["Die", "Blum", "der", "Bl\u00fc\u00b7me\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Floram zu ruck sie treibet/", "tokens": ["Flo\u00b7ram", "zu", "ruck", "sie", "trei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sampt all jhrm Blumen schrein.", "tokens": ["Sampt", "all", "jhrm", "Blu\u00b7men", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Komm ", "tokens": ["Komm"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wendt von mir allen schmertz/", "tokens": ["Wendt", "von", "mir", "al\u00b7len", "schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PIAT", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "All frewden zu mir wende/", "tokens": ["All", "frew\u00b7den", "zu", "mir", "wen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaff mir ein newes Hertz.", "tokens": ["Schaff", "mir", "ein", "ne\u00b7wes", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Du kanst mit deinem Munde", "tokens": ["Du", "kanst", "mit", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sawrheit dieser Quell", "tokens": ["Die", "Sawr\u00b7heit", "die\u00b7ser", "Quell"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verzuckern/ vnd gesunde", "tokens": ["Ver\u00b7zu\u00b7ckern", "/", "vnd", "ge\u00b7sun\u00b7de"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$(", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich wieder machen schnell.", "tokens": ["Mich", "wie\u00b7der", "ma\u00b7chen", "schnell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du/ du allein kanst machen", "tokens": ["Du", "/", "du", "al\u00b7lein", "kanst", "ma\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Au\u00df diesem gantzen Krei\u00df", "tokens": ["Au\u00df", "die\u00b7sem", "gant\u00b7zen", "Krei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch dein eintzig anlachen", "tokens": ["Durch", "dein", "eint\u00b7zig", "an\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein irrdisch Paradei\u00df.", "tokens": ["Ein", "irr\u00b7disch", "Pa\u00b7ra\u00b7dei\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So komm/ komm aber balde/", "tokens": ["So", "komm", "/", "komm", "a\u00b7ber", "bal\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd komstu nit zu mir/", "tokens": ["Vnd", "koms\u00b7tu", "nit", "zu", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ist nichts da\u00df mich halte/", "tokens": ["So", "ist", "nichts", "da\u00df", "mich", "hal\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "KOUS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Phyllis, ich mu\u00df zu dir.", "tokens": ["Phyl\u00b7lis", ",", "ich", "mu\u00df", "zu", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}