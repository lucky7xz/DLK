{"textgrid.poem.59494": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Waldfr\u00e4ulein", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Falke war im Wald verflogen", "tokens": ["Fal\u00b7ke", "war", "im", "Wald", "ver\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Hunde irrten weit,", "tokens": ["Und", "die", "Hun\u00b7de", "irr\u00b7ten", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jagdm\u00fcd lehnt' an eine Eiche", "tokens": ["Jagd\u00b7m\u00fcd", "lehnt'", "an", "ei\u00b7ne", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Ritter im Gestein,", "tokens": ["Sich", "der", "Rit\u00b7ter", "im", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Jungfrau da erschrocken", "tokens": ["Ei\u00b7ne", "Jung\u00b7frau", "da", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Wipfels Dunkelheit", "tokens": ["In", "des", "Wip\u00b7fels", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sah er stehen, ihre Locken", "tokens": ["Sah", "er", "ste\u00b7hen", ",", "ih\u00b7re", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rings umgaben Stamm und Zweig.", "tokens": ["Rings", "um\u00b7ga\u00b7ben", "Stamm", "und", "Zweig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbstaune nicht und la\u00df dein Grauen,", "tokens": ["\u00bb", "stau\u00b7ne", "nicht", "und", "la\u00df", "dein", "Grau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "KON", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bin ein K\u00f6nigst\u00f6chterlein,", "tokens": ["Bin", "ein", "K\u00f6\u00b7nig\u00b7st\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sieben Zauberfraun mich haben", "tokens": ["Sie\u00b7ben", "Zau\u00b7ber\u00b7fraun", "mich", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf der Amme Scho\u00df gefeit,", "tokens": ["Auf", "der", "Am\u00b7me", "Scho\u00df", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Da\u00df ich sieben Jahr mu\u00df wohnen", "tokens": ["Da\u00df", "ich", "sie\u00b7ben", "Jahr", "mu\u00df", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Hier in Waldeseinsamkeit.", "tokens": ["Hier", "in", "Wal\u00b7de\u00b7sein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Sieben Jahr sind heut verflossen", "tokens": ["Sie\u00b7ben", "Jahr", "sind", "heut", "ver\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Oder morgen um die Zeit,", "tokens": ["O\u00b7der", "mor\u00b7gen", "um", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Bitte dich um Gottes willen,", "tokens": ["Bit\u00b7te", "dich", "um", "Got\u00b7tes", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "F\u00fchr mich aus dem Walde heim,", "tokens": ["F\u00fchr", "mich", "aus", "dem", "Wal\u00b7de", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Will als Ehefrau dir dienen,", "tokens": ["Will", "als", "E\u00b7he\u00b7frau", "dir", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Oder auch dein Liebchen sein.\u00ab", "tokens": ["O\u00b7der", "auch", "dein", "Lieb\u00b7chen", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbfr\u00e4ulein, noch bis morgen fr\u00fche", "tokens": ["\u00bb", "fr\u00e4u\u00b7lein", ",", "noch", "bis", "mor\u00b7gen", "fr\u00fc\u00b7he"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Harret in dem Walde mein,", "tokens": ["Har\u00b7ret", "in", "dem", "Wal\u00b7de", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab zu Haus 'ne weise Mutter,", "tokens": ["Hab", "zu", "Haus", "'ne", "wei\u00b7se", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will erst fragen, was sie meint.\u00ab \u2013", "tokens": ["Will", "erst", "fra\u00b7gen", ",", "was", "sie", "meint", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie vom Baum rief: \u00bbWeh dem Ritter,", "tokens": ["Sie", "vom", "Baum", "rief", ":", "\u00bb", "Weh", "dem", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "$.", "$(", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der die Jungfrau l\u00e4\u00dft allein!\u00ab", "tokens": ["Der", "die", "Jung\u00b7frau", "l\u00e4\u00dft", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Er ritt fort, sie blieb im Walde,", "tokens": ["Er", "ritt", "fort", ",", "sie", "blieb", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mutter riet, er sollt sie frein.", "tokens": ["Mut\u00b7ter", "riet", ",", "er", "sollt", "sie", "frein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als er morgens kehrt' zur\u00fccke,", "tokens": ["Als", "er", "mor\u00b7gens", "kehrt'", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War's so stille im Gestein,", "tokens": ["Wa\u00b7r's", "so", "stil\u00b7le", "im", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Konnt den Baum nicht wiederfinden,", "tokens": ["Konnt", "den", "Baum", "nicht", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber weit, vom Walde weit", "tokens": ["A\u00b7ber", "weit", ",", "vom", "Wal\u00b7de", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah er ziehn ein F\u00e4hnlein Reiter,", "tokens": ["Sah", "er", "ziehn", "ein", "F\u00e4hn\u00b7lein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrten fort das Waldfr\u00e4ulein;", "tokens": ["F\u00fchr\u00b7ten", "fort", "das", "Wald\u00b7fr\u00e4u\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er st\u00fcrzt zu Boden nieder", "tokens": ["Und", "er", "st\u00fcrzt", "zu", "Bo\u00b7den", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der gr\u00fcnen Einsamkeit:", "tokens": ["In", "der", "gr\u00fc\u00b7nen", "Ein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbschwer Gericht verdient der Ritter,", "tokens": ["\u00bb", "schwer", "Ge\u00b7richt", "ver\u00b7dient", "der", "Rit\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der verloren solche Maid!", "tokens": ["Der", "ver\u00b7lo\u00b7ren", "sol\u00b7che", "Maid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVPP", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ich will selbst den Stab mir brechen,", "tokens": ["Ich", "will", "selbst", "den", "Stab", "mir", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Ich will selbst mein Richter sein,", "tokens": ["Ich", "will", "selbst", "mein", "Rich\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Abhaun soll man mir die Rechte", "tokens": ["Ab\u00b7haun", "soll", "man", "mir", "die", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und mich schleifen durch die Heid!\u00ab", "tokens": ["Und", "mich", "schlei\u00b7fen", "durch", "die", "Heid", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Falke war im Wald verflogen", "tokens": ["Fal\u00b7ke", "war", "im", "Wald", "ver\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Hunde irrten weit,", "tokens": ["Und", "die", "Hun\u00b7de", "irr\u00b7ten", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jagdm\u00fcd lehnt' an eine Eiche", "tokens": ["Jagd\u00b7m\u00fcd", "lehnt'", "an", "e