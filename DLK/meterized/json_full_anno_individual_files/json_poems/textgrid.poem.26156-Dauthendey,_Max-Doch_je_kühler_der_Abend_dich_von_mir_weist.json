{"textgrid.poem.26156": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Doch je k\u00fchler der Abend dich von mir weist", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der l\u00fcsterne Abend kommt durchs Fenster mit Wohlgeruch,", "tokens": ["Der", "l\u00fcs\u00b7ter\u00b7ne", "A\u00b7bend", "kommt", "durchs", "Fens\u00b7ter", "mit", "Wohl\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als murmelt er vor sich hin manch verf\u00fchrenden Spruch.", "tokens": ["Als", "mur\u00b7melt", "er", "vor", "sich", "hin", "manch", "ver\u00b7f\u00fch\u00b7ren\u00b7den", "Spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PRF", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Er will dich im Dunkel von meiner Seite fortr\u00fccken.", "tokens": ["Er", "will", "dich", "im", "Dun\u00b7kel", "von", "mei\u00b7ner", "Sei\u00b7te", "fort\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Er stiehlt dich meinen Augen, bis ich nichts behielt", "tokens": ["Er", "stiehlt", "dich", "mei\u00b7nen", "Au\u00b7gen", ",", "bis", "ich", "nichts", "be\u00b7hielt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als dein Bild im Geist und mein stilles Entz\u00fccken", "tokens": ["Als", "dein", "Bild", "im", "Geist", "und", "mein", "stil\u00b7les", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und deine H\u00e4nde, die mich dunkel an sich dr\u00fccken.", "tokens": ["Und", "dei\u00b7ne", "H\u00e4n\u00b7de", ",", "die", "mich", "dun\u00b7kel", "an", "sich", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch je k\u00fchler der Abend dich von mir weist,", "tokens": ["Doch", "je", "k\u00fch\u00b7ler", "der", "A\u00b7bend", "dich", "von", "mir", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Desto w\u00e4rmer dein Atem um meinen kreist,", "tokens": ["Des\u00b7to", "w\u00e4r\u00b7mer", "dein", "A\u00b7tem", "um", "mei\u00b7nen", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Desto n\u00e4her meine Lippen zu deinen r\u00fccken.", "tokens": ["Des\u00b7to", "n\u00e4\u00b7her", "mei\u00b7ne", "Lip\u00b7pen", "zu", "dei\u00b7nen", "r\u00