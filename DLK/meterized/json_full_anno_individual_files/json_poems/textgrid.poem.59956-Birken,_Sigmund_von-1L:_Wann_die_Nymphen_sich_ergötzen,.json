{"textgrid.poem.59956": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann die Nymphen sich erg\u00f6tzen,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann die Nymphen sich erg\u00f6tzen,", "tokens": ["Wann", "die", "Nym\u00b7phen", "sich", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie etwan in dem Hetzen", "tokens": ["Und", "sie", "et\u00b7wan", "in", "dem", "Het\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Eines Dornes Stachel ritzt:", "tokens": ["Ei\u00b7nes", "Dor\u00b7nes", "Sta\u00b7chel", "ritzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Ritz sich hin ergossen,", "tokens": ["Wo", "der", "Ritz", "sich", "hin", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht er Floramoren sprossen,", "tokens": ["Macht", "er", "Flo\u00b7ra\u00b7mo\u00b7ren", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blutes Purpur auf sie spritzt.", "tokens": ["Blu\u00b7tes", "Pur\u00b7pur", "auf", "sie", "spritzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["AD