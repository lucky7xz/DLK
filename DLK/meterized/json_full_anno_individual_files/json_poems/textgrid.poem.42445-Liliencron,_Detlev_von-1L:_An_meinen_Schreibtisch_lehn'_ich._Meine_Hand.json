{"textgrid.poem.42445": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An meinen Schreibtisch lehn' ich. Meine Hand", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meinen Schreibtisch lehn' ich. Meine Hand", "tokens": ["An", "mei\u00b7nen", "Schreib\u00b7tisch", "lehn'", "ich", ".", "Mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchgleitet leicht ein rotes Nackenband,", "tokens": ["Durch\u00b7glei\u00b7tet", "leicht", "ein", "ro\u00b7tes", "Na\u00b7cken\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erinnrung einer Zeit, die l\u00e4ngst verflo\u00df,", "tokens": ["E\u00b7rinn\u00b7rung", "ei\u00b7ner", "Zeit", ",", "die", "l\u00e4ngst", "ver\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da hei\u00df ein M\u00e4dchen mir den Hals umschlo\u00df.", "tokens": ["Da", "hei\u00df", "ein", "M\u00e4d\u00b7chen", "mir", "den", "Hals", "um\u00b7schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die junge Gr\u00e4fin, heimgekehrt, mir graut,", "tokens": ["Die", "jun\u00b7ge", "Gr\u00e4\u00b7fin", ",", "heim\u00b7ge\u00b7kehrt", ",", "mir", "graut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Soll heut ich wiedersehn, des andern Braut.", "tokens": ["Soll", "heut", "ich", "wie\u00b7der\u00b7sehn", ",", "des", "an\u00b7dern", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Haide, wo so reiches Leben sprie\u00dft,", "tokens": ["Die", "Hai\u00b7de", ",", "wo", "so", "rei\u00b7ches", "Le\u00b7ben", "sprie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PWAV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die unabsehbar auseinanderflie\u00dft,", "tokens": ["Die", "un\u00b7ab\u00b7seh\u00b7bar", "aus\u00b7ein\u00b7an\u00b7der\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Trennt mich von ihr; die mu\u00df ich erst durchgehn,", "tokens": ["Trennt", "mich", "von", "ihr", ";", "die", "mu\u00df", "ich", "erst", "durch\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "$.", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eh' kann ich nicht des Schlosses T\u00fcrme sehn.", "tokens": ["Eh'", "kann", "ich", "nicht", "des", "Schlos\u00b7ses", "T\u00fcr\u00b7me", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schon bin ich auf dem Weg. Nur eine Birke,", "tokens": ["Schon", "bin", "ich", "auf", "dem", "Weg", ".", "Nur", "ei\u00b7ne", "Bir\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als einziger Baum im ganzen Grenzbezirke,", "tokens": ["Als", "ein\u00b7zi\u00b7ger", "Baum", "im", "gan\u00b7zen", "Grenz\u00b7be\u00b7zir\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Steht auf der Haide, trostlos und verloren,", "tokens": ["Steht", "auf", "der", "Hai\u00b7de", ",", "trost\u00b7los", "und", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NE", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00e4tte diesen Platz f\u00fcr sich erkoren", "tokens": ["Als", "h\u00e4t\u00b7te", "die\u00b7sen", "Platz", "f\u00fcr", "sich", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PDAT", "NN", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Tr\u00e4umender, als f\u00e4nd' er hier den Frieden", "tokens": ["Ein", "Tr\u00e4u\u00b7men\u00b7der", ",", "als", "f\u00e4nd'", "er", "hier", "den", "Frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In tiefem Denken, allem abgeschieden.", "tokens": ["In", "tie\u00b7fem", "Den\u00b7ken", ",", "al\u00b7lem", "ab\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Herbstwind nahm ihr alle Bl\u00e4tter fort,", "tokens": ["Der", "Herbst\u00b7wind", "nahm", "ihr", "al\u00b7le", "Bl\u00e4t\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur eines blieb, es weht, verwelkt, verdorrt", "tokens": ["Nur", "ei\u00b7nes", "blieb", ",", "es", "weht", ",", "ver\u00b7welkt", ",", "ver\u00b7dorrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am h\u00f6chsten Zweige, wie vom hohen Mast,", "tokens": ["Am", "h\u00f6chs\u00b7ten", "Zwei\u00b7ge", ",", "wie", "vom", "ho\u00b7hen", "Mast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Sonnengold durchtr\u00e4nkt, in \u00dcberhast.", "tokens": ["Von", "Son\u00b7nen\u00b7gold", "durch\u00b7tr\u00e4nkt", ",", "in", "\u00dc\u00b7ber\u00b7hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So wimpelt wohl vom Schiff das F\u00e4hnchen her,", "tokens": ["So", "wim\u00b7pelt", "wohl", "vom", "Schiff", "das", "F\u00e4hn\u00b7chen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kehrt's heimatshafenfroh aus weitem Meer.", "tokens": ["Kehrt's", "hei\u00b7mats\u00b7ha\u00b7fen\u00b7froh", "aus", "wei\u00b7tem", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich bin zur Stelle und geziemendlich", "tokens": ["Ich", "bin", "zur", "Stel\u00b7le", "und", "ge\u00b7zie\u00b7mend\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Verbeug' ich vor der sch\u00f6nen Gr\u00e4fin mich.", "tokens": ["Ver\u00b7beug'", "ich", "vor", "der", "sch\u00f6\u00b7nen", "Gr\u00e4\u00b7fin", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein wenig l\u00e4nger halt' ich ihre Hand", "tokens": ["Ein", "we\u00b7nig", "l\u00e4n\u00b7ger", "halt'", "ich", "ih\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Beim Kusse, wie ein altes Liebespfand.", "tokens": ["Beim", "Kus\u00b7se", ",", "wie", "ein", "al\u00b7tes", "Lie\u00b7be\u00b7spfand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ihr Auge bittet mich, ihr Auge fleht,", "tokens": ["Ihr", "Au\u00b7ge", "bit\u00b7tet", "mich", ",", "ihr", "Au\u00b7ge", "fleht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und \u00fcberwunden, ist das Gl\u00fcck verweht.", "tokens": ["Und", "\u00fc\u00b7berw\u00b7un\u00b7den", ",", "ist", "das", "Gl\u00fcck", "ver\u00b7weht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wir lachen, scherzen, sprechen dies und das,", "tokens": ["Wir", "la\u00b7chen", ",", "scher\u00b7zen", ",", "spre\u00b7chen", "dies", "und", "das", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das Menschenleben ist ein Faschingsspa\u00df.", "tokens": ["Das", "Men\u00b7schen\u00b7le\u00b7ben", "ist", "ein", "Fa\u00b7schings\u00b7spa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wieder bin ich auf dem Weg nach Haus,", "tokens": ["Und", "wie\u00b7der", "bin", "ich", "auf", "dem", "Weg", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein milder, sanfter Regen weint sich aus,", "tokens": ["Ein", "mil\u00b7der", ",", "sanf\u00b7ter", "Re\u00b7gen", "weint", "sich", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Fr\u00fchlingsregen. Langsam schreit' ich hin,", "tokens": ["Wie", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen", ".", "Lang\u00b7sam", "schreit'", "ich", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir ist der Gang so schwer, so tr\u00fcb' der Sinn.", "tokens": ["Mir", "ist", "der", "Gang", "so", "schwer", ",", "so", "tr\u00fcb'", "der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es \u00fcberholte mich ein Kr\u00e4henschwarm \u2013", "tokens": ["Es", "\u00fc\u00b7berh\u00b7ol\u00b7te", "mich", "ein", "Kr\u00e4\u00b7hen\u00b7schwarm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Um ihre Schulter legt' ich meinen Arm,", "tokens": ["Um", "ih\u00b7re", "Schul\u00b7ter", "legt'", "ich", "mei\u00b7nen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So war es mir; wir zogen ohne Wort", "tokens": ["So", "war", "es", "mir", ";", "wir", "zo\u00b7gen", "oh\u00b7ne", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gesenkten Hauptes in die Ferne fort.", "tokens": ["Ge\u00b7senk\u00b7ten", "Haup\u00b7tes", "in", "die", "Fer\u00b7ne", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Kind ging mit uns wie von ungef\u00e4hr,", "tokens": ["Ein", "Kind", "ging", "mit", "uns", "wie", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "KOKOM", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein kleiner Knabe, und ich wei\u00df es, wer.", "tokens": ["Ein", "klei\u00b7ner", "Kna\u00b7be", ",", "und", "ich", "wei\u00df", "es", ",", "wer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "PWS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er gab die H\u00e4ndchen uns, sein Antlitz tr\u00e4gt", "tokens": ["Er", "gab", "die", "H\u00e4nd\u00b7chen", "uns", ",", "sein", "Ant\u00b7litz", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der holden Mutter Z\u00fcge eingepr\u00e4gt.", "tokens": ["Der", "hol\u00b7den", "Mut\u00b7ter", "Z\u00fc\u00b7ge", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Du Knabe, nie geboren \u2013 und allein", "tokens": ["Du", "Kna\u00b7be", ",", "nie", "ge\u00b7bo\u00b7ren", "\u2013", "und", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "ADV", "VVPP", "$(", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Nur wandert mit mir meine Seelenpein.", "tokens": ["Nur", "wan\u00b7dert", "mit", "mir", "mei\u00b7ne", "See\u00b7len\u00b7pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Bald bin ich bei der Birke angelangt,", "tokens": ["Bald", "bin", "ich", "bei", "der", "Bir\u00b7ke", "an\u00b7ge\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Bl\u00e4ttchen oben hat nach mir gebangt.", "tokens": ["Dem", "Bl\u00e4tt\u00b7chen", "o\u00b7ben", "hat", "nach", "mir", "ge\u00b7bangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es h\u00e4ngt so still in nebelfeuchter Ruh,", "tokens": ["Es", "h\u00e4ngt", "so", "still", "in", "ne\u00b7bel\u00b7feuch\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+