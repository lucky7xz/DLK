{"textgrid.poem.60489": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Dahin!", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst, o n\u00e4chtlicher Himmel! blickt ich", "tokens": ["Einst", ",", "o", "n\u00e4cht\u00b7li\u00b7cher", "Him\u00b7mel", "!", "blickt", "ich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "FM", "ADJA", "NN", "$.", "VVFIN", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Selig empor zu dir, umschlungen", "tokens": ["Se\u00b7lig", "em\u00b7por", "zu", "dir", ",", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "PPER", "$,", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Von der Geliebten, und ich weinte", "tokens": ["Von", "der", "Ge\u00b7lieb\u00b7ten", ",", "und", "ich", "wein\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dank dem ewigen Gott!", "tokens": ["Dank", "dem", "e\u00b7wi\u00b7gen", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und sie pfl\u00fcckte mit K\u00fcssen mir die", "tokens": ["Und", "sie", "pfl\u00fcck\u00b7te", "mit", "K\u00fcs\u00b7sen", "mir", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PPER", "ART"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Bl\u00fcte der Wonne von der Wang, und", "tokens": ["Bl\u00