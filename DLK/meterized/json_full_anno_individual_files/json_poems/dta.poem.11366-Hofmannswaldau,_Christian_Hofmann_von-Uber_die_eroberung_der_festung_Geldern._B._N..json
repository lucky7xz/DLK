{"dta.poem.11366": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die eroberung der festung Geldern.  \n B. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geldern liegt und b\u00fccket sich,", "tokens": ["Gel\u00b7dern", "liegt", "und", "b\u00fc\u00b7cket", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Starcker Held! zu deinen f\u00fcssen;", "tokens": ["Star\u00b7cker", "Held", "!", "zu", "dei\u00b7nen", "f\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Europa sieht durch dich", "tokens": ["Und", "Eu\u00b7ro\u00b7pa", "sieht", "durch", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6lich seinen feldzug schliessen.", "tokens": ["Fr\u00f6\u00b7lich", "sei\u00b7nen", "feld\u00b7zug", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du fiengst ihn am ersten an,", "tokens": ["Du", "fi\u00b7engst", "ihn", "am", "ers\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dort, als Bonn dir muste weichen;", "tokens": ["Dort", ",", "als", "Bonn", "dir", "mus\u00b7te", "wei\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jtzund hast du ohne leichen,", "tokens": ["Jt\u00b7zund", "hast", "du", "oh\u00b7ne", "lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne blut ihn abgethan.", "tokens": ["Oh\u00b7ne", "blut", "ihn", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Herr! wer sieget so, wie du?", "tokens": ["Herr", "!", "wer", "sie\u00b7get", "so", ",", "wie", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Andre opffern gantze heere;", "tokens": ["And\u00b7re", "opf\u00b7fern", "gant\u00b7ze", "hee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Deines nimmt im felde zu,", "tokens": ["Dei\u00b7nes", "nimmt", "im", "fel\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Als ob es zu hause w\u00e4re.", "tokens": ["Als", "ob", "es", "zu", "hau\u00b7se", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Wenig tausend mann von dir,", "tokens": ["We\u00b7nig", "tau\u00b7send", "mann", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Die doch keinen sturm gewaget,", "tokens": ["Die", "doch", "kei\u00b7nen", "sturm", "ge\u00b7wa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Schreiben ihm gesetze f\u00fcr.", "tokens": ["Schrei\u00b7ben", "ihm", "ge\u00b7set\u00b7ze", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Prahlt, ihr Frantzen! wie ihr pflegt,", "tokens": ["Prahlt", ",", "ihr", "Frant\u00b7zen", "!", "wie", "ihr", "pflegt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df ihr Landau weggenommen;", "tokens": ["Da\u00df", "ihr", "Lan\u00b7dau", "weg\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Wer mit so viel wunden schl\u00e4gt,", "tokens": ["Wer", "mit", "so", "viel", "wun\u00b7den", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Hat sehr wenig einbekommen.", "tokens": ["Hat", "sehr", "we\u00b7nig", "ein\u00b7be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Blut verschwenden ist kein ruhm:", "tokens": ["Blut", "ver\u00b7schwen\u00b7den", "ist", "kein", "ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "So kan auch ein Barbar kriegen;", "tokens": ["So", "kan", "auch", "ein", "Bar\u00b7bar", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Aber ohne schaden siegen,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "scha\u00b7den", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ist der helden eigenthum.", "tokens": ["Ist", "der", "hel\u00b7den", "ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "So schlug Josua vor dem;", "tokens": ["So", "schlug", "Jo\u00b7sua", "vor", "dem", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.25": {"text": "Heute thut es unser K\u00f6nig.", "tokens": ["Heu\u00b7te", "thut", "es", "un\u00b7ser", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Vielen ist die macht bequem;", "tokens": ["Vie\u00b7len", "ist", "die", "macht", "be\u00b7quem", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Seinem sieg ist sie zu wenig,", "tokens": ["Sei\u00b7nem", "sieg", "ist", "sie", "zu", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Weil es zwar durch volck geschehn,", "tokens": ["Weil", "es", "zwar", "durch", "volck", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Aber auch mu\u00df so gerathen,", "tokens": ["A\u00b7ber", "auch", "mu\u00df", "so", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ADV", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.30": {"text": "Da\u00df man gleich aus Friedrichs thaten", "tokens": ["Da\u00df", "man", "gleich", "aus", "Fried\u00b7richs", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Kan die thaten GOttes sehn.", "tokens": ["Kan", "die", "tha\u00b7ten", "Got\u00b7tes", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Wer erkennt sie nicht allhier?", "tokens": ["Wer", "er\u00b7kennt", "sie", "nicht", "all\u00b7hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Wer gesteht, o prei\u00df der helden!", "tokens": ["Wer", "ge\u00b7steht", ",", "o", "prei\u00df", "der", "hel\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Nicht gantz williglich mit mir?", "tokens": ["Nicht", "gantz", "wil\u00b7lig\u00b7lich", "mit", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Da\u00df man nichts von dir kan melden,", "tokens": ["Da\u00df", "man", "nichts", "von", "dir", "kan", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Da\u00df man nichts von dir erticht,", "tokens": ["Da\u00df", "man", "nichts", "von", "dir", "er\u00b7ticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Was nicht GOttes finger lencket,", "tokens": ["Was", "nicht", "Got\u00b7tes", "fin\u00b7ger", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Was du nicht, eh man es dencket,", "tokens": ["Was", "du", "nicht", ",", "eh", "man", "es", "den\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Schon viel gr\u00f6sser ausgericht.", "tokens": ["Schon", "viel", "gr\u00f6s\u00b7ser", "aus\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Deine wercke sind bekannt,", "tokens": ["Dei\u00b7ne", "wer\u00b7cke", "sind", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Und man darff sie nicht erst sagen.", "tokens": ["Und", "man", "darff", "sie", "nicht", "erst", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Wer noch zweifelt, mag dein land,", "tokens": ["Wer", "noch", "zwei\u00b7felt", ",", "mag", "dein", "land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Mag die feinde selber fragen.", "tokens": ["Mag", "die", "fein\u00b7de", "sel\u00b7ber", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Diese zittern, wo du bist;", "tokens": ["Die\u00b7se", "zit\u00b7tern", ",", "wo", "du", "bist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Jenes, wenn du von ihm eilest;", "tokens": ["Je\u00b7nes", ",", "wenn", "du", "von", "ihm", "ei\u00b7lest", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Weil doch, wo du dich verweilest,", "tokens": ["Weil", "doch", ",", "wo", "du", "dich", "ver\u00b7wei\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Auch zugleich sein leben ist.", "tokens": ["Auch", "zu\u00b7gleich", "sein", "le\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Unvergleichlicher Regent!", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7li\u00b7cher", "Re\u00b7gent", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.49": {"text": "Du herrschst vielen zum exempel.", "tokens": ["Du", "herrschst", "vie\u00b7len", "zum", "ex\u00b7em\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.50": {"text": "Wer dich nur von weitem kennt,", "tokens": ["Wer", "dich", "nur", "von", "wei\u00b7tem", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Wird auch gleich dein ehren-tempel;", "tokens": ["Wird", "auch", "gleich", "dein", "eh\u00b7ren\u00b7tem\u00b7pel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.52": {"text": "Wer dich aber n\u00e4her schaut,", "tokens": ["Wer", "dich", "a\u00b7ber", "n\u00e4\u00b7her", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Wird, wohin er sich auch wendet,", "tokens": ["Wird", ",", "wo\u00b7hin", "er", "sich", "auch", "wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "So durch deinen hof geblendet,", "tokens": ["So", "durch", "dei\u00b7nen", "hof", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Da\u00df er kaum den augen traut.", "tokens": ["Da\u00df", "er", "kaum", "den", "au\u00b7gen", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.56": {"text": "Herr! ist es denn wunderns-werth?", "tokens": ["Herr", "!", "ist", "es", "denn", "wun\u00b7derns\u00b7werth", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Da ich gar dein lob gesungen:", "tokens": ["Da", "ich", "gar", "dein", "lob", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.58": {"text": "Da\u00df ich da mein brod begehrt,", "tokens": ["Da\u00df", "ich", "da", "mein", "brod", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.59": {"text": "Wo man mir mein hertz bezwungen.", "tokens": ["Wo", "man", "mir", "mein", "hertz", "be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Mancher h\u00e4tte brod f\u00fcr mich;", "tokens": ["Man\u00b7cher", "h\u00e4t\u00b7te", "brod", "f\u00fcr", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Doch ich bin nicht mehr zu binden,", "tokens": ["Doch", "ich", "bin", "nicht", "mehr", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Denn man kan wohl herren finden,", "tokens": ["Denn", "man", "kan", "wohl", "her\u00b7ren", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Aber keinen Friderich.", "tokens": ["A\u00b7ber", "kei\u00b7nen", "Fri\u00b7de\u00b7rich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.64": {"text": "Aber keinen solchen held,", "tokens": ["A\u00b7ber", "kei\u00b7nen", "sol\u00b7chen", "held", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.65": {"text": "Der fast t\u00e4glich neue thaten", "tokens": ["Der", "fast", "t\u00e4g\u00b7lich", "neu\u00b7e", "tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Meiner Mus\u2019 entgegen stellt,", "tokens": ["Mei\u00b7ner", "Mus'", "ent\u00b7ge\u00b7gen", "stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.67": {"text": "Wenn sie will in schlaf gerathen.", "tokens": ["Wenn", "sie", "will", "in", "schlaf", "ge\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Dieses, Herr! macht mich allein,", "tokens": ["Die\u00b7ses", ",", "Herr", "!", "macht", "mich", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.69": {"text": "W\u00e4r ichs nicht, zu deinem sclaven:", "tokens": ["W\u00e4r", "ichs", "nicht", ",", "zu", "dei\u00b7nem", "scla\u00b7ven", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "$,", "APPR", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Warum mu\u00df des gl\u00fcckes hafen", "tokens": ["Wa\u00b7rum", "mu\u00df", "des", "gl\u00fc\u00b7ckes", "ha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Mir denn stets verschlossen seyn?", "tokens": ["Mir", "denn", "stets", "ver\u00b7schlos\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}