{"textgrid.poem.36299": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich soll den Schleier, Isis, dir zerrei\u00dfen.", "tokens": ["Ich", "soll", "den", "Schlei\u00b7er", ",", "I\u00b7sis", ",", "dir", "zer\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "NE", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ringt das k\u00fchne Herz, dem keiner wehre,", "tokens": ["Es", "ringt", "das", "k\u00fch\u00b7ne", "Herz", ",", "dem", "kei\u00b7ner", "weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu schaun, wie sich die innre Kraft geb\u00e4re;", "tokens": ["Zu", "schaun", ",", "wie", "sich", "die", "inn\u00b7re", "Kraft", "ge\u00b7b\u00e4\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was frommen Schleier da, so sch\u00f6n sie glei\u00dfen?", "tokens": ["Was", "from\u00b7men", "Schlei\u00b7er", "da", ",", "so", "sch\u00f6n", "sie", "glei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PTKVZ", "$,", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie