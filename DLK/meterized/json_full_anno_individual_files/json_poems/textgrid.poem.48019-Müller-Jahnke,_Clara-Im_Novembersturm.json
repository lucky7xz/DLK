{"textgrid.poem.48019": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Im Novembersturm", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sturmwind rast und der Regen schl\u00e4gt", "tokens": ["Der", "Sturm\u00b7wind", "rast", "und", "der", "Re\u00b7gen", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ans Fenster in schweren Tropfen \u2013", "tokens": ["ans", "Fens\u00b7ter", "in", "schwe\u00b7ren", "Trop\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich f\u00fchl in der tollen Novembernacht", "tokens": ["Ich", "f\u00fchl", "in", "der", "tol\u00b7len", "No\u00b7vem\u00b7ber\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "mein Herz wohl h\u00f6rbar klopfen.", "tokens": ["mein", "Herz", "wohl", "h\u00f6r\u00b7bar", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schl\u00e4gt in brennender Ungeduld", "tokens": ["Es", "schl\u00e4gt", "in", "bren\u00b7nen\u00b7der", "Un\u00b7ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sehns\u00fcchtig und beklommen . . .", "