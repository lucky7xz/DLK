{"textgrid.poem.35716": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "40. An den Genius der Menschlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg, wer k\u00fchn ins Heiligtum,", "tokens": ["Hin\u00b7weg", ",", "wer", "k\u00fchn", "ins", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unreines Herzens, drang!", "tokens": ["Un\u00b7rei\u00b7nes", "Her\u00b7zens", ",", "drang", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Weins Erschaffer, ihm zum Ruhm", "tokens": ["Des", "Weins", "Er\u00b7schaf\u00b7fer", ",", "ihm", "zum", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6nt der Hochgesang!", "tokens": ["Er\u00b7t\u00f6nt", "der", "Hoch\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es bebt der Saal in G\u00f6tterglanz!", "tokens": ["Es", "bebt", "der", "Saal", "in", "G\u00f6t\u00b7ter\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heil, Heil dir! guter Geist,", "tokens": ["Heil", ",", "Heil", "dir", "!", "gu\u00b7ter", "Geist", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der uns, entw\u00f6hnt des niedern Tands,", "tokens": ["Der", "uns", ",", "ent\u00b7w\u00f6hnt", "des", "nie\u00b7dern", "Tands", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Sturm und Wolken rei\u00dft!", "tokens": ["Durch", "Sturm", "und", "Wol\u00b7ken", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast die Menschen zum Genu\u00df", "tokens": ["Du", "hast", "die", "Men\u00b7schen", "zum", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens erst geweiht,", "tokens": ["Des", "Le\u00b7bens", "erst", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O namenreicher Genius", "tokens": ["O", "na\u00b7men\u00b7rei\u00b7cher", "Ge\u00b7nius"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der edlern Menschlichkeit.", "tokens": ["Der", "ed\u00b7lern", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Du lehrst, als Noah, als Osir,", "tokens": ["Du", "lehrst", ",", "als", "Noah", ",", "als", "O\u00b7sir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NE", "$,", "KOUS", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die rohe Wildnis bau'n;", "tokens": ["Die", "ro\u00b7he", "Wild\u00b7nis", "bau'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Preis dir, Jao! Bacchus, dir!", "tokens": ["Preis", "dir", ",", "Jao", "!", "Bac\u00b7chus", ",", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$.", "NE", "$,", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Erschallt's von gr\u00fcnen Au'n.", "tokens": ["Er\u00b7schallt's", "von", "gr\u00fc\u00b7nen", "Au'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NE", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nach Beer' und Eichel, ungeschlacht,", "tokens": ["Nach", "Beer'", "und", "Ei\u00b7chel", ",", "un\u00b7ge\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchbrach der Mensch den Wald,", "tokens": ["Durch\u00b7brach", "der", "Mensch", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum schlau zum Fischfang und zur Jagd,", "tokens": ["Kaum", "schlau", "zum", "Fischfang", "und", "zur", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und haust' in Kluft und Spalt.", "tokens": ["Und", "haust'", "in", "Kluft", "und", "Spalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Weib und Kind durchheult' um ihn", "tokens": ["Sein", "Weib", "und", "Kind", "durch\u00b7heult'", "um", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sturm, ohn' H\u00fcll' und Glut;", "tokens": ["Den", "Sturm", ",", "ohn'", "H\u00fcll'", "und", "Glut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oft naht' ein Feind, vom Hunger k\u00fchn,", "tokens": ["Oft", "naht'", "ein", "Feind", ",", "vom", "Hun\u00b7ger", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Graun! und schwelgt' in Blut.", "tokens": ["O", "Graun", "!", "und", "schwelgt'", "in", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein Lenz erschien: die Wilde traf", "tokens": ["Dein", "Lenz", "er\u00b7schien", ":", "die", "Wil\u00b7de", "traf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lamm ges\u00e4ugt am Bach!", "tokens": ["Ein", "Lamm", "ge\u00b7s\u00e4ugt", "am", "Bach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie reichte Klee dem frommen Schaf,", "tokens": ["Sie", "reich\u00b7te", "Klee", "dem", "from\u00b7men", "Schaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bl\u00f6kend folgt' es nach.", "tokens": ["Und", "bl\u00f6\u00b7kend", "folgt'", "es", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Herd' und Hund durchschweifte man", "tokens": ["Mit", "Herd'", "und", "Hund", "durch\u00b7schweif\u00b7te", "man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Forthin die \u00f6de Welt;", "tokens": ["For\u00b7thin", "die", "\u00f6\u00b7de", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Die Hirtin melkt' und sang und spann,", "tokens": ["Die", "Hir\u00b7tin", "melkt'", "und", "sang", "und", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wirtlich raucht' ihr Zelt.", "tokens": ["Und", "wirt\u00b7lich", "raucht'", "ihr", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schon milder, trennte schmerzhaft sich", "tokens": ["Schon", "mil\u00b7der", ",", "trenn\u00b7te", "schmerz\u00b7haft", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ADJD", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom sch\u00f6nen Thal die Schar,", "tokens": ["Vom", "sch\u00f6\u00b7nen", "Thal", "die", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ach vom Freund, der nachbarlich", "tokens": ["Und", "ach", "vom", "Freund", ",", "der", "nach\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Trost und Umgang war.", "tokens": ["Ihr", "Trost", "und", "Um\u00b7gang", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da pflanztest du des Landes Frucht", "tokens": ["Da", "pflanz\u00b7test", "du", "des", "Lan\u00b7des", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ins sch\u00f6ne Thal hinein:", "tokens": ["Ins", "sch\u00f6\u00b7ne", "Thal", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Getreid' und Obst in reicher Zucht,", "tokens": ["Ge\u00b7treid'", "und", "Obst", "in", "rei\u00b7cher", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Honig, \u00d6l und Wein.", "tokens": ["Und", "Ho\u00b7nig", ",", "\u00d6l", "und", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Ordnung schm\u00fcckte Dorf und Stadt,", "tokens": ["Die", "Ord\u00b7nung", "schm\u00fcck\u00b7te", "Dorf", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom sch\u00f6nen Volk umbl\u00fcht,", "tokens": ["Vom", "sch\u00f6\u00b7nen", "Volk", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kunst mit Mei\u00dfel, Schnur und Rad,", "tokens": ["Die", "Kunst", "mit", "Mei\u00b7\u00dfel", ",", "Schnur", "und", "Rad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weisheit Red' und Lied.", "tokens": ["Der", "Weis\u00b7heit", "Red'", "und", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vom Staube lehrte himmelw\u00e4rts", "tokens": ["Vom", "Stau\u00b7be", "lehr\u00b7te", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Religion entfliehn,", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und wonnevoll vernahm das Herz", "tokens": ["Und", "won\u00b7ne\u00b7voll", "ver\u00b7nahm", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sph\u00e4ren Harmonien.", "tokens": ["Der", "Sph\u00e4\u00b7ren", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O weh ihm, wessen Hand ein Glied", "tokens": ["O", "weh", "ihm", ",", "wes\u00b7sen", "Hand", "ein", "Glied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PWAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kette frech zerrei\u00dft,", "tokens": ["Der", "Ket\u00b7te", "frech", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sanft empor zur Gottheit zieht", "tokens": ["Die", "sanft", "em\u00b7por", "zur", "Got\u00b7theit", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKVZ", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des G\u00f6ttersohnes Geist!", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7soh\u00b7nes", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Tier des Feldes, w\u00fchlt er nur", "tokens": ["Ein", "Tier", "des", "Fel\u00b7des", ",", "w\u00fchlt", "er", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach schn\u00f6der S\u00e4ttigung;", "tokens": ["Nach", "schn\u00f6\u00b7der", "S\u00e4t\u00b7ti\u00b7gung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ihn labte nie dein Reiz, Natur,", "tokens": ["Ihn", "lab\u00b7te", "nie", "dein", "Reiz", ",", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihn nie des Liedes Schwung!", "tokens": ["Ihn", "nie", "des", "Lie\u00b7des", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Heil, Heil! erhabner Genius", "tokens": ["Heil", ",", "Heil", "!", "er\u00b7hab\u00b7ner", "Ge\u00b7nius"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der edlern Menschlichkeit,", "tokens": ["Der", "ed\u00b7lern", "Menschlich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Sinn' und Herzen zum Genu\u00df", "tokens": ["Der", "Sinn'", "und", "Her\u00b7zen", "zum", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "