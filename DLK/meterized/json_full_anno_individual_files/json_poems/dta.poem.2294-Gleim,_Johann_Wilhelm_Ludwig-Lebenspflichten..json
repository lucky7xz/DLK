{"dta.poem.2294": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lebenspflichten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich mich mit Sorgen qu\u00e4len?", "tokens": ["Soll", "ich", "mich", "mit", "Sor\u00b7gen", "qu\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, so glich ich meiner Mutter;", "tokens": ["Nein", ",", "so", "glich", "ich", "mei\u00b7ner", "Mut\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll ich reichen Narren schmeicheln?", "tokens": ["Soll", "ich", "rei\u00b7chen", "Nar\u00b7ren", "schmei\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, so w\u00fcrd ich selbst zum Narren;", "tokens": ["Nein", ",", "so", "w\u00fcrd", "ich", "selbst", "zum", "Nar\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich meine Br\u00fcder strafen?", "tokens": ["Soll", "ich", "mei\u00b7ne", "Br\u00fc\u00b7der", "stra\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, sie wissen meine Fehler;", "tokens": ["Nein", ",", "sie", "wis\u00b7sen", "mei\u00b7ne", "Feh\u00b7ler", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll ich mir viel Freunde suchen?", "tokens": ["Soll", "ich", "mir", "viel", "Freun\u00b7de", "su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nein, ich werde sie nicht finden;", "tokens": ["Nein", ",", "ich", "wer\u00b7de", "sie", "nicht", "fin\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll ich mir den Himmel w\u00fcnschen?", "tokens": ["Soll", "ich", "mir", "den", "Him\u00b7mel", "w\u00fcn\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nein, dann w\u00fcnscht ich ia zu sterben.", "tokens": ["Nein", ",", "dann", "w\u00fcnscht", "ich", "i\u00b7a", "zu", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Soll ich an der Welt was tadeln?", "tokens": ["Soll", "ich", "an", "der", "Welt", "was", "ta\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PWS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nein, sie wird nicht besser werden;", "tokens": ["Nein", ",", "sie", "wird", "nicht", "bes\u00b7ser", "wer\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Soll ich trinken? Soll ich lieben?", "tokens": ["Soll", "ich", "trin\u00b7ken", "?", "Soll", "ich", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Soll ich tanzen? Soll ich lachen?", "tokens": ["Soll", "ich", "tan\u00b7zen", "?", "Soll", "ich", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Soll ich mich mit Rosen kr\u00f6nen?", "tokens": ["Soll", "ich", "mich", "mit", "Ro\u00b7sen", "kr\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Soll ich schmausen? Soll ich k\u00fcssen?", "tokens": ["Soll", "ich", "schmau\u00b7sen", "?", "Soll", "ich", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Soll ich spielen? Soll ich scherzen?", "tokens": ["Soll", "ich", "spie\u00b7len", "?", "Soll", "ich", "scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Soll ich mich um nichts bek\u00fcmmern?", "tokens": ["Soll", "ich", "mich", "um", "nichts", "be\u00b7k\u00fcm\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Soll ich mit den Sch\u00f6nen t\u00e4ndeln?", "tokens": ["Soll", "ich", "mit", "den", "Sch\u00f6\u00b7nen", "t\u00e4n\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Ja, dis soll ich, und mein Vater", "tokens": ["Ja", ",", "dis", "soll", "ich", ",", "und", "mein", "Va\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Lehrt es mich bei grauen Haaren,", "tokens": ["Lehrt", "es", "mich", "bei", "grau\u00b7en", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und er nennt es: Lebenspflichten.", "tokens": ["Und", "er", "nennt", "es", ":", "Le\u00b7bens\u00b7pflich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}