{"dta.poem.2630": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uberdru\u00df Menschlichen Lebens/  \n Bey Beerdigung Hn. J. L. D. zu St. E.  \n den 18. Junii 1671.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So schleust du/ Seeliger/ die Wallfarth deiner Tage/", "tokens": ["So", "schleust", "du", "/", "See\u00b7li\u00b7ger", "/", "die", "Wall\u00b7farth", "dei\u00b7ner", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schickst den m\u00fcden Leib zu der gew\u00fcnschten Ruh\u2019!", "tokens": ["Und", "schickst", "den", "m\u00fc\u00b7den", "Leib", "zu", "der", "ge\u00b7w\u00fcnschten", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So hat sein End\u2019 erreicht des Leidens lange Plage/", "tokens": ["So", "hat", "sein", "End'", "er\u00b7reicht", "des", "Lei\u00b7dens", "lan\u00b7ge", "Pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und di\u00df was irrdisch war/ deckt Erde wieder zu!", "tokens": ["Und", "di\u00df", "was", "irr\u00b7disch", "war", "/", "deckt", "Er\u00b7de", "wie\u00b7der", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "ADJD", "VAFIN", "$(", "VVFIN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es konte dir die Welt mit ihren schn\u00f6den Sachen/", "tokens": ["Es", "kon\u00b7te", "dir", "die", "Welt", "mit", "ih\u00b7ren", "schn\u00f6\u00b7den", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So blosses Blend-Werck sind und eitles Gauckelspiel/", "tokens": ["So", "blos\u00b7ses", "Blen\u00b7d\u00b7Werck", "sind", "und", "eit\u00b7les", "Gau\u00b7ckel\u00b7spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Nur Eckel und Verdru\u00df hinfort zu leben machen/", "tokens": ["Nur", "E\u00b7ckel", "und", "Ver\u00b7dru\u00df", "hin\u00b7fort", "zu", "le\u00b7ben", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil deiner Seele nicht ihr b\u00f6ses Thun gefiel.", "tokens": ["Weil", "dei\u00b7ner", "See\u00b7le", "nicht", "ihr", "b\u00f6\u00b7ses", "Thun", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du sahft mit Salomon da\u00df alles alles eitel/", "tokens": ["Du", "sahft", "mit", "Sa\u00b7lo\u00b7mon", "da\u00df", "al\u00b7les", "al\u00b7les", "ei\u00b7tel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "KOUS", "PIS", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und w\u00fcnscht mit Davids-Mund von Mesech weg zu gehn.", "tokens": ["Und", "w\u00fcnscht", "mit", "Da\u00b7vids\u00b7Mund", "von", "Me\u00b7sech", "weg", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja weil wir gantz verderbt vom Fu\u00dfbret bi\u00df zur Scheitel/", "tokens": ["Ja", "weil", "wir", "gantz", "ver\u00b7derbt", "vom", "Fu\u00df\u00b7bret", "bi\u00df", "zur", "Schei\u00b7tel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "ADV", "ADJD", "APPRART", "NN", "APPR", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So muste Hiob dir zu dem Bewei\u00dfthumb stehn.", "tokens": ["So", "mus\u00b7te", "Hiob", "dir", "zu", "dem", "Be\u00b7wei\u00df\u00b7thumb", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.13": {"text": "Sein abgemartert Leib und au\u00dfgekreischte Knochen/", "tokens": ["Sein", "ab\u00b7ge\u00b7mar\u00b7tert", "Leib", "und", "au\u00df\u00b7ge\u00b7kreischte", "Kno\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Das Eyter volle Fleisch/ der he\u00dfliche Gestanck/", "tokens": ["Das", "Ey\u00b7ter", "vol\u00b7le", "Fleisch", "/", "der", "he\u00df\u00b7li\u00b7che", "Ge\u00b7stanck", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Glied er hin und her vom F\u00e4ulni\u00df gantz durchkrochen/", "tokens": ["Die", "Glied", "er", "hin", "und", "her", "vom", "F\u00e4ul\u00b7ni\u00df", "gantz", "durch\u00b7kro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "KON", "ADV", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Er selbst/ sein Folterer und eigne Folterbanck/", "tokens": ["Er", "selbst", "/", "sein", "Fol\u00b7te\u00b7rer", "und", "eig\u00b7ne", "Fol\u00b7ter\u00b7banck", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Belehrten/ was der Mensch/ und sein verg\u00e4nglich Wesen/", "tokens": ["Be\u00b7lehr\u00b7ten", "/", "was", "der", "Mensch", "/", "und", "sein", "ver\u00b7g\u00e4ng\u00b7lich", "We\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "ART", "NN", "$(", "KON", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie er noch lebendig sich schon zu Grabe tr\u00e4gt;", "tokens": ["Wie", "er", "noch", "le\u00b7ben\u00b7dig", "sich", "schon", "zu", "Gra\u00b7be", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Da\u00df immer Noth auf Noth/ so bald er nur genesen/", "tokens": ["Da\u00df", "im\u00b7mer", "Noth", "auf", "Noth", "/", "so", "bald", "er", "nur", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "APPR", "NN", "$(", "ADV", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auf seines Lebens Schiff/ gleich einer Welle/ schl\u00e4gt.", "tokens": ["Auf", "sei\u00b7nes", "Le\u00b7bens", "Schiff", "/", "gleich", "ei\u00b7ner", "Wel\u00b7le", "/", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "ADV", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und vielen wird der Leib der Mutter auch zum Grabe/", "tokens": ["Und", "vie\u00b7len", "wird", "der", "Leib", "der", "Mut\u00b7ter", "auch", "zum", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Viel fallen Blumen gleich im ersten Fr\u00fchling hin.", "tokens": ["Viel", "fal\u00b7len", "Blu\u00b7men", "gleich", "im", "ers\u00b7ten", "Fr\u00fch\u00b7ling", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gesetzt schon/ da\u00df der Mensch von Jahren etwas habe/", "tokens": ["Ge\u00b7setzt", "schon", "/", "da\u00df", "der", "Mensch", "von", "Jah\u00b7ren", "et\u00b7was", "ha\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "NN", "APPR", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Raubt nicht die Eitelkeit den Rest noch zum Gewin?", "tokens": ["Raubt", "nicht", "die", "Ei\u00b7tel\u00b7keit", "den", "Rest", "noch", "zum", "Ge\u00b7win", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was ist die Wissenschafft/ in welcher wir uns br\u00fcsten?", "tokens": ["Was", "ist", "die", "Wis\u00b7sen\u00b7schafft", "/", "in", "wel\u00b7cher", "wir", "uns", "br\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein Wahn/ der meistentheils aus falschem Grunde quillt.", "tokens": ["Ein", "Wahn", "/", "der", "meis\u00b7ten\u00b7theils", "aus", "fal\u00b7schem", "Grun\u00b7de", "quillt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was ist die Wei\u00dfheit denn/ nach der so viel gel\u00fcsten?", "tokens": ["Was", "ist", "die", "Wei\u00df\u00b7heit", "denn", "/", "nach", "der", "so", "viel", "ge\u00b7l\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "$(", "APPR", "ART", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein eingebildtes Thun/ so bey sich selbst nur gilt.", "tokens": ["Ein", "ein\u00b7ge\u00b7bild\u00b7tes", "Thun", "/", "so", "bey", "sich", "selbst", "nur", "gilt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Was ist denn der Verstand? ein unvollkommen Sinnen.", "tokens": ["Was", "ist", "denn", "der", "Ver\u00b7stand", "?", "ein", "un\u00b7voll\u00b7kom\u00b7men", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was die Erforschung denn? gantz eine leere M\u00fch.", "tokens": ["Was", "die", "Er\u00b7for\u00b7schung", "denn", "?", "gantz", "ei\u00b7ne", "lee\u00b7re", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "$.", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df Reichthum? ein solch Gut/ das fl\u00fcchtig mu\u00df zerrinnen.", "tokens": ["Da\u00df", "Reicht\u00b7hum", "?", "ein", "solch", "Gut", "/", "das", "fl\u00fcch\u00b7tig", "mu\u00df", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$.", "ART", "PIAT", "NN", "$(", "ART", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Sch\u00f6nheit der Gestalt? der Augen Phantasie.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "der", "Ge\u00b7stalt", "?", "der", "Au\u00b7gen", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was die Gerechtigkeit? ein Tuch/ das/ sehr beflecket.", "tokens": ["Was", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "?", "ein", "Tuch", "/", "das", "/", "sehr", "be\u00b7fle\u00b7cket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$.", "ART", "NN", "$(", "ART", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was Hoheit? ein glatt Ei\u00df/ auf dem man leichtlich f\u00e4llt.", "tokens": ["Was", "Ho\u00b7heit", "?", "ein", "glatt", "Ei\u00df", "/", "auf", "dem", "man", "leicht\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "ART", "ADJD", "NN", "$(", "APPR", "PRELS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was m\u00e4chtige Gewalt? ein Nest/ das S\u00fcnden hecket:", "tokens": ["Was", "m\u00e4ch\u00b7ti\u00b7ge", "Ge\u00b7walt", "?", "ein", "Nest", "/", "das", "S\u00fcn\u00b7den", "he\u00b7cket", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$.", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und kurtz; ein Schattenwerck ist aller Pacht der Welt.", "tokens": ["Und", "kurtz", ";", "ein", "Schat\u00b7ten\u00b7werck", "ist", "al\u00b7ler", "Pacht", "der", "Welt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "ART", "NN", "VAFIN", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wenn nun ein feurig Geist in Himmel gantz verliebet/", "tokens": ["Wenn", "nun", "ein", "feu\u00b7rig", "Geist", "in", "Him\u00b7mel", "gantz", "ver\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und von der Ewigkeit schon einen Vorschmack f\u00fchlt/", "tokens": ["Und", "von", "der", "E\u00b7wig\u00b7keit", "schon", "ei\u00b7nen", "Vor\u00b7schmack", "f\u00fchlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das Marter-Haus den Leib gro\u00dfm\u00fctig \u00fcbergiebet/", "tokens": ["Das", "Mar\u00b7ter\u00b7Haus", "den", "Leib", "gro\u00df\u00b7m\u00fc\u00b7tig", "\u00fc\u00b7berg\u00b7ie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wer zweiffelt/ da\u00df er nicht den besten Zweck erziehlt?", "tokens": ["Wer", "zweif\u00b7felt", "/"