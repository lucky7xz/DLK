{"dta.poem.9820": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft.  \n Der seeligen Fr. v. B***.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt die B***. das beyspiel wahrer liebe/", "tokens": ["Hjer", "liegt", "die", "B", "*", "*", "*", ".", "das", "bey\u00b7spiel", "wah\u00b7rer", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "XY", "XY", "XY", "XY", "$.", "ART", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die/ was sie nur gehabt/ gut/ sch\u00f6nheit und verstand/", "tokens": ["Die", "/", "was", "sie", "nur", "ge\u00b7habt", "/", "gut", "/", "sch\u00f6n\u00b7heit", "und", "ver\u00b7stand", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADV", "VAPP", "$(", "ADJD", "$(", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr leben und ihr hertz dem ehmann zugewandt/", "tokens": ["Ihr", "le\u00b7ben", "und", "ihr", "hertz", "dem", "eh\u00b7mann", "zu\u00b7ge\u00b7wandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nichts von ihm begehrt/ denn da\u00df er sie begr\u00fcbe.", "tokens": ["Und", "nichts", "von", "ihm", "be\u00b7gehrt", "/", "denn", "da\u00df", "er", "sie", "be\u00b7gr\u00fc\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVPP", "$(", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drey kinder hatte sie ihm h\u00f6chst vergn\u00fcgt gebracht/", "tokens": ["Drey", "kin\u00b7der", "hat\u00b7te", "sie", "ihm", "h\u00f6chst", "ver\u00b7gn\u00fcgt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die gesetzte zahl der liebe vollgemacht.", "tokens": ["Und", "die", "ge\u00b7setz\u00b7te", "zahl", "der", "lie\u00b7be", "voll\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch weil sie auch dadurch/ worzu sie war gekommen/", "tokens": ["Doch", "weil", "sie", "auch", "da\u00b7durch", "/", "wor\u00b7zu", "sie", "war", "ge\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV", "$(", "PWAV", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr gantzes liebeswerck vollf\u00fchret und erf\u00fcllt/", "tokens": ["Ihr", "gant\u00b7zes", "lie\u00b7bes\u00b7werck", "voll\u00b7f\u00fch\u00b7ret", "und", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ward sie dem zum bewei\u00df/ als wie ein liebes-bild/", "tokens": ["Ward", "sie", "dem", "zum", "be\u00b7wei\u00df", "/", "als", "wie", "ein", "lie\u00b7bes\u00b7bild", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "APPRART", "ADJD", "$(", "KOUS", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit einem kind\u2019 im arm/ in himmel aufgenommen.", "tokens": ["Mit", "ei\u00b7nem", "kind'", "im", "arm", "/", "in", "him\u00b7mel", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJD", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun tr\u00e4gt sie dort den lohn der ehelichen treu;", "tokens": ["Nun", "tr\u00e4gt", "sie", "dort", "den", "lohn", "der", "e\u00b7he\u00b7li\u00b7chen", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df aber sie auch hier nicht unbelohnet sey/", "tokens": ["Da\u00df", "a\u00b7ber", "sie", "auch", "hier", "nicht", "un\u00b7be\u00b7loh\u00b7net", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mu\u00df sie (o schwerer dienst!) ihr ehmann selbst begraben.", "tokens": ["Mu\u00df", "sie", "(", "o", "schwe\u00b7rer", "dienst", "!", ")", "ihr", "eh\u00b7mann", "selbst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "ADJA", "NN", "$.", "$(", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df ist die danckbarkeit/ die sie hat wollen haben.", "tokens": ["Di\u00df", "ist", "die", "dan\u00b7ck\u00b7bar\u00b7keit", "/", "die", "sie", "hat", "wol\u00b7len", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER", "VAFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}