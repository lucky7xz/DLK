{"dta.poem.12048": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00fcter unter'm Werth ver\u00e4chtlich anzuschlagen,", "tokens": ["Die", "G\u00fc\u00b7ter", "un\u00b7ter'm", "Werth", "ver\u00b7\u00e4cht\u00b7lich", "an\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herabzusetzen sie, um leichter zu entsagen,", "tokens": ["Her\u00b7ab\u00b7zu\u00b7set\u00b7zen", "sie", ",", "um", "leich\u00b7ter", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist nur ein Kunstgriff, der wo's gilt dich l\u00e4\u00dft in Stich.", "tokens": ["Ist", "nur", "ein", "Kunst\u00b7griff", ",", "der", "wo's", "gilt", "dich", "l\u00e4\u00dft", "in", "Stich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "NE", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel anders als du dich gedacht hast, f\u00fchlst du dich.", "tokens": ["Viel", "an\u00b7ders", "als", "du", "dich", "ge\u00b7dacht", "hast", ",", "f\u00fchlst", "du", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man f\u00fchlt, was man gehabt, wann man es lassen mu\u00df;", "tokens": ["Man", "f\u00fchlt", ",", "was", "man", "ge\u00b7habt", ",", "wann", "man", "es", "las\u00b7sen", "mu\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PIS", "VAPP", "$,", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was hilft es, sich zuvor verk\u00fcmmern den Genu\u00df?", "tokens": ["Was", "hilft", "es", ",", "sich", "zu\u00b7vor", "ver\u00b7k\u00fcm\u00b7mern", "den", "Ge\u00b7nu\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PRF", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum la\u00df in ihrem Werth die G\u00fcter fein bestehn,", "tokens": ["Drum", "la\u00df", "in", "ih\u00b7rem", "Werth", "die", "G\u00fc\u00b7ter", "fein", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besonnen im Besitz, besonnen im Entgehn.", "tokens": ["Be\u00b7son\u00b7nen", "im", "Be\u00b7sitz", ",", "be\u00b7son\u00b7nen", "im", "Ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}