{"textgrid.poem.54144": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Shaw. So ernst, wie der heiter tut, ist er gar nicht.", "tokens": ["Shaw", ".", "So", "ernst", ",", "wie", "der", "hei\u00b7ter", "tut", ",", "ist", "er", "gar", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Golf, sagte einmal jemand, ist ein verdorbener Spaziergang.", "tokens": ["Golf", ",", "sag\u00b7te", "ein\u00b7mal", "je\u00b7mand", ",", "ist", "ein", "ver\u00b7dor\u00b7be\u00b7ner", "Spa\u00b7zier\u00b7gang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PIS", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+---+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "N\u00e4hme man den Zeitungen den Fettdruck \u2013: um wieviel stiller w\u00e4re es in der Welt \u2013!", "tokens": ["N\u00e4h\u00b7me", "man", "den", "Zei\u00b7tun\u00b7gen", "den", "Fett\u00b7druck", "\u2013", ":", "um", "wie\u00b7viel", "stil\u00b7ler", "w\u00e4\u00b7re", "es", "in", "der", "Welt", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "NN", "$(", "$.", "APPR", "PWAV", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+---+--+-+-+--+-+", "me