{"textgrid.poem.51294": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Seele, Geist und Leib den wilden Wellen traut,", "tokens": ["Der", "See\u00b7le", ",", "Geist", "und", "Leib", "den", "wil\u00b7den", "Wel\u00b7len", "traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in die H\u00f6lle steigt und sich nach Geld umschaut,", "tokens": ["Der", "in", "die", "H\u00f6l\u00b7le", "steigt", "und", "sich", "nach", "Geld", "um\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der seine S\u00fcnden l\u00e4st in Marmel Ha\u00fcser schlie\u00dfen,", "tokens": ["Der", "sei\u00b7ne", "S\u00fcn\u00b7den", "l\u00e4st", "in", "Mar\u00b7mel", "Ha\u00fc\u00b7ser", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der geitzig laufft und rennt umb Wollust zu genie\u00dfen.", "tokens": ["Der", "geit\u00b7zig", "laufft", "und", "rennt", "umb", "Wol\u00b7lust", "zu", "ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN