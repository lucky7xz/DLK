{"dta.poem.12710": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Herr H\u00f6ltzeln   Med. D.  \n D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war der krancken artzt, und offt des todes tod;", "tokens": ["Ich", "war", "der", "kran\u00b7cken", "artzt", ",", "und", "offt", "des", "to\u00b7des", "tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht die verwechselung! itzt wird er mir zum leben,", "tokens": ["Seht", "die", "ver\u00b7wech\u00b7se\u00b7lung", "!", "itzt", "wird", "er", "mir", "zum", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "PPER", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das in der ewigkeit GOtt pflegt durch ihn zu geben:", "tokens": ["Das", "in", "der", "e\u00b7wig\u00b7keit", "Gott", "pflegt", "durch", "ihn", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O \u00fcberlebter tod! o freuden-reiche noth!", "tokens": ["O", "\u00fc\u00b7ber\u00b7leb\u00b7ter", "tod", "!", "o", "freu\u00b7den\u00b7rei\u00b7che", "noth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das holtz im paradie\u00df macht, da\u00df ich auch mu\u00df sterben,", "tokens": ["Das", "holtz", "im", "pa\u00b7ra\u00b7die\u00df", "macht", ",", "da\u00df", "ich", "auch", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der an dem holtze starb l\u00e4st mich das leben erben.", "tokens": ["Der", "an", "dem", "holt\u00b7ze", "starb", "l\u00e4st", "mich", "das", "le\u00b7ben", "er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "VVFIN", "PPER", "PDS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}