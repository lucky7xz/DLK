{"textgrid.poem.39490": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Vorwitz, nur die Ruh des weisen Alten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Vorwitz, nur die Ruh des weisen Alten", "tokens": ["Wenn", "Vor\u00b7witz", ",", "nur", "die", "Ruh", "des", "wei\u00b7sen", "Al\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu st\u00f6ren, drang in Proteus d\u00fcstre Grotte,", "tokens": ["Zu", "st\u00f6\u00b7ren", ",", "drang", "in", "Pro\u00b7teus", "d\u00fcst\u00b7re", "Grot\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "APPR", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wandelt' er dem Frager sich zum Spotte", "tokens": ["So", "wan\u00b7delt'", "er", "dem", "Fra\u00b7ger", "sich", "zum", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In tausend unvernehmliche Gestalten.", "tokens": ["In", "tau\u00b7send", "un\u00b7ver\u00b7nehm\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch wen Begeistrung trieb, ihn festzuhalten,", "tokens": ["Doch", "wen", "Be\u00b7geis\u00b7trung", "trieb", ",", "ihn", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "t