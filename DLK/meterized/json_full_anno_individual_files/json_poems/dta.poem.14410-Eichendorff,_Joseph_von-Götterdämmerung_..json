{"dta.poem.14410": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da in den lichten R\u00e4umen", "tokens": ["Da", "in", "den", "lich\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erwacht das Menschenkind,", "tokens": ["Er\u00b7wacht", "das", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fcttelt b\u00f6ses Tr\u00e4umen", "tokens": ["Und", "sch\u00fct\u00b7telt", "b\u00f6\u00b7ses", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von seinem Haupt geschwind.", "tokens": ["Von", "sei\u00b7nem", "Haupt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}