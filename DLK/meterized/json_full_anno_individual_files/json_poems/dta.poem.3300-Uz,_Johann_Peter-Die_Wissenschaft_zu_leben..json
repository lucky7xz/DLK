{"dta.poem.3300": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Wissenschaft zu leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer und vielleicht der gr\u00f6\u00dfte Theil des Lebens,", "tokens": ["Ein", "gro\u00b7\u00dfer", "und", "viel\u00b7leicht", "der", "gr\u00f6\u00df\u00b7te", "Theil", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir die Parce zugedacht,", "tokens": ["Das", "mir", "die", "Par\u00b7ce", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schlich, wie ein Traum der Nacht,", "tokens": ["Schlich", ",", "wie", "ein", "Traum", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit leisen Fl\u00fcgeln hin, und war vielleicht vergebens!", "tokens": ["Mit", "lei\u00b7sen", "Fl\u00fc\u00b7geln", "hin", ",", "und", "war", "viel\u00b7leicht", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vergebens flammten mir so vieler Tage Sonnen,", "tokens": ["Ver\u00b7ge\u00b7bens", "flamm\u00b7ten", "mir", "so", "vie\u00b7ler", "Ta\u00b7ge", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich, vom Sch\u00f6pfer aufgestellt,", "tokens": ["Wenn", "ich", ",", "vom", "Sch\u00f6p\u00b7fer", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als B\u00fcrger einer Welt,", "tokens": ["Als", "B\u00fcr\u00b7ger", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch eine gute That nicht ieden Tag gewonnen:", "tokens": ["Durch", "ei\u00b7ne", "gu\u00b7te", "That", "nicht", "ie\u00b7den", "Tag", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKNEG", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn ich der Tugend Freund und gro\u00df durch Men-", "tokens": ["Wenn", "ich", "der", "Tu\u00b7gend", "Freund", "und", "gro\u00df", "durch", "Men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "schenliebe,", "tokens": ["schen\u00b7lie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Frey von des Wahnes Tyranney,", "tokens": ["Frey", "von", "des", "Wah\u00b7nes", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wahrhaftig gro\u00df und frey,", "tokens": ["Wahr\u00b7haf\u00b7tig", "gro\u00df", "und", "frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erst werden soll, nicht bin, und es zu seyn verschiebe.", "tokens": ["Erst", "wer\u00b7den", "soll", ",", "nicht", "bin", ",", "und", "es", "zu", "seyn", "ver\u00b7schie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "VMFIN", "$,", "PTKNEG", "VAFIN", "$,", "KON", "PPER", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie? wer nach Golde geizt, obgleich kein Gold", "tokens": ["Wie", "?", "wer", "nach", "Gol\u00b7de", "geizt", ",", "ob\u00b7gleich", "kein", "Gold"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "PWS", "APPR", "NN", "VVPP", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "begl\u00fccket,", "tokens": ["be\u00b7gl\u00fc\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Braucht alle Stunden zum Gewinn,", "tokens": ["Braucht", "al\u00b7le", "Stun\u00b7den", "zum", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4uft nach Wucher hin,", "tokens": ["Und", "l\u00e4uft", "nach", "Wu\u00b7cher", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wann kaum der junge Tag aus weissen Wolken blicket.", "tokens": ["Wann", "kaum", "der", "jun\u00b7ge", "Tag", "aus", "weis\u00b7sen", "Wol\u00b7ken", "bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Inde\u00df die halbe Welt, vom sanften Schlaf umflogen,", "tokens": ["In\u00b7de\u00df", "die", "hal\u00b7be", "Welt", ",", "vom", "sanf\u00b7ten", "Schlaf", "um\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In bleicher D\u00e4mmrung Stille tr\u00e4umt;", "tokens": ["In", "blei\u00b7cher", "D\u00e4mm\u00b7rung", "Stil\u00b7le", "tr\u00e4umt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat jener, unges\u00e4umt,", "tokens": ["Hat", "je\u00b7ner", ",", "un\u00b7ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schon Gelder angelegt, schon Zinsen abgezogen.", "tokens": ["Schon", "Gel\u00b7der", "an\u00b7ge\u00b7legt", ",", "schon", "Zin\u00b7sen", "ab\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wir leben niemals heut! wir schieben auf, zu le-", "tokens": ["Wir", "le\u00b7ben", "nie\u00b7mals", "heut", "!", "wir", "schie\u00b7ben", "auf", ",", "zu", "le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "ben,", "tokens": ["ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Bis einst ein g\u00fcnstiges Geschick", "tokens": ["Bis", "einst", "ein", "g\u00fcns\u00b7ti\u00b7ges", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns ein getr\u00e4umtes Gl\u00fcck", "tokens": ["Uns", "ein", "ge\u00b7tr\u00e4um\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach Vorschrift unsers Plans und Eigensinns gegeben.", "tokens": ["Nach", "Vor\u00b7schrift", "un\u00b7sers", "Plans", "und", "Ei\u00b7gen\u00b7sinns", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So stark herrscht \u00fcberall der Thorheit alter Glaube,", "tokens": ["So", "stark", "herrscht", "\u00fc\u00b7be\u00b7rall", "der", "Thor\u00b7heit", "al\u00b7ter", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als k\u00f6nnten wir uns nicht erfreun,", "tokens": ["Als", "k\u00f6nn\u00b7ten", "wir", "uns", "nicht", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht weis\u2019 und gl\u00fccklich seyn", "tokens": ["Nicht", "weis'", "und", "gl\u00fcck\u00b7lich", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKVZ", "KON", "ADJD", "VAINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In einem ieden Stand, im Purpur und im Staube!", "tokens": ["In", "ei\u00b7nem", "ie\u00b7den", "Stand", ",", "im", "Pur\u00b7pur", "und", "im", "Stau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Auf Bluhmen seh ich hier den armen Landmann lie-", "tokens": ["Auf", "Bluh\u00b7men", "seh", "ich", "hier", "den", "ar\u00b7men", "Land\u00b7mann", "lie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen,", "tokens": ["gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Den ein gepachtet karges Feld", "tokens": ["Den", "ein", "ge\u00b7pach\u00b7tet", "kar\u00b7ges", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur k\u00fcmmerlich erh\u00e4lt:", "tokens": ["Nur", "k\u00fcm\u00b7mer\u00b7lich", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um seine braune Stirn lacht ruhiges Vergn\u00fcgen.", "tokens": ["Um", "sei\u00b7ne", "brau\u00b7ne", "Stirn", "lacht", "ru\u00b7hi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Er lebt, wann sein Tyrann, der ieden Tag bethr\u00e4net,", "tokens": ["Er", "lebt", ",", "wann", "sein", "Ty\u00b7rann", ",", "der", "ie\u00b7den", "Tag", "be\u00b7th\u00b7r\u00e4\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sich um das Leben selbst betr\u00fcgt,", "tokens": ["Sich", "um", "das", "Le\u00b7ben", "selbst", "be\u00b7tr\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, immer unvergn\u00fcgt,", "tokens": ["Und", ",", "im\u00b7mer", "un\u00b7ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Reich, aber hungrig stets, nach gr\u00f6sserm Reichthum g\u00e4h-", "tokens": ["Reich", ",", "a\u00b7ber", "hung\u00b7rig", "stets", ",", "nach", "gr\u00f6s\u00b7serm", "Reicht\u00b7hum", "g\u00e4h"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADJD", "ADV", "$,", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "net.", "tokens": ["net", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}}, "stanza.10": {"line.1": {"text": "Doch Chlotho wartet nicht, bis wir genug erlangen;", "tokens": ["Doch", "Chlo\u00b7tho", "war\u00b7tet", "nicht", ",", "bis", "wir", "ge\u00b7nug", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wann sie uns zur k\u00fchlen Gruft", "tokens": ["Und", "wann", "sie", "uns", "zur", "k\u00fch\u00b7len", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in die Stille ruft,", "tokens": ["Und", "in", "die", "Stil\u00b7le", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So haben viele nie zu leben angefangen.", "tokens": ["So", "ha\u00b7ben", "vie\u00b7le", "nie", "zu", "le\u00b7ben", "an\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}