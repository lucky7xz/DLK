{"dta.poem.8994": {"metadata": {"author": {"name": "Schmolck, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.  \n  Die gecreutzigte Liebe.  \n Am Char-Freytage.  \n Melod. Wohlan es geht nunmehr zum Ende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1712", "urn": "urn:nbn:de:kobv:b4-20543-0", "language": ["de:0.99"], "booktitle": "Schmolck, Benjamin: Der Lustige Sabbath. Jauer u. a., 1712."}, "poem": {"stanza.1": {"line.1": {"text": "1. (ben/", "tokens": ["(", "ben", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Mein bester Freund ist mir gestor-", "tokens": ["Mein", "bes\u00b7ter", "Freund", "ist", "mir", "ge\u00b7stor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach sollt ich nicht im Leide gehn?", "tokens": ["Ach", "sollt", "ich", "nicht", "im", "Lei\u00b7de", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mir den Himmel hat erworben/", "tokens": ["Der", "mir", "den", "Him\u00b7mel", "hat", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den seh ich auff der Bahre stehn.", "tokens": ["Den", "seh", "ich", "auff", "der", "Bah\u00b7re", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mir das Leben hat gebracht/", "tokens": ["Der", "mir", "das", "Le\u00b7ben", "hat", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Versinckt in schwartze Todes-Nacht", "tokens": ["Ver\u00b7sinckt", "in", "schwart\u00b7ze", "To\u00b7des\u00b7Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wie ist mir/ seh ich JEsum sterben?", "tokens": ["Wie", "ist", "mir", "/", "seh", "ich", "Je\u00b7sum", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$(", "VVFIN", "PPER", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach ja! ich seh es allzu klar/", "tokens": ["Ach", "ja", "!", "ich", "seh", "es", "all\u00b7zu", "klar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sich die blassen Lippen f\u00e4rben/", "tokens": ["Wie", "sich", "die", "blas\u00b7sen", "Lip\u00b7pen", "f\u00e4r\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Antlitz stellt die Sonne dar/", "tokens": ["Sein", "Ant\u00b7litz", "stellt", "die", "Son\u00b7ne", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn sie zu R\u00fcste gangen ist/", "tokens": ["Wenn", "sie", "zu", "R\u00fcs\u00b7te", "gan\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und allen ihren Glantz verschl\u00fc\u00dft.", "tokens": ["Und", "al\u00b7len", "ih\u00b7ren", "Glantz", "ver\u00b7schl\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Die Augen sind nicht nur gebrochen/", "tokens": ["Die", "Au\u00b7gen", "sind", "nicht", "nur", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil schon das Hertze selbsten bricht.", "tokens": ["Weil", "schon", "das", "Hert\u00b7ze", "selbs\u00b7ten", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum ist das letzte Wort gesprochen/", "tokens": ["Kaum", "ist", "das", "letz\u00b7te", "Wort", "ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da man von seinem Tode spr", "tokens": ["Da", "man", "von", "sei\u00b7nem", "To\u00b7de", "spr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Haupt/ das so viel Strahlen", "tokens": ["Das", "Haupt", "/", "das", "so", "viel", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PDS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "zeigt/", "tokens": ["zeigt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Hat sich zur Erden schon geneigt.", "tokens": ["Hat", "sich", "zur", "Er\u00b7den", "schon", "ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. (f\u00e4hret/", "tokens": ["(", "f\u00e4h\u00b7ret", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "O Schwerdt/ das meine Brust durch-", "tokens": ["O", "Schwerdt", "/", "das", "mei\u00b7ne", "Brust", "durch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "PDS", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Todt/ der mich zum Todten macht.", "tokens": ["O", "Todt", "/", "der", "mich", "zum", "Tod\u00b7ten", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Hertz ist mir gantz umbgekehret/", "tokens": ["Mein", "Hertz", "ist", "mir", "gantz", "umb\u00b7ge\u00b7keh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Indem es seinen Freund betracht/", "tokens": ["In\u00b7dem", "es", "sei\u00b7nen", "Freund", "be\u00b7tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie er den letzten Abschieds-Ku\u00df", "tokens": ["Wie", "er", "den", "letz\u00b7ten", "Ab\u00b7schieds\u00b7Ku\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit kalten Lippen geben mu\u00df.", "tokens": ["Mit", "kal\u00b7ten", "Lip\u00b7pen", "ge\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. (len/", "tokens": ["(", "len", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Die Sonne selbst versteckt die Strah-", "tokens": ["Die", "Son\u00b7ne", "selbst", "ver\u00b7steckt", "die", "Strah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Himmel kreucht in einen Sack.", "tokens": ["Der", "Him\u00b7mel", "kreucht", "in", "ei\u00b7nen", "Sack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Echo rufft zu tausend mahlen:", "tokens": ["Das", "E\u00b7cho", "rufft", "zu", "tau\u00b7send", "mah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist ein rechter Trauer-Tag.", "tokens": ["Das", "ist", "ein", "rech\u00b7ter", "Trau\u00b7e\u00b7rTag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Felsen Ritz macht ein Geschrey/", "tokens": ["Der", "Fel\u00b7sen", "Ritz", "macht", "ein", "Ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df GOttes Sohn gestorben sey.", "tokens": ["Da\u00df", "Got\u00b7tes", "Sohn", "ge\u00b7stor\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "6.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ach sterbet in mir alle Kr\u00e4ffte/", "tokens": ["Ach", "ster\u00b7bet", "in", "mir", "al\u00b7le", "Kr\u00e4ff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "APPR", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil JEsus stirbt/ ist meine Pflicht/", "tokens": ["Weil", "Je\u00b7sus", "stirbt", "/", "ist", "mei\u00b7ne", "Pflicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$(", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich mich an sein Creutze heffte/", "tokens": ["Da\u00df", "ich", "mich", "an", "sein", "Creut\u00b7ze", "heff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da mein Hertz an dem seinen bricht.", "tokens": ["Da", "mein", "Hertz", "an", "dem", "sei\u00b7nen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach JEsu nihm mein Leben hin/", "tokens": ["Ach", "Je\u00b7su", "nihm", "mein", "Le\u00b7ben", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich ruh nicht/ bi\u00df ich bey dir bin.", "tokens": ["Ich", "ruh", "nicht", "/", "bi\u00df", "ich", "bey", "dir", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "7.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Erblaste Lippen/ last euch k\u00fcssen/", "tokens": ["Er\u00b7blas\u00b7te", "Lip\u00b7pen", "/", "last", "euch", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr trifft von lauter Honig noch.", "tokens": ["Ihr", "trifft", "von", "lau\u00b7ter", "Ho\u00b7nig", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df dich/ geneigtes Haupt umschl\u00fcss\u1ebd/", "tokens": ["La\u00df", "dich", "/", "ge\u00b7neig\u00b7tes", "Haupt", "um\u00b7schl\u00fcss\u1ebd", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das mir nach lauter Balsam rvch.", "tokens": ["Das", "mir", "nach", "lau\u00b7ter", "Bal\u00b7sam", "rvch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erlaube/ tieffer Seiten-Ritz/", "tokens": ["Er\u00b7lau\u00b7be", "/", "tief\u00b7fer", "Sei\u00b7ten\u00b7Ritz", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dir mir einen Tauben-Sitz.", "tokens": ["In", "dir", "mir", "ei\u00b7nen", "Tau\u00b7ben\u00b7Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "8.", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Wo soll ich dich nun hin begraben/", "tokens": ["Wo", "soll", "ich", "dich", "nun", "hin", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich kan immer bey dir seyn?", "tokens": ["Da\u00df", "ich", "kan", "im\u00b7mer", "bey", "dir", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du follt zur Grufft mein hertze haben/", "tokens": ["Du", "follt", "zur", "Grufft", "mein", "hert\u00b7z