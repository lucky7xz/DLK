{"dta.poem.3138": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "183.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gelegenheitsgedicht ist zu verachten nicht,", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heits\u00b7ge\u00b7dicht", "ist", "zu", "ver\u00b7ach\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKZU", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das der Gelegenheit Bedeutung recht ausspricht.", "tokens": ["Das", "der", "Ge\u00b7le\u00b7gen\u00b7heit", "Be\u00b7deu\u00b7tung", "recht", "aus\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gen\u00fcgt es nur dem Tag, so ist es schon zu loben,", "tokens": ["Ge\u00b7n\u00fcgt", "es", "nur", "dem", "Tag", ",", "so", "ist", "es", "schon", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch f\u00fcr die Ewigkeit wird es nicht aufgehoben.", "tokens": ["Doch", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "wird", "es", "nicht", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Nur wenn es Ewiges im Zeitlichen enth\u00e4lt,", "tokens": ["Nur", "wenn", "es", "E\u00b7wi\u00b7ges", "im", "Zeit\u00b7li\u00b7chen", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJA", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist heut es f\u00fcr das Fest, und morgen f\u00fcr die Welt.", "tokens": ["Ist", "heut", "es", "f\u00fcr", "das", "Fest", ",", "und", "mor\u00b7gen", "f\u00fcr", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}