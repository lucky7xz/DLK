{"textgrid.poem.35439": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Gold", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine bebenden Finger halten das blutrote Gold umspannt \u2013", "tokens": ["Mei\u00b7ne", "be\u00b7ben\u00b7den", "Fin\u00b7ger", "hal\u00b7ten", "das", "blut\u00b7ro\u00b7te", "Gold", "um\u00b7spannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+--+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Es liegt wie brennende Schande in meiner eiskalten Hand \u2013", "tokens": ["Es", "liegt", "wie", "bren\u00b7nen\u00b7de", "Schan\u00b7de", "in", "mei\u00b7ner", "eis\u00b7kal\u00b7ten", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die gierigen Augen st\u00fcrzen auf seinen grellglei\u00dfenden Glanz \u2013 \u2013", "tokens": ["Die", "gie\u00b7ri\u00b7gen", "Au\u00b7gen", "st\u00fcr\u00b7zen", "auf", "sei\u00b7nen", "grell\u00b7glei\u00b7\u00dfen\u00b7den", "Glanz", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$("], "meter": "-+--+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und an mir rast vor\u00fcber der Menschheit wahnsinniger Faschingstanz ...", "tokens": ["Und", "an", "mir", "rast", "vor\u00b7\u00fc\u00b7ber", "der", "Menschheit", "wahn\u00b7sin\u00b7ni\u00b7ger", "Fa\u00b7sching\u00b7stanz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Es w\u00f6lbt sich zur Riesenlawine vor meinem Seherblick,", "tokens": ["Es", "w\u00f6lbt", "sich", "zur", "Rie\u00b7sen\u00b7la\u00b7wi\u00b7ne", "vor", "mei\u00b7nem", "Se\u00b7her\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Zur blind hinrollenden, tauben, dies erb\u00e4rmlich winzige St\u00fcck \u2013", "tokens": ["Zur", "blind", "hin\u00b7rol\u00b7len\u00b7den", ",", "tau\u00b7ben", ",", "dies", "er\u00b7b\u00e4rm\u00b7lich", "win\u00b7zi\u00b7ge", "St\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVPP", "$,", "VVFIN", "$,", "PDS", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ich f\u00fchle Millionen Herzen zucken nach seinem Besitz \u2013", "tokens": ["Ich", "f\u00fch\u00b7le", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", "zu\u00b7cken", "nach", "sei\u00b7nem", "Be\u00b7sitz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+----+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich h\u00f6re Millionen Lippen freveln in bl\u00f6dem Aberwitz ...", "tokens": ["Ich", "h\u00f6\u00b7re", "Mil\u00b7lion\u00b7en", "Lip\u00b7pen", "fre\u00b7veln", "in", "bl\u00f6\u00b7dem", "A\u00b7ber\u00b7witz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich schaue Millionen F\u00e4uste in lohendem Groll gereckt \u2013", "tokens": ["Ich", "schau\u00b7e", "Mil\u00b7lion\u00b7en", "F\u00e4us\u00b7te", "in", "lo\u00b7hen\u00b7dem", "Groll", "ge\u00b7reckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nach goldnen Lawinenkrumen inbr\u00fcnstig ausgestreckt \u2013", "tokens": ["Nach", "gold\u00b7nen", "La\u00b7wi\u00b7nen\u00b7kru\u00b7men", "in\u00b7br\u00fcns\u00b7tig", "aus\u00b7ge\u00b7streckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+----+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich h\u00f6re Millionen Fl\u00fcche, dieweil nur Zundergest\u00e4ub", "tokens": ["Ich", "h\u00f6\u00b7re", "Mil\u00b7lion\u00b7en", "Fl\u00fc\u00b7che", ",", "die\u00b7weil", "nur", "Zun\u00b7der\u00b7ge\u00b7st\u00e4ub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NN", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Statt purpurner Pracht und Geschmeides sich klebt um den schlotternden Bettlerleib.", "tokens": ["Statt", "pur\u00b7pur\u00b7ner", "Pracht", "und", "Ge\u00b7schmei\u00b7des", "sich", "klebt", "um", "den", "schlot\u00b7tern\u00b7den", "Bett\u00b7ler\u00b7leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}}, "stanza.4": {"line.1": {"text": "Zeiten um Zeiten fliegen, Jahrtausende mir vorbei \u2013", "tokens": ["Zei\u00b7ten", "um", "Zei\u00b7ten", "flie\u00b7gen", ",", "Jahr\u00b7tau\u00b7sen\u00b7de", "mir", "vor\u00b7bei", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,", "NN", "PPER", "PTKVZ", "$("], "meter": "+--+-+-++--+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Durch alle Zeiten dr\u00f6hnt es, das gellende Jagdgeschrei ...", "tokens": ["Durch", "al\u00b7le", "Zei\u00b7ten", "dr\u00f6hnt", "es", ",", "das", "gel\u00b7len\u00b7de", "Jagd\u00b7ge\u00b7schrei", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da droben auf ihrem Throne schlief wohl die Gottheit ein \u2013", "tokens": ["Da", "dro\u00b7ben", "auf", "ih\u00b7rem", "Thro\u00b7ne", "schlief", "wohl", "die", "Got\u00b7theit", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bricht denn durch ihre Lider nicht der Scheiterhaufen Flammenschein?", "tokens": ["Bricht", "denn", "durch", "ih\u00b7re", "Li\u00b7der", "nicht", "der", "Schei\u00b7ter\u00b7hau\u00b7fen", "Flam\u00b7men\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Der Scheiterhaufen, darauf sie, die Menschheit, wahnsinnverkrampft,", "tokens": ["Der", "Schei\u00b7ter\u00b7hau\u00b7fen", ",", "da\u00b7rauf", "sie", ",", "die", "Menschheit", ",", "wahn\u00b7sinn\u00b7ver\u00b7krampft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+--+--++--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihr bi\u00dfchen Gottheit geopfert, d\u00e4monen\u00fcberstampft!", "tokens": ["Ihr", "bi\u00df\u00b7chen", "Got\u00b7theit", "ge\u00b7o\u00b7pfert", ",", "d\u00e4\u00b7mo\u00b7nen\u00b7\u00fc\u00b7bers\u00b7tampft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ja! Ihren Namen nannte die Lippe je und je \u2013", "tokens": ["Ja", "!", "Ih\u00b7ren", "Na\u00b7men", "nann\u00b7te", "die", "Lip\u00b7pe", "je", "und", "je", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und troff zugleich von Sehnsucht, nach einem \u2013Riesenportemonnaie.", "tokens": ["Und", "troff", "zu\u00b7gleich", "von", "Sehn\u00b7sucht", ",", "nach", "ei\u00b7nem", "\u2013", "Rie\u00b7sen\u00b7por\u00b7te\u00b7mon\u00b7nai\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$,", "APPR", "ART", "$(", "NE", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Kommt \u00fcber die unstete Menschheit denn nie die Erl\u00f6sungsruh?", "tokens": ["Kommt", "\u00fc\u00b7ber", "die", "un\u00b7ste\u00b7te", "Menschheit", "denn", "nie", "die", "Er\u00b7l\u00f6\u00b7sungs\u00b7ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Rast in Aeonen sie weiter, immer und immerzu? \u2013", "tokens": ["Rast", "in", "A\u00b7e\u00b7o\u00b7nen", "sie", "wei\u00b7ter", ",", "im\u00b7mer", "und", "im\u00b7mer\u00b7zu", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "ADV", "$,", "ADV", "KON", "ADV", "$.", "$("], "meter": "+-+-+--+-++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Meine Finger klammern ums Gold sich, das zur Lawine schwoll \u2013", "tokens": ["Mei\u00b7ne", "Fin\u00b7ger", "klam\u00b7mern", "ums", "Gold", "sich", ",", "das", "zur", "La\u00b7wi\u00b7ne", "schwoll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PRF", "$,", "PRELS", "APPRART", "NN", "ADJD", "$("], "meter": "+-+-+--+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wach auf, du schlafender Himmel! Das Ma\u00df ist \u00fcber- und \u00fcbervoll!", "tokens": ["Wach", "auf", ",", "du", "schla\u00b7fen\u00b7der", "Him\u00b7mel", "!", "Das", "Ma\u00df", "ist", "\u00fc\u00b7ber", "und", "\u00fc\u00b7ber\u00b7voll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+--+--+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.7": {