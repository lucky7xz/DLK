{"textgrid.poem.62137": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[wenn man die M\u00fctter aus der Erde graben k\u00f6nnte]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn man die M\u00fctter aus der Erde graben k\u00f6nnte,", "tokens": ["Wenn", "man", "die", "M\u00fct\u00b7ter", "aus", "der", "Er\u00b7de", "gra\u00b7ben", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann w\u00fcrden alle Menschenh\u00e4nde graben,", "tokens": ["Dann", "w\u00fcr\u00b7den", "al\u00b7le", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit einer Eil', als wenn es brennte:", "tokens": ["Mit", "ei\u00b7ner", "Eil'", ",", "als", "wenn", "es", "brenn\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn jeder will die Mutter wieder haben.", "tokens": ["Denn", "je\u00b7der", "will", "die", "Mut\u00b7ter", "wie\u00b7der", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn man die M\u00fctter aus der Erde k\u00f6nnte graben,", "tokens": ["Wenn", "man", "die"