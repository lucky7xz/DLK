{"textgrid.poem.30994": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich, Apollo, ruffen \u00c4rtzte; dich, Apollo, ruffen Tichter;", "tokens": ["Dich", ",", "A\u00b7pol\u00b7lo", ",", "ruf\u00b7fen", "\u00c4rtz\u00b7te", ";", "dich", ",", "A\u00b7pol\u00b7lo", ",", "ruf\u00b7fen", "Tich\u00b7ter", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "NN", "$.", "PPER", "$,", "NE", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wem du soltest vor erscheinen, darff es einen rechten Richter.", "tokens": ["Wem", "du", "sol\u00b7test", "vor", "er\u00b7schei\u00b7nen", ",", "darff", "es", "ei\u00b7nen", "rech\u00b7ten", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPR", "ADJA", "$,", "VMFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O, die \u00c4rtzte tichten auch, machen offt die Kranckheit arg,", "tokens": ["O", ",", "die", "\u00c4rtz\u00b7te", "tich\u00b7ten", "auch", ",", "ma\u00b7chen", "offt", "die", "Kran\u00b7ck\u00b7heit", "arg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+-+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df der Krancke, wann er frisch, sey zum schencken minder karg;", "tokens": ["Da\u00df", "der", "Kran\u00b7cke", ",", "wann", "er", "frisch", ",", "sey", "zum", "schen\u00b7cken", "min\u00b7der", "karg", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "$,", "VAFIN", "APPRART", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Was sie gr\u00f6blich offt versehen, hat gethan der Kranckheit St\u00e4rcke;", "tokens": ["Was", "sie", "gr\u00f6b\u00b7lich", "offt", "ver\u00b7se\u00b7hen", ",", "hat", "ge\u00b7than", "der", "Kran\u00b7ck\u00b7heit", "St\u00e4r\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVPP", "$,", "VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Wo sie gleich gar nichts geholffen, thatens dennoch ihre Wercke.", "tokens": ["Wo", "sie", "gleich", "gar", "nichts", "ge\u00b7holf\u00b7fen", ",", "tha\u00b7tens", "den\u00b7noch", "ih\u00b7re", "Wer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "PIS", "VVPP", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Hat, Apollo, dich ein Artzt wo geruffen, kumme bald!", "tokens": ["Hat", ",", "A\u00b7pol\u00b7lo", ",", "dich", "ein", "Artzt", "wo", "ge\u00b7ruf\u00b7fen", ",", "kum\u00b7me", "bald", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PRF", "ART", "NN", "PWAV", "VVPP", "$,", "VVFIN", "ADV", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Tichten hat nicht viel Verlust; kranck seyn aber braucht Gewalt.", "tokens": ["Tich\u00b7ten", "hat", "nicht", "viel", "Ver\u00b7lust", ";", "kranck", "seyn", "a\u00b7ber", "braucht", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PIAT", "NN", "$.", "ADJD", "VAINF", "ADV", "VVFIN", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dich, Apollo, ruffen \u00c4rtzte; dich, Apollo, ruffen Tichter;", "tokens": ["Dich", ",", "A\u00b7pol\u00b7lo", ",", "ruf\u00b7fen", "\u00c4rtz\u00b7te", ";", "dich", ",", "A\u00b7pol\u00b7lo", ",", "ruf\u00b7fen", "Tich\u00b7ter", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVFIN", "NN", "$.", "PPER", "$,", "NE", "$,", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wem du soltest vor erscheinen, darff es einen rechten Richter.", "tokens": ["Wem", "du", "sol\u00b7test", "vor", "er\u00b7schei\u00b7nen", ",", "darff", "es", "ei\u00b7nen", "rech\u00b7ten", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPR", "ADJA", "$,", "VMFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O, die \u00c4rtzte tichten auch, machen offt die Kranckheit arg,", "tokens": ["O", ",", "die", "\u00c4rtz\u00b7te", "tich\u00b7ten", "auch", ",", "ma\u00b7chen", "offt", "die", "Kran\u00b7ck\u00b7heit", "arg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "ADV", "$,", "VVFIN", "ADV", "