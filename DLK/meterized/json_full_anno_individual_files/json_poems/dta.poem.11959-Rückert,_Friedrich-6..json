{"dta.poem.11959": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aufs Ungl\u00fcck sei gefa\u00dft, denn morgen kann es kommen,", "tokens": ["Aufs", "Un\u00b7gl\u00fcck", "sei", "ge\u00b7fa\u00dft", ",", "denn", "mor\u00b7gen", "kann", "es", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gefa\u00dft wie auf den Gast, der seyn will aufgenommen.", "tokens": ["Ge\u00b7fa\u00dft", "wie", "auf", "den", "Gast", ",", "der", "seyn", "will", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPR", "ART", "NN", "$,", "PRELS", "VAINF", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wie es kommen kann, so kanns auch au\u00dfenbleiben,", "tokens": ["Doch", "wie", "es", "kom\u00b7men", "kann", ",", "so", "kanns", "auch", "au\u00b7\u00dfen\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und niemal sollst du selbst dein Ungemach betreiben.", "tokens": ["Und", "nie\u00b7mal", "sollst", "du", "selbst", "dein", "Un\u00b7ge\u00b7mach", "be\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei nur darauf gefa\u00dft, nie sei darum beklommen,", "tokens": ["Sei", "nur", "da\u00b7rauf", "ge\u00b7fa\u00dft", ",", "nie", "sei", "da\u00b7rum", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVPP", "$,", "ADV", "VAFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag nun der leid'ge Gast ausbleiben oder kommen.", "tokens": ["Mag", "nun", "der", "lei\u00b7d'\u00b7ge", "Gast", "aus\u00b7blei\u00b7ben", "o\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}