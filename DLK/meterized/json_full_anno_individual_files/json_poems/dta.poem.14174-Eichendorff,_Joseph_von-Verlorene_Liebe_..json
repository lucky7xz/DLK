{"dta.poem.14174": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verlorene Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lauten h\u00f6r' ich ferne klingen,", "tokens": ["Lau\u00b7ten", "h\u00f6r'", "ich", "fer\u00b7ne", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lust'ge Bursche zieh'n vom Schmaus,", "tokens": ["Lust'\u00b7ge", "Bur\u00b7sche", "zieh'n", "vom", "Schmaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4ndchen sie den Liebsten bringen,", "tokens": ["St\u00e4nd\u00b7chen", "sie", "den", "Liebs\u00b7ten", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das lockt mich mit hinaus.", "tokens": ["Und", "das", "lockt", "mich", "mit", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4dchen hinter'm bl\u00fch'nden Baume", "tokens": ["M\u00e4d\u00b7chen", "hin\u00b7ter'm", "bl\u00fch'n\u00b7den", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Winkt und macht das Fenster auf", "tokens": ["Winkt", "und", "macht", "das", "Fens\u00b7ter", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ich steige wie im Traume", "tokens": ["Und", "ich", "stei\u00b7ge", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das kleine Haus hinauf.", "tokens": ["Durch", "das", "klei\u00b7ne", "Haus", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00fctt'le nur die dunklen Locken", "tokens": ["Sch\u00fctt'\u00b7le", "nur", "die", "dunk\u00b7len", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus dem sch\u00f6nen Angesicht!", "tokens": ["Aus", "dem", "sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sieh, ich stehe ganz erschrocken:", "tokens": ["Sieh", ",", "ich", "ste\u00b7he", "ganz", "er\u00b7schro\u00b7cken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das sind ", "tokens": ["Das", "sind"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "+-", "measure": "trochaic.single"}}}}}