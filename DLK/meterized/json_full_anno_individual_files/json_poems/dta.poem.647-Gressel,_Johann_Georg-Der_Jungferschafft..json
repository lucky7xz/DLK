{"dta.poem.647": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Jungferschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich starb/ eh ich einmahl das Leben recht erkannte/", "tokens": ["Ich", "starb", "/", "eh", "ich", "ein\u00b7mahl", "das", "Le\u00b7ben", "recht", "er\u00b7kann\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein angenehmer Stich gab mir den letzten Sto\u00df/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Stich", "gab", "mir", "den", "letz\u00b7ten", "Sto\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht wie ", "tokens": ["Nicht", "wie"], "token_info": ["word", "word"], "pos": ["PTKNEG", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Doch stund ich wieder auf/ als man mich Fraue nannte.", "tokens": ["Doch", "stund", "ich", "wie\u00b7der", "auf", "/", "als", "man", "mich", "Frau\u00b7e", "nann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "$(", "KOUS", "PIS", "PRF", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}