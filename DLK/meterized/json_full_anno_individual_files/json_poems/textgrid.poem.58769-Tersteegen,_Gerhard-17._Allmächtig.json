{"textgrid.poem.58769": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "17. Allm\u00e4chtig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, deine Macht ist Allmacht ja,", "tokens": ["Herr", ",", "dei\u00b7ne", "Macht", "ist", "All\u00b7macht", "ja", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Durch deinen Will'n steht alles da,", "tokens": ["Durch", "dei\u00b7nen", "Will'n", "steht", "al\u00b7les", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es steht, es geht nach deinen Winken.", "tokens": ["Es", "steht", ",", "es", "geht", "nach", "dei\u00b7nen", "Win\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6r's, hoher Geist, h\u00f6r's, starker Mann,", "tokens": ["H\u00f6r's", ",", "ho\u00b7her", "Geist", ",", "h\u00f6r's", ",", "star\u00b7ker", "Mann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott schafft's, wenn man was Rechtes kann,", "tokens": ["Gott", "schafft's", ",", "wenn", "man", "was", "Rech\u00b7tes", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["N