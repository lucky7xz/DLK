{"dta.poem.3057": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Allahs Paradies, wie sein Profet verhie\u00df,", "tokens": ["In", "Al\u00b7lahs", "Pa\u00b7ra\u00b7dies", ",", "wie", "sein", "Pro\u00b7fet", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll spro\u00dfen jede Frucht den Gl\u00e4ub'gen zum Ersprie\u00df.", "tokens": ["Soll", "spro\u00b7\u00dfen", "je\u00b7de", "Frucht", "den", "Gl\u00e4ub'\u00b7gen", "zum", "Er\u00b7sprie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PIAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch in zwiefacher Art ist jede Frucht vorhanden,", "tokens": ["Doch", "in", "zwie\u00b7fac\u00b7her", "Art", "ist", "je\u00b7de", "Frucht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eine, wie sie gern auf Erden hier sie fanden,", "tokens": ["Die", "ei\u00b7ne", ",", "wie", "sie", "gern", "auf", "Er\u00b7den", "hier", "sie", "fan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die andere, wie sie auf Erden niemal sahn;", "tokens": ["Die", "an\u00b7de\u00b7re", ",", "wie", "sie", "auf", "Er\u00b7den", "nie\u00b7mal", "sahn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "PPER", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit beiden aber wird es also seyn gethan:", "tokens": ["Mit", "bei\u00b7den", "a\u00b7ber", "wird", "es", "al\u00b7so", "seyn", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die eine welche sie als langbekannte finden,", "tokens": ["Die", "ei\u00b7ne", "wel\u00b7che", "sie", "als", "lang\u00b7be\u00b7kann\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PRELS", "PPER", "KOUS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "L\u00e4\u00dft einen v\u00f6llig unbekannten Schmack empfinden;", "tokens": ["L\u00e4\u00dft", "ei\u00b7nen", "v\u00f6l\u00b7lig", "un\u00b7be\u00b7kann\u00b7ten", "Schmack", "emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die andre aber, die sie als ganz neu entdecken,", "tokens": ["Die", "and\u00b7re", "a\u00b7ber", ",", "die", "sie", "als", "ganz", "neu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "$,", "PRELS", "PPER", "KOKOM", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird ihnen ganz bekannt, nur etwas besser schmecken.", "tokens": ["Wird", "ih\u00b7nen", "ganz", "be\u00b7kannt", ",", "nur", "et\u00b7was", "bes\u00b7ser", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das hei\u00dft: sie werden sich im Alten stets des Neuen", "tokens": ["Das", "hei\u00dft", ":", "sie", "wer\u00b7den", "sich", "im", "Al\u00b7ten", "stets", "des", "Neu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "PRF", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem Neuen dort des Alten ewig freuen.", "tokens": ["Und", "in", "dem", "Neu\u00b7en", "dort", "des", "Al\u00b7ten", "e\u00b7wig", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Fr\u00fcchte denk' ich gern, so oft es mir behagt,", "tokens": ["Der", "Fr\u00fcch\u00b7te", "denk'", "ich", "gern", ",", "so", "oft", "es", "mir", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am liebsten, wenn die Welt dergleichen mir versagt.", "tokens": ["Am", "liebs\u00b7ten", ",", "wenn", "die", "Welt", "derg\u00b7lei\u00b7chen", "mir", "ver\u00b7sagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUS", "ART", "NN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wollt' auch bekannte Frucht nur immer Gott mir schenken,", "tokens": ["Wollt'", "auch", "be\u00b7kann\u00b7te", "Frucht", "nur", "im\u00b7mer", "Gott", "mir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "ADV", "ADV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Allahs Paradies wollt' ich bei Gott nicht denken.", "tokens": ["An", "Al\u00b7lahs", "Pa\u00b7ra\u00b7dies", "wollt'", "ich", "bei", "Gott", "nicht", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "VMFIN", "PPER", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Der Datteln wollt' ich gern entbehren und der Pfirschen,", "tokens": ["Der", "Dat\u00b7teln", "wollt'", "ich", "gern", "ent\u00b7beh\u00b7ren", "und", "der", "Pfir\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt' ich das ganze Jahr nur Trauben oder Kirschen.", "tokens": ["H\u00e4tt'", "ich", "das", "gan\u00b7ze", "Jahr", "nur", "Trau\u00b7ben", "o\u00b7der", "Kir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}