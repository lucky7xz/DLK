{"dta.poem.18959": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "37.  \n  Der Sp: Soldaten  \n Grabschrifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jn diser erden ist ein saht", "tokens": ["In", "di\u00b7ser", "er\u00b7den", "ist", "ein", "saht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des gebeins deren Raht vnd that", "tokens": ["Des", "ge\u00b7beins", "de\u00b7ren", "Raht", "vnd", "that"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRELAT", "NN", "KON", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bef\u00fcrdert des Lands krieg vnd plagen.", "tokens": ["Be\u00b7f\u00fcr\u00b7dert", "des", "Lands", "krieg", "vnd", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Solt nu der grund so fruchtbar sein", "tokens": ["Solt", "nu", "der", "grund", "so", "frucht\u00b7bar", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er f\u00fcr eins solt hundert tragen/", "tokens": ["Da\u00df", "er", "f\u00fcr", "eins", "solt", "hun\u00b7dert", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "VMFIN", "CARD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So verleyh Got/ da\u00df stehts darein", "tokens": ["So", "ver\u00b7leyh", "Got", "/", "da\u00df", "stehts", "da\u00b7rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$(", "KOUS", "PIS", "PAV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der hagel/ plitz vnd dunder schlagen!", "tokens": ["Der", "ha\u00b7gel", "/", "plitz", "vnd", "dun\u00b7der", "schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}