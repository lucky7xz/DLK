{"dta.poem.1669": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ade! o werthes Land/ du Anfang meiner Reisen! ", "tokens": ["A\u00b7de", "!", "o", "wert\u00b7hes", "Land", "/", "du", "An\u00b7fang", "mei\u00b7ner", "Rei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "FM", "ADJA", "NN", "$(", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ade von Wasser/ Wein und Nimphen reiche Stadt.", "tokens": ["A\u00b7de", "von", "Was\u00b7ser", "/", "Wein", "und", "Nim\u00b7phen", "rei\u00b7che", "Stadt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "NN", "KON", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wo so manch gutter Freund mit mir gelebet hat/", "tokens": ["Wo", "so", "manch", "gut\u00b7ter", "Freund", "mit", "mir", "ge\u00b7le\u00b7bet", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihren hohen Thurm die edlen B\u00fcrger preisen.", "tokens": ["Und", "ih\u00b7ren", "ho\u00b7hen", "Thurm", "die", "ed\u00b7len", "B\u00fcr\u00b7ger", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kan ich dir nicht den Leib auff l\u00e4ngre Jahre schencken/", "tokens": ["Kan", "ich", "dir", "nicht", "den", "Leib", "auff", "l\u00e4ng\u00b7re", "Jah\u00b7re", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So stirbet doch in mir niemahls dein Angedencken.", "tokens": ["So", "stir\u00b7bet", "doch", "in", "mir", "nie\u00b7mahls", "dein", "An\u00b7ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ade/ o Vater Rhein/ ich gr\u00fcsse deinen R\u00fccken/", "tokens": ["A\u00b7de", "/", "o", "Va\u00b7ter", "Rhein", "/", "ich", "gr\u00fcs\u00b7se", "dei\u00b7nen", "R\u00fc\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "NE", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der mich gel\u00fccklich trug nach seinem Niederland", "tokens": ["Der", "mich", "ge\u00b7l\u00fcck\u00b7lich", "trug", "nach", "sei\u00b7nem", "Nie\u00b7der\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In mir getreuer Flutt. Ich ehre deinen Strand/", "tokens": ["In", "mir", "ge\u00b7treu\u00b7er", "Flutt", ".", "Ich", "eh\u00b7re", "dei\u00b7nen", "Strand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den so viel sch\u00f6ne St\u00e4dt und Schl\u00f6sser \u00fcbersticken.", "tokens": ["Den", "so", "viel", "sch\u00f6\u00b7ne", "St\u00e4dt", "und", "Schl\u00f6s\u00b7ser", "\u00fc\u00b7bers\u00b7ti\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es m\u00fcsse keine Kr\u00f6t an deinem Ufer wohnen/", "tokens": ["Es", "m\u00fcs\u00b7se", "kei\u00b7ne", "Kr\u00f6t", "an", "dei\u00b7nem", "U\u00b7fer", "woh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Waffen deinen Strom und freyen Schilff verschonen!", "tokens": ["Die", "Waf\u00b7fen", "dei\u00b7nen", "Strom", "und", "frey\u00b7en", "Schilff", "ver\u00b7scho\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ade/ o Land/ dem Wind und Wellen m\u00fcssen dienen/", "tokens": ["A\u00b7de", "/", "o", "Land", "/", "dem", "Wind", "und", "Wel\u00b7len", "m\u00fcs\u00b7sen", "die\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "$(", "ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dem Indien gr\u00e4bt Gold/ Molucca Pfeffer tr\u00e4gt/", "tokens": ["Dem", "In\u00b7di\u00b7en", "gr\u00e4bt", "Gold", "/", "Mo\u00b7luc\u00b7ca", "Pfef\u00b7fer", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$(", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Moscau seine Frucht in Wasser-schauren legt/", "tokens": ["Und", "Mo\u00b7scau", "sei\u00b7ne", "Frucht", "in", "Was\u00b7ser\u00b7schau\u00b7ren", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sich mit K\u00f6nigen zu kriegen darff erk\u00fchnen:", "tokens": ["Das", "sich", "mit", "K\u00f6\u00b7ni\u00b7gen", "zu", "krie\u00b7gen", "darff", "er\u00b7k\u00fch\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du Zaum der frechen See/ die dein Gestade netzet/", "tokens": ["Du", "Zaum", "der", "fre\u00b7chen", "See", "/", "die", "dein", "Ge\u00b7sta\u00b7de", "net\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der dein Be ", "tokens": ["Der", "dein", "Be"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ade/ o Land/ ber\u00fchmt von Ubung gutter Sitten!", "tokens": ["A\u00b7de", "/", "o", "Land", "/", "be\u00b7r\u00fchmt", "von", "U\u00b7bung", "gut\u00b7ter", "Sit\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "$(", "ADJD", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ade zu-grosse Stadt/ zu-kleines Land Pari\u00df/", "tokens": ["A\u00b7de", "zu\u00b7gros\u00b7se", "Stadt", "/", "zu\u00b7klei\u00b7nes", "Land", "Pa\u00b7ri\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "ADJA", "NN", "NE", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Das ich nicht unbereut so zeitlich hinterlie\u00df!", "tokens": ["Das", "ich", "nicht", "un\u00b7be\u00b7reut", "so", "zeit\u00b7lich", "hin\u00b7ter\u00b7lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "ADJD", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ade/ o Reich/ das izt so vielen wil gebitten/", "tokens": ["A\u00b7de", "/", "o", "Reich", "/", "das", "izt", "so", "vie\u00b7len", "wil", "ge\u00b7bit\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "$(", "PDS", "ADV", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wo sich mit Dienstbarkeit der Freyheit Schein verm\u00e4hlet/", "tokens": ["Wo", "sich", "mit", "Dienst\u00b7bar\u00b7keit", "der", "Frey\u00b7heit", "Schein", "ver\u00b7m\u00e4h\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo stete Neuigkeit hat ihren Sitz erw\u00e4hlet!", "tokens": ["Wo", "ste\u00b7te", "Neu\u00b7ig\u00b7keit", "hat", "ih\u00b7ren", "Sitz", "er\u00b7w\u00e4h\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ade/ o sch\u00f6nes Land/ du Paradi\u00df der Erden!", "tokens": ["A\u00b7de", "/", "o", "sch\u00f6\u00b7nes", "Land", "/", "du", "Pa\u00b7ra\u00b7di\u00df", "der", "Er\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "ADJA", "NN", "$(", "PPER", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ade/ o Flu\u00df/ dem vor gehorsam war die See!", "tokens": ["A\u00b7de", "/", "o", "Flu\u00df", "/", "dem", "vor", "ge\u00b7hor\u00b7sam", "war", "die", "See", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "$(", "ART", "APPR", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Du weyland andre Welt/ der Erden Haubt/ Ade!", "tokens": ["Du", "wey\u00b7land", "and\u00b7re", "Welt", "/", "der", "Er\u00b7den", "Haubt", "/", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$(", "ART", "NN", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ade/ o reiche Stadt/ der Wellen W\u00e4lle werden/", "tokens": ["A\u00b7de", "/", "o", "rei\u00b7che", "Stadt", "/", "der", "Wel\u00b7len", "W\u00e4l\u00b7le", "wer\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "ADJA", "NN", "$(", "ART", "NN", "NN", "VAINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Zum Graben dient das Meer! Und du der St\u00e4dte Prei\u00df/", "tokens": ["Zum", "Gra\u00b7ben", "dient", "das", "Meer", "!", "Und", "du", "der", "St\u00e4d\u00b7te", "Prei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die von Vesevus Glutt so viel zu sagen wei\u00df.", "tokens": ["Die", "von", "Ve\u00b7se\u00b7vus", "Glutt", "so", "viel", "zu", "sa\u00b7gen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "NN", "ADV", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Ade/ o Berg und Thal! Ade/ o P\u00fcsch und W\u00e4lder/", "tokens": ["A\u00b7de", "/", "o", "Berg", "und", "Thal", "!", "A\u00b7de", "/", "o", "P\u00fcsch", "und", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "NN", "KON", "NN", "$.", "NN", "$(", "FM", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Durch welche mich die Lust zu reisen hat gef\u00fchrt!", "tokens": ["Durch", "wel\u00b7che", "mich", "die", "Lust", "zu", "rei\u00b7sen", "hat", "ge\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil mein gesezter Lauff nunmehr sein Ziel ber\u00fchrt/", "tokens": ["Weil", "mein", "ge\u00b7sez\u00b7ter", "Lauff", "nun\u00b7mehr", "sein", "Ziel", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mu\u00df mein gezwungner Leib gesegnen fremde Felder/", "tokens": ["Mu\u00df", "mein", "ge\u00b7zwung\u00b7ner", "Leib", "ge\u00b7seg\u00b7nen", "frem\u00b7de", "Fel\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und eilen den Befehl der Sternen zu vollbringen:", "tokens": ["Und", "ei\u00b7len", "den", "Be\u00b7fehl", "der", "Ster\u00b7nen", "zu", "voll\u00b7brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jedoch den freyen Sinn soll kein Vergessen zwingen.", "tokens": ["Je\u00b7doch", "den", "frey\u00b7en", "Sinn", "soll", "kein", "Ver\u00b7ges\u00b7sen", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ade/ o werthe Schaar/ o Auszug meiner Lieben/", "tokens": ["A\u00b7de", "/", "o", "wert\u00b7he", "Schaar", "/", "o", "Aus\u00b7zug", "mei\u00b7ner", "Lie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "FM", "VVFIN", "NN", "$(", "FM", "NN", "PPOSAT", "ADJA", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die treue Redligkeit mit Freundschafft mir verband!", "tokens": ["Die", "treu\u00b7e", "Red\u00b7lig\u00b7keit", "mit", "Freund\u00b7schafft", "mir", "ver\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich trenne mich von dir: nimm hin zum Unterpfand", "tokens": ["Ich", "tren\u00b7ne", "mich", "von", "dir", ":", "nimm", "hin", "zum", "Un\u00b7ter\u00b7pfand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$.", "VVIMP", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Hertze/ das sich dir zur Dienstbarkeit verschrieben.", "tokens": ["Ein", "Hert\u00b7ze", "/", "das", "sich", "dir", "zur", "Dienst\u00b7bar\u00b7keit", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "PRELS", "PRF", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dich Filidor/ mein Kind/ will ich mit GOtt noch schauen/", "tokens": ["Dich", "Fi\u00b7li\u00b7dor", "/", "mein", "Kind", "/", "will", "ich", "mit", "Gott", "noch", "schau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "w