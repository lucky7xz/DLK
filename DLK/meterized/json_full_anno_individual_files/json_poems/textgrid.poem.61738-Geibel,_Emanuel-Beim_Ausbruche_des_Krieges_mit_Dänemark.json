{"textgrid.poem.61738": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Beim Ausbruche des Krieges mit D\u00e4nemark", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren also lang im Traum gelegen,", "tokens": ["Wir", "wa\u00b7ren", "al\u00b7so", "lang", "im", "Traum", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df uns der Kraft Ged\u00e4chtnis schier entschwunden,", "tokens": ["Da\u00df", "uns", "der", "Kraft", "Ge\u00b7d\u00e4cht\u00b7nis", "schier", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein schw\u00fcler Zauber hielt den Sinn gebunden,", "tokens": ["Ein", "schw\u00fc\u00b7ler", "Zau\u00b7ber", "hielt", "den", "Sinn", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da blitzt es auf \u2013 o jeder Blitz ein Segen!", "tokens": ["Da", "blitzt", "es", "auf", "\u2013", "o", "je\u00b7der", "Blitz", "ein", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "FM", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich gr\u00fc\u00dfe dich, du heil'ger Feuerregen,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dich", ",", "du", "heil'\u00b7ger", "Feu\u00b7er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Sturm des Zorns nach so viel bangen Stunden!", "tokens": ["Du", "Sturm", "des", "Zorns", "nach", "so", "viel", "ban\u00b7gen", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In deinen Flammen werden wir gesunden,", "tokens": ["In", "dei\u00b7nen", "Flam\u00b7men", "wer\u00b7den", "wir", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jauchzend schl\u00e4gt dir diese Brust entgegen.", "tokens": ["Und", "jauch\u00b7zend", "schl\u00e4gt", "dir", "die\u00b7se", "Brust", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vorbei ist's endlich mit dem Dr\u00e4un und R\u00fcgen,", "tokens": ["Vor\u00b7bei", "ist's", "end\u00b7lich", "mit", "dem", "Dr\u00e4un", "und", "R\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es spricht die Tat, wo Worte nichts verfingen;", "tokens": ["Es", "spricht", "die", "Tat", ",", "wo", "Wor\u00b7te", "nichts", "ver\u00b7fin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schwert durchhaut das Schmachgeweb' der L\u00fcgen.", "tokens": ["Das", "Schwert", "durch\u00b7haut", "das", "Schmach\u00b7ge\u00b7web'", "der", "L\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vorw\u00e4rts, ihr Adler, mit den starken Schwingen!", "tokens": ["Vor\u00b7w\u00e4rts", ",", "ihr", "Ad\u00b7ler", ",", "mit", "den", "star\u00b7ken", "Schwin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schon atmet Deutschland auf bei euren Fl\u00fcgen", "tokens": ["Schon", "at\u00b7met", "Deutschland", "auf", "bei", "eu\u00b7ren", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und stimmt die Harfen, euren Sieg zu singen.", "tokens": ["Und", "stimmt", "die", "Har\u00b7fen", ",", "eu\u00b7ren", "Sieg", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir waren also lang im Traum gelegen,", "tokens": ["Wir", "wa\u00b7ren", "al\u00b7