{"dta.poem.13085": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich, sieh! betrachte mich! mich aller K\u00fcnste Kunst,", "tokens": ["\u201e", "ich", ",", "sieh", "!", "be\u00b7trach\u00b7te", "mich", "!", "mich", "al\u00b7ler", "K\u00fcns\u00b7te", "Kunst", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "VVIMP", "$.", "VVFIN", "PPER", "$.", "PPER", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "170\u201dIch prange mehr, als du, mit kluger Herzen Gunst.", "tokens": ["\"", "Ich", "pran\u00b7ge", "mehr", ",", "als", "du", ",", "mit", "klu\u00b7ger", "Her\u00b7zen", "Gunst", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich wei\u00df Natur und Kunst so gut in Eins zu f\u00fcgen,", "tokens": ["\u201e", "ich", "wei\u00df", "Na\u00b7tur", "und", "Kunst", "so", "gut", "in", "Eins", "zu", "f\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "ADV", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eda\u00df meiner Bilder Pracht Aug und Verstand betr\u00fcgen.", "tokens": ["\u201e", "da\u00df", "mei\u00b7ner", "Bil\u00b7der", "Pracht", "Aug", "und", "Ver\u00b7stand", "be\u00b7tr\u00fc\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "\u201eerblicktest du sein Bild in Marmel ausgehaut,", "tokens": ["\u201e", "er\u00b7blick\u00b7test", "du", "sein", "Bild", "in", "Mar\u00b7mel", "aus\u00b7ge\u00b7haut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eso glaubtst du, da\u00df dein Aug auf diesen F\u00fcrsten schaut.", "tokens": ["\u201e", "so", "glaubtst", "du", ",", "da\u00df", "dein", "Aug", "auf", "die\u00b7sen", "F\u00fcrs\u00b7ten", "schaut", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "175\u201dDer ausgeschnizte Stein m\u00fc\u00dft ihm so k\u00fcnstlich gleichen,", "tokens": ["\"", "Der", "aus\u00b7ge\u00b7schniz\u00b7te", "Stein", "m\u00fc\u00dft", "ihm", "so", "k\u00fcnst\u00b7lich", "glei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df er fast vor sich selbst erstaunend wurde weichen.", "tokens": ["\u201e", "da\u00df", "er", "fast", "vor", "sich", "selbst", "er\u00b7stau\u00b7nend", "wur\u00b7de", "wei\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "APPR", "PRF", "ADV", "VVPP", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edu st\u00fcndest selbst entz\u00fcckt, und fragtest deinen Sinn,", "tokens": ["\u201e", "du", "st\u00fcn\u00b7dest", "selbst", "ent\u00b7z\u00fcckt", ",", "und", "frag\u00b7test", "dei\u00b7nen", "Sinn", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "VVPP", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eob er lebendig sey, ob Geist und Blut darinn?", "tokens": ["\u201e", "ob", "er", "le\u00b7ben\u00b7dig", "sey", ",", "ob", "Geist", "und", "Blut", "da\u00b7rinn", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edu solltest vor dem Bild dich voller Ehrfurcht zeigen,", "tokens": ["\u201e", "du", "soll\u00b7test", "vor", "dem", "Bild", "dich", "vol\u00b7ler", "Ehr\u00b7furcht", "zei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "APPR", "ART", "NN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "180\u201dUnd dich, wie wann du ihn lebendig s\u00e4hest, neigen.", "tokens": ["\"", "Und", "dich", ",", "wie", "wann", "du", "ihn", "le\u00b7ben\u00b7dig", "s\u00e4\u00b7hest", ",", "nei\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "PPER", "$,", "PWAV", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}