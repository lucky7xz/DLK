{"dta.poem.13915": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch droben auf der Zinne", "tokens": ["Doch", "dro\u00b7ben", "auf", "der", "Zin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht noch der Heldengeist,", "tokens": ["Steht", "noch", "der", "Hel\u00b7den\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u2014 was die Zeit beginne \u2014", "tokens": ["Der", "was", "die", "Zeit", "be\u00b7gin\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Still nach dem Kreuze weis't.", "tokens": ["Still", "nach", "dem", "Kreu\u00b7ze", "weis'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}