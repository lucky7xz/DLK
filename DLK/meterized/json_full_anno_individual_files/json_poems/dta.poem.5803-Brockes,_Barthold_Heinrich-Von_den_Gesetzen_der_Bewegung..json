{"dta.poem.5803": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Eindruck, den sie bey uns wirckt,", "tokens": ["Der", "Ein\u00b7druck", ",", "den", "sie", "bey", "uns", "wirckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist fast in einem Wort bezirckt;", "tokens": ["Ist", "fast", "in", "ei\u00b7nem", "Wort", "be\u00b7zirckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man heisset sie LOCAL. Ohn\u2019 sich vom Ort zu trennen,", "tokens": ["Man", "heis\u00b7set", "sie", "Lo\u00b7CAL", ".", "Ohn'", "sich", "vom", "Ort", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NE", "$.", "APPR", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wird man Bewegungen nicht f\u00fcglich fassen k\u00f6nnen.", "tokens": ["Wird", "man", "Be\u00b7we\u00b7gun\u00b7gen", "nicht", "f\u00fcg\u00b7lich", "fas\u00b7sen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "PTKNEG", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}