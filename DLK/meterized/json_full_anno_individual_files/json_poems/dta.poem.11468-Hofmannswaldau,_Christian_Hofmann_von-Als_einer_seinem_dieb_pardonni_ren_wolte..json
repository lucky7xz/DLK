{"dta.poem.11468": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als einer seinem dieb  pardonni ren wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Furbienus dir verspricht:", "tokens": ["Da\u00df", "Fur\u00b7bie\u00b7nus", "dir", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er wolle nicht mehr griffe machen,", "tokens": ["Er", "wol\u00b7le", "nicht", "mehr", "grif\u00b7fe", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da mu\u00df ich deiner einfalt lachen;", "tokens": ["Da", "mu\u00df", "ich", "dei\u00b7ner", "ein\u00b7falt", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die katze l\u00e4\u00dft das mausen nicht.", "tokens": ["Die", "kat\u00b7ze", "l\u00e4\u00dft", "das", "mau\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}