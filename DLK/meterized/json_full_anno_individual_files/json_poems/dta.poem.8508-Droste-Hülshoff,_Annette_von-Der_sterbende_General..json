{"dta.poem.8508": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende General.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er lag im dichtverh&#228;ngten Saal,", "tokens": ["Er", "lag", "im", "dicht\u00b7verh", "&#228;", "ng\u00b7ten", "Saal", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo grau der Sonnenstrahl sich brach,", "tokens": ["Wo", "grau", "der", "Son\u00b7nen\u00b7strahl", "sich", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seinem Schmerzensbette lag", "tokens": ["Auf", "sei\u00b7nem", "Schmer\u00b7zens\u00b7bet\u00b7te", "lag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alte kranke General.", "tokens": ["Der", "al\u00b7te", "kran\u00b7ke", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gen\u00fcber ihm am Spiegel hing", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "ihm", "am", "Spie\u00b7gel", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Echarpe, Orden, Feldherrnstab.", "tokens": ["E\u00b7char\u00b7pe", ",", "Or\u00b7den", ",", "Feld\u00b7herrns\u00b7tab", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Still war die Luft, am Fenster ging", "tokens": ["Still", "war", "die", "Luft", ",", "am", "Fens\u00b7ter", "ging"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Langsam die Schildwach auf und ab.", "tokens": ["Lang\u00b7sam", "die", "Schild\u00b7wach", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wie der verwitterte Soldat", "tokens": ["Wie", "der", "ver\u00b7wit\u00b7ter\u00b7te", "Sol\u00b7dat"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So stumm die letzte Fehde k\u00e4mpft!", "tokens": ["So", "stumm", "die", "letz\u00b7te", "Feh\u00b7de", "k\u00e4mpft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zw\u00f6lf Stunden, seit zuletzt ged\u00e4mpft", "tokens": ["Zw\u00f6lf", "Stun\u00b7den", ",", "seit", "zu\u00b7letzt", "ge\u00b7d\u00e4mpft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "KOUS", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um \u201eWasser\u201c er, um \u201eWasser\u201c bat.", "tokens": ["Um", "\u201e", "Was\u00b7ser", "\u201c", "er", ",", "um", "\u201e", "Was\u00b7ser", "\u201c", "bat", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "$(", "NN", "$(", "PPER", "$,", "KOUI", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An seinem Kissen beugten Zwei,", "tokens": ["An", "sei\u00b7nem", "Kis\u00b7sen", "beug\u00b7ten", "Zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Einen Auge rothgeweint,", "tokens": ["Des", "Ei\u00b7nen", "Au\u00b7ge", "roth\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Andern d\u00fcster, fest und treu,", "tokens": ["Des", "An\u00b7dern", "d\u00fcs\u00b7ter", ",", "fest", "und", "treu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Diener und ein alter Freund.", "tokens": ["Ein", "Die\u00b7ner", "und", "ein", "al\u00b7ter", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201etritt seitw\u00e4rts,\u201c sprach der Eine, \u201ela\u00df", "tokens": ["\u201e", "tritt", "seit\u00b7w\u00e4rts", ",", "\u201c", "sprach", "der", "Ei\u00b7ne", ",", "\u201e", "la\u00df"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "VVFIN", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "VVIMP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn seines Standes Ehren seh\u2019n! \u2014", "tokens": ["Ihn", "sei\u00b7nes", "Stan\u00b7des", "Eh\u00b7ren", "seh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Vorhang weg, da\u00df flatternd weh\u2019n", "tokens": ["Den", "Vor\u00b7hang", "weg", ",", "da\u00df", "flat\u00b7ternd", "weh'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die B\u00e4nder an dem Spiegelglas!\u201c", "tokens": ["Die", "B\u00e4n\u00b7der", "an", "dem", "Spie\u00b7gel\u00b7glas", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kranke schlug die Augen auf,", "tokens": ["Der", "Kran\u00b7ke", "schlug", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man sah wohl, da\u00df er ihn verstand,", "tokens": ["Man", "sah", "wohl", ",", "da\u00df", "er", "ihn", "ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Blick, ein leuchtender, und drauf", "tokens": ["Ein", "Blick", ",", "ein", "leuch\u00b7ten\u00b7der", ",", "und", "drauf"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "KON", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat er sich d\u00fcster abgewandt.", "tokens": ["Hat", "er", "sich", "d\u00fcs\u00b7ter", "ab\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201edenkst du, mein alter Kamerad,", "tokens": ["\u201e", "denkst", "du", ",", "mein", "al\u00b7ter", "Ka\u00b7me\u00b7rad", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jubelnden Vietoria?", "tokens": ["Der", "ju\u00b7beln\u00b7den", "Vie\u00b7to\u00b7ria", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie flogen unsre Banner da", "tokens": ["Wie", "flo\u00b7gen", "uns\u00b7re", "Ban\u00b7ner", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch der gem\u00e4hten Feinde Saat!", "tokens": ["Durch", "der", "ge\u00b7m\u00e4h\u00b7ten", "Fein\u00b7de", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denkst du an unsers Prinzen Wort:", "tokens": ["Denkst", "du", "an", "un\u00b7sers", "Prin\u00b7zen", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man sieht es gleich, hier stand der Wart!\u201c", "tokens": ["Man", "sieht", "es", "gleich", ",", "hier", "stand", "der", "Wart", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eschnell, Konrad, nehmt die Decke fort,", "tokens": ["\u201e", "schnell", ",", "Kon\u00b7rad", ",", "nehmt", "die", "De\u00b7cke", "fort", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Odem wird so kurz und hart!\u201c", "tokens": ["Sein", "O\u00b7dem", "wird", "so", "kurz", "und", "hart", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Obrist lauscht, er murmelt sacht:", "tokens": ["Der", "O\u00b7brist", "lauscht", ",", "er", "mur\u00b7melt", "sacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201everk\u00fcmmert wie ein welkes Blatt!", "tokens": ["\u201e", "ver\u00b7k\u00fcm\u00b7mert", "wie", "ein", "wel\u00b7kes", "Blatt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Dutzend Friedensjahre hat", "tokens": ["Das", "Dut\u00b7zend", "Frie\u00b7dens\u00b7jah\u00b7re", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kapuziner ihn gemacht. \u2014", "tokens": ["Zum", "Ka\u00b7pu\u00b7zi\u00b7ner", "ihn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wart! Wart! du hast fo frisch und licht", "tokens": ["Wart", "!", "Wart", "!", "du", "hast", "fo", "frisch", "und", "licht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So oft dem Tode dich gestellt,", "tokens": ["So", "oft", "dem", "To\u00b7de", "dich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Furcht, ich wei\u00df es, kennst du nicht,", "tokens": ["Die", "Furcht", ",", "ich", "wei\u00df", "es", ",", "kennst", "du", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stirb auch freudig wie ein Held!", "tokens": ["So", "stirb", "auch", "freu\u00b7dig", "wie", "ein", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Stirb, wie ein Leue, adelich,", "tokens": ["Stirb", ",", "wie", "ein", "Leu\u00b7e", ",", "a\u00b7de\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Brust das Bleigescho\u00df,", "tokens": ["In", "sei\u00b7ner", "Brust", "das", "Blei\u00b7ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O stirb nicht, wie ein zahnlos Ro\u00df,", "tokens": ["O", "stirb", "nicht", ",", "wie", "ein", "zahn\u00b7los", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das zappelt vor des Henkers Stich! \u2014", "tokens": ["Das", "zap\u00b7pelt", "vor", "des", "Hen\u00b7kers", "Stich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2014 Ha, seinem Auge kehrt der Strahl! \u2014", "tokens": ["Ha", ",", "sei\u00b7nem", "Au\u00b7ge", "kehrt", "der", "Strahl", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stirb, alter Freund, stirb wie ein Mann!\u201c", "tokens": ["Stirb", ",", "al\u00b7ter", "Freund", ",", "stirb", "wie", "ein", "Mann", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Kranke zuckt, zuckt noch einmal,", "tokens": ["Der", "Kran\u00b7ke", "zuckt", ",", "zuckt", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und \u201eWasser, Wasser\u201c st\u00f6hnt er dann.", "tokens": ["Und", "\u201e", "Was\u00b7ser", ",", "Was\u00b7ser", "\u201c", "st\u00f6hnt", "er", "dann", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$,", "NN", "$(", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}