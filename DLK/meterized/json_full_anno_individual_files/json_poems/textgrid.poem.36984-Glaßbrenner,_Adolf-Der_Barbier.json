{"textgrid.poem.36984": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Der Barbier", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kopfsch\u00fcttelnd entstieg ich dem Bade, da rief", "tokens": ["Kopf\u00b7sch\u00fct\u00b7telnd", "ent\u00b7stieg", "ich", "dem", "Ba\u00b7de", ",", "da", "rief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es \u00bbHerein!\u00ab und der Fiskus, der flinke,", "tokens": ["Es", "\u00bb", "Her\u00b7ein", "!", "\u00ab", "und", "der", "Fis\u00b7kus", ",", "der", "flin\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$(", "PTKVZ", "$.", "$(", "KON", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit dem purpursammetnen Scheersack trat ein", "tokens": ["Mit", "dem", "pur\u00b7pur\u00b7sam\u00b7met\u00b7nen", "Scheer\u00b7sack", "trat", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und reichte mir zum Handku\u00df die Linke.", "tokens": ["Und", "reich\u00b7te", "mir", "zum", "Hand\u00b7ku\u00df", "die", "Lin\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dann schob er mich fort und stellte mich auf,", "tokens": ["Dann", "schob", "er", "mich", "fort", "und", "stell\u00b7te", "mich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem R\u00fccken steif an, an der Th\u00fcre,", "tokens": ["Mit", "dem", "R\u00fc\u00b7cken", "steif", "an", ",", "an", "der", "Th\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und sagte, es koste den Hals mir, wenn ich", "tokens": ["Und", "sag\u00b7te", ",", "es", "kos\u00b7te", "den", "Hals", "mir", ",", "wenn", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bei dem Bartabnehmen mich r\u00fchre.", "tokens": ["Bei", "dem", "Bar\u00b7tab\u00b7neh\u00b7men", "mich", "r\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbder Unterthan,\u00ab sprach er, \u00bbder wahre, darf selbst", "tokens": ["\u00bb", "der", "Un\u00b7ter\u00b7than", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "der", "wah\u00b7re", ",", "darf", "selbst"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ART", "ADJA", "$,", "VMFIN", "ADV"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Unter Schmerzen nicht mucken und zucken!", "tokens": ["Un\u00b7ter", "Schmer\u00b7zen", "nicht", "mu\u00b7cken", "und", "zu\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVINF", "KON", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Den gerechtesten Ingrimm, den edelsten Zorn", "tokens": ["Den", "ge\u00b7rech\u00b7tes\u00b7ten", "In\u00b7grimm", ",", "den", "e\u00b7dels\u00b7ten", "Zorn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Mu\u00df er still und geduldig verschlucken!\u00ab", "tokens": ["Mu\u00df", "er", "still", "und", "ge\u00b7dul\u00b7dig", "ver\u00b7schlu\u00b7cken", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4hrend er so mich beschwatzte, ergriff", "tokens": ["Und", "w\u00e4h\u00b7rend", "er", "so", "mich", "be\u00b7schwatz\u00b7te", ",", "er\u00b7griff"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Er ein schartiges Messer und kratzte", "tokens": ["Er", "ein", "schar\u00b7ti\u00b7ges", "Mes\u00b7ser", "und", "kratz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mich derma\u00dfen, da\u00df mir denn doch die Geduld,", "tokens": ["Mich", "der\u00b7ma\u00b7\u00dfen", ",", "da\u00df", "mir", "denn", "doch", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Die germanisch-unendliche, platzte!", "tokens": ["Die", "ger\u00b7ma\u00b7nischu\u00b7nend\u00b7li\u00b7che", ",", "platz\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein: ", "tokens": ["Nein", ":"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "So wollte ich sagen \u2013 wenn ich nicht", "tokens": ["So", "woll\u00b7te", "ich", "sa\u00b7gen", "\u2013", "wenn", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$(", "KOUS", "PPER", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Folgen, die m\u00f6glichen, h\u00e4tte bedacht,", "tokens": ["Die", "Fol\u00b7gen", ",", "die", "m\u00f6g\u00b7li\u00b7chen", ",", "h\u00e4t\u00b7te", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und h\u00e4tte beruhiget mich nicht.", "tokens": ["Und", "h\u00e4t\u00b7te", "be\u00b7ru\u00b7hi\u00b7get", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Und grade als mir meine teutsche Geduld", "tokens": ["Und", "gra\u00b7de", "als", "mir", "mei\u00b7ne", "teut\u00b7sche", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Absicht verrathen, zu platzen,", "tokens": ["Die", "Ab\u00b7sicht", "ver\u00b7ra\u00b7then", ",", "zu", "plat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und ich sie beruhiget, endete er,", "tokens": ["Und", "ich", "sie", "be\u00b7ru\u00b7hi\u00b7get", ",", "en\u00b7de\u00b7te", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fiskus, sein Schinden und Kratzen.", "tokens": ["Der", "Fis\u00b7kus", ",", "sein", "Schin\u00b7den", "und", "Krat\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Doch nahm noch Derselbe \u2013 begriff ich's auch nicht", "tokens": ["Doch", "nahm", "noch", "Der\u00b7sel\u00b7be", "\u2013", "be\u00b7griff", "ich's", "auch", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "$(", "VVFIN", "PIS", "ADV", "PTKNEG"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie all dies konnt' amtliche Pflicht sein \u2013", "tokens": ["Wie", "all", "dies", "konnt'", "amt\u00b7li\u00b7che", "Pflicht", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PDS", "VMFIN", "ADJA", "NN", "VAINF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schaumbecken und Pinsel und seifete mir", "tokens": ["Schaum\u00b7be\u00b7cken", "und", "Pin\u00b7sel", "und", "sei\u00b7fe\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "VVFIN", "PPER"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Damit war verflossen denn endlich, o Gott!", "tokens": ["Da\u00b7mit", "war", "ver\u00b7flos\u00b7sen", "denn", "end\u00b7lich", ",", "o", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "VVPP", "KON", "ADV", "$,", "FM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die fiskalische Marter- und Qualzeit;", "tokens": ["Die", "fis\u00b7ka\u00b7li\u00b7sche", "Mar\u00b7ter", "und", "Qual\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Der Staatsdiener h\u00fcpfte zur Th\u00fcre hinaus", "tokens": ["Der", "Staats\u00b7die\u00b7ner", "h\u00fcpf\u00b7te", "zur", "Th\u00fc\u00b7re", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit dem Wunsche: \u00bbGesegnete Mahlzeit!\u00ab", "tokens": ["Mit", "dem", "Wun\u00b7sche", ":", "\u00bb", "Ge\u00b7se\u00b7gne\u00b7te", "Mahl\u00b7zeit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Kopfsch\u00fcttelnd entstieg ich dem Ba