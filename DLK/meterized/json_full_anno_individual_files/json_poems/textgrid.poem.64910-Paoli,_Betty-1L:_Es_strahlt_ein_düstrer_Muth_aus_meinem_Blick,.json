{"textgrid.poem.64910": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es strahlt ein d\u00fcstrer Muth aus meinem Blick,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es strahlt ein d\u00fcstrer Muth aus meinem Blick,", "tokens": ["Es", "strahlt", "ein", "d\u00fcst\u00b7rer", "Muth", "aus", "mei\u00b7nem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Indessen Thr\u00e4nen dir im Auge zittern,", "tokens": ["In\u00b7des\u00b7sen", "Thr\u00e4\u00b7nen", "dir", "im", "Au\u00b7ge", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geknickt hat dich, zerschmettert das Geschick,", "tokens": ["Ge\u00b7knickt", "hat", "dich", ",", "zer\u00b7schmet\u00b7tert", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich hat's gefeit mit seinen Ungewittern.", "tokens": ["Mich", "hat's", "ge\u00b7feit", "mit", "sei\u00b7nen", "Un\u00b7ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du fragst dich \u00e4ngstlich, ob in deiner Brust", "tokens": ["Du", "fragst", "dich", "\u00e4ngst\u00b7lich", ",", "ob", "in", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch Raum vielleicht f\u00fcr eine neue Wunde:", "tokens": ["Noch", "Raum", "viel\u00b7leicht", "f\u00fcr", "ei\u00b7ne", "neu\u00b7e", "Wun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich aber bin mir's tief und klar bewu\u00dft:", "tokens": ["Ich", "a\u00b7ber", "bin", "mir's", "tief", "und", "klar", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "NE", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor\u00fcber ist des Lebens schwerste Stunde!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "des", "Le\u00b7bens", "schwers\u00b7te", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und doch f\u00fchl' ich dein weichres Leiden mit,", "tokens": ["Und", "doch", "f\u00fchl'", "ich", "dein", "weich\u00b7res", "Lei\u00b7den", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wohl der Fels, den St\u00fcrmen preisgegeben,", "tokens": ["Wie", "wohl", "der", "Fels", ",", "den", "St\u00fcr\u00b7men", "preis\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Blitzen trotzend, doch mit Wehmuth sieht", "tokens": ["Den", "Blit\u00b7zen", "trot\u00b7zend", ",", "doch", "mit", "Weh\u00b7muth", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Nord zertr\u00fcmmert einer Blume Leben.", "tokens": ["Vom", "Nord", "zer\u00b7tr\u00fcm\u00b7mert", "ei\u00b7ner", "Blu\u00b7me", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und sinkt sie in die gr\u00fcne Bl\u00e4ttergruft,", "tokens": ["Und", "sinkt", "sie", "in", "die", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "M\u00f6cht' er sie wohl um dieses Loos beneiden,", "tokens": ["M\u00f6cht'", "er", "sie", "wohl", "um", "die\u00b7ses", "Loos", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Statt schroff zu ragen in die leere Luft,", "tokens": ["Statt", "schroff", "zu", "ra\u00b7gen", "in", "die", "lee\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKZU", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic