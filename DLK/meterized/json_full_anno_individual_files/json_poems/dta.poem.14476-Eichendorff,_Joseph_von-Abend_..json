{"dta.poem.14476": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Von \u00fcppig bl\u00fchenden Schmerzen", "tokens": ["Von", "\u00fcp\u00b7pig", "bl\u00fc\u00b7hen\u00b7den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rauscht eine Wildni\u00df im Grund,", "tokens": ["Rauscht", "ei\u00b7ne", "Wild\u00b7ni\u00df", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da spielt wie in wahnsinnigen Scherzen", "tokens": ["Da", "spielt", "wie", "in", "wahn\u00b7sin\u00b7ni\u00b7gen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Herz an dem schwindlichten Schlund. \u2014", "tokens": ["Das", "Herz", "an", "dem", "schwind\u00b7lich\u00b7ten", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}