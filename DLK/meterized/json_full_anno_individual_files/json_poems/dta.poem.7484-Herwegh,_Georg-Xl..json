{"dta.poem.7484": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob die Locken eine Glorie quellen", "tokens": ["Ob", "die", "Lo\u00b7cken", "ei\u00b7ne", "Glo\u00b7rie", "quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um Dein Antlitz und Du himmlischmild", "tokens": ["Um", "Dein", "Ant\u00b7litz", "und", "Du", "himm\u00b7lischmild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "KON", "PPER", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Auf mich blickst, ein stumm Marienbild,", "tokens": ["Auf", "mich", "blickst", ",", "ein", "stumm", "Ma\u00b7ri\u00b7en\u00b7bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das zwei blaue Sterne fromm erhellen,", "tokens": ["Das", "zwei", "blau\u00b7e", "Ster\u00b7ne", "fromm", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ob Dein Haar in ungebundnen Wellen", "tokens": ["Ob", "Dein", "Haar", "in", "un\u00b7ge\u00b7bund\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um den Nacken flutet, stolz und wild,", "tokens": ["Um", "den", "Na\u00b7cken", "flu\u00b7tet", ",", "stolz", "und", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und Dein Aug' ein harter Demantschild,", "tokens": ["Und", "Dein", "Aug'", "ein", "har\u00b7ter", "De\u00b7mant\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dran die k\u00fchnsten W\u00fcnsche jach zerschellen;", "tokens": ["Dran", "die", "k\u00fchns\u00b7ten", "W\u00fcn\u00b7sche", "jach", "zer\u00b7schel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ob ich sehe mit dem Heil'genscheine", "tokens": ["Ob", "ich", "se\u00b7he", "mit", "dem", "Heil'\u00b7gen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dich, ob mit des Unmut's d\u00fcstrer Falte,", "tokens": ["Dich", ",", "ob", "mit", "des", "Un\u00b7mut's", "d\u00fcst\u00b7rer", "Fal\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ewig, ewig fleh' ich nur das Eine:", "tokens": ["E\u00b7wig", ",", "e\u00b7wig", "fleh'", "ich", "nur", "das", "Ei\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df Dein sch\u00f6ner Mund doch nie erkalte,", "tokens": ["Da\u00df", "Dein", "sch\u00f6\u00b7ner", "Mund", "doch", "nie", "er\u00b7kal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df Dein sch\u00f6nes Auge niemals weine,", "tokens": ["Da\u00df", "Dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "nie\u00b7mals", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und mir Gott Dein sch\u00f6nes Herz erhalte.", "tokens": ["Und", "mir", "Gott", "Dein", "sch\u00f6\u00b7nes", "Herz", "er\u00b7hal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}