{"textgrid.poem.47301": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah empor und sah in allen R\u00e4umen eines;", "tokens": ["Ich", "sah", "em\u00b7por", "und", "sah", "in", "al\u00b7len", "R\u00e4u\u00b7men", "ei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinab ins Meer und sah in allen Wellensch\u00e4umen eines.", "tokens": ["Hin\u00b7ab", "ins", "Meer", "und", "sah", "in", "al\u00b7len", "Wel\u00b7len\u00b7sch\u00e4u\u00b7men", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ich sah ins Herz, es war ein Meer, ein Raum der Welten,", "tokens": ["Ich", "sah", "ins", "Herz", ",", "es", "war", "ein", "Meer", ",", "ein", "Raum", "der", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Voll tausend Tr\u00e4um'; ich sah in allen Tr\u00e4umen eines.", "tokens": ["Voll", "tau\u00b7send", "Tr\u00e4um'", ";", "ich", "sah", "in", "al\u00b7len", "Tr\u00e4u\u00b7men", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "CARD", "NN", "$.", "PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du bist das Erste, Letzte, \u00c4u\u00dfre, Innre, Ganze;", "tokens": ["Du", "bist", "das", "Ers\u00b7te", ",", "Letz\u00b7te", ",", "\u00c4u\u00df\u00b7re", ",", "Inn\u00b7re", ",", "Gan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "$,", "NN", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es strahlt dein Licht in allen Farbens\u00e4umen eines.", "tokens": ["Es", "strahlt", "dein", "Licht", "in", "al\u00b7len", "Far\u00b7ben\u00b7s\u00e4u\u00b7men", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du schaust von Ostens Grenze bis zur Grenz' im Westen,", "tokens": ["Du", "schaust", "von", "Os\u00b7tens", "Gren\u00b7ze", "bis", "zur", "Grenz'", "im", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "NE", "APPR", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir bl\u00fcht das Laub an allen gr\u00fcnen B\u00e4umen eines.", "tokens": ["Dir", "bl\u00fcht", "das", "Laub", "an", "al\u00b7len", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7men", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "ART", "$."], "meter": "-+-+-+-+-