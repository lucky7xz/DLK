{"textgrid.poem.64151": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Heinrich VI", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein gro\u00dfer, tapfrer Vater, zu ehrlich war dein Sinn:", "tokens": ["Mein", "gro\u00b7\u00dfer", ",", "tapf\u00b7rer", "Va\u00b7ter", ",", "zu", "ehr\u00b7lich", "war", "dein", "Sinn", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer Treue h\u00e4lt den F\u00fcchsen, hat des viel Ungewinn.", "tokens": ["Wer", "Treu\u00b7e", "h\u00e4lt", "den", "F\u00fcch\u00b7sen", ",", "hat", "des", "viel", "Un\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "ART", "NN", "$,", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn der Papst der L\u00fcge Vater und der F\u00fcrst ihr Liebster ist: \u2013", "tokens": ["Wenn", "der", "Papst", "der", "L\u00fc\u00b7ge", "Va\u00b7ter", "und", "der", "F\u00fcrst", "ihr", "Liebs\u00b7ter", "ist", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hei, Falscheid wider Meineid und Arglist gegen List!", "tokens": ["Hei", ",", "Fal\u00b7scheid", "wi\u00b7der", "Mei\u00b7neid", "und", "Arg\u00b7list", "ge\u00b7gen", "List", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "NN", "KON", "NN", "APPR", "NE", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6r' euch unterh\u00f6hlen den Thron mir Nacht und Tag: \u2013", "tokens": ["Ich", "h\u00f6r'", "euch", "un\u00b7ter\u00b7h\u00f6h\u00b7len", "den", "Thron", "mir", "Nacht", "und", "Tag", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "La\u00df sehn, wer leiser graben, wer tiefer w\u00fchlen mag.", "tokens": ["La\u00df", "sehn", ",", "wer", "lei\u00b7ser", "gra\u00b7ben", ",", "wer", "tie\u00b7fer", "w\u00fch\u00b7len", "mag."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "ADJD", "VVINF", "$,", "PWS", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "La\u00df sehn, wer s\u00fc\u00dfer l\u00e4cheln und bittrer hassen kann", "tokens": ["La\u00df", "sehn", ",", "wer", "s\u00fc\u00b7\u00dfer", "l\u00e4\u00b7cheln", "und", "bit\u00b7trer", "has\u00b7sen", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVINF", "$,", "PWS", "ADJD", "VVFIN", "KON", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sicherer im Ansprung erdr\u00fccken seinen Mann.", "tokens": ["Und", "si\u00b7che\u00b7rer", "im", "An\u00b7sprung", "er\u00b7dr\u00fc\u00b7cken", "sei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Vor meinen Kaiserwagen hab' ich zwei L\u00f6wen gespannt:", "tokens": ["Vor", "mei\u00b7nen", "Kai\u00b7ser\u00b7wa\u00b7gen", "hab'", "ich", "zwei", "L\u00f6\u00b7wen", "ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die hei\u00dfen Herzog Heinrich und Richard Engelland.", "tokens": ["Die", "hei\u00b7\u00dfen", "Her\u00b7zog", "Hein\u00b7rich", "und", "Ric\u00b7hard", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "KON", "NE", "NE", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Unfl\u00fcgg' hast, alter Welfe, du des Adlers Nestling gedacht? \u2013 \u2013", "tokens": ["Un\u00b7fl\u00fcg\u00b7g'", "hast", ",", "al\u00b7ter", "Wel\u00b7fe", ",", "du", "des", "Ad\u00b7lers", "Nest\u00b7ling", "ge\u00b7dacht", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VAFIN", "$,", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "VVPP", "$.", "$(", "$("], "meter": "----+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Ein Schlag von seinen F\u00e4ngen hat die Geier zu Fall gebracht.", "tokens": ["Ein", "Schlag", "von", "sei\u00b7nen", "F\u00e4n\u00b7gen", "hat", "die", "Gei\u00b7er", "zu", "Fall", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN"