{"dta.poem.89": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-Gespr\u00e4ch zwischen unsern  \n ersten Eltern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Welch unbekannte Brunst beseelet meine Brust?", "tokens": ["Welch", "un\u00b7be\u00b7kann\u00b7te", "Brunst", "be\u00b7see\u00b7let", "mei\u00b7ne", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was vor ein him\u0303lisch Feur erw\u00e4rmet meine Glieder?", "tokens": ["Was", "vor", "ein", "him\u0303\u00b7lisch", "Feur", "er\u00b7w\u00e4r\u00b7met", "mei\u00b7ne", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJD", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In meinem Hertzen glimmt ein Zunder fremder Lust/", "tokens": ["In", "mei\u00b7nem", "Hert\u00b7zen", "glimmt", "ein", "Zun\u00b7der", "frem\u00b7der", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Ohr/ das h\u00f6ret nichts als lauter Liebes-Lieder.", "tokens": ["Mein", "Ohr", "/", "das", "h\u00f6\u00b7ret", "nichts", "als", "lau\u00b7ter", "Lie\u00b7bes\u00b7Lie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Vogel lockt das Weib mit halb erstorbner Stimm/", "tokens": ["Der", "Vo\u00b7gel", "lockt", "das", "Weib", "mit", "halb", "ers\u00b7torb\u00b7ner", "Stimm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Fische in dem Meer in Lust zusammen schertzen/", "tokens": ["Die", "Fi\u00b7sche", "in", "dem", "Meer", "in", "Lust", "zu\u00b7sam\u00b7men", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Leue wird ein Lamm/ er heget keinen Grimm/", "tokens": ["Der", "Leu\u00b7e", "wird", "ein", "Lamm", "/", "er", "he\u00b7get", "kei\u00b7nen", "Grimm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "PPER", "VVFIN", "PIAT", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Thiere dieser Welt sich in die Wette hertzen.", "tokens": ["Die", "Thie\u00b7re", "die\u00b7ser", "Welt", "sich", "in", "die", "Wet\u00b7te", "hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur ich/ ich aller Herr was dieser Erd-Krey\u00df hegt/", "tokens": ["Nur", "ich", "/", "ich", "al\u00b7ler", "Herr", "was", "die\u00b7ser", "Erd\u00b7Krey\u00df", "hegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "PPER", "PIAT", "NN", "PWS", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mu\u00df in der stummen Glut ohn einzig Labsahl brennen/", "tokens": ["Mu\u00df", "in", "der", "stum\u00b7men", "Glut", "ohn", "ein\u00b7zig", "Lab\u00b7sahl", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich finde kein Gesch\u00f6pff zu dem mein Sinn sich legt/", "tokens": ["Ich", "fin\u00b7de", "kein", "Ge\u00b7sch\u00f6pff", "zu", "dem", "mein", "Sinn", "sich", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geh\u00fclffen kan ich nicht zu meiner K\u00fchlung kennen.", "tokens": ["Ge\u00b7h\u00fclf\u00b7fen", "kan", "ich", "nicht", "zu", "mei\u00b7ner", "K\u00fch\u00b7lung", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch was beklag\u2019 ich mich? da ich zur Stillung hab", "tokens": ["Doch", "was", "be\u00b7klag'", "ich", "mich", "?", "da", "ich", "zur", "Stil\u00b7lung", "hab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "$.", "KOUS", "PPER", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Die mir des Sch\u00f6pffers Hand zur Mitgesellin gab/", "tokens": ["Die", "mir", "des", "Sch\u00f6pf\u00b7fers", "Hand", "zur", "Mit\u00b7ge\u00b7sel\u00b7lin", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Bey der ich/ wann ich will/ die Ruhe kan bekommen.", "tokens": ["Bey", "der", "ich", "/", "wann", "ich", "will", "/", "die", "Ru\u00b7he", "kan", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$(", "PWAV", "PPER", "VMFIN", "$(", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie! da\u00df ich denn so gar vor grosser Liebe blind/", "tokens": ["Wie", "!", "da\u00df", "ich", "denn", "so", "gar", "vor", "gros\u00b7ser", "Lie\u00b7be", "blind", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und meinen Engel nicht zur Seiten sehe stehen?", "tokens": ["Und", "mei\u00b7nen", "En\u00b7gel", "nicht", "zur", "Sei\u00b7ten", "se\u00b7he", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was wilden Thieren ist zu ihrer Lust verg\u00fcnt/", "tokens": ["Was", "wil\u00b7den", "Thie\u00b7ren", "ist", "zu", "ih\u00b7rer", "Lust", "ver\u00b7g\u00fcnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kan mir zu gr\u00f6\u00dfrer Lust von ", "tokens": ["Kan", "mir", "zu", "gr\u00f6\u00df\u00b7rer", "Lust", "von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Wie sich ein jedes Thier zu seines gleichen stellet?", "tokens": ["Wie", "sich", "ein", "je\u00b7des", "Thier", "zu", "sei\u00b7nes", "glei\u00b7chen", "stel\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist es? da\u00df in dir zur Zaudrung Anla\u00df gibt?", "tokens": ["Was", "ist", "es", "?", "da\u00df", "in", "dir", "zur", "Zaud\u00b7rung", "An\u00b7la\u00df", "gibt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "KOUS", "APPR", "PPER", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie/ da\u00df sich deine Brunst nicht gleich zu mir gesellet?", "tokens": ["Wie", "/", "da\u00df", "sich", "dei\u00b7ne", "Brunst", "nicht", "gleich", "zu", "mir", "ge\u00b7sel\u00b7let", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Wesen gr\u00f6sser Lust und Anmuth in sich h\u00e4lt/", "tokens": ["Mein", "We\u00b7sen", "gr\u00f6s\u00b7ser", "Lust", "und", "An\u00b7muth", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als alles/ so die Welt in ihren Gr\u00e4ntzen heget:", "tokens": ["Als", "al\u00b7les", "/", "so", "die", "Welt", "in", "ih\u00b7ren", "Gr\u00e4nt\u00b7zen", "he\u00b7get", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Umsonst ist dieser Schmuck mir ja nicht zugestelt/", "tokens": ["Um\u00b7sonst", "ist", "die\u00b7ser", "Schmuck", "mir", "ja", "nicht", "zu\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie! da\u00df dein Wunsch sich nicht zu meinem Wollen leget.", "tokens": ["Wie", "!", "da\u00df", "dein", "Wunsch", "sich", "nicht", "zu", "mei\u00b7nem", "Wol\u00b7len", "le\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "NN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sieh meiner Glieder-Pracht/ die wohl-gew\u00f6lbte Brust/", "tokens": ["Sieh", "mei\u00b7ner", "Glie\u00b7der\u00b7Pracht", "/", "die", "wohl\u00b7ge\u00b7w\u00f6lb\u00b7te", "Brust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sie aus heisser Brunst nach ", "tokens": ["Wie", "sie", "aus", "heis\u00b7ser", "Brunst", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Schau jener Gegend Zier/ den Wohn-Platz aller Lust/", "tokens": ["Schau", "je\u00b7ner", "Ge\u00b7gend", "Zier", "/", "den", "Wohn\u00b7Platz", "al\u00b7ler", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.12": {"text": "Woran uns die Natur durch ihren Trieb gewehnet.", "tokens": ["Wo\u00b7ran", "uns", "die", "Na\u00b7tur", "durch", "ih\u00b7ren", "Trieb", "ge\u00b7weh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie stehst du als entz\u00fcckt/ verblendet dich die Pracht", "tokens": ["Wie", "stehst", "du", "als", "ent\u00b7z\u00fcckt", "/", "ver\u00b7blen\u00b7det", "dich", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "KOKOM", "VVPP", "$(", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der angenehmen Brust und Perlen voller Glieder?", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7men", "Brust", "und", "Per\u00b7len", "vol\u00b7ler", "Glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie/ da\u00df dein Sehnen nicht zu meiner Brust sich macht/", "tokens": ["Wie", "/", "da\u00df", "dein", "Seh\u00b7nen", "nicht", "zu", "mei\u00b7ner", "Brust", "sich", "macht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und sich erhitzet legt in meinem Schoosse nieder?", "tokens": ["Und", "sich", "er\u00b7hit\u00b7zet", "legt", "in", "mei\u00b7nem", "Schoos\u00b7se", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Komm k\u00fchle dich mein Kind/ und stille deine Brunst/", "tokens": ["Komm", "k\u00fch\u00b7le", "dich", "mein", "Kind", "/", "und", "stil\u00b7le", "dei\u00b7ne", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was uns erg\u00f6tzen kan ist gleich entbrandter Wille/", "tokens": ["Was", "uns", "er\u00b7g\u00f6t\u00b7zen", "kan", "ist", "gleich", "ent\u00b7brand\u00b7ter", "Wil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "VAFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Thiere Schertzen ist bey unser Liebe Dunst;", "tokens": ["Der", "Thie\u00b7re", "Schert\u00b7zen", "ist", "bey", "un\u00b7ser", "Lie\u00b7be", "Dunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Komm\u2019 eilend liebster Schatz mein Feur der Sehnsucht stille.", "tokens": ["Komm'", "ei\u00b7lend", "liebs\u00b7ter", "Schatz", "mein", "Feur", "der", "Sehn\u00b7sucht", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ADJA", "NN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein sch\u00f6nes Wesen nimmt mir meine Geister ein/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "We\u00b7sen", "nimmt", "mir", "mei\u00b7ne", "Geis\u00b7ter", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es zwinget mich der Schmuck von deinem sch\u00f6nen Leibe/", "tokens": ["Es", "zwin\u00b7get", "mich", "der", "Schmuck", "von", "dei\u00b7nem", "sch\u00f6\u00b7nen", "Lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word