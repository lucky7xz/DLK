{"textgrid.poem.48812": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "37. Auf die von ihnen Dreien ihm \u00fcbersendete Krone", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei stolz, mein w\u00fcrdigs Haar, das ist der teure Kranz,", "tokens": ["Sei", "stolz", ",", "mein", "w\u00fcr\u00b7digs", "Haar", ",", "das", "ist", "der", "teu\u00b7re", "Kranz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den nach Apollen dir drei Charitinnen schicken,", "tokens": ["den", "nach", "A\u00b7pol\u00b7len", "dir", "drei", "Cha\u00b7ri\u00b7tin\u00b7nen", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dich mit verdienter Zier der Ehren auszuschm\u00fccken", "tokens": ["dich", "mit", "ver\u00b7dien\u00b7ter", "Zier", "der", "Eh\u00b7ren", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "f\u00fcr dieser Verse Preis, der mir des Monden Glanz", "tokens": ["f\u00fcr", "die\u00b7ser", "Ver\u00b7se", "Preis", ",", "der", "mir", "des", "Mon\u00b7den", "Glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "nach Ph\u00f6bus seinem gibt. Ich bin es unwert ganz,", "tokens": ["nach", "Ph\u00f6\u00b7bus", "sei\u00b7nem", "gibt", ".", "Ich", "bin", "es", "un\u00b7wert", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Schmuckes dieses Ruhms. Ich nehms mit tiefem B\u00fccken", "tokens": ["des", "Schmu\u00b7ckes", "die\u00b7ses", "Ruhms", ".", "Ich", "nehms", "mit", "tie\u00b7fem", "B\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und halbem Willen an. Und, weils so wol will gl\u00fccken,", "tokens": ["und", "hal\u00b7bem", "Wil\u00b7len", "an", ".", "Und", ",", "weils", "so", "wol", "will", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$.", "KON", "$,", "KOUS", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so will ich gerne gehn mit an der Musen Tanz.", "tokens": ["so", "will", "ich", "ger\u00b7ne", "gehn", "mit", "an", "der", "Mu\u00b7sen", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wilkommen, edles Werk, voll g\u00f6tlicher Geberden,", "tokens": ["Wil\u00b7kom\u00b7men", ",", "ed\u00b7les", "Werk", ",", "voll", "g\u00f6t\u00b7li\u00b7cher", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von dem die Geister mir nun wieder himmlisch werden.", "tokens": ["von", "dem", "die", "Geis\u00b7ter", "mir", "nun", "wie\u00b7der", "himm\u00b7lisch", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der st\u00e4rkende Geruch, der kraftgef\u00fcllte Schein,", "tokens": ["Der", "st\u00e4r\u00b7ken\u00b7de", "Ge\u00b7ruch", ",", "der", "kraft\u00b7ge\u00b7f\u00fcll\u00b7te", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "da tausent Amorn sich bald zeigen, bald verkriechen,", "tokens": ["da", "tau\u00b7sent", "A\u00b7morn", "sich", "bald", "zei\u00b7gen", ",", "bald", "ver\u00b7krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PRF", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "macht einen stillen Zank in meinem Sehn und R\u00fcchen.", "tokens": ["macht", "ei\u00b7nen", "stil\u00b7len", "Zank", "in", "mei\u00b7nem", "Sehn", "und", "R\u00fc\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], 