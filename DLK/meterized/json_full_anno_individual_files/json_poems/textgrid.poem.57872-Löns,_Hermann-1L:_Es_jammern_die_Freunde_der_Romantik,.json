{"textgrid.poem.57872": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es jammern die Freunde der Romantik,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es jammern die Freunde der Romantik,", "tokens": ["Es", "jam\u00b7mern", "die", "Freun\u00b7de", "der", "Ro\u00b7man\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gar zu prosaisch w\u00fcrde die Welt.", "tokens": ["Gar", "zu", "pro\u00b7sa\u00b7isch", "w\u00fcr\u00b7de", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Na, Kinder, kommt nur nach Hannover,", "tokens": ["Na", ",", "Kin\u00b7der", ",", "kommt", "nur", "nach", "Han\u00b7no\u00b7ver", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "ADV", "APPR", "NE", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier hat die Romantik noch ein Feld.", "tokens": ["Hier", "hat", "die", "Ro\u00b7man\u00b7tik", "noch", "ein", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es steht in der Nordstadt ein riesiges Bauwerk,", "tokens": ["Es", "steht", "in", "der", "Nord\u00b7stadt", "ein", "rie\u00b7si\u00b7ges", "Bau\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gespenstig wie eine R\u00e4uberburg,", "tokens": ["Ge\u00b7spens\u00b7tig", "wie", "ei\u00b7ne", "R\u00e4u\u00b7ber\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Kassenboten, die hierher kommen,", "tokens": ["Den", "Kas\u00b7sen\u00b7bo\u00b7ten", ",", "die", "hier\u00b7her", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nimmt man das Geld ab und haut sie durch.", "tokens": ["Nimmt", "man", "das", "Geld", "ab", "und", "haut", "sie", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Verbrechen und Liederlichkeit, die geben", "tokens": ["Ver\u00b7bre\u00b7chen", "und", "Lie\u00b7der\u00b7lich\u00b7keit", ",", "die", "ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sich oftmals hier ein Stelldichein,", "tokens": ["Sich", "oft\u00b7mals", "hier", "ein", "Stell\u00b7dich\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer oberfl\u00e4chlich die Sache betrachtet,", "tokens": ["Wer", "o\u00b7berf\u00b7l\u00e4ch\u00b7lich", "die", "Sa\u00b7che", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der denkt verwundert: \u00bbDarf denn das sein?\u00ab", "tokens": ["Der", "denkt", "ver\u00b7wun\u00b7dert", ":", "\u00bb", "Darf", "denn", "das", "sein", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "$.", "$(", "VMFIN", "ADV", "ART", "VAINF", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "So etwas zu fragen ist faktisch l\u00e4ppisch.", "tokens": ["So", "et\u00b7was", "zu", "fra\u00b7gen", "ist", "fak\u00b7tisch", "l\u00e4p\u00b7pisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKZU", "VVINF", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Denkt doch blo\u00df nach und seid gescheit:", "tokens": ["Denkt", "doch", "blo\u00df", "nach", "und", "seid", "ge\u00b7scheit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die \u00dcberf\u00fchrung dient ja gerade", "tokens": ["Die", "\u00dc\u00b7berf\u00b7\u00fch\u00b7rung", "dient", "ja", "ge\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der \u00f6ffentlichen Sicherheit!", "tokens": ["Der", "\u00f6f\u00b7fent\u00b7li\u00b7chen", "Si\u00b7cher\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "d