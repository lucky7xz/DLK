{"textgrid.poem.40095": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Protest", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Deutschheit wollet ihr bekehren,", "tokens": ["Zur", "Deutschheit", "wol\u00b7let", "ihr", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00fcgt ihr der Menschheit ins Gesicht?", "tokens": ["l\u00fcgt", "ihr", "der", "Menschheit", "ins", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "die Mannesw\u00fcrde wollt ihr lehren", "tokens": ["die", "Man\u00b7nes\u00b7w\u00fcr\u00b7de", "wollt", "ihr", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und ehrt die Menschenw\u00fcrde nicht?", "tokens": ["und", "ehrt", "die", "Men\u00b7schen\u00b7w\u00fcr\u00b7de", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch m\u00f6gt ihr ruhig weitersingen", "tokens": ["Doch", "m\u00f6gt", "ihr", "ru\u00b7hig", "wei\u00b7ter\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von eurer eignen Herrlichkeit:", "tokens": ["von", "eu\u00b7rer", "eig\u00b7nen", "Herr\u00b7