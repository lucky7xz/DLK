{"textgrid.poem.61119": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Winter mit den eis'gen Locken", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Winter mit den eis'gen Locken", "tokens": ["Win\u00b7ter", "mit", "den", "eis'\u00b7gen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War mir immer sonst so leid,", "tokens": ["War", "mir", "im\u00b7mer", "sonst", "so", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er hielt mit seinen Flocken", "tokens": ["Denn", "er", "hielt", "mit", "sei\u00b7nen", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Freuden eingeschneit.", "tokens": ["Al\u00b7le", "Freu\u00b7den", "ein\u00b7ge\u00b7schneit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die V\u00f6glein lustig sangen,", "tokens": ["Wenn", "die", "V\u00f6\u00b7glein", "lus\u00b7tig", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das B\u00e4chlein rauschend zog,", "tokens": ["Wenn", "das", "B\u00e4ch\u00b7lein", "rau\u00b7schend", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam er pl\u00f6tzlich hergegangen", "tokens": ["Kam", "er", "pl\u00f6tz\u00b7lich", "her\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein m\u00fcrr'scher P\u00e4dagog:", "tokens": ["Wie", "ein", "m\u00fcrr'\u00b7scher", "P\u00e4d\u00b7a\u00b7gog", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbv\u00f6glein, la\u00dft das dumme L\u00e4rmen!", "tokens": ["\u00bb", "v\u00f6\u00b7glein", ",", "la\u00dft", "das", "dum\u00b7me", "L\u00e4r\u00b7men", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcfte, la\u00dft das laue Wehn!", "tokens": ["L\u00fcf\u00b7te", ",", "la\u00dft", "das", "lau\u00b7e", "Wehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4chlein, willst du ewig schw\u00e4rmen?", "tokens": ["B\u00e4ch\u00b7lein", ",", "willst", "du", "e\u00b7wig", "schw\u00e4r\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser ist's, fein still zu stehn.", "tokens": ["Bes\u00b7ser", "ist's", ",", "fein", "still", "zu", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fort, du ausgela\u00dfne Erde,", "tokens": ["Fort", ",", "du", "aus\u00b7ge\u00b7la\u00df\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem bunten Narrenkleid!", "tokens": ["Mit", "dem", "bun\u00b7ten", "Nar\u00b7ren\u00b7kleid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein Anblick ehrbar werde,", "tokens": ["Da\u00df", "dein", "An\u00b7blick", "ehr\u00b7bar", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt' ich schon ein Hemd bereit.", "tokens": ["Halt'", "ich", "schon", "ein", "Hemd", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ihr andern wilden Rangen,", "tokens": ["Und", "ihr", "an\u00b7dern", "wil\u00b7den", "Ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumenduft und Sonnenstrahl,", "tokens": ["Blu\u00b7men\u00b7duft", "und", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner soll sich unterfangen,", "tokens": ["Kei\u00b7ner", "soll", "sich", "un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir zu st\u00f6ren die Moral.\u00ab", "tokens": ["Mir", "zu", "st\u00f6\u00b7ren", "die", "Mo\u00b7ral", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und die Blumen wurden selten,", "tokens": ["Und", "die", "Blu\u00b7men", "wur\u00b7den", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4chlein stand, und Vogel schwieg,", "tokens": ["B\u00e4ch\u00b7lein", "stand", ",", "und", "Vo\u00b7gel", "schwieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der P\u00e4dagog mit Schelten", "tokens": ["Als", "der", "P\u00e4d\u00b7a\u00b7gog", "mit", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Eiskatheder stieg.", "tokens": ["Auf", "den", "Eis\u00b7ka\u00b7the\u00b7der", "stieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schadenfroh mit arger T\u00fccke", "tokens": ["Scha\u00b7den\u00b7froh", "mit", "ar\u00b7ger", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlug er in den lust'gen Wald,", "tokens": ["Schlug", "er", "in", "den", "lust'\u00b7gen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es stob aus der Per\u00fccke", "tokens": ["Und", "es", "stob", "aus", "der", "Pe\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm ein Schneegew\u00f6lk alsbald.", "tokens": ["Ihm", "ein", "Schnee\u00b7ge\u00b7w\u00f6lk", "als\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der Sturm, sein b\u00f6ser Husten,", "tokens": ["Und", "der", "Sturm", ",", "sein", "b\u00f6\u00b7ser", "Hus\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df sich h\u00f6ren weit und breit,", "tokens": ["Lie\u00df", "sich", "h\u00f6\u00b7ren", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir armen Menschen wu\u00dften", "tokens": ["Und", "wir", "ar\u00b7men", "Men\u00b7schen", "wu\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts zu tun in solcher Zeit. -", "tokens": ["Nichts", "zu", "tun", "in", "sol\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch der S\u00fcden, o wie ist er", "tokens": ["Doch", "der", "S\u00fc\u00b7den", ",", "o", "wie", "ist", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "FM", "KOKOM", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doppelt nun mir lieb und wert,", "tokens": ["Dop\u00b7pelt", "nun", "mir", "lieb", "und", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit er diesen Erzphilister", "tokens": ["Seit", "er", "die\u00b7sen", "Erz\u00b7phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber zur Vernunft bekehrt!", "tokens": ["Sel\u00b7ber", "zur", "Ver\u00b7nunft", "be\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nicht mehr in die enge Stube", "tokens": ["Nicht", "mehr", "in", "die", "en\u00b7ge", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft mich jetzt der Januar,", "tokens": ["Schlie\u00dft", "mich", "jetzt", "der", "Ja\u00b7nu\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, er ward ein toller Bube,", "tokens": ["Nein", ",", "er", "ward", "ein", "tol\u00b7ler", "Bu\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat ein Auge gro\u00df und klar.", "tokens": ["Hat", "ein", "Au\u00b7ge", "gro\u00df", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "An den Bergesh\u00e4ngen springt er", "tokens": ["An", "den", "Ber\u00b7ges\u00b7h\u00e4n\u00b7gen", "springt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig hin im gr\u00fcnen Kleid;", "tokens": ["Lus\u00b7tig", "hin", "im", "gr\u00fc\u00b7nen", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den hohen L\u00fcften singt er,", "tokens": ["In", "den", "ho\u00b7hen", "L\u00fcf\u00b7ten", "singt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen streut er weit und breit.", "tokens": ["Blu\u00b7men", "streut", "er", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Kommt einmal Gew\u00f6lk gezogen,", "tokens": ["Kommt", "ein\u00b7mal", "Ge\u00b7w\u00f6lk", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wurmt ihn gleich der dunkle Tand,", "tokens": ["Wurmt", "ihn", "gleich", "der", "dunk\u00b7le", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den bunten Regenbogen", "tokens": ["Und", "den", "bun\u00b7ten", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spannt er drauf mit leichter Hand.", "tokens": ["Spannt", "er", "drauf", "mit", "leich\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "G\u00e4nzlich hat er auch vergessen", "tokens": ["G\u00e4nz\u00b7lich", "hat", "er", "auch", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "P\u00e4dagogik und Moral,", "tokens": ["P\u00e4d\u00b7a\u00b7go\u00b7gik", "und", "Mo\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unter Palmen und Zypressen", "tokens": ["Un\u00b7ter", "Pal\u00b7men", "und", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sonnt er m\u00fc\u00dfig sich im Strahl.", "tokens": ["Sonnt", "er", "m\u00fc\u00b7\u00dfig", "sich", "im", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Manchmal nur in seltnen Zungen", "tokens": ["Manch\u00b7mal", "nur", "in", "selt\u00b7nen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwatzt er von der Freude Macht,", "tokens": ["Schwatzt", "er", "von", "der", "Freu\u00b7de", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinem Hauch durchdrungen", "tokens": ["Und", "von", "sei\u00b7nem", "Hauch", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich dieses Lied erdacht.", "tokens": ["Hab'", "ich", "die\u00b7ses", "Lied", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Winter mit den eis'gen Locken", "tokens": ["Win\u00b7ter", "mit", "den", "eis'\u00b7gen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War mir immer sonst so leid,", "tokens": ["War", "mir", "im\u00b7mer", "sonst", "so", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er hielt mit seinen Flocken", "tokens": ["Denn", "er", "hielt", "mit", "sei\u00b7nen", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Freuden eingeschneit.", "tokens": ["Al\u00b7le", "Freu\u00b7den", "ein\u00b7ge\u00b7schneit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wenn die V\u00f6glein lustig sangen,", "tokens": ["Wenn", "die", "V\u00f6\u00b7glein", "lus\u00b7tig", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das B\u00e4chlein rauschend zog,", "tokens": ["Wenn", "das", "B\u00e4ch\u00b7lein", "rau\u00b7schend", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kam er pl\u00f6tzlich hergegangen", "tokens": ["Kam", "er", "pl\u00f6tz\u00b7lich", "her\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein m\u00fcrr'scher P\u00e4dagog:", "tokens": ["Wie", "ein", "m\u00fcrr'\u00b7scher", "P\u00e4d\u00b7a\u00b7gog", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "\u00bbv\u00f6glein, la\u00dft das dumme L\u00e4rmen!", "tokens": ["\u00bb", "v\u00f6\u00b7glein", ",", "la\u00dft", "das", "dum\u00b7me", "L\u00e4r\u00b7men", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcfte, la\u00dft das laue Wehn!", "tokens": ["L\u00fcf\u00b7te", ",", "la\u00dft", "das", "lau\u00b7e", "Wehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00e4chlein, willst du ewig schw\u00e4rmen?", "tokens": ["B\u00e4ch\u00b7lein", ",", "willst", "du", "e\u00b7wig", "schw\u00e4r\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser ist's, fein still zu stehn.", "tokens": ["Bes\u00b7ser", "ist's", ",", "fein", "still", "zu", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Fort, du ausgela\u00dfne Erde,", "tokens": ["Fort", ",", "du", "aus\u00b7ge\u00b7la\u00df\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem bunten Narrenkleid!", "tokens": ["Mit", "dem", "bun\u00b7ten", "Nar\u00b7ren\u00b7kleid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dein Anblick ehrbar werde,", "tokens": ["Da\u00df", "dein", "An\u00b7blick", "ehr\u00b7bar", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt' ich schon ein Hemd bereit.", "tokens": ["Halt'", "ich", "schon", "ein", "Hemd", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Und ihr andern wilden Rangen,", "tokens": ["Und", "ihr", "an\u00b7dern", "wil\u00b7den", "Ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumenduft und Sonnenstrahl,", "tokens": ["Blu\u00b7men\u00b7duft", "und", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner soll sich unterfangen,", "tokens": ["Kei\u00b7ner", "soll", "sich", "un\u00b7ter\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir zu st\u00f6ren die Moral.\u00ab", "tokens": ["Mir", "zu", "st\u00f6\u00b7ren", "die", "Mo\u00b7ral", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Und die Blumen wurden selten,", "tokens": ["Und", "die", "Blu\u00b7men", "wur\u00b7den", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4chlein stand, und Vogel schwieg,", "tokens": ["B\u00e4ch\u00b7lein", "stand", ",", "und", "Vo\u00b7gel", "schwieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der P\u00e4dagog mit Schelten", "tokens": ["Als", "der", "P\u00e4d\u00b7a\u00b7gog", "mit", "Schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Eiskatheder stieg.", "tokens": ["Auf", "den", "Eis\u00b7ka\u00b7the\u00b7der", "stieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Schadenfroh mit arger T\u00fccke", "tokens": ["Scha\u00b7den\u00b7froh", "mit", "ar\u00b7ger", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlug er in den lust'gen Wald,", "tokens": ["Schlug", "er", "in", "den", "lust'\u00b7gen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es stob aus der Per\u00fccke", "tokens": ["Und", "es", "stob", "aus", "der", "Pe\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm ein Schneegew\u00f6lk alsbald.", "tokens": ["Ihm", "ein", "Schnee\u00b7ge\u00b7w\u00f6lk", "als\u00b7bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Und der Sturm, sein b\u00f6ser Husten,", "tokens": ["Und", "der", "Sturm", ",", "sein", "b\u00f6\u00b7ser", "Hus\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df sich h\u00f6ren weit und breit,", "tokens": ["Lie\u00df", "sich", "h\u00f6\u00b7ren", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir armen Menschen wu\u00dften", "tokens": ["Und", "wir", "ar\u00b7men", "Men\u00b7schen", "wu\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts zu tun in solcher Zeit. -", "tokens": ["Nichts", "zu", "tun", "in", "sol\u00b7cher", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Doch der S\u00fcden, o wie ist er", "tokens": ["Doch", "der", "S\u00fc\u00b7den", ",", "o", "wie", "ist", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "FM", "KOKOM", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doppelt nun mir lieb und wert,", "tokens": ["Dop\u00b7pelt", "nun", "mir", "lieb", "und", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit er diesen Erzphilister", "tokens": ["Seit", "er", "die\u00b7sen", "Erz\u00b7phi\u00b7lis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber zur Vernunft bekehrt!", "tokens": ["Sel\u00b7ber", "zur", "Ver\u00b7nunft", "be\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Nicht mehr in die enge Stube", "tokens": ["Nicht", "mehr", "in", "die", "en\u00b7ge", "Stu\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dft mich jetzt der Januar,", "tokens": ["Schlie\u00dft", "mich", "jetzt", "der", "Ja\u00b7nu\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, er ward ein toller Bube,", "tokens": ["Nein", ",", "er", "ward", "ein", "tol\u00b7ler", "Bu\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat ein Auge gro\u00df und klar.", "tokens": ["Hat", "ein", "Au\u00b7ge", "gro\u00df", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "An den Bergesh\u00e4ngen springt er", "tokens": ["An", "den", "Ber\u00b7ges\u00b7h\u00e4n\u00b7gen", "springt", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lustig hin im gr\u00fcnen Kleid;", "tokens": ["Lus\u00b7tig", "hin", "im", "gr\u00fc\u00b7nen", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den hohen L\u00fcften singt er,", "tokens": ["In", "den", "ho\u00b7hen", "L\u00fcf\u00b7ten", "singt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen streut er weit und breit.", "tokens": ["Blu\u00b7men", "streut", "er", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Kommt einmal Gew\u00f6lk gezogen,", "tokens": ["Kommt", "ein\u00b7mal", "Ge\u00b7w\u00f6lk", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wurmt ihn gleich der dunkle Tand,", "tokens": ["Wurmt", "ihn", "gleich", "der", "dunk\u00b7le", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den bunten Regenbogen", "tokens": ["Und", "den", "bun\u00b7ten", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spannt er drauf mit leichter Hand.", "tokens": ["Spannt", "er", "drauf", "mit", "leich\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "G\u00e4nzlich hat er auch vergessen", "tokens": ["G\u00e4nz\u00b7lich", "hat", "er", "auch", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "P\u00e4dagogik und Moral,", "tokens": ["P\u00e4d\u00b7a\u00b7go\u00b7gik", "und", "Mo\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unter Palmen und Zypressen", "tokens": ["Un\u00b7ter", "Pal\u00b7men", "und", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sonnt er m\u00fc\u00dfig sich im Strahl.", "tokens": ["Sonnt", "er", "m\u00fc\u00b7\u00dfig", "sich", "im", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Manchmal nur in seltnen Zungen", "tokens": ["Manch\u00b7mal", "nur", "in", "selt\u00b7nen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwatzt er von der Freude Macht,", "tokens": ["Schwatzt", "er", "von", "der", "Freu\u00b7de", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinem Hauch durchdrungen", "tokens": ["Und", "von", "sei\u00b7nem", "Hauch", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich dieses Lied erdacht.", "tokens": ["Hab'", "ich", "die\u00b7ses", "Lied", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}