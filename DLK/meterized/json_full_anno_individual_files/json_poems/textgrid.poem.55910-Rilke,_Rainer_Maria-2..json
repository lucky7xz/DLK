{"textgrid.poem.55910": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig sa\u00df und sann den leeren Tag", "tokens": ["Der", "K\u00f6\u00b7nig", "sa\u00df", "und", "sann", "den", "lee\u00b7ren", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "getaner Taten, ungef\u00fchlter L\u00fcste", "tokens": ["ge\u00b7ta\u00b7ner", "Ta\u00b7ten", ",", "un\u00b7ge\u00b7f\u00fchl\u00b7ter", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und seiner Lieblingsh\u00fcndin, der er pflag \u2013.", "tokens": ["und", "sei\u00b7ner", "Lieb\u00b7lings\u00b7h\u00fcn\u00b7din", ",", "der", "er", "pflag", "\u2013", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aber am Abend w\u00f6lbte Abisag", "tokens": ["A\u00b7ber", "am", "A\u00b7bend", "w\u00f6lb\u00b7te", "A\u00b7bi\u00b7sag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "sich \u00fcber ihm. Sein wirres Leben lag", "tokens": ["sich", "\u00fc\u00b7ber", "ihm", ".", "Sein", "wir\u00b7res", "Le\u00b7ben", "lag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", 