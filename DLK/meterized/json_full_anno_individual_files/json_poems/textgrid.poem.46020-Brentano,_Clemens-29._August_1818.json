{"textgrid.poem.46020": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "29. August 1818", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach w\u00e4r' ich doch der Welt schon los!\u00ab", "tokens": ["\u00bb", "ach", "w\u00e4r'", "ich", "doch", "der", "Welt", "schon", "los", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwas haben Sie denn nun schon wieder?\u00ab", "tokens": ["\u00bb", "was", "ha\u00b7ben", "Sie", "denn", "nun", "schon", "wie\u00b7der", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbich sage, wird die Last zu gro\u00df,", "tokens": ["\u00bb", "ich", "sa\u00b7ge", ",", "wird", "die", "Last", "zu", "gro\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So seufzt der Tr\u00e4ger und es zieht ihn nieder.", "tokens": ["So", "seufzt", "der", "Tr\u00e4\u00b7ger", "und", "es", "zieht", "ihn", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ich heut sah, f\u00e4llt mir da ein,", "tokens": ["Was", "ich", "heut", "sah", ",", "f\u00e4llt", "mir", "da", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum konnte ich des Weinens mich erwehren,", "tokens": ["Kaum", "konn\u00b7te", "ich", "des", "Wei\u00b7nens", "mich", "er\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Markte sa\u00df auf einem Stein", "tokens": ["Am", "Mark\u00b7te", "sa\u00df", "auf", "ei\u00b7nem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein altes bleiches Weib in bittern Z\u00e4hren.", "tokens": ["Ein", "al\u00b7tes", "blei\u00b7ches", "Weib", "in", "bit\u00b7tern", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sie hatte bei den Bauren sich", "tokens": ["Sie", "hat\u00b7te", "bei", "den", "Bau\u00b7ren", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bi\u00dfchen Gr\u00fcns und R\u00fcben beigeschnurret", "tokens": ["Ein", "bi\u00df\u00b7chen", "Gr\u00fcns", "und", "R\u00fc\u00b7ben", "bei\u00b7ge\u00b7schnur\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sah kein Mensch sie an, als ich", "tokens": ["Es", "sah", "kein", "Mensch", "sie", "an", ",", "als", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie weinte still vor sich, hat nicht gemurret.\u00ab", "tokens": ["Sie", "wein\u00b7te", "still", "vor", "sich", ",", "hat", "nicht", "ge\u00b7mur\u00b7ret", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "$,", "VAFIN", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbwarum gabst du nicht alles Ihr,", "tokens": ["\u00bb", "wa\u00b7rum", "gabst", "du", "nicht", "al\u00b7les", "Ihr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PTKNEG", "PIS", "PPER", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gott giebt's mir wieder, gern bin ich dein Leiher,\u00ab", "tokens": ["Gott", "giebt's", "mir", "wie\u00b7der", ",", "gern", "bin", "ich", "dein", "Lei\u00b7her", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbich hatte selbst nicht viel bei mir,", "tokens": ["\u00bb", "ich", "hat\u00b7te", "selbst", "nicht", "viel", "bei", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich gab ihr, was ich hatte hin, sechs Dreier.", "tokens": ["Ich", "gab", "ihr", ",", "was", "ich", "hat\u00b7te", "hin", ",", "sechs", "Drei\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "ADV", "$,", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als ich am Knoten zerren mu\u00dft'", "tokens": ["Als", "ich", "am", "Kno\u00b7ten", "zer\u00b7ren", "mu\u00dft'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den das Geld im Schnupftuch war geschlungen,", "tokens": ["In", "den", "das", "Geld", "im", "Schnupf\u00b7tuch", "war", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist ihr so recht aus tiefer Brust", "tokens": ["Ist", "ihr", "so", "recht", "aus", "tie\u00b7fer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schwergef\u00fchltes Klagewort gedrungen.", "tokens": ["Ein", "schwer\u00b7ge\u00b7f\u00fchl\u00b7tes", "Kla\u00b7ge\u00b7wort", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In ihren Scho\u00df sprach sie gar schwer,", "tokens": ["In", "ih\u00b7ren", "Scho\u00df", "sprach", "sie", "gar", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie die Armut deckt mit welken H\u00e4nden,", "tokens": ["Wo", "sie", "die", "Ar\u00b7mut", "deckt", "mit", "wel\u00b7ken", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u203aach wer doch erst da drunten w\u00e4r',", "tokens": ["\u203a", "ach", "wer", "doch", "erst", "da", "drun\u00b7ten", "w\u00e4r'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "PWS", "ADV", "ADV", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier wird doch nimmermehr die Sorge enden!\u2039", "tokens": ["Hier", "wird", "doch", "nim\u00b7mer\u00b7mehr", "die", "Sor\u00b7ge", "en\u00b7den", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich dacht', das konnt' ich wohl verstehn,", "tokens": ["Ich", "dacht'", ",", "das", "konnt'", "ich", "wohl", "ver\u00b7stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das brauchtest du mir gar nicht erst zu sagen,", "tokens": ["Das", "brauch\u00b7test", "du", "mir", "gar", "nicht", "erst", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich hab' es dir gleich angesehn,", "tokens": ["Ich", "hab'", "es", "dir", "gleich", "an\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wollte dich darum auch gar nicht fragen.", "tokens": ["Und", "woll\u00b7te", "dich", "da\u00b7rum", "auch", "gar", "nicht", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PAV", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Warum denn fragst du mich, lieb Kind!", "tokens": ["Wa\u00b7rum", "denn", "fragst", "du", "mich", ",", "lieb", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u203awas haben Sie denn nun schon wieder?\u2039", "tokens": ["\u203a", "was", "ha\u00b7ben", "Sie", "denn", "nun", "schon", "wie\u00b7der", "?", "\u2039"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird denn an mir dein Scharfblick blind?", "tokens": ["Wird", "denn", "an", "mir", "dein", "Scharf\u00b7blick", "blind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich zieht es wie die Arme auch ja nieder.", "tokens": ["Mich", "zieht", "es", "wie", "die", "Ar\u00b7me", "auch", "ja", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Auch ich sitz' alt und bleich am Stein,", "tokens": ["Auch", "ich", "sitz'", "alt", "und", "bleich", "am", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' mir ein bi\u00dfchen Gr\u00fcns von dir erschnurret,", "tokens": ["Hab'", "mir", "ein", "bi\u00df\u00b7chen", "Gr\u00fcns", "von", "dir", "er\u00b7schnur\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Weinen siehst auch du allein,", "tokens": ["Mein", "Wei\u00b7nen", "siehst", "auch", "du", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ich hab' nur geseufzt und nicht gemurret.", "tokens": ["Auch", "ich", "hab'", "nur", "ge\u00b7seufzt", "und", "nicht", "ge\u00b7mur\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Auch mir reichst aus dem Schnupftuch du", "tokens": ["Auch", "mir", "reichst", "aus", "dem", "Schnupf\u00b7tuch", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Schatz, sechs Dreier, gestern, heute, morgen", "tokens": ["Den", "Schatz", ",", "sechs", "Drei\u00b7er", ",", "ge\u00b7stern", ",", "heu\u00b7te", ",", "mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "CARD", "NN", "$,", "ADV", "$,", "ADV", "$,", "ADV"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch ich m\u00f6cht' bald hinab zur Ruh'", "tokens": ["Auch", "ich", "m\u00f6cht'", "bald", "hin\u00b7ab", "zur", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn hier wird doch kein Ende je der Sorgen.", "tokens": ["Denn", "hier", "wird", "doch", "kein", "En\u00b7de", "je", "der", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ich sorg', es nahe eine Zeit", "tokens": ["Ich", "sor\u00b7g'", ",", "es", "na\u00b7he", "ei\u00b7ne", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo du den Knoten allzu fest wirst schlingen,", "tokens": ["Wo", "du", "den", "Kno\u00b7ten", "all\u00b7zu", "fest", "wirst", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKA", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da tun dir dann die Finger leid,", "tokens": ["Da", "tun", "dir", "dann", "die", "Fin\u00b7ger", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die milde Gabe mir herauszuzwingen.", "tokens": ["Die", "mil\u00b7de", "Ga\u00b7be", "mir", "her\u00b7aus\u00b7zu\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ich sorg', es steigt ein Tag empor,", "tokens": ["Ich", "sor\u00b7g'", ",", "es", "steigt", "ein", "Tag", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ich mein' er guckt schon aus dem Keller dr\u00fcben,", "tokens": ["Ich", "mein'", "er", "guckt", "schon", "aus", "dem", "Kel\u00b7ler", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wirfst du mir die Dreier vor,", "tokens": ["Da", "wirfst", "du", "mir", "die", "Drei\u00b7er", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und g\u00f6nnst mir nicht das bi\u00dfchen Gr\u00fcns und R\u00fc