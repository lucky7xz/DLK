{"textgrid.poem.61141": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Mein Weg", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6r' es wohl, es rufen die Partein:", "tokens": ["Ich", "h\u00f6r'", "es", "wohl", ",", "es", "ru\u00b7fen", "die", "Par\u00b7tein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbkomm her und woll' uns endlich angeh\u00f6ren!", "tokens": ["\u00bb", "komm", "her", "und", "woll'", "uns", "end\u00b7lich", "an\u00b7ge\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "KON", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der r\u00fcst'ge Harfner sei zu unsern Ch\u00f6ren", "tokens": ["Der", "r\u00fcst'\u00b7ge", "Harf\u00b7ner", "sei", "zu", "un\u00b7sern", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schling als Kranz dein Lied um unsern Wein!\u00ab", "tokens": ["Und", "schling", "als", "Kranz", "dein", "Lied", "um", "un\u00b7sern", "Wein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein ewig Echo bleibt ein ruhig: \u00bbNein!\u00ab", "tokens":