{"textgrid.poem.39828": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Auf Friedrich Creuzers Jubil\u00e4um", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erfinderin, Bewegung,", "tokens": ["Er\u00b7fin\u00b7de\u00b7rin", ",", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du dringst herein mit Macht;", "tokens": ["Du", "dringst", "her\u00b7ein", "mit", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch diese Bergumhegung", "tokens": ["Auch", "die\u00b7se", "Ber\u00b7gum\u00b7he\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist dein willkommner Schacht.", "tokens": ["Ist", "dein", "will\u00b7komm\u00b7ner", "Schacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4\u00dft, wo schaumdurchflogen", "tokens": ["Du", "l\u00e4\u00dft", ",", "wo", "schaum\u00b7durch\u00b7flo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Flut aus Steinen droht,", "tokens": ["Die", "Flut", "aus", "Stei\u00b7nen", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im freien Waldstrom wogen", "tokens": ["Im", "frei\u00b7en", "Wald\u00b7strom", "wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein dampfbeschwingtes Boot:", "tokens": ["Dein", "dampf\u00b7be\u00b7schwing\u00b7tes", "Boot", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wo er, Fluren theilend,", "tokens": ["Und", "wo", "er", ",", "Flu\u00b7ren", "thei\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entflieht wie ein Gescho\u00df,", "tokens": ["Ent\u00b7flieht", "wie", "ein", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da \u00fcberholen eilend", "tokens": ["Da", "\u00fc\u00b7berh\u00b7o\u00b7len", "ei\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn Wagen ohne Ro\u00df. \u2013", "tokens": ["Ihn", "Wa\u00b7gen", "oh\u00b7ne", "Ro\u00df", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Erfinderin, Bewegung,", "tokens": ["Er\u00b7fin\u00b7de\u00b7rin", ",", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Versammelt sind wir auch", "tokens": ["Ver\u00b7sam\u00b7melt", "sind", "wir", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu preisen deine Regung,", "tokens": ["Zu", "prei\u00b7sen", "dei\u00b7ne", "Re\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht die von Rad und Rauch:", "tokens": ["Nicht", "die", "von", "Rad", "und", "Rauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Regung in den Geistern,", "tokens": ["Die", "Re\u00b7gung", "in", "den", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die einst in diesem Thal", "tokens": ["Die", "einst", "in", "die\u00b7sem", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ausging von jungen Meistern", "tokens": ["Aus\u00b7ging", "von", "jun\u00b7gen", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als neuer Strom und Stral.", "tokens": ["Als", "neu\u00b7er", "Strom", "und", "Stral", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Entdeckungslust und Ahnung", "tokens": ["Ent\u00b7de\u00b7ckungs\u00b7lust", "und", "Ah\u00b7nung"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Erprobte da die Kraft", "tokens": ["Er\u00b7prob\u00b7te", "da", "die", "Kraft"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unternahm die Bahnung", "tokens": ["Und", "un\u00b7ter\u00b7nahm", "die", "Bah\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erneuter Wissenschaft.", "tokens": ["Er\u00b7neu\u00b7ter", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vor einem weiten Meere", "tokens": ["Vor", "ei\u00b7nem", "wei\u00b7ten", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lag dieses Thales Bucht,", "tokens": ["Lag", "die\u00b7ses", "Tha\u00b7les", "Bucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und F\u00e4hre ward um F\u00e4hre", "tokens": ["Und", "F\u00e4h\u00b7re", "ward", "um", "F\u00e4h\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gezimmert in der Schlucht.", "tokens": ["Ge\u00b7zim\u00b7mert", "in", "der", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die k\u00fchnsten Schiffe sandte", "tokens": ["Die", "k\u00fchns\u00b7ten", "Schif\u00b7fe", "sand\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Geist als Kreuzer aus,", "tokens": ["Der", "Geist", "als", "Kreu\u00b7zer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ohne Beute wandte", "tokens": ["Und", "oh\u00b7ne", "Beu\u00b7te", "wand\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Segel sich nach Haus.", "tokens": ["Kein", "Se\u00b7gel", "sich", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Alten ruhn, sie schlafen", "tokens": ["Die", "Al\u00b7ten", "ruhn", ",", "sie", "schla\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf ihrem Lorbeerkranz,", "tokens": ["Auf", "ih\u00b7rem", "Lor\u00b7beer\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sind im andern Hafen,", "tokens": ["Sie", "sind", "im", "an\u00b7dern", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie, dieses Werftes Glanz:", "tokens": ["Sie", ",", "die\u00b7ses", "Werf\u00b7tes", "Glanz", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch Einer blieb dem Lande,", "tokens": ["Doch", "Ei\u00b7ner", "blieb", "dem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ankert fest am Strande", "tokens": ["Er", "an\u00b7kert", "fest", "am", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit stolzer Flagge Zier.", "tokens": ["Mit", "stol\u00b7zer", "Flag\u00b7ge", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Tiefsinn in tausend Bildern", "tokens": ["Tief\u00b7sinn", "in", "tau\u00b7send", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "CARD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "War seine Siegesfracht;", "tokens": ["War", "sei\u00b7ne", "Sie\u00b7ges\u00b7fracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man liest auf blanken Schildern", "tokens": ["Man", "liest", "auf", "blan\u00b7ken", "Schil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Zeichen seiner Jacht:", "tokens": ["Das", "Zei\u00b7chen", "sei\u00b7ner", "Jacht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In goldnen, von dem Schiff.", "tokens": ["In", "gold\u00b7nen", ",", "von", "dem", "Schiff", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das flog, umrollt von Wettern,", "tokens": ["Das", "flog", ",", "um\u00b7rollt", "von", "Wet\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vorbei an manchem Riff.", "tokens": ["Vor\u00b7bei", "an", "man\u00b7chem", "Riff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.13": {"line.1": {"text": "Es gl\u00e4nzt in lautrem Ruhme", "tokens": ["Es", "gl\u00e4nzt", "in", "laut\u00b7rem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein schimmerndes Verdeck,", "tokens": ["Sein", "schim\u00b7mern\u00b7des", "Ver\u00b7deck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und unsres Festes Blume", "tokens": ["Und", "uns\u00b7res", "Fes\u00b7tes", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versteckt nicht Einen Leck.", "tokens": ["Ver\u00b7steckt", "nicht", "Ei\u00b7nen", "Leck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Noch funkl' es lang am Ufer,", "tokens": ["Noch", "funkl'", "es", "lang", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leuchtthurm in der Nacht,", "tokens": ["Ein", "Leucht\u00b7thurm", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Mahner und ein Rufer", "tokens": ["Ein", "Mah\u00b7ner", "und", "ein", "Ru\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Kampf in edler Schlacht!", "tokens": ["Zum", "Kampf", "in", "ed\u00b7ler", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Denn wie viel Seglerscharen", "tokens": ["Denn", "wie", "viel", "Seg\u00b7ler\u00b7scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat dieser Golf entsandt;", "tokens": ["Hat", "die\u00b7ser", "Golf", "ent\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie oft, seit vierzig Jahren,", "tokens": ["Wie", "oft", ",", "seit", "vier\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stie\u00df Forschung hier vom Strand! \u2013", "tokens": ["Stie\u00df", "For\u00b7schung", "hier", "vom", "Strand", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Erfinderin, Bewegung,", "tokens": ["Er\u00b7fin\u00b7de\u00b7rin", ",", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchathme diesen Port!", "tokens": ["Durch\u00b7ath\u00b7me", "die\u00b7sen", "Port", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie seit der Grundsteinlegung", "tokens": ["Wie", "seit", "der", "Grund\u00b7stein\u00b7le\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Hafens, wirke fort!", "tokens": ["Des", "Ha\u00b7fens", ",", "wir\u00b7ke", "fort", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Nicht blos aus Boot und Wagen", "tokens": ["Nicht", "blos", "aus", "Boot", "und", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steig' auf, in Dampf und Rauch;", "tokens": ["Steig'", "auf", ",", "in", "Dampf", "und", "Rauch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Von deinem Lebenshauch!", "tokens": ["Von", "dei\u00b7nem", "Le\u00b7bens\u00b7hauch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "me