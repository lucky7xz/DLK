{"dta.poem.10755": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  Von der Liebsten flucht in Kriegszeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ach was hab ich nun erlebt vor schwere Zeiten/", "tokens": ["Ach", "ach", "was", "hab", "ich", "nun", "er\u00b7lebt", "vor", "schwe\u00b7re", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir wird mein Mut vnd Sinn von Vnmuth all verst\u00f6rt/", "tokens": ["Mir", "wird", "mein", "Mut", "vnd", "Sinn", "von", "Vn\u00b7muth", "all", "ver\u00b7st\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "PIAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Vatterlandt das wird von Freund vnd Feind verh\u00f6rt", "tokens": ["Das", "Vat\u00b7ter\u00b7landt", "das", "wird", "von", "Freund", "vnd", "Feind", "ver\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VAFIN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem mit seinem Volck ", "tokens": ["In", "dem", "mit", "sei\u00b7nem", "Volck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein liebste weicht von mir/ ich kan sie nicht geleiten/", "tokens": ["Mein", "liebs\u00b7te", "weicht", "von", "mir", "/", "ich", "kan", "sie", "nicht", "ge\u00b7lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "APPR", "PPER", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dagegen ", "tokens": ["Da\u00b7ge\u00b7gen"], "token_info": ["word"], "pos": ["PAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Es hilffet nicht/ wie sehr sich auch mein Hertze wehrt/", "tokens": ["Es", "hilf\u00b7fet", "nicht", "/", "wie", "sehr", "sich", "auch", "mein", "Hert\u00b7ze", "wehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOKOM", "ADV", "PRF", "ADV", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Kein Mensche kan zugleich mit zweyen G\u00f6ttern streiten.", "tokens": ["Kein", "Men\u00b7sche", "kan", "zu\u00b7gleich", "mit", "zwe\u00b7yen", "G\u00f6t\u00b7tern", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPR", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun was der Krieg hinnimbt/ es sey Gut oder Goldt/", "tokens": ["Nun", "was", "der", "Krieg", "hin\u00b7nimbt", "/", "es", "sey", "Gut", "o\u00b7der", "Goldt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVPP", "$(", "PPER", "VAFIN", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Kan alles mit der Zeit widrumb gewonnen werden/", "tokens": ["Kan", "al\u00b7les", "mit", "der", "Zeit", "wid\u00b7rumb", "ge\u00b7won\u00b7nen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihrs gleichen aber kan ich finden nicht vf Erden.", "tokens": ["Ihrs", "glei\u00b7chen", "a\u00b7ber", "kan", "ich", "fin\u00b7den", "nicht", "vf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADV", "VMFIN", "PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Wann ", "tokens": ["Wann"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Ein F\u00fcncklein meiner Lieb vor jhr geleite geben.", "tokens": ["Ein", "F\u00fcnc\u00b7klein", "mei\u00b7ner", "Lieb", "vor", "jhr", "ge\u00b7lei\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}