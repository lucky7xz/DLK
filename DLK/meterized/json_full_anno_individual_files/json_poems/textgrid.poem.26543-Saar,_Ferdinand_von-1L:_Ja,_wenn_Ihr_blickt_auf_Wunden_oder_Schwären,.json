{"textgrid.poem.26543": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, wenn Ihr blickt auf Wunden oder Schw\u00e4ren,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, wenn Ihr blickt auf Wunden oder Schw\u00e4ren,", "tokens": ["Ja", ",", "wenn", "Ihr", "blickt", "auf", "Wun\u00b7den", "o\u00b7der", "Schw\u00e4\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn die Leute liegen auf dem Schragen,", "tokens": ["Und", "wenn", "die", "Leu\u00b7te", "lie\u00b7gen", "auf", "dem", "Schra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da d\u00fcrft Ihr Trost und Hilfe nicht versagen \u2013", "tokens": ["Da", "d\u00fcrft", "Ihr", "Trost", "und", "Hil\u00b7fe", "nicht", "ver\u00b7sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weint sogar noch ernst gemeinte Z\u00e4hren.", "tokens": ["Und", "weint", "so\u00b7gar", "noch", "ernst", "ge\u00b7mein\u00b7te", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wann aber sah man Euch ein Ungl\u00fcck