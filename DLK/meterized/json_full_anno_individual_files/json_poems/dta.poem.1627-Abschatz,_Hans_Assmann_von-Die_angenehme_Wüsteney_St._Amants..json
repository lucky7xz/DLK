{"dta.poem.1627": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die angenehme W\u00fcsteney St. Amants.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wiewohl schl\u00e4gt mir die \u00f6de Gegend zu! ", "tokens": ["Wie\u00b7wohl", "schl\u00e4gt", "mir", "die", "\u00f6\u00b7de", "Ge\u00b7gend", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Di\u00df brauner Nacht geweyhte Feld/", "tokens": ["Di\u00df", "brau\u00b7ner", "Nacht", "ge\u00b7weyh\u00b7te", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entfernet vom Geschrey der Welt/", "tokens": ["Ent\u00b7fer\u00b7net", "vom", "Ge\u00b7schrey", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meiner Unruh s\u00fcsse Ruh:", "tokens": ["Ist", "mei\u00b7ner", "Un\u00b7ruh", "s\u00fcs\u00b7se", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Di\u00df Thal/ darinn ich mich verborgen", "tokens": ["Di\u00df", "Thal", "/", "da\u00b7rinn", "ich", "mich", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "NN", "$(", "PAV", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist ein Vegr\u00e4bnis meiner Sorgen.", "tokens": ["Ist", "ein", "Ve\u00b7gr\u00e4b\u00b7nis", "mei\u00b7ner", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Auge schaut hier mit Vergn\u00fcgen an", "tokens": ["Mein", "Au\u00b7ge", "schaut", "hier", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der dick-belaubten B\u00e4ume Schaar/", "tokens": ["Der", "dick\u00b7be\u00b7laub\u00b7ten", "B\u00e4u\u00b7me", "Schaar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darvon so mancher gleiche Jahr", "tokens": ["Dar\u00b7von", "so", "man\u00b7cher", "glei\u00b7che", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Welt und Erde z\u00e4hlen kan/", "tokens": ["Mit", "Welt", "und", "Er\u00b7de", "z\u00e4h\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den seiner Faunen Gunst bewahret", "tokens": ["Den", "sei\u00b7ner", "Fau\u00b7nen", "Gunst", "be\u00b7wah\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und bi\u00df auff diese Zeit gesparet.", "tokens": ["Und", "bi\u00df", "auff", "die\u00b7se", "Zeit", "ge\u00b7spa\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die frische Lufft spielt um ihr stoltzes Haubt/", "tokens": ["Die", "fri\u00b7sche", "Lufft", "spielt", "um", "ihr", "stolt\u00b7zes", "Haubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Zephyr k\u00fcst sie Tag und Racht/", "tokens": ["Und", "Ze\u00b7phyr", "k\u00fcst", "sie", "Tag", "und", "Racht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts als der hohe Wipffel macht", "tokens": ["Nichts", "als", "der", "ho\u00b7he", "Wipf\u00b7fel", "macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr greises Alterthum beglaubt:", "tokens": ["Ihr", "grei\u00b7ses", "Al\u00b7ter\u00b7thum", "be\u00b7glaubt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie den ersten Tag geschienen", "tokens": ["Wie", "sie", "den", "ers\u00b7ten", "Tag", "ge\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sieht man sie noch heute gr\u00fcnen.", "tokens": ["So", "sieht", "man", "sie", "noch", "heu\u00b7te", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier ists/ wo mir in stiller Einsamkeit", "tokens": ["Hier", "ists", "/", "wo", "mir", "in", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Schatten-Kind die Nachtigall", "tokens": ["Das", "Schat\u00b7ten\u00b7Kind", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihrer s\u00fcssen Stimme Schall", "tokens": ["Mit", "ih\u00b7rer", "s\u00fcs\u00b7sen", "Stim\u00b7me", "Schall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertreibt so manche schwere Zeit/", "tokens": ["Ver\u00b7treibt", "so", "man\u00b7che", "schwe\u00b7re", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sie den L\u00fcfften saget wieder", "tokens": ["Wo", "sie", "den", "L\u00fcff\u00b7ten", "sa\u00b7get", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Inhalt meiner Trauer-Lieder.", "tokens": ["Den", "In\u00b7halt", "mei\u00b7ner", "Trau\u00b7e\u00b7rLie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier wird von mir mit h\u00f6chster Lust geschaut", "tokens": ["Hier", "wird", "von", "mir", "mit", "h\u00f6chs\u00b7ter", "Lust", "ge\u00b7schaut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie auff den Felsen dar und hier", "tokens": ["Wie", "auff", "den", "Fel\u00b7sen", "dar", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umklettert manch verwegnes Thier/", "tokens": ["Um\u00b7klet\u00b7tert", "manch", "ver\u00b7weg\u00b7nes", "Thier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie die Natur so k\u00fchn gebaut/", "tokens": ["Wie", "die", "Na\u00b7tur", "so", "k\u00fchn", "ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und offt so schlechten Grund/ offt keinen", "tokens": ["Und", "offt", "so", "schlech\u00b7ten", "Grund", "/", "offt", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$(", "ADV", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gegeben hat den schweren Steinen.", "tokens": ["Ge\u00b7ge\u00b7ben", "hat", "den", "schwe\u00b7ren", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier schau ich wie die Silber-helle Bach", "tokens": ["Hier", "schau", "ich", "wie", "die", "Sil\u00b7ber\u00b7hel\u00b7le", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort von dem hohen Berge fliest", "tokens": ["Dort", "von", "dem", "ho\u00b7hen", "Ber\u00b7ge", "fliest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieses gr\u00fcne Thal begiest/", "tokens": ["Und", "die\u00b7ses", "gr\u00fc\u00b7ne", "Thal", "be\u00b7giest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da geh ich ihrem Ufer nach", "tokens": ["Da", "geh", "ich", "ih\u00b7rem", "U\u00b7fer", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seh die gl\u00e4ntzenden Forellen", "tokens": ["Und", "seh", "die", "gl\u00e4nt\u00b7zen\u00b7den", "Fo\u00b7rel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus ihren frischen Steinen quellen.", "tokens": ["Aus", "ih\u00b7ren", "fri\u00b7schen", "Stei\u00b7nen", "quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier bild ich mir die sch\u00f6ne Gegend ein/", "tokens": ["Hier", "bild", "ich", "mir", "die", "sch\u00f6\u00b7ne", "Ge\u00b7gend", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die man der Musen Wohnung hie\u00df/", "tokens": ["Die", "man", "der", "Mu\u00b7sen", "Woh\u00b7nung", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ieder Tropffen/ ieder Kie\u00df", "tokens": ["Wo", "ie\u00b7der", "Tropf\u00b7fen", "/", "ie\u00b7der", "Kie\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$(", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Crystall und Perle muste seyn/", "tokens": ["Crys\u00b7tall", "und", "Per\u00b7le", "mus\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man nichts auff bebl\u00fcmten Auen", "tokens": ["Wo", "man", "nichts", "auff", "be\u00b7bl\u00fcm\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Edelsteine konte schauen.", "tokens": ["Als", "E\u00b7del\u00b7stei\u00b7ne", "kon\u00b7te", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wiewohl gef\u00e4llt mir dieser stille See/", "tokens": ["Wie\u00b7wohl", "ge\u00b7f\u00e4llt", "mir", "die\u00b7