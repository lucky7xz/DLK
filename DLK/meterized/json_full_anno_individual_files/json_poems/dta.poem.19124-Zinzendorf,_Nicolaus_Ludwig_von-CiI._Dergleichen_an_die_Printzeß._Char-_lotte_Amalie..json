{"dta.poem.19124": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CiI.  Dergleichen an die Printze\u00df. Char-  \n lotte Amalie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist ein Empfehlungs Wort zu dem ewig-treuen GOtte,", "tokens": ["Hier", "ist", "ein", "Emp\u00b7feh\u00b7lungs", "Wort", "zu", "dem", "e\u00b7wig\u00b7treu\u00b7en", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "F\u00fcr die mehr ", "tokens": ["F\u00fcr", "die", "mehr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Denn, so hoch der Ehren-Gipfel, da die ", "tokens": ["Denn", ",", "so", "hoch", "der", "Eh\u00b7ren\u00b7Gip\u00b7fel", ",", "da", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "ADJD", "ART", "NN", "$,", "KOUS", "ART"], "meter": "+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Solch ein Abgrund ist die Gnade, womit ihr die Liebe lohnt.", "tokens": ["Solch", "ein", "Ab\u00b7grund", "ist", "die", "Gna\u00b7de", ",", "wo\u00b7mit", "ihr", "die", "Lie\u00b7be", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "K\u00f6nigs-Tochter! dieses ist meiner W\u00fcnsche Ziel und Ende:", "tokens": ["K\u00f6\u00b7nigs\u00b7Toch\u00b7ter", "!", "die\u00b7ses", "ist", "mei\u00b7ner", "W\u00fcn\u00b7sche", "Ziel", "und", "En\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Unser Heyland nehme doch ", "tokens": ["Un\u00b7ser", "Hey\u00b7land", "neh\u00b7me", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seine ", "tokens": ["Sei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Und becr\u00f6ne mit Erbarmen die ", "tokens": ["Und", "be\u00b7cr\u00f6\u00b7ne", "mit", "Er\u00b7bar\u00b7men", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Dieser Jahres-Wechsel soll keine Aenderung in Dingen,", "tokens": ["Die\u00b7ser", "Jah\u00b7res\u00b7Wech\u00b7sel", "soll", "kei\u00b7ne", "A\u00b7en\u00b7de\u00b7rung", "in", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+--+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Die der grossen Erb-Printze\u00df seelig w\u00e4ren, mit sich bringen;", "tokens": ["Die", "der", "gros\u00b7sen", "Er\u00b7bPrint\u00b7ze\u00df", "see\u00b7lig", "w\u00e4\u00b7ren", ",", "mit", "sich", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADJD", "VAFIN", "$,", "APPR", "PRF", "VVINF", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Aber was ihr Hertz betr\u00fcbte, und doch ohne Nutzen war,", "tokens": ["A\u00b7ber", "was", "ihr", "Hertz", "be\u00b7tr\u00fcb\u00b7te", ",", "und", "doch", "oh\u00b7ne", "Nut\u00b7zen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Nehme unser HErr von dannen, mache dero Augen klar.", "tokens": ["Neh\u00b7me", "un\u00b7ser", "Herr", "von", "dan\u00b7nen", ",", "ma\u00b7che", "de\u00b7ro", "Au\u00b7gen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADV", "$,", "VVFIN", "PDS", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "K\u00f6nigs-Tochter lassen sie JEsum Christum, ihren K\u00f6nig,", "tokens": ["K\u00f6\u00b7nigs\u00b7Toch\u00b7ter", "las\u00b7sen", "sie", "Je\u00b7sum", "Chris\u00b7tum", ",", "ih\u00b7ren", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Jhrer W\u00fcnsche Ziel-Stand seyn, d\u00fcnckt sie seine Ehe wenig?", "tokens": ["Ih\u00b7rer", "W\u00fcn\u00b7sche", "Ziel\u00b7Stand", "seyn", ",", "d\u00fcnckt", "sie", "sei\u00b7ne", "E\u00b7he", "we\u00b7nig", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAINF", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PIS", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Holder Br\u00e4utigam! verleite dieser deiner Magd die Welt;", "tokens": ["Hol\u00b7der", "Br\u00e4u\u00b7ti\u00b7gam", "!", "ver\u00b7lei\u00b7te", "die\u00b7ser", "dei\u00b7ner", "Magd", "die", "Welt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PDS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Aber schencke ihrer Seele das, was ewig wieder h\u00e4lt.", "tokens": ["A\u00b7ber", "schen\u00b7cke", "ih\u00b7rer", "See\u00b7le", "das", ",", "was", "e\u00b7wig", "wie\u00b7der", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PDS", "$,", "PRELS", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}