ct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Du tr\u00e4gst sie als Schmuck auf deiner Brust.", "tokens": ["Du", "tr\u00e4gst", "sie", "als", "Schmuck", "auf", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zwei Funken, die vom Feuer sprangen,", "tokens": ["Zwei", "Fun\u00b7ken", ",", "die", "vom", "Feu\u00b7er", "spran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blieben an deinen Augen hangen.", "tokens": ["Blie\u00b7ben", "an", "dei\u00b7nen", "Au\u00b7gen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schaue mich an, bis ich ganz verbrannt,", "tokens": ["Schau\u00b7e", "mich", "an", ",", "bis", "ich", "ganz", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann streu' meine Asche \u00fcber das Land.", "tokens": ["Dann", "streu'", "mei\u00b7ne", "A\u00b7sche", "\u00fc\u00b7ber", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von mi