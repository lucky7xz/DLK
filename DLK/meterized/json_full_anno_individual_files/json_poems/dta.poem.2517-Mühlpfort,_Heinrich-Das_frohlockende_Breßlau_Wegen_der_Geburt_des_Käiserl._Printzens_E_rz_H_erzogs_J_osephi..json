{"dta.poem.2517": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das frohlockende Bre\u00dflau  \n Wegen der Geburt des K\u00e4iserl.  \n Printzens  \n  E rz  H erzogs  J osephi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welt geprisens Oesterreich", "tokens": ["Welt", "ge\u00b7pri\u00b7sens", "O\u00b7es\u00b7ter\u00b7reich"], "token_info": ["word", "word", "word"], "pos": ["NN", "NE", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das nur G\u00f6tter hat gebohren/", "tokens": ["Das", "nur", "G\u00f6t\u00b7ter", "hat", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem nichts auff der Erden gleich/", "tokens": ["Dem", "nichts", "auff", "der", "Er\u00b7den", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das von Osten bi\u00df zu Westen/", "tokens": ["Das", "von", "Os\u00b7ten", "bi\u00df", "zu", "Wes\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "APPR", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Prangt mit seinen Lorbern Aesten.", "tokens": ["Prangt", "mit", "sei\u00b7nen", "Lor\u00b7bern", "A\u00b7es\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Schaue was der Himmel schafft", "tokens": ["Schau\u00b7e", "was", "der", "Him\u00b7mel", "schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Zweige sollen leben/", "tokens": ["Dei\u00b7ne", "Zwei\u00b7ge", "sol\u00b7len", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unserer Nachwelt Schatten geben/", "tokens": ["Un\u00b7se\u00b7rer", "Nach\u00b7welt", "Schat\u00b7ten", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und der Wurtzel Krafft und Safft/", "tokens": ["Und", "der", "Wurt\u00b7zel", "Krafft", "und", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meldet durch den Printz und Erben", "tokens": ["Mel\u00b7det", "durch", "den", "Printz", "und", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oesterreich kan nicht absterben.", "tokens": ["O\u00b7es\u00b7ter\u00b7reich", "kan", "nicht", "abs\u00b7ter\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Schlesien das treue Land", "tokens": ["Schle\u00b7si\u00b7en", "das", "treu\u00b7e", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein ander Aug und Hertze", "tokens": ["Und", "sein", "an\u00b7der", "Aug", "und", "Hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bre\u00dflau brennt gleich einer Kertze", "tokens": ["Bre\u00df\u00b7lau", "brennt", "gleich", "ei\u00b7ner", "Kert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt bey diesem Freuden Stand.", "tokens": ["Jetzt", "bey", "die\u00b7sem", "Freu\u00b7den", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df der L\u00e4nder W\u00fcnsch und hoffen/", "tokens": ["Da\u00df", "der", "L\u00e4n\u00b7der", "W\u00fcnsch", "und", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So gesegnet eingetroffen.", "tokens": ["So", "ge\u00b7seg\u00b7net", "ein\u00b7ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht wie sich ", "tokens": ["Seht", "wie", "sich"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KOKOM", "PRF"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Jhrem K\u00e4iser zu gefallen/", "tokens": ["Ih\u00b7rem", "K\u00e4i\u00b7ser", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie unter schal