{"textgrid.poem.67935": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Voluspa", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweiget alle, heilige Wesen!", "tokens": ["Schwei\u00b7get", "al\u00b7le", ",", "hei\u00b7li\u00b7ge", "We\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Heimdalls Kinder", "tokens": ["Heim\u00b7dalls", "Kin\u00b7der"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ich will Allvaters Geheimni\u00df reden,", "tokens": ["Ich", "will", "All\u00b7va\u00b7ters", "Ge\u00b7heim\u00b7ni\u00df", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Urwelt Sagen hab' ich geh\u00f6rt.", "tokens": ["Der", "Ur\u00b7welt", "Sa\u00b7gen", "hab'", "ich", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df noch Riesen, die Urbewohner,", "tokens": ["Ich", "wei\u00df", "noch", "Rie\u00b7sen", ",", "die", "Ur\u00b7be\u00b7woh\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und was vor Jahren sie mir erz\u00e4hlt.", "tokens": ["Und", "was", "vor", "Jah\u00b7ren", "sie", "mir", "