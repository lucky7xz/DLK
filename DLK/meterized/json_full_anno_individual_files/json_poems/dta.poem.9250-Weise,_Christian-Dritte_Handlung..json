{"dta.poem.9250": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Dritte Handlung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Floretto! la\u00df dein gl\u00fccke", "tokens": ["Ach", "Flo\u00b7ret\u00b7to", "!", "la\u00df", "dein", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "VVIMP", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dir nicht aus den h\u00e4nden gehn/", "tokens": ["Dir", "nicht", "aus", "den", "h\u00e4n\u00b7den", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerne doch in diesem st\u00fccke", "tokens": ["Ler\u00b7ne", "doch", "in", "die\u00b7sem", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine wolfahrt recht verstehn/", "tokens": ["Dei\u00b7ne", "wol\u00b7fahrt", "recht", "ver\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil die sch\u00f6nste von der welt:", "tokens": ["Weil", "die", "sch\u00f6ns\u00b7te", "von", "der", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleichsam dir zu fusse f\u00e4lt.", "tokens": ["Gleich\u00b7sam", "dir", "zu", "fus\u00b7se", "f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schau doch her/ sie legt sich nieder/", "tokens": ["Schau", "doch", "her", "/", "sie", "legt", "sich", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$(", "PPER", "VVFIN", "PRF", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Brauche der gelegenheit/", "tokens": ["Brau\u00b7che", "der", "ge\u00b7le\u00b7gen\u00b7heit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Diese stunde kommt nicht wieder/", "tokens": ["Die\u00b7se", "stun\u00b7de", "kommt", "nicht", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo man ihre gunst verstreut/", "tokens": ["Wo", "man", "ih\u00b7re", "gunst", "ver\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jtzund hast du noch die wahl/", "tokens": ["Jt\u00b7zund", "hast", "du", "noch", "die", "wahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fcrchte dich ein ander mahl.", "tokens": ["F\u00fcrch\u00b7te", "dich", "ein", "an\u00b7der", "mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Brich die rosen von den wangen/", "tokens": ["Brich", "die", "ro\u00b7sen", "von", "den", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Welche dir zu ehren bl\u00fchn/", "tokens": ["Wel\u00b7che", "dir", "zu", "eh\u00b7ren", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Welche dich und dein verlangen/", "tokens": ["Wel\u00b7che", "dich", "und", "dein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Als magneten an sich ziehn/", "tokens": ["Als", "mag\u00b7ne\u00b7ten", "an", "sich", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "APPR", "PRF", "VVINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Nimm das wollen-weiche pfand", "tokens": ["Nimm", "das", "wol\u00b7len\u00b7wei\u00b7che", "pfand"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Jhrer finger in die hand.", "tokens": ["Ih\u00b7rer", "fin\u00b7ger", "in", "die", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Schau die alabaster-ballen", "tokens": ["Schau", "die", "a\u00b7la\u00b7bas\u00b7ter\u00b7bal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "An der unbefleckten brust/", "tokens": ["An", "der", "un\u00b7be\u00b7fleck\u00b7ten", "brust", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie sie auf und nieder fallen/", "tokens": ["Wie", "sie", "auf", "und", "nie\u00b7der", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "KON", "PTKVZ", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Aus begierde zu der lust/", "tokens": ["Aus", "be\u00b7gier\u00b7de", "zu", "der", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Welche das verliebte ziel", "tokens": ["Wel\u00b7che", "das", "ver\u00b7lieb\u00b7te", "ziel"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDS", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Blo\u00df in dir erlangen will.", "tokens": ["Blo\u00df", "in", "dir", "er\u00b7lan\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wilst du sie noch mehr betr\u00fcben?", "tokens": ["Wilst", "du", "sie", "noch", "mehr", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Dieser leib und was sie hat.", "tokens": ["Die\u00b7ser", "leib", "und", "was", "sie", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Stellt sie gantz in dein belieben/", "tokens": ["Stellt", "sie", "gantz", "in", "dein", "be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Drum erfahr es in der that/", "tokens": ["Drum", "er\u00b7fahr", "es", "in", "der", "that", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Da\u00df Clarisse wird allein/", "tokens": ["Da\u00df", "Cla\u00b7ris\u00b7se", "wird", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Dem Floretto g\u00fcnstig seyn.", "tokens": ["Dem", "Flo\u00b7ret\u00b7to", "g\u00fcns\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}