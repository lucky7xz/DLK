{"textgrid.poem.34563": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vor dem Kamin, in dem die Flamme flackerte,", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Kamin, in dem die Flamme flackerte,", "tokens": ["Vor", "dem", "Ka\u00b7min", ",", "in", "dem", "die", "Flam\u00b7me", "fla\u00b7cker\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verstummten sie und dachten ihres Lebens nach.", "tokens": ["ver\u00b7stumm\u00b7ten", "sie", "und", "dach\u00b7ten", "ih\u00b7res", "Le\u00b7bens", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Alsdann versank so Vieles, was sie sonst besass,", "tokens": ["Als\u00b7dann", "ver\u00b7sank", "so", "Vie\u00b7les", ",", "was", "sie", "sonst", "be\u00b7sass", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und rein im reinen Augenblicke lebten sie.", "tokens": ["und", "rein", "im", "rei\u00b7nen", "Au\u00b7gen\u00b7bli\u00b7cke", "leb\u00b7ten", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der dunkle Wein, der drunten in der Asche stand,", "tokens": ["Der", "dunk\u00b7le", "Wein", ",", "der", "drun\u00b7ten", "in", "der", "A\u00b7sche", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ergl\u00fchte da und gab ein heimlich tiefes Roth.", "tokens": ["er\u00b7gl\u00fch\u00b7te", "da", "und", "gab", "ein", "heim\u00b7lich", "tie\u00b7fes", "Roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonne sank \u2013 der Schnee der Berge leuchtete \u2013", "tokens": ["Die", "Son\u00b7ne", "sank", "\u2013", "der", "Schnee", "der", "Ber\u00b7ge", "leuch\u00b7te\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Winterhimmel draussen schien geformt aus Stahl.", "tokens": ["der", "Win\u00b7ter\u00b7him\u00b7mel", "draus\u00b7sen", "schien", "ge\u00b7formt", "aus", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da sahen sie sich staunend an und doch vertraut \u2013", "tokens": ["Da", "sa\u00b7hen", "sie", "sich", "stau\u00b7nend", "an", "und", "doch", "ver\u00b7traut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und fragten nichts \u2013 und griffen herrisch Hand in Hand.", "tokens": ["und", "frag\u00b7ten", "nichts", "\u2013", "und", "grif\u00b7fen", "her\u00b7risch", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$(", "KON", "VVFIN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auf beider Stirnen lag ein seltner Widerschein", "tokens": ["Auf", "bei\u00b7der", "Stir\u00b7nen", "lag", "ein", "selt\u00b7ner", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von dem Kamin, in dem die Flamme flackerte.", "tokens": ["von", "dem", "Ka\u00b7min", ",", "in", "dem", "die", "Flam\u00b7me", "fla\u00b7cker\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vor dem Kamin, in dem die Flamme flackerte,", "tokens": ["Vor", "dem", "Ka\u00b7min", ",", "in", "dem", "die", "Flam\u00b7me", "fla\u00b7cker\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verstummten sie und dachten ihres Lebens nach.", "tokens": ["ver\u00b7stumm\u00b7ten", "sie", "und", "dach\u00b7t