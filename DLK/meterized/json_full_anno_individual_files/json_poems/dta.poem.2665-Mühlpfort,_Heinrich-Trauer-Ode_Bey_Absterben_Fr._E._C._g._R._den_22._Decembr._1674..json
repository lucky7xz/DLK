{"dta.poem.2665": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode/  \n  Bey Absterben Fr. E. C. g. R. den 22.  \n Decembr. 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl-Edler Herr/ Pirenens Quellen/", "tokens": ["Wohl\u00b7Ed\u00b7ler", "Herr", "/", "Pi\u00b7re\u00b7nens", "Quel\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NE", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der Hippocrenen Silber-fluth/", "tokens": ["Der", "Hip\u00b7po\u00b7cre\u00b7nen", "Sil\u00b7ber\u00b7fluth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Pindus Lorber-reiche Stellen/", "tokens": ["Des", "Pin\u00b7dus", "Lor\u00b7ber\u00b7rei\u00b7che", "Stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mufen heil\u2019ge Dichter-gluth/", "tokens": ["Der", "Mu\u00b7fen", "heil'\u00b7ge", "Dich\u00b7ter\u00b7gluth", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Apollo selbst/ mit Harff und Lauten/", "tokens": ["A\u00b7pol\u00b7lo", "selbst", "/", "mit", "Harff", "und", "Lau\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die w\u00fcrden ihm zu Dienste stehn/", "tokens": ["Die", "w\u00fcr\u00b7den", "ihm", "zu", "Diens\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn unter seiner Trauer-Rauten", "tokens": ["Wenn", "un\u00b7ter", "sei\u00b7ner", "Trau\u00b7e\u00b7rRau\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Parnassus Blum-Werck k\u00f6nt\u2019 aufgehn.", "tokens": ["Par\u00b7nas\u00b7sus", "Blum\u00b7\u00b7Werck", "k\u00f6nt'", "auf\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "VVINF", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und solten sie nicht diesen ehren/", "tokens": ["Und", "sol\u00b7ten", "sie", "nicht", "die\u00b7sen", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00e4ngst zu ihrem hohen Ruhm/", "tokens": ["Der", "l\u00e4ngst", "zu", "ih\u00b7rem", "ho\u00b7hen", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6ne Lieder lassen h\u00f6ren/", "tokens": ["So", "sch\u00f6\u00b7ne", "Lie\u00b7der", "las\u00b7sen", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Pallas nennt ihr Eigenthum?", "tokens": ["Die", "Pal\u00b7las", "nennt", "ihr", "Ei\u00b7gen\u00b7thum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der l\u00e4ngst ins Buch der Ewigkeiten", "tokens": ["Der", "l\u00e4ngst", "ins", "Buch", "der", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch kluges Schreiben sich gepregt/", "tokens": ["Durch", "klu\u00b7ges", "Schrei\u00b7ben", "sich", "ge\u00b7pregt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem opfferten sie Klang und Seiten.", "tokens": ["Dem", "opf\u00b7fer\u00b7ten", "sie", "Klang", "und", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Ach w\u00fcrde so sein Leid gelegt!", "tokens": ["Ach", "w\u00fcr\u00b7de", "so", "sein", "Leid", "ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein wenn zwey verm\u00e4hlte Hertzen", "tokens": ["Al\u00b7lein", "wenn", "zwey", "ver\u00b7m\u00e4hl\u00b7te", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Todes eisern Arm zerbricht.", "tokens": ["Des", "To\u00b7des", "ei\u00b7sern", "Arm", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wird durch so ergrimmte Schmertzen", "tokens": ["So", "wird", "durch", "so", "er\u00b7grimm\u00b7te", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fast Geist und Seele hingericht.", "tokens": ["Fast", "Geist", "und", "See\u00b7le", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn dieses ", "tokens": ["Wenn", "die\u00b7ses"], "token_info": ["word", "word"], "pos": ["KOUS", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Das unauffl\u00f6\u00dflich schien zu seyn;", "tokens": ["Das", "un\u00b7auf\u00b7fl\u00f6\u00df\u00b7lich", "schien", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So bleibet auch kein Held nicht stehen/", "tokens": ["So", "blei\u00b7bet", "auch", "kein", "Held", "nicht", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er l\u00e4sst sich Trauren nehmen ein.", "tokens": ["Er", "l\u00e4sst", "sich", "Trau\u00b7ren", "neh\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl-Edler Herr/ er mu\u00df ja klagen", "tokens": ["Wohl\u00b7Ed\u00b7ler", "Herr", "/", "er", "mu\u00df", "ja", "kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$(", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun bey der Jahre grauem Schnee", "tokens": ["Nun", "bey", "der", "Jah\u00b7re", "grau\u00b7em", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebste wird dahin getragen;", "tokens": ["Die", "Liebs\u00b7te", "wird", "da\u00b7hin", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun nach so Seegen-voller Eh/", "tokens": ["Nun", "nach", "so", "See\u00b7gen\u00b7vol\u00b7ler", "Eh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sol in \u00f6den Einsamkeiten/", "tokens": ["Er", "sol", "in", "\u00f6\u00b7den", "Ein\u00b7sam\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beschliessen seines Lebens Rest.", "tokens": ["Be\u00b7schlies\u00b7sen", "sei\u00b7nes", "Le\u00b7bens", "Rest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schmertz/ der die Seele kan bestreiten/", "tokens": ["Schmertz", "/", "der", "die", "See\u00b7le", "kan", "be\u00b7strei\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und aller Hoffnung Licht ausbl\u00e4st!", "tokens": ["Und", "al\u00b7ler", "Hoff\u00b7nung", "Licht", "aus\u00b7bl\u00e4st", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bevor/ wenn er zur\u00fcck gedencket/", "tokens": ["Be\u00b7vor", "/", "wenn", "er", "zu\u00b7r\u00fcck", "ge\u00b7den\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "PPER", "PTKVZ", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bey verkehrter L\u00e4uffte Zeit/", "tokens": ["Wie", "bey", "ver\u00b7kehr\u00b7ter", "L\u00e4uff\u00b7te", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da Krieg und Brand das Land gekr\u00e4ncket/", "tokens": ["Da", "Krieg", "und", "Brand", "das", "Land", "ge\u00b7kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Pest die Menschen abgemeyt/", "tokens": ["Die", "Pest", "die", "Men\u00b7schen", "ab\u00b7ge\u00b7meyt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Theurung alles auffgerieben;", "tokens": ["Die", "Theu\u00b7rung", "al\u00b7les", "auff\u00b7ge\u00b7rie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie in best\u00e4ndigster Gedult", "tokens": ["Sie", "in", "be\u00b7st\u00e4n\u00b7digs\u00b7ter", "Ge\u00b7dult"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Ehristenthumes treu geblieben/", "tokens": ["Des", "E\u00b7hris\u00b7ten\u00b7thu\u00b7mes", "treu", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und ihn erquickt mit Lieb\u2019 und Huld.", "tokens": ["Und", "ihn", "er\u00b7quickt", "mit", "Lieb'", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja wie sie auch zwo Stammes-S\u00e4ulen", "tokens": ["Ja", "wie", "sie", "auch", "zwo", "Stam\u00b7mes\u00b7S\u00e4u\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PWAV", "PPER", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begl\u00fcckt gebohren an das Licht/", "tokens": ["Be\u00b7gl\u00fcckt", "ge\u00b7boh\u00b7ren", "an", "das", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So von der Parcer M\u00f6rder-Pfeilen", "tokens": ["So", "von", "der", "Par\u00b7cer", "M\u00f6r\u00b7der\u00b7Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm ersten bl\u00fch\u2019n zwar hingericht/", "tokens": ["Jm", "ers\u00b7ten", "bl\u00fch'n", "zwar", "hin\u00b7ge\u00b7richt", "/"], "token_info": ["word", "wor