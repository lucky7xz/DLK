{"dta.poem.11093": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich abmahlen lie\u00df.  \n * *  T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Florette! trauest du dem albern mahler zu,", "tokens": ["Flo\u00b7ret\u00b7te", "!", "trau\u00b7est", "du", "dem", "al\u00b7bern", "mah\u00b7ler", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er dein englisch bild in farben k\u00f6nne treffen?", "tokens": ["Da\u00df", "er", "dein", "eng\u00b7lisch", "bild", "in", "far\u00b7ben", "k\u00f6n\u00b7ne", "tref\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJD", "NN", "APPR", "VVINF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach la\u00df dich, sch\u00f6nste! nicht den bl\u00f6den k\u00fcnstler \u00e4sfen,", "tokens": ["Ach", "la\u00df", "dich", ",", "sch\u00f6ns\u00b7te", "!", "nicht", "den", "bl\u00f6\u00b7den", "k\u00fcnst\u00b7ler", "\u00e4s\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "$,", "VVFIN", "$.", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil keiner farben glantz so strahlen kan wie du.", "tokens": ["Weil", "kei\u00b7ner", "far\u00b7ben", "glantz", "so", "strah\u00b7len", "kan", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo nun mein treuer rath bey dir, Florette! gilt,", "tokens": ["Wo", "nun", "mein", "treu\u00b7er", "rath", "bey", "dir", ",", "Flo\u00b7ret\u00b7te", "!", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So la\u00df die Venus dir dein blut zur farbe r\u00fchren,", "tokens": ["So", "la\u00df", "die", "Ve\u00b7nus", "dir", "dein", "blut", "zur", "far\u00b7be", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und deinen Seladon auf dich den pinsel f\u00fchren;", "tokens": ["Und", "dei\u00b7nen", "Se\u00b7la\u00b7don", "auf", "dich", "den", "pin\u00b7sel", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So schauest du alsdenn dein wahres ebenbild.", "tokens": ["So", "schau\u00b7est", "du", "als\u00b7denn", "dein", "wah\u00b7res", "e\u00b7ben\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}