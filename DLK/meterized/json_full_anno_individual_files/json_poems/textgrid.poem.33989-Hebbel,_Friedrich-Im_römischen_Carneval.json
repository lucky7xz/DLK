{"textgrid.poem.33989": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Im r\u00f6mischen Carneval", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst bin ich unter'm Maienbaum gelegen,", "tokens": ["Einst", "bin", "ich", "un\u00b7ter'm", "Mai\u00b7en\u00b7baum", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wie ich lag, hat sich ein Wind erhoben!", "tokens": ["Und", ",", "wie", "ich", "lag", ",", "hat", "sich", "ein", "Wind", "er\u00b7ho\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sind die Bl\u00fcten da um mich gestoben!", "tokens": ["Wie", "sind", "die", "Bl\u00fc\u00b7ten", "da", "um", "mich", "ge\u00b7sto\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie unerme\u00dflich schien des Fr\u00fchlings Segen!", "tokens": ["Wie", "un\u00b7er\u00b7me\u00df\u00b7lich", "schien", "des", "Fr\u00fch\u00b7lings", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt, d\u00e4ucht mir, seh' ich einen gleichen Regen,", "tokens": ["Jetzt", ",", "d\u00e4ucht", "mir", ",", "seh'", "ich", "ei\u00b7nen", "glei\u00b7chen", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch von Gestalten, Licht und Gut gewoben!", "tokens": ["Doch", "von", "Ge\u00b7stal\u00b7ten", ",", "Licht", "und", "Gut", "ge\u00b7wo\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als h\u00e4tten sich die gold'nen Sterne droben", "tokens": ["Als", "h\u00e4t\u00b7ten", "sich", "die", "gold'\u00b7nen", "Ster\u00b7ne", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PRF", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gesch\u00fcttelt, welche alles H\u00f6chste pflegen.", "tokens": ["Ge\u00b7sch\u00fct\u00b7telt", ",", "wel\u00b7che", "al\u00b7les", "H\u00f6chs\u00b7te", "pfle\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom stillen Reizenden zum Blendend-Sch\u00f6nen,", "tokens": ["Vom", "stil\u00b7len", "Rei\u00b7zen\u00b7den", "zum", "Blen\u00b7den\u00b7dSch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es fehlt kein Glied der holden Formenkette,", "tokens": ["Es", "fehlt", "kein", "Glied", "der", "hol\u00b7den", "For\u00b7men\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meinen Augen scheint sie nicht zu enden.", "tokens": ["Und", "mei\u00b7nen", "Au\u00b7gen", "scheint", "sie", "nicht", "zu", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum reicht den Kranz, die K\u00f6nigin zu kr\u00f6nen,", "tokens": ["Drum", "reicht", "den", "Kranz", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mir; denn eh' ich sie gefunden h\u00e4tte,", "tokens": ["Nicht", "mir", ";", "denn", "eh'", "ich", "sie", "ge\u00b7fun\u00b7den", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$.", "KON", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4r' er gewi\u00df verwelkt in meinen H\u00e4nden!", "tokens": ["W\u00e4r'", "er", "ge\u00b7wi\u00df", "ver\u00b7welkt", "in", "mei\u00b7nen", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Einst bin ich unter'm Maienbaum gelegen,", "tokens": ["Einst", "bin", "ich", "un\u00b7ter'm", "Mai\u00b7en\u