{"textgrid.poem.40290": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Gebet der Sucht", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals sah ich die Nacht begl\u00e4nzter,", "tokens": ["Nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "diamantisch reizen die Fernen;", "tokens": ["di\u00b7a\u00b7man\u00b7tisch", "rei\u00b7zen", "die", "Fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "durch mein staubiges Kellerfenster", "tokens": ["durch", "mein", "stau\u00b7bi\u00b7ges", "Kel\u00b7ler\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "sticht der Schein der Gaslaternen,", "tokens": ["sticht", "der", "Schein", "der", "Gas\u00b7la\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "schielt auf meine frierenden H\u00e4nde,", "tokens": ["schielt", "auf", "mei\u00b7ne", "frie\u00b7ren\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und ich f\u00fchle meinen Hunger;", "tokens": ["und", "ich", "f\u00fch\u00b7le", "mei\u00b7nen", "Hun\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "grau sind diese nackten W\u00e4nde,", "tokens": ["grau", "sind", "die\u00b7se", "nack\u00b7ten", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und sie flimmern. Und mein junger", "tokens": ["und", "sie", "flim\u00b7mern", ".", "Und", "mein", "jun\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "irrender Wille kann sich nicht mehr t\u00e4uschen", "tokens": ["ir\u00b7ren\u00b7der", "Wil\u00b7le", "kann", "sich", "nicht", "mehr", "t\u00e4u\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "unsre L\u00fcste wollen fruchtbar sein!", "tokens": ["uns\u00b7re", "L\u00fcs\u00b7te", "wol\u00b7len", "frucht\u00b7bar", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Schatten meiner keuschen", "tokens": ["Mit", "den", "Schat\u00b7ten", "mei\u00b7ner", "keu\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kammer spielt ein schw\u00fcler Schein.", "tokens": ["Kam\u00b7mer", "spielt", "ein", "schw\u00fc\u00b7ler", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "An den hohen H\u00e4usern dr\u00fcben gl\u00fchen", "tokens": ["An", "den", "ho\u00b7hen", "H\u00e4u\u00b7sern", "dr\u00fc\u00b7ben", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus der Finsternis die Fenster,", "tokens": ["aus", "der", "Fins\u00b7ter\u00b7nis", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die Freudenm\u00e4dchen bl\u00fchen \u2013", "tokens": ["wo", "die", "Freu\u00b7den\u00b7m\u00e4d\u00b7chen", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und die Sterne sind wie brennende Blicke,", "tokens": ["Und", "die", "Ster\u00b7ne", "sind", "wie", "bren\u00b7nen\u00b7de", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welten sehnen sich nach mir!", "tokens": ["Wel\u00b7ten", "seh\u00b7nen", "sich", "nach", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verschmachte. Ich ersticke.", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", ".", "Ich", "er\u00b7sti\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja: ich frevelte an Ihr!", "tokens": ["Ja", ":", "ich", "fre\u00b7vel\u00b7te", "an", "Ihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Selbst in meiner kalten Zelle", "tokens": ["Selbst", "in", "mei\u00b7ner", "kal\u00b7ten", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchlte ich das Leben toben,", "tokens": ["f\u00fchl\u00b7te", "ich", "das", "Le\u00b7ben", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der ich wagte, dieses schnelle", "tokens": ["der", "ich", "wag\u00b7te", ",", "die\u00b7ses", "schnel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz zu d\u00e4mpfen; aber oben", "tokens": ["Herz", "zu", "d\u00e4mp\u00b7fen", ";", "a\u00b7ber", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00fcber meinem dunklen Thale,", "tokens": ["\u00fc\u00b7ber", "mei\u00b7nem", "dunk\u00b7len", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Venus, seh ich angebrannt", "tokens": ["Ve\u00b7nus", ",", "seh", "ich", "an\u00b7ge\u00b7brannt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine flammenden Fanale,", "tokens": ["Dei\u00b7ne", "flam\u00b7men\u00b7den", "Fa\u00b7na\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und den Blick hinaufgewandt", "tokens": ["und", "den", "Blick", "hin\u00b7auf\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "ruf'ich aus dem tiefen Turme", "tokens": ["ruf'\u00b7ich", "aus", "dem", "tie\u00b7fen", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner Aengste zu dir hoch:", "tokens": ["mei\u00b7ner", "A\u00b7engs\u00b7te", "zu", "dir", "hoch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "G\u00f6ttin, wandle dich zum Wurme,", "tokens": ["G\u00f6t\u00b7tin", ",", "wand\u00b7le", "dich", "zum", "Wur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sei im Wurme G\u00f6ttin noch!", "tokens": ["sei", "im", "Wur\u00b7me", "G\u00f6t\u00b7tin", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sausend schaukelt eine Not mein Herz", "tokens": ["Sau\u00b7send", "schau\u00b7kelt", "ei\u00b7ne", "Not", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wie in erster s\u00fc\u00dfer Knabenfr\u00fche;", "tokens": ["wie", "in", "ers\u00b7ter", "s\u00fc\u00b7\u00dfer", "Kna\u00b7ben\u00b7fr\u00fc\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ich verschmachte! ich vergl\u00fche!", "tokens": ["ich", "ver\u00b7schmach\u00b7te", "!", "ich", "ver\u00b7gl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "jeder Stern ist mir ein Schmerz, \u2013", "tokens": ["je\u00b7der", "Stern", "ist", "mir", "ein", "Schmerz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "ihrer Strahlen ferne starre Ruten", "tokens": ["ih\u00b7rer", "Strah\u00b7len", "fer\u00b7ne", "star\u00b7re", "Ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "martern, wenn du mich nicht k\u00fchlst,", "tokens": ["mar\u00b7tern", ",", "wenn", "du", "mich", "nicht", "k\u00fchlst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn nicht Du mit deinem br\u00fcnstigen Blute", "tokens": ["wenn", "nicht", "Du", "mit", "dei\u00b7nem", "br\u00fcns\u00b7ti\u00b7gen", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "meine brennenden D\u00fcrste stillst!", "tokens": ["mei\u00b7ne", "bren\u00b7nen\u00b7den", "D\u00fcrs\u00b7te", "stillst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Sieh, es lichtet sich ein neues Fenster,", "tokens": ["Sieh", ",", "es", "lich\u00b7tet", "sich", "ein", "neu\u00b7es", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zuckt ein steiler Kerzenstreifen \u2013", "tokens": ["zuckt", "ein", "stei\u00b7ler", "Ker\u00b7zen\u00b7strei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ja: entz\u00fcnde dich dem Reifen,", "tokens": ["Ja", ":", "ent\u00b7z\u00fcn\u00b7de", "dich", "dem", "Rei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ewige, l\u00e4chle: Deine Kerzen bleiben,", "tokens": ["E\u00b7wi\u00b7ge", ",", "l\u00e4ch\u00b7le", ":", "Dei\u00b7ne", "Ker\u00b7zen", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "$.", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "alle andern sind verblichen!", "tokens": ["al\u00b7le", "an\u00b7dern", "sind", "ver\u00b7bli\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter jenen schwarzen Scheiben", "tokens": ["Hin\u00b7ter", "je\u00b7nen", "schwar\u00b7zen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schlafen alle Ordentlichen ...", "tokens": ["schla\u00b7fen", "al\u00b7le", "Or\u00b7dent\u00b7li\u00b7chen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Niemals sah ich die Nacht begl\u00e4nzter,", "tokens": ["Nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "diamantisch reizen die Fernen;", "tokens": ["di\u00b7a\u00b7man\u00b7tisch", "rei\u00b7zen", "die", "Fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "durch mein staubiges Kellerfenster", "tokens": ["durch", "mein", "stau\u00b7bi\u00b7ges", "Kel\u00b7ler\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "sticht der Schein der Gaslaternen,", "tokens": ["sticht", "der", "Schein", "der", "Gas\u00b7la\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "schielt auf meine frierenden H\u00e4nde,", "tokens": ["schielt", "auf", "mei\u00b7ne", "frie\u00b7ren\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und ich f\u00fchle meinen Hunger;", "tokens": ["und", "ich", "f\u00fch\u00b7le", "mei\u00b7nen", "Hun\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "grau sind diese nackten W\u00e4nde,", "tokens": ["grau", "sind", "die\u00b7se", "nack\u00b7ten", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und sie flimmern. Und mein junger", "tokens": ["und", "sie", "flim\u00b7mern", ".", "Und", "mein",