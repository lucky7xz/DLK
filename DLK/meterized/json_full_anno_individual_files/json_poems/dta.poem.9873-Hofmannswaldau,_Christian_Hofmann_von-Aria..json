{"dta.poem.9873": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weicht nun ihr bl\u00f6den sinnen/", "tokens": ["Weicht", "nun", "ihr", "bl\u00f6\u00b7den", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Welcht von hinnen/", "tokens": ["Welcht", "von", "hin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die die liebe nicht verstehn.", "tokens": ["Die", "die", "lie\u00b7be", "nicht", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es pflegt das sch\u00f6ne schertzen", "tokens": ["Denn", "es", "pflegt", "das", "sch\u00f6\u00b7ne", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Edlen hertzen", "tokens": ["Ed\u00b7len", "hert\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Zur vergn\u00fcgung anzugehn.", "tokens": ["Zur", "ver\u00b7gn\u00fc\u00b7gung", "an\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}