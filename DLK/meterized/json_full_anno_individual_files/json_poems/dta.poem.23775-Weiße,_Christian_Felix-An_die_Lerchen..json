{"dta.poem.23775": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "An die Lerchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Himmel, ach! ist das der Dank?", "tokens": ["Him\u00b7mel", ",", "ach", "!", "ist", "das", "der", "Dank", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "VAFIN", "ART", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann der reizende Gesang,", "tokens": ["Kann", "der", "rei\u00b7zen\u00b7de", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Den, wenn sich der Lenz verj\u00fcngt,", "tokens": ["Den", ",", "wenn", "sich", "der", "Lenz", "ver\u00b7j\u00fcngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhr der frohen Erde bringt,", "tokens": ["Ihr", "der", "fro\u00b7hen", "Er\u00b7de", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Euch f\u00fcr diese Wuth nicht b\u00fcrgen,", "tokens": ["Euch", "f\u00fcr", "die\u00b7se", "Wuth", "nicht", "b\u00fcr\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df die Menschen Euch erw\u00fcrgen?", "tokens": ["Da\u00df", "die", "Men\u00b7schen", "Euch", "er\u00b7w\u00fcr\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Arme kleine Lerchen, ach!", "tokens": ["Ar\u00b7me", "klei\u00b7ne", "Ler\u00b7chen", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich, ich f\u00fchle Eure Schmach:", "tokens": ["Ich", ",", "ich", "f\u00fch\u00b7le", "Eu\u00b7re", "Schmach", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fiel es mir auch zehnmal ein,", "tokens": ["Fiel", "es", "mir", "auch", "zehn\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie will ich so grausam seyn \u2014", "tokens": ["Nie", "will", "ich", "so", "grau\u00b7sam", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, bald h\u00e4tt ich es vergessen,", "tokens": ["Doch", ",", "bald", "h\u00e4tt", "ich", "es", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df wir heute Lerchen essen.", "tokens": ["Da\u00df", "wir", "heu\u00b7te", "Ler\u00b7chen", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}