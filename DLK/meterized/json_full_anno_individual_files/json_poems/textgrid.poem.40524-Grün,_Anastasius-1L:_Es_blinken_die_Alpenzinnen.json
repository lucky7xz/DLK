{"textgrid.poem.40524": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es blinken die Alpenzinnen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es blinken die Alpenzinnen", "tokens": ["Es", "blin\u00b7ken", "die", "Al\u00b7pen\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In Eis schon silbern ganz,", "tokens": ["In", "Eis", "schon", "sil\u00b7bern", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Herbst entlaubt im Thale", "tokens": ["Der", "Herbst", "ent\u00b7laubt", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der B\u00e4ume gr\u00fcnen Kranz.", "tokens": ["Der", "B\u00e4u\u00b7me", "gr\u00fc\u00b7nen", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ums D\u00f6rflein dort am Hange", "tokens": ["Ums", "D\u00f6r\u00b7flein", "dort", "am", "Han\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gr\u00fcnt noch die Wiese fort,", "tokens": ["Gr\u00fcnt", "noch", "die", "Wie\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch auf der Wiese die Blumen", "tokens": ["Doch", "auf", "der", "Wie\u00b7se", "die", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sind alle schon verdorrt.", "tokens": ["Sind", "al\u00b7le", "schon", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Horch, was erklingt vom Berge", "tokens": ["Horch", ",", "was", "er\u00b7klingt", "vom", "Ber\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "VVFIN", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wie voller Glockenklang?", "tokens": ["Wie", "vol\u00b7ler", "Glo\u00b7cken\u00b7klang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was t\u00f6nt zum Thale nieder", "tokens": ["Was", "t\u00f6nt", "zum", "Tha\u00b7le", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie s\u00fc\u00dfer Brautgesang?", "tokens": ["Wie", "s\u00fc\u00b7\u00dfer", "Braut\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das ist mit ihrer Heerde", "tokens": ["Das", "ist", "mit", "ih\u00b7rer", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die junge Sennerin,", "tokens": ["Die", "jun\u00b7ge", "Sen\u00b7ne\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die von den Alpen nieder", "tokens": ["Die", "von", "den", "Al\u00b7pen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Heimat wallt dahin.", "tokens": ["Zur", "Hei\u00b7mat", "wallt", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sch\u00f6nste ihrer K\u00fche", "tokens": ["Die", "sch\u00f6ns\u00b7te", "ih\u00b7rer", "K\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit hellem Glockenlaut,", "tokens": ["Mit", "hel\u00b7lem", "Glo\u00b7cken\u00b7laut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voran mit frischem Kranze,", "tokens": ["Vo\u00b7ran", "mit", "fri\u00b7schem", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschm\u00fcckt wie eine Braut.", "tokens": ["Ge\u00b7schm\u00fcckt", "wie", "ei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Rings um sie h\u00fcpft so fr\u00f6hlich", "tokens": ["Rings", "um", "sie", "h\u00fcpft", "so", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ganze Heerde drein,", "tokens": ["Die", "gan\u00b7ze", "Heer\u00b7de", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie treue Jugendgenossen,", "tokens": ["Wie", "treu\u00b7e", "Ju\u00b7gend\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die sich des Tages freun.", "tokens": ["Die", "sich", "des", "Ta\u00b7ges", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der schwarze Stier den Festzug", "tokens": ["Der", "schwar\u00b7ze", "Stier", "den", "Fest\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als w\u00fcrdiger Pater f\u00fchrt;", "tokens": ["Als", "w\u00fcr\u00b7di\u00b7ger", "Pa\u00b7ter", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er schreitet hin bed\u00e4chtlich,", "tokens": ["Er", "schrei\u00b7tet", "hin", "be\u00b7d\u00e4cht\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie's solchem Herrn geb\u00fchrt.", "tokens": ["Wie's", "sol\u00b7chem", "Herrn", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und vor dem ersten Hause", "tokens": ["Und", "vor", "dem", "ers\u00b7ten", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jauchzt dreimal hell die Maid,", "tokens": ["Jauchzt", "drei\u00b7mal", "hell", "die", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df laut es gellt durchs D\u00f6rflein,", "tokens": ["Da\u00df", "laut", "es", "gellt", "durchs", "D\u00f6r\u00b7flein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Thal und Alpen weit!", "tokens": ["Durch", "Thal", "und", "Al\u00b7pen", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die M\u00fctterlein und Dirnen", "tokens": ["Die", "M\u00fct\u00b7ter\u00b7lein", "und", "Dir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind flink herbeigerannt,", "tokens": ["Sind", "flink", "her\u00b7bei\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sennerin dr\u00fcckt Allen", "tokens": ["Die", "Sen\u00b7ne\u00b7rin", "dr\u00fcckt", "Al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So warm und treu die Hand:", "tokens": ["So", "warm", "und", "treu", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbviel Gr\u00fc\u00dfe, sch\u00f6ne, frische,", "tokens": ["\u00bb", "viel", "Gr\u00fc\u00b7\u00dfe", ",", "sch\u00f6\u00b7ne", ",", "fri\u00b7sche", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von gr\u00fcnen Alpenh\u00f6hn!", "tokens": ["Von", "gr\u00fc\u00b7nen", "Al\u00b7pen\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange, ach, wie lange,", "tokens": ["Wie", "lan\u00b7ge", ",", "ach", ",", "wie", "lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ITJ", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df wir uns nicht gesehn!", "tokens": ["Da\u00df", "wir", "uns", "nicht", "ge\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Den ganzen langen Sommer", "tokens": ["Den", "gan\u00b7zen", "lan\u00b7gen", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df ich so ganz allein", "tokens": ["Sa\u00df", "ich", "so", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Heerden und mit Bl\u00fcmlein,", "tokens": ["Mit", "Heer\u00b7den", "und", "mit", "Bl\u00fcm\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Sonn' und Mondenschein!\u00ab", "tokens": ["Mit", "Sonn'", "und", "Mon\u00b7den\u00b7schein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie gr\u00fc\u00dft die Burschen alle", "tokens": ["Sie", "gr\u00fc\u00dft", "die", "Bur\u00b7schen", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit heit'rem Angesicht,", "tokens": ["Mit", "heit'\u00b7rem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur einen, und den sch\u00f6nsten,", "tokens": ["Nur", "ei\u00b7nen", ",", "und", "den", "sch\u00f6ns\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den gr\u00fc\u00dft sie eben nicht.", "tokens": ["Den", "gr\u00fc\u00dft", "sie", "e\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Nicht scheint es ihn zu gr\u00e4men,", "tokens": ["Nicht", "scheint", "es", "ihn", "zu", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und l\u00e4chelnd l\u00e4\u00dft er's geschehn!", "tokens": ["Und", "l\u00e4\u00b7chelnd", "l\u00e4\u00dft", "er's", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er hat wohl auch die Sch\u00f6ne", "tokens": ["Er", "hat", "wohl", "auch", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So lange nicht gesehn?", "tokens": ["So", "lan\u00b7ge", "nicht", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Er tr\u00e4gt ein gr\u00fcnes H\u00fctlein", "tokens": ["Er", "tr\u00e4gt", "ein", "gr\u00fc\u00b7nes", "H\u00fct\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ums\u00e4umt von Rosen dicht.", "tokens": ["Um\u00b7s\u00e4umt", "von", "Ro\u00b7sen", "dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei, solche Alpenrosen,", "tokens": ["Ei", ",", "sol\u00b7che", "Al\u00b7pen\u00b7ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Thale bl\u00fchn sie nicht!", "tokens": ["Im", "Tha\u00b7le", "bl\u00fchn", "sie", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER"