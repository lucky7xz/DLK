{"dta.poem.5925": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da haben wir die Ersten C\u00f6rper nun.", "tokens": ["Da", "ha\u00b7ben", "wir", "die", "Ers\u00b7ten", "C\u00f6r\u00b7per", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da der Materie Partickelchen nicht ruhn,", "tokens": ["Da", "der", "Ma\u00b7te\u00b7rie", "Par\u00b7ti\u00b7ckel\u00b7chen", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und so ver\u00e4nderlich in allen Theilen seyn;", "tokens": ["Und", "so", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "in", "al\u00b7len", "Thei\u00b7len", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ist es nicht ungereimt von ihnen zu gedencken,", "tokens": ["Ist", "es", "nicht", "un\u00b7ge\u00b7reimt", "von", "ih\u00b7nen", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie, indem sie sich so gar verschiedlich lencken,", "tokens": ["Da\u00df", "sie", ",", "in\u00b7dem", "sie", "sich", "so", "gar", "ver\u00b7schied\u00b7lich", "len\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch die Beweg- und Aendrung der Figur,", "tokens": ["Durch", "die", "Be\u00b7weg", "und", "A\u00b7en\u00b7drung", "der", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+---", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Sich selbst in die Natur", "tokens": ["Sich", "selbst", "in", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vom andern Element ver\u00e4ndern und verkehren.", "tokens": ["Vom", "an\u00b7dern", "E\u00b7le\u00b7ment", "ver\u00b7\u00e4n\u00b7dern", "und", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vielleicht, da\u00df offt die kleinen C\u00f6rperlein", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "offt", "die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Vom erst- und zweyten Element", "tokens": ["Vom", "er\u00b7st", "und", "zwey\u00b7ten", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "TRUNC", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Ins dritte gar verwandelt seyn,", "tokens": ["Ins", "drit\u00b7te", "gar", "ver\u00b7wan\u00b7delt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und da\u00df auch dies, indem sichs reibt und trennt,", "tokens": ["Und", "da\u00df", "auch", "dies", ",", "in\u00b7dem", "sichs", "reibt", "und", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PDS", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sich r\u00fcndet, sich verd\u00fcnnt,", "tokens": ["Sich", "r\u00fcn\u00b7det", ",", "sich", "ver\u00b7d\u00fcnnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die Form des anderen gewinnt,", "tokens": ["Die", "Form", "des", "an\u00b7de\u00b7ren", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Vielleicht erh\u00e4lt es gar, durch seine Schnelligkeit,", "tokens": ["Viel\u00b7leicht", "er\u00b7h\u00e4lt", "es", "gar", ",", "durch", "sei\u00b7ne", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Des erstern Elements Beschaffenheit.", "tokens": ["Des", "ers\u00b7tern", "E\u00b7le\u00b7ments", "Be\u00b7schaf\u00b7fen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}