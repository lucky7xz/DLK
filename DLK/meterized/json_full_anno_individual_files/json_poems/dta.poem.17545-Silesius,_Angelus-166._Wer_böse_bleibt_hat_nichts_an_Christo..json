{"dta.poem.17545": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "166.  Wer b\u00f6se bleibt/ hat nichts an  \n Christo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch bleibestu verbost/ so ist dir nichts erworben:", "tokens": ["Mensch", "blei\u00b7be\u00b7stu", "ver\u00b7bost", "/", "so", "ist", "dir", "nichts", "er\u00b7wor\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott ist nur f\u00fcr das Schaf nicht f\u00fcr den Bok gestorben.", "tokens": ["Gott", "ist", "nur", "f\u00fcr", "das", "Schaf", "nicht", "f\u00fcr", "den", "Bok", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}