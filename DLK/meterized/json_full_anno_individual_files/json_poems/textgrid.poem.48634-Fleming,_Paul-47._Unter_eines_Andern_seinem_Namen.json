{"textgrid.poem.48634": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "47. Unter eines Andern seinem Namen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heran, gew\u00fcndschter Tag, heran", "tokens": ["He\u00b7ran", ",", "ge\u00b7w\u00fcnd\u00b7schter", "Tag", ",", "he\u00b7ran"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["PTKVZ", "$,", "ADJA", "NN", "$,", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und dupple deinen Schein!", "tokens": ["und", "dupp\u00b7le", "dei\u00b7nen", "Schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich die gleich nicht k\u00fcssen kan,", "tokens": ["Ob", "ich", "die", "gleich", "nicht", "k\u00fcs\u00b7sen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so soll gebunden sein.", "tokens": ["so", "soll", "ge\u00b7bun\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, geht ihr gleich frei aus die Hand,", "tokens": ["Doch", ",", "geht", "ihr", "gleich", "frei", "aus", "die", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "ihr Herze wei\u00df sein rechtes Band.", "tokens": ["ihr", "Her\u00b7ze", "wei\u00df", "sein", "rech\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Best\u00e4ndig hab' ich mich bedacht,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "hab'", "ich", "mich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sie bleibets, wie sies ist,", "tokens": ["sie", "blei\u00b7bets", ",", "wie", "sies", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die mich alleine fr\u00f6lich macht,", "tokens": ["die", "mich", "al\u00b7lei\u00b7ne", "fr\u00f6\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die ihm mein Wundsch erkiest,", "tokens": ["die", "ihm", "mein", "Wund\u00b7sch", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "der mehr nichts will auf aller Welt,", "tokens": ["der", "mehr", "nichts", "will", "auf", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PIS", "VMFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "als da\u00df sich Treue bei ihm h\u00e4lt.", "tokens": ["als", "da\u00df", "sich", "Treu\u00b7e", "bei", "ihm", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PRF", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An ihr vergn\u00fcget sich mein Mut;", "tokens": ["An", "ihr", "ver\u00b7gn\u00fc\u00b7get", "sich", "mein", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein Alles, das ist sie.", "tokens": ["mein", "Al\u00b7les", ",", "das", "ist", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie einig ist mein h\u00f6chstes Gut,", "tokens": ["Sie", "ei\u00b7nig", "ist", "mein", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Herzgeliebte, die.", "tokens": ["die", "Herz\u00b7ge\u00b7lieb\u00b7te", ",", "die", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn Liebe Gegenliebe wei\u00df,", "tokens": ["Wenn", "Lie\u00b7be", "Ge\u00b7gen\u00b7lie\u00b7be", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das hat f\u00fcr allen Sch\u00e4tzen Preis.", "tokens": ["das", "hat", "f\u00fcr", "al\u00b7len", "Sch\u00e4t\u00b7zen", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und mu\u00df ich noch einmal so weit", "tokens": ["Und", "mu\u00df", "ich", "noch", "ein\u00b7mal", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "durch wilde See und Land,", "tokens": ["durch", "wil\u00b7de", "See", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mein Sin ist bei ihr allezeit,", "tokens": ["mein", "Sin", "ist", "bei", "ihr", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und ihrer ist mein Pfand.", "tokens": ["und", "ih\u00b7rer", "ist", "mein", "Pfand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, da\u00df ihr Mund mich nicht lacht an,", "tokens": ["Doch", ",", "da\u00df", "ihr", "Mund", "mich", "nicht", "lacht", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "das macht, da\u00df ich nicht froh sein kan.", "tokens": ["das", "macht", ",", "da\u00df", "ich", "nicht", "froh", "sein", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bleib' ewig so, als wie du bist!", "tokens": ["Bleib'", "e\u00b7wig", "so", ",", "als", "wie", "du", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$,", "KOUS", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich \u00e4ndre nichts an mir.", "tokens": ["Ich", "\u00e4nd\u00b7re", "nichts", "an", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist um eine kurze Frist,", "tokens": ["Es", "ist", "um", "ei\u00b7ne", "kur\u00b7ze", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "so bin ich stets bei dir.", "tokens": ["so", "bin", "ich", "stets", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Tod, o Leben, wird allein", "tokens": ["Mein", "Tod", ",", "o", "Le\u00b7ben", ",", "wird", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bei dir mir zuckers\u00fc\u00dfe sein.", "tokens": ["bei", "dir", "mir", "zu\u00b7cker\u00b7s\u00fc\u00b7\u00dfe", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus diesem Becher schenk' ich dir,", "tokens": ["Aus", "die\u00b7sem", "Be\u00b7cher", "schenk'", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein Herze; trink' es Schatz!", "tokens": ["mein", "Her\u00b7ze", ";", "trink'", "es", "Schatz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$.", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich einig setz' ich allen f\u00fcr,", "tokens": ["Dich", "ei\u00b7nig", "setz'", "ich", "al\u00b7len", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "PIAT", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du hast der Liebsten Platz.", "tokens": ["du", "hast", "der", "Liebs\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und di\u00df Wort la\u00df den B\u00fcrgen sein,", "tokens": ["Und", "di\u00df", "Wort", "la\u00df", "den", "B\u00fcr\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVIMP", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bis ich mich einsten selbst stell' ein!", "tokens": ["bis", "ich", "mich", "eins\u00b7ten", "selbst", "stell'", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Heran, gew\u00fcndschter Tag, heran", "tokens": ["He\u00b7ran", ",", "ge\u00b7w\u00fcnd\u00b7schter", "Tag", ",", "he\u00b7ran"], "token_info": ["w