-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich einig setz' ich allen f\u00fcr,", "tokens": ["Dich", "ei\u00b7nig", "setz'", "ich", "al\u00b7len", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PPER", "PIAT", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du hast der Liebsten Platz.", "tokens": ["du", "hast", "der", "Liebs\u00b7ten", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und di\u00df Wort la\u00df den B\u00fcrgen sein,", "tokens": ["Und", "di\u00df", "Wort", "la\u00df", "den", "B\u00fcr\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVIMP", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "bis ich mich einsten selbst stell' ein!", "tokens": ["bis", "ich", "mich", "eins\u00b7ten", "selbst", "stell'", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}