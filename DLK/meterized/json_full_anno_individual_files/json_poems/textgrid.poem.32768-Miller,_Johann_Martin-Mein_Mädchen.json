{"textgrid.poem.32768": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Mein M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe, Liebe, welche Freuden", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ",", "wel\u00b7che", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gabst du mit der Holden mir!", "tokens": ["Gabst", "du", "mit", "der", "Hol\u00b7den", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel m\u00fcssen mich beneiden,", "tokens": ["En\u00b7gel", "m\u00fcs\u00b7sen", "mich", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruh' ich in den Armen ihr.", "tokens": ["Ruh'", "ich", "in", "den", "Ar\u00b7men", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmlischer als Maienbl\u00fcte", "tokens": ["Himm\u00b7li\u00b7scher", "als", "Mai\u00b7en\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lacht ihr mildes Angesicht;", "tokens": ["Lacht", "ihr", "mil\u00b7des", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "me