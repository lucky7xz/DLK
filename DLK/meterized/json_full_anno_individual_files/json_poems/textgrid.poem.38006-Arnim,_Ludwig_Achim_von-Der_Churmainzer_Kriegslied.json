 "measure": "iambic.tetra"}, "line.4": {"text": "Frisch gewagt, ist halb gewonnen,", "tokens": ["Frisch", "ge\u00b7wagt", ",", "ist", "halb", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht verzagt, es wird schon kommen,", "tokens": ["Nicht", "ver\u00b7zagt", ",", "es", "wird", "schon", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn's Churmainz geh\u00f6ren soll.", "tokens": ["Wenn's", "Chur\u00b7mainz", "ge\u00b7h\u00f6\u00b7ren", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "O ihr Churmainzer all zusammen,", "tokens": ["O", "ihr", "Chur\u00b7main\u00b7zer", "all", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PIAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Pferd, zu Fu\u00df in Gottes Namen,", "tokens": ["Zu", "Pferd", ",", "zu", "Fu\u00df", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ergreift den Feind nur herzhaft an,", "tokens": ["Er\u00b7greift", "den", "Feind", "nur", "herz\u00b7haft", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott der Herr wird uns besch\u00fctzen,", "tokens": ["Gott", "der", "Herr", "wird", "uns", "be\u00b7sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seinen Schutz und Seegen schicken,", "tokens": ["Sei\u00b7nen", "Schutz", "und", "See\u00b7gen", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "General Albini f\u00fchrt uns an.", "tokens": ["Ge\u00b7ne\u00b7ral", "Al\u00b7bi\u00b7ni", "f\u00fchrt", "uns", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}