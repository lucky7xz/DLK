{"textgrid.poem.41998": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sch\u00f6nheit, die dein ganzes Wesen f\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sch\u00f6nheit, die dein ganzes Wesen f\u00fcllt,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", ",", "die", "dein", "gan\u00b7zes", "We\u00b7sen", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Reiz, der sanft um deinen Mund sich webet,", "tokens": ["Der", "Reiz", ",", "der", "sanft", "um", "dei\u00b7nen", "Mund", "sich", "we\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der reine Sinn, der deinen Busen hebet,", "tokens": ["Der", "rei\u00b7ne", "Sinn", ",", "der", "dei\u00b7nen", "Bu\u00b7sen", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Scham, die deine Wang' in Purpur h\u00fcllt,", "tokens": ["Die", "Scham", ",", "die", "dei\u00b7ne", "Wang'", "in", "Pur\u00b7pur", "h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Mitleid,