{"textgrid.poem.60273": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand, der h\u00f6chste, gr\u00fcnste Baum,", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand, der h\u00f6chste, gr\u00fcnste Baum,", "tokens": ["Ich", "stand", ",", "der", "h\u00f6chs\u00b7te", ",", "gr\u00fcns\u00b7te", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Zeiten froh im Waldesraum.", "tokens": ["Vor", "Zei\u00b7ten", "froh", "im", "Wal\u00b7des\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir galt der Sonne erster Ku\u00df,", "tokens": ["Mir", "galt", "der", "Son\u00b7ne", "ers\u00b7ter", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich brachte, war sie schon geschieden,", "tokens": ["Ich", "brach\u00b7te", ",", "war", "sie", "schon", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Wanderer zum Abendfrieden", "tokens": ["Dem", "Wan\u00b7de\u00b7rer", "zum", "A\u00b7bend\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von ihr noch einen Purpurgru\u00df.", "tokens": ["Von", "ihr", "noch", "ei\u00b7nen", "Pur\u00b7pur\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sah mich ernst der K\u00fcfer ragen,", "tokens": ["Da", "sah", "mich", "ernst", "der", "K\u00fc\u00b7fer", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der kam und hat mich schnell erschlagen.", "tokens": ["Der", "kam", "und", "hat", "mich", "schnell", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ade! Ade! du gr\u00fcner Hain!", "tokens": ["A\u00b7de", "!", "A\u00b7de", "!", "du", "gr\u00fc\u00b7ner", "Hain", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Du Sonnenstrahl und Mondenschein!", "tokens": ["Du", "Son\u00b7nen\u00b7strahl", "und", "Mon\u00b7den\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du Vogelsang und Wetterklang,", "tokens": ["Du", "Vo\u00b7gel\u00b7sang", "und", "Wet\u00b7ter\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der freudig mir zur Wurzel drang!", "tokens": ["Der", "freu\u00b7dig", "mir", "zur", "Wur\u00b7zel", "drang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Waldeslust ist nun herum,", "tokens": ["Die", "Wal\u00b7des\u00b7lust", "ist", "nun", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ich wandre nach Elysium.", "tokens": ["Ich", "wand\u00b7re", "nach", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ihr Bruderb\u00e4ume, folgt mir nach", "tokens": ["Ihr", "Bru\u00b7der\u00b7b\u00e4u\u00b7me", ",", "folgt", "mir", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In dieses himmlische Gemach;", "tokens": ["In", "die\u00b7ses", "himm\u00b7li\u00b7sche", "Ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "O nehmt das Los der Auserkornen", "tokens": ["O", "nehmt", "das", "Los", "der", "Au\u00b7ser\u00b7kor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von all den tausend Waldgebornen,", "tokens": ["Von", "all", "den", "tau\u00b7send", "Wald\u00b7ge\u00b7bor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Das sch\u00f6ne Los, das gro\u00dfe Los:", "tokens": ["Das", "sch\u00f6\u00b7ne", "Los", ",", "das", "gro\u00b7\u00dfe", "Los", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Tief in des Grundes k\u00fchlem Scho\u00df", "tokens": ["Tief", "in", "des", "Grun\u00b7des", "k\u00fch\u00b7lem", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Ein Fa\u00df zu sein, ein Fa\u00df zu sein,", "tokens": ["Ein", "Fa\u00df", "zu", "sein", ",", "ein", "Fa\u00df", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nicht so ein stillverla\u00dfner Schrein;", "tokens": ["Nicht", "so", "ein", "still\u00b7ver\u00b7la\u00df\u00b7ner", "Schrein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ein Fa\u00df, dem lieben Wein ergeben,", "tokens": ["Ein", "Fa\u00df", ",", "dem", "lie\u00b7ben", "Wein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Erde heilges Herzblut h\u00fcllend,", "tokens": ["Der", "Er\u00b7de", "heil\u00b7ges", "Herz\u00b7blut", "h\u00fcl\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Den Zecher durch und durch erf\u00fcllend!", "tokens": ["Den", "Ze\u00b7cher", "durch", "und", "durch", "er\u00b7f\u00fcl\u00b7lend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Komm, komm, bewegter Erdengast,", "tokens": ["Komm", ",", "komm", ",", "be\u00b7weg\u00b7ter", "Er\u00b7den\u00b7gast", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und halte hier vergn\u00fcgte Rast.", "tokens": ["Und", "hal\u00b7te", "hier", "ver\u00b7gn\u00fcg\u00b7te", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mach dir das Herz im Weine flott,", "tokens": ["Mach", "dir", "das", "Herz", "im", "Wei\u00b7ne", "flott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Schenk ein! trink aus! merkst du den Gott?", "tokens": ["Schenk", "ein", "!", "trink", "aus", "!", "merkst", "du", "den", "Gott", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Braust dir der Geist durchs Innre hin,", "tokens": ["Braust", "dir", "der", "Geist", "durchs", "Inn\u00b7re", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Von dem ich selber trunken bin?", "tokens": ["Von", "dem", "ich", "sel\u00b7ber", "trun\u00b7ken", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Er ist so feurig, s\u00fc\u00df und stark:", "tokens": ["Er", "ist", "so", "feu\u00b7rig", ",", "s\u00fc\u00df", "und", "stark", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "O schl\u00fcrf ihn ein ins tiefste Mark! \u2013", "tokens": ["O", "schl\u00fcrf", "ihn", "ein", "ins", "tiefs\u00b7te", "Mark", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Nun Wandrer, wandre selig heiter", "tokens": ["Nun", "Wand\u00b7rer", ",", "wand\u00b7re", "se\u00b7lig", "hei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Von Fa\u00df zu Fa\u00df forttrinkend weiter!", "tokens": ["Von", "Fa\u00df", "zu", "Fa\u00df", "fort\u00b7trin\u00b7kend", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Schon tauchen dir im Rosenlichte", "tokens": ["Schon", "tau\u00b7chen", "dir", "im", "Ro\u00b7sen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Herauf gar liebliche Gesichte:", "tokens": ["Her\u00b7auf", "gar", "lieb\u00b7li\u00b7che", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Manch teures l\u00e4ngst verlornes Gut,", "tokens": ["Manch", "teu\u00b7res", "l\u00e4ngst", "ver\u00b7lor\u00b7nes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die Tr\u00e4um aus deinen Jugendjahren,", "tokens": ["Die", "Tr\u00e4um", "aus", "dei\u00b7nen", "Ju\u00b7gend\u00b7jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Sie kommen dir auf Weinesflut", "tokens": ["Sie", "kom\u00b7men", "dir", "auf", "Wei\u00b7nes\u00b7flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Jetzt frisch und froh herangefahren.", "tokens": ["Jetzt", "frisch", "und", "froh", "her\u00b7an\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Schenk ein! \u2013 du f\u00fchlst die alten Triebe", "tokens": ["Schenk", "ein", "!", "\u2013", "du", "f\u00fchlst", "die", "al\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Zu k\u00fchner Tat hinaus! hinaus!", "tokens": ["Zu", "k\u00fch\u00b7ner", "Tat", "hin\u00b7aus", "!", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Du gibst den Ku\u00df der ersten Liebe;", "tokens": ["Du", "gibst", "den", "Ku\u00df", "der", "ers\u00b7ten", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Schenk ein! du stehst im Vaterhaus.", "tokens": ["Schenk", "ein", "!", "du", "stehst", "im", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Wohl dir! wohl dir! schon bist du trunken,", "tokens": ["Wohl", "dir", "!", "wohl", "dir", "!", "schon", "bist", "du", "trun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "ADV", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Und Gram und Sorgen all versunken;", "tokens": ["Und", "Gram", "und", "Sor\u00b7gen", "all", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wir sch\u00fctzen dich, hier packt dich nicht", "tokens": ["Wir", "sch\u00fct\u00b7zen", "dich", ",", "hier", "packt", "dich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ihr freches, qu\u00e4lendes Gez\u00fccht,", "tokens": ["Ihr", "fre\u00b7ches", ",", "qu\u00e4\u00b7len\u00b7des", "Ge\u00b7z\u00fccht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Wir stehen Fa\u00df an Fa\u00df zusammen,", "tokens": ["Wir", "ste\u00b7hen", "Fa\u00df", "an", "Fa\u00df", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Wir lassen unsre Waffen flammen;", "tokens": ["Wir", "las\u00b7sen", "uns\u00b7re", "Waf\u00b7fen", "flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Und heimlich hinter unsern B\u00e4uchen", "tokens": ["Und", "heim\u00b7lich", "hin\u00b7ter", "un\u00b7sern", "B\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Mu\u00df dir die Zeit vor\u00fcberschleichen.", "tokens": ["Mu\u00df", "dir", "die", "Zeit", "vor\u00b7\u00fc\u00b7bersc\u00b7hlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Schenk ein, schenk ein, nur immer zu!", "tokens": ["Schenk", "ein", ",", "schenk", "ein", ",", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["w