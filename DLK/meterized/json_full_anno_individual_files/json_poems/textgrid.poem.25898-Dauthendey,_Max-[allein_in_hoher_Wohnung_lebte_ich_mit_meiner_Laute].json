{"textgrid.poem.25898": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[allein in hoher Wohnung lebte ich mit meiner Laute]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allein in hoher Wohnung lebte ich mit meiner Laute,", "tokens": ["Al\u00b7lein", "in", "ho\u00b7her", "Woh\u00b7nung", "leb\u00b7te", "ich", "mit", "mei\u00b7ner", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und wie die freien T\u00f6ne trug ich frei mein Haupt.", "tokens": ["Und", "wie", "die", "frei\u00b7en", "T\u00f6\u00b7ne", "trug", "ich", "frei", "mein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein k\u00fchler Abend war im k\u00fchlen Haus,", "tokens": ["Ein", "k\u00fch\u00b7ler", "A\u00b7bend", "war", "im", "k\u00fch\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sehr fern vom Erdreich lagen meine R\u00e4ume,", "tokens": ["Sehr", "fern", "vom", "Er\u00b7dreich", "la\u00b7gen", "mei\u00b7ne", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich spielte selbstzufrieden meine Laute", "tokens": ["Ich", "spiel\u00b7te", "selbst\u00b7zu\u00b7frie\u00b7den", "mei\u00b7ne", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wu\u00dfte, da\u00df es keine Menschen gab.", "tokens": ["Und", "wu\u00df\u00b7te", ",", "da\u00df", "es", "kei\u00b7ne", "Men\u00b7schen", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ging durch meine Zimmer, lauschte auf mein Lied,", "tokens": ["Ich", "ging", "durch", "mei\u00b7ne", "Zim\u00b7mer", ",", "lauschte", "auf", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und meine Lieder sprachen stets von mir.", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7der", "spra\u00b7chen", "stets", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es klopfte an die Scheiben meiner T\u00fcr,", "tokens": ["Es", "klopf\u00b7te", "an", "die", "Schei\u00b7ben", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich wu\u00dfte: vor der Flurt\u00fcr stand ein Mensch,", "tokens": ["Ich", "wu\u00df\u00b7te", ":", "vor", "der", "Flur\u00b7t\u00fcr", "stand", "ein", "Mensch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Als Freund klopfte der letzte Mensch an meine T\u00fcr.", "tokens": ["Als", "Freund", "klopf\u00b7te", "der", "letz\u00b7te", "Mensch", "an", "mei\u00b7ne", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.10": {"text": "Es klopfte heftig wieder, und es rief.", "tokens": ["Es", "klopf\u00b7te", "hef\u00b7tig", "wie\u00b7der", ",", "und", "es", "rief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich \u00f6ffne nicht, mein Lautenlied sprach eben sehr befriedigend von mir.", "tokens": ["Ich", "\u00f6ff\u00b7ne", "nicht", ",", "mein", "Lau\u00b7ten\u00b7lied", "sprach", "e\u00b7ben", "sehr", "be\u00b7frie\u00b7di\u00b7gend", "von", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.12": {"text": "Am T\u00fcrglas sehe ich des Freundes Hand.", "tokens": ["Am", "T\u00fcr\u00b7glas", "se\u00b7he", "ich", "des", "Freun\u00b7des", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich \u00f6ffne, endlich, steht ein Dunkel gro\u00df vor mir.", "tokens": ["Ich", "\u00f6ff\u00b7ne", ",", "end\u00b7lich", ",", "steht", "ein", "Dun\u00b7kel", "gro\u00df", "vor", "mir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "$,", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Weit von mir eine Stimme, die zur Erde f\u00e4llt.", "tokens": ["Weit", "von", "mir", "ei\u00b7ne", "Stim\u00b7me", ",", "die", "zur", "Er\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich biege mich zur Treppe, lausche tief,", "tokens": ["Ich", "bie\u00b7ge", "mich", "zur", "Trep\u00b7pe", ",", "lau\u00b7sche", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Im Hause unten sinkt es in den Stein.", "tokens": ["Im", "Hau\u00b7se", "un\u00b7ten", "sinkt", "es", "in", "den", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Vom tiefsten Grundstein ruft es meinen Namen noch.", "tokens": ["Vom", "tiefs\u00b7ten", "Grund\u00b7stein", "ruft", "es", "mei\u00b7nen", "Na\u00b7men", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann bleibt es leer. Die Angst scheint rot aus mir.", "tokens": ["Dann", "bleibt", "es", "leer", ".", "Die", "Angst", "scheint", "rot", "aus", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ein Dunkel wird zu einem lauten Seufzen,", "tokens": ["Ein", "Dun\u00b7kel", "wird", "zu", "ei\u00b7nem", "lau\u00b7ten", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Sehr qualvoll, weltverlassen, seufzt es auch in mir.", "tokens": ["Sehr", "qual\u00b7voll", ",", "welt\u00b7ver\u00b7las\u00b7sen", ",", "seufzt", "es", "auch", "in", "mir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAT", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Meine Laute finde ich nicht mehr,", "tokens": ["Mei\u00b7ne", "Lau\u00b7te", "fin\u00b7de", "ich", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Die Zimmer schwinden finster, finster Flur und T\u00fcr,", "tokens": ["Die", "Zim\u00b7mer", "schwin\u00b7den", "fins\u00b7ter", ",", "fins\u00b7ter", "Flur", "und", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das Dunkel tritt auf alle Schwellen, dr\u00e4ngt,", "tokens": ["Das", "Dun\u00b7kel", "tritt", "auf", "al\u00b7le", "Schwel\u00b7len", ",", "dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Um mich brennt rot die Angst als letzter Schein,", "tokens": ["Um", "mich", "brennt", "rot", "die", "Angst", "als", "letz\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADJD", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ich seufze k\u00f6rperlos und wei\u00df es unab\u00e4nderlich:", "tokens": ["Ich", "seuf\u00b7ze", "k\u00f6r\u00b7per\u00b7los", "und", "wei\u00df", "es", "un\u00b7ab\u00b7\u00e4n\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "So soll ich seufzen m\u00fcssen eine Ewigkeit,", "tokens": ["So", "soll", "ich", "seuf\u00b7zen", "m\u00fcs\u00b7sen", "ei\u00b7ne", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nie mehr ist Welt, und nie ein K\u00f6rper mehr.", "tokens": ["Nie", "mehr", "ist", "Welt", ",", "und", "nie", "ein", "K\u00f6r\u00b7per", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "$,", "KON", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Allein in hoher Wohnung lebte ich mit meiner Laute,", "tokens": ["Al\u00b7lein", "in", "ho\u00b7her", "Woh\u00b7nung", "leb\u00b7te", "ich", "mit", "mei\u00b7ner", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und wie die freien T\u00f6ne trug ich frei mein Haupt.", "tokens": ["Und", "wie", "die", "frei\u00b7en", "T\u00f6\u00b7ne", "trug", "ich", "frei", "mein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein k\u00fchler Abend war im k\u00fchlen Haus,", "tokens": ["Ein", "k\u00fch\u00b7ler", "A\u00b7bend", "war", "im", "k\u00fch\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sehr fern vom Erdreich lagen meine R\u00e4ume,", "tokens": ["Sehr", "fern", "vom", "Er\u00b7dreich", "la\u00b7gen", "mei\u00b7ne", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich spielte selbstzufrieden meine Laute", "tokens": [