{"dta.poem.6361": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein heisser C\u00f6rper ist formirt von kleinen Theilen,", "tokens": ["Ein", "heis\u00b7ser", "C\u00f6r\u00b7per", "ist", "for\u00b7mirt", "von", "klei\u00b7nen", "Thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sehr beweglich sind, die unaufh\u00f6rlich eilen.", "tokens": ["Die", "sehr", "be\u00b7weg\u00b7lich", "sind", ",", "die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Aus Theilchen, welche sich entweder nie bewegen,", "tokens": ["Aus", "Theil\u00b7chen", ",", "wel\u00b7che", "sich", "ent\u00b7we\u00b7der", "nie", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie, oder deren Krafft der Hitz\u2019 entgegen geht,", "tokens": ["Wie", ",", "o\u00b7der", "de\u00b7ren", "Krafft", "der", "Hitz'", "ent\u00b7ge\u00b7gen", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "PRELAT", "NN", "ART", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hefftig, da\u00df sie sich, gehemmt, nicht mehr kan regen.", "tokens": ["So", "heff\u00b7tig", ",", "da\u00df", "sie", "sich", ",", "ge\u00b7hemmt", ",", "nicht", "mehr", "kan", "re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "$,", "VVPP", "$,", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}