{"textgrid.poem.67583": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdie M\u00e4uslein sind zu sehr erschreckt;", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie M\u00e4uslein sind zu sehr erschreckt;", "tokens": ["\u00bb", "die", "M\u00e4us\u00b7lein", "sind", "zu", "sehr", "er\u00b7schreckt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bleiben, wo sie sind, versteckt,", "tokens": ["Sie", "blei\u00b7ben", ",", "wo", "sie", "sind", ",", "ver\u00b7steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ich leid' Hungersnoth!", "tokens": ["Und", "ich", "leid'", "Hun\u00b7gers\u00b7noth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was gilt's, ich stell' mich todt!", "tokens": ["Was", "gilt's", ",", "ich", "stell'", "mich", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kluge Frau", "tokens": ["Ich", "klu\u00b7ge", "Frau"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bin schlau.\u00ab", "tokens": ["Bin", "schlau", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Frau Katze sprach's und that's genau:", "tokens": ["Frau", "Kat\u00b7ze", "sprach's", "und", "that's", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da lag ihr Kopf", "tokens": ["Da", "lag", "ihr", "Kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und dort ihr langer Schwanz.", "tokens": ["Und", "dort", "ihr", "lan\u00b7ger", "Schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "\u00bbbist doch ein Tropf,", "tokens": ["\u00bb", "bist", "doch", "ein", "Tropf", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Hochweise Frau!", "tokens": ["Hoch\u00b7wei\u00b7se", "Frau", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Werd auch ein Sack! wir kennen Dich schon ganz.\u00ab", "tokens": ["Werd", "auch", "ein", "Sack", "!", "wir", "ken\u00b7nen", "Dich", "schon", "ganz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hochweise Frau,", "tokens": ["Hoch\u00b7wei\u00b7se", "Frau", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iamb