{"textgrid.poem.52122": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tischler machet nicht durch Hobeln und Beziehn", "tokens": ["Der", "Tischler", "ma\u00b7chet", "nicht", "durch", "Ho\u00b7beln", "und", "Be\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Zum Heiligthum ein Bild, du machst es durch dein Knien:", "tokens": ["Zum", "Hei\u00b7lig\u00b7thum", "ein", "Bild", ",", "du", "machst", "es", "durch", "dein", "Kni\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Vater hat dich nicht den Gottesdienst gelehret", "tokens": ["Dein", "Va\u00b7ter", "hat", "dich", "nicht", "den", "Got\u00b7tes\u00b7dienst", "ge\u00b7leh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn er die Peltze flickt, als wie es sich geh\u00f6ret.", "tokens": ["Wenn", "er", "die", "Pelt\u00b7ze", "flickt", ",", "als", "wie", "es", "sich", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn er die G\u00f6tzen fand, die vor ihm stets verblichen,", "tokens": ["Wenn", "er", "die", "G\u00f6t\u00b7zen", "fand", ",", "die", "vor", "ihm", "stets", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sprach er: Ducket euch, ihr sollt in Ofen kriechen:", "tokens": ["So", "sprach", "er", ":", "Du\u00b7cket", "euch", ",", "ihr", "sollt", "in", "O\u00b7fen", "krie\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bedencke wie du dich hast deinem Gott entwandt:", "tokens": ["Be\u00b7den\u00b7cke", "wie", "du", "dich", "hast", "dei\u00b7nem", "Gott", "ent\u00b7wandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was du verehrst, das hat dein Vater vor verbrannt.", "tokens": ["Was", "du", "ver\u00b7ehrst", ",", "das", "hat", "dein", "Va\u00b7ter", "vor", "ver\u00b7brannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Tischler machet nicht durch Hobeln und Beziehn"