{"textgrid.poem.60680": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Windgott und Sonnengott sahn einen Reiter,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Windgott und Sonnengott sahn einen Reiter,", "tokens": ["Wind\u00b7gott", "und", "Son\u00b7nen\u00b7gott", "sahn", "ei\u00b7nen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der gegen Wetterunbill sorglich ausger\u00fcstet.", "tokens": ["Der", "ge\u00b7gen", "Wet\u00b7te\u00b7run\u00b7bill", "sorg\u00b7lich", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vorsicht tut not dem, den's im Herbst gel\u00fcstet", "tokens": ["Vor\u00b7sicht", "tut", "not", "dem", ",", "den's", "im", "Herbst", "ge\u00b7l\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "ART", "$,", "PRELS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinauszuziehn: bald ist es tr\u00fcb, bald heiter;", "tokens": ["Hin\u00b7aus\u00b7zu\u00b7ziehn", ":", "bald", "ist", "es", "tr\u00fcb", ",", "bald", "hei\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIZU", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn sich Iris mit der bunten Sch\u00e4rpe br\u00fcstet,", "tokens": ["Und", "wenn", "sich", "I\u00b7ris", "mit", "der", "bun\u00b7ten", "Sch\u00e4r\u00b7pe", "br\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "NE", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ist's gescheiter,", "tokens": ["So", "ist's", "ge\u00b7schei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Den Mantel mitzunehmen als daheim zu lassen.", "tokens": ["Den", "Man\u00b7tel", "mit\u00b7zu\u00b7neh\u00b7men", "als", "da\u00b7heim", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u00bbunsichre Monde\u00ab, sagen die Lateiner drum", "tokens": ["\u00bb", "un\u00b7sich\u00b7re", "Mon\u00b7de", "\u00ab", ",", "sa\u00b7gen", "die", "La\u00b7tei\u00b7ner", "drum"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$(", "$,", "VVFIN", "ART", "NN", "PAV"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Von jener Zeit. Und unser Reiter war nicht dumm:", "tokens": ["Von", "je\u00b7ner", "Zeit", ".", "Und", "un\u00b7ser", "Rei\u00b7ter", "war", "nicht", "dumm", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$.", "KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er hatte seinen festen, dicken Mantel um", "tokens": ["Er", "hat\u00b7te", "sei\u00b7nen", "fes\u00b7ten", ",", "di\u00b7cken", "Man\u00b7tel", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und bangte nicht vor Sturm und Regenmassen.", "tokens": ["Und", "bang\u00b7te", "nicht", "vor", "Sturm", "und", "Re\u00b7gen\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "\u00bbf\u00fcr jeden Fall gesch\u00fctzt h\u00e4lt sich der gute Mann,\u00ab", "tokens": ["\u00bb", "f\u00fcr", "je\u00b7den", "Fall", "ge\u00b7sch\u00fctzt", "h\u00e4lt", "sich", "der", "gu\u00b7te", "Mann", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VVPP", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So sprach der Wind, \u00bber ahnt nicht, wie ich blasen kann.", "tokens": ["So", "sprach", "der", "Wind", ",", "\u00bb", "er", "ahnt", "nicht", ",", "wie", "ich", "bla\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kein Knopf h\u00e4lt stand vor meinem Wehen!", "tokens": ["Kein", "Knopf", "h\u00e4lt", "stand", "vor", "mei\u00b7nem", "We\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Teufel mu\u00df der Mantel gehen,", "tokens": ["Zum", "Teu\u00b7fel", "mu\u00df", "der", "Man\u00b7tel", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich es will. Willst du es sehen?", "tokens": ["Wenn", "ich", "es", "will", ".", "Willst", "du", "es", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "$.", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir h\u00e4tten unsre Freude dran.\u00ab", "tokens": ["Wir", "h\u00e4t\u00b7ten", "uns\u00b7re", "Freu\u00b7de", "dran", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbgut,\u00ab sagte Ph\u00f6bus, \u00bbda du so gewi\u00df es wei\u00dft,", "tokens": ["\u00bb", "gut", ",", "\u00ab", "sag\u00b7te", "Ph\u00f6\u00b7bus", ",", "\u00bb", "da", "du", "so", "ge\u00b7wi\u00df", "es", "wei\u00dft", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "VVFIN", "NE", "$,", "$(", "KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So la\u00df uns wetten, wer von uns zuerst dem Mann", "tokens": ["So", "la\u00df", "uns", "wet\u00b7ten", ",", "wer", "von", "uns", "zu\u00b7erst", "dem", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVFIN", "$,", "PWS", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Mantel von den Schultern rei\u00dft.", "tokens": ["Den", "Man\u00b7tel", "von", "den", "Schul\u00b7tern", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Beginne du! Ich will so lange mich verstecken.\u00ab", "tokens": ["Be\u00b7gin\u00b7ne", "du", "!", "Ich", "will", "so", "lan\u00b7ge", "mich", "ver\u00b7ste\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der siegessichre Bl\u00e4ser stopft sich voll mit Luft,", "tokens": ["Der", "sie\u00b7ges\u00b7sich\u00b7re", "Bl\u00e4\u00b7ser", "stopft", "sich", "voll", "mit", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bl\u00e4ht sich wie ein Ballon und tobt sodann", "tokens": ["Bl\u00e4ht", "sich", "wie", "ein", "Bal\u00b7lon", "und", "tobt", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOKOM", "ART", "NN", "KON", "VVFIN", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Los wie ein H\u00f6llenschrecken", "tokens": ["Los", "wie", "ein", "H\u00f6l\u00b7len\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und saust und pfeift und braust und pufft,", "tokens": ["Und", "saust", "und", "pfeift", "und", "braust", "und", "pufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Hebt D\u00e4cher ab auf seiner Bahn,", "tokens": ["Hebt", "D\u00e4\u00b7cher", "ab", "auf", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Versenkt im Flusse Schiff und Kahn.", "tokens": ["Ver\u00b7senkt", "im", "Flus\u00b7se", "Schiff", "und", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Um eines Mantels wegen alles dies!", "tokens": ["Um", "ei\u00b7nes", "Man\u00b7tels", "we\u00b7gen", "al\u00b7les", "dies", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "PIS", "PDS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Der Mann gab acht und lie\u00df", "tokens": ["Der", "Mann", "gab", "acht", "und", "lie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "CARD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Den Wind sich nicht im Mantel fangen.", "tokens": ["Den", "Wind", "sich", "nicht", "im", "Man\u00b7tel", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Das sch\u00fctzte ihn. Je mehr es um ihn braust und br\u00fcllt,", "tokens": ["Das", "sch\u00fctz\u00b7te", "ihn", ".", "Je", "mehr", "es", "um", "ihn", "braust", "und", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "ADV", "ADV", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.19": {"text": "Nur desto fester hat er sich hineingeh\u00fcllt.", "tokens": ["Nur", "des\u00b7to", "fes\u00b7ter", "hat", "er", "sich", "hin\u00b7ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So blieb der Mantel gut an seinem Platze hangen,", "tokens": ["So", "blieb", "der", "Man\u00b7tel", "gut", "an", "sei\u00b7nem", "Plat\u00b7ze", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wie der Sturm auch faucht und fegt,", "tokens": ["Und", "wie", "der", "Sturm", "auch", "faucht", "und", "fegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der Kragen und die Falten wurden nur bewegt.", "tokens": ["Der", "Kra\u00b7gen", "und", "die", "Fal\u00b7ten", "wur\u00b7den", "nur", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sobald der Wette festgesetzte Zeit vergangen,", "tokens": ["So\u00b7bald", "der", "Wet\u00b7te", "fest\u00b7ge\u00b7setz\u00b7te", "Zeit", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Verjagt die Sonne Sturm und Regen,", "tokens": ["Ver\u00b7jagt", "die", "Son\u00b7ne", "Sturm", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Erfreut den Reiter, den sie bald erhitzt,", "tokens": ["Er\u00b7freut", "den", "Rei\u00b7ter", ",", "den", "sie", "bald", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df er im Wettermantel schwitzt,", "tokens": ["Da\u00df", "er", "im", "Wet\u00b7ter\u00b7man\u00b7tel", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und zwingt ihn so, den Mantel abzulegen;", "tokens": ["Und", "zwingt", "ihn", "so", ",", "den", "Man\u00b7tel", "ab\u00b7zu\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Fand ohne gro\u00dfe M\u00fche ihn bereit.", "tokens": ["Fand", "oh\u00b7ne", "gro\u00b7\u00dfe", "M\u00fc\u00b7he", "ihn", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sanftmut kann mehr als Heftigkeit.", "tokens": ["Sanft\u00b7mut", "kann", "mehr", "als", "Hef\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Windgott und Sonnengott sahn einen Reiter,", "tokens": ["Wind\u00b7gott", "und", "Son\u00b7nen\u00b7gott", "sahn", "ei\u00b7nen", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der gegen Wetterunbill sorglich ausger\u00fcstet.", "tokens": ["Der", "ge\u00b7gen", "Wet\u00b7te\u00b7run\u00b7bill", "sorg\u00b7lich", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vorsicht tut not dem, den's im Herbst gel\u00fcstet", "tokens": ["Vor\u00b7sicht", "tut", "not", "dem", ",", "den's", "im", "Herbst", "ge\u00b7l\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "ART", "$,", "PRELS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hinauszuziehn: bald ist es tr\u00fcb, bald heiter;", "tokens": ["Hin\u00b7aus\u00b7zu\u00b7ziehn", ":", "bald", "ist", "es", "tr\u00fcb", ",", "bald", "hei\u00b7ter", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "w