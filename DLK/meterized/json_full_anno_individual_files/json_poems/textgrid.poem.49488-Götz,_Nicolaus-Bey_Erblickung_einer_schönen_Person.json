Thaus", "tokens": ["Und", "ihr", "Trop\u00b7fen", "hel\u00b7len", "Thaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Auf den weissen Busen spr\u00fctzen,", "tokens": ["Auf", "den", "weis\u00b7sen", "Bu\u00b7sen", "spr\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wo der \u00dcberflu\u00df sich bl\u00e4ht.", "tokens": ["Wo", "der", "\u00dc\u00b7berf\u00b7lu\u00df", "sich", "bl\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Vor ihr h\u00fcpft die Fr\u00f6lichkeit", "tokens": ["Vor", "ihr", "h\u00fcpft", "die", "Fr\u00f6\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "In dem weissen Sommer-Kleidgen,", "tokens": ["In", "dem", "weis\u00b7sen", "Som\u00b7mer\u00b7Kleid\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und die Schertze, nebst den Spielen,", "tokens": ["Und", "die", "Schert\u00b7ze", ",", "nebst", "den", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die, gleich kleinen Engelchen,", "tokens": ["Die", ",", "gleich", "klei\u00b7nen", "En\u00b7gel\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+--", "measure": "anapaest.init"}, "line.21": {"text": "Aus den angef\u00fcllten Sch\u00fcrtzgen", "tokens": ["Aus", "den", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Sch\u00fcrtz\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Mit den kleinen G\u00f6tterh\u00e4nden", "tokens": ["Mit", "den", "klei\u00b7nen", "G\u00f6t\u00b7ter\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Rosen, Veilgen, Lilgen holen,", "tokens": ["Ro\u00b7sen", ",", "Veil\u00b7gen", ",", "Lil\u00b7gen", "ho\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und die Sch\u00f6ne, und den Pfad,", "tokens": ["Und", "die", "Sch\u00f6\u00b7ne", ",", "und", "den", "Pfad", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Wo die Sch\u00f6ne geht, bestreuen.", "tokens": ["Wo", "die", "Sch\u00f6\u00b7ne", "geht", ",", "be\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Himmel! nun erkenn ich sie!", "tokens": ["Him\u00b7mel", "!", "nun", "er\u00b7kenn", "ich", "sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Himmel! ja es ist ", "tokens": ["Him\u00b7mel", "!", "ja", "es", "ist"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "PPER", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "O mit welcher Lieblichkeit!", "tokens": ["O", "mit", "wel\u00b7cher", "Lieb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Tr\u00e4gt sie auf den weichen Armen,", "tokens": ["Tr\u00e4gt", "sie", "auf", "den", "wei\u00b7chen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "N\u00e4chst dem Herzen, an der Brust,", "tokens": ["N\u00e4chst", "dem", "Her\u00b7zen", ",", "an", "der", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Ihre holde Augenweide", "tokens": ["Ih\u00b7re", "hol\u00b7de", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Das geliebte junge Lamm,", "tokens": ["Das", "ge\u00b7lieb\u00b7te", "jun\u00b7ge", "Lamm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Und begl\u00fccket es mit K\u00fcssen,", "tokens": ["Und", "be\u00b7gl\u00fc\u00b7cket", "es", "mit", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Die der Himmel selbst sich w\u00fcnscht!", "tokens": ["Die", "der", "Him\u00b7mel", "selbst", "sich", "w\u00fcnscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "O mit welcher Majest\u00e4t!", "tokens": ["O", "mit", "wel\u00b7cher", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Wallt sie nach dem Myrthenw\u00e4ldgen,", "tokens": ["Wallt", "sie", "nach", "dem", "Myr\u00b7then\u00b7w\u00e4ld\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Wo ihr liebster ", "tokens": ["Wo", "ihr", "liebs\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Voll Begierden auf sie wartet;", "tokens": ["Voll", "Be\u00b7gier\u00b7den", "auf", "sie", "war\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Cypria war minder sch\u00f6n,", "tokens": ["Cyp\u00b7ria", "war", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Wenn sie mit den keuschen Nymphen", "tokens": ["Wenn", "sie", "mit", "den", "keu\u00b7schen", "Nym\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Und den nackten Gratien", "tokens": ["Und", "den", "nack\u00b7ten", "Gra\u00b7ti\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Unterm hellen Abendsterne", "tokens": ["Un\u00b7term", "hel\u00b7len", "A\u00b7bends\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Von Siciliens Geb\u00fcrge,", "tokens": ["Von", "Si\u00b7ci\u00b7li\u00b7ens", "Ge\u00b7b\u00fcr\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "In die stillen Th\u00e4ler stieg.", "tokens": ["In", "die", "stil\u00b7len", "Th\u00e4\u00b7ler", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche sch\u00f6ne Sch\u00e4ferin,", "tokens": ["Wel\u00b7che", "sch\u00f6\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auf dieser Morgeninsel", "tokens": ["Die", "auf", "die\u00b7ser", "Mor\u00b7ge\u00b7nin\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die reinste Sonne strahlt?", "tokens": ["Wie", "die", "reins\u00b7te", "Son\u00b7ne", "strahlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keuschheit, Unschuld, Sittsamkeit", "tokens": ["Keuschheit", ",", "Un\u00b7schuld", ",", "