{"textgrid.poem.55919": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des Dichters", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er lag. Sein aufgestelltes Antlitz war", "tokens": ["Er", "lag", ".", "Sein", "auf\u00b7ge\u00b7stell\u00b7tes", "Ant\u00b7litz", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bleich und verweigernd in den steilen Kissen,", "tokens": ["bleich", "und", "ver\u00b7wei\u00b7gernd", "in", "den", "stei\u00b7len", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "seitdem die Welt und dieses von-ihr-Wissen,", "tokens": ["seit\u00b7dem", "die", "Welt", "und", "die\u00b7ses", "von\u00b7ih\u00b7rWis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "von seinen Sinnen abgerissen,", "tokens": ["von", "sei\u00b7nen", "Sin\u00b7nen", "ab\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "zur\u00fcckfiel an das teilnahmslose Jahr.", "tokens": ["zu\u00b7r\u00fcck\u00b7fiel", "an", "das", "teil\u00b7nahms\u00b7lo\u00b7se", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die, so ihn leben sahen, wu\u00dften nicht,", "tokens": ["Die", ",", "so", "ihn", "le\u00b7ben", "sa\u00b7hen", ",", "wu\u00df\u00b7ten", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "VVINF", "VVFIN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie sehr er Eines war mit allem diesen;", "tokens": ["wie", "sehr", "er", "Ei\u00b7nes", "war", "mit", "al\u00b7lem", "die\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PIS", "VAFIN", "APPR", "PIS", "PDAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "denn Dieses: diese Tiefen, diese Wiesen", "tokens": ["denn", "Die\u00b7ses", ":", "die\u00b7se", "Tie\u00b7fen", ",", "die\u00b7se", "Wie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "$.", "PDAT", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und diese Wasser ", "tokens": ["und", "die\u00b7se", "Was\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O sein Gesicht war diese ganze Weite,", "tokens": ["O", "sein", "Ge\u00b7sicht", "war", "die\u00b7se", "gan\u00b7ze", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "die jetzt noch zu ihm will und um ihn wirbt;", "tokens": ["die", "jetzt", "noch", "zu", "ihm", "will", "und", "um", "ihn", "wirbt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wor