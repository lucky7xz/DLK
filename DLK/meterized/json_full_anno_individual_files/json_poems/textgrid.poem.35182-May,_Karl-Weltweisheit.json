{"textgrid.poem.35182": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Weltweisheit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klingt ein Lied vom Himmel nieder", "tokens": ["Es", "klingt", "ein", "Lied", "vom", "Him\u00b7mel", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wunderlieb, so klar, so rein,", "tokens": ["So", "wun\u00b7der\u00b7lieb", ",", "so", "klar", ",", "so", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Seele singts ihm wieder;", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "singts", "ihm", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie will dem Himmel dankbar sein.", "tokens": ["Sie", "will", "dem", "Him\u00b7mel", "dank\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Andern lauschen rings im Kreise;", "tokens": ["Die", "An\u00b7dern", "lau\u00b7schen", "rings", "im", "Krei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann siehst du, da\u00df sie l\u00e4chelnd weitergehn.", "tokens": ["Dann", "siehst", "du", ",", "da\u00df", "sie", "l\u00e4\u00b7chelnd", "wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie sind zu klug, sie sind zu weise,", "tokens": ["Sie", "sind", "zu", "klug", ",", "sie", "sind", "zu", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um das, was dich beseligt, zu verstehn.", "tokens": ["Um", "das", ",", "was", "dich", "be\u00b7se\u00b7ligt", ",", "zu", "ver\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PWS", "PPER", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es kommt ein Strahl vom Himmel nieder;", "tokens": ["Es", "kommt", "ein", "Strahl", "vom", "Him\u00b7mel", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er leuchtet in dein Herz hinein,", "tokens": ["Er", "leuch\u00b7tet", "in", "dein", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dieses strahlt ihn Andern wieder;", "tokens": ["Und", "die\u00b7ses", "strahlt", "ihn", "An\u00b7dern", "wie\u00b7der", ";"], "token_info": ["word", "wor