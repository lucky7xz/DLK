{"textgrid.poem.56859": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: So m\u00f6cht ich sterben, wie ich jetzt mein Boot", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So m\u00f6cht ich sterben, wie ich jetzt mein Boot", "tokens": ["So", "m\u00f6cht", "ich", "ster\u00b7ben", ",", "wie", "ich", "jetzt", "mein", "Boot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus sonnenbunten Fluten heimw\u00e4rts treibe.", "tokens": ["aus", "son\u00b7nen\u00b7bun\u00b7ten", "Flu\u00b7ten", "heim\u00b7w\u00e4rts", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch gl\u00fcht die Luft, noch liegt ein g\u00fctig Gold", "tokens": ["Noch", "gl\u00fcht", "die", "Luft", ",", "noch", "liegt", "ein", "g\u00fc\u00b7tig", "Gold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf mir und allem um mich her gebreitet.", "tokens": ["auf", "mir", "und", "al\u00b7lem", "um", "mich", "her", "ge\u00b7brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PIS", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bereit und heiter tu ich Schlag auf Schlag", "tokens": ["Be\u