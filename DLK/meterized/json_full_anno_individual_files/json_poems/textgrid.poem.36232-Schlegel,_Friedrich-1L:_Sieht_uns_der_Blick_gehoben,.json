{"textgrid.poem.36232": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieht uns der Blick gehoben,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieht uns der Blick gehoben,", "tokens": ["Sieht", "uns", "der", "Blick", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So glaubt das Herz die Schwere zu besiegen,", "tokens": ["So", "glaubt", "das", "Herz", "die", "Schwe\u00b7re", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu den Himmlischen oben", "tokens": ["Zu", "den", "Himm\u00b7li\u00b7schen", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Will es dringen und fliegen.", "tokens": ["Will", "es", "drin\u00b7gen", "und", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Der Mensch, emporgeschwungen,", "tokens": ["Der", "Mensch", ",", "em\u00b7por\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Glaubt schon, er sei durch die Wolken gedrungen.", "tokens": ["Glaubt", "schon", ",", "er", "sei", "durch", "die", "Wol\u00b7ken", "ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald mu\u00df er staunend merken,", "tokens": ["Bald", "mu\u00df", "er", "stau\u00b7nend", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ewig fest wir auf uns selbst begr\u00fcndet.", "tokens": ["Wie", "e\u00b7wig", "fest", "wir", "auf", "uns", "selbst", "be\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es strebt in sichern Werken", "tokens": ["Es", "strebt", "in", "si\u00b7chern", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein ganzes Tun, verb\u00fcndet,", "tokens": ["Sein", "gan\u00b7zes", "Tun", ",", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom Grunde nie zu wanken,", "tokens": ["Vom", "Grun\u00b7de", "nie", "zu", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er baut wie Felsen den Bau der Gedanken.", "tokens": ["Er", "baut", "wie", "Fel\u00b7sen", "den", "Bau", "der", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word",