{"textgrid.poem.51273": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist umbsonst, o Mensch, darnach du so wilst rennen,", "tokens": ["Es", "ist", "um\u00b7bsonst", ",", "o", "Mensch", ",", "dar\u00b7nach", "du", "so", "wilst", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "PAV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6r, ob ich es erkannt, ich wil dein thun dir nennen:", "tokens": ["H\u00f6r", ",", "ob", "ich", "es", "er\u00b7kannt", ",", "ich", "wil", "dein", "thun", "dir", "nen\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,", "PPER", "VMFIN", "PPOSAT", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ehre, die du suchst, ist Rauch, die Kunst ist Wind,", "tokens": ["Die", "Eh\u00b7re", ",", "die", "du", "suchst", ",", "ist", "Rauch", ",", "die", "Kunst", "ist", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die Begier ist Glutt, die dich so sehr entz\u00fcndt.", "tokens": ["Und", "die", "Be\u00b7gier", "ist", "Glutt", ",", "die", "dich", "so", "sehr", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Bild der Eitelkeit, auff was wilstu wol trauen?", "tokens": ["Du", "Bild", "der", "Ei\u00b7tel\u00b7keit", ",", "auff", "was", "wils\u00b7tu", "wol", "trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "APPR", "PWS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wilt du auff diesen Grund dein Thun und Wesen bauen?", "tokens": ["Wilt", "du", "auff", "die\u00b7sen", "Grund", "dein", "Thun", "und", "We\u00b7sen", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Fall die freche Glutt die Sinnen dir verzehrt,", "tokens": ["Im", "Fall", "die", "fre\u00b7che", "Glutt", "die", "Sin\u00b7nen", "dir", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der scharffe Rauch dich blendt, der falsche Wind verkehrt.", "tokens": ["Der", "scharf\u00b7fe", "Rauch", "dich", "blendt", ",", "der", "fal\u00b7sche", "Wind", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Kanstu mit Billigkeit und Recht dich nicht beklagen?", "tokens": ["Kans\u00b7tu", "mit", "Bil\u00b7lig\u00b7keit", "und", "Recht", "dich", "nicht", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus deinem W\u00fcnschen blo\u00df entspringen diese Plagen:", "tokens": ["Aus", "dei\u00b7nem", "W\u00fcn\u00b7schen", "blo\u00df", "ent\u00b7sprin\u00b7gen", "die\u00b7se", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ists nicht, da\u00df der Begier du dich so gantz ergiebst,", "tokens": ["Ists", "nicht", ",", "da\u00df", "der", "Be\u00b7gier", "du", "dich", "so", "gantz", "er\u00b7giebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So geitzig thust nach Kunst, so eyfrig Ehre liebst.", "tokens": ["So", "geit\u00b7zig", "thust", "nach", "Kunst", ",", "so", "ey\u00b7frig", "Eh\u00b7re", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "NN", "$,", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seit Ehren, Kunst, Begier dich bracht in ihren Orden,", "tokens": ["Seit", "Eh\u00b7ren", ",", "Kunst", ",", "Be\u00b7gier", "dich", "bracht", "in", "ih\u00b7ren", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du dar\u00fcber auch Wind, Rauch und Feuer worden:", "tokens": ["Bist", "du", "da\u00b7r\u00fc\u00b7ber", "auch", "Wind", ",", "Rauch", "und", "Feu\u00b7er", "wor\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADV", "NN", "$,", "NN", "KON", "NN", "VAPP", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Doch nihmst du nur aus dir, o Mensch, ists nicht umbsonst,", "tokens": ["Doch", "nihmst", "du", "nur", "aus", "dir", ",", "o", "Mensch", ",", "ists", "nicht", "um\u00b7bsonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die du auff Erden suchst, Ehr und Begier und Kunst.", "tokens": ["Die", "du", "auff", "Er\u00b7den", "suchst", ",", "Ehr", "und", "Be\u00b7gier", "und", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Du Strahl der Ewigkeit kanstu zur\u00fccke gehen,", "tokens": ["Du", "Strahl", "der", "E\u00b7wig\u00b7keit", "kans\u00b7tu", "zu\u00b7r\u00fc\u00b7cke", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird deine Glutt, dein Rauch, dein Wind gar wol bestehen.", "tokens": ["Wird", "dei\u00b7ne", "Glutt", ",", "dein", "Rauch", ",", "dein", "Wind", "gar", "wol", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Glut sey des Glaubens Krafft, Wind, der sie feuert an,", "tokens": ["Glut", "sey", "des", "Glau\u00b7bens", "Krafft", ",", "Wind", ",", "der", "sie", "feu\u00b7ert", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,", "NN", "$,", "PRELS", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Des Geistes Sanfftigkeit, Rauch, des Gebethes Bahn.", "tokens": ["Des", "Geis\u00b7tes", "Sanff\u00b7tig\u00b7keit", ",", "Rauch", ",", "des", "Ge\u00b7be\u00b7thes", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wann die Hoffnung drauff die Bahn wird wegsam machen,", "tokens": ["Und", "wann", "die", "Hoff\u00b7nung", "drauff", "die", "Bahn", "wird", "weg\u00b7sam", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PAV", "ART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe sanffte wehn: Der Glauben kr\u00e4ftig krachen:", "tokens": ["Die", "Lie\u00b7be", "sanff\u00b7te", "wehn", ":", "Der", "Glau\u00b7ben", "kr\u00e4f\u00b7tig", "kra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$.", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wird aus Rauch die Ehr, aus Feuer die Begier,", "tokens": ["So", "wird", "aus", "Rauch", "die", "Ehr", ",", "aus", "Feu\u00b7er", "die", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und aus dem Winde sich die Kunst erst machen f\u00fcr.", "tokens": ["Und", "aus", "dem", "Win\u00b7de", "sich", "die", "Kunst", "erst", "ma\u00b7chen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "ART", "NN", "ADV", "VVINF", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Kunst ist einig seyn: Die Ehr ist Gott erkennen,", "tokens": ["Die", "Kunst", "ist", "ei\u00b7nig", "seyn", ":", "Die", "Ehr", "ist", "Gott", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAINF", "$.", "ART", "NN", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": 