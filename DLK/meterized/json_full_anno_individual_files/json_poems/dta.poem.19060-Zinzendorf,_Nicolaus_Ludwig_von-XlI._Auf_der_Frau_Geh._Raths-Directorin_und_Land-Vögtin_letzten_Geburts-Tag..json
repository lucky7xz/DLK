euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Hebt eure Augen auf, und sehet auf die Hunde,", "tokens": ["Hebt", "eu\u00b7re", "Au\u00b7gen", "auf", ",", "und", "se\u00b7het", "auf", "die", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Vielleicht erblickt ihr auch ein wohlgeartet Lamm,", "tokens": ["Viel\u00b7leicht", "er\u00b7blickt", "ihr", "auch", "ein", "wohl\u00b7ge\u00b7ar\u00b7tet", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Und damit la\u00dft euch ein, vernehmt aus seinem Munde:", "tokens": ["Und", "da\u00b7mit", "la\u00dft", "euch", "ein", ",", "ver\u00b7nehmt", "aus", "sei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Wie ihm geschehen ist, da es zum Hirten kam.", "tokens": ["Wie", "ihm", "ge\u00b7sche\u00b7hen", "ist", ",", "da", "es", "zum", "Hir\u00b7ten", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Vermuthet ihr viel