{"dta.poem.19060": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlI.  Auf der Frau Geh. Raths-Directorin  \n und Land-V\u00f6gtin letzten Geburts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar bebt der Erden-Krei\u00df von so viel eiteln Lasten;", "tokens": ["Zwar", "bebt", "der", "Er\u00b7den\u00b7Krei\u00df", "von", "so", "viel", "ei\u00b7teln", "Las\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch ist sein kleinster Theil am allerschlimmsten dran:", "tokens": ["Doch", "ist", "sein", "kleins\u00b7ter", "Theil", "am", "al\u00b7ler\u00b7schlimms\u00b7ten", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil er das gantze Heer der Christlichen Phantasten", "tokens": ["Weil", "er", "das", "gant\u00b7ze", "Heer", "der", "Christ\u00b7li\u00b7chen", "Phan\u00b7tas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf seinen Boden tr\u00e4gt, und kaum ertragen kan.", "tokens": ["Auf", "sei\u00b7nen", "Bo\u00b7den", "tr\u00e4gt", ",", "und", "kaum", "er\u00b7tra\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo hat ein Wanders-Mann bey solcherley Barbaren,", "tokens": ["Wo", "hat", "ein", "Wan\u00b7der\u00b7sMann", "bey", "sol\u00b7cher\u00b7ley", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da man das Menschen-Fleisch zu feilen Kauffe tr\u00e4gt,", "tokens": ["Da", "man", "das", "Men\u00b7schen\u00b7Fleisch", "zu", "fei\u00b7len", "Kauf\u00b7fe", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So einen Glaubens-Grund gelesen und erfahren,", "tokens": ["So", "ei\u00b7nen", "Glau\u00b7bens\u00b7Grund", "ge\u00b7le\u00b7sen", "und", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drauf mancher unter uns sein Haupt so sanfte legt?", "tokens": ["Drauf", "man\u00b7cher", "un\u00b7ter", "uns", "sein", "Haupt", "so", "sanf\u00b7te", "legt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "APPR", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Solt ein und anderer der fernen Hottentotten", "tokens": ["Solt", "ein", "und", "an\u00b7de\u00b7rer", "der", "fer\u00b7nen", "Hot\u00b7ten\u00b7tot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "KON", "ADJA", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Christliche Gesetz, und einen Christen sehn,", "tokens": ["Das", "Christ\u00b7li\u00b7che", "Ge\u00b7setz", ",", "und", "ei\u00b7nen", "Chris\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und w\u00fcste nicht, wie der, des Heiligsten zu spotten;", "tokens": ["Und", "w\u00fcs\u00b7te", "nicht", ",", "wie", "der", ",", "des", "Hei\u00b7ligs\u00b7ten", "zu", "spot\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "So w\u00fcrden ihm gewi\u00df die Augen \u00fcbergehn.", "tokens": ["So", "w\u00fcr\u00b7den", "ihm", "ge\u00b7wi\u00df", "die", "Au\u00b7gen", "\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie man dem Teufel sonst in abgelegnem H\u00e4yne", "tokens": ["Wie", "man", "dem", "Teu\u00b7fel", "sonst", "in", "ab\u00b7ge\u00b7leg\u00b7nem", "H\u00e4y\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit zitterndem Verdru\u00df bey denen Heyden dient;", "tokens": ["Mit", "zit\u00b7tern\u00b7dem", "Ver\u00b7dru\u00df", "bey", "de\u00b7nen", "Hey\u00b7den", "dient", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So weyht man hier dem HErrn die unbelebten Steine,", "tokens": ["So", "weyht", "man", "hier", "dem", "Herrn", "die", "un\u00b7be\u00b7leb\u00b7ten", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da Satans Bild und Sinn davor im Hertzen gr\u00fcnt.", "tokens": ["Da", "Sa\u00b7tans", "Bild", "und", "Sinn", "da\u00b7vor", "im", "Hert\u00b7zen", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "KON", "NN", "PAV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vermeynte wohl ein Mann von unge\u00fcbten Sinnen,", "tokens": ["Ver\u00b7meyn\u00b7te", "wohl", "ein", "Mann", "von", "un\u00b7ge\u00b7\u00fcb\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey mancher Prediger geschminckten K\u00fcnsteley,", "tokens": ["Bey", "man\u00b7cher", "Pre\u00b7di\u00b7ger", "ge\u00b7schminck\u00b7ten", "K\u00fcns\u00b7te\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und bey der H\u00f6renden ver\u00e4chtlichen Beginnen;", "tokens": ["Und", "bey", "der", "H\u00f6\u00b7ren\u00b7den", "ver\u00b7\u00e4cht\u00b7li\u00b7chen", "Be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Obs auch der pure Ernst mit allen beyden sey?", "tokens": ["Obs", "auch", "der", "pu\u00b7re", "Ernst", "mit", "al\u00b7len", "bey\u00b7den", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und dennoch kommet man bey nahe ins Gedr\u00e4nge,", "tokens": ["Und", "den\u00b7noch", "kom\u00b7met", "man", "bey", "na\u00b7he", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "APPR", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn ein geweyhter Tag mit anzusehen ist.", "tokens": ["Wenn", "ein", "ge\u00b7weyh\u00b7ter", "Tag", "mit", "an\u00b7zu\u00b7se\u00b7hen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer ists? Wem dienet sie, die ungeheure Menge?", "tokens": ["Wer", "ists", "?", "Wem", "die\u00b7net", "sie", ",", "die", "un\u00b7ge\u00b7heu\u00b7re", "Men\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "PWS", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie dient: Sie nennet sich nach einem: ", "tokens": ["Sie", "dient", ":", "Sie", "nen\u00b7net", "sich", "nach", "ei\u00b7nem", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nach JEsu? Das ist der, von dem auch Feinde sagten:", "tokens": ["Nach", "Je\u00b7su", "?", "Das", "ist", "der", ",", "von", "dem", "auch", "Fein\u00b7de", "sag\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "PDS", "VAFIN", "ART", "$,", "APPR", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Er sey ein freundlicher, ein angenehmer Mann;", "tokens": ["Er", "sey", "ein", "freund\u00b7li\u00b7cher", ",", "ein", "an\u00b7ge\u00b7neh\u00b7mer", "Mann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Er thu den B\u00f6sen guts, er helffe den Geplagten,", "tokens": ["Er", "thu", "den", "B\u00f6\u00b7sen", "guts", ",", "er", "helf\u00b7fe", "den", "Ge\u00b7plag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und niemand sehe ihm was ungeschicktes an.", "tokens": ["Und", "nie\u00b7mand", "se\u00b7he", "ihm", "was", "un\u00b7ge\u00b7schick\u00b7tes", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PIS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer aber seyn denn die, so sich nach JEsu heissen?", "tokens": ["Wer", "a\u00b7ber", "seyn", "denn", "die", ",", "so", "sich", "nach", "Je\u00b7su", "heis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAINF", "KON", "ART", "$,", "ADV", "PRF", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der eine liegt und schl\u00e4ft, der andre fri\u00dft und s\u00e4uft;", "tokens": ["Der", "ei\u00b7ne", "liegt", "und", "schl\u00e4ft", ",", "der", "and\u00b7re", "fri\u00dft", "und", "s\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der dritte m\u00f6chte sich mit allen Menschen schmeissen;", "tokens": ["Der", "drit\u00b7te", "m\u00f6ch\u00b7te", "sich", "mit", "al\u00b7len", "Men\u00b7schen", "schmeis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Indem der vierdte Geld und Gut zusammen h\u00e4uft.", "tokens": ["In\u00b7dem", "der", "vierd\u00b7te", "Geld", "und", "Gut", "zu\u00b7sam\u00b7men", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Hier seh\u2019 ich Gleichnisse von so viel fremden Thieren;", "tokens": ["Hier", "seh'", "ich", "Gleich\u00b7nis\u00b7se", "von", "so", "viel", "frem\u00b7den", "Thie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Hier d\u00e4cht ich nimmermehr ein rechtes Menschen-Bild", "tokens": ["Hier", "d\u00e4cht", "ich", "nim\u00b7mer\u00b7mehr", "ein", "rech\u00b7tes", "Men\u00b7schen\u00b7Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Geschweige JEsu Sinn und Wesen aus zu sp\u00fcren,", "tokens": ["Ge\u00b7schwei\u00b7ge", "Je\u00b7su", "Sinn", "und", "We\u00b7sen", "aus", "zu", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "KON", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "N\u00e4chst diesen ist die Welt mit Narren angef\u00fcllt.", "tokens": ["N\u00e4chst", "die\u00b7sen", "ist", "die", "Welt", "mit", "Nar\u00b7ren", "an\u00b7ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie geht es immer zu? Kommt etwa JEsu Wandel", "tokens": ["Wie", "geht", "es", "im\u00b7mer", "zu", "?", "Kommt", "et\u00b7wa", "Je\u00b7su", "Wan\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "VVFIN", "ADV", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den armen Sterblichen zu unbegreiflich f\u00fcr?", "tokens": ["Den", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", "zu", "un\u00b7be\u00b7greif\u00b7lich", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKA", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gewi\u00df, wo dieses nicht ein ungereimter Handel,", "tokens": ["Ge\u00b7wi\u00df", ",", "wo", "die\u00b7ses", "nicht", "ein", "un\u00b7ge\u00b7reim\u00b7ter", "Han\u00b7del", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PDS", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df JEsus immer spricht: ", "tokens": ["Da\u00df", "Je\u00b7sus", "im\u00b7mer", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Und hat es unser HErr nicht allzu hoch getrieben;", "tokens": ["Und", "hat", "es", "un\u00b7ser", "Herr", "nicht", "all\u00b7zu", "hoch", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Man solte in der That auch andre Christen sehn.", "tokens": ["Man", "sol\u00b7te", "in", "der", "That", "auch", "and\u00b7re", "Chris\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Drum ist bey mir der Schlu\u00df nach allen \u00fcbrig blieben:", "tokens": ["Drum", "ist", "bey", "mir", "der", "Schlu\u00df", "nach", "al\u00b7len", "\u00fcb\u00b7rig", "blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "PPER", "ART", "NN", "APPR", "PIAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dem JEsu Schritt vor Schritt in Schwachheit nach zugehn.", "tokens": ["Dem", "Je\u00b7su", "Schritt", "vor", "Schritt", "in", "Schwach\u00b7heit", "nach", "zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "APPR", "NN", "APPR", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wird sich nun dermaleins vor seinem Richtstuhl zeigen,", "tokens": ["Wird", "sich", "nun", "der\u00b7ma\u00b7leins", "vor", "sei\u00b7nem", "Richt\u00b7stuhl", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Da\u00df ich die S\u00e4yte so, wie er, zu hoch gespannt:", "tokens": ["Da\u00df", "ich", "die", "S\u00e4y\u00b7te", "so", ",", "wie", "er", ",", "zu", "hoch", "ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "PWAV", "PPER", "$,", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So kan ich m