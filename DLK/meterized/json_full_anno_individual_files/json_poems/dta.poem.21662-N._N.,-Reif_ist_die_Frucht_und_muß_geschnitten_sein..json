{"dta.poem.21662": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Reif ist die Frucht und mu\u00df geschnitten sein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Gewitterschwanger dr\u00e4ut es Tag und Nacht,", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7schwan\u00b7ger", "dr\u00e4ut", "es", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch f\u00e4llt kein Blitz, kein starker Donner kracht.", "tokens": ["Doch", "f\u00e4llt", "kein", "Blitz", ",", "kein", "star\u00b7ker", "Don\u00b7ner", "kracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zuweilen flammt am Horizont ein Schein,", "tokens": ["Zu\u00b7wei\u00b7len", "flammt", "am", "Ho\u00b7ri\u00b7zont", "ein", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dann folgt ein schwaches Grollen hinterdrein.", "tokens": ["Dann", "folgt", "ein", "schwa\u00b7ches", "Grol\u00b7len", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Todm\u00fcde r\u00f6chelnd ringt die Well\u2019 nach Luft,", "tokens": ["Tod\u00b7m\u00fc\u00b7de", "r\u00f6\u00b7chelnd", "ringt", "die", "Well'", "nach", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Als schmachte sie in dumpfer Leichengruft.", "tokens": ["Als", "schmach\u00b7te", "sie", "in", "dum\u00b7pfer", "Lei\u00b7chen\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O brich herein mit Donnersturmgetos,", "tokens": ["O", "brich", "her\u00b7ein", "mit", "Don\u00b7ner\u00b7sturm\u00b7ge\u00b7tos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df deiner schwarzen Rosse Z\u00fcgel los,", "tokens": ["La\u00df", "dei\u00b7ner", "schwar\u00b7zen", "Ros\u00b7se", "Z\u00fc\u00b7gel", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Sturmj\u00e4ger, auf, wir alle harren dein,", "tokens": ["Sturm\u00b7j\u00e4\u00b7ger", ",", "auf", ",", "wir", "al\u00b7le", "har\u00b7ren", "dein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PPER", "PIS", "VVFIN", "PPOSAT", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Nicht l\u00e4nger kann die Qual ertragen sein.", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "kann", "die", "Qual", "er\u00b7tra\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Siehst du die bangen Haufen murrend stehn?", "tokens": ["Siehst", "du", "die", "ban\u00b7gen", "Hau\u00b7fen", "mur\u00b7rend", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die Zeit ist hoch, was sein mu\u00df, ", "tokens": ["Die", "Zeit", "ist", "hoch", ",", "was", "sein", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "VAINF", "VMFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Und flammen tausend D\u00e4cher auf in Rauch,", "tokens": ["Und", "flam\u00b7men", "tau\u00b7send", "D\u00e4\u00b7cher", "auf", "in", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und bricht zusammen uralt heil\u2019ger Brauch,", "tokens": ["Und", "bricht", "zu\u00b7sam\u00b7men", "ur\u00b7alt", "heil'\u00b7ger", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und giebt\u2019s ein Jammern, da\u00df die Luft zerbirst,", "tokens": ["Und", "giebt's", "ein", "Jam\u00b7mern", ",", "da\u00df", "die", "Luft", "zer\u00b7birst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "La\u00df dich nicht m\u00e4\u00dfigen, Gewitterf\u00fcrst!", "tokens": ["La\u00df", "dich", "nicht", "m\u00e4\u00b7\u00dfi\u00b7gen", ",", "Ge\u00b7wit\u00b7ter\u00b7f\u00fcrst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Donner auf Donner, rother Strahl auf Strahl,", "tokens": ["Don\u00b7ner", "auf", "Don\u00b7ner", ",", "ro\u00b7ther", "Strahl", "auf", "Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.18": {"text": "Rein mu\u00df es werden von Gebirg\u2019 zu Thal,", "tokens": ["Rein", "mu\u00df", "es", "wer\u00b7den", "von", "Ge\u00b7bir\u00b7g'", "zu", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "In Schauern birgt ein gl\u00fcckliches Geschlecht,", "tokens": ["In", "Schau\u00b7ern", "birgt", "ein", "gl\u00fcck\u00b7li\u00b7ches", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Was m\u00fchvoll wir ges\u00e4et Knecht an Knecht.", "tokens": ["Was", "m\u00fch\u00b7voll", "wir", "ge\u00b7s\u00e4et", "Knecht", "an", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVPP", "NN", "APPR", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.21": {"text": "Was gelten ", "tokens": ["Was", "gel\u00b7ten"], "token_info": ["word", "word"], "pos": ["PWS", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Reif ist die Frucht und mu\u00df geschnitten sein.", "tokens": ["Reif", "ist", "die", "Frucht", "und", "mu\u00df", "ge\u00b7schnit\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}