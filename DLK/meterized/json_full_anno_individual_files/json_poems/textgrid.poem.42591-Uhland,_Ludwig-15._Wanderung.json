{"textgrid.poem.42591": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "15. Wanderung", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich nahm den Stab zu wandern,", "tokens": ["Ich", "nahm", "den", "Stab", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Deutschland ging die Fahrt,", "tokens": ["Durch", "Deutschland", "ging", "die", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Man pries mir ja vor andern", "tokens": ["Man", "pries", "mir", "ja", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Deutschen Sinn und Art.", "tokens": ["Der", "Deut\u00b7schen", "Sinn", "und", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Lande blieb ich ferne,", "tokens": ["Dem", "Lan\u00b7de", "blieb", "ich", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo die Orangen gl\u00fchn;", "tokens": ["Wo", "die", "O\u00b7ran\u00b7gen", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erst kennt ich jenes gerne,", "tokens": ["Erst", "kennt", "ich", "je\u00b7nes", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo die Kartoffeln bl\u00fchn.", "tokens": ["Wo", "die", "Kar\u00b7tof\u00b7feln", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam zum F\u00fcrstenhofe,", "tokens": ["Ich", "kam", "zum", "F\u00fcrs\u00b7ten\u00b7ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo man die K\u00fcnste kr\u00e4nzt,", "tokens": ["Wo", "man", "die", "K\u00fcns\u00b7te", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Prunksaal und Alkove", "tokens": ["Wo", "Prunk\u00b7saal", "und", "Al\u00b7ko\u00b7ve"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von G\u00f6tterbildern gl\u00e4nzt.", "tokens": ["Von", "G\u00f6t\u00b7ter\u00b7bil\u00b7dern", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Baum, der nicht im groben", "tokens": ["Ein", "Baum", ",", "der", "nicht", "im", "gro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Volksboden sich gen\u00e4hrt,", "tokens": ["Volks\u00b7bo\u00b7den", "sich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nein einer, der nach oben", "tokens": ["Nein", "ei\u00b7ner", ",", "der", "nach", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PIS", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sogar die Wurzeln kehrt!", "tokens": ["So\u00b7gar", "die", "Wur\u00b7zeln", "kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich ging zur Hohenschule,", "tokens": ["Ich", "ging", "zur", "Ho\u00b7hen\u00b7schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sch\u00f6pft ich reines Licht,", "tokens": ["Da", "sch\u00f6pft", "ich", "rei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo vom Prophetenstuhle", "tokens": ["Wo", "vom", "Pro\u00b7phe\u00b7ten\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wahre Freiheit spricht;", "tokens": ["Die", "wah\u00b7re", "Frei\u00b7heit", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo uns der Meister t\u00e4glich", "tokens": ["Wo", "uns", "der", "Meis\u00b7ter", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den innern Sinn befreit,", "tokens": ["Den", "in\u00b7nern", "Sinn", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Indes ihm selbst ertr\u00e4glich", "tokens": ["In\u00b7des", "ihm", "selbst", "er\u00b7tr\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ird'sche Leib gedeiht.", "tokens": ["Der", "ird'\u00b7sche", "Leib", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich schritt zum S\u00e4ngerwalde,", "tokens": ["Ich", "schritt", "zum", "S\u00e4n\u00b7ger\u00b7wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sucht ich Lebenshauch;", "tokens": ["Da", "sucht", "ich", "Le\u00b7bens\u00b7hauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00df ein edler Skalde", "tokens": ["Da", "sa\u00df", "ein", "ed\u00b7ler", "Skal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und pfl\u00fcckt' am Lorbeerstrauch;", "tokens": ["Und", "pfl\u00fcckt'", "am", "Lor\u00b7beer\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht hatt er Zeit, zu achten", "tokens": ["Nicht", "hatt", "er", "Zeit", ",", "zu", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf eines Volkes Schmerz,", "tokens": ["Auf", "ei\u00b7nes", "Vol\u00b7kes", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er konnte nur betrachten", "tokens": ["Er", "konn\u00b7te", "nur", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich ging zur Tempelhalle,", "tokens": ["Ich", "ging", "zur", "Tem\u00b7pel\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da h\u00f6rt ich christlich Recht:", "tokens": ["Da", "h\u00f6rt", "ich", "christ\u00b7lich", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier innen Br\u00fcder alle,", "tokens": ["Hier", "in\u00b7nen", "Br\u00fc\u00b7der", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da drau\u00dfen Herr und Knecht!", "tokens": ["Da", "drau\u00b7\u00dfen", "Herr", "und", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Festesrede Giebel", "tokens": ["Der", "Fes\u00b7tes\u00b7re\u00b7de", "Gie\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War: duck dich! schweig dabei?", "tokens": ["War", ":", "duck", "dich", "!", "schweig", "da\u00b7bei", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$.", "VVIMP", "PPER", "$.", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als ob die ganze Bibel", "tokens": ["Als", "ob", "die", "gan\u00b7ze", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Buch der K\u00f6n'ge sei.", "tokens": ["Ein", "Buch", "der", "K\u00f6n'\u00b7ge", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich kam zum B\u00fcrgerhause,", "tokens": ["Ich", "kam", "zum", "B\u00fcr\u00b7ger\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gern denk ich dran zur\u00fcck,", "tokens": ["Gern", "denk", "ich", "dran", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fern vom Parteigebrause", "tokens": ["Fern", "vom", "Par\u00b7tei\u00b7ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bl\u00fcht Tugend hier und Gl\u00fcck.", "tokens": ["Bl\u00fcht", "Tu\u00b7gend", "hier", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lebt h\u00e4uslich fort wie heute!", "tokens": ["Lebt", "h\u00e4us\u00b7lich", "fort", "wie", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KOKOM", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald wird vom Belt zum Rhein", "tokens": ["Bald", "wird", "vom", "Belt", "zum", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPRART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja! ", "tokens": ["Ja", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Ich ging zum Hospitale,", "tokens": ["Ich", "ging", "zum", "Hos\u00b7pi\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da fand ich alles nett,", "tokens": ["Da", "fand", "ich", "al\u00b7les", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Gr\u00fctz und Kraut zum Mahle", "tokens": ["Viel", "Gr\u00fctz", "und", "Kraut", "zum", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und reinlich Krankenbett;", "tokens": ["Und", "rein\u00b7lich", "Kran\u00b7ken\u00b7bett", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch sorgt ein sch\u00f6n Erbarmen", "tokens": ["Auch", "sorgt", "ein", "sch\u00f6n", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr manch verwahrlost Kind.", "tokens": ["F\u00fcr", "manch", "ver\u00b7wahr\u00b7lost", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer denkt des Volks von Armen,", "tokens": ["Wer", "denkt", "des", "Volks", "von", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die altverwahrlost sind?", "tokens": ["Die", "alt\u00b7ver\u00b7wahr\u00b7lost", "sind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich sa\u00df im St\u00e4ndesaale,", "tokens": ["Ich", "sa\u00df", "im", "St\u00e4n\u00b7de\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schlief ich ein und tr\u00e4umt,", "tokens": ["Da", "schlief", "ich", "ein", "und", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sei noch im Spitale,", "tokens": ["Ich", "sei", "noch", "im", "Spi\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ich doch l\u00e4ngst ger\u00e4umt.", "tokens": ["Den", "ich", "doch", "l\u00e4ngst", "ge\u00b7r\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Mann, der dort im Fieber,", "tokens": ["Ein", "Mann", ",", "der", "dort", "im", "Fie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im kalten Fieber lag,", "tokens": ["Im", "kal\u00b7ten", "Fie\u00b7ber", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er rief: nur nichts, mein Lieber,", "tokens": ["Er", "rief", ":", "nur", "nichts", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PIS", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur nichts vom Bundestag!", "tokens": ["Nur", "nichts", "vom", "Bun\u00b7des\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich mischte mich zum Volke,", "tokens": ["Ich", "mischte", "mich", "zum", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das nach dem Festplatz zog,", "tokens": ["Das", "nach", "dem", "Fest\u00b7platz", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo durch die Staubeswolke", "tokens": ["Wo", "durch", "die", "Stau\u00b7bes\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch d\u00fcrrer Renner flog;", "tokens": ["Manch", "d\u00fcr\u00b7rer", "Ren\u00b7ner", "flog", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da lernt es, da\u00df die Eile", "tokens": ["Da", "lernt", "es", ",", "da\u00df", "die", "Ei\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Reiter \u00fcberst\u00fcrzt", "tokens": ["Den", "Rei\u00b7ter", "\u00fc\u00b7bers\u00b7t\u00fcrzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und da\u00df man gut die Weile", "tokens": ["Und", "da\u00df", "man", "gut", "die", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Wurst und Bier sich k\u00fcrzt.", "tokens": ["Mit", "Wurst", "und", "Bier", "sich", "k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ein Adler, fl\u00fcgelstrebend,", "tokens": ["Ein", "Ad\u00b7ler", ",", "fl\u00fc\u00b7gel\u00b7stre\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War Reichspanier hievor,", "tokens": ["War", "Reich\u00b7spa\u00b7nier", "hie\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah ihn noch, wie lebend,", "tokens": ["Ich", "sah", "ihn", "noch", ",", "wie", "le\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu N\u00fcrnberg an dem Tor.", "tokens": ["Zu", "N\u00fcrn\u00b7berg", "an", "dem", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt fliegt man nicht zum Zwecke,", "tokens": ["Jetzt", "fliegt", "man", "nicht", "zum", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Wahlspruch ist: Gott geb's!", "tokens": ["Der", "Wahl\u00b7spruch", "ist", ":", "Gott", "ge\u00b7b's", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das Wappen ist die Schnecke,", "tokens": ["Das", "Wap\u00b7pen", "ist", "die", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schildhalter ist der Krebs.", "tokens": ["Schild\u00b7hal\u00b7ter", "ist", "der", "Krebs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Als ich mir ", "tokens": ["Als", "ich", "mir"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Kehrt ich den Stab nach Haus;", "tokens": ["Kehrt", "ich", "den", "Stab", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann einst das Heil gekommen,", "tokens": ["Wann", "einst", "das", "Heil", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann reis ich wieder aus:", "tokens": ["Dann", "reis", "ich", "wie\u00b7der", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl werd ich's nicht erleben,", "tokens": ["Wohl", "werd", "ich's", "nicht", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch an der Sehnsucht Hand", "tokens": ["Doch", "an", "der", "Sehn\u00b7sucht", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als Schatten noch durchschweben", "tokens": ["Als", "Schat\u00b7ten", "noch", "durch\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein freies Vaterland.", "tokens": ["Mein", "frei\u00b7es", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich nahm den Stab zu wandern,", "tokens": ["Ich", "nahm", "den", "Stab", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Deutschland ging die Fahrt,", "tokens": ["Durch", "Deutschland", "ging", "die", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Man pries mir ja vor andern", "tokens": ["Man", "pries", "mir", "ja", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Deutschen Sinn und Art.", "tokens": ["Der", "Deut\u00b7schen", "Sinn", "und", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Lande blieb ich ferne,", "tokens": ["Dem", "Lan\u00b7de", "blieb", "ich", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo die Orangen gl\u00fchn;", "tokens": ["Wo", "die", "O\u00b7ran\u00b7gen", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erst kennt ich jenes gerne,", "tokens": ["Erst", "kennt", "ich", "je\u00b7nes", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo die Kartoffeln bl\u00fchn.", "tokens": ["Wo", "die", "Kar\u00b7tof\u00b7feln", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich kam zum F\u00fcrstenhofe,", "tokens": ["Ich", "kam", "zum", "F\u00fcrs\u00b7ten\u00b7ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo man die K\u00fcnste kr\u00e4nzt,", "tokens": ["Wo", "man", "die", "K\u00fcns\u00b7te", "kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Prunksaal und Alkove", "tokens": ["Wo", "Prunk\u00b7saal", "und", "Al\u00b7ko\u00b7ve"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von G\u00f6tterbildern gl\u00e4nzt.", "tokens": ["Von", "G\u00f6t\u00b7ter\u00b7bil\u00b7dern", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Baum, der nicht im groben", "tokens": ["Ein", "Baum", ",", "der", "nicht", "im", "gro\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Volksboden sich gen\u00e4hrt,", "tokens": ["Volks\u00b7bo\u00b7den", "sich", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nein einer, der nach oben", "tokens": ["Nein", "ei\u00b7ner", ",", "der", "nach", "o\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PIS", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sogar die Wurzeln kehrt!", "tokens": ["So\u00b7gar", "die", "Wur\u00b7zeln", "kehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ich ging zur Hohenschule,", "tokens": ["Ich", "ging", "zur", "Ho\u00b7hen\u00b7schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sch\u00f6pft ich reines Licht,", "tokens": ["Da", "sch\u00f6pft", "ich", "rei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo vom Prophetenstuhle", "tokens": ["Wo", "vom", "Pro\u00b7phe\u00b7ten\u00b7stuh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die wahre Freiheit spricht;", "tokens": ["Die", "wah\u00b7re", "Frei\u00b7heit", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo uns der Meister t\u00e4glich", "tokens": ["Wo", "uns", "der", "Meis\u00b7ter", "t\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den innern Sinn befreit,", "tokens": ["Den", "in\u00b7nern", "Sinn", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Indes ihm selbst ertr\u00e4glich", "tokens": ["In\u00b7des", "ihm", "selbst", "er\u00b7tr\u00e4g\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der ird'sche Leib gedeiht.", "tokens": ["Der", "ird'\u00b7sche", "Leib", "ge\u00b7deiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ich schritt zum S\u00e4ngerwalde,", "tokens": ["Ich", "schritt", "zum", "S\u00e4n\u00b7ger\u00b7wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sucht ich Lebenshauch;", "tokens": ["Da", "sucht", "ich", "Le\u00b7bens\u00b7hauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sa\u00df ein edler Skalde", "tokens": ["Da", "sa\u00df", "ein", "ed\u00b7ler", "Skal\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und pfl\u00fcckt' am Lorbeerstrauch;", "tokens": ["Und", "pfl\u00fcckt'", "am", "Lor\u00b7beer\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht hatt er Zeit, zu achten", "tokens": ["Nicht", "hatt", "er", "Zeit", ",", "zu", "ach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf eines Volkes Schmerz,", "tokens": ["Auf", "ei\u00b7nes", "Vol\u00b7kes", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er konnte nur betrachten", "tokens": ["Er", "konn\u00b7te", "nur", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich ging zur Tempelhalle,", "tokens": ["Ich", "ging", "zur", "Tem\u00b7pel\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da h\u00f6rt ich christlich Recht:", "tokens": ["Da", "h\u00f6rt", "ich", "christ\u00b7lich", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier innen Br\u00fcder alle,", "tokens": ["Hier", "in\u00b7nen", "Br\u00fc\u00b7der", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da drau\u00dfen Herr und Knecht!", "tokens": ["Da", "drau\u00b7\u00dfen", "Herr", "und", "Knecht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Festesrede Giebel", "tokens": ["Der", "Fes\u00b7tes\u00b7re\u00b7de", "Gie\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War: duck dich! schweig dabei?", "tokens": ["War", ":", "duck", "dich", "!", "schweig", "da\u00b7bei", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$.", "VVIMP", "PPER", "$.", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als ob die ganze Bibel", "tokens": ["Als", "ob", "die", "gan\u00b7ze", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Buch der K\u00f6n'ge sei.", "tokens": ["Ein", "Buch", "der", "K\u00f6n'\u00b7ge", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Ich kam zum B\u00fcrgerhause,", "tokens": ["Ich", "kam", "zum", "B\u00fcr\u00b7ger\u00b7hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gern denk ich dran zur\u00fcck,", "tokens": ["Gern", "denk", "ich", "dran", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fern vom Parteigebrause", "tokens": ["Fern", "vom", "Par\u00b7tei\u00b7ge\u00b7brau\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bl\u00fcht Tugend hier und Gl\u00fcck.", "tokens": ["Bl\u00fcht", "Tu\u00b7gend", "hier", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Lebt h\u00e4uslich fort wie heute!", "tokens": ["Lebt", "h\u00e4us\u00b7lich", "fort", "wie", "heu\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KOKOM", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald wird vom Belt zum Rhein", "tokens": ["Bald", "wird", "vom", "Belt", "zum", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPRART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja! ", "tokens": ["Ja", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.18": {"line.1": {"text": "Ich ging zum Hospitale,", "tokens": ["Ich", "ging", "zum", "Hos\u00b7pi\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da fand ich alles nett,", "tokens": ["Da", "fand", "ich", "al\u00b7les", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Gr\u00fctz und Kraut zum Mahle", "tokens": ["Viel", "Gr\u00fctz", "und", "Kraut", "zum", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und reinlich Krankenbett;", "tokens": ["Und", "rein\u00b7lich", "Kran\u00b7ken\u00b7bett", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch sorgt ein sch\u00f6n Erbarmen", "tokens": ["Auch", "sorgt", "ein", "sch\u00f6n", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr manch verwahrlost Kind.", "tokens": ["F\u00fcr", "manch", "ver\u00b7wahr\u00b7lost", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer denkt des Volks von Armen,", "tokens": ["Wer", "denkt", "des", "Volks", "von", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die altverwahrlost sind?", "tokens": ["Die", "alt\u00b7ver\u00b7wahr\u00b7lost", "sind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Ich sa\u00df im St\u00e4ndesaale,", "tokens": ["Ich", "sa\u00df", "im", "St\u00e4n\u00b7de\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schlief ich ein und tr\u00e4umt,", "tokens": ["Da", "schlief", "ich", "ein", "und", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sei noch im Spitale,", "tokens": ["Ich", "sei", "noch", "im", "Spi\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ich doch l\u00e4ngst ger\u00e4umt.", "tokens": ["Den", "ich", "doch", "l\u00e4ngst", "ge\u00b7r\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Mann, der dort im Fieber,", "tokens": ["Ein", "Mann", ",", "der", "dort", "im", "Fie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im kalten Fieber lag,", "tokens": ["Im", "kal\u00b7ten", "Fie\u00b7ber", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er rief: nur nichts, mein Lieber,", "tokens": ["Er", "rief", ":", "nur", "nichts", ",", "mein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PIS", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nur nichts vom Bundestag!", "tokens": ["Nur", "nichts", "vom", "Bun\u00b7des\u00b7tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Ich mischte mich zum Volke,", "tokens": ["Ich", "mischte", "mich", "zum", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das nach dem Festplatz zog,", "tokens": ["Das", "nach", "dem", "Fest\u00b7platz", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo durch die Staubeswolke", "tokens": ["Wo", "durch", "die", "Stau\u00b7bes\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Manch d\u00fcrrer Renner flog;", "tokens": ["Manch", "d\u00fcr\u00b7rer", "Ren\u00b7ner", "flog", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da lernt es, da\u00df die Eile", "tokens": ["Da", "lernt", "es", ",", "da\u00df", "die", "Ei\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Reiter \u00fcberst\u00fcrzt", "tokens": ["Den", "Rei\u00b7ter", "\u00fc\u00b7bers\u00b7t\u00fcrzt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und da\u00df man gut die Weile", "tokens": ["Und", "da\u00df", "man", "gut", "die", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Wurst und Bier sich k\u00fcrzt.", "tokens": ["Mit", "Wurst", "und", "Bier", "sich", "k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Ein Adler, fl\u00fcgelstrebend,", "tokens": ["Ein", "Ad\u00b7ler", ",", "fl\u00fc\u00b7gel\u00b7stre\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War Reichspanier hievor,", "tokens": ["War", "Reich\u00b7spa\u00b7nier", "hie\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah ihn noch, wie lebend,", "tokens": ["Ich", "sah", "ihn", "noch", ",", "wie", "le\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu N\u00fcrnberg an dem Tor.", "tokens": ["Zu", "N\u00fcrn\u00b7berg", "an", "dem", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt fliegt man nicht zum Zwecke,", "tokens": ["Jetzt", "fliegt", "man", "nicht", "zum", "Zwe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Wahlspruch ist: Gott geb's!", "tokens": ["Der", "Wahl\u00b7spruch", "ist", ":", "Gott", "ge\u00b7b's", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das Wappen ist die Schnecke,", "tokens": ["Das", "Wap\u00b7pen", "ist", "die", "Schne\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schildhalter ist der Krebs.", "tokens": ["Schild\u00b7hal\u00b7ter", "ist", "der", "Krebs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Als ich mir ", "tokens": ["Als", "ich", "mir"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Kehrt ich den Stab nach Haus;", "tokens": ["Kehrt", "ich", "den", "Stab", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann einst das Heil gekommen,", "tokens": ["Wann", "einst", "das", "Heil", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann reis ich wieder aus:", "tokens": ["Dann", "reis", "ich", "wie\u00b7der", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl werd ich's nicht erleben,", "tokens": ["Wohl", "werd", "ich's", "nicht", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch an der Sehnsucht Hand", "tokens": ["Doch", "an", "der", "Sehn\u00b7sucht", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als Schatten noch durchschweben", "tokens": ["Als", "Schat\u00b7ten", "noch", "durch\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein freies Vaterland.", "tokens": ["Mein", "frei\u00b7es", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}