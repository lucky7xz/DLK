{"textgrid.poem.62544": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Mittwoch Morgens", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist, gib dich zufrieden,", "tokens": ["Mein", "Geist", ",", "gib", "dich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was dir dein Gott beschieden,", "tokens": ["Was", "dir", "dein", "Gott", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sei dir recht und gleich;", "tokens": ["Das", "sei", "dir", "recht", "und", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er wird auch diesen Morgen", "tokens": ["Er", "wird", "auch", "die\u00b7sen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich v\u00e4terlich versorgen,", "tokens": ["Dich", "v\u00e4\u00b7ter\u00b7lich", "ver\u00b7sor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So wie sein ganzes Reich.", "tokens": ["So", "wie", "sein", "gan\u00b7zes", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df, Herr, von deinen Gaben", "tokens": ["La\u00df", ",", "Herr", ",", "von", "dei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich heute so viel haben,", "tokens": ["Mich", "heu\u00b7te", "so", "viel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ich bedarf zur Noth;", "tokens": ["Als", "ich", "be\u00b7darf", "zur", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df meinen Flei\u00df gelingen,", "tokens": ["La\u00df", "mei\u00b7nen", "Flei\u00df", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und kann ich's nicht erschwingen,", "tokens": ["Und", "kann", "ich's", "nicht", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gib mir mein t\u00e4glich Brod.", "tokens": ["Gib", "mir", "mein", "t\u00e4g\u00b7lich", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Geist, gib dich zufrieden,", "tokens": ["Mein", "Geist", ",", "gib", "dich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "