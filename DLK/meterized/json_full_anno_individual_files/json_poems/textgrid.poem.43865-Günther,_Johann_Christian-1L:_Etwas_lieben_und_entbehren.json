"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dencke, wie es martern m\u00fc\u00dfe,", "tokens": ["Den\u00b7cke", ",", "wie", "es", "mar\u00b7tern", "m\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ein m\u00fcder Pilgersmann", "tokens": ["Wenn", "ein", "m\u00fc\u00b7der", "Pil\u00b7gers\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Ufer tiefer Fl\u00fc\u00dfe", "tokens": ["Von", "dem", "U\u00b7fer", "tie\u00b7fer", "Fl\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Trunck erreichen kan", "tokens": ["Kei\u00b7nen", "Trunck", "er\u00b7rei\u00b7chen", "kan"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Sehnsucht und Verdru\u00df", "tokens": ["Und", "mit", "Sehn\u00b7sucht", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wasser sehn und dursten mu\u00df.", "tokens": ["Was\u00b7ser", "sehn", "und", "durs\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deiner Sch\u00f6nheit reife Fr\u00fcchte", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "rei\u00b7fe", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Martern mich ja auch zu scharf,", "tokens": ["Mar\u00b7tern", "mich", "ja", "auch", "zu", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie sind nur Schaugerichte,", "tokens": ["Denn", "sie", "sind", "nur", "Schau\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mein Mund nicht kosten darf.", "tokens": ["Die", "mein", "Mund", "nicht", "kos\u00b7ten", "darf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O betr\u00fcbter Appetit,", "tokens": ["O", "be\u00b7tr\u00fcb\u00b7ter", "Ap\u00b7pe\u00b7tit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der verbothne Fr\u00fcchte sieht!", "tokens": ["Der", "ver\u00b7both\u00b7ne", "Fr\u00fcch\u00b7te", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schilt dein zorniges Empfinden", "tokens": ["Schilt", "dein", "zor\u00b7ni\u00b7ges", "Emp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein verwegen L\u00fcsternseyn,", "tokens": ["Mein", "ver\u00b7we\u00b7gen", "L\u00fcs\u00b7tern\u00b7seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So vergieb den sch\u00f6nen S\u00fcnden,", "tokens": ["So", "ver\u00b7gieb", "den", "sch\u00f6\u00b7nen", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie sind haupts\u00e4chlich dein,", "tokens": ["Denn", "sie", "sind", "haupt\u00b7s\u00e4ch\u00b7lich", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil du gar so reizend bist,", "tokens": ["Weil", "du", "gar", "so", "rei\u00b7zend", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man sich aus Lust vergi\u00dft.", "tokens": ["Da\u00df", "man", "sich", "aus", "Lust", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So ein feuerreich Gem\u00fcthe,", "tokens": ["So", "ein", "feu\u00b7er\u00b7reich", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die netten Glieder lenckt", "tokens": ["Das", "die", "net\u00b7ten", "Glie\u00b7der", "lenckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Und sowohl Verstand als G\u00fcte", "tokens": ["Und", "so\u00b7wohl", "Ver\u00b7stand", "als", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KON", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Blick' und K\u00fc\u00dfe mengt,", "tokens": ["Un\u00b7ter", "Blick'", "und", "K\u00fc\u00b7\u00dfe", "mengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solches, sag ich, l\u00e4st nicht zu,", "tokens": ["Sol\u00b7ches", ",", "sag", "ich", ",", "l\u00e4st", "nicht", "zu", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df man unempfindlich thu.", "tokens": ["Da\u00df", "man", "un\u00b7emp\u00b7find\u00b7lich", "thu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gleichwohl lern ich mich bescheiden", "tokens": ["Gleich\u00b7wohl", "lern", "ich", "mich", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und begn\u00fcge mich daran,", "tokens": ["Und", "be\u00b7gn\u00fc\u00b7ge", "mich", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dein Bild mein stummes Leiden", "tokens": ["Wenn", "dein", "Bild", "mein", "stum\u00b7mes", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur im Traume lindern kan", "tokens": ["Nur", "im", "Trau\u00b7me", "lin\u00b7dern", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich nachmahls auf den Tag", "tokens": ["Und", "ich", "nach\u00b7mahls", "auf", "den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir die Ehrfurcht zeigen mag.", "tokens": ["Dir", "die", "Ehr\u00b7furcht", "zei\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Etwas lieben und entbehren", "tokens": ["Et\u00b7was", "lie\u00b7ben", "und", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Schmerz, der heimlich qu\u00e4lt;", "tokens": ["Ist", "ein", "Schmerz", ",", "der", "heim\u00b7lich", "qu\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Blicke Zungen w\u00e4ren,", "tokens": ["Wenn", "die", "Bli\u00b7cke", "Zun\u00b7gen", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic