{"textgrid.poem.24809": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Wenn unter duft'gen Bl\u00fctenzweigen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn unter duft'gen Bl\u00fctenzweigen", "tokens": ["Wenn", "un\u00b7ter", "duft'\u00b7gen", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir ruhen, Haupt an Haupt gelehnt,", "tokens": ["Wir", "ru\u00b7hen", ",", "Haupt", "an", "Haupt", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie s\u00fc\u00df der K\u00fcsse Wechseltausch!", "tokens": ["Wie", "s\u00fc\u00df", "der", "K\u00fcs\u00b7se", "Wech\u00b7sel\u00b7tausch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welch Fl\u00fcstern in der Liebe Rausch!", "tokens": ["Welch", "Fl\u00fcs\u00b7tern", "in", "der", "Lie\u00b7be", "Rausch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie spricht, so oft die Lippen schweigen,", "tokens": ["Wie", "spricht", ",", "so", "oft", "die", "Lip\u00b7pen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Auge, das von Wonne thr\u00e4nt!", "tokens": ["Das", "Au\u00b7ge", ",", "das", "von", "Won\u00b7ne", "thr\u00e4nt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Locke hier mu\u00dft du mir lassen", "tokens": ["Die", "Lo\u00b7cke", "hier", "mu\u00dft", "du", "mir", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr jene Zeit, wo fern du bist,", "tokens": ["F\u00fcr", "je\u00b7ne", "Zeit", ",", "wo", "fern", "du", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df an dem Pfand sich mein Gedanke", "tokens": ["Da\u00df", "an", "dem", "Pfand", "sich", "mein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufrichte, wenn ich zweifelnd schwanke", "tokens": ["Auf\u00b7rich\u00b7te", ",", "wenn", "ich", "zwei\u00b7felnd", "schwan\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nicht mehr glauben kann, nicht fassen,", "tokens": ["Und", "nicht", "mehr", "glau\u00b7ben", "kann", ",", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,", "PTKNE