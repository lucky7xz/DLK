{"textgrid.poem.46561": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du gingest nicht, du flogest]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gingest nicht, du flogest,", "tokens": ["Du", "gin\u00b7gest", "nicht", ",", "du", "flo\u00b7gest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gefl\u00fcgelt war dein Tritt,", "tokens": ["Ge\u00b7fl\u00fc\u00b7gelt", "war", "dein", "Tritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du kein H\u00e4lmchen bogest;", "tokens": ["Da\u00df", "du", "kein", "H\u00e4lm\u00b7chen", "bo\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schwebtest du, und zogest", "tokens": ["So", "schweb\u00b7test", "du", ",", "und", "zo\u00b7gest"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bei jedem leichten Schritt", "tokens": ["Bei", "je\u00b7dem", "leich\u00b7ten", "Schritt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Mutter Blicke mit.", "tokens": ["Der", "Mut\u00b7ter", "Bli\u00b7cke", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "st