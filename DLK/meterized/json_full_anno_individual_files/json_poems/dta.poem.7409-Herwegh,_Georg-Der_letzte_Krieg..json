{"dta.poem.7409": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Krieg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seine H\u00e4nde falten kann,", "tokens": ["Wer", "sei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bet' um ein gutes Schwert,", "tokens": ["Bet'", "um", "ein", "gu\u00b7tes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Um einen Helden, einen Mann,", "tokens": ["Um", "ei\u00b7nen", "Hel\u00b7den", ",", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gottes Zorn bewehrt!", "tokens": ["Den", "Got\u00b7tes", "Zorn", "be\u00b7wehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und drin der sch\u00f6nste Sieg,", "tokens": ["Und", "drin", "der", "sch\u00f6ns\u00b7te", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der letzte Kampf auf Erden,", "tokens": ["Der", "letz\u00b7te", "Kampf", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der letzte heilige Krieg!", "tokens": ["Der", "letz\u00b7te", "hei\u00b7li\u00b7ge", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Herbei, herbei, ihr V\u00f6lker all,", "tokens": ["Her\u00b7bei", ",", "her\u00b7bei", ",", "ihr", "V\u00f6l\u00b7ker", "all", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um euer Schlachtpanier!", "tokens": ["Um", "eu\u00b7er", "Schlacht\u00b7pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Freiheit ist jetzt Feldmarschall,", "tokens": ["Die", "Frei\u00b7heit", "ist", "jetzt", "Feld\u00b7mar\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Vorw\u00e4rts heissen wir.", "tokens": ["Und", "Vor\u00b7w\u00e4rts", "heis\u00b7sen", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Zeiger weist die Stunde,", "tokens": ["Der", "Zei\u00b7ger", "weist", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O flieg', mein Polen, flieg',", "tokens": ["O", "flieg'", ",", "mein", "Po\u00b7len", ",", "flieg'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit jedem Stern im Bunde,", "tokens": ["Mit", "je\u00b7dem", "Stern", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Voran zum heiligen Krieg!", "tokens": ["Vo\u00b7ran", "zum", "hei\u00b7li\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ja! vorw\u00e4rts, bis der Morgen blinkt,", "tokens": ["Ja", "!", "vor\u00b7w\u00e4rts", ",", "bis", "der", "Mor\u00b7gen", "blinkt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja! vorw\u00e4rts, frisch und froh!", "tokens": ["Ja", "!", "vor\u00b7w\u00e4rts", ",", "frisch", "und", "froh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vorw\u00e4rts, bis hinter uns versinkt", "tokens": ["Vor\u00b7w\u00e4rts", ",", "bis", "hin\u00b7ter", "uns", "ver\u00b7sinkt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die Brut des Pharao!", "tokens": ["Die", "Brut", "des", "Pha\u00b7rao", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Er wird auch f\u00fcr uns sprechen,", "tokens": ["Er", "wird", "auch", "f\u00fcr", "uns", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Herr, der f\u00fcr uns schwieg,", "tokens": ["Der", "Herr", ",", "der", "f\u00fcr", "uns", "schwieg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und unsre Ketten brechen", "tokens": ["Und", "uns\u00b7re", "Ket\u00b7ten", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im letzten heiligen Krieg.", "tokens": ["Im", "letz\u00b7ten", "hei\u00b7li\u00b7gen", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "O walle hin, du Opferbrand,", "tokens": ["O", "wal\u00b7le", "hin", ",", "du", "Op\u00b7fer\u00b7brand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hin \u00fcber Land und Meer,", "tokens": ["Hin", "\u00fc\u00b7ber", "Land", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schling' ein einig Feuerband", "tokens": ["Und", "schling'", "ein", "ei\u00b7nig", "Feu\u00b7er\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um alle V\u00f6lker her;", "tokens": ["Um", "al\u00b7le", "V\u00f6l\u00b7ker", "her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wird er uns beschieden,", "tokens": ["So", "wird", "er", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gro\u00dfe, gro\u00dfe Sieg,", "tokens": ["Der", "gro\u00b7\u00dfe", ",", "gro\u00b7\u00dfe", "Sieg", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der ewige V\u00f6lker-Frieden, \u2014", "tokens": ["Der", "e\u00b7wi\u00b7ge", "V\u00f6l\u00b7ker\u00b7Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Frisch auf, zum heiligen Krieg!", "tokens": ["Frisch", "auf", ",", "zum", "hei\u00b7li\u00b7gen", "Krieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}