{"dta.poem.687": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Zusatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wil f\u00fcr dir/ mit dir/ durch dich/ in deinen wegen ", "tokens": ["Ich", "wil", "f\u00fcr", "dir", "/", "mit", "dir", "/", "durch", "dich", "/", "in", "dei\u00b7nen", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "$(", "APPR", "PPER", "$(", "APPR", "PPER", "$(", "APPR", "PPOSAT", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit vollen schritten gehn/", "tokens": ["Mit", "vol\u00b7len", "schrit\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wo kein sterben sich/ vnd keine seuchen regen", "tokens": ["Da", "wo", "kein", "ster\u00b7ben", "sich", "/", "vnd", "kei\u00b7ne", "seu\u00b7chen", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PIAT", "VVFIN", "PRF", "$(", "KON", "PIAT", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo keine gr\u00e4ber stehn/ ", "tokens": ["Wo", "kei\u00b7ne", "gr\u00e4\u00b7ber", "stehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo alles lebt/ wo alles singt vnd klingt ", "tokens": ["Wo", "al\u00b7les", "lebt", "/", "wo", "al\u00b7les", "singt", "vnd", "klingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "$(", "PWAV", "PIS", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vnd einen danck dir nach dem andern bringt;", "tokens": ["Vnd", "ei\u00b7nen", "danck", "dir", "nach", "dem", "an\u00b7dern", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo alle schmertzen frey/ dich der du selbst das leben/ ", "tokens": ["Wo", "al\u00b7le", "schmert\u00b7zen", "frey", "/", "dich", "der", "du", "selbst", "das", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADJD", "$(", "PPER", "ART", "PPER", "ADV", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit alzeit newem prei\u00df in ewigkeit erheben.", "tokens": ["Mit", "al\u00b7zeit", "ne\u00b7wem", "prei\u00df", "in", "e\u00b7wig\u00b7keit", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}