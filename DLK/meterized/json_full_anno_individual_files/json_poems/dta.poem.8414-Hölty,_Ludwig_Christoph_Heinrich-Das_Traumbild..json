{"dta.poem.8414": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Traumbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich im Garten tr\u00e4umte,", "tokens": ["Als", "ich", "im", "Gar\u00b7ten", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Haar den Rosmarin mir wand,", "tokens": ["Ins", "Haar", "den", "Ros\u00b7ma\u00b7rin", "mir", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der um mein Lager keimte?", "tokens": ["Der", "um", "mein", "La\u00b7ger", "ke\u00b7im\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo bist du, Bild, das vor mir stand,", "tokens": ["Wo", "bist", "du", ",", "Bild", ",", "das", "vor", "mir", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir in die Seele blickte,", "tokens": ["Mir", "in", "die", "See\u00b7le", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und eine warme M\u00e4dchenhand", "tokens": ["Und", "ei\u00b7ne", "war\u00b7me", "M\u00e4d\u00b7chen\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir an die Wangen dr\u00fcckte?", "tokens": ["Mir", "an", "die", "Wan\u00b7gen", "dr\u00fcck\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun such' ich dich, mit Harm erf\u00fcllt,", "tokens": ["Nun", "such'", "ich", "dich", ",", "mit", "Harm", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald bei des Dorfes Linden,", "tokens": ["Bald", "bei", "des", "Dor\u00b7fes", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Bald in der Stadt, geliebtes Bild,", "tokens": ["Bald", "in", "der", "Stadt", ",", "ge\u00b7lieb\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann dich nirgends finden.", "tokens": ["Und", "kann", "dich", "nir\u00b7gends", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach jedem Fenster blick' ich hin,", "tokens": ["Nach", "je\u00b7dem", "Fens\u00b7ter", "blick'", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo nur ein Schleier wehet,", "tokens": ["Wo", "nur", "ein", "Schlei\u00b7er", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und habe meine Lieblingin", "tokens": ["Und", "ha\u00b7be", "mei\u00b7ne", "Lieb\u00b7lin\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch nirgends ausgesp\u00e4het.", "tokens": ["Noch", "nir\u00b7gends", "aus\u00b7ge\u00b7sp\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm selber, s\u00fcsses Bild der Nacht,", "tokens": ["Komm", "sel\u00b7ber", ",", "s\u00fcs\u00b7ses", "Bild", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Komm mit den Engelminen,", "tokens": ["Komm", "mit", "den", "En\u00b7gel\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und in der leichten Sch\u00e4fertracht,", "tokens": ["Und", "in", "der", "leich\u00b7ten", "Sch\u00e4\u00b7fer\u00b7tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worin du mir erschienen!", "tokens": ["Wo\u00b7rin", "du", "mir", "er\u00b7schie\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bring mit die schwanenweisse Hand,", "tokens": ["Bring", "mit", "die", "schwa\u00b7nen\u00b7weis\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die mir das Herz gestolen,", "tokens": ["Die", "mir", "das", "Herz", "ge\u00b7sto\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das purpurrothe Busenband,", "tokens": ["Das", "pur\u00b7pur\u00b7ro\u00b7the", "Bu\u00b7sen\u00b7band", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Str\u00e4usschen von Violen;", "tokens": ["Das", "Str\u00e4us\u00b7schen", "von", "Vi\u00b7o\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dein grosses blaues Augenpaar,", "tokens": ["Dein", "gros\u00b7ses", "blau\u00b7es", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woraus ein Engel blickte;", "tokens": ["Wo\u00b7raus", "ein", "En\u00b7gel", "blick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Stirne, die so freundlich war,", "tokens": ["Die", "Stir\u00b7ne", ",", "die", "so", "freund\u00b7lich", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und guten Abend nickte;", "tokens": ["Und", "gu\u00b7ten", "A\u00b7bend", "nick\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Mund, der Liebe Paradies,", "tokens": ["Den", "Mund", ",", "der", "Lie\u00b7be", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die kleinen Wangengr\u00fcbchen,", "tokens": ["Die", "klei\u00b7nen", "Wan\u00b7gen\u00b7gr\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo sich der Himmel offen wies,", "tokens": ["Wo", "sich", "der", "Him\u00b7mel", "of\u00b7fen", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bring alles mit, mein Liebchen!", "tokens": ["Bring", "al\u00b7les", "mit", ",", "mein", "Lieb\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}