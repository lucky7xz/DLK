{"dta.poem.20870": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Sehr reich zu seyn auf dieser Erden/", "tokens": ["Sehr", "reich", "zu", "seyn", "auf", "die\u00b7ser", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehrt nicht seines Fleisses Glut/", "tokens": ["Ver\u00b7mehrt", "nicht", "sei\u00b7nes", "Fleis\u00b7ses", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er \u00e4ndert niemals die Geberden/", "tokens": ["Er", "\u00e4n\u00b7dert", "nie\u00b7mals", "die", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man nehm\u2019 ihm Aecker/ Vieh und Guth.", "tokens": ["Man", "nehm'", "ihm", "A\u00b7e\u00b7cker", "/", "Vieh", "und", "Guth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Man la\u00df ihn gantz zum Bettler werden/", "tokens": ["Man", "la\u00df", "ihn", "gantz", "zum", "Bett\u00b7ler", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und t\u00f6dt ihm selber Fleisch und Blut.", "tokens": ["Und", "t\u00f6dt", "ihm", "sel\u00b7ber", "Fleisch", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sein Sinn hat nie dahin getracht", "tokens": ["Sein", "Sinn", "hat", "nie", "da\u00b7hin", "ge\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel hohe Titul zu erreichen/", "tokens": ["Viel", "ho\u00b7he", "Ti\u00b7tul", "zu", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ehre Licht/ der Schande Nacht", "tokens": ["Der", "Eh\u00b7re", "Licht", "/", "der", "Schan\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan sein Gem\u00fchte leicht vergleichen/", "tokens": ["Kan", "sein", "Ge\u00b7m\u00fch\u00b7te", "leicht", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Furchte so das Sterben macht/", "tokens": ["Die", "Furch\u00b7te", "so", "das", "Ster\u00b7ben", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kan seine Wolfahrt nicht bestreichen.", "tokens": ["Kan", "sei\u00b7ne", "Wol\u00b7fahrt", "nicht", "be\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Wei\u00dfheit kan ihn nur erqvicken/", "tokens": ["Die", "Wei\u00df\u00b7heit", "kan", "ihn", "nur", "er\u00b7qvi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lehrt ihn ohne Furcht zu seyn/", "tokens": ["Und", "lehrt", "ihn", "oh\u00b7ne", "Furcht", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann sich die Todes-Angst l\u00e4st blicken/", "tokens": ["Wann", "sich", "die", "To\u00b7des\u00b7Angst", "l\u00e4st", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Geist empfindet keine Pein/", "tokens": ["Sein", "Geist", "emp\u00b7fin\u00b7det", "kei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann er sol aus dem Leben r\u00fccken/", "tokens": ["Wann", "er", "sol", "aus", "dem", "Le\u00b7ben", "r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Er liebt des Himmels Hau\u00df allein.", "tokens": ["Er", "liebt", "des", "Him\u00b7mels", "Hau\u00df", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der geht mit Freuden aus der Welt;", "tokens": ["Der", "geht", "mit", "Freu\u00b7den", "aus", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die aber voller groben Sinnen/", "tokens": ["Die", "a\u00b7ber", "vol\u00b7ler", "gro\u00b7ben", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und welcher Geist nichts h\u00f6hers h\u00e4lt/", "tokens": ["Und", "wel\u00b7cher", "Geist", "nichts", "h\u00f6\u00b7hers", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die wissen dann nichts zu beginnen/", "tokens": ["Die", "wis\u00b7sen", "dann", "nichts", "zu", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie meinen wie der C\u00f6rper f\u00e4lt/", "tokens": ["Sie", "mei\u00b7nen", "wie", "der", "C\u00f6r\u00b7per", "f\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auch die Seele mu\u00df zurinnen.", "tokens": ["Da\u00df", "auch", "die", "See\u00b7le", "mu\u00df", "zu\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}