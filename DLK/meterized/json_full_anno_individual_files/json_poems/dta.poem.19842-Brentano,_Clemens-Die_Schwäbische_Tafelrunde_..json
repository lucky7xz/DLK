{"dta.poem.19842": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Schw\u00e4bische Tafelrunde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neun Schwaben giengen \u00fcber Land,               ", "tokens": ["Neun", "Schwa\u00b7ben", "gien\u00b7gen", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einer Dornenhecken,", "tokens": ["Zu", "ei\u00b7ner", "Dor\u00b7nen\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Alda der Jokel stille stand,", "tokens": ["Al\u00b7da", "der", "Jo\u00b7kel", "stil\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00e4t Abentheuer schmecken.", "tokens": ["Th\u00e4t", "A\u00b7bent\u00b7heu\u00b7er", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schlief ein Haas ganz starr im Gras,", "tokens": ["Es", "schlief", "ein", "Haas", "ganz", "starr", "im", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ohren th\u00e4t er recken,", "tokens": ["Die", "Oh\u00b7ren", "th\u00e4t", "er", "re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen offen, hart wie Glas,", "tokens": ["Die", "Au\u00b7gen", "of\u00b7fen", ",", "hart", "wie", "Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es war ein rechter Schrecken.", "tokens": ["Es", "war", "ein", "rech\u00b7ter", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt jeder ein Gewehr, gewi\u00df", "tokens": ["H\u00e4tt", "je\u00b7der", "ein", "Ge\u00b7wehr", ",", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wollts f\u00fcr'n andern strecken,", "tokens": ["Er", "wollts", "f\u00fcr'n", "an\u00b7dern", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hattens all neun nur ein Spie\u00df,", "tokens": ["So", "hat\u00b7tens", "all", "neun", "nur", "ein", "Spie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "CARD", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wer darf den Haas mit wecken.", "tokens": ["Wer", "darf", "den", "Haas", "mit", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum hieltens einen Kriegesrath,", "tokens": ["Drum", "hiel\u00b7tens", "ei\u00b7nen", "Krie\u00b7ges\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All neun ganz einig schiere,", "tokens": ["All", "neun", "ganz", "ei\u00b7nig", "schie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wollten thun ein k\u00fchne That", "tokens": ["Sie", "woll\u00b7ten", "thun", "ein", "k\u00fch\u00b7ne", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An dem grausamen Thiere.", "tokens": ["An", "dem", "grau\u00b7sa\u00b7men", "Thie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "All neun an ihrem Schwabenspies", "tokens": ["All", "neun", "an", "ih\u00b7rem", "Schwa\u00b7ben\u00b7spies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "CARD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn m\u00e4nnlich hint'r einander,", "tokens": ["Stehn", "m\u00e4nn\u00b7lich", "hint'r", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Jokel bist der vorderst g'wi\u00df,", "tokens": ["Du", "Jo\u00b7kel", "bist", "der", "vor\u00b7derst", "g'\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sprach einer zu dem ander.", "tokens": ["Sprach", "ei\u00b7ner", "zu", "dem", "an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du Ragenohr, geh du voran!", "tokens": ["Du", "Ra\u00b7gen\u00b7ohr", ",", "geh", "du", "vo\u00b7ran", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der vorderst th\u00e4t auch sprechen:", "tokens": ["Der", "vor\u00b7derst", "th\u00e4t", "auch", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df dahinten vorne stahn,", "tokens": ["Ich", "mu\u00df", "da\u00b7hin\u00b7ten", "vor\u00b7ne", "stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schieb, du mu\u00dft nur stechen.", "tokens": ["Ich", "schieb", ",", "du", "mu\u00dft", "nur", "ste\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der vorderst sprach: w\u00e4rst du vorn dran,", "tokens": ["Der", "vor\u00b7derst", "sprach", ":", "w\u00e4rst", "du", "vorn", "dran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "VAFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Du spr\u00e4chst nit mein Geselle,", "tokens": ["Du", "spr\u00e4chst", "nit", "mein", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Ragenohr, geh du voran,", "tokens": ["Du", "Ra\u00b7gen\u00b7ohr", ",", "geh", "du", "vo\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist ein harte Stelle.", "tokens": ["Hier", "ist", "ein", "har\u00b7te", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Haas erwacht ob ihrem Streit,", "tokens": ["Der", "Haas", "er\u00b7wacht", "ob", "ih\u00b7rem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieng in den Wald hinschweifen,", "tokens": ["Gieng", "in", "den", "Wald", "hin\u00b7schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der schw\u00e4bisch Bund th\u00e4t als ein Beut", "tokens": ["Der", "schw\u00e4\u00b7bisch", "Bund", "th\u00e4t", "als", "ein", "Beut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Haasen Panner ergreifen.", "tokens": ["Des", "Haa\u00b7sen", "Pan\u00b7ner", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Sie wollten auch dem Feind zur Flucht", "tokens": ["Sie", "woll\u00b7ten", "auch", "dem", "Feind", "zur", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein goldne Br\u00fccken schlagen,", "tokens": ["Ein", "gold\u00b7ne", "Br\u00fc\u00b7cken", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und han da lang ein Flu\u00df gesucht,", "tokens": ["Und", "han", "da", "lang", "ein", "Flu\u00df", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kunnten kein erfragen.", "tokens": ["Und", "kunn\u00b7ten", "kein", "er\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da stand ihn'n auch ein See im Weg,", "tokens": ["Da", "stand", "ihn'n", "auch", "ein", "See", "im", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bracht ihn'n grosse Sorgen,", "tokens": ["Der", "bracht", "ihn'n", "gros\u00b7se", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil in dem Gras, nit weit vom Steg", "tokens": ["Weil", "in", "dem", "Gras", ",", "nit", "weit", "vom", "Steg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PTKNEG", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Frosch sa\u00df unverborgen.", "tokens": ["Ein", "Frosch", "sa\u00df", "un\u00b7ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der immerdar geschrieen hat", "tokens": ["Der", "im\u00b7mer\u00b7dar", "ge\u00b7schri\u00b7een", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der quaterten Stimme,", "tokens": ["Mit", "der", "qua\u00b7ter\u00b7ten", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wadwad, Wadwad, Wadwad, Wadwad,", "tokens": ["Wad\u00b7wad", ",", "Wad\u00b7wad", ",", "Wad\u00b7wad", ",", "Wad\u00b7wad", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da giengs dem Ragenohr schlimme.", "tokens": ["Da", "giengs", "dem", "Ra\u00b7gen\u00b7ohr", "schlim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Glaubt, da\u00df der Spiritus ihm rief", "tokens": ["Glaubt", ",", "da\u00df", "der", "Spi\u00b7ri\u00b7tus", "ihm", "rief"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wad, wad! er k\u00f6nnt durchwaden,", "tokens": ["Wad", ",", "wad", "!", "er", "k\u00f6nnt", "durch\u00b7wa\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da th\u00e4t er in dem Wasser tief", "tokens": ["Da", "th\u00e4t", "er", "in", "dem", "Was\u00b7ser", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ersaufen ohn zu baden.", "tokens": ["Er\u00b7sau\u00b7fen", "ohn", "zu", "ba\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sein Schaubhut auf dem Wasser schwamm,", "tokens": ["Sein", "Schaub\u00b7hut", "auf", "dem", "Was\u00b7ser", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da lobten ihn die andern:", "tokens": ["Da", "lob\u00b7ten", "ihn", "die", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seht bis an Hut, der gut Landsmann!", "tokens": ["Seht", "bis", "an", "Hut", ",", "der", "gut", "Lands\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,", "PRELS", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durchs Wasser thut er wandern.", "tokens": ["Durchs", "Was\u00b7ser", "thut", "er", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Frosch schrie wieder Wadwad, Wad,", "tokens": ["Der", "Frosch", "schrie", "wie\u00b7der", "Wad\u00b7wad", ",", "Wad", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Jokel sprach: uns allen", "tokens": ["Der", "Jo\u00b7kel", "sprach", ":", "uns", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Landsmann ruft auf seinen Pfad,", "tokens": ["Der", "Lands\u00b7mann", "ruft", "auf", "sei\u00b7nen", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen nit lang kallen.", "tokens": ["Wir", "sol\u00b7len", "nit", "lang", "kal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wir sollen wahrlich jezt vielmeh", "tokens": ["Wir", "sol\u00b7len", "wahr\u00b7lich", "jezt", "viel\u00b7meh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alsbald ohn Kriegesrathe,", "tokens": ["Als\u00b7bald", "ohn", "Krie\u00b7ges\u00b7ra\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl alle springen in den See,", "tokens": ["Wohl", "al\u00b7le", "sprin\u00b7gen", "in", "den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil wir noch sehn den Pfade.", "tokens": ["Weil", "wir", "noch", "sehn", "den", "Pfa\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}