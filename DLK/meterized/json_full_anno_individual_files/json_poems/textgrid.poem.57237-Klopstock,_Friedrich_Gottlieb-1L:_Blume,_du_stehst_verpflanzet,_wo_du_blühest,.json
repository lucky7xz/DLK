 ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wirst du sch\u00f6n seyn, werden wir dich bewundern,", "tokens": ["Wirst", "du", "sch\u00f6n", "seyn", ",", "wer\u00b7den", "wir", "dich", "be\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VAINF", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aber durch Thr\u00e4nen!", "tokens": ["A\u00b7ber", "durch", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Reizend noch stets, noch immer liebensw\u00fcrdig,", "tokens": ["Rei\u00b7zend", "noch", "stets", ",", "noch", "im\u00b7mer", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Lag Clarissa, da sie uns weggebl\u00fcht war,", "tokens": ["Lag", "Cla\u00b7ris\u00b7sa", ",", "da", "sie", "uns", "weg\u00b7ge\u00b7bl\u00fcht", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und noch stille R\u00f6the die hingesunkne", "tokens": ["Und", "noch", "stil\u00b7le", "R\u00f6\u00b7the", "die", "hin\u00b7ge\u00b7sunk\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "ART", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wange bedeckte.", "tokens": ["Wan\u00b7ge", "be\u00b7deck\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Freudiger war entronnen ihre Seele,", "tokens": ["Freu\u00b7di\u00b7ger", "war", "ent\u00b7ron\u00b7nen", "ih\u00b7re", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "War zu Seelen gekommen, welch' ihr glichen,", "tokens": ["War", "zu", "See\u00b7len", "ge\u00b7kom\u00b7men", ",", "welch'", "ihr", "gli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sch\u00f6nen, ihr verwandten, geliebten Seelen,", "tokens": ["Sch\u00f6\u00b7nen", ",", "ihr", "ver\u00b7wand\u00b7ten", ",", "ge\u00b7lieb\u00b7ten", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Die sie empfingen,", "tokens": ["Die", "sie", "emp\u00b7fin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dass in dem Himmel sanft die liedervollen,", "tokens": ["Dass", "in", "dem", "Him\u00b7mel", "sanft", "die", "lie\u00b7der\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Frohen H\u00fcgel umher zugleich ert\u00f6nten:", "tokens": ["Fro\u00b7hen", "H\u00fc\u00b7gel", "um\u00b7her", "zu\u00b7gleich", "er\u00b7t\u00f6n\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "ADV", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ruhe dir, und Kronen des Siegs, o Seele,", "tokens": ["Ru\u00b7he", "dir", ",", "und", "Kro\u00b7nen", "des", "Siegs", ",", "o", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KON", "NN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Weil du so sch\u00f6n warst!", "tokens": ["Weil", "du", "so", "sch\u00f6n", "warst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "So triumphirten, die es w\u00fcrdig waren.", "tokens": ["So", "tri\u00b7um\u00b7phir\u00b7ten", ",", "die", "es", "w\u00fcr\u00b7dig", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kom, und lass wie ein Fest die Stund' uns, Cidli,", "tokens": ["Kom", ",", "und", "lass", "wie", "ein", "Fest", "die", "Stund'", "uns", ",", "Cid\u00b7li", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "PPER", "$,", "XY", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da sie fliehend uns ihr erhabnes Bild liess,", "tokens": ["Da", "sie", "flie\u00b7hend", "uns", "ihr", "er\u00b7hab\u00b7nes", "Bild", "liess", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einsamer feyren!", "tokens": ["Ein\u00b7sa\u00b7mer", "fey\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Samle Zipressen, dass des Trauerlaubes", "tokens": ["Sam\u00b7le", "Zip\u00b7res\u00b7sen", ",", "dass", "des", "Trau\u00b7er\u00b7lau\u00b7bes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kr\u00e4nz' ich winde, du dann auf diese Kr\u00e4nze", "tokens": ["Kr\u00e4nz'", "ich", "win\u00b7de", ",", "du", "dann", "auf", "die\u00b7se", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mitgeweinte Thr\u00e4nen zur ernsten Feyer", "tokens": ["Mit\u00b7ge\u00b7wein\u00b7te", "Thr\u00e4\u00b7nen", "zur", "erns\u00b7ten", "Fe\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Schwesterlich weinest!", "tokens": ["Schwes\u00b7ter\u00b7lich", "wei\u00b7nest", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Blume, du stehst verpflanzet, wo du bl\u00fchest,", "tokens": ["Blu\u00b7me", ",", "du", "steh