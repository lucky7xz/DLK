{"textgrid.poem.40393": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Taucher", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Taucher werden,", "tokens": ["Ich", "kann", "ein", "Tau\u00b7cher", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "einsam auf Meeres Grund.", "tokens": ["ein\u00b7sam", "auf", "Mee\u00b7res", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da k\u00f6nnt ihr St\u00fcrme nicht hinab;", "tokens": ["Da", "k\u00f6nnt", "ihr", "St\u00fcr\u00b7me", "nicht", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "still wie in Todes Schlund", "tokens": ["still", "wie", "in", "To\u00b7des", "Schlund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "tu' ich mein k\u00fchnes Werk.", "tokens": ["tu'", "ich", "mein", "k\u00fch\u00b7nes", "Werk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lautlose Wirbel schauern", "tokens": ["Laut\u00b7lo\u00b7se", "Wir\u00b7bel", "schau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00fcber und unter mir;", "tokens": ["\u00fc\u00b7ber", "und", "un\u00b7ter", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "mit dunklen Fangarmen lauert", "tokens": ["mit", "dunk\u00b7len", "Fang\u00b7ar\u00b7men", "lau\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "heimt\u00fcckisches Getier", "tokens": ["heim\u00b7t\u00fc\u00b7cki\u00b7sches", "Ge\u00b7tier"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "zwischen den grauen Riffen.", "tokens": ["zwi\u00b7schen", "den", "grau\u00b7en", "Rif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da mu\u00df ich die Sch\u00e4tze heben,", "tokens": ["Da", "mu\u00df", "ich", "die", "Sch\u00e4t\u00b7ze", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die f\u00fcr die Menschen taugen;", "tokens": ["die", "f\u00fcr", "die", "Men\u00b7schen", "tau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "gespenstische Wesen schweben", "tokens": ["ge\u00b7spens\u00b7ti\u00b7sche", "We\u00b7sen", "schwe\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "mit bunten Phosphoraugen", "tokens": ["mit", "bun\u00b7ten", "Phos\u00b7phor\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "um meine Glocke hin.", "tokens": ["um", "mei\u00b7ne", "Glo\u00b7cke", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und hab ich sie gehoben,", "tokens": ["Und", "hab", "ich", "sie", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dann sperrt wohl noch ein Hai", "tokens": ["dann", "sperrt", "wohl", "noch", "ein", "Hai"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sein schiefes Maulwerk nach mir auf.", "tokens": ["sein", "schie\u00b7fes", "Maul\u00b7werk", "nach", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem bringt's mein Fu\u00dftritt bei:", "tokens": ["Dem", "bringt's", "mein", "Fu\u00df\u00b7tritt", "bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "hier schwebt ein kleiner Held!", "tokens": ["hier", "schwebt", "ein", "klei\u00b7ner", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich kann ein Taucher werden,", "tokens": ["Ich", "kann", "