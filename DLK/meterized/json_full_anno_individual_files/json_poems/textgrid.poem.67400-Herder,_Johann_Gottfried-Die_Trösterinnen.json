k und mit uns selbst vers\u00f6hnen.", "tokens": ["Die", "uns", "mit", "Gott", "und", "Gl\u00fcck", "und", "mit", "uns", "selbst", "ver\u00b7s\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "KON", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Herzens Tr\u00f6sterin ist Herzenspoesie.", "tokens": ["Des", "Her\u00b7zens", "Tr\u00f6s\u00b7te\u00b7rin", "ist", "Her\u00b7zen\u00b7spo\u00b7e\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}