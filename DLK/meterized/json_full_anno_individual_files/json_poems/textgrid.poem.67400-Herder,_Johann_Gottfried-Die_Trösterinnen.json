{"textgrid.poem.67400": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Tr\u00f6sterinnen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die zarte Laute nicht mit ihrem sanften Beben,", "tokens": ["Die", "zar\u00b7te", "Lau\u00b7te", "nicht", "mit", "ih\u00b7rem", "sanf\u00b7ten", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, philosophisch Rohr, Du sollst mir Labung geben;", "tokens": ["Du", ",", "phi\u00b7lo\u00b7so\u00b7phisch", "Rohr", ",", "Du", "sollst", "mir", "La\u00b7bung", "ge\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "NN", "$,", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Dir, o Tr\u00f6sterin, entschwindet mir das Leben,", "tokens": ["Aus", "Dir", ",", "o", "Tr\u00f6s\u00b7te\u00b7rin", ",", "ent\u00b7schwin\u00b7det", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von Lippen kaum ber\u00fchrt, ein leichtes W\u00f6lkchen hin.", "tokens": ["Von", "Lip\u00b7pen", "kaum", "be\u00b7r\u00fchrt", ",", "ein", "leich\u00b7tes", "W\u00f6lk\u00b7chen", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mit dem W\u00f6lkchen sind des Lebens harte Stunden,", "tokens": ["Und", "mit", "dem", "W\u00f6lk\u00b7chen", "sind", "des", "Le\u00b7bens", "har\u00b7te", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie Traumgebilde, kaum ber\u00fchret und verschwunden,", "tokens": ["Wie", "Traum\u00b7ge\u00b7bil\u00b7de", ",", "kaum", "be\u00b7r\u00fch\u00b7ret", "und", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADV", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verschwunden ungemerkt dem froh-entw\u00f6lkten Sinn.", "tokens": ["Ver\u00b7schwun\u00b7den", "un\u00b7ge\u00b7merkt", "dem", "froh\u00b7ent\u00b7w\u00f6lk\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer reichet mir das Rohr? Es soll mir Wahn und Glauben", "tokens": ["Wer", "rei\u00b7chet", "mir", "das", "Rohr", "?", "Es", "soll", "mir", "Wahn", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder Zukunft Traum, umw\u00f6lbt mit vollen Trauben,", "tokens": ["Und", "je\u00b7der", "Zu\u00b7kunft", "Traum", ",", "um\u00b7w\u00f6lbt", "mit", "vol\u00b7len", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir Hoffnung, Ahnung, Wunsch, Gef\u00fchl und Sehnsucht rauben;", "tokens": ["Mir", "Hoff\u00b7nung", ",", "Ah\u00b7nung", ",", "Wunsch", ",", "Ge\u00b7f\u00fchl", "und", "Sehn\u00b7sucht", "rau\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Menschen W\u00fcrd' und Werth ist T\u00fcrken-Apathie! \u2013", "tokens": ["Des", "Men\u00b7schen", "W\u00fcrd'", "und", "Werth", "ist", "T\u00fcr\u00b7ken\u00b7A\u00b7pa\u00b7thie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Wie aber? w\u00e4re mir mit allen Lebensstunden", "tokens": ["Wie", "a\u00b7ber", "?", "w\u00e4\u00b7re", "mir", "mit", "al\u00b7len", "Le\u00b7bens\u00b7stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$.", "VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Leben selbst, Gef\u00fchl und Mitgef\u00fchl verschwunden,", "tokens": ["Das", "Le\u00b7ben", "selbst", ",", "Ge\u00b7f\u00fchl", "und", "Mit\u00b7ge\u00b7f\u00fchl", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So tr\u00f6stete mich Rauch und Rauchphilosophie.", "tokens": ["So", "tr\u00f6s\u00b7te\u00b7te", "mich", "Rauch", "und", "Rauch\u00b7phi\u00b7lo\u00b7so\u00b7phie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Komm, zarte Laute, Du mit Deinem zarten Beben,", "tokens": ["Komm", ",", "zar\u00b7te", "Lau\u00b7te", ",", "Du", "mit", "Dei\u00b7nem", "zar\u00b7ten", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schone meiner nicht! Du sollst mir Thr\u00e4nen geben,", "tokens": ["Und", "scho\u00b7ne", "mei\u00b7ner", "nicht", "!", "Du", "sollst", "mir", "Thr\u00e4\u00b7nen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jeder Ton in Dir zum Himmel mich erheben,", "tokens": ["Und", "je\u00b7der", "Ton", "in", "Dir", "zum", "Him\u00b7mel", "mich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erheben mich in Klang und Maa\u00df und Sympathie.", "tokens": ["Er\u00b7he\u00b7ben", "mich", "in", "Klang", "und", "Maa\u00