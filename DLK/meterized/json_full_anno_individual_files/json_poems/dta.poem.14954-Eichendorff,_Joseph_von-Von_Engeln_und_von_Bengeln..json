{"dta.poem.14954": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Von Engeln und von Bengeln.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die machten bald wichtige Mienen", "tokens": ["Die", "mach\u00b7ten", "bald", "wich\u00b7ti\u00b7ge", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und wurden erstaunlich klug,", "tokens": ["Und", "wur\u00b7den", "er\u00b7staun\u00b7lich", "klug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fl\u00fcgel gar unn\u00fctz ihn'n schienen,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "gar", "un\u00b7n\u00fctz", "ihn'n", "schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00e4mten sich deren genug.", "tokens": ["Sie", "sch\u00e4m\u00b7ten", "sich", "de\u00b7ren", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}