{"textgrid.poem.52173": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin den Rhein hinauf gezogen", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin den Rhein hinauf gezogen", "tokens": ["Ich", "bin", "den", "Rhein", "hin\u00b7auf", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch manches schatt'ge Felsentor,", "tokens": ["Durch", "man\u00b7ches", "schatt'\u00b7ge", "Fel\u00b7sen\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entlang die blauen, frischen Wogen", "tokens": ["Ent\u00b7lang", "die", "blau\u00b7en", ",", "fri\u00b7schen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem hohen Quell empor.", "tokens": ["Zu", "sei\u00b7nem", "ho\u00b7hen", "Quell", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich glaubte, da\u00df der Rhein entspringe,", "tokens": ["Ich", "glaub\u00b7te", ",", "da\u00df", "der", "Rhein", "ent\u00b7sprin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So liedervoll, so weinumlaubt,", "tokens": ["So", "lie\u00b7der\u00b7voll", ",", "so", "wei\u00b7num\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus eines Sees lichtem Ringe,", "tokens": ["Aus", "ei\u00b7nes", "Sees", "lich\u00b7tem", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch fand ich nicht, was ich geglaubt.", "tokens": ["Doch", "fand", "ich", "nicht", ",", "was", "ich", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Indem ich durch die Matten irrte", "tokens": ["In\u00b7dem", "ich", "durch", "die", "Mat\u00b7ten", "irr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach solchen Bornes Freudeschein,", "tokens": ["Nach", "sol\u00b7chen", "Bor\u00b7nes", "Freu\u00b7de\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wies schweigend der befragte Hirte", "tokens": ["Wies", "schwei\u00b7gend", "der", "be\u00b7frag\u00b7te", "Hir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empor mich zum Granitgestein.", "tokens": ["Em\u00b7por", "mich", "zum", "Gra\u00b7nit\u00b7ge\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPE