{"dta.poem.13485": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebchen, dich lie\u00df ich zur\u00fccke,", "tokens": ["Ach", "Lieb\u00b7chen", ",", "dich", "lie\u00df", "ich", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mein liebes, herziges Kind,", "tokens": ["Mein", "lie\u00b7bes", ",", "her\u00b7zi\u00b7ges", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da lauern viel Menschen voll T\u00fccke,", "tokens": ["Da", "lau\u00b7ern", "viel", "Men\u00b7schen", "voll", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die sind dir so feindlich gesinnt.", "tokens": ["Die", "sind", "dir", "so", "feind\u00b7lich", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die m\u00f6chten so gerne zerst\u00f6ren", "tokens": ["Die", "m\u00f6ch\u00b7ten", "so", "ger\u00b7ne", "zer\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf Erden das sch\u00f6ne Fest,", "tokens": ["Auf", "Er\u00b7den", "das", "sch\u00f6\u00b7ne", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach k\u00f6nnte das Lieben aufh\u00f6ren,", "tokens": ["Ach", "k\u00f6nn\u00b7te", "das", "Lie\u00b7ben", "auf\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So m\u00f6gen sie nehmen den Rest.", "tokens": ["So", "m\u00f6\u00b7gen", "sie", "neh\u00b7men", "den", "Rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und alle die gr\u00fcnen Orte,", "tokens": ["Und", "al\u00b7le", "die", "gr\u00fc\u00b7nen", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo wir gegangen im Wald,", "tokens": ["Wo", "wir", "ge\u00b7gan\u00b7gen", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die sind nun wohl anders geworden,", "tokens": ["Die", "sind", "nun", "wohl", "an\u00b7ders", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADV", "VAPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da ist's nun so still und kalt.", "tokens": ["Da", "ist's", "nun", "so", "still", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Da sind nun am kalten Himmel", "tokens": ["Da", "sind", "nun", "am", "kal\u00b7ten", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Viel tausend Sterne gestellt,", "tokens": ["Viel", "tau\u00b7send", "Ster\u00b7ne", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es scheint ihr goldnes Gewimmel", "tokens": ["Es", "scheint", "ihr", "gold\u00b7nes", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weit \u00fcbers beschneite Feld.", "tokens": ["Weit", "\u00fc\u00b7bers", "be\u00b7schnei\u00b7te", "Feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Mein' Seele ist so beklommen,", "tokens": ["Mein'", "See\u00b7le", "ist", "so", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Gassen sind leer und todt,", "tokens": ["Die", "Gas\u00b7sen", "sind", "leer", "und", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da hab' ich die Laute genommen", "tokens": ["Da", "hab'", "ich", "die", "Lau\u00b7te", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und singe in meiner Noth.", "tokens": ["Und", "sin\u00b7ge", "in", "mei\u00b7ner", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ach w\u00e4r' ich im stillen Hafen!", "tokens": ["Ach", "w\u00e4r'", "ich", "im", "stil\u00b7len", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kalte Winde am Fenster gehn,", "tokens": ["Kal\u00b7te", "Win\u00b7de", "am", "Fens\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schlaf ruhig, mein Liebchen, schlafe,", "tokens": ["Schlaf", "ru\u00b7hig", ",", "mein", "Lieb\u00b7chen", ",", "schla\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treu' Liebe wird ewig bestehn!", "tokens": ["Treu'", "Lie\u00b7be", "wird", "e\u00b7wig", "be\u00b7stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}