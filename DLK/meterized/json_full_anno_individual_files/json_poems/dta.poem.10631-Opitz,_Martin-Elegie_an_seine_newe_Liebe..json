{"dta.poem.10631": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Elegie an seine newe Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Vnd du wirst auch bey meiner Buhlschafft stehen/", "tokens": ["Vnd", "du", "wirst", "auch", "bey", "mei\u00b7ner", "Buhl\u00b7schafft", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Delia du Bildtnu\u00df aller Zier.", "tokens": ["O", "De\u00b7lia", "du", "Bildt\u00b7nu\u00df", "al\u00b7ler", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich will auch dich durch meine Ver\u00df erh\u00f6hen/", "tokens": ["Ich", "will", "auch", "dich", "durch", "mei\u00b7ne", "Ver\u00df", "er\u00b7h\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich will dein Lob erh\u00f6hen f\u00fcr vnd f\u00fcr.", "tokens": ["Ich", "will", "dein", "Lob", "er\u00b7h\u00f6\u00b7hen", "f\u00fcr", "vnd", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verzeihe mir/ Asterie mein Leben/", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "/", "As\u00b7te\u00b7rie", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weil ich jetzundt so sehr weit von dir bin/", "tokens": ["Weil", "ich", "je\u00b7tzundt", "so", "sehr", "weit", "von", "dir", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich mich hab in ander Holdt ergeben/", "tokens": ["Da\u00df", "ich", "mich", "hab", "in", "an\u00b7der", "Holdt", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vnd frembde Gunst mir kommen in den Sinn.", "tokens": ["Vnd", "fremb\u00b7de", "Gunst", "mir", "kom\u00b7men", "in", "den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich habe dich in jhren Augen funden/", "tokens": ["Ich", "ha\u00b7be", "dich", "in", "jhren", "Au\u00b7gen", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dein Angesicht/ dein rosinfarben Mundt/", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "/", "dein", "ro\u00b7sin\u00b7far\u00b7ben", "Mundt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dein sch\u00f6nes Haar ist so in jhr verbunden/", "tokens": ["Dein", "sch\u00f6\u00b7nes", "Haar", "ist", "so", "in", "jhr", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df ich sie nicht f\u00fcr dir erkennen kundt.", "tokens": ["Da\u00df", "ich", "sie", "nicht", "f\u00fcr", "dir", "er\u00b7ken\u00b7nen", "kundt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ich fandt in jhr/ was ich bey dir verlassen/", "tokens": ["Ich", "fandt", "in", "jhr", "/", "was", "ich", "bey", "dir", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "$(", "PWS", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich fandt in jhr dich so gebildet ein/", "tokens": ["Ich", "fandt", "in", "jhr", "dich", "so", "ge\u00b7bil\u00b7det", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PRF", "ADV", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df ich vermein\u2019 ich k\u00f6nne sie nicht hassen/", "tokens": ["Da\u00df", "ich", "ver\u00b7mein'", "ich", "k\u00f6n\u00b7ne", "sie", "nicht", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ich m\u00fcsse dann auch dir zuwider sein.", "tokens": ["Ich", "m\u00fcs\u00b7se", "dann", "auch", "dir", "zu\u00b7wi\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O Delia du Spiegel meiner Freuden/", "tokens": ["O", "De\u00b7lia", "du", "Spie\u00b7gel", "mei\u00b7ner", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Du Ebenbildt der Sch\u00f6nsten in der W", "tokens": ["Du", "E\u00b7ben\u00b7bildt", "der", "Sch\u00f6ns\u00b7ten", "in", "der", "W"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verg\u00f6nne doch/ da\u00df sich mein Augen weiden/", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "doch", "/", "da\u00df", "sich", "mein", "Au\u00b7gen", "wei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "KOUS", "PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Weil dein Gesicht mein Leben in sich helt.", "tokens": ["Weil", "dein", "Ge\u00b7sicht", "mein", "Le\u00b7ben", "in", "sich", "helt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Weil jhr Gesicht ist so in dich geschrieben/", "tokens": ["Weil", "jhr", "Ge\u00b7sicht", "ist", "so", "in", "dich", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da\u00df sie jhr selbst nicht ehnlicher sein kan/", "tokens": ["Da\u00df", "sie", "jhr", "selbst", "nicht", "ehn\u00b7li\u00b7cher", "sein", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "VMFIN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Wie wolt\u2019 ich dich/ mein Augenlust/ nicht lieben?", "tokens": ["Wie", "wolt'", "ich", "dich", "/", "mein", "Au\u00b7gen\u00b7lust", "/", "nicht", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "$(", "PPOSAT", "NN", "$(", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ach nimb mich doch von jhrentwegen an.", "tokens": ["Ach", "nimb", "mich", "doch", "von", "jhrent\u00b7we\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "PTKVZ", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.25": {"text": "So will ich mit vnsterbligkeit verehren", "tokens": ["So", "will", "ich", "mit", "vns\u00b7ter\u00b7blig\u00b7keit", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Dein hohe Zier/ dein edel\u2019 \u00e4ugelein/", "tokens": ["Dein", "ho\u00b7he", "Zier", "/", "dein", "e\u00b7del'", "\u00e4u\u00b7ge\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "So lange man von Liebe nur wirdt h\u00f6ren/", "tokens": ["So", "lan\u00b7ge", "man", "von", "Lie\u00b7be", "nur", "wirdt", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "NN", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wirdt man zugleich auch deiner eindenck sein.", "tokens": ["Wirdt", "man", "zu\u00b7gleich", "auch", "dei\u00b7ner", "ein\u00b7denck", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}