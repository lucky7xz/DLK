{"dta.poem.18395": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie die zeichen ihrer vorigen  \n gunst wieder blicken lie\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht wundre/ sch\u00f6nste/ dich wann meine schwache Hand", "tokens": ["Nicht", "wund\u00b7re", "/", "sch\u00f6ns\u00b7te", "/", "dich", "wann", "mei\u00b7ne", "schwa\u00b7che", "Hand"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "VVFIN", "$(", "PPER", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wei\u00df vor diesesmahl die feder recht zu f\u00fchren", "tokens": ["Nicht", "wei\u00df", "vor", "die\u00b7ses\u00b7mahl", "die", "fe\u00b7der", "recht", "zu", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "APPR", "ADV", "ART", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast durch einen ku\u00df die seele mir entwand/", "tokens": ["Du", "hast", "durch", "ei\u00b7nen", "ku\u00df", "die", "see\u00b7le", "mir", "ent\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mit der seelen will sich alle krafft verliehren!", "tokens": ["Und", "mit", "der", "see\u00b7len", "will", "sich", "al\u00b7le", "krafft", "ver\u00b7lieh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVINF", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch schaue diese schrifft mit sanfften blicken an/", "tokens": ["Doch", "schau\u00b7e", "die\u00b7se", "schrifft", "mit", "sanff\u00b7ten", "bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein trieb der st\u00e4rcket mich/ da\u00df ich noch schreiben kan/", "tokens": ["Dein", "trieb", "der", "st\u00e4r\u00b7cket", "mich", "/", "da\u00df", "ich", "noch", "schrei\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sonst k\u00f6nt ich meine faust ohn deinen zug nicht regen.", "tokens": ["Sonst", "k\u00f6nt", "ich", "mei\u00b7ne", "faust", "ohn", "dei\u00b7nen", "zug", "nicht", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verwirff die fehler nicht/ die hier enthalten seyn/", "tokens": ["Ver\u00b7wirff", "die", "feh\u00b7ler", "nicht", "/", "die", "hier", "ent\u00b7hal\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "$(", "ART", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie sind selbst deiner macht ber\u00fchmte sieges zeichen;", "tokens": ["Sie", "sind", "selbst", "dei\u00b7ner", "macht", "be\u00b7r\u00fchm\u00b7te", "sie\u00b7ges", "zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und g\u00f6nne/ da\u00df ich darff di\u00df schlechte opffer streun/", "tokens": ["Und", "g\u00f6n\u00b7ne", "/", "da\u00df", "ich", "darff", "di\u00df", "schlech\u00b7te", "opf\u00b7fer", "streun", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "PDS", "VVFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein selave kan sonst nichts als seine ketten reichen.", "tokens": ["Ein", "se\u00b7la\u00b7ve", "kan", "sonst", "nichts", "als", "sei\u00b7ne", "ket\u00b7ten", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "PIS", "KOKOM", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Du weist ja meine noth und meine liebes-pein/", "tokens": ["Du", "weist", "ja", "mei\u00b7ne", "noth", "und", "mei\u00b7ne", "lie\u00b7bes\u00b7pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kennest meinen trieb und meine freundschafft-flammen/", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7nen", "trieb", "und", "mei\u00b7ne", "freund\u00b7schafft\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die auch an reinigkeit der sonnen gleiche seyn/", "tokens": ["Die", "auch", "an", "rei\u00b7nig\u00b7keit", "der", "son\u00b7nen", "glei\u00b7che", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "ART", "ADJA", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und von dem himmel selbst und seiner krafft herstammen.", "tokens": ["Und", "von", "dem", "him\u00b7mel", "selbst", "und", "sei\u00b7ner", "krafft", "her\u00b7stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zwar hab ich schon vorl\u00e4ngst in diesem feur gebrandt/", "tokens": ["Zwar", "hab", "ich", "schon", "vor\u00b7l\u00e4ngst", "in", "die\u00b7sem", "feur", "ge\u00b7brandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Eh noch mein jammer kont dein hartes hertz bewegen;", "tokens": ["Eh", "noch", "mein", "jam\u00b7mer", "kont", "dein", "har\u00b7tes", "hertz", "be\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch endlich wird mir nun das labsal erst bekant/", "tokens": ["Doch", "end\u00b7lich", "wird", "mir", "nun", "das", "lab\u00b7sal", "erst", "be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das meinen krancken geist mit s\u00fcsser lust kan regen.", "tokens": ["Das", "mei\u00b7nen", "kran\u00b7cken", "geist", "mit", "s\u00fcs\u00b7ser", "lust", "kan", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich schau ietzt nicht allein dein himmlisch augen-licht/", "tokens": ["Ich", "schau", "ietzt", "nicht", "al\u00b7lein", "dein", "himm\u00b7lisch", "au\u00b7gen\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich mag auch manches wort von deinen lippen sp\u00fchren/", "tokens": ["Ich", "mag", "auch", "man\u00b7ches", "wort", "von", "dei\u00b7nen", "lip\u00b7pen", "sp\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wa\u00df mir noch zur zeit durch alle geister bricht/", "tokens": ["Und", "wa\u00df", "mir", "noch", "zur", "zeit", "durch", "al\u00b7le", "geis\u00b7ter", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich kan auch deinen mund mit einem ku\u00df ber\u00fchren.", "tokens": ["Ich", "kan", "auch", "dei\u00b7nen", "mund", "mit", "ei\u00b7nem", "ku\u00df", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gewi\u00df ich bin verpflicht vor diese g\u00fctigkeit/", "tokens": ["Ge\u00b7wi\u00df", "ich", "bin", "ver\u00b7pflicht", "vor", "die\u00b7se", "g\u00fc\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der himmel wil mir wohl/ ich kan von freuden sagen/", "tokens": ["Der", "him\u00b7mel", "wil", "mir", "wohl", "/", "ich", "kan", "von", "freu\u00b7den", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "$(", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn da mein hertz zuvor mit kummer ward bestreut/", "tokens": ["Denn", "da", "mein", "hertz", "zu\u00b7vor", "mit", "kum\u00b7mer", "ward", "be\u00b7streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Will mich das gl\u00fcck jetzt selbst auff seinen h\u00e4nden tragen.", "tokens": ["Will", "mich", "das", "gl\u00fcck", "jetzt", "selbst", "auff", "sei\u00b7nen", "h\u00e4n\u00b7den", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch glaube/ da\u00df mein gl\u00fcck noch unvollkommen sey/", "tokens": ["Doch", "glau\u00b7be", "/", "da\u00df", "mein", "gl\u00fcck", "noch", "un\u00b7voll\u00b7kom\u00b7men", "sey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wenn ich nicht ferner darff de\u00df mundes Purpur k\u00fcssen/", "tokens": ["Wenn", "ich", "nicht", "fer\u00b7ner", "darff", "de\u00df", "mun\u00b7des", "Pur\u00b7pur", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn kennstu meine noth; so lerne die\u00df dabey/", "tokens": ["Denn", "kenns\u00b7tu", "mei\u00b7ne", "noth", ";", "so", "ler\u00b7ne", "die\u00df", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PDS", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich kan zwar deiner pracht/ doch niemahls gnug/ geniessen.", "tokens": ["Ich", "kan", "zwar", "dei\u00b7ner", "pracht", "/", "doch", "nie\u00b7mahls", "gnug", "/", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$(", "ADV", "ADV", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie selten aber darff ich wol dein auge schaun?", "tokens": ["Wie", "sel\u00b7ten", "a\u00b7ber", "darff", "ich", "wol", "dein", "au\u00b7ge", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie mu\u00df ich mehrentheils in einsamkeiten schweben", "tokens": ["Wie", "mu\u00df", "ich", "meh\u00b7ren\u00b7theils", "in", "ein\u00b7sam\u00b7kei\u00b7ten", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und meine kurtze freud auff furcht und hoffnung baun?", "tokens": ["Und", "mei\u00b7ne", "kurt\u00b7ze", "freud", "auff", "furcht", "und", "hoff\u00b7nung", "baun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie sparsam will mich doch dein sonnen-glantz umbgeben.", "tokens": ["Wie", "spar\u00b7sam", "will", "mich", "doch", "dein", "son\u00b7nen\u00b7glantz", "umb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach! meine zeit verstreicht in unbekanter lust/", "tokens": ["Ach", "!", "mei\u00b7ne", "zeit", "ver\u00b7streicht", "in", "un\u00b7be\u00b7kan\u00b7ter", "lust", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du setzest meiner lieb so enge maa\u00df und schrancken/", "tokens": ["Du", "set\u00b7zest", "mei\u00b7ner", "lieb", "so", "en\u00b7ge", "maa\u00df", "und", "schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "ADV", "ADJA", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und hemmst auf solche art das feur in meiner brust/", "tokens": ["Und", "hemmst", "auf", "sol\u00b7che", "art", "das", "feur", "in", "mei\u00b7ner", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df offt die geister selbst aus ihren zirckeln wancken.", "tokens": ["Da\u00df", "offt", "die", "geis\u00b7ter", "selbst", "aus", "ih\u00b7ren", "zir\u00b7ckeln", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch glaube/ kan ich schon nicht stetig bey dir seyn/", "tokens": ["Doch", "glau\u00b7be", "/", "kan", "ich", "schon", "nicht", "ste\u00b7tig", "bey", "dir", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "APPR", "PPER", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So bleibt die schrifft dennoch mein trost in den beschwerden/", "tokens": ["So", "bleibt", "die", "schrifft", "den\u00b7noch", "mein", "trost", "in", "den", "be\u00b7schwer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und was die feder nicht wei\u00df auf di\u00df blatt zu streun/", "tokens": ["Und", "was", "die", "fe\u00b7der", "nicht", "wei\u00df", "auf", "di\u00df", "blatt", "zu", "streun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "PDS", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Mu\u00df durch gedancken doch im sinn vollstrecket werden;", "tokens": ["Mu\u00df", "durch", "ge\u00b7dan\u00b7cken", "doch", "im", "sinn", "voll\u00b7stre\u00b7cket", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ach! die gedancken seynd ein wunderwerck der zeit/", "tokens": ["Ach", "!", "die", "ge\u00b7dan\u00b7cken", "seynd", "ein", "wun\u00b7der\u00b7werck", "der", "zeit", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wer tadelt w\u00f6l allhier ein k\u00fchnes unterfangen?", "tokens": ["Wer", "ta\u00b7delt", "w\u00f6l", "all\u00b7hier", "ein", "k\u00fch\u00b7nes", "un\u00b7ter\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Wer straffet unsere lust? wer st\u00f6rt die liebligkeit?", "tokens": ["Wer", "straf\u00b7fet", "un\u00b7se\u00b7re", "lust", "?", "wer", "st\u00f6rt", "die", "lieb\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.47": {"text": "Und worzu kan man nicht durch diesen zug gelangen?", "tokens": ["Und", "wor\u00b7zu", "kan", "man", "nicht", "durch", "die\u00b7sen", "zug", "ge\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PIS", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hier schertzt man ohn gefahr/ man redet/ lacht und k\u00fcst/", "tokens": ["Hier", "schertzt", "man", "ohn", "ge\u00b7fahr", "/", "man", "re\u00b7det", "/", "lacht", "und", "k\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$(", "PIS", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Hier kan man ohn gestrafft die sch\u00f6nste brust ber\u