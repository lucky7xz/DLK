{"textgrid.poem.39163": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort, Nebelbilder ihr, aus finst'rem Norden,", "tokens": ["Fort", ",", "Ne\u00b7bel\u00b7bil\u00b7der", "ihr", ",", "aus", "finst'\u00b7rem", "Nor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die w\u00fcst, gespenstisch mir das Hirn umtanzen;", "tokens": ["Die", "w\u00fcst", ",", "ge\u00b7spens\u00b7tisch", "mir", "das", "Hirn", "um\u00b7tan\u00b7zen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fort, Schlachtges\u00e4nge ihr, von Blut und Morden,", "tokens": ["Fort", ",", "Schlacht\u00b7ge\u00b7s\u00e4n\u00b7ge", "ihr", ",", "von", "Blut", "und", "Mor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die ihr erdr\u00f6hnt von Schwertern und von Lanzen!", "tokens": ["Die", "ihr", "er\u00b7dr\u00f6hnt", "von", "Schwer\u00b7tern", "und", "von", "Lan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weicht vor des S\u00fcdens weicheren Akkorden,", "tokens": ["Weicht", "vor", "des", "S\u00fc\u00b7dens", "wei\u00b7che\u00b7ren", "Ak\u00b7kor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ich beschwor in Florentiner Stanzen;", "tokens": ["Die", "ich", "be\u00b7schwor", "in", "Flo\u00b7ren\u00b7ti\u00b7ner", "Stan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Satt will ich eurer blut'gen Bacchanalien,", "tokens": ["Satt", "will", "ich", "eu\u00b7rer", "blut'\u00b7gen", "Bac\u00b7cha\u00b7na\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mich schaukeln auf dem Wohllaut von Italien.", "tokens": ["Mich", "schau\u00b7keln", "auf", "dem", "Wohl\u00b7laut", "von", "I\u00b7ta\u00b7li\u00b7en."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Fort, Nebelbilder ihr, aus finst'rem Norden,", "tokens": ["Fort", ",", "Ne\u00b7bel\u00b7bil\u00b7der", "ihr", ",", "aus", "finst'\u00b7rem", "Nor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die w\u00fcst, gespenstisch mir das Hirn umtanzen;", "tokens": ["Die", "w\u00fcst", ",", "ge\u00b7spens\u00b7tisch", "mir", "das", "Hirn", "um\u00b7tan\u00b7zen", ";"], "token_info": ["word", "wor