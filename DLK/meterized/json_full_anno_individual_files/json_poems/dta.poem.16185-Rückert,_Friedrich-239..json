{"dta.poem.16185": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "239.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Weltbetrachtungsart und Ueberzeugungsweise,", "tokens": ["Die", "Welt\u00b7be\u00b7trach\u00b7tungs\u00b7art", "und", "Ue\u00b7ber\u00b7zeu\u00b7gungs\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich gebildet hat ein Volk in seinem Kreise,", "tokens": ["Die", "sich", "ge\u00b7bil\u00b7det", "hat", "ein", "Volk", "in", "sei\u00b7nem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ersch\u00fcttert mu\u00df sie seyn und innerlich gest\u00f6rt,", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "mu\u00df", "sie", "seyn", "und", "in\u00b7ner\u00b7lich", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sobald ein Volk allein nicht mehr sich angeh\u00f6rt;", "tokens": ["So\u00b7bald", "ein", "Volk", "al\u00b7lein", "nicht", "mehr", "sich", "an\u00b7ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sobald es auch nur h\u00f6rt von fremden Sitten sagen", "tokens": ["So\u00b7bald", "es", "auch", "nur", "h\u00f6rt", "von", "frem\u00b7den", "Sit\u00b7ten", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Meinungen, die nicht mit seinen sich vertragen:", "tokens": ["Und", "Mei\u00b7nun\u00b7gen", ",", "die", "nicht", "mit", "sei\u00b7nen", "sich", "ver\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PTKNEG", "APPR", "PPOSAT", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zuerst erwehrt es sich andringender Gefahren", "tokens": ["Zu\u00b7erst", "er\u00b7wehrt", "es", "sich", "and\u00b7rin\u00b7gen\u00b7der", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dadurch, da\u00df es mit Stolz die Fremden nennt Barbaren.", "tokens": ["Da\u00b7durch", ",", "da\u00df", "es", "mit", "Stolz", "die", "Frem\u00b7den", "nennt", "Bar\u00b7ba\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch halten kann nicht lang des stolzen Wahns Umschildung,", "tokens": ["Doch", "hal\u00b7ten", "kann", "nicht", "lang", "des", "stol\u00b7zen", "Wahns", "Um\u00b7schil\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Einbildung schmilzt mit fortgeschrittner Bildung.", "tokens": ["Und", "die", "Ein\u00b7bil\u00b7dung", "schmilzt", "mit", "fort\u00b7ge\u00b7schritt\u00b7ner", "Bil\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dann droht ein andrer Wahn mit n\u00e4herer Gefahr:", "tokens": ["Dann", "droht", "ein", "an\u00b7drer", "Wahn", "mit", "n\u00e4\u00b7he\u00b7rer", "Ge\u00b7fahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df in der Menschenwelt nichts sei unwandelbar.", "tokens": ["Da\u00df", "in", "der", "Men\u00b7schen\u00b7welt", "nichts", "sei", "un\u00b7wan\u00b7del\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das eine gelte hier, das andre gelte dort,", "tokens": ["Das", "ei\u00b7ne", "gel\u00b7te", "hier", ",", "das", "and\u00b7re", "gel\u00b7te", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "ADV", "$,", "PRELS", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an sich Geltendes sei drum an keinem Ort.", "tokens": ["Und", "an", "sich", "Gel\u00b7ten\u00b7des", "sei", "drum", "an", "kei\u00b7nem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "NN", "VAFIN", "PAV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Kein an sich Geltendes des Guten, Sch\u00f6nen, Rechten;", "tokens": ["Kein", "an", "sich", "Gel\u00b7ten\u00b7des", "des", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen", ",", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "APPR", "PRF", "NN", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist der Kampf, den nun die Bildung durch mu\u00df fechten.", "tokens": ["Das", "ist", "der", "Kampf", ",", "den", "nun", "die", "Bil\u00b7dung", "durch", "mu\u00df", "fech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihn helfe fechten, wer zuerst ihn angeregt,", "tokens": ["Ihn", "hel\u00b7fe", "fech\u00b7ten", ",", "wer", "zu\u00b7erst", "ihn", "an\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "PWS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weltweisheit, die die Welt vor Augen uns gelegt:", "tokens": ["Welt\u00b7weis\u00b7heit", ",", "die", "die", "Welt", "vor", "Au\u00b7gen", "uns", "ge\u00b7legt", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Sie zeig' uns, da\u00df die Form des Guten mancherlei,", "tokens": ["Sie", "zeig'", "uns", ",", "da\u00df", "die", "Form", "des", "Gu\u00b7ten", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch stets an einem Ort nur eins das Rechte sei.", "tokens": ["Doch", "stets", "an", "ei\u00b7nem", "Ort", "nur", "eins", "das", "Rech\u00b7te", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ADV", "PIS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Der Bildung Gipfel sei, an Fremden anerkennen", "tokens": ["Der", "Bil\u00b7dung", "Gip\u00b7fel", "sei", ",", "an", "Frem\u00b7den", "an\u00b7er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fremde, doch sich selbst nicht von sich selber trennen.", "tokens": ["Das", "Frem\u00b7de", ",", "doch", "sich", "selbst", "nicht", "von", "sich", "sel\u00b7ber", "tren\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ADV", "PTKNEG", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}