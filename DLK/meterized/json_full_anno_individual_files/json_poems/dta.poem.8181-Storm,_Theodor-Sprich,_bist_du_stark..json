{"dta.poem.8181": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Sprich, bist du stark.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und nur nicht scheiden kann von deinen Blicken,", "tokens": ["Und", "nur", "nicht", "schei\u00b7den", "kann", "von", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Auge, das von deiner Liebe sprach,", "tokens": ["Das", "Au\u00b7ge", ",", "das", "von", "dei\u00b7ner", "Lie\u00b7be", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf Nimmerwiedersehen zuzudr\u00fccken?", "tokens": ["Auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7se\u00b7hen", "zu\u00b7zu\u00b7dr\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und bist du stark, was sonst das Herz verf\u00fchrt,", "tokens": ["Und", "bist", "du", "stark", ",", "was", "sonst", "das", "Herz", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn es sich schmeichelnd, zwingend dargeboten,", "tokens": ["Wenn", "es", "sich", "schmei\u00b7chelnd", ",", "zwin\u00b7gend", "dar\u00b7ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir stets zu weigern fest und unber\u00fchrt,", "tokens": ["Dir", "stets", "zu", "wei\u00b7gern", "fest", "und", "un\u00b7be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVFIN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jungfr\u00e4ulich zu hangen an dem Todten?", "tokens": ["Und", "jung\u00b7fr\u00e4u\u00b7lich", "zu", "han\u00b7gen", "an", "dem", "Tod\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und bist du stark, da\u00df durch den tr\u00fcben Flor,", "tokens": ["Und", "bist", "du", "stark", ",", "da\u00df", "durch", "den", "tr\u00fc\u00b7ben", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df durch die Einsamkeit m\u00fchsel'ger Jahre,", "tokens": ["Da\u00df", "durch", "die", "Ein\u00b7sam\u00b7keit", "m\u00fch\u00b7sel'\u00b7ger", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn dein Ged\u00e4chtni\u00df schon mein Bild verlor,", "tokens": ["Wenn", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "schon", "mein", "Bild", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch unsre Liebe noch dein Herz bewahre?", "tokens": ["Doch", "uns\u00b7re", "Lie\u00b7be", "noch", "dein", "Herz", "be\u00b7wah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}