{"textgrid.poem.60253": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Wandel der Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie doch d\u00fcnkte mir die Fahrt so lang,", "tokens": ["Wie", "doch", "d\u00fcnk\u00b7te", "mir", "die", "Fahrt", "so", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "O wie sehnt ich mich zur\u00fcck so bang", "tokens": ["O", "wie", "sehnt", "ich", "mich", "zu\u00b7r\u00fcck", "so", "bang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der weiten, fremden Meeresw\u00fcste", "tokens": ["Aus", "der", "wei\u00b7ten", ",", "frem\u00b7den", "Mee\u00b7res\u00b7w\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nach der lieben, fernen Heimatk\u00fcste.", "tokens": ["Nach", "der", "lie\u00b7ben", ",", "fer\u00b7nen", "Hei\u00b7mat\u00b7k\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Endlich winkte das ersehnte Land,", "tokens": ["End\u00b7lich", "wink\u00b7te", "das", "er\u00b7sehn\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jubelnd sprang ich an den teuern Strand,", "tokens": ["Ju\u00b7belnd", "sprang", "ich", "an", "den", "teu\u00b7ern", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und als wiedergr\u00fcne Jugendtr\u00e4ume", "tokens": ["Und", "als", "wie\u00b7der\u00b7gr\u00fc\u00b7ne", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Gr\u00fc\u00dften mich die heimatlichen B\u00e4ume.", "tokens": ["Gr\u00fc\u00df\u00b7ten", "mich", "die", "hei\u00b7mat\u00b7li\u00b7chen", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hold, und s\u00fc\u00dfverwandt, wie nie zuvor,", "tokens": ["Hold", ",", "und", "s\u00fc\u00df\u00b7ver\u00b7wandt", ",", "wie", "nie", "zu\u00b7vor", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ADJD", "$,", "PWAV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Klang das Lied der V\u00f6gel an mein Ohr;", "tokens": ["Klang", "das", "Lied", "der", "V\u00f6\u00b7gel", "an", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Gerne, nach so schmerzlichem Vermissen,", "tokens": ["Ger\u00b7ne", ",", "nach", "so", "schmerz\u00b7li\u00b7chem", "Ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00e4tt ich jeden Stein ans Herz gerissen.", "tokens": ["H\u00e4tt", "ich", "je\u00b7den", "Stein", "ans", "Herz", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, da fand ich dich, und \u2013 todesschwank", "tokens": ["Doch", ",", "da", "fand", "ich", "dich", ",", "und", "\u2013", "to\u00b7des\u00b7schwank"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "$(", "XY"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jede Freude dir zu F\u00fc\u00dfen sank,", "tokens": ["Je\u00b7de", "Freu\u00b7de", "dir", "zu", "F\u00fc\u00b7\u00dfen", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mir ist im Herzen nur geblieben", "tokens": ["Und", "mir", "ist", "im", "Her\u00b7zen", "nur", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Grenzenloses, hoffnungsloses Lieben.", "tokens": ["Gren\u00b7zen\u00b7lo\u00b7ses", ",", "hoff\u00b7nungs\u00b7lo\u00b7ses", "Lie\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "O wie seh