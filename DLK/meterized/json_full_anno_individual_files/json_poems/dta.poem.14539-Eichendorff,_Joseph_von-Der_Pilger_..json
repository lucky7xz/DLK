{"dta.poem.14539": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Man setzt uns auf die Schwelle", "tokens": ["Man", "setzt", "uns", "auf", "die", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir wissen nicht, woher?", "tokens": ["Wir", "wis\u00b7sen", "nicht", ",", "wo\u00b7her", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da gl\u00fcht der Morgen helle,", "tokens": ["Da", "gl\u00fcht", "der", "Mor\u00b7gen", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinaus verlangt uns sehr.", "tokens": ["Hin\u00b7aus", "ver\u00b7langt", "uns", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Erde Klang und Bilder,", "tokens": ["Der", "Er\u00b7de", "Klang", "und", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tiefblaue Fr\u00fchlingslust,", "tokens": ["Tief\u00b7blau\u00b7e", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verlockend mild und wilder,", "tokens": ["Ver\u00b7lo\u00b7ckend", "mild", "und", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bewegen da die Brust.", "tokens": ["Be\u00b7we\u00b7gen", "da", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Bald wird es rings so schw\u00fcle,", "tokens": ["Bald", "wird", "es", "rings", "so", "schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die Welt erathmet kaum,", "tokens": ["Die", "Welt", "e\u00b7rath\u00b7met", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Berg', Schlo\u00df und W\u00e4lder k\u00fchle", "tokens": ["Ber\u00b7g'", ",", "Schlo\u00df", "und", "W\u00e4l\u00b7der", "k\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Steh'n lautlos wie im Traum,", "tokens": ["Steh'n", "laut\u00b7los", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und ein geheimes Grausen", "tokens": ["Und", "ein", "ge\u00b7hei\u00b7mes", "Grau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Beschleichet unsern Sinn:", "tokens": ["Be\u00b7schlei\u00b7chet", "un\u00b7sern", "Sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Wir sehnen uns nach Hause", "tokens": ["Wir", "seh\u00b7nen", "uns", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und wissen nicht wohin?", "tokens": ["Und", "wis\u00b7sen", "nicht", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}