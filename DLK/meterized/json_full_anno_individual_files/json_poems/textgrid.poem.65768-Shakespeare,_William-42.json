{"textgrid.poem.65768": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "42", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du sie hast, ist nicht mein ganzer Schmerz;", "tokens": ["Da\u00df", "du", "sie", "hast", ",", "ist", "nicht", "mein", "gan\u00b7zer", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "VAFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und habe doch f\u00fcrwahr sie treu geliebt.", "tokens": ["Und", "ha\u00b7be", "doch", "f\u00fcr\u00b7wahr", "sie", "treu", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sie dich hat, ist meines Kummers Herz,", "tokens": ["Da\u00df", "sie", "dich", "hat", ",", "ist", "mei\u00b7nes", "Kum\u00b7mers", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Liebesraub, der tiefer mich betr\u00fcbt.", "tokens": ["Ein", "Lie\u00b7bes\u00b7raub", ",", "der", "tie\u00b7fer", "mich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch Liebess\u00fcnder will ich so verteid'gen:", "tokens": ["Euch", "Lie\u00b7bes\u00b7s\u00fcn\u00b7der", "will", "ich", "so", "ver\u00b7tei\u00b7d'\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Du liebst die, weil du wei\u00dft, da\u00df sie mir wert;", "tokens": ["Du", "liebst", "die", ",", "weil", "du", "wei\u00dft", ",", "da\u00df", "sie", "mir", "wert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und so auch sie mu\u00df mich um meinethalb beleid'gen,", "tokens": ["Und", "so", "auch", "sie", "mu\u00df", "mich", "um", "mei\u00b7net\u00b7halb", "be\u00b7lei\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "VMFIN", "PRF", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-++-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Erh\u00f6rend meinen Freund, der meinethalb sie ehrt.", "tokens": ["Er\u00b7h\u00f6\u00b7rend", "mei\u00b7nen", "Freund", ",", "der", "mei\u00b7net\u00b7halb", "sie", "ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verlier' ich dich, mein Liebchen nimmt die Beute;", "tokens": ["Ver\u00b7lier'", "ich", "dich", ",", "mein", "Lieb\u00b7chen", "nimmt", "die", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+