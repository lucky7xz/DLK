{"textgrid.poem.41727": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gru\u00df Euch, Ihr S\u00e4nger! einen Festesgru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gru\u00df Euch, Ihr S\u00e4nger! einen Festesgru\u00df", "tokens": ["Gru\u00df", "Euch", ",", "Ihr", "S\u00e4n\u00b7ger", "!", "ei\u00b7nen", "Fes\u00b7tes\u00b7gru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus meiner Heimat schall es Euch entgegen,", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", "schall", "es", "Euch", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meiner Heimat, wo im Vorgenu\u00df", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", ",", "wo", "im", "Vor\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich frisch und fr\u00f6hlich alle Herzen regen.", "tokens": ["Sich", "frisch", "und", "fr\u00f6h\u00b7lich", "al\u00b7le", "Her\u00b7zen", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gesegnet all ihr liederreichen Scharen,", "tokens": ["Ge\u00b7seg\u00b7net", "all", "ihr", "lie\u00b7der\u00b7rei\u00b7chen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gleich wie V\u00f6glein wir im Lenz gewahren,", "tokens": ["Die", "gleich", "wie", "V\u00f6\u00b7glein", "wir", "im", "Lenz", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu unsren B\u00fcschen unsren Bergen kehren,", "tokens": ["Zu", "un\u00b7sren", "B\u00fc\u00b7schen", "un\u00b7sren", "Ber\u00b7gen", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit ihren Liedern unser Elbthal ehren.", "tokens": ["Mit", "ih\u00b7ren", "Lie\u00b7dern", "un\u00b7ser", "Elb\u00b7thal", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier, wo der Dom, Denkmal der Gotenzeit,", "tokens": ["Hier", ",", "wo", "der", "Dom", ",", "Denk\u00b7mal", "der", "Go\u00b7ten\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel strebt mit seinen Zackenspitzen,", "tokens": ["Zum", "Him\u00b7mel", "strebt", "mit", "sei\u00b7nen", "Za\u00b7cken\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zeugnis heil'ger Gottestrunkenheit,", "tokens": ["Ein", "Zeug\u00b7nis", "heil'\u00b7ger", "Got\u00b7tes\u00b7trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihren Bau geweiht mit Geistesblitzen \u2013", "tokens": ["Die", "ih\u00b7ren", "Bau", "ge\u00b7weiht", "mit", "Geis\u00b7tes\u00b7blit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier singt auch Ihr im Tempel der Germanen", "tokens": ["Hier", "singt", "auch", "Ihr", "im", "Tem\u00b7pel", "der", "Ger\u00b7ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem heiligen Verm\u00e4chtnis unsrer Ahnen,", "tokens": ["Dem", "hei\u00b7li\u00b7gen", "Ver\u00b7m\u00e4cht\u00b7nis", "uns\u00b7rer", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Gunst und Beifall ist dem Fest gewonnen:", "tokens": ["Und", "Gunst", "und", "Bei\u00b7fall", "ist", "dem", "Fest", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.3": {"line.1": {"text": "Doch nicht im Tempel nur von Menschenhand!", "tokens": ["Doch", "nicht", "im", "Tem\u00b7pel", "nur", "von", "Men\u00b7schen\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es lockt Natur zu sich heraus in's Freie", "tokens": ["Es", "lockt", "Na\u00b7tur", "zu", "sich", "he\u00b7raus", "in's", "Frei\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PRF", "APZR", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Bergen und in W\u00e4ldern haltet Stand", "tokens": ["Auf", "Ber\u00b7gen", "und", "in", "W\u00e4l\u00b7dern", "hal\u00b7tet", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gebt dem Tag die frohe Sangesweihe", "tokens": ["Und", "gebt", "dem", "Tag", "die", "fro\u00b7he", "San\u00b7ges\u00b7wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In Ch\u00f6ren die von tausend deutschen Zungen", "tokens": ["In", "Ch\u00f6\u00b7ren", "die", "von", "tau\u00b7send", "deut\u00b7schen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus tausend Herzen sich zugleich gerungen", "tokens": ["Aus", "tau\u00b7send", "Her\u00b7zen", "sich", "zu\u00b7gleich", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum freien Lied gleichwie viel kleine Flammen", "tokens": ["Zum", "frei\u00b7en", "Lied", "gleich\u00b7wie", "viel", "klei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu einer gro\u00dfen Flamme gl\u00fchn zusammen.", "tokens": ["Zu", "ei\u00b7ner", "gro\u00b7\u00dfen", "Flam\u00b7me", "gl\u00fchn", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es wohnt Begeisterung in jedem Sang,", "tokens": ["Es", "wohnt", "Be\u00b7geis\u00b7te\u00b7rung", "in", "je\u00b7dem", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O wie viel mehr in Euren Bundesliedern.", "tokens": ["O", "wie", "viel", "mehr", "in", "Eu\u00b7ren", "Bun\u00b7des\u00b7lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo in der T\u00f6ne Harmonienklang", "tokens": ["Wo", "in", "der", "T\u00f6\u00b7ne", "Har\u00b7mo\u00b7ni\u00b7en\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich Gr\u00fc\u00dfe wechselnd finden und erwidern \u2013", "tokens": ["Sich", "Gr\u00fc\u00b7\u00dfe", "wech\u00b7selnd", "fin\u00b7den", "und", "er\u00b7wi\u00b7dern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADJD", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis dann in einem Ton die Unbekannten", "tokens": ["Bis", "dann", "in", "ei\u00b7nem", "Ton", "die", "Un\u00b7be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vertraut verbunden sich im Liede fanden,", "tokens": ["Ver\u00b7traut", "ver\u00b7bun\u00b7den", "sich", "im", "Lie\u00b7de", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in der Ch\u00f6re Ineinanderklingen", "tokens": ["Und", "in", "der", "Ch\u00f6\u00b7re", "In\u00b7ein\u00b7an\u00b7der\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Bruderband sie alle um sich schlingen.", "tokens": ["Ein", "Bru\u00b7der\u00b7band", "sie", "al\u00b7le", "um", "sich", "schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PIS", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das ist die Losung der verj\u00fcngten Zeiten", "tokens": ["Das", "ist", "die", "Lo\u00b7sung", "der", "ver\u00b7j\u00fcng\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das kn\u00fcpft nicht nur, das wahret fort und fort", "tokens": ["Das", "kn\u00fcpft", "nicht", "nur", ",", "das", "wah\u00b7ret", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "$,", "PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das nehmt mit Euch als Liebespfand beim Scheiden,", "tokens": ["Das", "nehmt", "mit", "Euch", "als", "Lie\u00b7be\u00b7spfand", "beim", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "KOUS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bei des Festes freundlichen Gedenken", "tokens": ["Und", "bei", "des", "Fes\u00b7tes", "freund\u00b7li\u00b7chen", "Ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "M\u00f6gt Ihr es auch den Heimgebliebnen schenken,", "tokens": ["M\u00f6gt", "Ihr", "es", "auch", "den", "Heim\u00b7ge\u00b7blieb\u00b7nen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df nicht nur Tausenden, nein allen, allen", "tokens": ["Da\u00df", "nicht", "nur", "Tau\u00b7sen\u00b7den", ",", "nein", "al\u00b7len", ",", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "NN", "$,", "PTKANT", "PIAT", "$,", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Bruderworte aus den Herzen schallen.", "tokens": ["Die", "Bru\u00b7der\u00b7wor\u00b7te", "aus", "den", "Her\u00b7zen", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und ist der S\u00e4nger nun dem S\u00e4nger ", "tokens": ["Und", "ist", "der", "S\u00e4n\u00b7ger", "nun", "dem", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Warum denn f\u00fchlen sich nicht ", "tokens": ["Wa\u00b7rum", "denn", "f\u00fch\u00b7len", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gewi\u00df! es kommt die Zeit so gro\u00df, so reich,", "tokens": ["Ge\u00b7wi\u00df", "!", "es", "kommt", "die", "Zeit", "so", "gro\u00df", ",", "so", "reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da sind wir einig wohl durch ", "tokens": ["Da", "sind", "wir", "ei\u00b7nig", "wohl", "durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da singen wir der ", "tokens": ["Da", "sin\u00b7gen", "wir", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da steht die Saat in segensreichen Halmen", "tokens": ["Da", "steht", "die", "Saat", "in", "se\u00b7gens\u00b7rei\u00b7chen", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu der wir hoffend jetzt den Samen streuen", "tokens": ["Zu", "der", "wir", "hof\u00b7fend", "jetzt", "den", "Sa\u00b7men", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gott gebe, da\u00df wir uns der Ernte freuen!", "tokens": ["Gott", "ge\u00b7be", ",", "da\u00df", "wir", "uns", "der", "Ern\u00b7te", "freu\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Gru\u00df Euch, Ihr S\u00e4nger! einen Festesgru\u00df", "tokens": ["Gru\u00df", "Euch", ",", "Ihr", "S\u00e4n\u00b7ger", "!", "ei\u00b7nen", "Fes\u00b7tes\u00b7gru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus meiner Heimat schall es Euch entgegen,", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", "schall", "es", "Euch", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meiner Heimat, wo im Vorgenu\u00df", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", ",", "wo", "im", "Vor\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "APPRART", "NN"