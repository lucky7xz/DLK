{"dta.poem.19379": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Stolz genie\u00dfen wir, was wir mit Kampf erwarben;", "tokens": ["Mit", "Stolz", "ge\u00b7nie\u00b7\u00dfen", "wir", ",", "was", "wir", "mit", "Kampf", "er\u00b7war\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wunden sind geheilt, es schm\u00fccken uns die Narben.", "tokens": ["Die", "Wun\u00b7den", "sind", "ge\u00b7heilt", ",", "es", "schm\u00fc\u00b7cken", "uns", "die", "Nar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch einen Stachel l\u00e4\u00dft der Kampf zur\u00fcck im Herzen;", "tokens": ["Doch", "ei\u00b7nen", "Sta\u00b7chel", "l\u00e4\u00dft", "der", "Kampf", "zu\u00b7r\u00fcck", "im", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei b\u00f6sem Wetter wird die Ehrennarbe schmerzen.", "tokens": ["Bei", "b\u00f6\u00b7sem", "Wet\u00b7ter", "wird", "die", "Eh\u00b7ren\u00b7nar\u00b7be", "schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}