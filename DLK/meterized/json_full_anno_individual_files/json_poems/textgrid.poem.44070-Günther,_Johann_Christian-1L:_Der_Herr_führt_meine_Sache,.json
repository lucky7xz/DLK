{"textgrid.poem.44070": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Herr f\u00fchrt meine Sache,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr f\u00fchrt meine Sache,", "tokens": ["Der", "Herr", "f\u00fchrt", "mei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum scheu ich keinen Feind.", "tokens": ["Drum", "scheu", "ich", "kei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Misgunst tob und lache,", "tokens": ["Die", "Mis\u00b7gunst", "tob", "und", "la\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil mir kein Gl\u00fccksstern scheint,", "tokens": ["Weil", "mir", "kein", "Gl\u00fccks\u00b7stern", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die L\u00e4strer k\u00fczeln sich", "tokens": ["Die", "L\u00e4st\u00b7rer", "k\u00fc\u00b7zeln", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "An meinen Ungl\u00fcckspfeilen,", "tokens": ["An", "mei\u00b7nen", "Un\u00b7gl\u00fccks\u00b7pfei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gott wird die Wunden heilen,", "tokens": ["Gott", "wird", "die", "Wun\u00b7den", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und de\u00dfen tr\u00f6st ich mich.", "tokens": ["Und", "de\u00b7\u00dfen", "tr\u00f6st", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dr\u00fcckt mich der S\u00fcnden Menge,", "tokens": ["Dr\u00fcckt", "mich", "der", "S\u00fcn\u00b7den", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Heiland schw\u00e4cht die Last;", "tokens": ["Mein", "Hei\u00b7land", "schw\u00e4cht", "die", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hilft aus dem Gedr\u00e4nge,", "tokens": ["Er", "hilft", "aus", "dem", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn mich die Furcht umfast.", "tokens": ["Wenn", "mich", "die", "Furcht", "um\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Schr\u00f6cken tiefer Nacht", "tokens": ["Das", "Schr\u00f6\u00b7cken", "tie\u00b7fer", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df unverz\u00fcglich weichen,", "tokens": ["Mu\u00df", "un\u00b7ver\u00b7z\u00fcg\u00b7lich", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn mich das Siegeszeichen", "tokens": ["Wenn", "mich", "das", "Sie\u00b7ges\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Creuzes tapfer macht.", "tokens": ["Des", "Creu\u00b7zes", "tap\u00b7fer", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bin ich anjezt verla\u00dfen", "tokens": ["Bin", "ich", "an\u00b7jezt", "ver\u00b7la\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und alles Trostes leer,", "tokens": ["Und", "al\u00b7les", "Tros\u00b7tes", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Geist, du must dich fa\u00dfen,", "tokens": ["Mein", "Geist", ",", "du", "must", "dich", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst wird die Noth zu schwer.", "tokens": ["Sonst", "wird", "die", "Noth", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Himmel lebt ein Freund,", "tokens": ["Im", "Him\u00b7mel", "lebt", "ein", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der wird mir nicht entfallen,", "tokens": ["Der", "wird", "mir", "nicht", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Obgleich die Welt von allen", "tokens": ["Ob\u00b7gleich", "die", "Welt", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich zu entbl\u00f6\u00dfen scheint.", "tokens": ["Mich", "zu", "ent\u00b7bl\u00f6\u00b7\u00dfen", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mu\u00df ich mich in der Fremde", "tokens": ["Mu\u00df", "ich", "mich", "in", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Brodt und Wa\u00dfer m\u00fchn", "tokens": ["Um", "Brodt", "und", "Wa\u00b7\u00dfer", "m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem na\u00dfen Hemde", "tokens": ["Und", "mit", "dem", "na\u00b7\u00dfen", "Hem\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Blut vom Leibe ziehn,", "tokens": ["Mein", "Blut", "vom", "Lei\u00b7be", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will dem H\u00f6chsten traun,", "tokens": ["Ich", "will", "dem", "H\u00f6chs\u00b7ten", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er wird die Armuth wenden", "tokens": ["Er", "wird", "die", "Ar\u00b7muth", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mir mit starcken H\u00e4nden", "tokens": ["Und", "mir", "mit", "star\u00b7cken", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein eignes H\u00fcttchen baun.", "tokens": ["Mein", "eig\u00b7nes", "H\u00fctt\u00b7chen", "baun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Rast immerhin, ihr Sp\u00f6tter,", "tokens": ["Rast", "im\u00b7mer\u00b7hin", ",", "ihr", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht meiner Thr\u00e4nen viel;", "tokens": ["Macht", "mei\u00b7ner", "Thr\u00e4\u00b7nen", "viel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht macht auch das Wetter", "tokens": ["Viel\u00b7leicht", "macht", "auch", "das", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus euch ein Ungl\u00fccksziel.", "tokens": ["Aus", "euch", "ein", "Un\u00b7gl\u00fccks\u00b7ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich w\u00fcntsch es nimmermehr;", "tokens": ["Ich", "w\u00fcnt\u00b7sch", "es", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Darum bekehrt euch lieber,", "tokens": ["Da\u00b7rum", "be\u00b7kehrt", "euch", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr seyd noch nicht hin\u00fcber.", "tokens": ["Ihr", "seyd", "noch", "nicht", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Worauf trozt ihr so sehr?", "tokens": ["Wo\u00b7rauf", "trozt", "ihr", "so", "sehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr, f\u00fchre meine Sache;", "tokens": ["Herr", ",", "f\u00fch\u00b7re", "mei\u00b7ne", "Sa\u00b7che", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich \u00fcbergebe dir", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7ge\u00b7be", "dir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Lohn und auch die Rache.", "tokens": ["Den", "Lohn", "und", "auch", "die", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steh meiner Schwachheit f\u00fcr", "tokens": ["Steh", "mei\u00b7ner", "Schwach\u00b7heit", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und las mir doch einmahl", "tokens": ["Und", "las", "mir", "doch", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Tag des Heils erscheinen.", "tokens": ["Den", "Tag", "des", "Heils", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du siehst mein bittres Weinen", "tokens": ["Du", "siehst", "mein", "bitt\u00b7res", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sch\u00e4zest meine Qual.", "tokens": ["Und", "sch\u00e4\u00b7zest", "mei\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Herr f\u00fchrt meine Sache,", "tokens": ["Der", "Herr", "f\u00fchrt", "mei\u00b7ne", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN"