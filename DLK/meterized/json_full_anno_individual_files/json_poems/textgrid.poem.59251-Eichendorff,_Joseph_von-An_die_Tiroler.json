{"textgrid.poem.59251": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Tiroler", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Waldesrauschen, k\u00fchnem Sturz der Wogen,", "tokens": ["Bei", "Wal\u00b7des\u00b7rau\u00b7schen", ",", "k\u00fch\u00b7nem", "Sturz", "der", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Herden einsam l\u00e4uten an den Kl\u00fcften,", "tokens": ["Wo", "Her\u00b7den", "ein\u00b7sam", "l\u00e4u\u00b7ten", "an", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Habt ihr in eurer Berge heitern L\u00fcften", "tokens": ["Habt", "ihr", "in", "eu\u00b7rer", "Ber\u00b7ge", "hei\u00b7tern", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit Lebensatem eingesogen.", "tokens": ["Der", "Frei\u00b7heit", "Le\u00b7ben\u00b7sa\u00b7tem", "ein\u00b7ge\u00b7so\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Euch selbst die Retter, seid ihr ausgezogen,", "tokens": ["Euch", "selbst", "die", "Ret\u00b7ter", ",", "seid", "ihr", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie helle B\u00e4che brechen aus den Kl\u00fcften;", "tokens": ["Wie", "hel\u00b7le", "B\u00e4\u00b7che", "bre\u00b7chen", "aus", "den", "Kl\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinunter schwindelt T\u00fccke nach den Schl\u00fcften,", "tokens": ["Hin\u00b7un\u00b7ter", "schwin\u00b7delt", "T\u00fc\u00b7cke", "nach", "den", "Schl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit Burg sind eure Felsenbogen.", "tokens": ["Der", "Frei\u00b7heit", "Burg", "sind", "eu\u00b7re", "Fel\u00b7sen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hochherzig Volk, Genosse gr\u00f6\u00dfrer Zeiten!", "tokens": ["Hoch\u00b7her\u00b7zig", "Volk", ",", "Ge\u00b7nos\u00b7se", "gr\u00f6\u00df\u00b7rer", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sinkst nun in der eignen H\u00e4user Brande,", "tokens": ["Du", "sinkst", "nun", "in", "der", "eig\u00b7nen", "H\u00e4u\u00b7ser", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-