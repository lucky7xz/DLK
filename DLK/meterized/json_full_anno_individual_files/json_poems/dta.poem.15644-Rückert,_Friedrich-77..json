{"dta.poem.15644": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich schon einmal war, so hab' ichs nun vergessen;", "tokens": ["Wenn", "ich", "schon", "ein\u00b7mal", "war", ",", "so", "hab'", "ichs", "nun", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jetzt ich bin, werd' ich mich einst erinnern dessen?", "tokens": ["Was", "jetzt", "ich", "bin", ",", "werd'", "ich", "mich", "einst", "e\u00b7rin\u00b7nern", "des\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VAFIN", "$,", "VAFIN", "PPER", "PRF", "ADV", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ob ich mich dessen auch erinnre nicht, ich bin", "tokens": ["Ob", "ich", "mich", "des\u00b7sen", "auch", "e\u00b7rinn\u00b7re", "nicht", ",", "ich", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PDS", "ADV", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder der ich war, und bleib' es immerhin.", "tokens": ["Nicht", "min\u00b7der", "der", "ich", "war", ",", "und", "bleib'", "es", "im\u00b7mer\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie, wem durch Fieberglut erlosch Erinnerung,", "tokens": ["Wie", ",", "wem", "durch", "Fie\u00b7berg\u00b7lut", "er\u00b7losch", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "APPR", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht auf als neuer Mensch, und lebt von vorne jung;", "tokens": ["Steht", "auf", "als", "neu\u00b7er", "Mensch", ",", "und", "lebt", "von", "vor\u00b7ne", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "KOUS", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So kann der Geist, vom Sinnzerst\u00f6rer Tod genesen,", "tokens": ["So", "kann", "der", "Geist", ",", "vom", "Sinn\u00b7zer\u00b7st\u00f6\u00b7rer", "Tod", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wissend was er war, doch seyn was er gewesen.", "tokens": ["Nicht", "wis\u00b7send", "was", "er", "war", ",", "doch", "seyn", "was", "er", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PWS", "PPER", "VAFIN", "$,", "ADV", "VAINF", "PWS", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}