{"textgrid.poem.66174": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An General von Bouwinghausen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freudeweinend zu dem Dankaltar;", "tokens": ["Freu\u00b7de\u00b7wei\u00b7nend", "zu", "dem", "Dan\u00b7kal\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Auf mein Antlitz la\u00df mich mit dir fallen,", "tokens": ["Auf", "mein", "Ant\u00b7litz", "la\u00df", "mich", "mit", "dir", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Heut' am Fest, das dich gebar.", "tokens": ["Heut'", "am", "Fest", ",", "das", "dich", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nichts zu deinem Lobe will ich sagen,", "tokens": ["Nichts", "zu", "dei\u00b7nem", "Lo\u00b7be", "will", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weil dein Blick voll Demuth mir's verbeut;", "tokens": ["Weil", "dein", "Blick", "voll", "De\u00b7muth", "mir's", "ver\u00b7beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "NN", "NE", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wird durch Menschenlob entweiht.", "tokens": ["Wird", "durch", "Men\u00b7schen\u00b7lob", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber danken, la\u00df uns freudig danken,", "tokens": ["A\u00b7ber", "dan\u00b7ken", ",", "la\u00df", "uns", "freu\u00b7dig", "dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df der Freudentropfe niederst\u00fcrzt,", "tokens": ["Da\u00df", "der", "Freu\u00b7den\u00b7trop\u00b7fe", "nie\u00b7ders\u00b7t\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Noch hat Gott in deines Lebens Schranken", "tokens": ["Noch", "hat", "Gott", "in", "dei\u00b7nes", "Le\u00b7bens", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dir die Laufbahn nicht verk\u00fcrzt.", "tokens": ["Dir", "die", "Lauf\u00b7bahn", "nicht", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Danken wollen wir der Vaterg\u00fcte,", "tokens": ["Dan\u00b7ken", "wol\u00b7len", "wir", "der", "Va\u00b7ter\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die dich aus dem Mutterleibe ri\u00df;", "tokens": ["Die", "dich", "aus", "dem", "Mut\u00b7ter\u00b7lei\u00b7be", "ri\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dich selbst in Mahomets Gebiete", "tokens": ["Und", "dich", "selbst", "in", "Ma\u00b7ho\u00b7mets", "Ge\u00b7bie\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einst auf Jesum taufen lie\u00df.", "tokens": ["Einst", "auf", "Je\u00b7sum", "tau\u00b7fen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat dich Gott in deinen Bl\u00fctentagen,", "tokens": ["Hat", "dich", "Gott", "in", "dei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "V\u00e4terlich geleitet und getragen", "tokens": ["V\u00e4\u00b7ter\u00b7lich", "ge\u00b7lei\u00b7tet", "und", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In dein Biedervaterland?", "tokens": ["In", "dein", "Bie\u00b7der\u00b7va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hoch herunter st\u00fcrztest du als Knabe", "tokens": ["Hoch", "her\u00b7un\u00b7ter", "st\u00fcrz\u00b7test", "du", "als", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von der Zinne: doch ein Engel kam,", "tokens": ["Von", "der", "Zin\u00b7ne", ":", "doch", "ein", "En\u00b7gel", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der dich sch\u00fctzte vor dem nahen Grabe,", "tokens": ["Der", "dich", "sch\u00fctz\u00b7te", "vor", "dem", "na\u00b7hen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und in seine Fl\u00fcgel nahm.", "tokens": ["Und", "in", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Hat die Vorsicht nicht am G\u00e4ngelbande", "tokens": ["Hat", "die", "Vor\u00b7sicht", "nicht", "am", "G\u00e4n\u00b7gel\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mehr als deine Mutter dich gef\u00fchrt?", "tokens": ["Mehr", "als", "dei\u00b7ne", "Mut\u00b7ter", "dich", "ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dich erzogen deinem Vaterlande?", "tokens": ["Dich", "er\u00b7zo\u00b7gen", "dei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit Mannkraft dich geziert?", "tokens": ["Und", "mit", "Mann\u00b7kraft", "dich", "ge\u00b7ziert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zeigte dir der Retter aus Gefahren", "tokens": ["Zeig\u00b7te", "dir", "der", "Ret\u00b7ter", "aus", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht auf deines Lebens Ehrenbahn,", "tokens": ["Nicht", "auf", "dei\u00b7nes", "Le\u00b7bens", "Eh\u00b7ren\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df man auch im Kleide des Husaren", "tokens": ["Da\u00df", "man", "auch", "im", "Klei\u00b7de", "des", "Hu\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Christus' Liebe f\u00fchlen kann?", "tokens": ["Chris\u00b7tus'", "Lie\u00b7be", "f\u00fch\u00b7len", "kann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer hat dich in seinen Schutz genommen?", "tokens": ["Wer", "hat", "dich", "in", "sei\u00b7nen", "Schutz", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als du einst den S\u00e4bel muthig schwangst,", "tokens": ["Als", "du", "einst", "den", "S\u00e4\u00b7bel", "mut\u00b7hig", "schwangst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die Elbe zweimal hingeschwommen,", "tokens": ["Durch", "die", "El\u00b7be", "zwei\u00b7mal", "hin\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit Friedrichs Helden rangst.", "tokens": ["Und", "mit", "Fried\u00b7richs", "Hel\u00b7den", "rangst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "That's nicht Gott, dem Feind an dir zu weisen,", "tokens": ["That's", "nicht", "Gott", ",", "dem", "Feind", "an", "dir", "zu", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "NN", "$,", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df der Christ, voll wahrer Tapferkeit,", "tokens": ["Da\u00df", "der", "Christ", ",", "voll", "wah\u00b7rer", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht den furchtbarn S\u00e4belblitz der Preu\u00dfen,", "tokens": ["Nicht", "den", "furcht\u00b7barn", "S\u00e4\u00b7bel\u00b7blitz", "der", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht den Donner Friedrichs scheut?", "tokens": ["Nicht", "den", "Don\u00b7ner", "Fried\u00b7richs", "scheut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ohne, wie ein Neger, dich zu b\u00fccken,", "tokens": ["Oh\u00b7ne", ",", "wie", "ein", "Ne\u00b7ger", ",", "dich", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Warst du doch des F\u00fcrsten treuster Mann;", "tokens": ["Warst", "du", "doch", "des", "F\u00fcrs\u00b7ten", "treus\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht durch Sklavenbeugsamkeit im R\u00fccken,", "tokens": ["Nicht", "durch", "Skla\u00b7ven\u00b7beug\u00b7sam\u00b7keit", "im", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch dein Herz ihm unterthan.", "tokens": ["Durch", "dein", "Herz", "ihm", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sprich: wer half das Leben dir ertragen?", "tokens": ["Sprich", ":", "wer", "half", "das", "Le\u00b7ben", "dir", "er\u00b7tra\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWS", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wer erleichtert dir den Schmerz der Gicht?", "tokens": ["Wer", "er\u00b7leich\u00b7tert", "dir", "den", "Schmerz", "der", "Gicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn sie oft, wie Vipernz\u00e4hne nagen,", "tokens": ["Wenn", "sie", "oft", ",", "wie", "Vi\u00b7pern\u00b7z\u00e4h\u00b7ne", "na\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich in deine Sohlen sticht?", "tokens": ["Dich", "in", "dei\u00b7ne", "Soh\u00b7len", "sticht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Steht nicht mitten im Gef\u00fchl der Schmerzen,", "tokens": ["Steht", "nicht", "mit\u00b7ten", "im", "Ge\u00b7f\u00fchl", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach, der Blutende auf Golgatha,", "tokens": ["Ach", ",", "der", "Blu\u00b7ten\u00b7de", "auf", "Gol\u00b7ga\u00b7tha", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Steht er nicht vor deinem bangen Herzen", "tokens": ["Steht", "er", "nicht", "vor", "dei\u00b7nem", "ban\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der Duldermiene da?", "tokens": ["Mit", "der", "Dul\u00b7der\u00b7mie\u00b7ne", "da", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Christus' Beispiel giebt dir dann die Regel:", "tokens": ["Chris\u00b7tus'", "Bei\u00b7spiel", "giebt", "dir", "dann", "die", "Re\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leiden ist des Christen erste Pflicht!", "tokens": ["Lei\u00b7den", "ist", "des", "Chris\u00b7ten", "ers\u00b7te", "Pflicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O, dann achtest du den Stich der N\u00e4gel", "tokens": ["O", ",", "dann", "ach\u00b7test", "du", "den", "Stich", "der", "N\u00e4\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "troch