{"dta.poem.20102": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Treu-Lischen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emein Lischen, stell das Weinen ein,", "tokens": ["\u201e", "mein", "Li\u00b7schen", ",", "stell", "das", "Wei\u00b7nen", "ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Regen folgt ja Sonnenschein,", "tokens": ["Auf", "Re\u00b7gen", "folgt", "ja", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kehr\u2019 mit Schwalb\u2019 und Flieder", "tokens": ["Ich", "kehr'", "mit", "Schwalb'", "und", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wohl noch fr\u00fcher wieder.\u201c", "tokens": ["Und", "wohl", "noch", "fr\u00fc\u00b7her", "wie\u00b7der", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Bursche sprach\u2019s. Vom Giebeldach", "tokens": ["Der", "Bur\u00b7sche", "sprach'", "s.", "Vom", "Gie\u00b7bel\u00b7dach"], "token_info": ["word", "word", "word", "abbreviation", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ihm Treu-Lischen lange nach,", "tokens": ["Sah", "ihm", "Treu\u00b7Li\u00b7schen", "lan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis Hoffnung wiederkehrte", "tokens": ["Bis", "Hoff\u00b7nung", "wie\u00b7der\u00b7kehr\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihren Thr\u00e4nen wehrte.", "tokens": ["Und", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "wehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Aeuglein wurden wieder klar,", "tokens": ["Die", "A\u00b7e\u00b7u\u00b7glein", "wur\u00b7den", "wie\u00b7der", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Herze jeden Kummers bar, \u2014", "tokens": ["Das", "Her\u00b7ze", "je\u00b7den", "Kum\u00b7mers", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wu\u00dfte, mit dem Flieder", "tokens": ["Sie", "wu\u00df\u00b7te", ",", "mit", "dem", "Flie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kam ihr der Liebste wieder.", "tokens": ["Kam", "ihr", "der", "Liebs\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Fr\u00fchling kam mit Duft und Klang,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kam", "mit", "Duft", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Treu-Lischen harrte mondenlang,", "tokens": ["Treu\u00b7Li\u00b7schen", "harr\u00b7te", "mon\u00b7den\u00b7lang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herbstwind durchfuhr den Garten, \u2014", "tokens": ["Herbst\u00b7wind", "durch\u00b7fuhr", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vergeblich war ihr Warten.", "tokens": ["Ver\u00b7geb\u00b7lich", "war", "ihr", "War\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl kam der Fr\u00fchling viele Mal,", "tokens": ["Wohl", "kam", "der", "Fr\u00fch\u00b7ling", "vie\u00b7le", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Liebster nimmermehr in\u2019s Thal,", "tokens": ["Ihr", "Liebs\u00b7ter", "nim\u00b7mer\u00b7mehr", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Lenz um Lenz auf\u2019s Neue,", "tokens": ["Doch", "Lenz", "um", "Lenz", "auf's", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rief sie: \u201enun kommt der Treue!\u201c", "tokens": ["Rief", "sie", ":", "\u201e", "nun", "kommt", "der", "Treu\u00b7e", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es konnt ihr Herz, das Jahr um Jahr", "tokens": ["Es", "konnt", "ihr", "Herz", ",", "das", "Jahr", "um", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Liebsten treu geblieben war,", "tokens": ["Dem", "Liebs\u00b7ten", "treu", "ge\u00b7blie\u00b7ben", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es konnt\u2019s ihr Herz nicht fassen,", "tokens": ["Es", "konnt's", "ihr", "Herz", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er habe sie verlassen.", "tokens": ["Er", "ha\u00b7be", "sie", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Grau ward ihr Haar, welk ihr Gesicht,", "tokens": ["Grau", "ward", "ihr", "Haar", ",", "welk", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "PWAT", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das Alter kam, sie wu\u00dft\u2019 es nicht,", "tokens": ["Das", "Al\u00b7ter", "kam", ",", "sie", "wu\u00dft'", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Hoffen und ihr Lieben,", "tokens": ["Ihr", "Hof\u00b7fen", "und", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Herz war jung geblieben.", "tokens": ["Ihr", "Herz", "war", "jung", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und als der Tod sie heimgef\u00fchrt,", "tokens": ["Und", "als", "der", "Tod", "sie", "heim\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat ihn das treue Herz ger\u00fchrt,", "tokens": ["Hat", "ihn", "das", "treu\u00b7e", "Herz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit des Liebsten Mienen", "tokens": ["Und", "mit", "des", "Liebs\u00b7ten", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist er vor ihr erschienen.", "tokens": ["Ist", "er", "vor", "ihr", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}