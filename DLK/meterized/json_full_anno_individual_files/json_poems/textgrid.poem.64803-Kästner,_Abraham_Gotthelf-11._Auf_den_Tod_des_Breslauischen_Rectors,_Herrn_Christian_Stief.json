{"textgrid.poem.64803": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "11. Auf den Tod des Breslauischen Rectors, Herrn Christian Stief", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Einer Kenntni\u00df stets verzagt und tr\u00e4ge stehn,", "tokens": ["Bey", "Ei\u00b7ner", "Kennt\u00b7ni\u00df", "stets", "ver\u00b7zagt", "und", "tr\u00e4\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie lehrbegier'gen Blick nach andrer Wahrheit drehn,", "tokens": ["Nie", "lehr\u00b7be\u00b7gier'\u00b7gen", "Blick", "nach", "an\u00b7drer", "Wahr\u00b7heit", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wie der Thiere Witz nur Eine Kunst vollbringet,", "tokens": ["So", "wie", "der", "Thie\u00b7re", "Witz", "nur", "Ei\u00b7ne", "Kunst", "voll\u00b7brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Spinne nichts, als webt, die Lerche nichts, als singet,", "tokens": ["Die", "Spin\u00b7ne", "nichts", ",", "als", "webt", ",", "die", "Ler\u00b7che", "nichts", ",", "als", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "KOUS", "VVFIN", "$,", "ART", "NN", "PIS", "$,", "KOUS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kommt schlechten Seelen zu; erhabner Geister Kraft", "tokens": ["Kommt", "schlech\u00b7ten", "See\u00b7len", "zu", ";", "er\u00b7hab\u00b7ner", "Geis\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$.", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ersch\u00f6pft die Arbeit nicht von Einer Wissenschaft.", "tokens": ["Er\u00b7sch\u00f6pft", "die", "Ar\u00b7beit", "nicht", "von", "Ei\u00b7ner", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr Flei\u00df, den nichts vermag in engen Raum zu schr\u00e4nken,", "tokens": ["Ihr", "Flei\u00df", ",", "den", "nichts", "ver\u00b7mag", "in", "en\u00b7gen", "Raum", "zu", "schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durchreist der Wahrheit Reich, und denkt wo Menschen denken.", "tokens": ["Durc\u00b7hreist", "der", "Wahr\u00b7heit", "Reich", ",", "und", "denkt", "wo", "Men\u00b7schen", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "VVFIN", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So war, zu Breslau's Ruhm, der theure Greis gelehrt,", "tokens": ["So", "war", ",", "zu", "Bres\u00b7lau's", "Ruhm", ",", "der", "theu\u00b7re", "Greis", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "NE", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den es fast funfzig Jahr und noch zu kurz verehrt.", "tokens": ["Den", "es", "fast", "funf\u00b7zig", "Jahr", "und", "noch", "zu", "kurz", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "CARD", "NN", "KON", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu wenig war f\u00fcr ihn, der Weisen Sprachen kennen,", "tokens": ["Zu", "we\u00b7nig", "war", "f\u00fcr", "ihn", ",", "der", "Wei\u00b7sen", "Spra\u00b7chen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "APPR", "PPER", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Sch\u00e4tzung ihres Werths die gro\u00dfen Alten nennen;", "tokens": ["Mit", "Sch\u00e4t\u00b7zung", "ih\u00b7res", "Werths", "die", "gro\u00b7\u00dfen", "Al\u00b7ten", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch bey der Alten Werth der Neuern Werth verstehn,", "tokens": ["Noch", "bey", "der", "Al\u00b7ten", "Werth", "der", "Neu\u00b7ern", "Werth", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Schicksal manches Volks der Vorsicht Herrschaft sehn;", "tokens": ["Im", "Schick\u00b7sal", "man\u00b7ches", "Volks", "der", "Vor\u00b7sicht", "Herr\u00b7schaft", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch was Natur und Kunst f\u00fcr Wunderwerke zeigen,", "tokens": ["Auch", "was", "Na\u00b7tur", "und", "Kunst", "f\u00fcr", "Wun\u00b7der\u00b7wer\u00b7ke", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da, wo der Sch\u00f6pfer spricht, und B\u00fccher \u00f6fters schweigen;", "tokens": ["Da", ",", "wo", "der", "Sch\u00f6p\u00b7fer", "spricht", ",", "und", "B\u00fc\u00b7cher", "\u00f6f\u00b7ters", "schwei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie viel der kleine Mensch voll Ohnmacht sich erk\u00fchnt;", "tokens": ["Wie", "viel", "der", "klei\u00b7ne", "Mensch", "voll", "Ohn\u00b7macht", "sich", "er\u00b7k\u00fchnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ADJD", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie Himmel, Erd' und Meer nur seinem Witze dient;", "tokens": ["Wie", "Him\u00b7mel", ",", "Erd'", "und", "Meer", "nur", "sei\u00b7nem", "Wit\u00b7ze", "dient", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "War seiner Einsicht werth; er sah in tiefen Kl\u00fcften", "tokens": ["War", "sei\u00b7ner", "Ein\u00b7sicht", "werth", ";", "er", "sah", "in", "tie\u00b7fen", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des armen Bergmanns Flei\u00df; in Gluth und Schwefeld\u00fcften,", "tokens": ["Des", "ar\u00b7men", "Berg\u00b7manns", "Flei\u00df", ";", "in", "Gluth", "und", "Schwe\u00b7fel\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie Bley aus Steine flie\u00dft; mit n\u00fctzlichem Bem\u00fchn,", "tokens": ["Wie", "Bley", "aus", "Stei\u00b7ne", "flie\u00dft", ";", "mit", "n\u00fctz\u00b7li\u00b7chem", "Be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Lie\u00df er erst fremden Klee auf Breslau's Wiesen bl\u00fchn.", "tokens": ["Lie\u00df", "er", "erst", "frem\u00b7den", "Klee", "auf", "Bres\u00b7lau's", "Wie\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So ward durch seinen Flei\u00df auch Andrer Flei\u00df gelenket,", "tokens": ["So", "ward", "durch", "sei\u00b7nen", "Flei\u00df", "auch", "A\u00b7ndrer", "Flei\u00df", "ge\u00b7len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zu sehn, was Gott erschuf, zu brauchen, was er schenket.", "tokens": ["Zu", "sehn", ",", "was", "Gott", "er\u00b7schuf", ",", "zu", "brau\u00b7chen", ",", "was", "er", "schen\u00b7ket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vollkommner sieht er jetzt, von langem Dienst befreyt,", "tokens": ["Voll\u00b7komm\u00b7ner", "sieht", "er", "jetzt", ",", "von", "lan\u00b7gem", "Dienst", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sch\u00f6pfer unsrer Welt, als Herrn der Seligkeit.", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "uns\u00b7rer", "Welt", ",", "als", "Herrn", "der", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O m\u00f6chte, da wir ihn f\u00fcr uns noch fr\u00fch verlieren,", "tokens": ["O", "m\u00f6ch\u00b7te", ",", "da", "wir", "ihn", "f\u00fcr", "uns", "noch", "fr\u00fch", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur manchen Geist bey uns sein gro\u00dfes Beyspiel f\u00fchren!", "tokens": ["Nur", "man\u00b7chen", "Geist", "bey", "uns", "sein", "gro\u00b7\u00dfes", "Bey\u00b7spiel", "f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bey Einer Kenntni\u00df stets verzagt und tr\u00e4ge stehn,", "tokens": ["Bey", "Ei\u00b7ner", "Kennt\u00b7ni\u00df", "stets", "ver\u00b7zagt", "und", "tr\u00e4\u00b7ge", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie lehrbegier'gen Blick nach andrer Wahrheit drehn,", "tokens": ["Nie", "lehr\u00b7be\u00b7gier'\u00b7gen", "Blick", "nach", "an\u00b7drer", "Wahr\u00b7heit", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.