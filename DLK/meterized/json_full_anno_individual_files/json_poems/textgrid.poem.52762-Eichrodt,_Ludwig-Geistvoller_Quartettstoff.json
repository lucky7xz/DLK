s": ["Bei", "Wei\u00b7bern", ",", "sonst", "wie", "al\u00b7ler\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir sie recht gem\u00fcthlich bitten,", "tokens": ["Wenn", "wir", "sie", "recht", "ge\u00b7m\u00fcth\u00b7lich", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So rauchen Weiber selbst zum Scherz.", "tokens": ["So", "rau\u00b7chen", "Wei\u00b7ber", "selbst", "zum", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man k\u00f6nnte sich viel' Langweil' sparen.", "tokens": ["Man", "k\u00f6nn\u00b7te", "sich", "viel'", "Lang\u00b7weil'", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vermehrte man h\u00fcbsch die Cigarren!", "tokens": ["Ver\u00b7mehr\u00b7te", "man", "h\u00fcbsch", "die", "Ci\u00b7gar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}