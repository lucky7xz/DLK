{"textgrid.poem.52762": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Geistvoller Quartettstoff", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cigarren sind gar wack're Schluzer,", "tokens": ["Ci\u00b7gar\u00b7ren", "sind", "gar", "wack'\u00b7re", "Schlu\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Menschenkind ist ihnen hold,", "tokens": ["Das", "Men\u00b7schen\u00b7kind", "ist", "ih\u00b7nen", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der zierliche, galante Stutzer,", "tokens": ["Der", "zier\u00b7li\u00b7che", ",", "ga\u00b7lan\u00b7te", "Stut\u00b7zer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der roh' gemeine Trunkenbold,", "tokens": ["Der", "roh'", "ge\u00b7mei\u00b7ne", "Trun\u00b7ken\u00b7bold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die allerbesten Schluzer waren,", "tokens": ["Die", "al\u00b7ler\u00b7bes\u00b7ten", "Schlu\u00b7zer", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit man Tabak baut, die Cigarren.", "tokens": ["Seit", "man", "Ta\u00b7bak", "baut", ",", "die", "Ci\u00b7gar\u00b7ren", "."], "token_info": ["word", "word", "word"