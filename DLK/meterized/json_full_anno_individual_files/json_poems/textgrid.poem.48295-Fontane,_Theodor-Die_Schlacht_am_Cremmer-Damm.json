, "Gra\u00b7ben", "und", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NE", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dahinter sich zu verstecken.", "tokens": ["Da\u00b7hin\u00b7ter", "sich", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Markgraf Ludwig aber, der tapfere Held,", "tokens": ["Mark\u00b7graf", "Lud\u00b7wig", "a\u00b7ber", ",", "der", "tap\u00b7fe\u00b7re", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dr\u00fcben sah man ihn reiten,", "tokens": ["Dr\u00fc\u00b7ben", "sah", "man", "ihn", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Er dachte: \u203aDie Pommern stehen im Feld", "tokens": ["Er", "dach\u00b7te", ":", "\u203a", "Die", "Pom\u00b7mern", "ste\u00b7hen", "im", "Feld"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und werden den Damm \u00fcberschreiten.\u2039", "tokens": ["Und", "wer\u00b7den", "den", "Damm", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "