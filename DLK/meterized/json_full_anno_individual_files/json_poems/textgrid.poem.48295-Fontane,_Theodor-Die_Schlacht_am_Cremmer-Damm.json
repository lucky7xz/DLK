{"textgrid.poem.48295": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Schlacht am Cremmer-Damm", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als Herzog Barnim, der vielkleine Mann,", "tokens": ["Und", "als", "Her\u00b7zog", "Bar\u00b7nim", ",", "der", "viel\u00b7klei\u00b7ne", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Um mit Markgraf Ludwig zu fechten,", "tokens": ["Um", "mit", "Mark\u00b7graf", "Lud\u00b7wig", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NE", "NE", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "War bis an den Cremmer-Damm heran,", "tokens": ["War", "bis", "an", "den", "Crem\u00b7mer\u00b7Damm", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sprach er zu Rittern und Knechten:", "tokens": ["Sprach", "er", "zu", "Rit\u00b7tern", "und", "Knech\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbdas Cremmer Luch ist ein garstig Loch,", "tokens": ["\u00bb", "das", "Crem\u00b7mer", "Luch", "ist", "ein", "gars\u00b7tig", "Loch", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NE", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und den Feind daraus zu vertreiben,", "tokens": ["Und", "den", "Feind", "da\u00b7raus", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich denke, Leute, wir lassen's noch", "tokens": ["Ich", "den\u00b7ke", ",", "Leu\u00b7te", ",", "wir", "las\u00b7sen's", "noch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wollen diesseits bleiben.", "tokens": ["Und", "wol\u00b7len", "dies\u00b7seits", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir schreiben aus eine gro\u00dfe Steu'r,", "tokens": ["Wir", "schrei\u00b7ben", "aus", "ei\u00b7ne", "gro\u00b7\u00dfe", "Steu'r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wer sich nicht will bequemen,", "tokens": ["Und", "wer", "sich", "nicht", "will", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Den zwingen wir mit Wasser und Feu'r", "tokens": ["Den", "zwin\u00b7gen", "wir", "mit", "Was\u00b7ser", "und", "Feu'r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und wollen das Vieh ihm nehmen.\u00ab", "tokens": ["Und", "wol\u00b7len", "das", "Vieh", "ihm", "neh\u00b7men", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Rat gefiel den Pommern all,", "tokens": ["Der", "Rat", "ge\u00b7fiel", "den", "Pom\u00b7mern", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und verquer an den beiden Ecken", "tokens": ["Und", "ver\u00b7quer", "an", "den", "bei\u00b7den", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "PIAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gruben sie hastig Graben und Wall,", "tokens": ["Gru\u00b7ben", "sie", "has\u00b7tig", "Gra\u00b7ben", "und", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NE", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dahinter sich zu verstecken.", "tokens": ["Da\u00b7hin\u00b7ter", "sich", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Markgraf Ludwig aber, der tapfere Held,", "tokens": ["Mark\u00b7graf", "Lud\u00b7wig", "a\u00b7ber", ",", "der", "tap\u00b7fe\u00b7re", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Dr\u00fcben sah man ihn reiten,", "tokens": ["Dr\u00fc\u00b7ben", "sah", "man", "ihn", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Er dachte: \u203aDie Pommern stehen im Feld", "tokens": ["Er", "dach\u00b7te", ":", "\u203a", "Die", "Pom\u00b7mern", "ste\u00b7hen", "im", "Feld"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und werden den Damm \u00fcberschreiten.\u2039", "tokens": ["Und", "wer\u00b7den", "den", "Damm", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Als aber keiner sich's unterwand,", "tokens": ["Als", "a\u00b7ber", "kei\u00b7ner", "sich's", "un\u00b7ter\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lie\u00df er seinen Trompeter kommen", "tokens": ["Lie\u00df", "er", "sei\u00b7nen", "Trom\u00b7pe\u00b7ter", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und sagte: \u00bbNimm deine Trompet in die Hand", "tokens": ["Und", "sag\u00b7te", ":", "\u00bb", "Nimm", "dei\u00b7ne", "Trom\u00b7pet", "in", "die", "Hand"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und blas, bis sie's dr\u00fcben vernommen.", "tokens": ["Und", "blas", ",", "bis", "sie's", "dr\u00fc\u00b7ben", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und sage dem Herzog Barnim an,", "tokens": ["Und", "sa\u00b7ge", "dem", "Her\u00b7zog", "Bar\u00b7nim", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich h\u00e4tte gro\u00df Verlangen,", "tokens": ["Ich", "h\u00e4t\u00b7te", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn und seine Ritter, Mann f\u00fcr Mann,", "tokens": ["Ihn", "und", "sei\u00b7ne", "Rit\u00b7ter", ",", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hier diesseits zu empfangen.", "tokens": ["Hier", "dies\u00b7seits", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Und wenn es hier diesseits ihm nicht behagt,", "tokens": ["Und", "wenn", "es", "hier", "dies\u00b7seits", "ihm", "nicht", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wollt' ich ihm versprechen,", "tokens": ["So", "wollt'", "ich", "ihm", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch auf dem Luch-Damm unverzagt", "tokens": ["Auch", "auf", "dem", "Luch\u00b7Damm", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine Lanze mit ihm zu brechen.\u00ab", "tokens": ["Ei\u00b7ne", "Lan\u00b7ze", "mit", "ihm", "zu", "bre\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Drauf ", "tokens": ["Drauf"], "token_info": ["word"], "pos": ["PAV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Sie wollten sich treffen und wollten sehn,", "tokens": ["Sie", "woll\u00b7ten", "sich", "tref\u00b7fen", "und", "woll\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wer das Spiel am besten verst\u00fcnde.", "tokens": ["Wer", "das", "Spiel", "am", "bes\u00b7ten", "ver\u00b7st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Da ging es vom Graben den Damm hinauf,", "tokens": ["Da", "ging", "es", "vom", "Gra\u00b7ben", "den", "Damm", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Drauf standen dicht die M\u00e4rker,", "tokens": ["Drauf", "stan\u00b7den", "dicht", "die", "M\u00e4r\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die wehrten sich einzeln und zu Hauf,", "tokens": ["Die", "wehr\u00b7ten", "sich", "ein\u00b7zeln", "und", "zu", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "KON", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber Herzog Barnim war st\u00e4rker.", "tokens": ["A\u00b7ber", "Her\u00b7zog", "Bar\u00b7nim", "war", "st\u00e4r\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Die M\u00e4rkischen konnten nicht bestahn,", "tokens": ["Die", "M\u00e4r\u00b7ki\u00b7schen", "konn\u00b7ten", "nicht", "be\u00b7stahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Luch war ihr Verderben,", "tokens": ["Das", "Luch", "war", "ihr", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und viele mu\u00dften da liegen gahn", "tokens": ["Und", "vie\u00b7le", "mu\u00df\u00b7ten", "da", "lie\u00b7gen", "gahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "ADV", "VVFIN", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ohne Wunde sterben.", "tokens": ["Und", "oh\u00b7ne", "Wun\u00b7de", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und m\u00e4hlich wichen sie Schritt f\u00fcr Schritt,", "tokens": ["Und", "m\u00e4h\u00b7lich", "wi\u00b7chen", "sie", "Schritt", "f\u00fcr", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Cremmen weiter zu fechten,", "tokens": ["Vor", "Crem\u00b7men", "wei\u00b7ter", "zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Pommern folgten in festem Tritt,", "tokens": ["Die", "Pom\u00b7mern", "folg\u00b7ten", "in", "fes\u00b7tem", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Ritter mitsamt den Knechten.", "tokens": ["Die", "Rit\u00b7ter", "mit\u00b7samt", "den", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Aber vor Cremmen hielt man an", "tokens": ["A\u00b7ber", "vor", "Crem\u00b7men", "hielt", "man", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIS", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und mu\u00dfte drau\u00dfen bleiben,", "tokens": ["Und", "mu\u00df\u00b7te", "drau\u00b7\u00dfen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00e4rkischen standen da Mann an Mann", "tokens": ["Die", "M\u00e4r\u00b7ki\u00b7schen", "stan\u00b7den", "da", "Mann", "an", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und waren nicht zu vertreiben.", "tokens": ["Und", "wa\u00b7ren", "nicht", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Sie schossen hinunter aus Turm und Tor", "tokens": ["Sie", "schos\u00b7sen", "hin\u00b7un\u00b7ter", "aus", "Turm", "und", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In das pommersche Gedr\u00e4nge,", "tokens": ["In", "das", "pom\u00b7mer\u00b7sche", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann dr\u00e4ngten sie selber wieder vor,", "tokens": ["Dann", "dr\u00e4ng\u00b7ten", "sie", "