{"dta.poem.10342": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ein meiner Brust, da sitzt ein Weh,", "tokens": ["\u201e", "in", "mei\u00b7ner", "Brust", ",", "da", "sitzt", "ein", "Weh", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das will die Brust zersprengen;", "tokens": ["Das", "will", "die", "Brust", "zer\u00b7spren\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo ich steh' und wo ich geh',", "tokens": ["Und", "wo", "ich", "steh'", "und", "wo", "ich", "geh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will's mich von hinnen dr\u00e4ngen.", "tokens": ["Will's", "mich", "von", "hin\u00b7nen", "dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es treibt mich nach der Liebsten N\u00e4h',", "tokens": ["Es", "treibt", "mich", "nach", "der", "Liebs\u00b7ten", "N\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als k\u00f6nnt's die Grete heilen;", "tokens": ["Als", "k\u00f6nnt's", "die", "Gre\u00b7te", "hei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch wenn ich der in's Auge seh',", "tokens": ["Doch", "wenn", "ich", "der", "in's", "Au\u00b7ge", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df ich von hinnen eilen.", "tokens": ["Mu\u00df", "ich", "von", "hin\u00b7nen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich steig' hinauf des Berges H\u00f6h',", "tokens": ["Ich", "steig'", "hin\u00b7auf", "des", "Ber\u00b7ges", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort ist man doch alleine;", "tokens": ["Dort", "ist", "man", "doch", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich still dort oben steh',", "tokens": ["Und", "wenn", "ich", "still", "dort", "o\u00b7ben", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann steh' ich still und weine.\u201c", "tokens": ["Dann", "steh'", "ich", "still", "und", "wei\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}