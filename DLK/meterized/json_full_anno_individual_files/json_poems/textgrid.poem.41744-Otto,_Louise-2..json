{"textgrid.poem.41744": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch geschah's \u2013 die Ernte ging zu Ende,", "tokens": ["Und", "doch", "ge\u00b7scha\u00b7h's", "\u2013", "die", "Ern\u00b7te", "ging", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "$(", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im stillen Dorf beim letzten Abendrot.", "tokens": ["Im", "stil\u00b7len", "Dorf", "beim", "letz\u00b7ten", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch dort \u2013 doch dort gab es noch flei\u00dfge H\u00e4nde,", "tokens": ["Doch", "dort", "\u2013", "doch", "dort", "gab", "es", "noch", "flei\u00df\u00b7ge", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und eine andre Ernte hielt der Tod.", "tokens": ["Und", "ei\u00b7ne", "and\u00b7re", "Ern\u00b7te", "hielt", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Sonne hat es wohl voraus gesagt", "tokens": ["Die", "Son\u00b7ne", "hat", "es", "wohl", "vo\u00b7raus", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und hat die Nacht als blutig schon verklagt,", "tokens": ["Und", "hat", "die", "Nacht", "als", "blu\u00b7tig", "schon", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "KOKOM", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als mit dem Purpurmantel weit umhangen,", "tokens": ["Als", "mit", "dem", "Pur\u00b7pur\u00b7man\u00b7tel", "weit", "um\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Der sch\u00f6ne Tag zur finstern Ruh gegangen.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Tag", "zur", "fins\u00b7tern", "Ruh", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Traun, dieser Nacht, da gab's nicht sanfte Tr\u00e4ume,", "tokens": ["Traun", ",", "die\u00b7ser", "Nacht", ",", "da", "gab's", "nicht", "sanf\u00b7te", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDAT", "NN", "$,", "KOUS", "NE", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ward ein Schauerdrama aufgef\u00fchrt \u2013", "tokens": ["Es", "ward", "ein", "Schau\u00b7er\u00b7dra\u00b7ma", "auf\u00b7ge\u00b7f\u00fchrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da gab's viel Volk und weite B\u00fchnenr\u00e4ume,", "tokens": ["Da", "gab's", "viel", "Volk", "und", "wei\u00b7te", "B\u00fch\u00b7nen\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und manche Brust im innersten ger\u00fchrt,", "tokens": ["Und", "man\u00b7che", "Brust", "im", "in\u00b7ners\u00b7ten", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und manches Herz, das pl\u00f6tzlich stille stand,", "tokens": ["Und", "man\u00b7ches", "Herz", ",", "das", "pl\u00f6tz\u00b7lich", "stil\u00b7le", "stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und manche Seele, die zum Himmel schwand,", "tokens": ["Und", "man\u00b7che", "See\u00b7le", ",", "die", "zum", "Him\u00b7mel", "schwand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und manchen Schrei, der, wenn auch hier verwehret,", "tokens": ["Und", "man\u00b7chen", "Schrei", ",", "der", ",", "wenn", "auch", "hier", "ver\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "$,", "KOUS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vor Gottes Throne ward gewi\u00df geh\u00f6ret.", "tokens": ["Vor", "Got\u00b7tes", "Thro\u00b7ne", "ward", "ge\u00b7wi\u00df", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das war kein Girren holder Nachtigallen,", "tokens": ["Das", "war", "kein", "Gir\u00b7ren", "hol\u00b7der", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Heimchenzirpen, das so sp\u00e4t erklang!", "tokens": ["Kein", "Heim\u00b7chen\u00b7zir\u00b7pen", ",", "das", "so", "sp\u00e4t", "er\u00b7klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Hilferuf h\u00f6rt man zum Himmel schallen,", "tokens": ["Nur", "Hil\u00b7fe\u00b7ruf", "h\u00f6rt", "man", "zum", "Him\u00b7mel", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sch\u00f6nste Lied war manches Schwanensang.", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Lied", "war", "man\u00b7ches", "Schwa\u00b7nen\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sch\u00f6nste Lied \u2013 Ihr macht es nicht zum Spott,", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Lied", "\u2013", "Ihr", "macht", "es", "nicht", "zum", "Spott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn: \u00bb", "tokens": ["Denn", ":", "\u00bb"], "token_info": ["word", "punct", "punct"], "pos": ["KON", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und lie\u00df er auch die n\u00e4cht'ge That geschehen,", "tokens": ["Und", "lie\u00df", "er", "auch", "die", "n\u00e4cht'\u00b7ge", "That", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir bleiben doch in dem Vertrauen stehen.", "tokens": ["Wir", "blei\u00b7ben", "doch", "in", "dem", "Ver\u00b7trau\u00b7en", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Mondessichel schied vom Firmamente,", "tokens": ["Die", "Mon\u00b7des\u00b7si\u00b7chel", "schied", "vom", "Fir\u00b7ma\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sterne wandeln den gewohnten Gang,", "tokens": ["Die", "Ster\u00b7ne", "wan\u00b7deln", "den", "ge\u00b7wohn\u00b7ten", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sahn herab auf hoch erhobne H\u00e4nde.", "tokens": ["Sie", "sahn", "her\u00b7ab", "auf", "hoch", "er\u00b7hob\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Jammerruf, der sich der Brust entrang,", "tokens": ["Zum", "Jam\u00b7mer\u00b7ruf", ",", "der", "sich", "der", "Brust", "ent\u00b7rang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Nacht hat wohl f\u00fcr Klagelieder Raum,", "tokens": ["Die", "Nacht", "hat", "wohl", "f\u00fcr", "Kla\u00b7ge\u00b7lie\u00b7der", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch keinen mehr zu einem sanften Traum,", "tokens": ["Doch", "kei\u00b7nen", "mehr", "zu", "ei\u00b7nem", "sanf\u00b7ten", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbein' feste Burg ist unser Gott!\u00ab t\u00f6nt's wieder,", "tokens": ["\u00bb", "ein'", "fes\u00b7te", "Burg", "ist", "un\u00b7ser", "Gott", "!", "\u00ab", "t\u00f6nt's", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$(", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text"