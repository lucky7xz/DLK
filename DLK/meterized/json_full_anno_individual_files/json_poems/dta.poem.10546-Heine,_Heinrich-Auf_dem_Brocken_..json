{"dta.poem.10546": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Brocken .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Heller wird es schon im Osten", "tokens": ["Hel\u00b7ler", "wird", "es", "schon", "im", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Sonne kleines Glimmen,", "tokens": ["Durch", "der", "Son\u00b7ne", "klei\u00b7nes", "Glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weit und breit die Bergesgipfel", "tokens": ["Weit", "und", "breit", "die", "Ber\u00b7ges\u00b7gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Nebelmeere schwimmen.", "tokens": ["In", "dem", "Ne\u00b7bel\u00b7mee\u00b7re", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' ich Siebenmeilenstiefel,", "tokens": ["H\u00e4tt'", "ich", "Sie\u00b7ben\u00b7mei\u00b7len\u00b7stie\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lief ich mit der Hast des Windes,", "tokens": ["Lief", "ich", "mit", "der", "Hast", "des", "Win\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber jene Bergesgipfel,", "tokens": ["Ue\u00b7ber", "je\u00b7ne", "Ber\u00b7ges\u00b7gip\u00b7fel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Haus des lieben Kindes.", "tokens": ["Nach", "dem", "Haus", "des", "lie\u00b7ben", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von dem Bettchen, wo sie schlummert,", "tokens": ["Von", "dem", "Bett\u00b7chen", ",", "wo", "sie", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Z\u00f6g' ich leise die Gardinen,", "tokens": ["Z\u00f6g'", "ich", "lei\u00b7se", "die", "Gar\u00b7di\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise k\u00fc\u00dft' ich ihre Stirne,", "tokens": ["Lei\u00b7se", "k\u00fc\u00dft'", "ich", "ih\u00b7re", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leise ihres Munds Rubinen.", "tokens": ["Lei\u00b7se", "ih\u00b7res", "Munds", "Ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und noch leiser wollt' ich fl\u00fcstern", "tokens": ["Und", "noch", "lei\u00b7ser", "wollt'", "ich", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die kleinen Lilien-Ohren:", "tokens": ["In", "die", "klei\u00b7nen", "Li\u00b7li\u00b7en\u00b7Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denk' im Traum, da\u00df wir uns lieben,", "tokens": ["Denk'", "im", "Traum", ",", "da\u00df", "wir", "uns", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da\u00df wir uns nie verloren.", "tokens": ["Und", "da\u00df", "wir", "uns", "nie", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}