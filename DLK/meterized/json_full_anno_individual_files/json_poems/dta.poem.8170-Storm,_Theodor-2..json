{"dta.poem.8170": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df so wie sonst die Sonne lacht;", "tokens": ["Da\u00df", "so", "wie", "sonst", "die", "Son\u00b7ne", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KOKOM", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df wie in deinen Lebenstagen", "tokens": ["Da\u00df", "wie", "in", "dei\u00b7nen", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Uhren gehn, die Glocken schlagen,", "tokens": ["Die", "Uh\u00b7ren", "gehn", ",", "die", "Glo\u00b7cken", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einf\u00f6rmig wechseln Tag und Nacht;", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "wech\u00b7seln", "Tag", "und", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wenn des Tages Lichter schwanden,", "tokens": ["Da\u00df", ",", "wenn", "des", "Ta\u00b7ges", "Lich\u00b7ter", "schwan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sonst der Abend uns vereint;", "tokens": ["Wie", "sonst", "der", "A\u00b7bend", "uns", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df, wo sonst dein Stuhl gestanden,", "tokens": ["Und", "da\u00df", ",", "wo", "sonst", "dein", "Stuhl", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schon Andre ihre Pl\u00e4tze fanden,", "tokens": ["Schon", "And\u00b7re", "ih\u00b7re", "Pl\u00e4t\u00b7ze", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nichts dich zu vermissen scheint;", "tokens": ["Und", "nichts", "dich", "zu", "ver\u00b7mis\u00b7sen", "scheint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Indessen von den Gitterst\u00e4ben", "tokens": ["In\u00b7des\u00b7sen", "von", "den", "Git\u00b7ter\u00b7st\u00e4\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mondesstreifen schmal und karg", "tokens": ["Die", "Mon\u00b7des\u00b7strei\u00b7fen", "schmal", "und", "karg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deine Gruft hinunterweben,", "tokens": ["In", "dei\u00b7ne", "Gruft", "hin\u00b7un\u00b7ter\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit gespenstisch tr\u00fcbem Leben", "tokens": ["Und", "mit", "ge\u00b7spens\u00b7tisch", "tr\u00fc\u00b7bem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hinwandeln \u00fcber deinen Sarg.", "tokens": ["Hin\u00b7wan\u00b7deln", "\u00fc\u00b7ber", "dei\u00b7nen", "Sarg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}