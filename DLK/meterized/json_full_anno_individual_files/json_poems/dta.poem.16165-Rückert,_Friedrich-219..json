{"dta.poem.16165": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "219.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich ehr' ich, wenn du nie verwechselt Zweck und Mittel;", "tokens": ["Dich", "ehr'", "ich", ",", "wenn", "du", "nie", "ver\u00b7wech\u00b7selt", "Zweck", "und", "Mit\u00b7tel", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Anspruch hast du dann auf h\u00f6chsten Ehrentitel,", "tokens": ["Doch", "An\u00b7spruch", "hast", "du", "dann", "auf", "h\u00f6chs\u00b7ten", "Eh\u00b7ren\u00b7ti\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn, was als eigner Zweck gen\u00fcgend w\u00e4r' erschienen,", "tokens": ["Wenn", ",", "was", "als", "eig\u00b7ner", "Zweck", "ge\u00b7n\u00fc\u00b7gend", "w\u00e4r'", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "KOUS", "ADJA", "NN", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Mittel sich erweist dem h\u00f6hern Zweck zu dienen.", "tokens": ["Als", "Mit\u00b7tel", "sich", "er\u00b7weist", "dem", "h\u00f6\u00b7hern", "Zweck", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos"