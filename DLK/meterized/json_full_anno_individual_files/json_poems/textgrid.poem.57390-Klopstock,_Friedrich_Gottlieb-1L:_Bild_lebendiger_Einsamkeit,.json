{"textgrid.poem.57390": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bild lebendiger Einsamkeit,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bild lebendiger Einsamkeit,", "tokens": ["Bild", "le\u00b7ben\u00b7di\u00b7ger", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schwebe n\u00e4her! Sie ist, die sie war,", "tokens": ["Schwe\u00b7be", "n\u00e4\u00b7her", "!", "Sie", "ist", ",", "die", "sie", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da ich einst sie genoss, da ich voll Glut", "tokens": ["Da", "ich", "einst", "sie", "ge\u00b7noss", ",", "da", "ich", "voll", "Glut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dichtete, ordnete,", "tokens": ["Dich\u00b7te\u00b7te", ",", "ord\u00b7ne\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Seelen gab dem Erfundenen,", "tokens": ["See\u00b7len", "gab", "dem", "Er\u00b7fun\u00b7de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ihnen t\u00f6nenden Leib. T\u00f6nte der", "tokens": ["Ih\u00b7nen", "t\u00f6\u00b7nen\u00b7den", "Leib", ".", "T\u00f6n\u00b7te", "der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "NN", "ART"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie nur an; so erschien leere Gestalt,", "tokens": ["Sie", "nur", "an", ";", "so", "er\u00b7schien", "lee\u00b7re", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie in Elysium", "tokens": ["Wie", "in", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Irrt der Schemen, an Lethe's Strom", "tokens": ["Irrt", "der", "Sche\u00b7men", ",", "an", "Lethe's", "Strom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigend flattert. Get\u00f6nt waren sie", "tokens": ["Schwei\u00b7gend", "flat\u00b7tert", ".", "Ge\u00b7t\u00f6nt", "wa\u00b7ren", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$.", "VVPP", "VAFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jene Seelen, der Leib sank nicht, wie du", "tokens": ["Je\u00b7ne", "See\u00b7len", ",", "der", "Leib", "sank", "nicht", ",", "wie", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schatten Euridize,", "tokens": ["Schat\u00b7ten", "Eu\u00b7ri\u00b7di\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Dort hinab, und ich klagete", "tokens": ["Dort", "hin\u00b7ab", ",", "und", "ich", "kla\u00b7ge\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht dem schwindenden nach. Denn gew\u00e4hlt", "tokens": ["Nicht", "dem", "schwin\u00b7den\u00b7den", "nach", ".", "Denn", "ge\u00b7w\u00e4hlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "PTKVZ", "$.", "KON", "VVPP"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hatt' ich Leiber, die voll gleichender Kraft,", "tokens": ["Hatt'", "ich", "Lei\u00b7ber", ",", "die", "voll", "glei\u00b7chen\u00b7der", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Treffend gestalteten;", "tokens": ["Tref\u00b7fend", "ge\u00b7stal\u00b7te\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Hatte, suchend im alten Hain", "tokens": ["Hat\u00b7te", ",", "su\u00b7chend", "im", "al\u00b7ten", "Hain"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Thuiskona's, vom Stamm' hergef\u00fchrt", "tokens": ["Thuis\u00b7ko\u00b7na's", ",", "vom", "Stamm'", "her\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Neue Leiber, wenn mir w\u00fcrdig der Wahl", "tokens": ["Neu\u00b7e", "Lei\u00b7ber", ",", "wenn", "mir", "w\u00fcr\u00b7dig", "der", "Wahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Keiner im Walde schien.", "tokens": ["Kei\u00b7ner", "im", "Wal\u00b7de", "schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Nothdurft war mir diess, war nicht Plan,", "tokens": ["Noth\u00b7durft", "war", "mir", "diess", ",", "war", "nicht", "Plan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PDS", "$,", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wenn es mir nicht gelang; zog ich vor", "tokens": ["Wenn", "es", "mir", "nicht", "ge\u00b7lang", ";", "zog", "ich", "vor"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$.", "VVFIN", "PPER", "APPR"], "meter": "+--+-++-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zu verstummen, und warf schnell in die Glut", "tokens": ["Zu", "ver\u00b7stum\u00b7men", ",", "und", "warf", "schnell", "in", "die", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "'s Blatt, so nur stammelte.", "tokens": ["'s", "Blatt", ",", "so", "nur", "stam\u00b7mel\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Bild lebendiger Einsamkeit,", "tokens": ["Bild", "le\u00b7ben\u00b7di\u00b7ger", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Flieh nicht, weile! Sie ist, die sie einst", "tokens": ["Flieh", "nicht", ",", "wei\u00b7le", "!", "Sie", "ist", ",", "die", "sie", "einst"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "VVFIN", "$.", "PPER", "VAFIN", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "War im ersten Genuss, da ich erfand", "tokens": ["War", "im", "ers\u00b7ten", "Ge\u00b7nuss", ",", "da", "ich", "er\u00b7fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seelen, und T\u00f6ne gab;", "tokens": ["See\u00b7len", ",", "und", "T\u00f6\u00b7ne", "gab", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Weile, weile! In ihr durchdrang", "tokens": ["Wei\u00b7le", ",", "wei\u00b7le", "!", "In", "ihr", "durch\u00b7drang"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Frohes innig gef\u00fchlt den, der k\u00fchn", "tokens": ["Fro\u00b7hes", "in\u00b7nig", "ge\u00b7f\u00fchlt", "den", ",", "der", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "ADJD", "VVPP", "ART", "$,", "PRELS", "ADJD"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In der Dichtenden H\u00f6hn schwebte, durchdrang", "tokens": ["In", "der", "Dich\u00b7ten\u00b7den", "H\u00f6hn", "schweb\u00b7te", ",", "durch\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wonne den feyrenden!", "tokens": ["Won\u00b7ne", "den", "fey\u00b7ren\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "O der Wonne! Ich h\u00e4tte sie", "tokens": ["O", "der", "Won\u00b7ne", "!", "Ich", "h\u00e4t\u00b7te", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "PPER", "VAFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Da selbst, als sie mir ward, durch das Lied", "tokens": ["Da", "selbst", ",", "als", "sie", "mir", "ward", ",", "durch", "das", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nicht erreicht. Sie ergriff m\u00e4chtiges Arms,", "tokens": ["Nicht", "er\u00b7reicht", ".", "Sie", "er\u00b7griff", "m\u00e4ch\u00b7ti\u00b7ges", "Arms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Riss wie in Str\u00f6men fort!", "tokens": ["Riss", "wie", "in", "Str\u00f6\u00b7men", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Bild lebendiger Einsamkeit,", "tokens": ["Bild", "le\u00b7ben\u00b7di\u00b7ger", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schwebe n\u00e4her! Sie ist, die sie war,", "tokens": ["Schwe\u00b7be", "n\u00e4\u00b7her", "!", "Sie", "ist", ",", "die", "sie", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da ich einst sie genoss, da ich voll Glut", "tokens": ["Da", "ich", "einst", "sie", "ge\u00b7noss", ",", "da", "ich", "voll", "Glut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dichtete, ordnete,", "tokens": ["Dich\u00b7te\u00b7te", ",", "ord\u00b7ne\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Seelen gab dem Erfundenen,", "tokens": ["See\u00b7len", "gab", "dem", "Er\u00b7fun\u00b7de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ihnen t\u00f6nenden Leib. T\u00f6nte der", "tokens": ["Ih\u00b7nen", "t\u00f6\u00b7nen\u00b7den", "Leib", ".", "T\u00f6n\u00b7te", "der"], "token_info": ["wor