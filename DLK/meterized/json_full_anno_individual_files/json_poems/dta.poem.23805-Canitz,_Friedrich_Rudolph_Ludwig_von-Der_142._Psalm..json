{"dta.poem.23805": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Der 142. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schrey/ O HErr/ aus Ungedult und Schmertzen/", "tokens": ["Ich", "schrey", "/", "O", "Herr", "/", "aus", "Un\u00b7ge\u00b7dult", "und", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "NN", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich flehe dich nur an/ und sch\u00fctte von dem Hertzen", "tokens": ["Ich", "fle\u00b7he", "dich", "nur", "an", "/", "und", "sch\u00fct\u00b7te", "von", "dem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was mich so hefftig dr\u00fcckt/ in deinen Vater Schoo\u00df/", "tokens": ["Was", "mich", "so", "heff\u00b7tig", "dr\u00fcckt", "/", "in", "dei\u00b7nen", "Va\u00b7ter", "Schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du hast mich ja wol ehr gemacht von Aengsten lo\u00df.", "tokens": ["Du", "hast", "mich", "ja", "wol", "ehr", "ge\u00b7macht", "von", "A\u00b7engs\u00b7ten", "lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "NN", "VVPP", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Itzt thu ich keinen Schritt so find ich neue Stricke/", "tokens": ["Itzt", "thu", "ich", "kei\u00b7nen", "Schritt", "so", "find", "ich", "neu\u00b7e", "Stri\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beruf ich mich auf Recht/ so st\u00f6\u00dft man mich zur\u00fccke.", "tokens": ["Be\u00b7ruf", "ich", "mich", "auf", "Recht", "/", "so", "st\u00f6\u00dft", "man", "mich", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "NN", "$(", "ADV", "VVFIN", "PIS", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Flucht ist mir gesperrt. Die Seele leidet Noth/", "tokens": ["Die", "Flucht", "ist", "mir", "ge\u00b7sperrt", ".", "Die", "See\u00b7le", "lei\u00b7det", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "ART", "NN", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist mein Lebens-Theil/ und ich bin gleichsam todt.", "tokens": ["Du", "bist", "mein", "Le\u00b7bens\u00b7T\u00b7heil", "/", "und", "ich", "bin", "gleich\u00b7sam", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "KON", "PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich traue ja auf dich/ wie kanst du mich vergessen/", "tokens": ["Ich", "trau\u00b7e", "ja", "auf", "dich", "/", "wie", "kanst", "du", "mich", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "PWAV", "VMFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Herr h\u00f6re mein Geschrey/ die Feinde die mich pressen/", "tokens": ["Herr", "h\u00f6\u00b7re", "mein", "Ge\u00b7schrey", "/", "die", "Fein\u00b7de", "die", "mich", "pres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind mir sonst gar zu starck. Rei\u00df doch das Band", "tokens": ["Sind", "mir", "sonst", "gar", "zu", "starck", ".", "Rei\u00df", "doch", "das", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Das meine Seele sp\u00fchrt/ so r\u00fchm ich deine Treu.", "tokens": ["Das", "mei\u00b7ne", "See\u00b7le", "sp\u00fchrt", "/", "so", "r\u00fchm", "ich", "dei\u00b7ne", "Treu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und thust du mir itzt wohl/ so werden alle Frommen/", "tokens": ["Und", "thust", "du", "mir", "itzt", "wohl", "/", "so", "wer\u00b7den", "al\u00b7le", "From\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$(", "ADV", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An mir ein Zeichen sehn und freudig zu dir kommen.", "tokens": ["An", "mir", "ein", "Zei\u00b7chen", "sehn", "und", "freu\u00b7dig", "zu", "dir", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}