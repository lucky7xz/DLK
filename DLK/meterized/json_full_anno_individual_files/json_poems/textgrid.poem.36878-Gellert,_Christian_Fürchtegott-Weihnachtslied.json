: ["NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf den die V\u00e4ter hoffend sahn,", "tokens": ["Auf", "den", "die", "V\u00e4\u00b7ter", "hof\u00b7fend", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, Gott, Messias, bet ich an.", "tokens": ["Dich", ",", "Gott", ",", "Mes\u00b7si\u00b7as", ",", "bet", "ich", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.18": {"line.1": {"text": "Du, unser Heil und h\u00f6chstes Gut,", "tokens": ["Du", ",", "un\u00b7ser", "Heil", "und", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vereinest dich mit Fleisch und Blut,", "tokens": ["Ver\u00b7ei\u00b7nest", "dich", "mit", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst unser Freund und Bruder hier,", "tokens": ["Wirst", "un\u00b7ser", "Freund", "und", "Bru\u00b7der", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gottes Kinder werden wir.", "tokens": ["Und", "Got\u00b7tes", "Kin\u00b7der", "wer\u00b7den", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN"