{"textgrid.poem.36878": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtslied", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist der Tag, den Gott gemacht;", "tokens": ["Dies", "ist", "der", "Tag", ",", "den", "Gott", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein werd in aller Welt gedacht!", "tokens": ["Sein", "werd", "in", "al\u00b7ler", "Welt", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn preise, was durch Jesum Christ", "tokens": ["Ihn", "prei\u00b7se", ",", "was", "durch", "Je\u00b7sum", "Christ"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmel und auf Erden ist!", "tokens": ["Im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die V\u00f6lker haben dein geharrt,", "tokens": ["Die", "V\u00f6l\u00b7ker", "ha\u00b7ben", "dein", "ge\u00b7harrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df die Zeit erf\u00fcllet ward;", "tokens": ["Bis", "da\u00df", "die", "Zeit", "er\u00b7f\u00fcl\u00b7let", "ward", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sandte Gott von seinem Thron", "tokens": ["Da", "sand\u00b7te", "Gott", "von", "sei\u00b7nem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Heil der Welt, dich, seinen Sohn.", "tokens": ["Das", "Heil", "der", "Welt", ",", "dich", ",", "sei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich dies Wunder fassen will:", "tokens": ["Wenn", "ich", "dies", "Wun\u00b7der", "fas\u00b7sen", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So steht mein Geist vor Ehrfurcht still;", "tokens": ["So", "steht", "mein", "Geist", "vor", "Ehr\u00b7furcht", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er betet an, und er ermi\u00dft,", "tokens": ["Er", "be\u00b7tet", "an", ",", "und", "er", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Gottes Lieb unendlich ist.", "tokens": ["Da\u00df", "Got\u00b7tes", "Lieb", "un\u00b7end\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Damit der S\u00fcnder Gnad erh\u00e4lt,", "tokens": ["Da\u00b7mit", "der", "S\u00fcn\u00b7der", "Gnad", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erniedrigst du dich, Herr der Welt,", "tokens": ["Er\u00b7nied\u00b7rigst", "du", "dich", ",", "Herr", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimmst selbst an unsrer Menschheit teil,", "tokens": ["Nimmst", "selbst", "an", "uns\u00b7rer", "Menschheit", "teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erscheinst im Fleisch, und wirst uns Heil.", "tokens": ["Er\u00b7scheinst", "im", "Fleisch", ",", "und", "wirst", "uns", "Heil", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KON", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dein K\u00f6nig, Zion, k\u00f6mmt zu dir.", "tokens": ["Dein", "K\u00f6\u00b7nig", ",", "Zi\u00b7on", ",", "k\u00f6mmt", "zu", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich komm, im Buche steht von mir;", "tokens": ["\u00bb", "ich", "komm", ",", "im", "Bu\u00b7che", "steht", "von", "mir", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott, deinen Willen tu ich gern.\u00ab", "tokens": ["Gott", ",", "dei\u00b7nen", "Wil\u00b7len", "tu", "ich", "gern", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelobt sei, der da k\u00f6mmt im Herrn!", "tokens": ["Ge\u00b7lobt", "sei", ",", "der", "da", "k\u00f6mmt", "im", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, der du Mensch geboren wirst,", "tokens": ["Herr", ",", "der", "du", "Mensch", "ge\u00b7bo\u00b7ren", "wirst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Immanuel und Friedef\u00fcrst,", "tokens": ["Im\u00b7ma\u00b7nu\u00b7el", "und", "Frie\u00b7de\u00b7f\u00fcrst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf den die V\u00e4ter hoffend sahn,", "tokens": ["Auf", "den", "die", "V\u00e4\u00b7ter", "hof\u00b7fend", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, Gott, Messias, bet ich an.", "tokens": ["Dich", ",", "Gott", ",", "Mes\u00b7si\u00b7as", ",", "bet", "ich", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Du, unser Heil und h\u00f6chstes Gut,", "tokens": ["Du", ",", "un\u00b7ser", "Heil", "und", "h\u00f6chs\u00b7tes", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vereinest dich mit Fleisch und Blut,", "tokens": ["Ver\u00b7ei\u00b7nest", "dich", "mit", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wirst unser Freund und Bruder hier,", "tokens": ["Wirst", "un\u00b7ser", "Freund", "und", "Bru\u00b7der", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gottes Kinder werden wir.", "tokens": ["Und", "Got\u00b7tes", "Kin\u00b7der", "wer\u00b7den", "wir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gedanke voller Majest\u00e4t!", "tokens": ["Ge\u00b7dan\u00b7ke", "vol\u00b7ler", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist es, der das Herz erh\u00f6ht.", "tokens": ["Du", "bist", "es", ",", "der", "das", "Herz", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedanke voller Seligkeit!", "tokens": ["Ge\u00b7dan\u00b7ke", "vol\u00b7ler", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist es, der das Herz erfreut.", "tokens": ["Du", "bist", "es", ",", "der", "das", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Was zagt der Mensch, wenn der ihn sch\u00fctzt,", "tokens": ["Was", "zagt", "der", "Mensch", ",", "wenn", "der", "ihn", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der in des Vaters Scho\u00dfe sitzt?", "tokens": ["Der", "in", "des", "Va\u00b7ters", "Scho\u00b7\u00dfe", "sitzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Jauchzt, Himmel, die ihr ihn erfuhrt,", "tokens": ["Jauchzt", ",", "Him\u00b7mel", ",", "die", "ihr", "ihn", "er\u00b7fuhrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tag der heiligsten Geburt;", "tokens": ["Den", "Tag", "der", "hei\u00b7ligs\u00b7ten", "Ge\u00b7burt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und Erde, die ihn heute sieht,", "tokens": ["Und", "Er\u00b7de", ",", "die", "ihn", "heu\u00b7te", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing ihm, dem Herrn, ein neues Lied!", "tokens": ["Sing", "ihm", ",", "dem", "Herrn", ",", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dies ist der Tag, den Gott gemacht;", "tokens": ["Dies", "ist", "der", "Tag", ",", "den", "Gott", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein werd in aller Welt gedacht!", "tokens": ["Sein", "werd", "in", "al\u00b7ler", "Welt", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn preise, was du