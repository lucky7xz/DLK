{"dta.poem.5749": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "D' Infinit&#233;, d'Immensit&#233;,", "tokens": ["D'", "In\u00b7fi\u00b7nit", "&#233;", ",", "d'\u00b7Im\u00b7men\u00b7sit", "&#233;", ","], "token_info": ["word", "word", "XML_entity", "punct", "word", "XML_entity", "punct"], "pos": ["FM.la", "FM.la", "$(", "$,", "FM.la", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ne parlons, qu'avec retenue,", "tokens": ["Ne", "par\u00b7lons", ",", "qu'\u00b7a\u00b7vec", "re\u00b7te\u00b7nue", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM.la", "FM.la", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "A la seule Divinit&#233;", "tokens": ["A", "la", "seu\u00b7le", "Di\u00b7vi\u00b7nit", "&#233;"], "token_info": ["word", "word", "word", "word", "XML_entity"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La Gloire en est proprement d&#251;e.", "tokens": ["La", "Gloi\u00b7re", "en", "est", "prop\u00b7re\u00b7ment", "d", "&#251;", "e."], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ces termes toute fois peuvent se rapporter", "tokens": ["Ces", "ter\u00b7mes", "tou\u00b7te", "fois", "peu\u00b7vent", "se", "rap\u00b7por\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Aux Sujets, que l'Esprit ne sauroit limiter.", "tokens": ["Aux", "Su\u00b7jets", ",", "que", "l'\u00b7E\u00b7sprit", "ne", "sau\u00b7roit", "li\u00b7mi\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dans ses Discours, dictez par la Sagesse,", "tokens": ["Dans", "ses", "Dis\u00b7cours", ",", "dic\u00b7tez", "par", "la", "Sa\u00b7ges\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "DeS CARTES seulement admet l' Ind&#233;fini;", "tokens": ["DeS", "CaR\u00b7TES", "seu\u00b7le\u00b7ment", "ad\u00b7met", "l'", "Ind", "&#233;", "fi\u00b7ni", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+--+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Un indiscret orgueil par-la se voit banni.", "tokens": ["Un", "in\u00b7di\u00b7scret", "or\u00b7gu\u00b7eil", "pa\u00b7rla", "se", "voit", "ban\u00b7ni", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Car enfin il faudra, que notre Esprit confesse,", "tokens": ["Car", "en\u00b7fin", "il", "faud\u00b7ra", ",", "que", "not\u00b7re", "E\u00b7sprit", "con\u00b7fes\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Qu'on ne peut se tracer un Monde limit&#233;;", "tokens": ["Qu'\u00b7on", "ne", "peut", "se", "tra\u00b7cer", "un", "Mon\u00b7de", "li\u00b7mit", "&#233;", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Et l'etendant sans borne, alors notre foiblesse", "tokens": ["Et", "l'\u00b7e\u00b7ten\u00b7dant", "sans", "bor\u00b7ne", ",", "a\u00b7lors", "not\u00b7re", "foib\u00b7les\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "S'abime dans l' Infinit&#233;.", "tokens": ["S'\u00b7a\u00b7bi\u00b7me", "dans", "l'", "In\u00b7fi\u00b7nit", "&#233;", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "L'un &amp; l'autre parti doit donc &#234;tre &#233;vit&#233;.", "tokens": ["L'\u00b7un", "&amp;", "l'\u00b7au\u00b7tre", "par\u00b7ti", "doit", "donc", "&#234;", "tre", "&#233;", "vit", "&#233;", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word", "XML_entity", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}