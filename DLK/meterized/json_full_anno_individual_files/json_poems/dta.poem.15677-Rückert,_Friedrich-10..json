{"dta.poem.15677": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der wird nicht wirken viel mit allen seinen Werken,", "tokens": ["Der", "wird", "nicht", "wir\u00b7ken", "viel", "mit", "al\u00b7len", "sei\u00b7nen", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "VVFIN", "ADV", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer gleich bei jedem Werk die Wirkung will bemerken.", "tokens": ["Wer", "gleich", "bei", "je\u00b7dem", "Werk", "die", "Wir\u00b7kung", "will", "be\u00b7mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du wirke fort und fort in deinen Werkbezirken!", "tokens": ["Du", "wir\u00b7ke", "fort", "und", "fort", "in", "dei\u00b7nen", "Werk\u00b7be\u00b7zir\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wirkt nicht das Einzelne, doch wird das Ganze wirken.", "tokens": ["Wirkt", "nicht", "das", "Ein\u00b7zel\u00b7ne", ",", "doch", "wird", "das", "Gan\u00b7ze", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "$,", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ist Eines abgethan, so fang' ein Anders an,", "tokens": ["Ist", "Ei\u00b7nes", "ab\u00b7ge\u00b7than", ",", "so", "fang'", "ein", "An\u00b7ders", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und warte nicht bis erst dein Erstes Lohn empfahn.", "tokens": ["Und", "war\u00b7te", "nicht", "bis", "erst", "dein", "Ers\u00b7tes", "Lohn", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie der Zitronenbaum zu neuer Bl\u00fcte greift,", "tokens": ["Wie", "der", "Zit\u00b7ro\u00b7nen\u00b7baum", "zu", "neu\u00b7er", "Bl\u00fc\u00b7te", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' abzuwarten bis zur Frucht die alte reift.", "tokens": ["Ohn'", "ab\u00b7zu\u00b7war\u00b7ten", "bis", "zur", "Frucht", "die", "al\u00b7te", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "APPR", "APPRART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Als Knabe lie\u00df ich so gestellte Dohnen hangen", "tokens": ["Als", "Kna\u00b7be", "lie\u00df", "ich", "so", "ge\u00b7stell\u00b7te", "Doh\u00b7nen", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und blieb nicht stehn dabei, bis etwas sich gefangen.", "tokens": ["Und", "blieb", "nicht", "stehn", "da\u00b7bei", ",", "bis", "et\u00b7was", "sich", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN", "PAV", "$,", "KOUS", "PIS", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich that nach anderm Ziel indessen einen Gang", "tokens": ["Ich", "that", "nach", "an\u00b7derm", "Ziel", "in\u00b7des\u00b7sen", "ei\u00b7nen", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hob beim Heimweg aus den Dohnen meinen Fang.", "tokens": ["Und", "hob", "beim", "Heim\u00b7weg", "aus", "den", "Doh\u00b7nen", "mei\u00b7nen", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wo m\u00fc\u00dfig lauernd ich mich h\u00e4tt' im Busch versteckt,", "tokens": ["Wo", "m\u00fc\u00b7\u00dfig", "lau\u00b7ernd", "ich", "mich", "h\u00e4tt'", "im", "Busch", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "PPER", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt' ich mir selber nur die V\u00f6gel weggeschreckt.", "tokens": ["H\u00e4tt'", "ich", "mir", "sel\u00b7ber", "nur", "die", "V\u00f6\u00b7gel", "weg\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}