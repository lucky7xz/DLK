{"textgrid.poem.48065": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klinge denn, klinge, mein Saitenspiel,", "tokens": ["Klin\u00b7ge", "denn", ",", "klin\u00b7ge", ",", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "will mich nicht gr\u00e4men noch sch\u00e4men:", "tokens": ["will", "mich", "nicht", "gr\u00e4\u00b7men", "noch", "sch\u00e4\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "da\u00df mir die Liebe vom Himmel fiel,", "tokens": ["da\u00df", "mir", "die", "Lie\u00b7be", "vom", "Him\u00b7mel", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "mag es die Welt vernehmen!", "tokens": ["mag", "es", "die", "Welt", "ver\u00b7neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "K\u00fcsse denn, k\u00fcsse zum andern Mal,", "tokens": ["K\u00fcs\u00b7se", "denn", ",", "k\u00fcs\u00b7se", "zum", "an\u00b7dern", "Mal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-