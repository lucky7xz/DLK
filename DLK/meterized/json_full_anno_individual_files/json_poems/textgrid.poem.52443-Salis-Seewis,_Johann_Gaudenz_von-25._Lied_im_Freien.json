{"textgrid.poem.52443": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "25. Lied im Freien", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist's im Freien!", "tokens": ["Wie", "sch\u00f6n", "ist's", "im", "Frei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei gr\u00fcnenden Maien", "tokens": ["Bei", "gr\u00fc\u00b7nen\u00b7den", "Mai\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Walde, wie sch\u00f6n!", "tokens": ["Im", "Wal\u00b7de", ",", "wie", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wie s\u00fc\u00df, sich zu sonnen,", "tokens": ["Wie", "s\u00fc\u00df", ",", "sich", "zu", "son\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den St\u00e4dten entronnen,", "tokens": ["Den", "St\u00e4d\u00b7ten", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Auf luftigen H\u00f6hn!", "tokens": ["Auf", "luf\u00b7ti\u00b7gen", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Wo unter den Hecken", "tokens": ["Wo", "un\u00b7ter", "den", "He\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit goldenen Flecken", "tokens": ["Mit", "gol\u00b7de\u00b7nen", "Fle\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Schatten sich mischt,", "tokens": ["Der", "Schat\u00b7ten", "sich", "mischt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da l\u00e4\u00dft man sich nieder,", "tokens": ["Da", "l\u00e4\u00dft", "man", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Von Haseln und Flieder", "tokens": ["Von", "Ha\u00b7seln", "und", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mit Laubduft erfrischt.", "tokens": ["Mit", "Laub\u00b7duft", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "D'rauf schlendert man weiter,", "tokens": ["D'\u00b7rauf", "schlen\u00b7dert", "man", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Pfl\u00fcckt Blumen und Kr\u00e4uter", "tokens": ["Pfl\u00fcckt", "Blu\u00b7men", "und", "Kr\u00e4u\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Erdbeern im Gehn;", "tokens": ["Und", "Erd\u00b7beern", "im", "Gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Man kann sich mit Zweigen,", "tokens": ["Man", "kann", "sich", "mit", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Erhitzet vom Steigen,", "tokens": ["Er\u00b7hit\u00b7zet", "vom", "Stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Wangen umwehn.", "tokens": ["Die", "Wan\u00b7gen", "um\u00b7wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Dort heben und tunken,", "tokens": ["Dort", "he\u00b7ben", "und", "tun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gleich blinkenden Funken,", "tokens": ["Gleich", "blin\u00b7ken\u00b7den", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sich Wellchen im Bach:", "tokens": ["Sich", "Well\u00b7chen", "im", "Bach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Man sieht sie verrinnen", "tokens": ["Man", "sieht", "sie", "ver\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In stillem Besinnen,", "tokens": ["In", "stil\u00b7lem", "Be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Halb tr\u00e4umend, halb wach.", "tokens": ["Halb", "tr\u00e4u\u00b7mend", ",", "halb", "wach", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "In weiten Bezirken,", "tokens": ["In", "wei\u00b7ten", "Be\u00b7zir\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit hangenden Birken", "tokens": ["Mit", "han\u00b7gen\u00b7den", "Bir\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Buchen besetzt,", "tokens": ["Und", "Bu\u00b7chen", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Gehn Dammhirsch und Rehe", "tokens": ["Gehn", "Damm\u00b7hirsch", "und", "Re\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "In traulicher N\u00e4he,", "tokens": ["In", "trau\u00b7li\u00b7cher", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von niemand gehetzt.", "tokens": ["Von", "nie\u00b7mand", "ge\u00b7hetzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Am schwankenden Reisig", "tokens": ["Am", "schwan\u00b7ken\u00b7den", "Rei\u00b7sig"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "H\u00e4ngt zwitschernd der Zeisig,", "tokens": ["H\u00e4ngt", "zwit\u00b7schernd", "der", "Zei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vor Schlingen nicht bang;", "tokens": ["Vor", "Schlin\u00b7gen", "nicht", "bang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Erfreut, ihn zu h\u00f6ren,", "tokens": ["Er\u00b7freut", ",", "ihn", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sucht keiner zu st\u00f6ren", "tokens": ["Sucht", "kei\u00b7ner", "zu", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des H\u00e4nflings Gesang.", "tokens": ["Des", "H\u00e4nf\u00b7lings", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Hier str\u00e4ubt sich kein Pf\u00f6rtner,", "tokens": ["Hier", "str\u00e4ubt", "sich", "kein", "Pf\u00f6rt\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hier schn\u00f6rkelt kein G\u00e4rtner", "tokens": ["Hier", "schn\u00f6r\u00b7kelt", "kein", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Kunstm\u00e4\u00dfig am Hain:", "tokens": ["Kunst\u00b7m\u00e4\u00b7\u00dfig", "am", "Hain", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Man braucht nicht des Geldes;", "tokens": ["Man", "braucht", "nicht", "des", "Gel\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Blumen des Feldes", "tokens": ["Die", "Blu\u00b7men", "des", "Fel\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sind allen gemein.", "tokens": ["Sind", "al\u00b7len", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Wie sch\u00f6n ist's im Freien!", "tokens": ["Wie", "sch\u00f6n", "ist's", "im", "Frei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Despoten entweihen", "tokens": ["Des\u00b7po\u00b7ten", "ent\u00b7wei\u00b7hen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier nicht die Natur.", "tokens": ["Hier", "nicht", "die", "Na\u00b7tur", "."], "token_inf