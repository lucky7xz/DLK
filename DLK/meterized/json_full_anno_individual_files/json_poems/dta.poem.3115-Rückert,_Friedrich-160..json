{"dta.poem.3115": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "160.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen Dingen der Natur der Mensch ist eines,", "tokens": ["Von", "al\u00b7len", "Din\u00b7gen", "der", "Na\u00b7tur", "der", "Mensch", "ist", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "ART", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein im unendlichen Gro\u00dfen unendlich kleines;", "tokens": ["Ein", "im", "un\u00b7end\u00b7li\u00b7chen", "Gro\u00b7\u00dfen", "un\u00b7end\u00b7lich", "klei\u00b7nes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "ADJD", "ADJA", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ein Theilchen Gotteskraft, das eng sich f\u00fchlt umzirkt", "tokens": ["Ein", "Theil\u00b7chen", "Got\u00b7tes\u00b7kraft", ",", "das", "eng", "sich", "f\u00fchlt", "um\u00b7zirkt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Ganzen dieser Kraft, die durch das Ganze wirkt;", "tokens": ["Vom", "Gan\u00b7zen", "die\u00b7ser", "Kraft", ",", "die", "durch", "das", "Gan\u00b7ze", "wirkt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vielf\u00e4ltig eingeschr\u00e4nkt und tausendfach bedingt,", "tokens": ["Viel\u00b7f\u00e4l\u00b7tig", "ein\u00b7ge\u00b7schr\u00e4nkt", "und", "tau\u00b7send\u00b7fach", "be\u00b7dingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von \u00fcberm\u00e4chtigen Einwirkungen umringt.", "tokens": ["Von", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Ein\u00b7wir\u00b7kun\u00b7gen", "um\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Allein nach Freiheit ringt er aus der Engigkeit,", "tokens": ["Al\u00b7lein", "nach", "Frei\u00b7heit", "ringt", "er", "aus", "der", "En\u00b7gig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der allabh\u00e4ngige nach Unabh\u00e4ngigkeit.", "tokens": ["Der", "al\u00b7lab\u00b7h\u00e4n\u00b7gi\u00b7ge", "nach", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Tropfen in dem Meer, gew\u00e4rtig der Verschwimmung,", "tokens": ["Der", "Trop\u00b7fen", "in", "dem", "Meer", ",", "ge\u00b7w\u00e4r\u00b7tig", "der", "Ver\u00b7schwim\u00b7mung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht unabweislichen Anspruch auf Selbstbestimmung;", "tokens": ["Macht", "un\u00b7ab\u00b7weis\u00b7li\u00b7chen", "An\u00b7spruch", "auf", "Selbst\u00b7be\u00b7stim\u00b7mung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Der Mensch, der nur was ihm von Gott bestimmt ist, nimmt,", "tokens": ["Der", "Mensch", ",", "der", "nur", "was", "ihm", "von", "Gott", "be\u00b7stimmt", "ist", ",", "nimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PWS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Selbstbestimmung auch ist ihm von Gott bestimmt.", "tokens": ["Die", "Selbst\u00b7be\u00b7stim\u00b7mung", "auch", "ist", "ihm", "von", "Gott", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}