{"dta.poem.8033": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Alle Damen schmachten, hoffen,", "tokens": ["Al\u00b7le", "Da\u00b7men", "schmach\u00b7ten", ",", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn, den Sch\u00f6nsten, zu empfahen;", "tokens": ["Ihn", ",", "den", "Sch\u00f6ns\u00b7ten", ",", "zu", "em\u00b7pfa\u00b7hen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Mohren zagen, zittern", "tokens": ["Al\u00b7le", "Moh\u00b7ren", "za\u00b7gen", ",", "zit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor des k\u00fchnsten Streiters Nahen.", "tokens": ["Vor", "des", "k\u00fchns\u00b7ten", "Strei\u00b7ters", "Na\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Damen! w\u00fcrdet nicht mehr hoffen,", "tokens": ["Da\u00b7men", "!", "w\u00fcr\u00b7det", "nicht", "mehr", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mohren! w\u00fcrdet nicht mehr zagen:", "tokens": ["Moh\u00b7ren", "!", "w\u00fcr\u00b7det", "nicht", "mehr", "za\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fc\u00dftet ihr, da\u00df im Gebirge", "tokens": ["W\u00fc\u00df\u00b7tet", "ihr", ",", "da\u00df", "im", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4ngst Gewitter ihn erschlagen.", "tokens": ["L\u00e4ngst", "Ge\u00b7wit\u00b7ter", "ihn", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}