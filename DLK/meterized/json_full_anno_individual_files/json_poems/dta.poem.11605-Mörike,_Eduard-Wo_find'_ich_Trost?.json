{"dta.poem.11605": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Wo find' ich Trost?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Eine Liebe kenn' ich, die ist treu,", "tokens": ["Ei\u00b7ne", "Lie\u00b7be", "kenn'", "ich", ",", "die", "ist", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "War getreu, so lang ich sie gefunden,", "tokens": ["War", "ge\u00b7treu", ",", "so", "lang", "ich", "sie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ADJD", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hat mit tiefem Seufzen immer neu,", "tokens": ["Hat", "mit", "tie\u00b7fem", "Seuf\u00b7zen", "im\u00b7mer", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stets vers\u00f6hnlich, sich mit mir verbunden.", "tokens": ["Stets", "ver\u00b7s\u00f6hn\u00b7lich", ",", "sich", "mit", "mir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Welcher einst mit himmlischem Gedulden", "tokens": ["Wel\u00b7cher", "einst", "mit", "himm\u00b7li\u00b7schem", "Ge\u00b7dul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bitter bittern Todestropfen trank,", "tokens": ["Bit\u00b7ter", "bit\u00b7tern", "To\u00b7de\u00b7strop\u00b7fen", "trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hing am Kreuz und b\u00fc\u00dfte mein Verschulden,", "tokens": ["Hing", "am", "Kreuz", "und", "b\u00fc\u00df\u00b7te", "mein", "Ver\u00b7schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Bis es in ein Meer von Gnade sank.", "tokens": ["Bis", "es", "in", "ein", "Meer", "von", "Gna\u00b7de", "sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und was ist's nun, da\u00df ich traurig bin,", "tokens": ["Und", "was", "ist's", "nun", ",", "da\u00df", "ich", "trau\u00b7rig", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich angstvoll mich am Boden winde?", "tokens": ["Da\u00df", "ich", "angst\u00b7voll", "mich", "am", "Bo\u00b7den", "win\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frage: H\u00fcter, ist die Nacht bald hin?", "tokens": ["Fra\u00b7ge", ":", "H\u00fc\u00b7ter", ",", "ist", "die", "Nacht", "bald", "hin", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und: was rettet mich von Tod und S\u00fcnde?", "tokens": ["Und", ":", "was", "ret\u00b7tet", "mich", "von", "Tod", "und", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "PWS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Arges Herze! ja gesteh es nur,", "tokens": ["Ar\u00b7ges", "Her\u00b7ze", "!", "ja", "ge\u00b7steh", "es", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du hast wieder b\u00f6se Lust empfangen;", "tokens": ["Du", "hast", "wie\u00b7der", "b\u00f6\u00b7se", "Lust", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frommer Liebe, frommer Treue Spur,", "tokens": ["From\u00b7mer", "Lie\u00b7be", ",", "from\u00b7mer", "Treu\u00b7e", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ach, das ist auf lange nun vergangen.", "tokens": ["Ach", ",", "das", "ist", "auf", "lan\u00b7ge", "nun", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "APPR", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, das ist's auch, da\u00df ich traurig bin,", "tokens": ["Ja", ",", "das", "ist's", "auch", ",", "da\u00df", "ich", "trau\u00b7rig", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ich angstvoll mich am Boden winde!", "tokens": ["Da\u00df", "ich", "angst\u00b7voll", "mich", "am", "Bo\u00b7den", "win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00fcter, H\u00fcter, ist die Nacht bald hin?", "tokens": ["H\u00fc\u00b7ter", ",", "H\u00fc\u00b7ter", ",", "ist", "die", "Nacht", "bald", "hin", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und was rettet mich von Tod und S\u00fcnde?", "tokens": ["Und", "was", "ret\u00b7tet", "mich", "von", "Tod", "und", "S\u00fcn\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}