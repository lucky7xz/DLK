{"dta.poem.8613": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An meine Rose .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Frohlocke, sch\u00f6ne junge Rose,", "tokens": ["Froh\u00b7lo\u00b7cke", ",", "sch\u00f6\u00b7ne", "jun\u00b7ge", "Ro\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Bild wird nicht verschwinden,", "tokens": ["Dein", "Bild", "wird", "nicht", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn auch die Gluth, die dauerlose,", "tokens": ["Wenn", "auch", "die", "Gluth", ",", "die", "dau\u00b7er\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verweht in Abendwinden.", "tokens": ["Ver\u00b7weht", "in", "Ab\u00b7end\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So s\u00fc\u00dfer Duft, so helle Flamme", "tokens": ["So", "s\u00fc\u00b7\u00dfer", "Duft", ",", "so", "hel\u00b7le", "Flam\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann nicht f\u00fcr irdisch gelten,", "tokens": ["Kann", "nicht", "f\u00fcr", "ir\u00b7disch", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du prangst am stolzen Rosenstamme,", "tokens": ["Du", "prangst", "am", "stol\u00b7zen", "Ro\u00b7sen\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verpflanzt aus andern Welten;", "tokens": ["Ver\u00b7pflanzt", "aus", "an\u00b7dern", "Wel\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aus B\u00fcschen, wo die G\u00f6tter gerne", "tokens": ["Aus", "B\u00fc\u00b7schen", ",", "wo", "die", "G\u00f6t\u00b7ter", "ger\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in die Schatten senken,", "tokens": ["Sich", "in", "die", "Schat\u00b7ten", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie in heilig stiller Ferne", "tokens": ["Wenn", "sie", "in", "hei\u00b7lig", "stil\u00b7ler", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Menschen Gl\u00fcck bedenken.", "tokens": ["Der", "Men\u00b7schen", "Gl\u00fcck", "be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Darum mich ein Hin\u00fcbersehnen", "tokens": ["Da\u00b7rum", "mich", "ein", "Hin\u00b7\u00fc\u00b7ber\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stets inniger umschmieget,", "tokens": ["Stets", "in\u00b7ni\u00b7ger", "um\u00b7schmie\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Je l\u00e4nger sich in meinen Thr\u00e4nen", "tokens": ["Je", "l\u00e4n\u00b7ger", "sich", "in", "mei\u00b7nen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein holdes Antlitz wieget.", "tokens": ["Dein", "hol\u00b7des", "Ant\u00b7litz", "wie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O weilten wir in jenen L\u00fcften,", "tokens": ["O", "weil\u00b7ten", "wir", "in", "je\u00b7nen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo keine Schranke wehrte,", "tokens": ["Wo", "kei\u00b7ne", "Schran\u00b7ke", "wehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich mit deinen Zauberd\u00fcften", "tokens": ["Da\u00df", "ich", "mit", "dei\u00b7nen", "Zau\u00b7ber\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ewigkeiten n\u00e4hrte! \u2014", "tokens": ["Die", "E\u00b7wig\u00b7kei\u00b7ten", "n\u00e4hr\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hier nah'n die Augenblicke, \u2014 schwinden", "tokens": ["Hier", "nah'n", "die", "Au\u00b7gen\u00b7bli\u00b7cke", ",", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "$(", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An dir vor\u00fcber immer,", "tokens": ["An", "dir", "vor\u00b7\u00fc\u00b7ber", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-++", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ein jeder eilt dich noch zu finden", "tokens": ["Ein", "je\u00b7der", "eilt", "dich", "noch", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Jugendschimmer;", "tokens": ["In", "dei\u00b7nem", "Ju\u00b7gend\u00b7schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und ich, wie sie, mu\u00df immer eilen", "tokens": ["Und", "ich", ",", "wie", "sie", ",", "mu\u00df", "im\u00b7mer", "ei\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PWAV", "PPER", "$,", "VMFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allem meinem Lieben", "tokens": ["Mit", "al\u00b7lem", "mei\u00b7nem", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An dir vorbei, darf nie verweilen,", "tokens": ["An", "dir", "vor\u00b7bei", ",", "darf", "nie", "ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von St\u00fcrmen fortgetrieben.", "tokens": ["Von", "St\u00fcr\u00b7men", "fort\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch hat, du holde Wunderblume,", "tokens": ["Doch", "hat", ",", "du", "hol\u00b7de", "Wun\u00b7der\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz voll s\u00fc\u00dfen Bebens", "tokens": ["Mein", "Herz", "voll", "s\u00fc\u00b7\u00dfen", "Be\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich mir gemalt zum Eigenthume", "tokens": ["Dich", "mir", "ge\u00b7malt", "zum", "Ei\u00b7gen\u00b7thu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VVPP", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ins Tiefste meines Lebens,", "tokens": ["Ins", "Tiefs\u00b7te", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wohin der Tod, der Ruhebringer,", "tokens": ["Wo\u00b7hin", "der", "Tod", ",", "der", "Ru\u00b7he\u00b7brin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich scheuen wird zu greifen,", "tokens": ["Sich", "scheu\u00b7en", "wird", "zu", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn endlich seine sanften Finger", "tokens": ["Wenn", "end\u00b7lich", "sei\u00b7ne", "sanf\u00b7ten", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Welkes niederstreifen.", "tokens": ["Mein", "Wel\u00b7kes", "nie\u00b7der\u00b7strei\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}