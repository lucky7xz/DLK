{"dta.poem.3581": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Schweizerisch Kriegsgebet .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.85", "hu:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft \u00fcs abermal betta               ", "tokens": ["La\u00dft", "\u00fcs", "a\u00b7ber\u00b7mal", "bet\u00b7ta"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr \u00fcsra Stadt und Flecka,", "tokens": ["F\u00fcr", "\u00fcs\u00b7ra", "Stadt", "und", "Flec\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr \u00fcsre K\u00fch und Geissa,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "K\u00fch", "und", "Geis\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr \u00fcsre Wittwa und Wei\u00dfa,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "Witt\u00b7wa", "und", "Wei\u00b7\u00dfa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "F\u00fcr \u00fcsre Ro\u00df und Rinder,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "Ro\u00df", "und", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr \u00fcsre Weib und Kinder,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "Weib", "und", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr \u00fcsre Henna und Hahna,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "Hen\u00b7na", "und", "Hah\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "KON", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "F\u00fcr \u00fcsre Kessel und Pfanna,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "Kes\u00b7sel", "und", "Pfan\u00b7na", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.9": {"text": "F\u00fcr \u00fcsre G\u00e4ns ", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "G\u00e4ns"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "F\u00fcr \u00fcsre Oberst und Regenta,", "tokens": ["F\u00fcr", "\u00fcs\u00b7re", "O\u00b7berst", "und", "Re\u00b7gen\u00b7ta", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "An insonderheit f\u00fcr \u00fcsre liebi Schwitz,", "tokens": ["An", "in\u00b7son\u00b7der\u00b7heit", "f\u00fcr", "\u00fcs\u00b7re", "lie\u00b7bi", "Schwitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Wenn der blutig Krieg wett ko,", "tokens": ["Wenn", "der", "blu\u00b7tig", "Krieg", "wett", "ko", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wett alls n\u00e4, so wetten wir \u00fcs treuli wehra,", "tokens": ["Wett", "alls", "n\u00e4", ",", "so", "wet\u00b7ten", "wir", "\u00fcs", "treu\u00b7li", "weh\u00b7ra", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ADV", "VVFIN", "PPER", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Und ihn niena dura loh,", "tokens": ["Und", "ihn", "ni\u00b7e\u00b7na", "du\u00b7ra", "loh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Au den Find gar ztod schloh,", "tokens": ["Au", "den", "Find", "gar", "ztod", "schloh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Und dann singa;", "tokens": ["Und", "dann", "sin\u00b7ga", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "\u201eeia Viktoria! der Find ischt ko, hett alles gno,", "tokens": ["\u201e", "ei\u00b7a", "Vik\u00b7to\u00b7ria", "!", "der", "Find", "ischt", "ko", ",", "hett", "al\u00b7les", "gno", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$.", "ART", "NN", "VVFIN", "NE", "$,", "VAFIN", "PIS", "NE", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.18": {"text": "\u201ehett Fenster i gschlaga, hets Blie drus graba,", "tokens": ["\u201e", "hett", "Fens\u00b7ter", "i", "gschla\u00b7ga", ",", "hets", "Blie", "drus", "gra\u00b7ba", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "NN", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "\u201ehett Kugla drus go\u00dfa, und dBaura erschossa;", "tokens": ["\u201e", "hett", "Kug\u00b7la", "drus", "go\u00b7\u00dfa", ",", "und", "dBau\u00b7ra", "er\u00b7schos\u00b7sa", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "FM", "FM", "FM", "$,", "KON", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "\u201eeia Viktoria! nu ischts us, geht wiedri na Hus.\u201c", "tokens": ["\u201e", "ei\u00b7a", "Vik\u00b7to\u00b7ria", "!", "nu", "ischts", "us", ",", "geht", "wied\u00b7ri", "na", "Hus", ".", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "$.", "ADV", "FM", "FM", "$,", "VVFIN", "FM", "FM", "FM", "$.", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}