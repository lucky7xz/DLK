{"textgrid.poem.63841": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Resignation", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Tage, milddurchsonnt,", "tokens": ["Die\u00b7se", "Ta\u00b7ge", ",", "mild\u00b7durch\u00b7sonnt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo dich sonst wohl freuen konnt'", "tokens": ["Wo", "dich", "sonst", "wohl", "freu\u00b7en", "konnt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Welt, die lieblich bl\u00fcht,", "tokens": ["Ei\u00b7ne", "Welt", ",", "die", "lieb\u00b7lich", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilen nicht dein wund Gem\u00fct.", "tokens": ["Hei\u00b7len", "nicht", "dein", "wund", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer bleibt die Sorge wach,", "tokens": ["Im\u00b7mer", "bleibt", "die", "Sor\u00b7ge", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht auf Schritt und Tritt dir nach,", "tokens": ["Geht", "auf", "Schritt", "und", "Tritt", "dir", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos