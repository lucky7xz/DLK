{"dta.poem.7400": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Des Arztes Verm\u00e4chtni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "So mild die Landschaft und so k\u00fchn,", "tokens": ["So", "mild", "die", "Land\u00b7schaft", "und", "so", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Felsenritzen Ranken bl\u00fchn;", "tokens": ["Aus", "Fel\u00b7sen\u00b7rit\u00b7zen", "Ran\u00b7ken", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wild das Wasser st\u00fcrmt und rauscht,", "tokens": ["So", "wild", "das", "Was\u00b7ser", "st\u00fcrmt", "und", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dr\u00fcber Soldanella ", "tokens": ["Und", "dr\u00fc\u00b7ber", "Sol\u00b7da\u00b7nel\u00b7la"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Nichts was ein wundes Herz so k\u00fchlt", "tokens": ["Nichts", "was", "ein", "wun\u00b7des", "Herz", "so", "k\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PWS", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Bergesluft die einsam spielt,", "tokens": ["Als", "Ber\u00b7ge\u00b7sluft", "die", "ein\u00b7sam", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Maienmorgens frische Rosen", "tokens": ["Wenn", "Mai\u00b7en\u00b7mor\u00b7gens", "fri\u00b7sche", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Fichtendunkel fl\u00fcsternd kosen.", "tokens": ["Mit", "Fich\u00b7ten\u00b7dun\u00b7kel", "fl\u00fcs\u00b7ternd", "ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo \u00fcber'm Wipfelmeer das Riff", "tokens": ["Wo", "\u00fc\u00b7ber'm", "Wip\u00b7fel\u00b7meer", "das", "Riff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Aether steht, ein flaggend Schiff,", "tokens": ["Im", "A\u00b7e\u00b7ther", "steht", ",", "ein", "flag\u00b7gend", "Schiff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Um seinen Mast der Geier schweift:", "tokens": ["Um", "sei\u00b7nen", "Mast", "der", "Gei\u00b7er", "schweift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Tief im Geb\u00fcsch das Berghuhn l\u00e4uft,", "tokens": ["Tief", "im", "Ge\u00b7b\u00fcsch", "das", "Berg\u00b7huhn", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Es stutzt \u2014 es kauert sich \u2014 es pfeift", "tokens": ["Es", "stutzt", "es", "kau\u00b7ert", "sich", "es", "pfeift"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PRF", "$(", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und flattert auf; \u2014 ein Bl\u00e4ttchen streift", "tokens": ["Und", "flat\u00b7tert", "auf", ";", "ein", "Bl\u00e4tt\u00b7chen", "streift"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Die Rolle in des J\u00fcnglings Hand.", "tokens": ["Die", "Rol\u00b7le", "in", "des", "J\u00fcng\u00b7lings", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der schaut, versunken, \u00fcber Land,", "tokens": ["Der", "schaut", ",", "ver\u00b7sun\u00b7ken", ",", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wie Einer, so in Stromes Rauschen", "tokens": ["Wie", "Ei\u00b7ner", ",", "so", "in", "Stro\u00b7mes", "Rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Will l\u00e4ngst verklungner Stimme lauschen.", "tokens": ["Will", "l\u00e4ngst", "ver\u00b7klung\u00b7ner", "Stim\u00b7me", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Er ruht am feuchten Uferrand. \u2014", "tokens": ["Er", "ruht", "am", "feuch\u00b7ten", "Uf\u00b7er\u00b7rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "In seinem Auge Einklang liegt", "tokens": ["In", "sei\u00b7nem", "Au\u00b7ge", "Ein\u00b7klang", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Mit dem, was \u00fcber ihm sich wiegt,", "tokens": ["Mit", "dem", ",", "was", "\u00fc\u00b7ber", "ihm", "sich", "wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Mit Windgest\u00f6hn' und linden Zweigen:", "tokens": ["Mit", "Wind\u00b7ge\u00b7st\u00f6hn'", "und", "lin\u00b7den", "Zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Was ist ihm fremd, und was sein eigen?", "tokens": ["Was", "ist", "ihm", "fremd", ",", "und", "was", "sein", "ei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$,", "KON", "PWS", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Gedankenvoll dem Boden ein", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7voll", "dem", "Bo\u00b7den", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Gr\u00e4bt Zeichen er mit spitzem Stein,", "tokens": ["Gr\u00e4bt", "Zei\u00b7chen", "er", "mit", "spit\u00b7zem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und l\u00f6st gedankenvoll das Band", "tokens": ["Und", "l\u00f6st", "ge\u00b7dan\u00b7ken\u00b7voll", "das", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Am Blatt, wo, regelloser Spur,", "tokens": ["Am", "Blatt", ",", "wo", ",", "re\u00b7gel\u00b7lo\u00b7ser", "Spur", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ach! eine Hand, zu theuer nur,", "tokens": ["Ach", "!", "ei\u00b7ne", "Hand", ",", "zu", "theu\u00b7er", "nur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "PTKA", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Vertraut gest\u00f6rter Seele Leiden,", "tokens": ["Ver\u00b7traut", "ge\u00b7st\u00f6r\u00b7ter", "See\u00b7le", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die Wahr und Falsch nicht konnte scheiden.", "tokens": ["Die", "Wahr", "und", "Falsch", "nicht", "konn\u00b7te", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und will er \u2014 soll er \u2014 dringen ein", "tokens": ["Und", "will", "er", "soll", "er", "drin\u00b7gen", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$(", "VMFIN", "PPER", "$(", "VVFIN", "ART"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "In ein Geheimni\u00df das nicht sein?", "tokens": ["In", "ein", "Ge\u00b7heim\u00b7ni\u00df", "das", "nicht", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDS", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Es sey! es sey! die Hand ist Staub,", "tokens": ["Es", "sey", "!", "es", "sey", "!", "die", "Hand", "ist", "Staub", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Und ein Verm\u00e4chtni\u00df ja kein Raub!", "tokens": ["Und", "ein", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", "ja", "kein", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Dann \u2014 Wasser, Felsen, Alles schwand.", "tokens": ["Dann", "Was\u00b7ser", ",", "Fel\u00b7sen", ",", "Al\u00b7les", "schwand", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$,", "NN", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}