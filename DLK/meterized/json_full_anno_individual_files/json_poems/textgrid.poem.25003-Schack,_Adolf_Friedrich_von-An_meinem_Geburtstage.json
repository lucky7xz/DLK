{"textgrid.poem.25003": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An meinem Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so folgt das Jahr dem Jahre,", "tokens": ["Und", "so", "folgt", "das", "Jahr", "dem", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit schwarzem Flor beh\u00e4ngt", "tokens": ["Und", "mit", "schwar\u00b7zem", "Flor", "be\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht ger\u00fcstet schon die Bahre,", "tokens": ["Steht", "ge\u00b7r\u00fcs\u00b7tet", "schon", "die", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im letzten mich empf\u00e4ngt.", "tokens": ["Die", "im", "letz\u00b7ten", "mich", "emp\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tiefer in des Lebens Bl\u00fcte", "tokens": ["Tie\u00b7fer", "in", "des", "Le\u00b7bens", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nagt sich t\u00e4glich ein der Wurm,", "tokens": ["Nagt", "sich", "t\u00e4g\u00b7lich", "ein", "der", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Glut, die in mir gl\u00fchte,", "tokens": ["Und", "die", "Glut", ",", "die", "in", "mir", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Stirbt erl\u00f6schend hin im Sturm.", "tokens": ["Stirbt", "er\u00b7l\u00f6\u00b7schend", "hin", "im", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hin mit jedem Tage schwindet", "tokens": ["Hin", "mit", "je\u00b7dem", "Ta\u00b7ge", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas, das mir teuer war,", "tokens": ["Et\u00b7was", ",", "das", "mir", "teu\u00b7er", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Augen Stern erblindet,", "tokens": ["Und", "der", "Au\u00b7gen", "Stern", "er\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und zu Grau erbleicht mein Haar.", "tokens": ["Und", "zu", "Grau", "er\u00b7bleicht", "mein", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-