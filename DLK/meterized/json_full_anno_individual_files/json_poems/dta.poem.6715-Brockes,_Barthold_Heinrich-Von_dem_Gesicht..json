{"dta.poem.6715": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst aber uns sein Amt recht eigentlich entdecken,", "tokens": ["Lasst", "a\u00b7ber", "uns", "sein", "Amt", "recht", "ei\u00b7gent\u00b7lich", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sonder Vorurtheil, die uns in Hertzen stecken,", "tokens": ["Und", "son\u00b7der", "Vor\u00b7urt\u00b7heil", ",", "die", "uns", "in", "Hert\u00b7zen", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Vorwurff, welcher sich dem Blick zeigt unterscheiden", "tokens": ["Den", "Vor\u00b7wurff", ",", "wel\u00b7cher", "sich", "dem", "Blick", "zeigt", "un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von den Empfindungen, die unsre Geister leiden.", "tokens": ["Von", "den", "Emp\u00b7fin\u00b7dun\u00b7gen", ",", "die", "uns\u00b7re", "Geis\u00b7ter", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Man bilde sich nicht ein,", "tokens": ["Man", "bil\u00b7de", "sich", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ob f\u00fclle Firmament und Lufft ein heller Schein;", "tokens": ["Ob", "f\u00fcl\u00b7le", "Fir\u00b7ma\u00b7ment", "und", "Lufft", "ein", "hel\u00b7ler", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man glaube nicht, da\u00df bunte Farben stehen", "tokens": ["Man", "glau\u00b7be", "nicht", ",", "da\u00df", "bun\u00b7te", "Far\u00b7ben", "ste\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An C\u00f6rpern, wo wir sie von aussen sehen.", "tokens": ["An", "C\u00f6r\u00b7pern", ",", "wo", "wir", "sie", "von", "aus\u00b7sen", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "PPER", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}