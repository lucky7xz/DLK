{"textgrid.poem.51105": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ich hab in kalten Wintertagen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in kalten Wintertagen,", "tokens": ["Ich", "hab", "in", "kal\u00b7ten", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkler, hoffnungsarmer Zeit", "tokens": ["In", "dunk\u00b7ler", ",", "hoff\u00b7nungs\u00b7ar\u00b7mer", "Zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz aus dem Sinne dich geschlagen,", "tokens": ["Ganz", "aus", "dem", "Sin\u00b7ne", "dich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Trugbild der Unsterblichkeit!", "tokens": ["O", "Trug\u00b7bild", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Nun, da der Sommer gl\u00fcht und gl\u00e4nzet.", "tokens": ["Nun", ",", "da", "der", "Som\u00b7mer", "gl\u00fcht", "und", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun seh ich, da\u00df ich wohlgetan;", "tokens": ["Nun", "seh", "ich", ",", "da\u00df", "ich", "wohl\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe neu das Herz umkr\u00e4nzet,", "tokens": ["Ich", "ha\u00b7be", "neu", "das", "Herz", "um\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe aber ruht der Wahn.", "tokens": ["Im", "Gra\u00b7be", "a\u00b7ber", "ruht", "der", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich fahre auf dem klaren Strome,", "tokens": ["Ich", "fah\u00b7re", "auf", "dem", "kla\u00b7ren", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er rinnt mir k\u00fchlend durch die Hand;", "tokens": ["Er", "rinnt", "mir", "k\u00fch\u00b7lend", "durch", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schau hinauf zum blauen Dome \u2013", "tokens": ["Ich", "schau", "hin\u00b7auf", "zum", "blau\u00b7en", "Do\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und such kein be\u00dfres Vaterland.", "tokens": [