{"textgrid.poem.34830": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich bequem verbluten kann,", "tokens": ["Da\u00df", "ich", "be\u00b7quem", "ver\u00b7blu\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebt mir ein edles, weites Feld!", "tokens": ["Gebt", "mir", "ein", "ed\u00b7les", ",", "wei\u00b7tes", "Feld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oh, la\u00dft mich nicht ersticken hier", "tokens": ["Oh", ",", "la\u00dft", "mich", "nicht", "er\u00b7sti\u00b7cken", "hier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PTKNEG", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser engen Kr\u00e4merwelt!", "tokens": ["In", "die\u00b7ser", "en\u00b7gen", "Kr\u00e4\u00b7mer\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie essen gut, sie trinken gut,", "tokens": ["Sie", "es\u00b7sen", "gut", ",", "sie", "trin\u00b7ken", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreun sich ihres Maulwurfgl\u00fccks,", "tokens": ["Er\u00b7freun", "sich", "ih\u00b7res", "Maul\u00b7wurf\u00b7gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihre Gro\u00dfmut ist so gro\u00df", "tokens": ["Und", "ih\u00b7re", "Gro\u00df\u00b7mut", "ist", "so", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wie das Loch der Armenb\u00fcchs'.", "tokens": ["Als", "wie", "das", "Loch", "der", "Ar\u00b7men\u00b7b\u00fcchs'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zigarren tragen sie im Maul", "tokens": ["Zi\u00b7gar\u00b7ren", "tra\u00b7gen", "sie", "im", "Maul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der Hosentasch' die H\u00e4nd';", "tokens": ["Und", "in", "der", "Ho\u00b7sen\u00b7ta\u00b7sch'", "die", "H\u00e4nd'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch die Verdauungskraft ist gut \u2013", "tokens": ["Auch", "die", "Ver\u00b7dau\u00b7ungs\u00b7kraft", "ist", "gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sie nur selbst verdauen k\u00f6nnt!", "tokens": ["Wer", "sie", "nur", "selbst", "ver\u00b7dau\u00b7en", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie handeln mit den Spezerei'n", "tokens": ["Sie", "han\u00b7deln", "mit", "den", "Spe\u00b7ze\u00b7rei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganzen Welt, doch in der Luft,", "tokens": ["Der", "gan\u00b7zen", "Welt", ",", "doch", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Trotz allen W\u00fcrzen, riecht man stets", "tokens": ["Trotz", "al\u00b7len", "W\u00fcr\u00b7zen", ",", "riecht", "man", "stets"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den faulen Schellfischseelenduft.", "tokens": ["Den", "fau\u00b7len", "Schell\u00b7fischsee\u00b7len\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Oh, da\u00df ich gro\u00dfe Laster s\u00e4h,", "tokens": ["Oh", ",", "da\u00df", "ich", "gro\u00b7\u00dfe", "Las\u00b7ter", "s\u00e4h", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verbrechen, blutig, kolossal \u2013", "tokens": ["Ver\u00b7bre\u00b7chen", ",", "blu\u00b7tig", ",", "ko\u00b7los\u00b7sal", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD