{"dta.poem.11506": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft eines gescheuten harlequin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lachte selber nicht; und doch hab ich gemacht,", "tokens": ["Ich", "lach\u00b7te", "sel\u00b7ber", "nicht", ";", "und", "doch", "hab", "ich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$.", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df vieler thoren mund ihr eigen thun belacht.", "tokens": ["Da\u00df", "vie\u00b7ler", "tho\u00b7ren", "mund", "ihr", "ei\u00b7gen", "thun", "be\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und also that ich mehr, als ein Democritus:", "tokens": ["Und", "al\u00b7so", "that", "ich", "mehr", ",", "als", "ein", "De\u00b7mo\u00b7cri\u00b7tus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie kommt es, da\u00df er weis\u2019, ich n\u00e4rrisch, heissen mu\u00df.", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "er", "weis'", ",", "ich", "n\u00e4r\u00b7risch", ",", "heis\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "p