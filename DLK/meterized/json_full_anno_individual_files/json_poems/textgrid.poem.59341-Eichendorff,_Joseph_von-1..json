, "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Ziehn die Br\u00fcder in den Wald hinunter;", "tokens": ["Ziehn", "die", "Br\u00fc\u00b7der", "in", "den", "Wald", "hin\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und bei dem Sange und H\u00f6rnerklange", "tokens": ["Und", "bei", "dem", "San\u00b7ge", "und", "H\u00f6r\u00b7ner\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Wird mir immer so bange, bange.", "tokens": ["Wird", "mir", "im\u00b7mer", "so", "ban\u00b7ge", ",", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00fc\u00dft ich nur immer, wo du jetzo bist,", "tokens": ["W\u00fc\u00dft", "ich", "nur", "im\u00b7mer", ",", "wo", "du", "jet\u00b7zo", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00fcrd mir schon wohler auf kurze Frist.", "tokens": ["W\u00fcrd", "mir", "schon", "woh\u00b7ler", "auf", "kur\u00b7ze", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "K\u00f6nntest du mich nur \u00f