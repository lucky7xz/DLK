{"dta.poem.10072": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit, sich der Sonne  \n zu freuen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein C\u00f6rper sp\u00fchrt das g\u00fcldne Sonnen-Licht,", "tokens": ["Dein", "C\u00f6r\u00b7per", "sp\u00fchrt", "das", "g\u00fcld\u00b7ne", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Thier\u2019 und Pflantzen; anders nicht.", "tokens": ["Wie", "Thier'", "und", "Pflant\u00b7zen", ";", "an\u00b7ders", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$.", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, wo aus dem Licht der Sonne", "tokens": ["Al\u00b7lein", ",", "wo", "aus", "dem", "Licht", "der", "Son\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht auch zugleich ein Freuden-Licht,", "tokens": ["Nicht", "auch", "zu\u00b7gleich", "ein", "Freu\u00b7den\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein heller Strahl, von Lust und Wonne,", "tokens": ["Ein", "hel\u00b7ler", "Strahl", ",", "von", "Lust", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In deiner Seele Wesen bricht;", "tokens": ["In", "dei\u00b7ner", "See\u00b7le", "We\u00b7sen", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nicht darin ein Andacht-Feur erreget,", "tokens": ["Und", "nicht", "da\u00b7rin", "ein", "An\u00b7dacht\u00b7Feur", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dich nicht zu dem Sch\u00f6pfer lenckt,", "tokens": ["Und", "dich", "nicht", "zu", "dem", "Sch\u00f6p\u00b7fer", "lenckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der ihre Herrlichkeit erschuff, und sie dir schenckt,", "tokens": ["Der", "ih\u00b7re", "Herr\u00b7lich\u00b7keit", "er\u00b7schuff", ",", "und", "sie", "dir", "schenckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dich zum dancken nicht beweget;", "tokens": ["Und", "dich", "zum", "dan\u00b7cken", "nicht", "be\u00b7we\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So siehest du wahrhaftig sie,", "tokens": ["So", "sie\u00b7hest", "du", "wahr\u00b7haf\u00b7tig", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht anders an, als wie ein Vieh.", "tokens": ["Nicht", "an\u00b7ders", "an", ",", "als", "wie", "ein", "Vieh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}