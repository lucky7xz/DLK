{"dta.poem.11714": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Um Mitternacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Bed\u00e4chtig stieg die Nacht an's Land,", "tokens": ["Be\u00b7d\u00e4ch\u00b7tig", "stieg", "die", "Nacht", "an's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lehnt tr\u00e4umend an der Berge Wand,", "tokens": ["Lehnt", "tr\u00e4u\u00b7mend", "an", "der", "Ber\u00b7ge", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Auge sieht die goldne Wage nun", "tokens": ["Ihr", "Au\u00b7ge", "sieht", "die", "gold\u00b7ne", "Wa\u00b7ge", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Zeit in gleichen Schalen stille ruhn,", "tokens": ["Der", "Zeit", "in", "glei\u00b7chen", "Scha\u00b7len", "stil\u00b7le", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und kecker rauschen die Quellen hervor,", "tokens": ["Und", "ke\u00b7cker", "rau\u00b7schen", "die", "Quel\u00b7len", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie singen der Mutter, der Nacht, in's Ohr", "tokens": ["Sie", "sin\u00b7gen", "der", "Mut\u00b7ter", ",", "der", "Nacht", ",", "in's", "Ohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Vom Tage,", "tokens": ["Vom", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Vom heute gewesenen Tage.", "tokens": ["Vom", "heu\u00b7te", "ge\u00b7we\u00b7se\u00b7nen", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Das uralt alte Schlummerlied,", "tokens": ["Das", "ur\u00b7alt", "al\u00b7te", "Schlum\u00b7mer\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie achtet's nicht, sie ist es m\u00fcd';", "tokens": ["Sie", "ach\u00b7tet's", "nicht", ",", "sie", "ist", "es", "m\u00fcd'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr klingt des Himmels Bl\u00e4ue s\u00fc\u00dfer noch,", "tokens": ["Ihr", "klingt", "des", "Him\u00b7mels", "Bl\u00e4u\u00b7e", "s\u00fc\u00b7\u00dfer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der fl\u00fccht'gen Stunden gleichgeschwungnes Joch.", "tokens": ["Der", "fl\u00fccht'\u00b7gen", "Stun\u00b7den", "gleich\u00b7ge\u00b7schwung\u00b7nes", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch immer behalten die Quellen das Wort,", "tokens": ["Doch", "im\u00b7mer", "be\u00b7hal\u00b7ten", "die", "Quel\u00b7len", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Es singen die Wasser im Schlafe noch fort", "tokens": ["Es", "sin\u00b7gen", "die", "Was\u00b7ser", "im", "Schla\u00b7fe", "noch", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Vom Tage,", "tokens": ["Vom", "Ta\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Vom heute gewesenen Tage.", "tokens": ["Vom", "heu\u00b7te", "ge\u00b7we\u00b7se\u00b7nen", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}