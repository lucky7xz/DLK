{"textgrid.poem.31295": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er siegt! mein Perseus siegt! \u2013 Ihr Freudenz\u00e4hren,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er siegt! mein Perseus siegt! \u2013 Ihr Freudenz\u00e4hren,", "tokens": ["Er", "siegt", "!", "mein", "Per\u00b7seus", "siegt", "!", "\u2013", "Ihr", "Freu\u00b7den\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstickt nicht meinen Lobgesang! \u2013", "tokens": ["Er\u00b7stickt", "nicht", "mei\u00b7nen", "Lob\u00b7ge\u00b7sang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Fluten meines Stroms, erz\u00e4hlt in allen Meeren", "tokens": ["O", "Flu\u00b7ten", "mei\u00b7nes", "Stroms", ",", "er\u00b7z\u00e4hlt", "in", "al\u00b7len", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Drachen Untergang!", "tokens": ["Das", "Dra\u00b7chen", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier, wo der Belt, mein Kolberg zu verschonen,", "tokens": ["Hier", ",", "wo", "der", "Belt", ",", "mein", "Kol\u00b7berg", "zu", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit D\u00fcnen sein Gestad' umzieht,", "tokens": ["Mit", "D\u00fc\u00b7nen", "sein", "Ge\u00b7stad'", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sass' ich, und sang entz\u00fcckt den horchenden Tritonen", "tokens": ["Sass'", "ich", ",", "und", "sang", "ent\u00b7z\u00fcckt", "den", "hor\u00b7chen\u00b7den", "Tri\u00b7to\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von meinem Freund' ein Lied.", "tokens": ["Von", "mei\u00b7nem", "Freund'", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bber schlug das Raubthier j\u00fcngst, das der beschneyte", "tokens": ["\u00bb", "er", "schlug", "das", "Raubt\u00b7hier", "j\u00fcngst", ",", "das", "der", "be\u00b7schney\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Riph\u00e4us auf mich ausgespien,", "tokens": ["Ri\u00b7ph\u00e4us", "auf", "mich", "aus\u00b7ge\u00b7spi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich, verlassen von den G\u00f6ttern, seine Beute", "tokens": ["Als", "ich", ",", "ver\u00b7las\u00b7sen", "von", "den", "G\u00f6t\u00b7tern", ",", "sei\u00b7ne", "Beu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Unwiederbringlich schien.\u00ab", "tokens": ["Un\u00b7wie\u00b7der\u00b7bring\u00b7lich", "schien", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sprachs: als ich urpl\u00f6tzlich einen Drachen", "tokens": ["Ich", "sprachs", ":", "als", "ich", "ur\u00b7pl\u00f6tz\u00b7lich", "ei\u00b7nen", "Dra\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus blauer Tiefe steigen sah", "tokens": ["Aus", "blau\u00b7er", "Tie\u00b7fe", "stei\u00b7gen", "sah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit f\u00fcnfzig aufgerissnen feuerspeynden Rachen:", "tokens": ["Mit", "f\u00fcnf\u00b7zig", "auf\u00b7ge\u00b7riss\u00b7nen", "feu\u00b7er\u00b7speyn\u00b7den", "Ra\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohnm\u00e4chtig lag ich da.", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "lag", "ich", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Perseus flog in diesem Augenblicke", "tokens": ["Mein", "Per\u00b7seus", "flog", "in", "die\u00b7sem", "Au\u00b7gen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NE", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herab von seiner Warte, schwang", "tokens": ["Her\u00b7ab", "von", "sei\u00b7ner", "War\u00b7te", ",", "schwang"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein glorreich Eisen, hielt den Tod im Meer zur\u00fccke", "tokens": ["Sein", "glor\u00b7reich", "Ei\u00b7sen", ",", "hielt", "den", "Tod", "im", "Meer", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dreymal neun Tage lang.", "tokens": ["Drey\u00b7mal", "neun", "Ta\u00b7ge", "lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ha! welche Flammenstr\u00f6me schoss die Hyder", "tokens": ["Ha", "!", "wel\u00b7che", "Flam\u00b7men\u00b7str\u00f6\u00b7me", "schoss", "die", "Hy\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAT", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach seinem Leben! \u2013 Endlich fand", "tokens": ["Nach", "sei\u00b7nem", "Le\u00b7ben", "!", "\u2013", "End\u00b7lich", "fand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Flehn der G\u00f6tter Ohr: und Waffen fielen nieder", "tokens": ["Mein", "Flehn", "der", "G\u00f6t\u00b7ter", "Ohr", ":", "und", "Waf\u00b7fen", "fie\u00b7len", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$.", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da, wo mein Gastfreund stand.", "tokens": ["Da", ",", "wo", "mein", "Gast\u00b7freund", "stand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So bald ihm Plutons Helm das Haupt verh\u00fcllte,", "tokens": ["So", "bald", "ihm", "Plu\u00b7tons", "Helm", "das", "Haupt", "ver\u00b7h\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NE", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn Hermes Fl\u00fcgel trug, der Speer", "tokens": ["Ihn", "Her\u00b7mes", "Fl\u00fc\u00b7gel", "trug", ",", "der", "Speer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NE", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schrecklichen Minerva seine Rechte f\u00fcllte:", "tokens": ["Der", "schreck\u00b7li\u00b7chen", "Mi\u00b7ner\u00b7va", "sei\u00b7ne", "Rech\u00b7te", "f\u00fcll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "St\u00fcrzt' er die Pest ins Meer.", "tokens": ["St\u00fcrzt'", "er", "die", "Pest", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Von meinen Lippen soll sein Lob erschallen,", "tokens": ["Von", "mei\u00b7nen", "Lip\u00b7pen", "soll", "sein", "Lob", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich feyre dankbar meinen Held,", "tokens": ["Ich", "fey\u00b7re", "dank\u00b7bar", "mei\u00b7nen", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lang' in dieses Hafens Arme Segel wallen", "tokens": ["So", "lang'", "in", "die\u00b7ses", "Ha\u00b7fens", "Ar\u00b7me", "Se\u00b7gel", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Ostwind' aufgeschwellt.", "tokens": ["Vom", "Ost\u00b7wind'", "auf\u00b7ge\u00b7schwellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihm selbst will ich, wann er den Strand begr\u00fcsset,", "tokens": ["Ihm", "selbst", "will", "ich", ",", "wann", "er", "den", "Strand", "be\u00b7gr\u00fcs\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf seine Wege Kalmus streun", "tokens": ["Auf", "sei\u00b7ne", "We\u00b7ge", "Kal\u00b7mus", "streun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Muscheln; denn mein Fluss ist arm: kein Goldfand fliesset,", "tokens": ["Und", "Mu\u00b7scheln", ";", "denn", "mein", "Fluss", "ist", "arm", ":", "kein", "Gold\u00b7fand", "flies\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kaum Ambra rollt hinein.", "tokens": ["Kaum", "A\u00b7mbra", "rollt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und du, mein Barde, der du vor den Thoren", "tokens": ["Und", "du", ",", "mein", "Bar\u00b7de", ",", "der", "du", "vor", "den", "Tho\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deiner m\u00fctterlichen Stadt", "tokens": ["Von", "dei\u00b7ner", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einst Lieder lalletest, wenn sie, die dich geboren,", "tokens": ["Einst", "Lie\u00b7der", "lal\u00b7le\u00b7test", ",", "wenn", "sie", ",", "die", "dich", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch deine Liebe hat:", "tokens": ["Noch", "dei\u00b7ne", "Lie\u00b7be", "hat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "So singe meinen Liebling, meinen Retter", "tokens": ["So", "sin\u00b7ge", "mei\u00b7nen", "Lieb\u00b7ling", ",", "mei\u00b7nen", "Ret\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jene Laute, die dir j\u00fcngst", "tokens": ["In", "je\u00b7ne", "Lau\u00b7te", ",", "die", "dir", "j\u00fcngst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Besaitet ward, in welche du den Kampf der G\u00f6tter", "tokens": ["Be\u00b7sai\u00b7tet", "ward", ",", "in", "wel\u00b7che", "du", "den", "Kampf", "der", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit den Titanen singst.", "tokens": ["Mit", "den", "Ti\u00b7ta\u00b7nen", "singst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er siegt! mein Perseus siegt! \u2013 Ihr Freudenz\u00e4hren,", "tokens": ["Er", "siegt", "!", "mein", "Per\u00b7seus", "siegt", "!", "\u2013", "Ihr", "Freu\u00b7den\u00b7z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstickt nicht meinen Lobgesang! \u2013", "tokens": ["Er\u00b7stickt", "nicht", "mei\u00b7nen", "Lob\u00b7ge\u00b7sang", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Fluten meines Stroms, erz\u00e4hlt in allen Meeren", "tokens": ["O", "Flu\u00b7ten", "mei\u00b7nes", "Stroms", ",", "er\u00b7z\u00e4hlt", "in", "al\u00b7len", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Drachen Untergang!", "tokens": ["Das", "Dra\u00b7chen", "Un\u00b7ter\u00b7gang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Hier, wo der Belt, mein Kolberg zu verschonen,", "tokens": ["Hier", ",", "wo", "der", "Belt", ",", "mein", "Kol\u00b7berg", "zu", "ver\u00b7scho\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit D\u00fcnen sein Gestad' umzieht,", "tok