{"dta.poem.10059": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Rose. Andere Betrachtungen der Rosen siehe  Tom. I. pag. 81. 92.  \n Tom. II. p. 194. Tom. III. p. 616. Tom. IV. p.  69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, frische Fr\u00fchlings-Rosen,", "tokens": ["Will\u00b7kom\u00b7men", ",", "fri\u00b7sche", "Fr\u00fch\u00b7lings\u00b7Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr die Seele selbst, in eurer Lieblichkeit,", "tokens": ["Die", "ihr", "die", "See\u00b7le", "selbst", ",", "in", "eu\u00b7rer", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Aug\u2019 und Nase, liebzukosen,", "tokens": ["Durch", "Aug'", "und", "Na\u00b7se", ",", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wunderbar erschaffen seyd:", "tokens": ["So", "wun\u00b7der\u00b7bar", "er\u00b7schaf\u00b7fen", "seyd", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ihr von Fingern der Natur", "tokens": ["Die", "ihr", "von", "Fin\u00b7gern", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit solcher lieb- und z\u00e4rtlichen Figur", "tokens": ["Mit", "sol\u00b7cher", "lie\u00b7b", "und", "z\u00e4rt\u00b7li\u00b7chen", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Bewunderns wehrt begabt,", "tokens": ["Be\u00b7wun\u00b7derns", "wehrt", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und eine Balsam-Krafft von ihr empfangen habt.", "tokens": ["Und", "ei\u00b7ne", "Bal\u00b7sam\u00b7Krafft", "von", "ihr", "emp\u00b7fan\u00b7gen", "habt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da ich euch ietzt zum ersten mahl", "tokens": ["Da", "ich", "euch", "ietzt", "zum", "ers\u00b7ten", "mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Fr\u00fchling wieder sehe,", "tokens": ["In", "die\u00b7sem", "Fr\u00fch\u00b7ling", "wie\u00b7der", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So deucht mich, da\u00df von euch ein kleiner rother Strahl", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "von", "euch", "ein", "klei\u00b7ner", "ro\u00b7ther", "Strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir durch den Blick ins Hertze gehe:", "tokens": ["Mir", "durch", "den", "Blick", "ins", "Hert\u00b7ze", "ge\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPRART", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch ich eigentlich empfinde,", "tokens": ["Wo\u00b7durch", "ich", "ei\u00b7gent\u00b7lich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wie er daselbst von Andacht und von Lust", "tokens": ["Wie", "er", "da\u00b7selbst", "von", "An\u00b7dacht", "und", "von", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PAV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein helles Feur in meiner Brust,", "tokens": ["Ein", "hel\u00b7les", "Feur", "in", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit froher Danck-Begier vermischt, entz\u00fcnde.", "tokens": ["Mit", "fro\u00b7her", "Dan\u00b7ck\u00b7Be\u00b7gier", "ver\u00b7mischt", ",", "ent\u00b7z\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich lencke mich, mit lechzendem Vergn\u00fcgen,", "tokens": ["Ich", "len\u00b7cke", "mich", ",", "mit", "lech\u00b7zen\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur ewig seeligen allm\u00e4chtgen Liebes-Gluht,", "tokens": ["Zur", "e\u00b7wig", "see\u00b7li\u00b7gen", "all\u00b7m\u00e4cht\u00b7gen", "Lie\u00b7bes\u00b7Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die alle diese Wunder thut;", "tokens": ["Die", "al\u00b7le", "die\u00b7se", "Wun\u00b7der", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch welche blo\u00df allein", "tokens": ["Durch", "wel\u00b7che", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Rosen ihrer Farben Schein,", "tokens": ["Die", "Ro\u00b7sen", "ih\u00b7rer", "Far\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Hertz-erquickenden Geruch und Kr\u00e4ffte kriegen.", "tokens": ["Und", "Hertz\u00b7er\u00b7qui\u00b7cken\u00b7den", "Ge\u00b7ruch", "und", "Kr\u00e4ff\u00b7te", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich dencke nach: wo kommt die Anmuth her,", "tokens": ["Ich", "den\u00b7cke", "nach", ":", "wo", "kommt", "die", "An\u00b7muth", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die in der holden Bluhme gl\u00fchet,", "tokens": ["Die", "in", "der", "hol\u00b7den", "Bluh\u00b7me", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die ein vern\u00fcuftigs Aug\u2019 ohn\u2019 innre Lust nicht siehet,", "tokens": ["Die", "ein", "ver\u00b7n\u00fcuf\u00b7tigs", "Aug'", "ohn'", "inn\u00b7re", "Lust", "nicht", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und die die Seel\u2019 aus ihr auch durch die Rase ziehet,", "tokens": ["Und", "die", "die", "Seel'", "aus", "ihr", "auch", "durch", "die", "Ra\u00b7se", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als blo\u00df von GOTT allein?", "tokens": ["Als", "blo\u00df", "von", "GoTT", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}