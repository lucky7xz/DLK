{"dta.poem.7669": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entbehren magst du ehr den Segen vom Geschicke,", "tokens": ["Ent\u00b7beh\u00b7ren", "magst", "du", "ehr", "den", "Se\u00b7gen", "vom", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als so gesegnet seyn, da\u00df es dich nicht erquicke.", "tokens": ["Als", "so", "ge\u00b7seg\u00b7net", "seyn", ",", "da\u00df", "es", "dich", "nicht", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "VAINF", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ehr, sonn- und regenlos, o Pflanze dich geh\u00e4rmt,", "tokens": ["Ehr", ",", "sonn", "und", "re\u00b7gen\u00b7los", ",", "o", "Pflan\u00b7ze", "dich", "ge\u00b7h\u00e4rmt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "TRUNC", "KON", "ADJD", "$,", "FM", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Regen, der nicht n\u00e4\u00dft, und Sonnschein der nicht w\u00e4rmt.", "tokens": ["Als", "Re\u00b7gen", ",", "der", "nicht", "n\u00e4\u00dft", ",", "und", "Sonn\u00b7schein", "der", "nicht", "w\u00e4rmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word",