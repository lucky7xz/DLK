{"dta.poem.7306": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Brennende Liebe.   Crategus pyracantha , auch sonst der \u201ebrennende Busch\u201c genannt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Und willst du wissen, warum", "tokens": ["Und", "willst", "du", "wis\u00b7sen", ",", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "PWAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So sinnend ich manche Zeit,", "tokens": ["So", "sin\u00b7nend", "ich", "man\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mitunter so th\u00f6richt und dumm,", "tokens": ["Mi\u00b7tun\u00b7ter", "so", "th\u00f6\u00b7richt", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So unverzeihlich zerstreut,", "tokens": ["So", "un\u00b7ver\u00b7zeih\u00b7lich", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Willst wissen auch ohne Gnade,", "tokens": ["Willst", "wis\u00b7sen", "auch", "oh\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was denn so Liebes enth\u00e4lt", "tokens": ["Was", "denn", "so", "Lie\u00b7bes", "ent\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJA", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die heimlich verschlossene Lade,", "tokens": ["Die", "heim\u00b7lich", "ver\u00b7schlos\u00b7se\u00b7ne", "La\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "An die ich mich \u00f6fters gestellt?", "tokens": ["An", "die", "ich", "mich", "\u00f6f\u00b7ters", "ge\u00b7stellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Zwei Augen hab' ich gesehn,", "tokens": ["Zwei", "Au\u00b7gen", "hab'", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wie der Strahl im Gew\u00e4sser sich bricht,", "tokens": ["Wie", "der", "Strahl", "im", "Ge\u00b7w\u00e4s\u00b7ser", "sich", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und wo zwei Augen nur stehn,", "tokens": ["Und", "wo", "zwei", "Au\u00b7gen", "nur", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "CARD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da denke ich an ihr Licht.", "tokens": ["Da", "den\u00b7ke", "ich", "an", "ihr", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ja, als du neulich entwandtest", "tokens": ["Ja", ",", "als", "du", "neu\u00b7lich", "ent\u00b7wand\u00b7test"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Blume vom bl\u00fchenden Rain,", "tokens": ["Die", "Blu\u00b7me", "vom", "bl\u00fc\u00b7hen\u00b7den", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und \u00bb", "tokens": ["Und", "\u00bb"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Da fielen die Augen mir ein.", "tokens": ["Da", "fie\u00b7len", "die", "Au\u00b7gen", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Auch giebt's einer Stimme Ton,", "tokens": ["Auch", "giebt's", "ei\u00b7ner", "Stim\u00b7me", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tief, zitternd, wie Hornes Hall,", "tokens": ["Tief", ",", "zit\u00b7ternd", ",", "wie", "Hor\u00b7nes", "Hall", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "PWAV", "NE", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die thut's mir v\u00f6llig zum Hohn,", "tokens": ["Die", "thut's", "mir", "v\u00f6l\u00b7lig", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Sie folget mir \u00fcberall.", "tokens": ["Sie", "fol\u00b7get", "mir", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als j\u00fcngst im flimmernden Saale", "tokens": ["Als", "j\u00fcngst", "im", "flim\u00b7mern\u00b7den", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mich qu\u00e4lte der Geigen Gegell,", "tokens": ["Mich", "qu\u00e4l\u00b7te", "der", "Gei\u00b7gen", "Ge\u00b7gell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Da h\u00f6rt ich mit einem Male", "tokens": ["Da", "h\u00f6rt", "ich", "mit", "ei\u00b7nem", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die Stimme im Violoncell.", "tokens": ["Die", "Stim\u00b7me", "im", "Vi\u00b7o\u00b7lon\u00b7cell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Auch wei\u00df ich eine Gestalt,", "tokens": ["Auch", "wei\u00df", "ich", "ei\u00b7ne", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So leicht und kr\u00e4ftig zugleich,", "tokens": ["So", "leicht", "und", "kr\u00e4f\u00b7tig", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die schreitet vor mir im Wald,", "tokens": ["Die", "schrei\u00b7tet", "vor", "mir", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und gleitet \u00fcber den Teich;", "tokens": ["Und", "glei\u00b7tet", "\u00fc\u00b7ber", "den", "Teich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ja, als ich eben in Sinnen", "tokens": ["Ja", ",", "als", "ich", "e\u00b7ben", "in", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sah \u00fcber des Mondes Aug'", "tokens": ["Sah", "\u00fc\u00b7ber", "des", "Mon\u00b7des", "Aug'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Einen Wolkenstreifen zerrinnen,", "tokens": ["Ei\u00b7nen", "Wol\u00b7ken\u00b7strei\u00b7fen", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Das war ihre Form, wie ein Rauch.", "tokens": ["Das", "war", "ih\u00b7re", "Form", ",", "wie", "ein", "Rauch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und h\u00f6re, h\u00f6re zuletzt,", "tokens": ["Und", "h\u00f6\u00b7re", ",", "h\u00f6\u00b7re", "zu\u00b7letzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Dort liegt, da drinnen im Schrein,", "tokens": ["Dort", "liegt", ",", "da", "drin\u00b7nen", "im", "Schrein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein Tuch mit Blute genetzt,", "tokens": ["Ein", "Tuch", "mit", "Blu\u00b7te", "ge\u00b7netzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das legte ich heimlich hinein.", "tokens": ["Das", "leg\u00b7te", "ich", "heim\u00b7lich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Er ritzte sich nur an der Schneide,", "tokens": ["Er", "ritz\u00b7te", "sich", "nur", "an", "der", "Schnei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Beeren vom Strauch er mir hieb,", "tokens": ["Als", "Bee\u00b7ren", "vom", "Strauch", "er", "mir", "hieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nun hab' ich sie alle Beide,", "tokens": ["Nun", "hab'", "ich", "sie", "al\u00b7le", "Bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PIAT", "PIS", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.8": {"text": "Sein Blut und meine brennende Lieb'.", "tokens": ["Sein", "Blut", "und", "mei\u00b7ne", "bren\u00b7nen\u00b7de", "Lieb'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}