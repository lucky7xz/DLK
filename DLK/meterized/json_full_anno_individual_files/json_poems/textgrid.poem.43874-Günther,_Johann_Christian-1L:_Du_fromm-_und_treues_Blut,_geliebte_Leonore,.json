{"textgrid.poem.43874": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fromm- und treues Blut, geliebte Leonore,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fromm- und treues Blut, geliebte Leonore,", "tokens": ["Du", "from\u00b7m", "und", "treu\u00b7es", "Blut", ",", "ge\u00b7lieb\u00b7te", "Le\u00b7o\u00b7no\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "ADJA", "NN", "$,", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Hier ist dies schlechte Lied, so ich dir schuldig war.", "tokens": ["Hier", "ist", "dies", "schlech\u00b7te", "Lied", ",", "so", "ich", "dir", "schul\u00b7dig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADJA", "NN", "$,", "ADV", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein allzu sichrer Mund wagt es auf die Gefahr", "tokens": ["Mein", "all\u00b7zu", "sich\u00b7rer", "Mund", "wagt", "es", "auf", "die", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PTKA", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und plagt dein zartes Ohr mit einem Haberrohre.", "tokens": ["Und", "plagt", "dein", "zar\u00b7tes", "Ohr", "mit", "ei\u00b7nem", "Ha\u00b7berr\u00b7oh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-