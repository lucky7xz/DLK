{"dta.poem.19291": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde hat ein Recht, sich selber anzusehn", "tokens": ["Die", "Er\u00b7de", "hat", "ein", "Recht", ",", "sich", "sel\u00b7ber", "an\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRF", "ADV", "VVIZU"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Mittelpunkt, um den sich alle Himmel drehn.", "tokens": ["Als", "Mit\u00b7tel\u00b7punkt", ",", "um", "den", "sich", "al\u00b7le", "Him\u00b7mel", "drehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUI", "ART", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unschuldig \u00fcbte sie dis Recht seit alten Zeiten,", "tokens": ["Un\u00b7schul\u00b7dig", "\u00fcb\u00b7te", "sie", "dis", "Recht", "seit", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PDS", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Aufkl\u00e4rung auch soll es ihr nicht bestreiten.", "tokens": ["Und", "die", "Auf\u00b7kl\u00e4\u00b7rung", "auch", "soll", "es", "ihr", "nicht", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Zur Einsicht kam sie zwar, da\u00df sie nur sei ein Theil", "tokens": ["Zur", "Ein\u00b7sicht", "kam", "sie", "zwar", ",", "da\u00df", "sie", "nur", "sei", "ein", "Theil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Ganzen, und auf sie nicht eingeschr\u00e4nkt das Heil.", "tokens": ["Vom", "Gan\u00b7zen", ",", "und", "auf", "sie", "nicht", "ein\u00b7ge\u00b7schr\u00e4nkt", "das", "Heil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "F\u00fcrs Ganze l\u00e4sset sie den Geist des Ganzen sorgen,", "tokens": ["F\u00fcrs", "Gan\u00b7ze", "l\u00e4s\u00b7set", "sie", "den", "Geist", "des", "Gan\u00b7zen", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begn\u00fcgt, da\u00df sie sich f\u00fchlt an ihrem Theil geborgen.", "tokens": ["Be\u00b7gn\u00fcgt", ",", "da\u00df", "sie", "sich", "f\u00fchlt", "an", "ih\u00b7rem", "Theil", "ge\u00b7bor\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie f\u00fchlet fest sich stehn, und sieht den Himmel drehn;", "tokens": ["Sie", "f\u00fch\u00b7let", "fest", "sich", "stehn", ",", "und", "sieht", "den", "Him\u00b7mel", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "VVINF", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was kann vereintem Sehn und F\u00fchlen widerstehn?", "tokens": ["Was", "kann", "ver\u00b7ein\u00b7tem", "Sehn", "und", "F\u00fch\u00b7len", "wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Sonne scheint f\u00fcr sie am Tag, und in der Nacht", "tokens": ["Die", "Son\u00b7ne", "scheint", "f\u00fcr", "sie", "am", "Tag", ",", "und", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schm\u00fcckt ihr das Himmelbett der Sterne goldne Pracht.", "tokens": ["Schm\u00fcckt", "ihr", "das", "Him\u00b7mel\u00b7bett", "der", "Ster\u00b7ne", "gold\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Geist steigt wie das Licht zu ihr im Traume nieder,", "tokens": ["Der", "Geist", "steigt", "wie", "das", "Licht", "zu", "ihr", "im", "Trau\u00b7me", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihr Gedanke steigt empor und ihre Lieder.", "tokens": ["Und", "ihr", "Ge\u00b7dan\u00b7ke", "steigt", "em\u00b7por", "und", "ih\u00b7re", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Es ist der Augenschein, kein Schein, was ihr erschienen;", "tokens": ["Es", "ist", "der", "Au\u00b7gen\u00b7schein", ",", "kein", "Schein", ",", "was", "ihr", "er\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PIAT", "NN", "$,", "PWS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie dienet Gott, und wei\u00df, da\u00df ihr die Himmel dienen.", "tokens": ["Sie", "die\u00b7net", "Gott", ",", "und", "wei\u00df", ",", "da\u00df", "ihr", "die", "Him\u00b7mel", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und dienen sie ihr nicht? Es h\u00e4ngt in diesem Tanze", "tokens": ["Und", "die\u00b7nen", "sie", "ihr", "nicht", "?", "Es", "h\u00e4ngt", "in", "die\u00b7sem", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Ganzen wol das Glied, doch auch am Glied das Ganze.", "tokens": ["Am", "Gan\u00b7zen", "wol", "das", "Glied", ",", "doch", "auch", "am", "Glied", "das", "Gan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "O wunderbarer Bau, o Herr des Baus und Meister!", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7rer", "Bau", ",", "o", "Herr", "des", "Baus", "und", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Grundstein bist du selbst, Grundpfeiler deiner Geister.", "tokens": ["Dein", "Grund\u00b7stein", "bist", "du", "selbst", ",", "Grund\u00b7pfei\u00b7ler", "dei\u00b7ner", "Geis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Du bist der Architekt, du bist der Architrab,", "tokens": ["Du", "bist", "der", "Ar\u00b7chi\u00b7tekt", ",", "du", "bist", "der", "Ar\u00b7chi\u00b7trab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der K\u00f6nig, der sich selbst den K\u00f6nigsbau aufgab.", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "der", "sich", "selbst", "den", "K\u00f6\u00b7nigs\u00b7bau", "auf\u00b7gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "So gro\u00df, vollkommen, sch\u00f6n ist dein Palast, die Welt,", "tokens": ["So", "gro\u00df", ",", "voll\u00b7kom\u00b7men", ",", "sch\u00f6n", "ist", "dein", "Pa\u00b7last", ",", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df jeder Winkel sich f\u00fcr deinen Thronsaal h\u00e4lt.", "tokens": ["Da\u00df", "je\u00b7der", "Win\u00b7kel", "sich", "f\u00fcr", "dei\u00b7nen", "Thron\u00b7sa\u00b7al", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}