{"textgrid.poem.44611": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Gold und Silber", "genre": "verse", "period": "N.A.", "pub_year": 1866, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wohl ein jeder wei\u00df,", "tokens": ["Wie", "wohl", "ein", "je\u00b7der", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch bleiben ", "tokens": ["Doch", "blei\u00b7ben"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Der Flei\u00df macht Gold; nicht jeder triffts,", "tokens": ["Der", "Flei\u00df", "macht", "Gold", ";", "nicht", "je\u00b7der", "triffts", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man plagt sich fr\u00fch und spat", "tokens": ["Man", "plagt", "sich", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dankt zuletzt dem lieben Gott,", "tokens": ["Und", "dankt", "zu\u00b7letzt", "dem", "lie\u00b7ben", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man sein Auskomm hat.", "tokens": ["Wenn", "man", "sein", "Aus\u00b7komm", "hat", "."], "token_i