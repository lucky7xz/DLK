{"dta.poem.23411": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vice versa.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Widder insgemein;", "tokens": ["Die", "Wid\u00b7der", "ins\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Warum soll's nicht der Adel thun?", "tokens": ["Wa\u00b7rum", "soll's", "nicht", "der", "A\u00b7del", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll er nicht dankbar sein?", "tokens": ["Soll", "er", "nicht", "dank\u00b7bar", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Der Adel will nur dankbar sein", "tokens": ["Der", "A\u00b7del", "will", "nur", "dank\u00b7bar", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und niemals mehr als jetzt:", "tokens": ["Und", "nie\u00b7mals", "mehr", "als", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "KOKOM", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schafe halten ja allein", "tokens": ["Die", "Scha\u00b7fe", "hal\u00b7ten", "ja", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Adel noch zuletzt,", "tokens": ["Den", "A\u00b7del", "noch", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}