{"textgrid.poem.42922": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich spreche von Flugmaschinen.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich spreche von Flugmaschinen.", "tokens": ["Ich", "spre\u00b7che", "von", "Flug\u00b7ma\u00b7schi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie summen lauter als Bienen", "tokens": ["Sie", "sum\u00b7men", "lau\u00b7ter", "als", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "KOKOM", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sind eine Kreuzung von Taube,", "tokens": ["Und", "sind", "ei\u00b7ne", "Kreu\u00b7zung", "von", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ente, Maik\u00e4fer und Schiffsschraube.", "tokens": ["En\u00b7te", ",", "Mai\u00b7k\u00e4\u00b7fer", "und", "Schiffs\u00b7schrau\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Sie nisten einzeln, paar- und gruppen-", "tokens": ["Sie", "nis\u00b7ten", "ein\u00b7zeln", ",", "paa\u00b7r", "und", "grup\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weise in Hallen und Schuppen.", "tokens": ["Wei\u00b7se