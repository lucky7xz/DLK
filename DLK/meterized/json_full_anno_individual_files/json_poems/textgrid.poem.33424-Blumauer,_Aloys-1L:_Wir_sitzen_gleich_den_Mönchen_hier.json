{"textgrid.poem.33424": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir sitzen gleich den M\u00f6nchen hier", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sitzen gleich den M\u00f6nchen hier", "tokens": ["Wir", "sit\u00b7zen", "gleich", "den", "M\u00f6n\u00b7chen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei immer festverschlo\u00df'ner Th\u00fcr", "tokens": ["Bei", "im\u00b7mer", "fest\u00b7ver\u00b7schlo\u00df'\u00b7ner", "Th\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In unser'm Speisesaale,", "tokens": ["In", "un\u00b7ser'm", "Spei\u00b7se\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keine von euch, Schwestern, wird", "tokens": ["Und", "kei\u00b7ne", "von", "euch", ",", "Schwes\u00b7tern", ",", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PIAT", "APPR", "PPER", "$,", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Maurern jemals invitirt", "tokens": ["Von", "Mau\u00b7rern", "je\u00b7mals", "in\u00b7vi\u00b7tirt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu ihrem Freudenmahle.", "tokens": ["Zu", "ih\u00b7rem", "Freu\u00b7den\u00b7mah\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Allein verdenkt uns dieses nicht;", "tokens": ["Al\u00b7lein", "ver\u00b7denkt", "uns", "die\u00b7ses", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir stehen auch in Eid und Pflicht,", "tokens": ["Wir", "ste\u00b7hen", "auch", "in", "Eid", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich andern Ordensgliedern,", "tokens": ["Gleich", "an\u00b7dern", "Or\u00b7dens\u00b7glie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hatten auch zu jeder Zeit", "tokens": ["Und", "hat\u00b7ten", "auch", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch manche kleine Aehnlichkeit", "tokens": ["Noch", "man\u00b7che", "klei\u00b7ne", "A\u00b7ehn\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit andern Ordensbr\u00fcdern.", "tokens": ["Mit", "an\u00b7dern", "Or\u00b7dens\u00b7br\u00fc\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein jeder neue Orden wird", "tokens": ["Ein", "je\u00b7der", "neu\u00b7e", "Or\u00b7den", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eine Bulle confirmirt:", "tokens": ["Durch", "ei\u00b7ne", "Bul\u00b7le", "con\u00b7fir\u00b7mirt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ward auch uns bescheeret.", "tokens": ["Die", "ward", "auch", "uns", "be\u00b7schee\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur hat der Papst, der uns zu Lieb'", "tokens": ["Nur", "hat", "der", "Papst", ",", "der", "uns", "zu", "Lieb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Confirmirungsbulle schrieb,", "tokens": ["Die", "Con\u00b7fir\u00b7mi\u00b7rungs\u00b7bul\u00b7le", "schrieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Feder umgekehret.", "tokens": ["Die", "Fe\u00b7der", "um\u00b7ge\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Ordensleute betteln gern,", "tokens": ["Die", "Or\u00b7dens\u00b7leu\u00b7te", "bet\u00b7teln", "gern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pflegen oft die grossen Herr'n", "tokens": ["Und", "pfle\u00b7gen", "oft", "die", "gros\u00b7sen", "Herr'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Geben zu erm\u00fcden;", "tokens": ["Mit", "Ge\u00b7ben", "zu", "er\u00b7m\u00fc\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch wir verstehen diese Kunst:", "tokens": ["Auch", "wir", "ver\u00b7ste\u00b7hen", "die\u00b7se", "Kunst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Doch fleh'n wir nur um eu're Gunst,", "tokens": ["Doch", "fleh'n", "wir", "nur", "um", "eu'\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sind damit zufrieden.", "tokens": ["Und", "sind", "da\u00b7mit", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die M\u00f6nche schw\u00f6ren dreimal laut,", "tokens": ["Die", "M\u00f6n\u00b7che", "schw\u00f6\u00b7ren", "drei\u00b7mal", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schw\u00f6ren, Freiheit, Geld und Braut", "tokens": ["Und", "schw\u00f6\u00b7ren", ",", "Frei\u00b7heit", ",", "Geld", "und", "Braut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Lebelang zu l\u00e4stern:", "tokens": ["Auf", "Le\u00b7be\u00b7lang", "zu", "l\u00e4s\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch Maurer schw\u00f6ren einen Eid,", "tokens": ["Auch", "Mau\u00b7rer", "schw\u00f6\u00b7ren", "ei\u00b7nen", "Eid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch den nur der Verschwiegenheit", "tokens": ["Doch", "den", "nur", "der", "Ver\u00b7schwie\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADV", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Zum Besten ihrer Schwestern.", "tokens": ["Zum", "Bes\u00b7ten", "ih\u00b7rer", "Schwes\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die M\u00f6nche sind bei Taufen stumm,", "tokens": ["Die", "M\u00f6n\u00b7che", "sind", "bei", "Tau\u00b7fen", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und haben oft Silentium;", "tokens": ["Und", "ha\u00b7ben", "oft", "Si\u00b7len\u00b7ti\u00b7um", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir habens oft zu Zeiten:", "tokens": ["Wir", "ha\u00b7bens", "oft", "zu", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch darf bei uns, wenn's einer bricht,", "tokens": ["Doch", "darf", "bei", "uns", ",", "wenn's", "ei\u00b7ner", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Th\u00e4ter darum eben nicht", "tokens": ["Der", "Th\u00e4\u00b7ter", "da\u00b7rum", "e\u00b7ben", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gleich auf dem Boden reiten.", "tokens": ["Gleich", "auf", "dem", "Bo\u00b7den", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Die M\u00f6nche senden Sammler aus,", "tokens": ["Die", "M\u00f6n\u00b7che", "sen\u00b7den", "Samm\u00b7ler", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schicken sie von Haus zu Haus", "tokens": ["Und", "schi\u00b7cken", "sie", "von", "Haus", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Wein und Schmalz und K\u00e4lber;", "tokens": ["Um", "Wein", "und", "Schmalz", "und", "K\u00e4l\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch Maurer sammeln so wie die,", "tokens": ["Auch", "Mau\u00b7rer", "sam\u00b7meln", "so", "wie", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ADV", "KOKOM", "ART", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch nur aus eig'nem Sack, und nie,", "tokens": ["Doch", "nur", "aus", "eig'\u00b7nem", "Sack", ",", "und", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jene, f\u00fcr sich selber.", "tokens": ["Wie", "je\u00b7ne", ",", "f\u00fcr", "sich", "sel\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auch einen Procurator h\u00e4lt", "tokens": ["Auch", "ei\u00b7nen", "Pro\u00b7cu\u00b7ra\u00b7tor", "h\u00e4lt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Sich jedes Kloster auf der Welt,", "tokens": ["Sich", "je\u00b7des", "Klos\u00b7ter", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lebt von seiner Gnade,", "tokens": ["Und", "lebt", "von", "sei\u00b7ner", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Mann wird auch bei uns gew\u00e4hlt,", "tokens": ["Der", "Mann", "wird", "auch", "bei", "uns", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Doch trinkt er nicht von unser'm Geld,", "tokens": ["Doch", "trinkt", "er", "nicht", "von", "un\u00b7ser'm", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie jener, Chokolade.", "tokens": ["Wie", "je\u00b7ner", ",", "Cho\u00b7ko\u00b7la\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und jedes fette Kloster hat", "tokens": ["Und", "je\u00b7des", "fet\u00b7te", "Klos\u00b7ter", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch einen eig'nen K\u00fcchenrath", "tokens": ["Auch", "ei\u00b7nen", "eig'\u00b7nen", "K\u00fc\u00b7chen\u00b7rath"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei allen E\u00dfgelagen;", "tokens": ["Bei", "al\u00b7len", "E\u00df\u00b7ge\u00b7la\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei uns ist dieses Aemtchen schwer,", "tokens": ["Bei", "uns", "ist", "die\u00b7ses", "A\u00b7emt\u00b7chen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PDAT", "NN", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Denn uns'rer sorgt f\u00fcr noch was mehr", "tokens": ["Denn", "un\u00b7s'\u00b7rer", 