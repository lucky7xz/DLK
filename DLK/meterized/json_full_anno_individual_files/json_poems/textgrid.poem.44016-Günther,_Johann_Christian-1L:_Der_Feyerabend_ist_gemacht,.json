ra"}, "line.3": {"text": "Die Mi\u00dfethat ist dir erla\u00dfen.", "tokens": ["Die", "Mi\u00b7\u00dfet\u00b7hat", "ist", "dir", "er\u00b7la\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum kan mein Glaube ganz getrost,", "tokens": ["Drum", "kan", "mein", "Glau\u00b7be", "ganz", "ge\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Welt und Satan schon erbost,", "tokens": ["Ist", "Welt", "und", "Sa\u00b7tan", "schon", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey deiner Warheit Ancker fa\u00dfen.", "tokens": ["Bey", "dei\u00b7ner", "War\u00b7heit", "An\u00b7cker", "fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Mein Abendopfer ist ein Lied,", "tokens": ["Mein", "A\u00b7ben\u00b7dop\u00b7fer", "ist", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir zu dancken sich bem\u00fcht,", "tokens": ["Das", "dir", "zu", "dan\u00b7cken", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVINF", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Brust entz\u00fcndet Andachtskerzen;", "token