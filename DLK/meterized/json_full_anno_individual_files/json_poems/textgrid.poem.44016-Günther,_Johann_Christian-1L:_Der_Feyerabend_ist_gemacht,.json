{"textgrid.poem.44016": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Feyerabend ist gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Feyerabend ist gemacht,", "tokens": ["Der", "Fe\u00b7ye\u00b7ra\u00b7bend", "ist", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Arbeit schl\u00e4ft, der Traum erwacht,", "tokens": ["Die", "Ar\u00b7beit", "schl\u00e4ft", ",", "der", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne f\u00fchrt die Pferde trincken;", "tokens": ["Die", "Son\u00b7ne", "f\u00fchrt", "die", "Pfer\u00b7de", "trin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erdkrei\u00df wandert zu der Ruh,", "tokens": ["Der", "Erd\u00b7krei\u00df", "wan\u00b7dert", "zu", "der", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Nacht dr\u00fcckt ihm die Augen zu,", "tokens": ["Die", "Nacht", "dr\u00fcckt", "ihm", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die schon dem s\u00fc\u00dfen Schlafe wincken.", "tokens": ["Die", "schon", "dem", "s\u00fc\u00b7\u00dfen", "Schla\u00b7fe", "win\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich, Sch\u00f6pfer, deine Creatur,", "tokens": ["Ich", ",", "Sch\u00f6p\u00b7fer", ",", "dei\u00b7ne", "Crea\u00b7tur", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bekenne, da\u00df ich auf der Spur", "tokens": ["Be\u00b7ken\u00b7ne", ",", "da\u00df", "ich", "auf", "der", "Spur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der S\u00fcnder diesen Tag gewandelt;", "tokens": ["Der", "S\u00fcn\u00b7der", "die\u00b7sen", "Tag", "ge\u00b7wan\u00b7delt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe dein Verboth verlezt,", "tokens": ["Ich", "ha\u00b7be", "dein", "Ver\u00b7both", "ver\u00b7lezt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich dir in allem widersezt", "tokens": ["Mich", "dir", "in", "al\u00b7lem", "wi\u00b7der\u00b7sezt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "APPR", "PIS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wider meine Pflicht gehandelt.", "tokens": ["Und", "wi\u00b7der", "mei\u00b7ne", "Pflicht", "ge\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch weil ein Quintchen Vaterhuld", "tokens": ["Doch", "weil", "ein", "Quint\u00b7chen", "Va\u00b7ter\u00b7huld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel tausend Centner meiner Schuld", "tokens": ["Viel", "tau\u00b7send", "Cent\u00b7ner", "mei\u00b7ner", "Schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch dein Erbarmen \u00fcberwieget,", "tokens": ["Durch", "dein", "Er\u00b7bar\u00b7men", "\u00fc\u00b7berw\u00b7ie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gieb Genade vor das Recht", "tokens": ["So", "gieb", "Ge\u00b7na\u00b7de", "vor", "das", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und z\u00fcrne nicht auf deinen Knecht,", "tokens": ["Und", "z\u00fcr\u00b7ne", "nicht", "auf", "dei\u00b7nen", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich an deinen F\u00fc\u00dfen schmieget.", "tokens": ["Der", "sich", "an", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "schmie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Beichte folgt das Gnadenwort:", "tokens": ["Der", "Beich\u00b7te", "folgt", "das", "Gna\u00b7den\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steh auf, mein Sohn, und wandre fort,", "tokens": ["Steh", "auf", ",", "mein", "Sohn", ",", "und", "wand\u00b7re", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mi\u00dfethat ist dir erla\u00dfen.", "tokens": ["Die", "Mi\u00b7\u00dfet\u00b7hat", "ist", "dir", "er\u00b7la\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum kan mein Glaube ganz getrost,", "tokens": ["Drum", "kan", "mein", "Glau\u00b7be", "ganz", "ge\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Welt und Satan schon erbost,", "tokens": ["Ist", "Welt", "und", "Sa\u00b7tan", "schon", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bey deiner Warheit Ancker fa\u00dfen.", "tokens": ["Bey", "dei\u00b7ner", "War\u00b7heit", "An\u00b7cker", "fa\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Abendopfer ist ein Lied,", "tokens": ["Mein", "A\u00b7ben\u00b7dop\u00b7fer", "ist", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir zu dancken sich bem\u00fcht,", "tokens": ["Das", "dir", "zu", "dan\u00b7cken", "sich", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVINF", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Brust entz\u00fcndet Andachtskerzen;", "tokens": ["Die", "Brust", "ent\u00b7z\u00fcn\u00b7det", "An\u00b7dachts\u00b7ker\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4llt dir dieser Brandaltar,", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "die\u00b7ser", "Bran\u00b7dal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mache die Verhei\u00dfung wahr:", "tokens": ["So", "ma\u00b7che", "die", "Ver\u00b7hei\u00b7\u00dfung", "wahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott heilet die zerschlagne Herzen.", "tokens": ["Gott", "hei\u00b7let", "die", "zer\u00b7schlag\u00b7ne", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du bester Anwalt, Jesu Christ,", "tokens": ["Du", "bes\u00b7ter", "An\u00b7walt", ",", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in den Schwachen m\u00e4chtig ist,", "tokens": ["Der", "in", "den", "Schwa\u00b7chen", "m\u00e4ch\u00b7tig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm und vollf\u00fchre meine Sache!", "tokens": ["Komm", "und", "voll\u00b7f\u00fch\u00b7re", "mei\u00b7ne", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Beweise, da\u00df dein theures Blut,", "tokens": ["Be\u00b7wei\u00b7se", ",", "da\u00df", "dein", "theu\u00b7res", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ich verbrochen, wieder gut", "tokens": ["Was", "ich", "ver\u00b7bro\u00b7chen", ",", "wie\u00b7der", "gut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und auch die S\u00fcnder seelig mache.", "tokens": ["Und", "auch", "die", "S\u00fcn\u00b7der", "see\u00b7lig", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du Geist der Warheit, breite dich", "tokens": ["Du", "Geist", "der", "War\u00b7heit", ",", "brei\u00b7te", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deinen Gaben \u00fcber mich;", "tokens": ["Mit", "dei\u00b7nen", "Ga\u00b7ben", "\u00fc\u00b7ber", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wort sey meines Fu\u00dfes Leuchte.", "tokens": ["Dein", "Wort", "sey", "mei\u00b7nes", "Fu\u00b7\u00dfes", "Leuch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verg\u00f6nne mir dein Gnadenlicht", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "dein", "Gna\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf meinen Wegen, da\u00df ich nicht", "tokens": ["Auf", "mei\u00b7nen", "We\u00b7gen", ",", "da\u00df", "ich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir selber zur Verdammn\u00fc\u00df leuchte.", "tokens": ["Mir", "sel\u00b7ber", "zur", "Ver\u00b7damm\u00b7n\u00fc\u00df", "leuch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Herr, deine Hand sey mein Panier,", "tokens": ["Herr", ",", "dei\u00b7ne", "Hand", "sey", "mein", "Pa\u00b7nier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dein Antliz aber zeige mir", "tokens": ["Dein", "Ant\u00b7liz", "a\u00b7ber", "zei\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch in dem Traume mein Vergn\u00fcgen.", "tokens": ["Auch", "in", "dem", "Trau\u00b7me", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Einsamkeit betr\u00fcbt den Geist,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "be\u00b7tr\u00fcbt", "den", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch, wo du meine Seite schleust,", "tokens": ["Doch", ",", "wo", "du", "mei\u00b7ne", "Sei\u00b7te", "schleust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So darf ich nicht alleine liegen.", "tokens": ["So", "darf", "ich", "nicht", "al\u00b7lei\u00b7ne", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das m\u00fcde Haupt sinckt auf den Pfiehl,", "tokens": ["Das", "m\u00fc\u00b7de", "Haupt", "sinckt", "auf", "den", "Pfiehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wo ich ruhig schlafen will,", "tokens": ["Doch", "wo", "ich", "ru\u00b7hig", "schla\u00b7fen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df ich deinen Engel bitten;", "tokens": ["So", "mu\u00df", "ich", "dei\u00b7nen", "En\u00b7gel", "bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der kan durch seine starcke Wacht", "tokens": ["Der", "kan", "durch", "sei\u00b7ne", "star\u00b7cke", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich vor dem Ungeth\u00fcm der Nacht", "tokens": ["Mich", "vor", "dem", "Un\u00b7ge\u00b7th\u00fcm", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um meine Lagerstatt beh\u00fcten.", "tokens": ["Um", "mei\u00b7ne", "La\u00b7ger\u00b7statt", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Soll mir der Pfiehl ein Leichenstein,", "tokens": ["Soll", "mir", "der", "Pfiehl", "ein", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schlaf ein Schlaf zum Tode seyn,", "tokens": ["Der", "Schlaf", "ein", "Schlaf", "zum", "To\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, soll das Bette mich begraben,", "tokens": ["Ja", ",", "soll", "das", "Bet\u00b7te", "mich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So las den Leichnahm in der Gruft,", "tokens": ["So", "las", "den", "Leich\u00b7nahm", "in", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis ihn die lezte Stimme ruft,", "tokens": ["Bis", "ihn", "die", "lez\u00b7te", "Stim\u00b7me", "ruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Geist im Himmel Friede haben.", "tokens": ["Den", "Geist", "im", "Him\u00b7mel", "Frie\u00b7de", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Will aber deine G\u00fctigkeit,", "tokens": ["Will", "a\u00b7ber", "dei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die alle Morgen sic