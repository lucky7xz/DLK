{"dta.poem.14119": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fcckliche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie in der Waldnacht zwischen den Schl\u00fcften", "tokens": ["Wie", "in", "der", "Wald\u00b7nacht", "zwi\u00b7schen", "den", "Schl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Pl\u00f6tzlich die Th\u00e4ler sonnig sich kl\u00fcften,", "tokens": ["Pl\u00f6tz\u00b7lich", "die", "Th\u00e4\u00b7ler", "son\u00b7nig", "sich", "kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Funkeln die Str\u00f6me, rauscht himmelw\u00e4rts", "tokens": ["Fun\u00b7keln", "die", "Str\u00f6\u00b7me", ",", "rauscht", "him\u00b7mel\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bl\u00fchende Wildni\u00df \u2014 so ist mein Herz!", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Wild\u00b7ni\u00df", "so", "ist", "mein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}