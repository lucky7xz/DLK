{"textgrid.poem.57537": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Nebelkr\u00e4he", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein graues Regenlaken h\u00e4ngt", "tokens": ["Ein", "grau\u00b7es", "Re\u00b7gen\u00b7la\u00b7ken", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unsauber auf die Erde,", "tokens": ["Un\u00b7sau\u00b7ber", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich stampfe durch das Heidekraut,", "tokens": ["Ich", "stamp\u00b7fe", "durch", "das", "Hei\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unwirsch ist meine Geb\u00e4rde.", "tokens": ["Un\u00b7wirsch", "ist", "mei\u00b7ne", "Ge\u00b7b\u00e4r\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Eint\u00f6nig pfeift der nasse West,", "tokens": ["Ein\u00b7t\u00f6\u00b7nig", "pfeift", "der", "nas\u00b7se", "West", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wallhecken versperren die Weite,", "tokens": ["Wall\u00b7he\u00b7cken", "ver\u00b7sper\u00b7ren", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Es spritzt der z\u00e4he Klei um mich,", "tokens": ["Es", "spritzt", "der", "z\u00e4\u00b7he", "Klei", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin ich geh' und schreite.", "tokens": ["Wo\u00b7hin", "ich", "geh'", "und", "schrei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein rauher, wilder Kr\u00e4henschrei", "tokens": ["Ein", "rau\u00b7her", ",", "wil\u00b7der", "Kr\u00e4\u00b7hen\u00b7schrei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klingt pl\u00f6tzlich durch das Wehen,", "tokens": ["Klingt", "pl\u00f6tz\u00b7lich", "durch", "das", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So frech und frank, so kr\u00e4chzen nicht", "tokens": ["So", "frech", "und", "frank", ",", "so", "kr\u00e4ch\u00b7zen", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schwarzen, westf\u00e4lischen Kr\u00e4hen.", "tokens": ["Die", "schwar\u00b7zen", ",", "west\u00b7f\u00e4\u00b7li\u00b7schen", "Kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "a