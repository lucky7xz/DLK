{"dta.poem.10192": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Einer sch\u00f6nen Freundin in\u2019s Stammbuch. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Den ganzen Tag nur auf der Ottomane,", "tokens": ["Den", "gan\u00b7zen", "Tag", "nur", "auf", "der", "Ot\u00b7to\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ylang-Ylang und lange Fingern\u00e4gel.", "tokens": ["Y\u00b7lang\u00b7Ylang", "und", "lan\u00b7ge", "Fin\u00b7ger\u00b7n\u00e4\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Anzugfrage, Wochenblattromane,", "tokens": ["Die", "An\u00b7zug\u00b7fra\u00b7ge", ",", "Wo\u00b7chen\u00b7blatt\u00b7ro\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schlaf, Nichtsthun, Flachgespr\u00e4ch ist Tagesregel.", "tokens": ["Schlaf", ",", "Nichtst\u00b7hun", ",", "Flach\u00b7ge\u00b7spr\u00e4ch", "ist", "Ta\u00b7ges\u00b7re\u00b7gel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich glaube gar, f\u00fcr eine Seidenfahne", "tokens": ["Ich", "glau\u00b7be", "gar", ",", "f\u00fcr", "ei\u00b7ne", "Sei\u00b7den\u00b7fah\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verkaufst du deinen Mann und Kind und Kegel.", "tokens": ["Ver\u00b7kaufst", "du", "dei\u00b7nen", "Mann", "und", "Kind", "und", "Ke\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So schaukeltst du, verfault, im Lebenskahne,", "tokens": ["So", "schau\u00b7keltst", "du", ",", "ver\u00b7fault", ",", "im", "Le\u00b7bens\u00b7kah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Herzlosigkeit und Hochmut sind die Segel.", "tokens": ["Herz\u00b7lo\u00b7sig\u00b7keit", "und", "Hoch\u00b7mut", "sind", "die", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}