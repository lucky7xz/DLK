{"dta.poem.4285": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Auer-Ochs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein f\u00fcrchterlich Gesch\u00f6pf! Welche schreckende", "tokens": ["Welch", "ein", "f\u00fcrch\u00b7ter\u00b7lich", "Ge\u00b7sch\u00f6pf", "!", "Wel\u00b7che", "schre\u00b7cken\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "ADJD", "NN", "$.", "PWAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Figur!", "tokens": ["Fi\u00b7gur", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Welche Muskeln, welche Sehnen! Welche fremde Creatur!", "tokens": ["Wel\u00b7che", "Mus\u00b7keln", ",", "wel\u00b7che", "Seh\u00b7nen", "!", "Wel\u00b7che", "frem\u00b7de", "Crea\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Wie ein Erden-Behemoth scheinen seine Knochen Erz,", "tokens": ["Wie", "ein", "Er\u00b7den\u00b7Be\u00b7he\u00b7moth", "schei\u00b7nen", "sei\u00b7ne", "Kno\u00b7chen", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Seine H\u00f6rner wirklichs Eisen. Hat doch mancher kaum", "tokens": ["Sei\u00b7ne", "H\u00f6r\u00b7ner", "wirk\u00b7lichs", "Ei\u00b7sen", ".", "Hat", "doch", "man\u00b7cher", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "VAFIN", "ADV", "PIS", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "das Herz", "tokens": ["das", "Herz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Jhn auf ", "tokens": ["Jhn", "auf"], "token_info": ["word", "word"], "pos": ["PPER", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Durch die f\u00fcrchterliche Gleichheit hindert die verj\u00fcngte", "tokens": ["Durch", "die", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Gleich\u00b7heit", "hin\u00b7dert", "die", "ver\u00b7j\u00fcng\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Maasse", "tokens": ["Maas\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Kaum, da\u00df wer es pl\u00f6tzlich sieht, fast nicht w\u00fcnscht ent-", "tokens": ["Kaum", ",", "da\u00df", "wer", "es", "pl\u00f6tz\u00b7lich", "sieht", ",", "fast", "nicht", "w\u00fcnscht", "ent"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PWS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "fernt zu stehn,", "tokens": ["fernt", "zu", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Und da\u00df er, bey solchem Umstand, sicher f\u00fcr Gefahr sey, fasse.", "tokens": ["Und", "da\u00df", "er", ",", "bey", "sol\u00b7chem", "Um\u00b7stand", ",", "si\u00b7cher", "f\u00fcr", "Ge\u00b7fahr", "sey", ",", "fas\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$,", "ADJD", "APPR", "NN", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie so rauch, verworren, gr\u00e4\u00dflich ist sein starker Kopf", "tokens": ["Wie", "so", "rauch", ",", "ver\u00b7wor\u00b7ren", ",", "gr\u00e4\u00df\u00b7lich", "ist", "sein", "star\u00b7ker", "Kopf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "$,", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "behaart,", "tokens": ["be\u00b7haart", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Es vermehrt sein wildes Ansehn sonderlich sein dicker", "tokens": ["Es", "ver\u00b7mehrt", "sein", "wil\u00b7des", "An\u00b7sehn", "son\u00b7der\u00b7lich", "sein", "di\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Baart.", "tokens": ["Baart", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ein gesetzter Muht belebt ihn, ihm ist keine Furcht be-", "tokens": ["Ein", "ge\u00b7setz\u00b7ter", "Muht", "be\u00b7lebt", "ihn", ",", "ihm", "ist", "kei\u00b7ne", "Furcht", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PIAT", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "kannt,", "tokens": ["kannt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Er kennt seiner H\u00f6rner Kraft, wo er stehet, h\u00e4lt er Stand.", "tokens": ["Er", "kennt", "sei\u00b7ner", "H\u00f6r\u00b7ner", "Kraft", ",", "wo", "er", "ste\u00b7het", ",", "h\u00e4lt", "er", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ist nun das Original recht Bewundernswehrt, so scheinet", "tokens": ["Ist", "nun", "das", "O\u00b7rig\u00b7i\u00b7nal", "recht", "Be\u00b7wun\u00b7derns\u00b7wehrt", ",", "so", "schei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VVPP", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die fast lebende Copie minder nicht Bewundernswehrt,", "tokens": ["Die", "fast", "le\u00b7ben\u00b7de", "Co\u00b7pie", "min\u00b7der", "nicht", "Be\u00b7wun\u00b7derns\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Weil in ihr der Menschen Geist sich mit der Natur vereinet.", "tokens": ["Weil", "in", "ihr", "der", "Men\u00b7schen", "Geist", "sich", "mit", "der", "Na\u00b7tur", "ver\u00b7ei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ART", "NN", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Unser Sch\u00f6pfer wird in beyden, durch vern\u00fcnftigs Sehn,", "tokens": ["Un\u00b7ser", "Sch\u00f6p\u00b7fer", "wird", "in", "bey\u00b7den", ",", "durch", "ver\u00b7n\u00fcnf\u00b7tigs", "Sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "geehrt.", "tokens": ["ge\u00b7ehrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}