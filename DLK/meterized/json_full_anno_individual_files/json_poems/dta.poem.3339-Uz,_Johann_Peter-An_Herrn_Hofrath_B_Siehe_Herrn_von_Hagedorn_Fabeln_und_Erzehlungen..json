{"dta.poem.3339": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Schnell h\u00f6rt ich einen Wind um alle Kl\u00fcfte heulen:", "tokens": ["Schnell", "h\u00f6rt", "ich", "ei\u00b7nen", "Wind", "um", "al\u00b7le", "Kl\u00fcf\u00b7te", "heu\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00f6hlen donnerten, bewohnt von scheuen Eulen.", "tokens": ["Die", "H\u00f6h\u00b7len", "don\u00b7ner\u00b7ten", ",", "be\u00b7wohnt", "von", "scheu\u00b7en", "Eu\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Sturm, der mich dahin gebracht,", "tokens": ["Der", "Sturm", ",", "der", "mich", "da\u00b7hin", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stie\u00df aus dem Schoos der Nacht,", "tokens": ["Stie\u00df", "aus", "dem", "Schoos", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Nach zwoen jahrelangen Stunden,", "tokens": ["Nach", "zwoen", "jah\u00b7re\u00b7lan\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich wieder an die Luft, wo Titans Auge lacht:", "tokens": ["Mich", "wie\u00b7der", "an", "die", "Luft", ",", "wo", "Ti\u00b7tans", "Au\u00b7ge", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$,", "PWAV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gnom, Kellermeister, Aff und alles war verschwunden.", "tokens": ["Gnom", ",", "Kel\u00b7ler\u00b7meis\u00b7ter", ",", "Aff", "und", "al\u00b7les", "war", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}