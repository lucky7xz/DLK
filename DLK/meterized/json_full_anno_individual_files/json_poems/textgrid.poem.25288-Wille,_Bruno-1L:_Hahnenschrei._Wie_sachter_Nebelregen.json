{"textgrid.poem.25288": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hahnenschrei. Wie sachter Nebelregen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hahnenschrei. Wie sachter Nebelregen", "tokens": ["Hah\u00b7nen\u00b7schrei", ".", "Wie", "sach\u00b7ter", "Ne\u00b7bel\u00b7re\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Rieselt Morgend\u00e4mmern bleich vom Himmel;", "tokens": ["Rie\u00b7selt", "Mor\u00b7gen\u00b7d\u00e4m\u00b7mern", "bleich", "vom", "Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Baum und Giebel grau und geisterhaft ...", "tokens": ["Baum", "und", "Gie\u00b7bel", "grau", "und", "geis\u00b7ter\u00b7haft", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hahnenschrei im Dorfe hin und wieder/", "tokens": ["Hah\u00b7nen\u00b7schrei", "im", "Dor\u00b7fe", "hin", "und", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "KON", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Fl\u00fcchtig Lallen einer Tagesahnung,", "tokens": ["Fl\u00fcch\u00b7tig", "Lal\u00b7len", "ei\u00b7ner", "Ta\u00b7ge\u00b7sah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die den Schlaf der Allnatur durchschauert.", "tokens": ["Die", "den", "Schlaf", "der", "All\u00b7na\u00b7tur", "durch\u00b7schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Horch, Einsiedler! Deine schwere Wacht", "tokens": ["Horch", ",", "Ein\u00b7sied\u00b7ler", "!", "Dei\u00b7ne", "schwe\u00b7re", "Wacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Geht zu Ende. Von der \u00fcbern\u00e4chtig", "tokens": ["Geht", "zu", "En\u00b7de", ".", "Von", "der", "\u00fc\u00b7bern\u00b7\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "$.", "APPR", "ART", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00fcden Stirne streife starre Sorgen,", "tokens": ["M\u00fc\u00b7den", "Stir\u00b7ne", "strei\u00b7fe", "star\u00b7re", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Streife deiner Sehnsucht rastlos Gr\u00fcbeln.", "tokens": ["Strei\u00b7fe", "dei\u00b7ner", "Sehn\u00b7sucht", "rast\u00b7los", "Gr\u00fc\u00b7beln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nur getrost! Die gro\u00dfe Fr\u00fchlingskraft,", "tokens": ["Nur", "ge\u00b7trost", "!", "Die", "gro\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Die geheimnisvoll der Erde Busen,", "tokens": ["Die", "ge\u00b7heim\u00b7nis\u00b7voll", "der", "Er\u00b7de", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wurzel, Knospentrieb und Menschenherzen", "tokens": ["Wur\u00b7zel", ",", "Knos\u00b7pen\u00b7trieb", "und", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sch\u00f6pferisch durchbebt/ sie pulset weiter,", "tokens": ["Sch\u00f6p\u00b7fe\u00b7risch", "durch\u00b7bebt", "/", "sie", "pul\u00b7set", "wei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Braucht dein Sorgen nicht. Sie pulset weiter,", "tokens": ["Braucht", "dein", "Sor\u00b7gen", "nicht", ".", "Sie", "pul\u00b7set", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Wenn dein W\u00e4chteraug auch bricht, und dunkle", "tokens": ["Wenn", "dein", "W\u00e4ch\u00b7ter\u00b7aug", "auch", "bricht", ",", "und", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Todesflut den morschen Leib umsp\u00fclt.", "tokens": ["To\u00b7des\u00b7flut", "den", "mor\u00b7schen", "Leib", "um\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Ruhst du ewig doch im Mutterscho\u00dfe;", "tokens": ["Ruhst", "du", "e\u00b7wig", "doch", "im", "Mut\u00b7ter\u00b7scho\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Da wird Todesflut zum Jugendborn.", "tokens": ["Da", "wird", "To\u00b7des\u00b7flut", "zum", "Ju\u00b7gend\u00b7born", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hahnenschrei. Nun auf, Einsiedler! Lisch", "tokens": ["Hah\u00b7nen\u00b7schrei", ".", "Nun", "auf", ",", "Ein\u00b7sied\u00b7ler", "!", "Lisch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "ADV", "PTKVZ", "$,", "NN", "$.", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Endlich kummervoller Menschenliebe", "tokens": ["End\u00b7lich", "kum\u00b7mer\u00b7vol\u00b7ler", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fackel/ die so d\u00fcster dir zu H\u00e4upten", "tokens": ["Fa\u00b7ckel", "/", "die", "so", "d\u00fcs\u00b7ter", "dir", "zu", "H\u00e4up\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "ADV", "ADJD", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwelte diese lange, bange Nacht.", "tokens": ["Schwel\u00b7te", "die\u00b7se", "lan\u00b7ge", ",", "ban\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "La\u00df an sanfter Ruhe treuen Busen", "tokens": ["La\u00df", "an", "sanf\u00b7ter", "Ru\u00b7he", "treu\u00b7en", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deine aufgel\u00f6sten Sinne sinken!", "tokens": ["Dei\u00b7ne", "auf\u00b7ge\u00b7l\u00f6s\u00b7ten", "Sin\u00b7ne", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "K\u00fchl und duftig um dein Lager wallen", "tokens": ["K\u00fchl", "und", "duf\u00b7tig", "um", "dein", "La\u00b7ger", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Fliederzweige ... Matter Hahnenschrei/", "tokens": ["Flie\u00b7der\u00b7zwei\u00b7ge", "...", "Mat\u00b7ter", "Hah\u00b7nen\u00b7schrei", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Letzter Scheidegru\u00df von jenem dunkeln", "tokens": ["Letz\u00b7ter", "Schei\u00b7de\u00b7gru\u00df", "von", "je\u00b7nem", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ufer, das die Seele, wie ertrinkend,", "tokens": ["U\u00b7fer", ",", "das", "die", "See\u00b7le", ",", "wie", "er\u00b7trin\u00b7kend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Doch so gern, verlor ... Ade, ade!", "tokens": ["Doch", "so", "gern", ",", "ver\u00b7lor", "...", "A\u00b7de", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "VVFIN", "$(", "NN", "$,", "ADV", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Einmal taucht sie noch empor; und zwischen", "tokens": ["Ein\u00b7mal", "taucht", "sie", "noch", "em\u00b7por", ";", "und", "zwi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlaf und Wachen tr\u00e4umend, h\u00f6rt sie leises", "tokens": ["Schlaf", "und", "Wa\u00b7chen", "tr\u00e4u\u00b7mend", ",", "h\u00f6rt", "sie", "lei\u00b7ses"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Lerchenzwitschern ... V\u00f6glein, lieber Herold,", "tokens": ["Ler\u00b7chen\u00b7zwit\u00b7schern", "...", "V\u00f6\u00b7glein", ",", "lie\u00b7ber", "He\u00b7rold", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$,", "ADV", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sp\u00fcrst du droben frischen Lebensodem,", "tokens": ["Sp\u00fcrst", "du", "dro\u00b7ben", "fri\u00b7schen", "Le\u00b7ben\u00b7so\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Neugebornes Licht, das aus der Nacht", "tokens": ["Neu\u00b7ge\u00b7bor\u00b7nes", "Licht", ",", "das", "aus", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Rosen\u00fcppig bl\u00fcht? Ja, Todesflut", "tokens": ["Ro\u00b7se\u00b7n\u00fcp\u00b7pig", "bl\u00fcht", "?", "Ja", ",", "To\u00b7des\u00b7flut"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$.", "PTKANT", "$,", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ward zum Jugendborn! Und gl\u00e4ubig l\u00e4chelnd", "tokens": ["Ward", "zum", "Ju\u00b7gend\u00b7born", "!", "Und", "gl\u00e4u\u00b7big", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "$.", "KON", "ADJD", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sinkt die Seele zum ersehnten Sterben", "tokens": ["Sinkt", "die", "See\u00b7le", "zum", "er\u00b7sehn\u00b7ten", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In die dunkle Flut ... Wie s\u00fc\u00df, wie s\u00fc\u00df!", "tokens": ["In", "die", "dunk\u00b7le", "Flut", "...", "Wie", "s\u00fc\u00df", ",", "wie", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hahnenschrei. Wie sachter Nebelregen", "tokens": ["Hah\u00b7nen\u00