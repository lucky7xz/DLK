{"dta.poem.17782": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "25.  Der Weise sucht keinen a\u00fcsern Ehren  \n Stand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise strebet nicht nach a\u00fcsrem Ehren stand:", "tokens": ["Der", "Wei\u00b7se", "stre\u00b7bet", "nicht", "nach", "a\u00b7\u00fcs\u00b7rem", "Eh\u00b7ren", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es ist ihm Ehr genug/ da\u00df er GOtt nah verwandt.", "tokens": ["Es", "ist", "ihm", "Ehr", "ge\u00b7nug", "/", "da\u00df", "er", "Gott", "nah", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADV", "$(", "KOUS", "PPER", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}