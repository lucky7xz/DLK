{"textgrid.poem.60543": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen Herzen wogt die Liebe,", "tokens": ["In", "die\u00b7sen", "Her\u00b7zen", "wogt", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In jenen dr\u00fcben kocht die Galle,", "tokens": ["In", "je\u00b7nen", "dr\u00fc\u00b7ben", "kocht", "die", "Gal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Feuer brachte sie doch alle", "tokens": ["Dein", "Feu\u00b7er", "brach\u00b7te", "sie", "doch", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Wallung; gut, wenn es so bliebe!", "tokens": ["In", "Wal\u00b7lung", ";", "gut", ",", "wenn", "es", "so", "blie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch gehst entgegen du dem Leide,", "tokens": ["Doch", "gehst", "ent\u00b7ge\u00b7gen", "du", "dem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo alles still wird um dich sein,", "tokens": ["Wo", "al\u00b7les", "still", "wird", "um", "dich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VAFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo du dein Leid f\u00fcr dich allein", "tokens": ["Wo", "du", "dein", "Leid", "f\u00fcr", "dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aufspielen wirst auf einer Heide;", "tokens": ["Auf\u00b7spie\u00b7len", "wirst", "auf", "ei\u00b7ner", "Hei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo du nach einem Wetterschlage", "tokens": ["Wo", "du", "nach", "ei\u00b7nem", "Wet\u00b7ter\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Hinausblickst von der tr\u00fcben Fl\u00e4che,", "tokens": ["Hin\u00b7aus\u00b7blickst", "von", "der", "tr\u00fc\u00b7ben", "Fl\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df er auf dich herunterbreche,", "tokens": ["Da\u00df", "er", "auf", "dich", "her\u00b7un\u00b7ter\u00b7bre\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Damit doch jemand nach dir frage.", "tokens": ["Da\u00b7mit", "doch", "je\u00b7mand", "nach", "dir", "fra\u00b