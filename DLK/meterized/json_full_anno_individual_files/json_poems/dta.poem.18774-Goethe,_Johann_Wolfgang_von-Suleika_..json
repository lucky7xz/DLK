{"dta.poem.18774": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich auf dem Euphrat schiffte,", "tokens": ["Als", "ich", "auf", "dem", "Eu\u00b7ph\u00b7rat", "schiff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Streifte sich der goldne Ring", "tokens": ["Streif\u00b7te", "sich", "der", "gold\u00b7ne", "Ring"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fingerab, in Wasserkl\u00fcfte,", "tokens": ["Fin\u00b7ge\u00b7rab", ",", "in", "Was\u00b7ser\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ich j\u00fcngst von dir empfing.", "tokens": ["Den", "ich", "j\u00fcngst", "von", "dir", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also tr\u00e4umt\u2019 ich, Morgenr\u00f6the", "tokens": ["Al\u00b7so", "tr\u00e4umt'", "ich", ",", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzt in\u2019s Auge durch den Baum,", "tokens": ["Blitzt", "in's", "Au\u00b7ge", "durch", "den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sag Poete, sag Prophete!", "tokens": ["Sag", "Poe\u00b7te", ",", "sag", "Pro\u00b7phe\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was bedeutet dieser Traum?", "tokens": ["Was", "be\u00b7deu\u00b7tet", "die\u00b7ser", "Traum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}