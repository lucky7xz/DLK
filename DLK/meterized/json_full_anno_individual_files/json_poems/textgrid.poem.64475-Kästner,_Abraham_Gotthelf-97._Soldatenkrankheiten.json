{"textgrid.poem.64475": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "97. Soldatenkrankheiten", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch wenn ihn Stahl und Bley nicht trifft,", "tokens": ["Auch", "wenn", "ihn", "Stahl", "und", "Bley", "nicht", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Qu\u00e4lt sich der Krieger doch durch mancher Krankheit Gift;", "tokens": ["Qu\u00e4lt", "sich", "der", "Krie\u00b7ger", "doch", "durch", "man\u00b7cher", "Krank\u00b7heit", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den plagt das Fieber, den die Ruhr,", "tokens": ["Den", "plagt", "das", "Fie\u00b7ber", ",", "den", "die", "Ruhr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Gallier braucht fluchend den Merkur;", "tokens": ["Der", "Gal\u00b7lier", "braucht", "flu\u00b7chend", "den", "Mer\u00b7kur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein \u00e4ngstlich warnender und epidemscher Schmerz", "tokens": ["Ein", "\u00e4ngst\u00b7lich", "war\u00b7nen\u00b7der", "und", "e\u00b7pi\u00b7dem\u00b7scher