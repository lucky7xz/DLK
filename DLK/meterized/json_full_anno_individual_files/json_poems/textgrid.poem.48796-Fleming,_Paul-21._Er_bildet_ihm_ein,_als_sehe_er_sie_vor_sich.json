{"textgrid.poem.48796": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "21. Er bildet ihm ein, als sehe er sie vor sich", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willkommen, s\u00fc\u00dfer Gast, du Balsam meiner Wunden!", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfer", "Gast", ",", "du", "Bal\u00b7sam", "mei\u00b7ner", "Wun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo k\u00f6mmst du itzund her? Mein Schatz, umfange mich!", "tokens": ["Wo", "k\u00f6mmst", "du", "it\u00b7zund", "her", "?", "Mein", "Schatz", ",", "um\u00b7fan\u00b7ge", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was h\u00e4ltst du mich doch auf, warum versteckst du dich?", "tokens": ["Was", "h\u00e4ltst", "du", "mich", "doch", "auf", ",", "wa\u00b7rum", "ver\u00b7steckst", "du", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo bist du? Komm doch her, ei! komm doch her von Stunden!", "tokens": ["Wo", "bist", "du", "?", "Komm", "doch", "her", ",", "ei", "!", "komm", "doch", "her", "von", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "pu