{"textgrid.poem.36287": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Buhlt l\u00e4nger nicht mit eitlem Wortgeklinge!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buhlt l\u00e4nger nicht mit eitlem Wortgeklinge!", "tokens": ["Buhlt", "l\u00e4n\u00b7ger", "nicht", "mit", "eit\u00b7lem", "Wort\u00b7ge\u00b7klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unedle la\u00dft in Hochmut sich aufbl\u00e4hen,", "tokens": ["U\u00b7ned\u00b7le", "la\u00dft", "in", "Hoch\u00b7mut", "sich", "auf\u00b7bl\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "APPR", "NN", "PRF", "VVIZU", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Sich um den eignen Geist bewundernd drehen,", "tokens": ["Sich", "um", "den", "eig\u00b7nen", "Geist", "be\u00b7wun\u00b7dernd", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beseligt, da\u00df so Einzigs ihm gelinge.", "tokens": ["Be\u00b7se\u00b7ligt", ",", "da\u00df", "so", "Ein\u00b7zigs", "ihm", "ge\u00b7lin\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "La\u00dft nicht der Eitelkeit verborgne Schlinge", "tokens": ["La\u00dft", "nicht", "der", "Ei\u00b7tel\u00b7keit", "ver\u00b7borg\u00b7ne", "Schlin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aush\u00f6hlend mich eu'r Herz umwinden sehen!", "tokens": ["Aus\u00b7h\u00f6h\u00b7lend", "mich", "eu'r", "Herz", "um\u00b7win\u00b7den", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Treu dienend nur erklimmt der Dichtkunst H\u00f6hen,", "tokens": ["Treu", "die\u00b7nend", "nur", "er\u00b7klimmt", "der", "Dicht\u00b7kunst", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer f\u00fchlt, wie heilig das sei, was er singe.", "tokens": ["Wer", "f\u00fchlt", ",", "wie", "hei\u00b7lig", "das", "sei", ",", "was", "er", "sin\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADJD", "PDS", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Den Heldenruhm, den sie zu sp\u00e4t jetzt achten,", "tokens": ["Den", "Hel\u00b7den\u00b7ruhm", ",", "den", "sie", "zu", "sp\u00e4t", "jetzt", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des deutschen Namens in den lichten Zeiten,", "tokens": ["Des", "deut\u00b7schen", "Na\u00b7mens", "in", "den", "lich\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Als Rittermut der Andacht sich verbunden;", "tokens": ["Als", "Rit\u00b7ter\u00b7mut", "der", "An\u00b7dacht", "sich", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die alte Sch\u00f6nheit, eh sie ganz verschwunden,", "tokens": ["Die", "al\u00b7te", "Sch\u00f6n\u00b7heit", ",", "eh", "sie", "ganz", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu retten, fern von allen Eitelkeiten,", "tokens": ["Zu", "ret\u00b7ten", ",", "fern", "von", "al\u00b7len", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das sei des Dichters hohes Ziel und Trachten!", "tokens": ["Das", "sei", "des", "Dich\u00b7ters", "ho\u00b7hes", "Ziel", "und", "Trach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Buhlt l\u00e4nger nicht mit eitlem Wortgeklinge!", "tokens": ["Buhlt", "l\u00e4n\u00b7ger", "nicht", "mit", "eit\u00b7lem", "Wort\u00b7ge\u00b7klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "wo