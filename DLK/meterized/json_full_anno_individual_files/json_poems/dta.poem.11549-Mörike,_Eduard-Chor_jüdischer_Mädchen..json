{"dta.poem.11549": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Chor j\u00fcdischer M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Er lud uns zum Mahle;", "tokens": ["Er", "lud", "uns", "zum", "Mah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So sind wir nun da.", "tokens": ["So", "sind", "wir", "nun", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Eia la la! Eia la la!", "tokens": ["Ei\u00b7a", "la", "la", "!", "Ei\u00b7a", "la", "la", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "$.", "FM", "FM", "FM", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ist doch auch des K\u00f6nigs sein T\u00f6chterlein da!", "tokens": ["Ist", "doch", "auch", "des", "K\u00f6\u00b7nigs", "sein", "T\u00f6ch\u00b7ter\u00b7lein", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Duftende Quellen", "tokens": ["Duf\u00b7ten\u00b7de", "Quel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Springen im Saal,", "tokens": ["Sprin\u00b7gen", "im", "Saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und wie Gazellen", "tokens": ["Und", "wie", "Ga\u00b7zel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wir h\u00fcpfen um's Mahl.", "tokens": ["Wir", "h\u00fcp\u00b7fen", "um's", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Keine soll stocken im Tanz!", "tokens": ["Kei\u00b7ne", "soll", "sto\u00b7cken", "im", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Sch\u00fcttelt nur Locken und Kranz!", "tokens": ["Sch\u00fct\u00b7telt", "nur", "Lo\u00b7cken", "und", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Lustig! im Taumel muthwilliger T\u00e4nze", "tokens": ["Lus\u00b7tig", "!", "im", "Tau\u00b7mel", "mut\u00b7hwil\u00b7li\u00b7ger", "T\u00e4n\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Fliegen die Kr\u00e4nze,", "tokens": ["Flie\u00b7gen", "die", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Fliegt es mit Rosen und B\u00e4ndern im Saal!", "tokens": ["Fliegt", "es", "mit", "Ro\u00b7sen", "und", "B\u00e4n\u00b7dern", "im", "Saal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "Eia la la! Eia la la! u. s. w.", "tokens": ["Ei\u00b7a", "la", "la", "!", "Ei\u00b7a", "la", "la", "!", "u.", "s.", "w."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "abbreviation", "abbreviation", "abbreviation"], "pos": ["FM", "FM", "FM", "$.", "FM", "FM", "FM", "$.", "KON", "VVIMP", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}