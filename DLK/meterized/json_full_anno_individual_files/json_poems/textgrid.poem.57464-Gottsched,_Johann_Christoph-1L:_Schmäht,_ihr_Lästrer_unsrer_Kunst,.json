info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sein Herz entz\u00fcndet haben.", "tokens": ["Die", "sein", "Herz", "ent\u00b7z\u00fcn\u00b7det", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weit entfernt, und doch getreu,", "tokens": ["Weit", "ent\u00b7fernt", ",", "und", "doch", "ge\u00b7treu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum ein einzigmal gesprochen,", "tokens": ["Kaum", "ein", "ein\u00b7zig\u00b7mal", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleichwohl sonder H\u00e4ucheley", "tokens": ["Gleich\u00b7wohl", "son\u00b7der", "H\u00e4u\u00b7che\u00b7ley"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Gel\u00fcbde nicht gebrochen;", "tokens": ["Sein", "Ge\u00b7l\u00fcb\u00b7de", "nicht", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses sind f\u00fcr eure Brut", "tokens": ["Die\u00b7ses", "sind", "f\u00fcr", "eu\u00b7re", "Brut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wahrlich viel zu edle Proben;", "tokens": ["Wahr\u00b7lich", "viel", "zu", "ed\u00b7le", "Pro\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch dafern ihrs gleichfalls thut,", "tokens": ["Doch", "da\u00b7fern", "ihrs", "gleich\u00b7falls", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Will ich euch gedoppelt loben.", "tokens": ["Will", "ich", "euch", "ge\u00b7dop\u00b7pelt", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber nein! ihr k\u00f6nnt es nicht,", "tokens": ["A\u00b7ber", "nein", "!", "ihr", "k\u00f6nnt", "es", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das geh\u00f6rt f\u00fcr edle Seelen,", "tokens": ["Das", "ge\u00b7h\u00f6rt", "f\u00fcr", "ed\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sich kein verf\u00fchrend Licht,", "tokens": ["Die", "sich", "kein", "ver\u00b7f\u00fch\u00b7rend", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "VVPP", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt des Leitgestirnes, w\u00e4hlen.", "tokens": ["Statt", "des", "Leit\u00b7ge\u00b7stir\u00b7nes", ",", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dichter, die der Himmel treibt,", "tokens": ["Dich\u00b7ter", ",", "die", "der", "Him\u00b7mel", "treibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieben nur des Himmels Kinder.", "tokens": ["Lie\u00b7ben", "nur", "des", "Him\u00b7mels", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur die Glut, die irdisch bleibt,", "tokens": ["Nur", "die", "Glut", ",", "die", "ir\u00b7disch", "bleibt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die verlodert auch geschwinder.", "tokens": ["Die", "ver\u00b7lo\u00b7dert", "auch", "ge\u00b7schwin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Auch in ", "tokens": ["Auch", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Kann sein Lieben nicht erkalten.", "tokens": ["Kann", "sein", "Lie\u00b7ben", "nicht", "er\u00b7kal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, er will es, wie es war,", "tokens": ["Nein", ",", "er", "will", "es", ",", "wie", "es", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zur kalten Gruft erhalten.", "tokens": ["Bis", "zur", "kal\u00b7ten", "Gruft", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O, was Wunder! da\u00df sie noch", "tokens": ["O", ",", "was", "Wun\u00b7der", "!", "da\u00df", "sie", "noch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "NN", "$.", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Da er ihrer Liebe Joch", "tokens": ["Da", "er", "ih\u00b7rer", "Lie\u00b7be", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch zerdr\u00fcmmert noch erhebet.", "tokens": ["Auch", "zer\u00b7dr\u00fcm\u00b7mert", "noch", "er\u00b7he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sch\u00f6nste ", "tokens": ["Sch\u00f6ns\u00b7te"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "So wird dich dein Dichter ehren!", "tokens": ["So", "wird", "dich", "dein", "Dich\u00b7ter", "eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn von Unbest\u00e4ndigkeit", "tokens": ["Denn", "von", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du wahrlich niemals h\u00f6ren.", "tokens": ["Sollst", "du", "wahr\u00b7lich", "nie\u00b7mals", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du doch des Himmels Kind,", "tokens": ["Bist", "du", "doch", "des", "Him\u00b7mels", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der mich selbst zu dir gef\u00fchret:", "tokens": ["Der", "mich", "selbst", "zu", "dir", "ge\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Darum bleib ich treu gesinnt,", "tokens": ["Da\u00b7rum", "bleib", "ich", "treu", "ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis mein letzter Puls sich r\u00fchret.", "tokens": ["Bis", "mein", "letz\u00b7ter", "Puls", "sich", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}