{"textgrid.poem.24973": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Vor einem Fenster", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleich am Himmel steht der Mond;", "tokens": ["Bleich", "am", "Him\u00b7mel", "steht", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Fenster zu dem Zimmer,", "tokens": ["In", "das", "Fens\u00b7ter", "zu", "dem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich ehedem gewohnt,", "tokens": ["Wo", "ich", "e\u00b7he\u00b7dem", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittert geisterhaft sein Schimmer,", "tokens": ["Zit\u00b7tert", "geis\u00b7ter\u00b7haft", "sein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zur\u00fcck glaub' ich zu schaun", "tokens": ["Und", "zu\u00b7r\u00fcck", "glaub'", "ich", "zu", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den lang versunknen Jahren,", "tokens": ["Zu", "den", "lang", "ver\u00b7sun\u00b7knen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als mir noch die Locken braun,", "tokens": ["Als", "mir", "noch", "die", "Lo\u00b7cken", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frisch die Lebensgeister waren.", "tokens": ["Frisch", "die", "Le\u00b7bens\u00b7geis\u00b7ter", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles drinnen wie bekannt!", "tokens": ["Al\u00b7les", "drin\u00b7nen", "wie", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort der Sessel vor dem Pulte", "tokens": ["Dort", "der", "Ses\u00b7sel", "vor", "dem", "Pul\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Spieluhr an der Wand,", "tokens": ["Und", "die", "Spie\u00b7luhr", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mich oft in Schlummer lullte;", "tokens": ["Die", "mich", "oft", "in", "Schlum\u00b7mer", "lull\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort bei einer Kerze Licht,", "tokens": ["Dort", "bei", "ei\u00b7ner", "Ker\u00b7ze", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "B\u00fccher vor ihm aufgeschlagen,", "tokens": ["B\u00fc\u00b7cher", "vor", "ihm", "auf\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sitzt ein J\u00fcngling; sein Gesicht", "tokens": ["Sitzt", "ein", "J\u00fcng\u00b7ling", ";", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist wie meins in fr\u00fchen Tagen.", "tokens": ["Ist", "wie", "meins", "in", "fr\u00fc\u00b7hen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sage mir, mein Schattenbild,", "tokens": ["Sa\u00b7ge", "mir", ",", "mein", "Schat\u00b7ten\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Du voll Lust, wie ich voll Trauer:", "tokens": ["Du", "voll", "Lust", ",", "wie", "ich", "voll", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$,", "PWAV", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubt dein Drang, der nie gestillt,", "tokens": ["Glaubt", "dein", "Drang", ",", "der", "nie", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch an ew'ge Lebensdauer?", "tokens": ["Noch", "an", "ew'\u00b7ge", "Le\u00b7bens\u00b7dau\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bei Folianten, Nachtgesell,", "tokens": ["Bei", "Fo\u00b7li\u00b7an\u00b7ten", ",", "Nacht\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Br\u00fctend bis zur Morgenstunde", "tokens": ["Br\u00fc\u00b7tend", "bis", "zur", "Mor\u00b7gen\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00fchst du dich, der Weisheit Quell", "tokens": ["M\u00fchst", "du", "dich", ",", "der", "Weis\u00b7heit", "Quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auszusch\u00f6pfen bis zum Grunde?", "tokens": ["Aus\u00b7zu\u00b7sch\u00f6p\u00b7fen", "bis", "zum", "Grun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwingen deinem Geiste wohl", "tokens": ["Schwin\u00b7gen", "dei\u00b7nem", "Geis\u00b7te", "wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst du weben durch dein Lernen;", "tokens": ["Willst", "du", "we\u00b7ben", "durch", "dein", "Ler\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkst zu fliegen an den Pol", "tokens": ["Denkst", "zu", "flie\u00b7gen", "an", "den", "Pol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Himmels fernsten Sternen;", "tokens": ["Zu", "des", "Him\u00b7mels", "ferns\u00b7ten", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4umst in jugendlichem Mut,", "tokens": ["Tr\u00e4umst", "in", "ju\u00b7gend\u00b7li\u00b7chem", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gro\u00dfes einst zu thun auf Erden \u2013", "tokens": ["Gro\u00b7\u00dfes", "einst", "zu", "thun", "auf", "Er\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber Kraft und Wangenglut,", "tokens": ["A\u00b7ber", "Kraft", "und", "Wan\u00b7gen\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "O wie bald sie schwinden werden!", "tokens": ["O", "wie", "bald", "sie", "schwin\u00b7den", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "PPER", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Geh und schlag die B\u00fccher zu!", "tokens": ["Geh", "und", "schlag", "die", "B\u00fc\u00b7cher", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sieh hernieder, wo ich stehe!", "tokens": ["Sieh", "her\u00b7nie\u00b7der", ",", "wo", "ich", "ste\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist ich, und ich bin du,", "tokens": ["Du", "bist", "ich", ",", "und", "ich", "bin", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur gebeugt von Gram und Wehe;", "tokens": ["Nur", "ge\u00b7beugt", "von", "Gram", "und", "We\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bitter an den Lippen klebt", "tokens": ["Bit\u00b7ter", "an", "den", "Lip\u00b7pen", "klebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir des Lebensbechers Hefe,", "tokens": ["Mir", "des", "Le\u00b7bens\u00b7be\u00b7chers", "He\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und, wie hei\u00df ich auch gestrebt,", "tokens": ["Und", ",", "wie", "hei\u00df", "ich", "auch", "ge\u00b7strebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADJD", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Labt kein Kranz die gl\u00fch'nde Schl\u00e4fe.", "tokens": ["Labt", "kein", "Kranz", "die", "gl\u00fch'n\u00b7de", "Schl\u00e4\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ich baute, sah zerst\u00f6rt", "tokens": ["Was", "ich", "bau\u00b7te", ",", "sah", "zer\u00b7st\u00f6rt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich zu Boden wieder rollen;", "tokens": ["Ich", "zu", "Bo\u00b7den", "wie\u00b7der", "rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Luft ist ungeh\u00f6rt", "tokens": ["In", "der", "Luft", "ist", "un\u00b7ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Worte Klang verschollen,", "tokens": ["Mei\u00b7ner", "Wor\u00b7te", "Klang", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bevor mein Volk, mein Land", "tokens": ["Und", "be\u00b7vor", "mein", "Volk", ",", "mein", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch erkannten, wen sie hatten,", "tokens": ["Noch", "er\u00b7kann\u00b7ten", ",", "wen", "sie", "hat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unbetrauert, ungenannt,", "tokens": ["Un\u00b7be\u00b7trau\u00b7ert", ",", "un\u00b7ge\u00b7nannt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Werd' ich eingehn zu den Schatten.", "tokens": ["Werd'", "ich", "ein\u00b7gehn", "zu", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Bleich am Himmel steht der Mond;", "tokens": ["Bleich", "am", "Him\u00b7mel", "steht", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Fenster zu dem Zimmer,", "tokens": ["In", "das", "Fens\u00b7ter", "zu", "dem", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich ehedem gewohnt,", "tokens": ["Wo", "ich", "e\u00b7he\u00b7dem", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittert geisterhaft sein Schimmer,", "tokens": ["Zit\u00b7tert", "geis\u00b7ter\u00b7haft", "sein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zur\u00fcck glaub' ich zu schaun", "tokens": ["Und", "zu\u00b7r\u00fcck", "glaub'", "ich", "zu", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu den lang versunknen Jahren,", "tokens": ["Zu", "den", "lang", "ver\u00b7sun\u00b7knen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Als mir noch die Locken braun,", "tokens": ["Als", "mir", "noch", "die", "Lo\u00b7cken", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Frisch die Lebensgeister waren.", "tokens": ["Frisch", "die", "Le\u00b7bens\u00b7geis\u00b7ter", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Alles drinnen wie bekannt!", "tokens": ["Al\u00b7les", "drin\u00b7nen", "wie", "be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort der Sessel vor dem Pulte", "tokens": ["Dort", "der", "Ses\u00b7sel", "vor", "dem", "Pul\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Spieluhr an der Wand,", "tokens": ["Und", "die", "Spie\u00b7luhr", 