{"dta.poem.21099": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Schicksal und das Ich.  \n ( Nach Jean Paul .)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ich.               ", "tokens": ["Das", "Ich", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Keine Freuden begehr' ich. Ich pfl\u00fcckte die Beere", "tokens": ["Kei\u00b7ne", "Freu\u00b7den", "be\u00b7gehr'", "ich", ".", "Ich", "pfl\u00fcck\u00b7te", "die", "Bee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "der Freude;", "tokens": ["der", "Freu\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Aber mit t\u00fcckischem Dorn ritzte die T\u00e4u-", "tokens": ["A\u00b7ber", "mit", "t\u00fc\u00b7cki\u00b7schem", "Dorn", "ritz\u00b7te", "die", "T\u00e4u"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "schende mich.", "tokens": ["schen\u00b7de", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Schimmert die Sonn' auch einmal durch die schwar-", "tokens": ["Schim\u00b7mert", "die", "Sonn'", "auch", "ein\u00b7mal", "durch", "die", "schwa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "zen dr\u00e4ngenden Wetter,", "tokens": ["zen", "dr\u00e4n\u00b7gen\u00b7den", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Werfen die Drohenden nur schw\u00e4rzere Schat-", "tokens": ["Wer\u00b7fen", "die", "Dro\u00b7hen\u00b7den", "nur", "schw\u00e4r\u00b7ze\u00b7re", "Schat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADJA", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "ten herab.", "tokens": ["ten", "her\u00b7ab", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Blinzelnder, gl\u00e4nzt es um dich? Es ist das Gl\u00e4n-", "tokens": ["Blin\u00b7zeln\u00b7der", ",", "gl\u00e4nzt", "es", "um", "dich", "?", "Es", "ist", "das", "Gl\u00e4n"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "zen des Schwertes,", "tokens": ["zen", "des", "Schwer\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Welchen der kommende Tag gegen den heu-", "tokens": ["Wel\u00b7chen", "der", "kom\u00b7men\u00b7de", "Tag", "ge\u00b7gen", "den", "heu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "tigen zuckt!", "tokens": ["ti\u00b7gen", "zuckt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Ich entsage dir, ", "tokens": ["Ich", "ent\u00b7sa\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "verlockest,", "tokens": ["ver\u00b7lo\u00b7ckest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Und den Verlockten umschlingst, und den", "tokens": ["Und", "den", "Ver\u00b7lock\u00b7ten", "um\u00b7schlingst", ",", "und", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "ART"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.12": {"text": "Umschlungnen erdr\u00fcckst.", "tokens": ["Um\u00b7schlung\u00b7nen", "er\u00b7dr\u00fcckst", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "\u00d6d' und leer wird das schwindelnde Herz im Wirbel", "tokens": ["\u00d6d'", "und", "leer", "wird", "das", "schwin\u00b7deln\u00b7de", "Herz", "im", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "der Freude.", "tokens": ["der", "Freu\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Du, o Schwermuth, allein f\u00fcllest das Lech-", "tokens": ["Du", ",", "o", "Schwer\u00b7muth", ",", "al\u00b7lein", "f\u00fcl\u00b7lest", "das", "Lech"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "FM", "NN", "$,", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "zende aus!", "tokens": ["zen\u00b7de", "aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Das Schicksal.               ", "tokens": ["Das", "Schick\u00b7sal", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sohn des Staubes, es haben die G\u00f6tter ihr Schooss-", "tokens": ["Sohn", "des", "Stau\u00b7bes", ",", "es", "ha\u00b7ben", "die", "G\u00f6t\u00b7ter", "ihr", "Schooss"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "PPOSAT", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "kind, die Liebe,", "tokens": ["kind", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Freundlich den Menschen gesandt. W\u00e4hlest", "tokens": ["Freund\u00b7lich", "den", "Men\u00b7schen", "ge\u00b7sandt", ".", "W\u00e4h\u00b7lest"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "du Liebe, so sprich.", "tokens": ["du", "Lie\u00b7be", ",", "so", "sprich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Das Ich.               ", "tokens": ["Das", "Ich", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Nein, nicht Liebe begehr' ich. Ich dr\u00fcckte die", "tokens": ["Nein", ",", "nicht", "Lie\u00b7be", "be\u00b7gehr'", "ich", ".", "Ich", "dr\u00fcck\u00b7te", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Rose der Liebe", "tokens": ["Ro\u00b7se", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "An die verschmachtende Brust, und sie", "tokens": ["An", "die", "ver\u00b7schmach\u00b7ten\u00b7de", "Brust", ",", "und", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "durchstach mir das Herz.", "tokens": ["durch\u00b7stach", "mir", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Warme Tropfen bethauten am Abend die duftende", "tokens": ["War\u00b7me", "Trop\u00b7fen", "be\u00b7thau\u00b7ten", "am", "A\u00b7bend", "die", "duf\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rose;", "tokens": ["Ro\u00b7se", ";"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aber im Athem der Nacht froren die Tropfen", "tokens": ["A\u00b7ber", "im", "A\u00b7them", "der", "Nacht", "fro\u00b7ren", "die", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "zu Eis.", "tokens": ["zu", "Eis", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Liebe, du sch\u00fcttest am Morgen des Lebens, ein", "tokens": ["Lie\u00b7be", ",", "du", "sch\u00fct\u00b7test", "am", "Mor\u00b7gen", "des", "Le\u00b7bens", ",", "ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "ART"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "strahlendes Fr\u00fchroth,", "tokens": ["strah\u00b7len\u00b7des", "Fr\u00fch\u00b7roth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "\u00dcber des Wanderers Haupt Rosen und Per-", "tokens": ["\u00dc\u00b7ber", "des", "Wan\u00b7de\u00b7rers", "Haupt", "Ro\u00b7sen", "und", "Per"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "NN", "KON", "TRUNC"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.8": {"text": "len herab.", "tokens": ["len", "her\u00b7ab", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Taumelnder Wandrer, zur\u00fcck! Betritt nicht die", "tokens": ["Tau\u00b7meln\u00b7der", "Wand\u00b7rer", ",", "zu\u00b7r\u00fcck", "!", "Be\u00b7tritt", "nicht", "die"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PTKVZ", "$.", "NN", "PTKNEG", "ART"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.10": {"text": "glimmende Wolke!", "tokens": ["glim\u00b7men\u00b7de", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ihre Rosen sind Dunst. Thr\u00e4nen, ihr Perlen,", "tokens": ["Ih\u00b7re", "Ro\u00b7sen", "sind", "Dunst", ".", "Thr\u00e4\u00b7nen", ",", "ihr", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "seyd ihr \u2014", "tokens": ["seyd", "ihr"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PPER", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ich entsage dir, ", "tokens": ["Ich", "ent\u00b7sa\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Schmerzen,", "tokens": ["Schmer\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "An erhabnerem Gram sterben als, Feindin,", "tokens": ["An", "er\u00b7hab\u00b7ne\u00b7rem", "Gram", "ster\u00b7ben", "als", ",", "Fein\u00b7din", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "KOKOM", "$,", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "an dir!", "tokens": ["an", "dir", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Das Schicksal.               ", "tokens": ["Das", "Schick\u00b7sal", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sohn des Staubes, es heilt der Freundschaft Balsam", "tokens": ["Sohn", "des", "Stau\u00b7bes", ",", "es", "heilt", "der", "Freund\u00b7schaft", "Bal\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "die Busen,", "tokens": ["die", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Welche die Liebe zerriss. W\u00e4hlest du", "tokens": ["Wel\u00b7che", "die", "Lie\u00b7be", "zer\u00b7riss", ".", "W\u00e4h\u00b7lest", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$.", "VVFIN", "PPER"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Freundschaft, so sprich.", "tokens": ["Freund\u00b7schaft", ",", "so", "sprich", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Das Ich.               ", "tokens": ["Das", "Ich", "."], "token_info": ["word", "word", "punct"], "pos": ["PDS", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Keine Freunde begehr' ich. \u2014 Wo seyd ihr, holde", "tokens": ["Kei\u00b7ne", "Freun\u00b7de", "be\u00b7gehr'", "ich", ".", "Wo", "seyd", "ihr", ",", "hol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$.", "$(", "PWAV", "VAFIN", "PPER", "$,", "ADJA"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gestalten,", "tokens": ["Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Die ihr mit liebendem Arm euren Geliebten", "tokens": ["Die", "ihr", "mit", "lie\u00b7ben\u00b7dem", "Arm", "eu\u00b7ren", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "umschlangt?", "tokens": ["um\u00b7schlangt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Siehe, wir standen auf hohlen, d\u00fcnn\u00fcberw\u00f6lbeten", "tokens": ["Sie\u00b7he", ",", "wir", "stan\u00b7den", "auf", "hoh\u00b7len", ",", "d\u00fcn\u00b7n\u00fc\u00b7ber\u00b7w\u00f6l\u00b7be\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "APPR", "ADJA", "$,", "VVFIN"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gr\u00e4bern,", "tokens": ["Gr\u00e4\u00b7bern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nah an einander geschmiegt, fest in einander", "tokens": ["Nah", "an", "ein\u00b7an\u00b7der", "ge\u00b7schmiegt", ",", "fest", "in", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "VVPP", "$,", "ADJD", "APPR", "PRF"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "versch\u00fcrzt . . .", "tokens": ["ver\u00b7sch\u00fcrzt", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["VVPP", "$.", "$.", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Und das Gew\u00f6lbe zerborst. Die Erblassenden san-", "tokens": ["Und", "das", "Ge\u00b7w\u00f6l\u00b7be", "zer\u00b7borst", ".", "Die", 