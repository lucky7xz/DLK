{"textgrid.poem.36183": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das einst auf ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das einst auf ", "tokens": ["Das", "einst", "auf"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Verschlug das Schicksal in die\u00df Land", "tokens": ["Ver\u00b7schlug", "das", "Schick\u00b7sal", "in", "die\u00df", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Strickzeugs und der Spinner\u00e4dchen.", "tokens": ["Des", "Strick\u00b7zeugs", "und", "der", "Spin\u00b7ne\u00b7r\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Spuhl' und Strumpf sind ihr nicht lieb;", "tokens": ["Doch", "Sp\u00b7uhl'", "und", "Strumpf", "sind", "ihr", "nicht", "lieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "D'rum treibt sie hier, was dort sie trieb.", "tokens": ["D'\u00b7rum", "treibt", "sie", "hier", ",", "was", "dort", "sie", "trieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wollt denn, ihr Leutchen, euch bequemen,", "tokens": ["Woll