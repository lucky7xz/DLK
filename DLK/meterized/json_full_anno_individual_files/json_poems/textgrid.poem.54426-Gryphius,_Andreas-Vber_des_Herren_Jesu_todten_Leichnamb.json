{"textgrid.poem.54426": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Vber des Herren Jesu todten Leichnamb", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! was seh Ich hier ein au\u00dfgestreckte Leichen/", "tokens": ["Ach", "weh", "!", "was", "seh", "Ich", "hier", "ein", "au\u00df\u00b7ge\u00b7streck\u00b7te", "Lei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An der man von fu\u00df auff nichts vnzerschlagen find/", "tokens": ["An", "der", "man", "von", "fu\u00df", "auff", "nichts", "vn\u00b7zer\u00b7schla\u00b7gen", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "APPR", "PTKVZ", "APPR", "PIS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seit au\u00df der das Blutt mit vollen Str\u00f6men rinnt;", "tokens": ["Die", "Seit", "au\u00df", "der", "das", "Blutt", "mit", "vol\u00b7len", "Str\u00f6\u00b7men", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wangen so von Schmertz v\u00f1 Todes-Angst erbleichen/", "tokens": ["Die", "Wan\u00b7gen", "so", "von", "Schmertz", "v\u00f1", "To\u00b7des\u00b7Angst", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", 