{"dta.poem.12289": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tabakslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wach auf! Wach auf, der Steuermann k\u00f6mmt,               ", "tokens": ["Wach", "auf", "!", "Wach", "auf", ",", "der", "Steu\u00b7er\u00b7mann", "k\u00f6mmt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Er hat sein gro\u00dfes Licht schon angez\u00fcndt.", "tokens": ["Er", "hat", "sein", "gro\u00b7\u00dfes", "Licht", "schon", "an\u00b7ge\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hat ers angez\u00fcndt, so giebts einen Schein,", "tokens": ["Hat", "ers", "an\u00b7ge\u00b7z\u00fcndt", ",", "so", "giebts", "ei\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Damit so fahren wir ins Bergwerk ein.", "tokens": ["Da\u00b7mit", "so", "fah\u00b7ren", "wir", "ins", "Berg\u00b7werk", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Eine gr\u00e4bt Silber, der Andre gr\u00e4bt Gold,", "tokens": ["Der", "Ei\u00b7ne", "gr\u00e4bt", "Sil\u00b7ber", ",", "der", "And\u00b7re", "gr\u00e4bt", "Gold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NN", "$,", "PRELS", "PIS", "VVFIN", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dem schwarzbraunen M\u00e4gdlein sind wir hold.", "tokens": ["Dem", "schwarz\u00b7brau\u00b7nen", "M\u00e4gd\u00b7lein", "sind", "wir", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Tabak! Tabak! \u00e4chtadliges Kraut!", "tokens": ["Ta\u00b7bak", "!", "Ta\u00b7bak", "!", "\u00e4ch\u00b7tad\u00b7li\u00b7ges", "Kraut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ADJA", "NN", "$."], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Tabak! Tabak! du stinkendes Kraut.", "tokens": ["Ta\u00b7bak", "!", "Ta\u00b7bak", "!", "du", "stin\u00b7ken\u00b7des", "Kraut", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Wer dich erfand, ist wohl lobenswerth,", "tokens": ["Wer", "dich", "er\u00b7fand", ",", "ist", "wohl", "lo\u00b7bens\u00b7werth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer dich erfand, ist wohl pl\u00fcgelnswerth.", "tokens": ["Wer", "dich", "er\u00b7fand", ",", "ist", "wohl", "pl\u00fc\u00b7gelns\u00b7werth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}