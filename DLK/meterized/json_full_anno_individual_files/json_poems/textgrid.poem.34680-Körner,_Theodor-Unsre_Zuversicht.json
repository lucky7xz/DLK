{"textgrid.poem.34680": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Unsre Zuversicht", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir rufen dich mit freud'gen Blicken", "tokens": ["Wir", "ru\u00b7fen", "dich", "mit", "freu\u00b7d'\u00b7gen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und halten fest an deinem Wort.", "tokens": ["Und", "hal\u00b7ten", "fest", "an", "dei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die H\u00f6lle soll uns nicht ber\u00fccken", "tokens": ["Die", "H\u00f6l\u00b7le", "soll", "uns", "nicht", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Aberwitz und Meuchelmord;", "tokens": ["Durch", "A\u00b7ber\u00b7witz", "und", "Meu\u00b7chel\u00b7mord", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was auch rings in Tr\u00fcmmern geht,", "tokens": ["Und", "was", "auch", "rings", "in", "Tr\u00fcm\u00b7mern", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wissen's, da\u00df dein Wort besteht.", "tokens": ["Wir", "wis\u00b7sen's", ",", "da\u00df", "dein", "Wort", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht leichten Kampfes siegt der Glaube,", "tokens": ["Nicht", "leich\u00b7ten", "Kamp\u00b7fes", "siegt", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Solch Gut will schwer errungen sein.", "tokens": ["Solch", "Gut", "will", "schwer", "er\u00b7run\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Freiwillig tr\u00e4nkt uns keine Traube,", "tokens": ["Frei\u00b7wil\u00b7lig", "tr\u00e4nkt", "uns", "kei\u00b7ne", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kelter nur erpre\u00dft den Wein;", "tokens": ["Die", "Kel\u00b7ter", "nur", "er\u00b7pre\u00dft", "den", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und will ein Engel himmelw\u00e4rts,", "tokens": ["Und", "will", "ein", "En\u00b7gel", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erst bricht im Tod ein Menschenherz.", "tokens": ["Erst", "bricht", "im", "Tod", "ein", "Men\u00b7schen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum mag auch noch im falschen Leben", "tokens": ["Drum", "mag", "auch", "noch", "im", "fal\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die L\u00fcge ihre Tempel bau'n,", "tokens": ["Die", "L\u00fc\u00b7ge", "ih\u00b7re", "Tem\u00b7pel", "bau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00f6gen goldne Schurken beben", "tokens": ["Und", "m\u00f6\u00b7gen", "gold\u00b7ne", "Schur\u00b7ken", "be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich vor Kraft und Tugend grau'n", "tokens": ["Und", "sich", "vor", "Kraft", "und", "Tu\u00b7gend", "grau'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit der Feigheit Schwindeldrehn", "tokens": ["Und", "mit", "der", "Feig\u00b7heit", "Schwin\u00b7del\u00b7drehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor dem erwachten Volke stehn,", "tokens": ["Vor", "dem", "er\u00b7wach\u00b7ten", "Vol\u00b7ke", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und m\u00f6gen sich noch Br\u00fcder trennen", "tokens": ["Und", "m\u00f6\u00b7gen", "sich", "noch", "Br\u00fc\u00b7der", "tren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sich in blut'gem Ha\u00df entzwei'n", "tokens": ["Und", "sich", "in", "blut'\u00b7gem", "Ha\u00df", "ent\u00b7zwei'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deutsche F\u00fcrsten es verkennen,", "tokens": ["Und", "deut\u00b7sche", "F\u00fcrs\u00b7ten", "es", "ver\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihre Kronen Schwestern sei'n", "tokens": ["Da\u00df", "ih\u00b7re", "Kro\u00b7nen", "Schwes\u00b7tern", "sei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df, wenn Deutschland einig blieb,", "tokens": ["Und", "da\u00df", ",", "wenn", "Deutschland", "ei\u00b7nig", "blieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Es einer Welt Gesetze schrieb:", "tokens": ["Es", "ei\u00b7ner", "Welt", "Ge\u00b7set\u00b7ze", "schrieb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir wollen nicht an dir verzagen", "tokens": ["Wir", "wol\u00b7len", "nicht", "an", "dir", "ver\u00b7za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und treu und festen Mutes sein.", "tokens": ["Und", "treu", "und", "fes\u00b7ten", "Mu\u00b7tes", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst den W\u00fct'rich doch erschlagen", "tokens": ["Du", "wirst", "den", "W\u00fct'\u00b7rich", "doch", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wirst dein deutsches Land befrei'n.", "tokens": ["Und", "wirst", "dein", "deut\u00b7sches", "Land", "be\u00b7frei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Liegt auch der Tag noch jahreweit \u2013", "tokens": ["Liegt", "auch", "der", "Tag", "noch", "jah\u00b7re\u00b7weit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer wei\u00df, als du, die rechte Zeit?", "tokens": ["Wer", "wei\u00df", ",", "als", "du", ",", "die", "rech\u00b7te", "Zeit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die rechte Zeit zur guten Sache,", "tokens": ["Die", "rech\u00b7te", "Zeit", "zur", "gu\u00b7ten", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Freiheit, zum Tyrannentod!", "tokens": ["Zur", "Frei\u00b7heit", ",", "zum", "Ty\u00b7ran\u00b7nen\u00b7tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor deinem Schwerte sinkt der Drache", "tokens": ["Vor", "dei\u00b7nem", "Schwer\u00b7te", "sinkt", "der", "Dra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4rbt die deutschen Str\u00f6me rot", "tokens": ["Und", "f\u00e4rbt", "die", "deut\u00b7schen", "Str\u00f6\u00b7me", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Sklavenblut und freiem Blut!", "tokens": ["Mit", "Skla\u00b7ven\u00b7blut", "und", "frei\u00b7em", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du treuer Gott, verwalt' es gut!", "tokens": ["Du", "treu\u00b7er", "Gott", ",", "ver\u00b7walt'", "es", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir rufen dich mit freud'gen Bli