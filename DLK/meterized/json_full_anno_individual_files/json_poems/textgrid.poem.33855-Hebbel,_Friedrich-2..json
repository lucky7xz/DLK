{"textgrid.poem.33855": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein eitles W\u00e4hnen!", "tokens": ["Das", "ist", "ein", "eit\u00b7les", "W\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei nicht so feig, mein Herz!", "tokens": ["Sei", "nicht", "so", "feig", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gieb redlich Thr\u00e4nen um Thr\u00e4nen,", "tokens": ["Gieb", "red\u00b7lich", "Thr\u00e4\u00b7nen", "um", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimm tapfer Schmerz um Schmerz!", "tokens": ["Nimm", "tap\u00b7fer", "Schmerz", "um", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will dich weinen sehen,", "tokens": ["Ich", "will", "dich", "wei\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum ersten und letzten Mal!", "tokens": ["Zum", "ers\u00b7ten", "und", "letz\u00b7ten", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["A