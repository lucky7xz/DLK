{"textgrid.poem.25717": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich danke Gott, da\u00df ich zu Fu\u00df mu\u00df gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich danke Gott, da\u00df ich zu Fu\u00df mu\u00df gehen,", "tokens": ["Ich", "dan\u00b7ke", "Gott", ",", "da\u00df", "ich", "zu", "Fu\u00df", "mu\u00df", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht fahren und nicht reiten kann!", "tokens": ["Nicht", "fah\u00b7ren", "und", "nicht", "rei\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Gemse Klippen und des Adlers H\u00f6hen", "tokens": ["Der", "Gem\u00b7se", "Klip\u00b7pen", "und", "des", "Ad\u00b7lers", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Klimmt keines F\u00fcrsten Ro\u00df hinan.", "tokens": ["Klimmt", "kei\u00b7nes", "F\u00fcrs\u00b7ten", "Ro\u00df", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich aber h\u00f6re, auf des Brockens Spitze,", "tokens": ["Ich", "a\u00b7ber", "h\u00f6\u00b7re", ",", "auf", "des", "Bro\u00b7ckens", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von meinem Fu\u00df' hinab ins Land", "tokens": ["Von", "mei\u00b7nem", "Fu\u00df'", "hin\u00b7ab", "ins", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Donner rollen, und die Blitze", "tokens": ["Die", "Don\u00b7ner", "rol\u00b7len", ",", "und", "die", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Greif' ich am Saum' mit meiner Hand.", "tokens": ["Greif'", "ich", "am", "Saum'", "mit", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mir ist, als m\u00fc\u00dft' ich mich an diese hangen,", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00dft'", "ich", "mich", "an", "die\u00b7se", "han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "PRF", "APPR", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als sollten sie mich nach sich ziehn;", "tokens": ["Als", "soll\u00b7ten", "sie", "mich", "nach", "sich", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn jeder Erdenwunsch ist mir vergangen,", "tokens": ["Denn", "je\u00b7der", "Er\u00b7den\u00b7wunsch", "ist", "mir", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und klein, was sonst so gro\u00df mir schien.", "tokens": ["Und", "klein", ",", "was", "sonst", "so", "gro\u00df", "mir", "schien", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wie magst du dieses nur einmal verstehen,", "tokens": ["Wie", "magst", "du", "die\u00b7ses", "nur", "ein\u00b7mal", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PDAT", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wie vollends f\u00fchlen, reicher Mann?", "tokens": ["Wie", "vol\u00b7lends", "f\u00fch\u00b7len", ",", "rei\u00b7cher", "Mann", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVINF", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Drum dank' ich Gott, da\u00df ich zu Fu\u00df mu\u00df gehen,", "tokens": ["Drum", "dank'", "ich", "Gott", ",", "da\u00df", "ich", "zu", "Fu\u00df", "mu\u00df", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nicht fahren und nicht reiten kann.", "tokens": ["Nicht", "fah\u00b7ren", "und", "nicht", "rei