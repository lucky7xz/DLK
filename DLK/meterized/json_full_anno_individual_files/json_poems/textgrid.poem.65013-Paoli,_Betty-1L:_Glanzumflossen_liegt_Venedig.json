{"textgrid.poem.65013": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Glanzumflossen liegt Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Glanzumflossen liegt Venedig", "tokens": ["Glan\u00b7zum\u00b7flos\u00b7sen", "liegt", "Ve\u00b7ne\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sanft vom Mondesstrahl umgaukelt,", "tokens": ["Sanft", "vom", "Mon\u00b7des\u00b7strahl", "um\u00b7gau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend die Lagune gn\u00e4dig", "tokens": ["W\u00e4h\u00b7rend", "die", "La\u00b7gu\u00b7ne", "gn\u00e4\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Unsre Gondel wiegt und schaukelt.", "tokens": ["Uns\u00b7re", "Gon\u00b7del", "wiegt", "und", "schau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still ist's in dem weiten Eden,", "tokens": ["Still", "ist's", "in", "dem", "wei\u00b7ten", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur die Woge sch\u00e4umt und rauschet", "tokens": ["Nur", "die", "Wo\u00b7ge", "sch\u00e4umt", "und", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich lausche deinen Reden,", "tokens": ["Und", "ich", "lau\u00b7sche", "dei\u00b7nen", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie man holden M\u00e4hrchen lauschet.", "tokens": ["Wie", "man", "hol\u00b7den", "M\u00e4hr\u00b7chen", "lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und du sprichst mir von der Einen,", "tokens": ["Und", "du", "sprichst", "mir", "von", "der", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprichst von ihr der Reizverkl\u00e4rten,", "tokens": ["Sprichst", "von", "ihr", "der", "Reiz\u00b7ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Blicke beten, weinen,", "tokens": ["De\u00b7ren", "Bli\u00b7cke", "be\u00b7ten", ",", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glauben dich und lieben lehrten.", "tokens": ["Glau\u00b7ben", "dich", "und", "lie\u00b7ben", "lehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du sagst mir, welche Dornen", "tokens": ["Und", "du", "sagst", "mir", ",", "wel\u00b7che", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Seele blutig ritzen,", "tokens": ["Dei\u00b7ne", "See\u00b7le", "blu\u00b7tig", "rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkst du sehnend der Erkornen,", "tokens": ["Denkst", "du", "seh\u00b7nend", "der", "Er\u00b7kor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du nimmer wirst besitzen.", "tokens": ["Die", "du", "nim\u00b7mer", "wirst", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alter Schmerzen Heer entsteiget", "tokens": ["Al\u00b7ter", "Schmer\u00b7zen", "Heer", "ent\u00b7stei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen dunkeln Gr\u00fcften wieder,", "tokens": ["Sei\u00b7nen", "dun\u00b7keln", "Gr\u00fcf\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Haupt, das stolze, neiget", "tokens": ["Und", "dein", "Haupt", ",", "das", "stol\u00b7ze", ",", "nei\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still auf meine Hand sich nieder.", "tokens": ["Still", "auf", "mei\u00b7ne", "Hand", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was verstummst du? Rede, klage!", "tokens": ["Was", "ver\u00b7stummst", "du", "?", "Re\u00b7de", ",", "kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dein Wort auf Geisterschwingen", "tokens": ["La\u00df", "dein", "Wort", "auf", "Geis\u00b7ter\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Duft vom Bl\u00fctenhage", "tokens": ["Die\u00b7sen", "Duft", "vom", "Bl\u00fc\u00b7ten\u00b7ha\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Jugend zu mir bringen.", "tokens": ["Dei\u00b7ner", "Ju\u00b7gend", "zu", "mir", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Rede, klage! denn dein Trauern", "tokens": ["Re\u00b7de", ",", "kla\u00b7ge", "!", "denn", "dein", "Trau\u00b7ern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nur ein verh\u00fcllter Segen,", "tokens": ["Ist", "nur", "ein", "ver\u00b7h\u00fcll\u00b7ter", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aehnlich den Gewitterschauern,", "tokens": ["A\u00b7ehn\u00b7lich", "den", "Ge\u00b7wit\u00b7ter\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die des Fr\u00fchlings Herz bewegen.", "tokens": ["Die", "des", "Fr\u00fch\u00b7lings", "Herz", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach wie bald zieht er vor\u00fcber", "tokens": ["Ach", "wie", "bald", "zieht", "er", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dieser Lenz! wie bald von hinnen!", "tokens": ["Die\u00b7ser", "Lenz", "!", "wie", "bald", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "PWAV", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Herbst, ein \u00f6der, tr\u00fcber,", "tokens": ["Und", "ein", "Herbst", ",", "ein", "\u00f6\u00b7der", ",", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dann auch f\u00fcr dich beginnen.", "tokens": ["Wird", "dann", "auch", "f\u00fcr", "dich", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "F\u00fchlen wirst in sp\u00e4tern Stunden", "tokens": ["F\u00fch\u00b7len", "wirst", "in", "sp\u00e4\u00b7tern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du im tief geheimsten Wesen,", "tokens": ["Du", "im", "tief", "ge\u00b7heims\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Schmerz, den du empfunden,", "tokens": ["Da\u00df", "der", "Schmerz", ",", "den", "du", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Daseins Schmuck gewesen.", "tokens": ["Dei\u00b7nes", "Da\u00b7seins", "Schmuck", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Fruchtlos dich zur\u00fcckesehnen", "tokens": ["Frucht\u00b7los", "dich", "zu\u00b7r\u00fc\u00b7ck\u00b7e\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wirst du dann nach jenem Eiland,", "tokens": ["Wirst", "du", "dann", "nach", "je\u00b7nem", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Himmelsthau die Thr\u00e4nen", "tokens": ["Wo", "ein", "Him\u00b7mel\u00b7sthau", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schmerz ein starker Heiland. \u2013", "tokens": ["Und", "der", "Schmerz", "ein", "star\u00b7ker", "Hei\u00b7land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Meine beiden H\u00e4nde falte", "tokens": ["Mei\u00b7ne", "bei\u00b7den", "H\u00e4n\u00b7de", "fal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segnend ich, indem wir scheiden,", "tokens": ["Seg\u00b7nend", "ich", ",", "in\u00b7dem", "wir", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich bete: Gott erhalte", "tokens": ["Und", "ich", "be\u00b7te", ":", "Gott", "er\u00b7hal\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir noch lang dein sch\u00f6nes Leiden.", "tokens": ["Dir", "noch", "lang", "dein", "sch\u00f6\u00b7nes", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Glanzumflossen liegt Venedig", "tokens": ["Glan\u00b7zum\u00b7flos\u00b7sen", "liegt", "Ve\u00b7ne\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sanft vom Mondesstrahl umgaukelt,", "tokens": ["Sanft", "vom", "Mon\u00b7des\u00b7strahl", "um\u00b7gau\u00b7kelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend die Lagune gn\u00e4dig", "tokens": ["W\u00e4h\u00b7rend", "die", "La\u00b7gu\u00b7ne", "gn\u00e4\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Unsre Gondel wiegt und schaukelt.", "tokens": ["Uns\u00b7re", "Gon\u00b7del", "wiegt", "und", "schau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Still ist's in dem weiten Eden,", "tokens": ["Still", "ist's", "in", "dem", "wei\u00b7ten", "E\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur die Woge sch\u00e4umt und rauschet", "tokens": ["Nur", "die", "Wo\u00b7ge", "sch\u00e4umt", "und", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich lausche deinen Reden,", "tokens": ["Und", "ich", "lau\u00b7sche", "dei\u00b7nen", "Re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie man holden M\u00e4hrchen lauschet.", "tokens": ["Wie", "man", "hol\u00b7den", "M\u00e4hr\u00b7chen", "lau\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und du sprichst mir von der Einen,", "tokens": ["Und", "du", "sprichst", "mir", "von", "der", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprichst von ihr der Reizverkl\u00e4rten,", "tokens": ["Sprichst", "von", "ihr", "der", "Reiz\u00b7ver\u00b7kl\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Blicke beten, weinen,", "tokens": ["De\u00b7ren", "Bli\u00b7cke", "be\u00b7ten", ",", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "NN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glauben dich und lieben lehrten.", "tokens": ["Glau\u00b7ben", "dich", "und", "lie\u00b7ben", "lehr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und du sagst mir, welche Dornen", "tokens": ["Und", "du", "sagst", "mir", ",", "wel\u00b7che", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Seele blutig ritzen,", "tokens": ["Dei\u00b7ne", "See\u00b7le", "blu\u00b7tig", "rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denkst du sehnend der Erkornen,", "tokens": ["Denkst", "du", "seh\u00b7nend", "der", "Er\u00b7kor\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du nimmer wirst besitzen.", "tokens": ["Die", "du", "nim\u00b7mer", "wirst", "be\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Alter Schmerzen Heer entsteiget", "tokens": ["Al\u00b7ter", "Schmer\u00b7zen", "Heer", "ent\u00b7stei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seinen dunkeln Gr\u00fcften wieder,", "tokens": ["Sei\u00b7nen", "dun\u00b7keln", "Gr\u00fcf\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dein Haupt, das stolze, neiget", "tokens": ["Und", "dein", "Haupt", ",", "das", "stol\u00b7ze", ",", "nei\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still auf meine Hand sich nieder.", "tokens": ["Still", "auf", "mei\u00b7ne", "Hand", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Was verstummst du? Rede, klage!", "tokens": ["Was", "ver\u00b7stummst", "du", "?", "Re\u00b7de", ",", "kla\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dein Wort auf Geisterschwingen", "tokens": ["La\u00df", "dein", "Wort", "auf", "Geis\u00b7ter\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diesen Duft vom Bl\u00fctenhage", "tokens": ["Die\u00b7sen", "Duft", "vom", "Bl\u00fc\u00b7ten\u00b7ha\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Jugend zu mir bringen.", "tokens": ["Dei\u00b7ner", "Ju\u00b7gend", "zu", "mir", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Rede, klage! denn dein Trauern", "tokens": ["Re\u00b7de", ",", "kla\u00b7ge", "!", "denn", "dein", "Trau\u00b7ern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nur ein verh\u00fcllter Segen,", "tokens": ["Ist", "nur", "ein", "ver\u00b7h\u00fcll\u00b7ter", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aehnlich den Gewitterschauern,", "tokens": ["A\u00b7ehn\u00b7lich", "den", "Ge\u00b7wit\u00b7ter\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die des Fr\u00fchlings Herz bewegen.", "tokens": ["Die", "des", "Fr\u00fch\u00b7lings", "Herz", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ach wie bald zieht er vor\u00fcber", "tokens": ["Ach", "wie", "bald", "zieht", "er", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Dieser Lenz! wie bald von hinnen!", "tokens": ["Die\u00b7ser", "Lenz", "!", "wie", "bald", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$.", "PWAV", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Herbst, ein \u00f6der, tr\u00fcber,", "tokens": ["Und", "ein", "Herbst", ",", "ein", "\u00f6\u00b7der", ",", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dann auch f\u00fcr dich beginnen.", "tokens": ["Wird", "dann", "auch", "f\u00fcr", "dich", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "F\u00fchlen wirst in sp\u00e4tern Stunden", "tokens": ["F\u00fch\u00b7len", "wirst", "in", "sp\u00e4\u00b7tern", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du im tief geheimsten Wesen,", "tokens": ["Du", "im", "tief", "ge\u00b7heims\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Schmerz, den du empfunden,", "tokens": ["Da\u00df", "der", "Schmerz", ",", "den", "du", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Daseins Schmuck gewesen.", "tokens": ["Dei\u00b7nes", "Da\u00b7seins", "Schmuck", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Fruchtlos dich zur\u00fcckesehnen", "tokens": ["Frucht\u00b7los", "dich", "zu\u00b7r\u00fc\u00b7ck\u00b7e\u00b7seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wirst du dann nach jenem Eiland,", "tokens": ["Wirst", "du", "dann", "nach", "je\u00b7nem", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein Himmelsthau die Thr\u00e4nen", "tokens": ["Wo", "ein", "Him\u00b7mel\u00b7sthau", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schmerz ein starker Heiland. \u2013", "tokens": ["Und", "der", "Schmerz", "ein", "star\u00b7ker", "Hei\u00b7land", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.22": {"line.1": {"text": "Meine beiden H\u00e4nde falte", "tokens": ["Mei\u00b7ne", "bei\u00b7den", "H\u00e4n\u00b7de", "fal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segnend ich, indem wir scheiden,", "tokens": ["Seg\u00b7nend", "ich", ",", "in\u00b7dem", "wir", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich bete: Gott erhalte", "tokens": ["Und", "ich", "be\u00b7te", ":", "Gott", "er\u00b7hal\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir noch lang dein sch\u00f6nes Leiden.", "tokens": ["Dir", "noch", "lang", "dein", "sch\u00f6\u00b7nes", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}