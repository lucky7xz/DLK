+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein Spiegel widerstrahlt?", "tokens": ["Was", "dein", "Spie\u00b7gel", "wi\u00b7der\u00b7strahlt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Anna neigt vom gr\u00fcnen Strande", "tokens": ["An\u00b7na", "neigt", "vom", "gr\u00fc\u00b7nen", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in ihres Bildes N\u00e4h,", "tokens": ["Sich", "in", "ih\u00b7res", "Bil\u00b7des", "N\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streift vom Busen die Gewande,", "tokens": ["Streift", "vom", "Bu\u00b7sen", "die", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft ihn leuchten in den See.", "tokens": ["L\u00e4\u00dft", "ihn", "leuch\u00b7ten", "in", "den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Nach dem Bilde niederhangend,", "tokens": ["Nach", "dem", "Bil\u00b7de", "nie\u00b7der\u00b7han\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starrt sie zweifelnd und begl\u00fcckt,", "tokens": ["Starrt", "sie", "zwei\u00b7felnd", "und", "be\u00b7gl\u00fcckt", ","], "t