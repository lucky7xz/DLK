{"textgrid.poem.60430": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Anna steht in sich versunken,", "tokens": ["An\u00b7na", "steht", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicket in den See hinein,", "tokens": ["Bli\u00b7cket", "in", "den", "See", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weidet, eigner Sch\u00f6nheit trunken,", "tokens": ["Wei\u00b7det", ",", "eig\u00b7ner", "Sch\u00f6n\u00b7heit", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich an ihrem Widerschein.", "tokens": ["Sich", "an", "ih\u00b7rem", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie beginnt hinab zu reden:", "tokens": ["Sie", "be\u00b7ginnt", "hin\u00b7ab", "zu", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderholde Jungfrau, sprich,", "tokens": ["Wun\u00b7der\u00b7hol\u00b7de", "Jung\u00b7frau", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nstes Bild im Lande Schweden,", "tokens": ["Sch\u00f6ns\u00b7tes", "Bild", "im", "Lan\u00b7de", "Schwe\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich du? und bist du ich?", "tokens": ["Bin", "ich", "du", "?", "und", "bist", "du", "ich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$.", "KON", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein, o nein, ich glaub es nimmer,", "tokens": ["Nein", ",", "o", "nein", ",", "ich", "glaub", "es", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es auch die Welt mir schw\u00f6rt,", "tokens": ["Wenn", "es", "auch", "die", "Welt", "mir", "schw\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df so heller Rosenschimmer", "tokens": ["Da\u00df", "so", "hel\u00b7ler", "Ro\u00b7sen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Wangen angeh\u00f6rt.", "tokens": ["Mei\u00b7nen", "Wan\u00b7gen", "an\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieser Mund, ist er der meine,", "tokens": ["Die\u00b7ser", "Mund", ",", "ist", "er", "der", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VAFIN", "PPER", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den dies s\u00fc\u00dfe L\u00e4cheln bricht?", "tokens": ["Den", "dies", "s\u00fc\u00b7\u00dfe", "L\u00e4\u00b7cheln", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seh ich doch, wie auch der deine", "tokens": ["Seh", "ich", "doch", ",", "wie", "auch", "der", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fragend mir entgegenspricht.", "tokens": ["Fra\u00b7gend", "mir", "ent\u00b7ge\u00b7gen\u00b7spricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebes Wasser, sag, erz\u00e4hle,", "tokens": ["Lie\u00b7bes", "Was\u00b7ser", ",", "sag", ",", "er\u00b7z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast mein Auge du gemalt?", "tokens": ["Hast", "mein", "Au\u00b7ge", "du", "ge\u00b7malt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder ist des Himmels Seele,", "tokens": ["O\u00b7der", "ist", "des", "Him\u00b7mels", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein Spiegel widerstrahlt?", "tokens": ["Was", "dein", "Spie\u00b7gel", "wi\u00b7der\u00b7strahlt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Anna neigt vom gr\u00fcnen Strande", "tokens": ["An\u00b7na", "neigt", "vom", "gr\u00fc\u00b7nen", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in ihres Bildes N\u00e4h,", "tokens": ["Sich", "in", "ih\u00b7res", "Bil\u00b7des", "N\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streift vom Busen die Gewande,", "tokens": ["Streift", "vom", "Bu\u00b7sen", "die", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft ihn leuchten in den See.", "tokens": ["L\u00e4\u00dft", "ihn", "leuch\u00b7ten", "in", "den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nach dem Bilde niederhangend,", "tokens": ["Nach", "dem", "Bil\u00b7de", "nie\u00b7der\u00b7han\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starrt sie zweifelnd und begl\u00fcckt,", "tokens": ["Starrt", "sie", "zwei\u00b7felnd", "und", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Bild, ihr nachverlangend,", "tokens": ["Und", "das", "Bild", ",", "ihr", "nach\u00b7ver\u00b7lan\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Starrt bewundernd und entz\u00fcckt.", "tokens": ["Starrt", "be\u00b7wun\u00b7dernd", "und", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fragt das Bild, im Wasser schwebend:", "tokens": ["Fragt", "das", "Bild", ",", "im", "Was\u00b7ser", "schwe\u00b7bend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anna, hab ich dich erreicht?", "tokens": ["An\u00b7na", ",", "hab", "ich", "dich", "er\u00b7reicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragt das M\u00e4dchen, freudig bebend:", "tokens": ["Fragt", "das", "M\u00e4d\u00b7chen", ",", "freu\u00b7dig", "be\u00b7bend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich sch\u00f6ner noch vielleicht?", "tokens": ["Bin", "ich", "sch\u00f6\u00b7ner", "noch", "viel\u00b7leicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "In den seligen Geb\u00e4rden,", "tokens": ["In", "den", "se\u00b7li\u00b7gen", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die das Bild ihr abgelauscht,", "tokens": ["Die", "das", "Bild", "ihr", "ab\u00b7ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht sich Anna sch\u00f6ner werden,", "tokens": ["Sieht", "sich", "An\u00b7na", "sch\u00f6\u00b7ner", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Jungfrau steht berauscht.", "tokens": ["Und", "die", "Jung\u00b7frau", "steht", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "\u00bbwenn so sch\u00f6n ich immer bliebe!", "tokens": ["\u00bb", "wenn", "so", "sch\u00f6n", "ich", "im\u00b7mer", "blie\u00b7be", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df dies Bild denn auch vergehn?\u00ab", "tokens": ["Mu\u00df", "dies", "Bild", "denn", "auch", "ver\u00b7gehn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PDS", "NN", "KON", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruft sie, eitler Eigenliebe,", "tokens": ["Ruft", "sie", ",", "eit\u00b7ler", "Ei\u00b7gen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horch! die Winde sausend wehn!", "tokens": ["Horch", "!", "die", "Win\u00b7de", "sau\u00b7send", "wehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Rauschend wird ihr Bild zertr\u00fcmmert", "tokens": ["Rau\u00b7schend", "wird", "ihr", "Bild", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im emp\u00f6rten Wellenschaum;", "tokens": ["Im", "em\u00b7p\u00f6r\u00b7ten", "Wel\u00b7len\u00b7schaum", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das M\u00e4dchen sieht bek\u00fcmmert", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "sieht", "be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich darin vergehn wie Traum.", "tokens": ["Sich", "da\u00b7rin", "ver\u00b7gehn", "wie", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "VVINF", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und im Walde knarrt es knickend,", "tokens": ["Und", "im", "Wal\u00b7de", "knarrt", "es", "kni\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und am Ufer schwankt das Rohr,", "tokens": ["Und", "am", "U\u00b7fer", "schwankt", "das", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Aus den Weiden, freundlich nickend,", "tokens": ["Aus", "den", "Wei\u00b7den", ",", "freund\u00b7lich", "ni\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Huscht ein altes Weib hervor.", "tokens": ["Huscht", "ein", "al\u00b7tes", "Weib", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Alte spricht, und weint verstohlen:", "tokens": ["Al\u00b7te", "spricht", ",", "und", "weint", "ver\u00b7stoh\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbwie dein Bild im Wind zerfuhr,", "tokens": ["\u00bb", "wie", "dein", "Bild", "im", "Wind", "zer\u00b7fuhr", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrden deine Kinder holen", "tokens": ["W\u00fcr\u00b7den", "dei\u00b7ne", "Kin\u00b7der", "ho\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner Sch\u00f6nheit letzte Spur.", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "letz\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Denn die Sch\u00f6nheit ihrer Mutter", "tokens": ["Denn", "die", "Sch\u00f6n\u00b7heit", "ih\u00b7rer", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Kinder liebster Fra\u00df,", "tokens": ["Ist", "der", "Kin\u00b7der", "liebs\u00b7ter", "Fra\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der Kinder feinstes Futter;", "tokens": ["Ist", "der", "Kin\u00b7der", "feins\u00b7tes", "Fut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ne Jungfrau, merk dir das!", "tokens": ["Sch\u00f6\u00b7ne", "Jung\u00b7frau", ",", "merk", "dir", "das", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wag es nur und kehre wieder", "tokens": ["Wag", "es", "nur", "und", "keh\u00b7re", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem ersten Wochenweh,", "tokens": ["Nach", "dem", "ers\u00b7ten", "Wo\u00b7chen\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm und spiegle deine Glieder", "tokens": ["Komm", "und", "spieg\u00b7le", "dei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann im peinlich klaren See.", "tokens": ["Dann", "im", "pein\u00b7lich", "kla\u00b7ren", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Komm und schau dann mit Entsetzen", "tokens": ["Komm", "und", "schau", "dann", "mit", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Br\u00fcste, junges Blut,", "tokens": ["Dei\u00b7ne", "Br\u00fcs\u00b7te", ",", "jun\u00b7ges", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich gezognen Fischernetzen", "tokens": ["Gleich", "ge\u00b7zog\u00b7nen", "Fi\u00b7scher\u00b7net\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zitternd schwimmen in der Flut.", "tokens": ["Zit\u00b7ternd", "schwim\u00b7men", "in", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "O dann frage deinen Schatten:", "tokens": ["O", "dann", "fra\u00b7ge", "dei\u00b7nen", "Schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wangen, seid ihr mein, so bleich?", "tokens": ["Wan\u00b7gen", ",", "seid", "ihr", "mein", ",", "so", "bleich", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PPOSAT", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Augen mein, ihr hohlen, matten?", "tokens": ["Au\u00b7gen", "mein", ",", "ihr", "hoh\u00b7len", ",", "mat\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "$,", "PPOSAT", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinen wirst du in den Teich.", "tokens": ["Wei\u00b7nen", "wirst", "du", "in", "den", "Teich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Kommt ein Mann, um dich zu freien,", "tokens": ["Kommt", "ein", "Mann", ",", "um", "dich", "zu", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUI", "PRF", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eile du zu mir geschwind:", "tokens": ["Ei\u00b7le", "du", "zu", "mir", "ge\u00b7schwind", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich will den Leib dir feien,", "tokens": ["Und", "ich", "will", "den", "Leib", "dir", "fei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du nie empf\u00e4ngst ein Kind.\u00ab", "tokens": ["Da\u00df", "du", "nie", "emp\u00b7f\u00e4ngst", "ein", "Kind", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Anna spricht mit dunklen Schauern:", "tokens": ["An\u00b7na", "spricht", "mit", "dunk\u00b7len", "Schau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbwenn du mir zu helfen meinst,", "tokens": ["\u00bb", "wenn", "du", "mir", "zu", "hel\u00b7fen", "meinst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Sch\u00f6nheit mir mag dauern,", "tokens": ["Da\u00df", "die", "Sch\u00f6n\u00b7heit", "mir", "mag", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fctterlein, so komm ich einst.\u00ab", "tokens": ["M\u00fct\u00b7ter\u00b7lein", ",", "so", "komm", "ich", "einst", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Anna steht in sich versunken,", "tokens": ["An\u00b7na", "steht", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blicket in den See hinein,", "tokens": ["Bli\u00b7cket", "in", "den", "See", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weidet, eigner Sch\u00f6nheit trunken,", "tokens": ["Wei\u00b7det", ",", "eig\u00b7ner", "Sch\u00f6n\u00b7heit", "trun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich an ihrem Widerschein.", "tokens": ["Sich", "an", "ih\u00b7rem", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Sie beginnt hinab zu reden:", "tokens": ["Sie", "be\u00b7ginnt", "hin\u00b7ab", "zu", "re\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderholde Jungfrau, sprich,", "tokens": ["Wun\u00b7der\u00b7hol\u00b7de", "Jung\u00b7frau", ",", "sprich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nstes Bild im Lande Schweden,", "tokens": ["Sch\u00f6ns\u00b7tes", "Bild", "im", "Lan\u00b7de", "Schwe\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich du? und bist du ich?", "tokens": ["Bin", "ich", "du", "?", "und", "bist", "du", "ich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$.", "KON", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Nein, o nein, ich glaub es nimmer,", "tokens": ["Nein", ",", "o", "nein", ",", "ich", "glaub", "es", "nim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es auch die Welt mir schw\u00f6rt,", "tokens": ["Wenn", "es", "auch", "die", "Welt", "mir", "schw\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df so heller Rosenschimmer", "tokens": ["Da\u00df", "so", "hel\u00b7ler", "Ro\u00b7sen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Wangen angeh\u00f6rt.", "tokens": ["Mei\u00b7nen", "Wan\u00b7gen", "an\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Dieser Mund, ist er der meine,", "tokens": ["Die\u00b7ser", "Mund", ",", "ist", "er", "der", "mei\u00b7ne", ","], "token_info": 