{"dta.poem.50": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Gelegenheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Beste an der schnellen Zeit,", "tokens": ["Das", "Bes\u00b7te", "an", "der", "schnel\u00b7len", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die unvermerket uns verschwin-", "tokens": ["Die", "un\u00b7ver\u00b7mer\u00b7ket", "uns", "ver\u00b7schwin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist eben die Gelegenheit,", "tokens": ["Ist", "e\u00b7ben", "die", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die man gar selten wieder findet.", "tokens": ["Die", "man", "gar", "sel\u00b7ten", "wie\u00b7der", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zeit gleicht einem strengen Flus,", "tokens": ["Die", "Zeit", "gleicht", "ei\u00b7nem", "stren\u00b7gen", "Flus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mit den Tropfen weiter schiesset,", "tokens": ["Der", "mit", "den", "Trop\u00b7fen", "wei\u00b7ter", "schies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da in desselben regen Gu\u00df", "tokens": ["Da", "in", "des\u00b7sel\u00b7ben", "re\u00b7gen", "Gu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bisweilen eine Perle fliesset.", "tokens": ["Bis\u00b7wei\u00b7len", "ei\u00b7ne", "Per\u00b7le", "flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wer solche nicht so gleich auff\u00e4ngt,", "tokens": ["Wer", "sol\u00b7che", "nicht", "so", "gleich", "auf\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn sie vor ihn vor\u00fcber eilet,", "tokens": ["Wenn", "sie", "vor", "ihn", "vor\u00b7\u00fc\u00b7ber", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der sieht, wenn ers hernach bedenkt,", "tokens": ["Der", "sieht", ",", "wenn", "ers", "her\u00b7nach", "be\u00b7denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df er zu lange sich verweilet.", "tokens": ["Da\u00df", "er", "zu", "lan\u00b7ge", "sich", "ver\u00b7wei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der Strom nimmt sie so gleich mit hin", "tokens": ["Der", "Strom", "nimmt", "sie", "so", "gleich", "mit", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und l\u00e4sset im Vor\u00fcbergehen,", "tokens": ["Und", "l\u00e4s\u00b7set", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Den leicht zu fangenden Gewin,", "tokens": ["Den", "leicht", "zu", "fan\u00b7gen\u00b7den", "Ge\u00b7win", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So bald, wol gar nicht wiedersehen.", "tokens": ["So", "bald", ",", "wol", "gar", "nicht", "wie\u00b7der\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Zeitlauf gehet nicht zur\u00fck,", "tokens": ["Der", "Zeit\u00b7lauf", "ge\u00b7het", "nicht", "zu\u00b7r\u00fck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und in dem Lauf der schnellen Stunde,", "tokens": ["Und", "in", "dem", "Lauf", "der", "schnel\u00b7len", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Entstehet oft ein Augenblik,", "tokens": ["Ent\u00b7ste\u00b7het", "oft", "ein", "Au\u00b7gen\u00b7blik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und eine fl\u00fcchtige Secunde", "tokens": ["Und", "ei\u00b7ne", "fl\u00fcch\u00b7ti\u00b7ge", "Se\u00b7cun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Darinenn uns das Gl\u00fck anlacht,", "tokens": ["Da\u00b7ri\u00b7nenn", "uns", "das", "Gl\u00fck", "an\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wer alsdenn als ein Fauler tr\u00e4umet:", "tokens": ["Wer", "als\u00b7denn", "als", "ein", "Fau\u00b7ler", "tr\u00e4u\u00b7met", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nimt es nicht aufmerksam in acht,", "tokens": ["Nimt", "es", "nicht", "auf\u00b7merk\u00b7sam", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "CARD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Hat die Gelegenheit vers\u00e4umet.", "tokens": ["Hat", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "ver\u00b7s\u00e4u\u00b7met", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}