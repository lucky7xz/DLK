{"dta.poem.6291": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wird aufgel\u00f6s\u2019t, ver\u00e4ndert, f\u00e4llt zusammen", "tokens": ["Wird", "auf\u00b7ge\u00b7l\u00f6s't", ",", "ver\u00b7\u00e4n\u00b7dert", ",", "f\u00e4llt", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "VVPP", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In eine heisse Fluth, verh\u00e4rtet sich aufs nen.", "tokens": ["In", "ei\u00b7ne", "heis\u00b7se", "Fluth", ",", "ver\u00b7h\u00e4r\u00b7tet", "sich", "aufs", "nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn nun der Hitze Krafft verschwunden;", "tokens": ["Wenn", "nun", "der", "Hit\u00b7ze", "Krafft", "ver\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist der Klump alsdenn weit dichter noch verbunden.", "tokens": ["So", "ist", "der", "Klump", "als\u00b7denn", "weit", "dich\u00b7ter", "noch", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}