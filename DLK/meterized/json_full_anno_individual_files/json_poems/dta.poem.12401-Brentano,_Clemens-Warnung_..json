{"dta.poem.12401": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Warnung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kukuk auf dem Zaune sa\u00df,               ", "tokens": ["Der", "Ku\u00b7kuk", "auf", "dem", "Zau\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es regnet sehr und er ward na\u00df,", "tokens": ["Es", "reg\u00b7net", "sehr", "und", "er", "ward", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam ein hoher Sonnenschein,", "tokens": ["Da", "kam", "ein", "ho\u00b7her", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kukuk, der ward h\u00fcbsch und fein,", "tokens": ["Der", "Ku\u00b7kuk", ",", "der", "ward", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schwang er sein Gefieder", "tokens": ["Dann", "schwang", "er", "sein", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wohl \u00fcbern See hin\u00fcber.", "tokens": ["Wohl", "\u00fc\u00b7bern", "See", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kukuk, Kukuk, Kukuk.", "tokens": ["Ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da wandte er sich schnelle her,", "tokens": ["Da", "wand\u00b7te", "er", "sich", "schnel\u00b7le", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sang so traurig, bange, schwer:", "tokens": ["Er", "sang", "so", "trau\u00b7rig", ",", "ban\u00b7ge", ",", "schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201evon rothem Gold ein Ringelein,", "tokens": ["\u201e", "von", "ro\u00b7them", "Gold", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201elie\u00df ich im Bett der Liebsten mein,", "tokens": ["\u201e", "lie\u00df", "ich", "im", "Bett", "der", "Liebs\u00b7ten", "mein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201eich schwing nicht mein Gefieder,", "tokens": ["\u201e", "ich", "schwing", "nicht", "mein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ebis mir das Ringlein wieder.", "tokens": ["\u201e", "bis", "mir", "das", "Rin\u00b7glein", "wie\u00b7der", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ekukuk, Kukuk, Kukuk.\u201c", "tokens": ["\u201e", "ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "\u201each Goldschmidt, lieber Goldschmidt mein,", "tokens": ["\u201e", "ach", "Gold\u00b7schmidt", ",", "lie\u00b7ber", "Gold\u00b7schmidt", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "ADV", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eschmied' mir von Gold ein Ringelein,", "tokens": ["\u201e", "schmied'", "mir", "von", "Gold", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u201eschmied mir ihn an die rechte Hand,", "tokens": ["\u201e", "schmied", "mir", "ihn", "an", "die", "rech\u00b7te", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich nehm ihn mit ins Vaterland,", "tokens": ["\u201e", "ich", "nehm", "ihn", "mit", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201edann schwing ich mein Gefieder,", "tokens": ["\u201e", "dann", "schwing", "ich", "mein", "Ge\u00b7fie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201ewohl \u00fcbern See hin\u00fcber.", "tokens": ["\u201e", "wohl", "\u00fc\u00b7bern", "See", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201ekukuk, Kukuk, Kukuk.\u201c", "tokens": ["\u201e", "ku\u00b7kuk", ",", "Ku\u00b7kuk", ",", "Ku\u00b7kuk", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u201each Kukuk, lieber Kukuk mein,", "tokens": ["\u201e", "ach", "Ku\u00b7kuk", ",", "lie\u00b7ber", "Ku\u00b7kuk", "mein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$,", "ADV", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eschmied ich dich an ein Ringelein,", "tokens": ["\u201e", "schmied", "ich", "dich", "an", "ein", "Rin\u00b7ge\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}