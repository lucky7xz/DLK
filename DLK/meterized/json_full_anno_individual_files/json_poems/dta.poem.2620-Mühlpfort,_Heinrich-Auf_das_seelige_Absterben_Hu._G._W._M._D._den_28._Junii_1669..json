{"dta.poem.2620": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf das seelige Absterben  \n  Hu. G. W.   M. D.   den 28. Junii  1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Umsonst gedenckt der Mensch dem Tode zu entfliehen/", "tokens": ["Um\u00b7sonst", "ge\u00b7denckt", "der", "Mensch", "dem", "To\u00b7de", "zu", "ent\u00b7flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergebens st\u00fcrmt sein Witz das Schlo\u00df der Sterb-", "tokens": ["Ver\u00b7ge\u00b7bens", "st\u00fcrmt", "sein", "Witz", "das", "Schlo\u00df", "der", "Ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er mu\u00df das Leichgewand/ wie klug er ist/ anziehen/", "tokens": ["Er", "mu\u00df", "das", "Leich\u00b7ge\u00b7wand", "/", "wie", "klug", "er", "ist", "/", "an\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$(", "PWAV", "ADJD", "PPER", "VAFIN", "$(", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und vor dem letzten Sto\u00df ist kein Galen befreyt.", "tokens": ["Und", "vor", "dem", "letz\u00b7ten", "Sto\u00df", "ist", "kein", "Ga\u00b7len", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es mag der Theophrast sein Perlen-Saltz erheben/", "tokens": ["Es", "mag", "der", "Theo\u00b7ph\u00b7rast", "sein", "Per\u00b7len\u00b7Saltz", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Womit er Sterbenden die Geister wieder bringt/", "tokens": ["Wo\u00b7mit", "er", "Ster\u00b7ben\u00b7den", "die", "Geis\u00b7ter", "wie\u00b7der", "bringt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er mag bald trinckbar Gold bald Hertz-Misturen geben/", "tokens": ["Er", "mag", "bald", "trin\u00b7ck\u00b7bar", "Gold", "bald", "Hertz\u00b7Mi\u00b7stu\u00b7ren", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "NN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bald Milch/ die er durch Kunst aus den Granaten zwingt;", "tokens": ["Bald", "Milch", "/", "die", "er", "durch", "Kunst", "aus", "den", "Gra\u00b7na\u00b7ten", "zwingt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So h\u00e4lt doch nichts zur\u00fcck der Libitinen W\u00fcten/", "tokens": ["So", "h\u00e4lt", "doch", "nichts", "zu\u00b7r\u00fcck", "der", "Li\u00b7bi\u00b7ti\u00b7nen", "W\u00fc\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIS", "PTKVZ", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie edel die Artzney/ wie hoch der Artzt bew\u00e4hrt/", "tokens": ["Wie", "e\u00b7del", "die", "Artz\u00b7ney", "/", "wie", "hoch", "der", "Artzt", "be\u00b7w\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$(", "PWAV", "ADJD", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Parcen haben nur den Faden abgeschnitten/", "tokens": ["Die", "Par\u00b7cen", "ha\u00b7ben", "nur", "den", "Fa\u00b7den", "ab\u00b7ge\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie sehr man widerstrebt/ und alle B\u00fcchsen leert.", "tokens": ["Wie", "sehr", "man", "wi\u00b7der\u00b7strebt", "/", "und", "al\u00b7le", "B\u00fcch\u00b7sen", "leert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "$(", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Lebens Balsam wird zum Abzug nicht mehr dienen/", "tokens": ["Des", "Le\u00b7bens", "Bal\u00b7sam", "wird", "zum", "Ab\u00b7zug", "nicht", "mehr", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "APPRART", "NN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und kein zermalmt Saphir macht uns vom Tode frey/", "tokens": ["Und", "kein", "zer\u00b7malmt", "Sa\u00b7phir", "macht", "uns", "vom", "To\u00b7de", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Noch die Smaragd-Essenz/ noch Oele von Rubinen/", "tokens": ["Noch", "die", "Sma\u00b7ragd\u00b7Es\u00b7senz", "/", "noch", "O\u00b7e\u00b7le", "von", "Ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$(", "ADV", "NN", "APPR", "NE", "$("], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Und so was herrlichers ein Podalir tr\u00e4gt bey.", "tokens": ["Und", "so", "was", "herr\u00b7li\u00b7chers", "ein", "Po\u00b7da\u00b7lir", "tr\u00e4gt", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der abgezogne Geist und Farbe von Corallen/", "tokens": ["Der", "ab\u00b7ge\u00b7zog\u00b7ne", "Geist", "und", "Far\u00b7be", "von", "Co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Womit die kluge Welt sich noch zu retten denckt/", "tokens": ["Wo\u00b7mit", "die", "klu\u00b7ge", "Welt", "sich", "noch", "zu", "ret\u00b7ten", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ist ohne Macht und Krafft/ wenn schon die Geister fallen/", "tokens": ["Ist", "oh\u00b7ne", "Macht", "und", "Krafft", "/", "wenn", "schon", "die", "Geis\u00b7ter", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "$(", "KOUS", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und tieffe Todes Nacht die Augen hat umbschrenckt.", "tokens": ["Und", "tief\u00b7fe", "To\u00b7des", "Nacht", "die", "Au\u00b7gen", "hat", "umbschrenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Da mu\u00df auch Mantua mit seinem Pulver weichen/", "tokens": ["Da", "mu\u00df", "auch", "Man\u00b7tua", "mit", "sei\u00b7nem", "Pul\u00b7ver", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "NE", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Kein g\u00fclden Latwerg st\u00e4rckt das matte Hertze mehr.", "tokens": ["Kein", "g\u00fcl\u00b7den", "Lat\u00b7werg", "st\u00e4rckt", "das", "mat\u00b7te", "Hert\u00b7ze", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der abgezehrte Mensch der soll und mu\u00df verbleichen/", "tokens": ["Der", "ab\u00b7ge\u00b7zehr\u00b7te", "Mensch", "der", "soll", "und", "mu\u00df", "ver\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VMFIN", "KON", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er brauche wie er wil/ und hoffe noch so sehr.", "tokens": ["Er", "brau\u00b7che", "wie", "er", "wil", "/", "und", "hof\u00b7fe", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$(", "KON", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Priester der Natur/ so Erd und See durchkrochen/", "tokens": ["Die", "Pries\u00b7ter", "der", "Na\u00b7tur", "/", "so", "Erd", "und", "See", "durch\u00b7kro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und jedes Element umb einen Rath gefragt/", "tokens": ["Und", "je\u00b7des", "E\u00b7le\u00b7ment", "umb", "ei\u00b7nen", "Rath", "ge\u00b7fragt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die m\u00fcssen ihre M\u00fch beym Hintritt noch verfluchen/", "tokens": ["Die", "m\u00fcs\u00b7sen", "ih\u00b7re", "M\u00fch", "beym", "Hin\u00b7tritt", "noch", "ver\u00b7flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und da\u00df sie sich zu weit/ doch ohne Nutz/ gewagt.", "tokens": ["Und", "da\u00df", "sie", "sich", "zu", "weit", "/", "doch", "oh\u00b7ne", "Nutz", "/", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKA", "ADJD", "$(", "ADV", "APPR", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und es ist wunderns werth/ was menschliches Beginnen", "tokens": ["Und", "es", "ist", "wun\u00b7derns", "werth", "/", "was", "menschli\u00b7ches", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "NN", "ADJD", "$(", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus Steinen hat erprest/ aus Gold und Staal gesaugt/", "tokens": ["Aus", "Stei\u00b7nen", "hat", "er\u00b7prest", "/", "aus", "Gold", "und", "Staal", "ge\u00b7saugt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$(", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Hoffnung doch noch so der Clotho zu entrinnen/", "tokens": ["In", "Hoff\u00b7nung", "doch", "noch", "so", "der", "Clo\u00b7tho", "zu", "ent\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADV", "ART", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und das was auff der Welt noch f\u00fcr das Sterben taugt.", "tokens": ["Und", "das", "was", "auff", "der", "Welt", "noch", "f\u00fcr", "das", "Ster\u00b7ben", "taugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "APPR", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein es hat daraus gantz Sonnen-klar erhellet/", "tokens": ["Al\u00b7lein", "es", "hat", "da\u00b7raus", "gantz", "Son\u00b7nen\u00b7klar", "er\u00b7hel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PAV", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df in jedwedem Glied der Tod uns finden kan.", "tokens": ["Da\u00df", "in", "jed\u00b7we\u00b7dem", "Glied", "der", "Tod", "uns", "fin\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird nicht zuerst das Haupt vom Schlag offt so gef\u00e4llet/", "tokens": ["Wird", "nicht", "zu\u00b7erst", "das", "Haupt", "vom", "Schlag", "offt", "so", "ge\u00b7f\u00e4l\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ART", "NN", "APPRART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df umb den gantzen Mensch im Augenblick gethan?", "tokens": ["Da\u00df", "umb", "den", "gant\u00b7zen", "Mensch", "im", "Au\u00b7gen\u00b7blick", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jtzt qu\u00e4lt das Aug ein Flu\u00df/ bald wirds das Sternsel dr\u00fccken/", "tokens": ["Jtzt", "qu\u00e4lt", "das", "Aug", "ein", "Flu\u00df", "/", "bald", "wirds", "das", "Stern\u00b7sel", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$(", "ADV", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jtzt saust es in dem Ohr/ dann schl\u00e4gt die Taubsucht zu;", "tokens": ["Jtzt", "saust", "es", "in", "dem", "Ohr", "/", "dann", "schl\u00e4gt", "die", "Taub\u00b7sucht", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald wird der Nase-Gang ein b\u00f6\u00df Geschw\u00fcr bestricken/", "tokens": ["Bald", "wird", "der", "Na\u00b7se\u00b7Gang", "ein", "b\u00f6\u00df", "Ge\u00b7schw\u00fcr", "be\u00b7stri\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bald l\u00e4st die F\u00e4ulni\u00df nicht des Mundes Gr\u00e4ntzen Ruh.", "tokens": ["Bald", "l\u00e4st", "die", "F\u00e4ul\u00b7ni\u00df", "nicht", "des", "Mun\u00b7des", "Gr\u00e4nt\u00b7zen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jtzt ist die Zung erstarrt/ itzt 