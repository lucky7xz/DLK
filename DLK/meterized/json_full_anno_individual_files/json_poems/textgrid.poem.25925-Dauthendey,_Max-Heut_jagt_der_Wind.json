{"textgrid.poem.25925": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Heut jagt der Wind", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut jagt der Wind dem Baum durch die M\u00e4hnen,", "tokens": ["Heut", "jagt", "der", "Wind", "dem", "Baum", "durch", "die", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er lacht, da\u00df es hallt, lacht mit gl\u00e4nzenden Z\u00e4hnen,", "tokens": ["Er", "lacht", ",", "da\u00df", "es", "hallt", ",", "lacht", "mit", "gl\u00e4n\u00b7zen\u00b7den", "Z\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bl\u00e4st die Sonn' an, da\u00df ihr das Feuer aufwallt.", "tokens": ["Bl\u00e4st", "die", "Sonn'", "an", ",", "da\u00df", "ihr", "das", "Feu\u00b7er", "auf\u00b7wallt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zwar lahm stehen \u00fcberall welkende Blumen,", "tokens": ["Zwar", "lahm", "ste\u00b7hen", "\u00fc\u00b7be\u00b7rall", "wel\u00b7ken\u00b7de", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Doch wir gehn nicht zahm und sammeln nicht Krumen.", "tokens": ["Doch", "wir", "gehn", "nicht", "zahm", "und", "sam\u00b7meln", "nicht", "Kru\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADJD", "KON", "VVFIN", "PTKNEG", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Sch\u00e4tzlein und ich, wir werden zwei Ross',", "tokens": ["Mein", "Sch\u00e4tz\u00b7lein", "und", "ich", ",", "wir", "wer\u00b7den", "zwei", "Ross'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPER", "$,", "PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit fegendem Fl\u00fcgelpaar schlagen wir los,", "tokens": ["Mit", "fe\u00b7gen\u00b7dem", "Fl\u00fc\u00b7gel\u00b7paar", "schla\u00b7gen", "wir", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Himmels frostblaue Scheiterflammen", "tokens": ["Des", "Him\u00b7mels", "frost\u00b7blau\u00b7e", "Schei\u00b7ter\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schlagen grell mit der Blutfarb zusammen.", "tokens": ["Schla\u00b7gen", "grell", "mit", "der", "Blut\u00b7farb", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wir gehen nicht nur mit den F\u00fc\u00d