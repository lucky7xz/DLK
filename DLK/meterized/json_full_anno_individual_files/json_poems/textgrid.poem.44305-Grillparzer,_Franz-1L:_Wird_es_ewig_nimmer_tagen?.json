{"textgrid.poem.44305": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wird es ewig nimmer tagen?", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wird es ewig nimmer tagen?", "tokens": ["Wird", "es", "e\u00b7wig", "nim\u00b7mer", "ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmert ewig mir kein Licht?", "tokens": ["D\u00e4m\u00b7mert", "e\u00b7wig", "mir", "kein", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand achtet meiner Klagen,", "tokens": ["Nie\u00b7mand", "ach\u00b7tet", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er, der M\u00e4chtge, h\u00f6rt sie nicht.", "tokens": ["Er", ",", "der", "M\u00e4cht\u00b7ge", ",", "h\u00f6rt", "sie", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott! nur einen Tropfen Wahrheit", "tokens": ["Gott", "!", "nur", "ei\u00b7nen", "Trop\u00b7fen", "Wahr\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schenk mir aus dem ewgen Quell,", "tokens": ["Schenk", "mir", "aus", "dem", "ew\u00b7gen", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Geist, erf\u00fcllt mit Klarheit,", "tokens": ["Und", "mein", "Geist", ",", "er\u00b7f\u00fcllt", "mit", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht der Dinge Wesen hell!", "tokens": ["Sieht", "der", "Din\u00b7ge", "We\u00b7sen", "hell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Uns umh\u00fcllt mit dunklem Schleier", "tokens": ["Uns", "um\u00b7h\u00fcllt", "mit", "dunk\u00b7lem", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahn und Trug mit tiefer Nacht,", "tokens": ["Wahn", "und", "Trug", "mit", "tie\u00b7fer", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann erst blickt das Auge freier,", "tokens": ["Dann", "erst", "blickt", "das", "Au\u00b7ge", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es jenseits neu erwacht.", "tokens": ["Wenn", "es", "jen\u00b7seits", "neu", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sende, Vater, wenn die Tr\u00e4ne", "tokens": ["Sen\u00b7de", ",", "Va\u00b7ter", ",", "wenn", "die", "Tr\u00e4\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Kinds dich doch erweicht,", "tokens": ["Dei\u00b7nes", "Kinds", "dich", "doch", "er\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen deiner h\u00f6hern S\u00f6hne,", "tokens": ["Ei\u00b7nen", "dei\u00b7ner", "h\u00f6\u00b7hern", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mir Lebensbalsam reicht,", "tokens": ["Der", "mir", "Le\u00b7bens\u00b7bal\u00b7sam", "reicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Der mich durch das Leben leite,", "tokens": ["Der", "mich", "durch", "das", "Le\u00b7ben", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frei von Irrtum und von Wahn,", "tokens": ["Frei", "von", "Irr\u00b7tum", "und", "von", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich helfend vor mir schreite", "tokens": ["Freund\u00b7lich", "hel\u00b7fend", "vor", "mir", "schrei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Wahrheit Dornenbahn!", "tokens": ["Auf", "der", "Wahr\u00b7heit", "Dor\u00b7nen\u00b7bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also rief von Horebs Spitze", "tokens": ["Al\u00b7so", "rief", "von", "Ho\u00b7rebs", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die weite Wildnis hin", "tokens": ["Durch", "die", "wei\u00b7te", "Wild\u00b7nis", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu des Allgewaltgen Sitze", "tokens": ["Zu", "des", "All\u00b7ge\u00b7walt\u00b7gen", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst der weise Nureddin,", "tokens": ["Einst", "der", "wei\u00b7se", "Nu\u00b7red\u00b7din", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Der in dunkeln Felsenschl\u00fcnden", "tokens": ["Der", "in", "dun\u00b7keln", "Fel\u00b7sen\u00b7schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Fasten und Gebet", "tokens": ["Un\u00b7ter", "Fas\u00b7ten", "und", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Lebte, um das zu ergr\u00fcnden,", "tokens": ["Leb\u00b7te", ",", "um", "das", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PDS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was des Menschen Aug entgeht.", "tokens": ["Was", "des", "Men\u00b7schen", "Aug", "ent\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh! da t\u00f6nen Himmelslieder,", "tokens": ["Sieh", "!", "da", "t\u00f6\u00b7nen", "Him\u00b7mels\u00b7lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Geisteswehen wallt", "tokens": ["Und", "mit", "Geis\u00b7tes\u00b7we\u00b7hen", "wallt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Wolken hehr hernieder", "tokens": ["Aus", "den", "Wol\u00b7ken", "hehr", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine g\u00f6ttliche Gestalt.", "tokens": ["Ei\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gl\u00e4nzend, gleich dem Morgenrote,", "tokens": ["Gl\u00e4n\u00b7zend", ",", "gleich", "dem", "Mor\u00b7gen\u00b7ro\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit freundlichem Gesicht,", "tokens": ["Und", "mit", "freund\u00b7li\u00b7chem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senket sich der Himmelsbote,", "tokens": ["Sen\u00b7ket", "sich", "der", "Him\u00b7mels\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt zu Nureddin und spricht:", "tokens": ["Tritt", "zu", "Nu\u00b7red\u00b7din", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbwisse, da\u00df um euch zu lehren", "tokens": ["\u00bb", "wis\u00b7se", ",", "da\u00df", "um", "euch", "zu", "leh\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "KOUS", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich die weise Allmacht schuf,", "tokens": ["Mich", "die", "wei\u00b7se", "All\u00b7macht", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus h\u00f6hern Himmelssph\u00e4ren", "tokens": ["Und", "aus", "h\u00f6\u00b7hern", "Him\u00b7mels\u00b7sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steig ich nun auf deinen Ruf.\u00ab", "tokens": ["Steig", "ich", "nun", "auf", "dei\u00b7nen", "Ruf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbgott erh\u00f6rte meine Bitte\u00ab,", "tokens": ["\u00bb", "gott", "er\u00b7h\u00f6r\u00b7te", "mei\u00b7ne", "Bit\u00b7te", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rufet Nureddin, \u00bber hei\u00dft", "tokens": ["Ru\u00b7fet", "Nu\u00b7red\u00b7din", ",", "\u00bb", "er", "hei\u00dft"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "NE", "$,", "$(", "PPER", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Aus der Himmelsbr\u00fcder Mitte", "tokens": ["Aus", "der", "Him\u00b7mels\u00b7br\u00fc\u00b7der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niedersteigen einen Geist,", "tokens": ["Nie\u00b7ders\u00b7tei\u00b7gen", "ei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Reich mir denn vom Baum des Lebens", "tokens": ["Reich", "mir", "denn", "vom", "Baum", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so seltne, edle Frucht,", "tokens": ["Die", "so", "selt\u00b7ne", ",", "ed\u00b7le", "Frucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Weise stets vergebens", "tokens": ["Die", "der", "Wei\u00b7se", "stets", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier in diesen T\u00e4lern sucht,", "tokens": ["Hier", "in", "die\u00b7sen", "T\u00e4\u00b7lern", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Schenke mir den Stein der Weisen\u00ab,", "tokens": ["Schen\u00b7ke", "mir", "den", "Stein", "der", "Wei\u00b7sen", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sowie Nureddin dies ruft,", "tokens": ["So\u00b7wie", "Nu\u00b7red\u00b7din", "dies", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Schwingt der Geist in weiten Kreisen", "tokens": ["Schwingt", "der", "Geist", "in", "wei\u00b7ten", "Krei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit Z\u00fcrnen in die Luft.", "tokens": ["Sich", "mit", "Z\u00fcr\u00b7nen", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und aus hohen Wolken schallet", "tokens": ["Und", "aus", "ho\u00b7hen", "Wol\u00b7ken", "schal\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geisterstimme in sein Ohr:", "tokens": ["Geis\u00b7ters\u00b7tim\u00b7me", "in", "sein", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwurm, der dort im Kote wallet\u00ab,", "tokens": ["\u00bb", "wurm", ",", "der", "dort", "im", "Ko\u00b7te", "wal\u00b7let", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufts von oben, \u00bbbl\u00f6der Tor,", "tokens": ["Rufts", "von", "o\u00b7ben", ",", "\u00bb", "bl\u00f6\u00b7der", "Tor", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$,", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Um den Menschen zu begl\u00fccken,", "tokens": ["Um", "den", "Men\u00b7schen", "zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab die Gottheit ihm Verstand,", "tokens": ["Gab", "die", "Got\u00b7theit", "ihm", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch in seines Geistes Blicken"