{"dta.poem.21140": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An das Clavier .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du triumphirende Macht \u00fcber den traurigen Gram,", "tokens": ["Du", "tri\u00b7um\u00b7phi\u00b7ren\u00b7de", "Macht", "\u00fc\u00b7ber", "den", "trau\u00b7ri\u00b7gen", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Du Meisterst\u00fcck der hohen Harmonie,", "tokens": ["Du", "Meis\u00b7ter\u00b7st\u00fcck", "der", "ho\u00b7hen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, mein getreues Clavier, o! singe die Tage hinweg,", "tokens": ["Du", ",", "mein", "ge\u00b7treu\u00b7es", "Cla\u00b7vier", ",", "o", "!", "sin\u00b7ge", "die", "Ta\u00b7ge", "hin\u00b7weg", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "NN", "$,", "FM", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die, N\u00e4chten gleich, mit schwarzen Fl\u00fcgeln fliehn.", "tokens": ["Die", ",", "N\u00e4ch\u00b7ten", "gleich", ",", "mit", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", "fliehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sonst rauscht\u2019 ein fr\u00f6hlicher Ton, wie er in Opern ent-", "tokens": ["Sonst", "rauscht'", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Ton", ",", "wie", "er", "in", "O\u00b7pern", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Saiten durch, und jauchzte Symphonien;", "tokens": ["Die", "Sai\u00b7ten", "durch", ",", "und", "jauchz\u00b7te", "Sym\u00b7pho\u00b7ni\u00b7en", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch klang ein gaukelnder Tanz, von pantomimischen", "tokens": ["Auch", "klang", "ein", "gau\u00b7keln\u00b7der", "Tanz", ",", "von", "pan\u00b7to\u00b7mi\u00b7mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dem schwarzen Gott der H\u00f6lle vorgetanzt.", "tokens": ["Dem", "schwar\u00b7zen", "Gott", "der", "H\u00f6l\u00b7le", "vor\u00b7ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sonst sang ein lachender Lied siegender Augen Triumph,", "tokens": ["Sonst", "sang", "ein", "la\u00b7chen\u00b7der", "Lied", "sie\u00b7gen\u00b7der", "Au\u00b7gen", "Tri\u00b7umph", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die himmelblau, als wie im Lenz die Luft,", "tokens": ["Die", "him\u00b7mel\u00b7blau", ",", "als", "wie", "im", "Lenz", "die", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KOUS", "KOKOM", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In mein empfindendes Herz die sanfte Liebe gefl\u00f6\u00dft,", "tokens": ["In", "mein", "emp\u00b7fin\u00b7den\u00b7des", "Herz", "die", "sanf\u00b7te", "Lie\u00b7be", "ge\u00b7fl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "F\u00fcr die allein mein Herz geschaffen war.", "tokens": ["F\u00fcr", "die", "al\u00b7lein", "mein", "Herz", "ge\u00b7schaf\u00b7fen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ietzt, verla\u00dfnes Clavier, schweiget das schmei-", "tokens": ["Doch", "ietzt", ",", "ver\u00b7la\u00df\u00b7nes", "Cla\u00b7vier", ",", "schwei\u00b7get", "das", "schmei"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Hagedorn der Freud und Jugend spielt.", "tokens": ["Das", "Ha\u00b7ge\u00b7dorn", "der", "Freud", "und", "Ju\u00b7gend", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Dissonanzen geh\u00fcllt, schaf ich mir einsam den Ton,", "tokens": ["In", "Dis\u00b7so\u00b7nan\u00b7zen", "ge\u00b7h\u00fcllt", ",", "schaf", "ich", "mir", "ein\u00b7sam", "den", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der meinen Schmerz in finstern Noten sagt.", "tokens": ["Der", "mei\u00b7nen", "Schmerz", "in", "fins\u00b7tern", "No\u00b7ten", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn der erheiternde Stral besserer Hofnung mir lacht,", "tokens": ["Wenn", "der", "er\u00b7hei\u00b7tern\u00b7de", "Stral", "bes\u00b7se\u00b7rer", "Hof\u00b7nung", "mir", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nicht mein Flehn der leichte Wind verweht;", "tokens": ["Und", "nicht", "mein", "Flehn", "der", "leich\u00b7te", "Wind", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann soll ein scherzendes Lied, dir, o Seline, geweiht,", "tokens": ["Dann", "soll", "ein", "scher\u00b7zen\u00b7des", "Lied", ",", "dir", ",", "o", "Se\u00b7li\u00b7ne", ",", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "$,", "PPER", "$,", "FM", "NE", "$,", "VVPP", "$,"], "meter": "-+-+--+--+---+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch deine Macht den Liebesgott erh\u00f6hn.", "tokens": ["Durch", "dei\u00b7ne", "Macht", "den", "Lie\u00b7bes\u00b7gott", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}