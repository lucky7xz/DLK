{"textgrid.poem.66385": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Bauer im Winter", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich leb' das ganze Jahr vergn\u00fcgt!", "tokens": ["Ich", "leb'", "das", "gan\u00b7ze", "Jahr", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fr\u00fchling wird das Feld gepfl\u00fcgt;", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wird", "das", "Feld", "ge\u00b7pfl\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da h\u00e4ngt die Lerche \u00fcber mir,", "tokens": ["Da", "h\u00e4ngt", "die", "Ler\u00b7che", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singt ihr krauses Lied mir f\u00fcr.", "tokens": ["Und", "singt", "ihr", "krau\u00b7ses", "Lied", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und kommt die liebe Sommerszeit,", "tokens": ["Und", "kommt", "die", "lie\u00b7be", "Som\u00b7mers\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hoch wird da mein Herz erfreut,", "tokens": ["Wie", "hoch", "wird", "da", "mein", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich vor meinem Acker steh,", "tokens": ["Wenn", "ich", "vor", "mei\u00b7nem", "A\u00b7cker", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so viel tausend Aehren seh!", "tokens": ["Und", "so", "viel", "tau\u00b7send", "A\u00b7eh\u00b7ren", "seh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Alsbald die Sicheln dengle ich,", "tokens": ["Als\u00b7bald", "die", "Si\u00b7cheln", "deng\u00b7le", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Grille Lied erg\u00f6tzet mich;", "tokens": ["Der", "Gril\u00b7le", "Lied", "er\u00b7g\u00f6t\u00b7zet", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann fahr' ich in das Feld hinaus,", "tokens": ["Dann", "fahr'", "ich", "in", "das", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schneid' meine Frucht und f\u00fchr's nach Haus.", "tokens": ["Schneid'", "mei\u00b7ne", "Frucht", "und", "f\u00fchr's", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Im Herbst seh ich die B\u00e4ume an,", "tokens": ["Im", "Herbst", "seh", "ich", "die", "B\u00e4u\u00b7me", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau' Aepfel, Birn und Zwetschgen dran;", "tokens": ["Schau'", "A\u00b7e\u00b7pfel", ",", "Birn", "und", "Zwetschgen", "dran", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sind sie reif, so sch\u00fcttl' ich sie.", "tokens": ["Und", "sind", "sie", "reif", ",", "so", "sch\u00fcttl'", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lohnet Gott des Bauern M\u00fch'.", "tokens": ["So", "loh\u00b7net", "Gott", "des", "Bau\u00b7ern", "M\u00fch'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt ist die kalte Winterszeit,", "tokens": ["Jetzt", "ist", "die", "kal\u00b7te", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schindeldach ist \u00fcberschneit;", "tokens": ["Mein", "Schin\u00b7del\u00b7dach", "ist", "\u00fc\u00b7bersc\u00b7hneit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ganze Feld ist kreidewei\u00df,", "tokens": ["Das", "gan\u00b7ze", "Feld", "ist", "krei\u00b7de\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Weiher ist bedeckt mit Eis.", "tokens": ["Mein", "Wei\u00b7her", "ist", "be\u00b7deckt", "mit", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich aber bleib' bei hellem Muth,", "tokens": ["Ich", "a\u00b7ber", "bleib'", "bei", "hel\u00b7lem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Pfeifle Taback schmeckt mir gut.", "tokens": ["Mein", "Pfeif\u00b7le", "Ta\u00b7back", "schmeckt", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von mir wird mancher Span geschnitzt,", "tokens": ["Von", "mir", "wird", "man\u00b7cher", "Span", "ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn 's Weible bei der Kunkel sitzt.", "tokens": ["Wenn", "'s", "Weib\u00b7le", "bei", "der", "Kun\u00b7kel", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Die Kinder h\u00fcpfen um mich 'rum", "tokens": ["Die", "Kin\u00b7der", "h\u00fcp\u00b7fen", "um", "mich", "'rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singen heisa dudeldum!", "tokens": ["Und", "sin\u00b7gen", "hei\u00b7sa", "du\u00b7del\u00b7dum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein' Urschel und mein kleiner Hans,", "tokens": ["Mein'", "Ur\u00b7schel", "und", "mein", "klei\u00b7ner", "Hans", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NE", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Die drehen sich im Schleifertanz.", "tokens": ["Die", "dre\u00b7hen", "sich", "im", "Schlei\u00b7fer\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und kommt der liebe Sonntag 'ran,", "tokens": ["Und", "kommt", "der", "lie\u00b7be", "Sonn\u00b7tag", "'ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zieh ich mein Scharlachwammes an,", "tokens": ["Zieh", "ich", "mein", "Schar\u00b7lach\u00b7wam\u00b7mes", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh' in die Kirch' in guter Ruh'", "tokens": ["Geh'", "in", "die", "Kirch'", "in", "gu\u00b7ter", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und h\u00f6r' des Pfarrers Predigt zu.", "tokens": ["Und", "h\u00f6r'", "des", "Pfar\u00b7rers", "Pre\u00b7digt", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und komm' ich heim, so wird verzehrt,", "tokens": ["Und", "komm'", "ich", "heim", ",", "so", "wird", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir der liebe Gott bescheert;", "tokens": ["Was", "mir", "der", "lie\u00b7be", "Gott", "be\u00b7scheert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nach dem Essen les' ich dann", "tokens": ["Und", "nach", "dem", "Es\u00b7sen", "les'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Krankentrost und Habermann.", "tokens": ["Im", "Kran\u00b7kent\u00b7rost", "und", "Ha\u00b7ber\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und bricht die Abendzeit herein,", "tokens": ["Und", "bricht", "die", "A\u00b7bend\u00b7zeit", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So trink' ich halt mein Sch\u00f6pple Wein;", "tokens": ["So", "trink'", "ich", "halt", "mein", "Sch\u00f6pp\u00b7le", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da liest der Herr Schulmeister mir", "tokens": ["Da", "liest", "der", "Herr", "Schul\u00b7meis\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Neues aus der Zeitung f\u00fcr.", "tokens": ["Was", "Neu\u00b7es", "aus", "der", "Zei\u00b7tung", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dann geh ich heim im K\u00f6pfle warm", "tokens": ["Dann", "geh", "ich", "heim", "im", "K\u00f6pf\u00b7le", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und nimm mein liebes Weib in Arm;", "tokens": ["Und", "nimm", "mein", "lie\u00b7bes", "Weib", "in", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Leg' mich ins Bett und schlaf froh ein,", "tokens": ["Leg'", "mich", "ins", "Bett", "und", "schlaf", "froh", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kann wohl ein Mensch vergn\u00fcgter sein?", "tokens": ["Kann", "wohl", "ein", "Mensch", "ver\u00b7gn\u00fcg\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich leb' das ganze Jahr vergn\u00fcgt!", "tokens": ["Ich", "leb'", "das", "gan\u00b7ze", "Jahr", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fr\u00fchling wird das Feld gepfl\u00fcgt;", "tokens": ["Im", "Fr\u00fch\u00b7ling", "wird", "das", "Feld", "ge\u00b7pfl\u00fcgt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da h\u00e4ngt die Lerche \u00fcber mir,", "tokens": ["Da", "h\u00e4ngt", "die", "Ler\u00b7che", "\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singt ihr krauses Lied mir f\u00fcr.", "tokens": ["Und", "singt", "ihr", "krau\u00b7ses", "Lied", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und kommt die liebe Sommerszeit,", "tokens": ["Und", "kommt", "die", "lie\u00b7be", "Som\u00b7mers\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie hoch wird da mein Herz erfreut,", "tokens": ["Wie", "hoch", "wird", "da", "mein", "Herz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich vor meinem Acker steh,", "tokens": ["Wenn", "ich", "vor", "mei\u00b7nem", "A\u00b7cker", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und so viel tausend Aehren seh!", "tokens": ["Und", "so", "viel", "tau\u00b7send", "A\u00b7eh\u00b7ren", "seh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.14": {"line.1": {"text": "Alsbald die Sicheln dengle ich,", "tokens": ["Als\u00b7bald", "die", "Si\u00b7cheln", "deng\u00b7le", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Grille Lied erg\u00f6tzet mich;", "tokens": ["Der", "Gril\u00b7le", "Lied", "er\u00b7g\u00f6t\u00b7zet", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann fahr' ich in das Feld hinaus,", "tokens": ["Dann", "fahr'", "ich", "in", "das", "Feld", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schneid' meine Frucht und f\u00fchr's nach Haus.", "tokens": ["Schneid'", "mei\u00b7ne", "Frucht", "und", "f\u00fchr's", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Im Herbst seh ich die B\u00e4ume an,", "tokens": ["Im", "Herbst", "seh", "ich", "die", "B\u00e4u\u00b7me", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schau' Aepfel, Birn und Zwetschgen dran;", "tokens": ["Schau'", "A\u00b7e\u00b7pfel", ",", "Birn", "und", "Zwetschgen", "dran", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sind sie reif, so sch\u00fcttl' ich sie.", "tokens": ["Und", "sind", "sie", "reif", ",", "so", "sch\u00fcttl'", "ich", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lohnet Gott des Bauern M\u00fch'.", "tokens": ["So", "loh\u00b7net", "Gott", "des", "Bau\u00b7ern", "M\u00fch'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Jetzt ist die kalte Winterszeit,", "tokens": ["Jetzt", "ist", "die", "kal\u00b7te", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Schindeldach ist \u00fcberschneit;", "tokens": ["Mein", "Schin\u00b7del\u00b7dach", "ist", "\u00fc\u00b7bersc\u00b7hneit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ganze Feld ist kreidewei\u00df,", "tokens": ["Das", "gan\u00b7ze", "Feld", "ist", "krei\u00b7de\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Weiher ist bedeckt mit Eis.", "tokens": ["Mein", "Wei\u00b7her", "ist", "be\u00b7deckt", "mit", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Ich aber bleib' bei hellem Muth,", "tokens": ["Ich", "a\u00b7ber", "bleib'", "bei", "hel\u00b7lem", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Pfeifle Taback schmeckt mir gut.", "tokens": ["Mein", "Pfeif\u00b7le", "Ta\u00b7back", "schmeckt", "mir", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADJD", "$."], "me