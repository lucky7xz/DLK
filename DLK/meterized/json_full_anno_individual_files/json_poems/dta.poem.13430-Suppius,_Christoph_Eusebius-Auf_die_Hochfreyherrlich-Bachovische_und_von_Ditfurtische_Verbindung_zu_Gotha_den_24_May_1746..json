{"dta.poem.13430": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Auf die  \n Hochfreyherrlich-Bachovische  \n und  \n von Ditfurtische  \n  Verbindung zu Gotha  \n den 24 May 1746.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Ich soll der Liebe Macht besingen;", "tokens": ["Ich", "soll", "der", "Lie\u00b7be", "Macht", "be\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Himmel! welch ein neues Lied!", "tokens": ["O", "Him\u00b7mel", "!", "welch", "ein", "neu\u00b7es", "Lied", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fccken mu\u00df es, reizend klingen,", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "mu\u00df", "es", ",", "rei\u00b7zend", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So, da\u00df es Herzen an sich zieht;", "tokens": ["So", ",", "da\u00df", "es", "Her\u00b7zen", "an", "sich", "zieht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich Aermster! der von Menschen ferne,", "tokens": ["Ich", "A\u00b7erms\u00b7ter", "!", "der", "von", "Men\u00b7schen", "fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "ART", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mich selbst noch erst versuchen lerne", "tokens": ["Mich", "selbst", "noch", "erst", "ver\u00b7su\u00b7chen", "ler\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADV", "VVINF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit meiner Feldschallmeye Ton;", "tokens": ["Mit", "mei\u00b7ner", "Feld\u00b7schall\u00b7me\u00b7ye", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer kann, den Vorsatz zu erzielen,", "tokens": ["Wer", "kann", ",", "den", "Vor\u00b7satz", "zu", "er\u00b7zie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wohl z\u00e4rtlicher als Sappho spielen,", "tokens": ["Wohl", "z\u00e4rt\u00b7li\u00b7cher", "als", "Sap\u00b7pho", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wer sch\u00f6ner als Anakreon?", "tokens": ["Wer", "sch\u00f6\u00b7ner", "als", "A\u00b7nak\u00b7re\u00b7on", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du G\u00f6ttin bist es! Wundersch\u00f6ne!", "tokens": ["Du", "G\u00f6t\u00b7tin", "bist", "es", "!", "Wun\u00b7der\u00b7sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du G\u00f6ttin von der kleinen Welt,", "tokens": ["Du", "G\u00f6t\u00b7tin", "von", "der", "klei\u00b7nen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die heute meine schlechten T\u00f6ne", "tokens": ["Die", "heu\u00b7te", "mei\u00b7ne", "schlech\u00b7ten", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch holde Reizung unterh\u00e4lt;", "tokens": ["Durch", "hol\u00b7de", "Rei\u00b7zung", "un\u00b7ter\u00b7h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bezwingerin ger\u00fchrter Herzen,", "tokens": ["Be\u00b7zwin\u00b7ge\u00b7rin", "ge\u00b7r\u00fchr\u00b7ter", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier brennen schon die Hochzeitkerzen,", "tokens": ["Hier", "bren\u00b7nen", "schon", "die", "Hoch\u00b7zeit\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auf! eile! komm! erscheine mir!", "tokens": ["Auf", "!", "ei\u00b7le", "!", "komm", "!", "er\u00b7schei\u00b7ne", "mir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "$.", "VVFIN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df dich von deinem Himmel nieder,", "tokens": ["La\u00df", "dich", "von", "dei\u00b7nem", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sey itzt der Inhalt meiner Lieder,", "tokens": ["Sey", "itzt", "der", "In\u00b7halt", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Ehrfurcht spielt! sie singt von dir!", "tokens": ["Die", "Ehr\u00b7furcht", "spielt", "!", "sie", "singt", "von", "dir", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Welch majest\u00e4tisch Angesichte,", "tokens": ["Welch", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll l\u00e4chelnder Zufriedenheit!", "tokens": ["Voll", "l\u00e4\u00b7cheln\u00b7der", "Zu\u00b7frie\u00b7den\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Auge blitzt in starkem Lichte", "tokens": ["Das", "Au\u00b7ge", "blitzt", "in", "star\u00b7kem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller seiner Herrlichkeit,", "tokens": ["Mit", "al\u00b7ler", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O! welcher Menschheit wird es gl\u00fccken,", "tokens": ["O", "!", "wel\u00b7cher", "Menschheit", "wird", "es", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Wesen v\u00f6llig auszudr\u00fccken,", "tokens": ["Das", "We\u00b7sen", "v\u00f6l\u00b7lig", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sie mit sich hernieder bracht,", "tokens": ["So", "sie", "mit", "sich", "her\u00b7nie\u00b7der", "bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie wird mir! es dreht mit den Dingen", "tokens": ["Wie", "wird", "mir", "!", "es", "dreht", "mit", "den", "Din\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Sich alles um, die Ohren klingen!", "tokens": ["Sich", "al\u00b7les", "um", ",", "die", "Oh\u00b7ren", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vor meinen Augen wird es Nacht!", "tokens": ["Vor", "mei\u00b7nen", "Au\u00b7gen", "wird", "es", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Liebe! Liebe! ja! von ferne", "tokens": ["O", "Lie\u00b7be", "!", "Lie\u00b7be", "!", "ja", "!", "von", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "NN", "$.", "PTKANT", "$.", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt sich bereits, wie stark du bist!", "tokens": ["Zeigt", "sich", "be\u00b7reits", ",", "wie", "stark", "du", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein schwacher Stand vergisst sich gerne,", "tokens": ["Mein", "schwa\u00b7cher", "Stand", "ver\u00b7gisst", "sich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich nach deiner Hoheit misst;", "tokens": ["Der", "sich", "nach", "dei\u00b7ner", "Ho\u00b7heit", "misst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Einflu\u00df kann den Geist erf\u00fcllen,", "tokens": ["Dein", "Ein\u00b7flu\u00df", "kann", "den", "Geist", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du triumphirest in dem Willen,", "tokens": ["Du", "tri\u00b7um\u00b7phi\u00b7rest", "in", "dem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Beherrschest L\u00e4nder, ja die See,", "tokens": ["Be\u00b7herr\u00b7schest", "L\u00e4n\u00b7der", ",", "ja", "die", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Orpheus steigt auf dein Bewegen,", "tokens": ["Und", "Or\u00b7pheus", "steigt", "auf", "dein", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Er holt auf surchterf\u00fcllten Stegen,", "tokens": ["Er", "holt", "auf", "surch\u00b7ter\u00b7f\u00fcll\u00b7ten", "Ste\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wen sonst, als dich, Euridice?", "tokens": ["Wen", "sonst", ",", "als", "dich", ",", "Eu\u00b7ri\u00b7di\u00b7ce", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "$,", "KOUS", "PPER", "$,", "NE", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Sie spielt mit angenehmen Flammen,", "tokens": ["Sie", "spielt", "mit", "an\u00b7ge\u00b7neh\u00b7men", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fchrt mit ihrer Mutterhand", "tokens": ["Und", "f\u00fchrt", "mit", "ih\u00b7rer", "Mut\u00b7ter\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Personen oftermahls zusammen,", "tokens": ["Per\u00b7so\u00b7nen", "of\u00b7ter\u00b7mahls", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kaum einander noch gekannt.", "tokens": ["Die", "kaum", "ein\u00b7an\u00b7der", "noch", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wei\u00df sie solche zu vereinen,", "tokens": ["Da", "wei\u00df", "sie", "sol\u00b7che", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie nur eine Seele scheinen,", "tokens": ["Da\u00df", "sie", "nur", "ei\u00b7ne", "See\u00b7le", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sich nur ihrentwegen liebt,", "tokens": ["Die", "sich", "nur", "ih\u00b7rent\u00b7we\u00b7gen", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und, deren H\u00e4lfte frey zu machen,", "tokens": ["Und", ",", "de\u00b7ren", "H\u00e4lf\u00b7te", "frey", "zu", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Tygranes mit gelassnem Lachen", "tokens": ["Ty\u00b7gra\u00b7nes", "mit", "ge\u00b7lass\u00b7nem", "La\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Gar gerne tausend Leben giebt.", "tokens": ["Gar", "ger\u00b7ne", "tau\u00b7send", "Le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie unbegreiflich ist ihr Wesen!", "tokens": ["Wie", "un\u00b7be\u00b7greif\u00b7lich", "ist", "ihr", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie unumschr\u00e4nkt ihr Machtgebot,", "tokens": ["Wie", "un\u00b7um\u00b7schr\u00e4nkt", "ihr", "Macht\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie sich einen Thron erlesen,", "tokens": ["Wo", "sie", "sich", "ei\u00b7nen", "Thron", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist sie st\u00e4rker als der Tod,", "tokens": ["Da", "ist", "sie", "st\u00e4r\u00b7ker", "als", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-