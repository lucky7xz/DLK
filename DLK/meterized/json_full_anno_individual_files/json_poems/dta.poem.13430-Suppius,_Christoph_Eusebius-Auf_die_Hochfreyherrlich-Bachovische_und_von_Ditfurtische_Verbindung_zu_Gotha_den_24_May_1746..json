": "iambic.tetra"}, "line.7": {"text": "Da war ein Meisterst\u00fcck beschert,", "tokens": ["Da", "war", "ein", "Meis\u00b7ter\u00b7st\u00fcck", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie bildete die sanften Triebe,", "tokens": ["Sie", "bil\u00b7de\u00b7te", "die", "sanf\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die\u00df sch\u00f6ne Herz; ist solche Liebe", "tokens": ["Die\u00df", "sch\u00f6\u00b7ne", "Herz", ";", "ist", "sol\u00b7che", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$.", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wohl anderer Belohnung werth?", "tokens": ["Wohl", "an\u00b7de\u00b7rer", "Be\u00b7loh\u00b7nung", "werth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "F\u00fcr Sie nun w\u00fcnscht Sie nur zu leben,", "tokens": ["F\u00fcr", "Sie", "nun", "w\u00fcnscht", "Sie", "nur", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie, Dero Freundin, Dero Braut,", "tokens": ["Sie", ",", "De\u00b7ro", "Freun\u00b7din", ",", "De\u00b7ro", "Braut", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wort ist \u00f6ffentlich gegeben,", "tokens": ["Das", "Wort", "ist", "\u00f6f\u00b7fent\u00b7lich", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Jhnen hat Sie Sich vertraut;", "tokens": ["Und", "Jh\u00b7nen", "hat", "Sie", "Sich", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00f6rt! wie die Liebe triumphiret!", "tokens": ["H\u00f6rt", "!", "wie", "die", "Lie\u00b7be", "tri\u00b7um\u00b7phi\u00b7ret", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was sagt sie? diese Braut verlieret,", "tokens": ["Was", "sagt", "sie", "?", "die\u00b7se", "Braut", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Verl\u00e4sset Freunde, Vaterland,", "tokens": ["Ver\u00b7l\u00e4s\u00b7set", "Freun\u00b7de", ",", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drauf lie\u00df Sie sich in Gotha nieder,", "tokens": ["Drauf", "lie\u00df", "Sie", "sich", "in", "Go\u00b7tha", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo Sie in kurzem alles wieder", "tokens": ["Wo", "Sie", "in", "kur\u00b7zem", "al\u00b7les", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "PIS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In ihrem werthen Bachov fand.", "tokens": ["In", "ih\u00b7rem", "wert\u00b7hen", "Ba\u00b7chov", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Gn\u00e4dige! durch solch Verm\u00e4hlen", "tokens": ["O", "Gn\u00e4\u00b7di\u00b7ge", "!", "durch", "solch", "Ver\u00b7m\u00e4h\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00df lauter herrliches entstehn", "tokens": ["Mu\u00df", "lau\u00b7ter", "herr\u00b7li\u00b7ches", "ent\u00b7stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "ADJA", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soviel, da\u00df man es nicht kann z\u00e4hlen,", "tokens": ["So\u00b7viel", ",", "da\u00df", "man", "es", "nicht", "kann", "z\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warum? der Ursprung ist zu sch\u00f6n!", "tokens": ["Wa\u00b7rum", "?", "der", "Ur\u00b7sprung", "ist", "zu", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gl\u00fcckseeligkeit! gie\u00df deine Segen,", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig\u00b7keit", "!", "gie\u00df", "dei\u00b7ne", "Se\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie einen warmen Abendregen,", "tokens": ["Wie", "ei\u00b7nen", "war\u00b7men", "A\u00b7bend\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Best\u00e4ndig \u00fcber Jhnen aus!", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "\u00fc\u00b7ber", "Jh\u00b7nen", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Welch Gl\u00fcck von meinen Lebenstagen!", "tokens": ["Welch", "Gl\u00fcck", "von", "mei\u00b7nen", "Le\u00b7bens\u00b7ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn ich darf ganz begeistert sagen:", "tokens": ["Wenn", "ich", "darf", "ganz", "be\u00b7geis\u00b7tert", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es gr\u00fcnet Bachovs hohes Haus.", "tokens": ["Es", "gr\u00fc\u00b7net", "Ba\u00b7chovs", "ho\u00b7hes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}