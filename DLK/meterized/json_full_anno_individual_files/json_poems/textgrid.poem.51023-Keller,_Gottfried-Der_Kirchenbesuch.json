{"textgrid.poem.51023": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Kirchenbesuch", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Fischlein in dem Garn", "tokens": ["Wie", "ein", "Fisc\u00b7hlein", "in", "dem", "Garn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Dom mich eingefangen,", "tokens": ["Hat", "der", "Dom", "mich", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da bin ich festgebannt \u2013", "tokens": ["Und", "da", "bin", "ich", "fest\u00b7ge\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum bin ich dreingegangen?", "tokens": ["Wa\u00b7rum", "bin", "ich", "drein\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, wie unter breiten Malven", "tokens": ["Ach", ",", "wie", "un\u00b7ter", "brei\u00b7ten", "Mal\u00b7ven"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Taubesprengt ein R\u00f6slein blitzt,", "tokens": ["Tau\u00b7be\u00b7sprengt", "ein", "R\u00f6s\u00b7lein", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zwischen guten B\u00fcrgerfrauen", "tokens": ["Zwi\u00b7schen", "gu\u00b7ten", "B\u00fcr\u00b7ger\u00b7frau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier mein feines Liebchen sitzt!", "tokens": ["Hier", "mein", "fei\u00b7nes", "Lieb\u00b7chen", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gemeinde schnarcht so sanft,", "tokens": ["Die", "Ge\u00b7mein\u00b7de", "schnarcht", "so", "sanft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Laub im Walde rauschet,", "tokens": ["Wie", "das", "Laub", "im", "Wal\u00b7de", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Bettler an der T\u00fcr", "tokens": ["Und", "der", "Bett\u00b7ler", "an", "der", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein R\u00e4uber guckt und lauschet;", "tokens": ["Als", "ein", "R\u00e4u\u00b7ber", "guckt", "und", "lau\u00b7schet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wie eines B\u00e4chleins Faden", "tokens": ["Doch", "wie", "ei\u00b7nes", "B\u00e4ch\u00b7leins", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Murmelnd durchs Geb\u00fcsche flie\u00dft,", "tokens": ["Mur\u00b7melnd", "durchs", "Ge\u00b7b\u00fc\u00b7sche", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So die lange d\u00fcnne Predigt", "tokens": ["So", "die", "lan\u00b7ge", "d\u00fcn\u00b7ne", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um die Pfeiler sich ergie\u00dft.", "tokens": ["Um", "die", "Pfei\u00b7ler", "sich", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eichenb\u00e4ume, hoch und schlank,", "tokens": ["Ei\u00b7chen\u00b7b\u00e4u\u00b7me", ",", "hoch", "und", "schlank", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All die gotischen Pfeiler ragen;", "tokens": ["All", "die", "go\u00b7ti\u00b7schen", "Pfei\u00b7ler", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein gew\u00f6lbtes Bl\u00e4tterdach", "tokens": ["Ein", "ge\u00b7w\u00f6lb\u00b7tes", "Bl\u00e4t\u00b7ter\u00b7dach"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre krausen \u00c4ste tragen;", "tokens": ["Ih\u00b7re", "krau\u00b7sen", "\u00c4s\u00b7te", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Untenher spielt hin und wieder", "tokens": ["Un\u00b7ten\u00b7her", "spielt", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "D\u00e4mmerhaft ein Sonnenschein;", "tokens": ["D\u00e4m\u00b7mer\u00b7haft", "ein", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wachend sind in dieser Stille", "tokens": ["Wa\u00b7chend", "sind", "in", "die\u00b7ser", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur mein Lieb und ich allein.", "tokens": ["Nur", "mein", "Lieb", "und", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwischen uns webt sich ein Netz", "tokens": ["Zwi\u00b7schen", "uns", "webt", "sich", "ein", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Von des Lichts gebrochnem Strahle,", "tokens": ["Von", "des", "Lichts", "ge\u00b7broch\u00b7nem", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin der Taufstein, gr\u00fcn und rot,", "tokens": ["Drin", "der", "Tauf\u00b7stein", ",", "gr\u00fcn", "und", "rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt sich zur Blumenschale;", "tokens": ["Wan\u00b7delt", "sich", "zur", "Blu\u00b7men\u00b7scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein gefl\u00fcgelt Kn\u00e4blein flattert", "tokens": ["Ein", "ge\u00b7fl\u00fc\u00b7gelt", "Kn\u00e4\u00b7blein", "flat\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Deckels altem Knauf,", "tokens": ["Auf", "des", "De\u00b7ckels", "al\u00b7tem", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es gehen uns im Busen", "tokens": ["Und", "es", "ge\u00b7hen", "uns", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch der Sehnsucht Rosen auf.", "tokens": ["Auch", "der", "Sehn\u00b7sucht", "Ro\u00b7sen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weit hinaus, ins Morgenland,", "tokens": ["Weit", "hin\u00b7aus", ",", "ins", "Mor\u00b7gen\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm, mein Kind, und la\u00df uns fliegen,", "tokens": ["Komm", ",", "mein", "Kind", ",", "und", "la\u00df", "uns", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Palmen schwanken am Meer", "tokens": ["Wo", "die", "Pal\u00b7men", "schwan\u00b7ken", "am", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und die sel'gen Inseln liegen,", "tokens": ["Und", "die", "sel'\u00b7gen", "In\u00b7seln", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flutend um die gro\u00dfe Sonne", "tokens": ["Flu\u00b7tend", "um", "die", "gro\u00b7\u00dfe", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Grundlos tief die Himmel blaun:", "tokens": ["Grund\u00b7los", "tief", "die", "Him\u00b7mel", "blaun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Angesichts der freien Wogen", "tokens": ["An\u00b7ge\u00b7sichts", "der", "frei\u00b7en", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unsre Seelen frei zu traun!", "tokens": ["Uns\u00b7re", "See\u00b7len", "frei", "zu", "traun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie ein Fischlein in dem Garn", "tokens": ["Wie", "ein", "Fisc\u00b7hlein", "in", "dem", "Garn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Dom mich eingefangen,", "tokens": ["Hat", "der", "Dom", "mich", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da bin ich festgebannt \u2013", "tokens": ["Und", "da", "bin", "ich", "fest\u00b7ge\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum bin ich dreingegangen?", "tokens": ["Wa\u00b7rum", "bin", "ich", "drein\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, wie unter breiten Malven", "tokens": ["Ach", ",", "wie", "un\u00b7ter", "brei\u00b7ten", "Mal\u00b7ven"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Taubesprengt ein R\u00f6slein blitzt,", "tokens": ["Tau\u00b7be\u00b7sprengt", "ein", "R\u00f6s\u00b7lein", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measur