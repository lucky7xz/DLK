{"textgrid.poem.35613": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So leise sind die gr\u00fcnen W\u00e4lder", "tokens": ["So", "lei\u00b7se", "sind", "die", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsrer Heimat,", "tokens": ["Uns\u00b7rer", "Hei\u00b7mat", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die kristallne Woge", "tokens": ["Die", "kris\u00b7tall\u00b7ne", "Wo\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hinsterbend an verfallner Mauer", "tokens": ["Hins\u00b7ter\u00b7bend", "an", "ver\u00b7fall\u00b7ner", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und wir haben im Schlaf geweint;", "tokens": ["Und", "wir", "ha\u00b7ben", "im", "Schlaf", "ge\u00b7weint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wandern mit z\u00f6gernden Schritten", "tokens": ["Wan\u00b7dern", "mit", "z\u00f6\u00b7gern\u00b7den", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "An der dornigen Hecke hin", "tokens": ["An", "der", "dor\u00b7ni\u00b7gen", "He\u00b7cke", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Singende im Abendsommer,", "tokens": ["Sin\u00b7gen\u00b7de", "im", "A\u00b7bend\u00b7som\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.9": {"text": "In heiliger Ruh", "tokens": ["In", "hei\u00b7li\u00b7ger", "Ruh"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Des fern verstrahlenden Weinbergs;", "tokens": ["Des", "fern", "ver\u00b7strah\u00b7len\u00b7den", "Wein\u00b7bergs", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "Schatten nun im k\u00fchlen Scho\u00df", "tokens": ["Schat\u00b7ten", "nun", "im", "k\u00fch\u00b7len", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Der Nacht, trauernde Adler.", "tokens": ["Der", "Nacht", ",", "trau\u00b7ern\u00b7de", "Ad\u00b7ler", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "So leise schlie\u00dft ein mondener Strahl", "tokens": ["So", "lei\u00b7se", "schlie\u00dft", "ein", "mon\u00b7de\u00b7ner", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Die purpurnen Male der Schwermut.", "tokens": ["Die", "pur\u00b7pur\u00b7nen", "Ma\u00b7le", "der", "Schwer\