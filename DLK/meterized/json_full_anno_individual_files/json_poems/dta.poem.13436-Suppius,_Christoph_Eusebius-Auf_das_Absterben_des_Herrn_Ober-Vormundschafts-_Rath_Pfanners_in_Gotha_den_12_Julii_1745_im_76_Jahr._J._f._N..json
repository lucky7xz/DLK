{"dta.poem.13436": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Absterben  \n  des Herrn Ober-Vormundschafts-  \n Rath  \n  Pfanners  \n in Gotha den 12 Julii 1745 im 76 Jahr.  \n J. f. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir uns alt und grau gewacht,", "tokens": ["Wenn", "wir", "uns", "alt", "und", "grau", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sehen wir der Mitternacht", "tokens": ["So", "se\u00b7hen", "wir", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Grabes sehnsuchtsvoll entgegen,", "tokens": ["Des", "Gra\u00b7bes", "sehn\u00b7suchts\u00b7voll", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir machen oft die Augen zu,", "tokens": ["Wir", "ma\u00b7chen", "oft", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir warten auf die lange Ruh,", "tokens": ["Wir", "war\u00b7ten", "auf", "die", "lan\u00b7ge", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hoffen, da\u00df man uns bald wird zu Bette legen.", "tokens": ["Und", "hof\u00b7fen", ",", "da\u00df", "man", "uns", "bald", "wird", "zu", "Bet\u00b7te", "le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Seelen Wohnplatz giebet nach,", "tokens": ["Der", "See\u00b7len", "Wohn\u00b7platz", "gie\u00b7bet", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Desselben Fugen werden schwach,", "tokens": ["Des\u00b7sel\u00b7ben", "Fu\u00b7gen", "wer\u00b7den", "schwach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine beyden Pfeiler sinken,", "tokens": ["Und", "sei\u00b7ne", "bey\u00b7den", "Pfei\u00b7ler", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir h\u00f6ren das Ger\u00e4usche schon,", "tokens": ["Wir", "h\u00f6\u00b7ren", "das", "Ge\u00b7r\u00e4u\u00b7sche", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der st\u00f6hnende gebrochne Ton", "tokens": ["Der", "st\u00f6h\u00b7nen\u00b7de", "ge\u00b7broch\u00b7ne", "Ton"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will uns den Umsturz bald zu prophezeyn bed\u00fcnken.", "tokens": ["Will", "uns", "den", "Um\u00b7sturz", "bald", "zu", "pro\u00b7phe\u00b7zeyn", "be\u00b7d\u00fcn\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Sinnen th\u00e4tige Gewalt", "tokens": ["Der", "Sin\u00b7nen", "th\u00e4\u00b7ti\u00b7ge", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird kraftlos, stockend, endlich kalt,", "tokens": ["Wird", "kraft\u00b7los", ",", "sto\u00b7ckend", ",", "end\u00b7lich", "kalt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweckt verdr\u00fc\u00dfliche Gedanken,", "tokens": ["Er\u00b7weckt", "ver\u00b7dr\u00fc\u00df\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vorhang decket das Gesicht,", "tokens": ["Ein", "Vor\u00b7hang", "de\u00b7cket", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Ohr bewegen T\u00f6ne nicht,", "tokens": ["Das", "Ohr", "be\u00b7we\u00b7gen", "T\u00f6\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Selbst der Geruch erstickt in seinen engen Schranken.", "tokens": ["Selbst", "der", "Ge\u00b7ruch", "er\u00b7stickt", "in", "sei\u00b7nen", "en\u00b7gen", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Das Wesen, welches will und denkt,", "tokens": ["Das", "We\u00b7sen", ",", "wel\u00b7ches", "will", "und", "denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht sich daher mehr eingeschr\u00e4nkt,", "tokens": ["Sieht", "sich", "da\u00b7her", "mehr", "ein\u00b7ge\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Kraft in Fesseln schmiegen;", "tokens": ["Und", "sei\u00b7ne", "Kraft", "in", "Fes\u00b7seln", "schmie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es hofft geneigten Abendwind,", "tokens": ["Es", "hofft", "ge\u00b7neig\u00b7ten", "Ab\u00b7end\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie Schiffer sehr aufmerksam sind,", "tokens": ["Wie", "Schif\u00b7fer", "sehr", "auf\u00b7merk\u00b7sam", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wegen Stille noch in einem Hafen liegen.", "tokens": ["Die", "we\u00b7gen", "Stil\u00b7le", "noch", "in", "ei\u00b7nem", "Ha\u00b7fen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer unterdessen gl\u00e4ubig wei\u00df,", "tokens": ["Wer", "un\u00b7ter\u00b7des\u00b7sen", "gl\u00e4u\u00b7big", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf welch allm\u00e4chtiges Gehei\u00df", "tokens": ["Auf", "welch", "all\u00b7m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Noch unsre Daurungs-Stunden w\u00e4hren,", "tokens": ["Noch", "uns\u00b7re", "Dau\u00b7rungs\u00b7Stun\u00b7den", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was erfolgt auf diese Zeit,", "tokens": ["Und", "was", "er\u00b7folgt", "auf", "die\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wartet mit Gelassenheit,", "tokens": ["Der", "war\u00b7tet", "mit", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er suchet nicht den Tod, mag ihn auch nicht begehren.", "tokens": ["Er", "su\u00b7chet", "nicht", "den", "Tod", ",", "mag", "ihn", "auch", "nicht", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Hoffnung zeiget der Natur", "tokens": ["Die", "Hoff\u00b7nung", "zei\u00b7get", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hingegen die erlaubte Spuhr,", "tokens": ["Hin\u00b7ge\u00b7gen", "die", "er\u00b7laub\u00b7te", "Spuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sich manches Kummers zu entlasten,", "tokens": ["Sich", "man\u00b7ches", "Kum\u00b7mers", "zu", "ent\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil sie die hochbejahrte Welt", "tokens": ["Weil", "sie", "die", "hoch\u00b7be\u00b7jahr\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sehr trost- und lehrreich unterh\u00e4lt,", "tokens": ["Sehr", "trost", "und", "lehr\u00b7reich", "un\u00b7ter\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie dorten ehemahls bey Noah in den Kasten.", "tokens": ["Wie", "dor\u00b7ten", "e\u00b7he\u00b7mahls", "bey", "Noah", "in", "den", "Kas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Kommt denn die letzte Botschaft nah,", "tokens": ["Kommt", "denn", "die", "letz\u00b7te", "Bot\u00b7schaft", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und heisset es: Mein Grab ist da!", "tokens": ["Und", "heis\u00b7set", "es", ":", "Mein", "Grab", "ist", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft die Verwesung welke Glieder,", "tokens": ["Ruft", "die", "Ver\u00b7we\u00b7sung", "wel\u00b7ke", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist man hurtig und bereit,", "tokens": ["So", "ist", "man", "hur\u00b7tig", "und", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man leget diese Sterblichkeit", "tokens": ["Man", "le\u00b7get", "die\u00b7se", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seiner sinkenden und morschen H\u00fctte nieder.", "tokens": ["Mit", "sei\u00b7ner", "sin\u00b7ken\u00b7den", "und", "mor\u00b7schen", "H\u00fct\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wohlseeliger! Dir gl\u00fcckt es nun!", "tokens": ["Wohl\u00b7see\u00b7li\u00b7ger", "!", "Dir", "gl\u00fcckt", "es", "nun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein schwacher Leib begehrt zu ruhn,", "tokens": ["Dein", "schwa\u00b7cher", "Leib", "be\u00b7gehrt", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Alter hoffte; da\u00df es st\u00fcrbe;", "tokens": ["Das", "Al\u00b7ter", "hoff\u00b7te", ";", "da\u00df", "es", "st\u00fcr\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Hafen landest Du nun an,", "tokens": ["Jm", "Ha\u00b7fen", "lan\u00b7dest", "Du", "nun", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dein herumgeworfner Kahn", "tokens": ["Und", "dein", "her\u00b7um\u00b7ge\u00b7worf\u00b7ner", "Kahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist von so manchem Sturm der Wintertage m\u00fcrbe.", "tokens": ["Ist", "von", "so", "man\u00b7chem", "Sturm", "der", "Win\u00b7ter\u00b7ta\u00b7ge", "m\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Dich hat hier eine lange Nacht,", "tokens": ["Dich", "hat", "hier", "ei\u00b7ne", "lan\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die um uns alles finster macht,", "tokens": ["Die", "um", "uns", "al\u00b7les", "fins\u00b7ter", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manche Jahre schon bedecket,", "tokens": ["So", "man\u00b7che", "Jah\u00b7re", "schon", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sterben warest Du bereit,", "tokens": ["Zu", "ster\u00b7ben", "wa\u00b7rest", "Du", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Du des Todes Bitterkeit", "tokens": ["Da\u00df", "Du", "des", "To\u00b7des", "Bit\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seinen herben Kelch dahero kaum geschmecket.", "tokens": ["Und", "sei\u00b7nen", "her\u00b