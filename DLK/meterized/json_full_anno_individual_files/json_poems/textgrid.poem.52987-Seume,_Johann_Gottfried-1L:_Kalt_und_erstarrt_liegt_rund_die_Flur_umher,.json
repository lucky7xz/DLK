{"textgrid.poem.52987": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kalt und erstarrt liegt rund die Flur umher,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt und erstarrt liegt rund die Flur umher,", "tokens": ["Kalt", "und", "er\u00b7starrt", "liegt", "rund", "die", "Flur", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo der Gedanke nur die Gruppen stellt;", "tokens": ["Wo", "der", "Ge\u00b7dan\u00b7ke", "nur", "die", "Grup\u00b7pen", "stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ohne Herz ist unsre sch\u00f6ne Welt", "tokens": ["Und", "oh\u00b7ne", "Herz", "ist", "uns\u00b7re", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein todtes Bild, und aller Freude leer.", "tokens": ["Ein", "tod\u00b7tes", "Bild", ",", "und", "al\u00b7ler", "Freu\u00b7de", "leer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Herz nur schafft in s\u00fc\u00dfer Sympathie", "tokens": ["Das", "Herz", "nur", "schafft", "in", "s\u00fc\u00b7\u00dfer", "Sym\u00b7pa\u00b7thie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus jedem Gegenstand sich Hochgenu\u00df,", "tokens": ["Aus", "je\u00b7dem", "Ge\u00b7gen\u00b7stand", "sich", "Hoch\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rt in dem Sturm der Liebe Morgengru\u00df,", "tokens": ["H\u00f6rt", "in", "dem", "Sturm", "der", "Lie\u00b7be", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sammelt Freuden aus des Lebens M\u00fch.", "tokens": ["Und", "sam\u00b7melt", "Freu\u00b7den", "aus", "des", "Le\u00b7bens", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Herz gie\u00dft neu die Farben um sich her,", "tokens": ["Das", "Herz", "gie\u00dft", "neu", "die", "Far\u00b7ben", "um", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und haucht ein neues gl\u00fchendes Gewand", "tokens": ["Und", "haucht", "ein", "neu\u00b7es", "gl\u00fc\u00b7hen\u00b7des", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit sch\u00f6nem Zauber um das nackte Land,", "tokens": ["Mit", "sch\u00f6\u00b7nem", "Zau\u00b7ber", "um", "das", "nack\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Macht Arme reich, und gibt den Reichen mehr.", "tokens": ["Macht", "Ar\u00b7me", "reich", ",", "und", "gibt", "den", "Rei\u00b7chen", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Herz schattirt der Auen Purpursaum,", "tokens": ["Das", "Herz", "schat\u00b7tirt", "der", "Au\u00b7en", "Pur\u00b7pur\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sorgenlos der frohe Knabe lief;", "tokens": ["Wo", "sor\u00b7gen\u00b7los", "der", "fro\u00b7he", "Kna\u00b7be", "lief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Heiligthum ist, wo der Vater schlief,", "tokens": ["Und", "Hei\u00b7lig\u00b7thum", "ist", ",", "wo", "der", "Va\u00b7ter", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die kleine Laube mit dem alten Baum.", "tokens": ["Die", "klei\u00b7ne", "Lau\u00b7be", "mit", "dem", "al\u00b7ten", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem Herzen ist das \u00d6rtchen eingeweiht,", "tokens": ["Dem", "Her\u00b7zen", "ist", "das", "\u00d6rt\u00b7chen", "ein\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo oft die Mutter einst mit milder Hand", "tokens": ["Wo", "oft", "die", "Mut\u00b7ter", "einst", "mit", "mil\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihrem Butterbrod zur Spende stand,", "tokens": ["Mit", "ih\u00b7rem", "But\u00b7ter\u00b7brod", "zur", "Spen\u00b7de", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In unsrer Jugend sch\u00f6ner Rosenzeit.", "tokens": ["In", "uns\u00b7rer", "Ju\u00b7gend", "sch\u00f6\u00b7ner", "Ro\u00b7sen\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Herz schafft sich die kleine Rasenbank,", "tokens": ["Das", "Herz", "schafft", "sich", "die", "klei\u00b7ne", "Ra\u00b7sen\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo l\u00e4ndlich auf des Lenzes weichem Gras", "tokens": ["Wo", "l\u00e4nd\u00b7lich", "auf", "des", "Len\u00b7zes", "wei\u00b7chem", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "J\u00fcngst fr\u00f6hlich das geliebte M\u00e4dchen sa\u00df,", "tokens": ["J\u00fcngst", "fr\u00f6h\u00b7lich", "das", "ge\u00b7lieb\u00b7te", "M\u00e4d\u00b7chen", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum hohen k\u00f6niglichen Marmorgang.", "tokens": ["Zum", "ho\u00b7hen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Mar\u00b7mor\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das Herz erhebt, in Lunens Silberschein,", "tokens": ["Das", "Herz", "er\u00b7hebt", ",", "in", "Lu\u00b7nens", "Sil\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo j\u00fcngst entz\u00fcckt in zephyrleichtem Flug", "tokens": ["Wo", "j\u00fcngst", "ent\u00b7z\u00fcckt", "in", "ze\u00b7phyr\u00b7leich\u00b7tem", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Abendlied sie ihre Triller schlug,", "tokens": ["Zum", "A\u00b7ben\u00b7dlied", "sie", "ih\u00b7re", "Tril\u00b7ler", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den kleinen Wald zu einem G\u00f6tterhain.", "tokens": ["Den", "klei\u00b7nen", "Wald", "zu", "ei\u00b7nem", "G\u00f6t\u00b7ter\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es zaubert sich aus einer Felsenwand", "tokens": ["Es", "zau\u00b7bert", "sich", "aus", "ei\u00b7ner", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Blumenbeet zum Feyerkleid hervor,", "tokens": ["Ein", "Blu\u00b7men\u00b7beet", "zum", "Fe\u00b7yer\u00b7kleid", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo still die Lauscherinn mit leisem Ohr", "tokens": ["Wo", "still", "die", "Lau\u00b7sche\u00b7rinn", "mit", "lei\u00b7sem", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bey ihrer Schwester Philomele stand.", "tokens": ["Bey", "ih\u00b7rer", "Schwes\u00b7ter", "Phi\u00b7lo\u00b7me\u00b7le", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Das Herz erh\u00f6ht die H\u00fctte sich zum Thron,", "tokens": ["Das", "Herz", "er\u00b7h\u00f6ht", "die", "H\u00fct\u00b7te", "sich", "zum", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Macht harte Kost zur sch\u00f6nsten Feerey,", "tokens": ["Macht", "har\u00b7te", "Kost", "zur", "sch\u00f6ns\u00b7ten", "Fee\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.3": {"text": "Spricht Schuldner los und gibt Verdammte frey,", "tokens": ["Spricht", "Schuld\u00b7ner", "los", "und", "gibt", "Ver\u00b7damm\u00b7te", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und gr\u00fc\u00dft als Freund den letzten Erdensohn.", "tokens": ["Und", "gr\u00fc\u00dft", "als", "Freund", "den", "letz\u00b7ten", "Er\u00b7den\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Die ganze Trift wird harmonienvoll,", "tokens": ["Die", "gan\u00b7ze", "Trift", "wird", "har\u00b7mo\u00b7ni\u00b7en\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur Oreade jeder Echolaut,", "tokens": ["Zur", "O\u00b7rea\u00b7de", "je\u00b7der", "E\u00b7cho\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und jeder Quell den Himmlischen vertraut,", "tokens": ["Und", "je\u00b7der", "Quell", "den", "Himm\u00b7li\u00b7schen", "ver\u00b7traut", ","]