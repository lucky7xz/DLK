{"textgrid.poem.48874": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "99. Er redet den Mund seiner Freundin an, die er bei sich hatte", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab' ich dich nun einmal, du K\u00fchlung meiner Hitze,", "tokens": ["Hab'", "ich", "dich", "nun", "ein\u00b7mal", ",", "du", "K\u00fch\u00b7lung", "mei\u00b7ner", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "ADV", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du Labsal meines Dursts, den du mir selbst gemacht", "tokens": ["du", "Lab\u00b7sal", "mei\u00b7nes", "Dursts", ",", "den", "du", "mir", "selbst", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nach deinem Lebenstau, als du mich angelacht", "tokens": ["nach", "dei\u00b7nem", "Le\u00b7ben\u00b7stau", ",", "als", "du", "mich", "an\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und ernstlich hast bestrahlt mit deiner \u00c4uglein Plitze!", "tokens": ["und", "ernst\u00b7lich", "hast", "be\u00b7strahlt", "mit", "dei\u00b7ner", "\u00c4ug\u00b7lein", "Plit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol mir Gl\u00fcckseligem, der ich den G\u00f6ttern sitze", "tokens": ["Wol", "mir", "Gl\u00fcck\u00b7se\u00b7li\u00b7gem", ",", "der", "ich", "den", "G\u00f6t\u00b7tern", "sit\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "selbselbsten in dem Scho\u00df'! Ich find an mir vollbracht", "tokens": ["selb\u00b7selbs\u00b7ten", "in", "dem", "Scho\u00df'", "!", "Ich", "find", "an", "mir", "voll\u00b7bracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "all', alle S\u00fc\u00dfigkeit, so werden kan erdacht,", "tokens": ["all'", ",", "al\u00b7le", "S\u00fc\u00b7\u00dfig\u00b7keit", ",", "so", "wer\u00b7den", "kan", "er\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIAT", "NN", "$,", "ADV", "VAINF", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und biete nun auf Lust den G\u00f6ttern selbst die Spitze.", "tokens": ["und", "bie\u00b7te", "nun", "auf", "Lust", "den", "G\u00f6t\u00b7tern", "selbst", "die", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Tut, bitt' ich, tut euch auf, o ihr Korallenpforten,", "tokens": ["Tut", ",", "bitt'", "ich", ",", "tut", "euch", "auf", ",", "o", "ihr", "Ko\u00b7ral\u00b7len\u00b7pfor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und sagt mir eure Gunst mit halbgemachten Worten.", "tokens": ["und", "sagt", "mir", "eu\u00b7re", "Gunst", "mit", "halb\u00b7ge\u00b7mach\u00b7ten", "Wor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch, was begehr ich das? Du bist ihr Herzens Rat,", "tokens": ["Doch", ",", "was", "be\u00b7gehr", "ich", "das", "?", "Du", "bist", "ihr", "Her\u00b7zens", "Rat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "VVFIN", "PPER", "PDS", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "du rosengleicher Mund, das, weils nicht kan geschehen,", "tokens": ["du", "ro\u00b7sen\u00b7glei\u00b7cher", "Mund", ",", "das", ",", "weils", "nicht", "kan", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PDS", "$,", "KOUS", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df sich es blo\u00df von mir und leiblich lasse sehen,", "tokens": ["da\u00df", "sich", "es", "blo\u00df", "von", "mir", "und", "leib\u00b7lich", "las\u00b7se", "se\u00b7hen", ","], "token_info