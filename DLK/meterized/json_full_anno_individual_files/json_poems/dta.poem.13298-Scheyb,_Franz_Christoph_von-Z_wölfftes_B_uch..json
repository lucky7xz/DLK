{"dta.poem.13298": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Majest\u00e4t fiel ein: \u201eErwarten wir das Ende!", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t", "fiel", "ein", ":", "\u201e", "Er\u00b7war\u00b7ten", "wir", "das", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewelch unerh\u00f6rter Tag der Freuden geht mir vor?", "tokens": ["\u201e", "welch", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "Tag", "der", "Freu\u00b7den", "geht", "mir", "vor", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eich sehe diesen Hof schon wieder an dem Thor", "tokens": ["\u201e", "ich", "se\u00b7he", "die\u00b7sen", "Hof", "schon", "wie\u00b7der", "an", "dem", "Thor"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PDAT", "NN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201egl\u00fcckw\u00fcnschungen des Volcks in Fr\u00f6hlichkeit empfangen,", "tokens": ["\u201e", "gl\u00fcck\u00b7w\u00fcn\u00b7schun\u00b7gen", "des", "Volcks", "in", "Fr\u00f6h\u00b7lich\u00b7keit", "emp\u00b7fan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "\u201eund mit noch einer Kron allhier zur\u00fcck gelangen.", "tokens": ["\u201e", "und", "mit", "noch", "ei\u00b7ner", "Kron", "all\u00b7hier", "zu\u00b7r\u00fcck", "ge\u00b7lan\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ADV", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "185\u201dIch bilde mir schon ein, wie manches Siegs-Ger\u00fcst", "tokens": ["\"", "Ich", "bil\u00b7de", "mir", "schon", "ein", ",", "wie", "man\u00b7ches", "Siegs\u00b7Ge\u00b7r\u00fcst"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eruhm, Ehren und Triumpf in goldne B\u00f6gen schlie\u00dft;", "tokens": ["\u201e", "ruhm", ",", "Eh\u00b7ren", "und", "Tri\u00b7umpf", "in", "gold\u00b7ne", "B\u00f6\u00b7gen", "schlie\u00dft", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "\u201ewie sich ", "tokens": ["\u201e", "wie", "sich"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PWAV", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "\u201eauf einem Schiff, mit Palm und Lorber umgeschlungen,", "tokens": ["\u201e", "auf", "ei\u00b7nem", "Schiff", ",", "mit", "Palm", "und", "Lor\u00b7ber", "um\u00b7ge\u00b7schlun\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "$,", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edem Volck, dem frohen Volck zur Lust vor Augen stellt,", "tokens": ["\u201e", "dem", "Volck", ",", "dem", "fro\u00b7hen", "Volck", "zur", "Lust", "vor", "Au\u00b7gen", "stellt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "190\u201dUnd dieses Vaterland als eine Soun' erhellt.", "tokens": ["\"", "Und", "die\u00b7ses", "Va\u00b7ter\u00b7land", "als", "ei\u00b7ne", "So\u00b7un'", "er\u00b7hellt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "\u201eich sehe wie das Gl\u00fcck sich um die Flaggen schwinget;", "tokens": ["\u201e", "ich", "se\u00b7he", "wie", "das", "Gl\u00fcck", "sich", "um", "die", "Flag\u00b7gen", "schwin\u00b7get", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KOKOM", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewie jeder Ruder-Schlag uns neue Wohlfart bringet.", "tokens": ["\u201e", "wie", "je\u00b7der", "Ru\u00b7der\u00b7Schlag", "uns", "neu\u00b7e", "Wohl\u00b7fart", "brin\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PIAT", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}