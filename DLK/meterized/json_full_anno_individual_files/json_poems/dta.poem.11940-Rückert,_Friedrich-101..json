{"dta.poem.11940": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von ferne kannst du nicht die Trommel h\u00f6ren schlagen,", "tokens": ["Von", "fer\u00b7ne", "kannst", "du", "nicht", "die", "Trom\u00b7mel", "h\u00f6\u00b7ren", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' unvermerkt im Takt darnach den Schritt zu tragen.", "tokens": ["Ohn'", "un\u00b7ver\u00b7merkt", "im", "Takt", "dar\u00b7nach", "den", "Schritt", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PAV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O h\u00f6rtest du auch so die Sternentrommel nur,", "tokens": ["O", "h\u00f6r\u00b7test", "du", "auch", "so", "die", "Ster\u00b7nen\u00b7trom\u00b7mel", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wonach das lichte Heer dort aufzieht im Azur.", "tokens": ["Wo\u00b7nach", "das", "lich\u00b7te", "Heer", "dort", "auf\u00b7zieht", "im", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gib Acht! du kannst den Ton vernehmen allerwegen", "tokens": ["Gib", "Acht", "!", "du", "kannst", "den", "Ton", "ver\u00b7neh\u00b7men", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dir, um jeden Tritt harmonisch mit zu regen.", "tokens": ["In", "dir", ",", "um", "je\u00b7den", "Tritt", "har\u00b7mo\u00b7nisch", "mit", "zu", "re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUI", "PIAT", "NN", "ADJD", "APPR", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}