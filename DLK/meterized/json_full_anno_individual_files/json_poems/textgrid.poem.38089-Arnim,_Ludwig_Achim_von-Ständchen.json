er": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Stehest du nicht auf und l\u00e4ssest mich nicht ein,", "tokens": ["Ste\u00b7hest", "du", "nicht", "auf", "und", "l\u00e4s\u00b7sest", "mich", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie k\u00f6nntest du so unbarmherzig seyn.", "tokens": ["Wie", "k\u00f6nn\u00b7test", "du", "so", "un\u00b7barm\u00b7her\u00b7zig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Harfenklang und Saitenspiel,", "tokens": ["Har\u00b7fen\u00b7klang", "und", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich lassen spielen so oft und viel,", "tokens": ["Hab", "ich", "las\u00b7sen", "spie\u00b7len", "so", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ich hab es lassen spielen so oft und viel,", "tokens": ["Ich", "hab", "es", "las\u00b7sen", "spie\u00b7len", "so", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So da\u00df mir keine Saite mehr klingen will.", "tokens": ["So", "da\u00df", "mir", "kei\u00b7ne", "Sai\u00b7te", "mehr", "klin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Berg und H\u00fcgel auch dieses Thal,", "tokens": ["Berg", "und", "H\u00fc\u00b7gel", "auch", "die\u00b7ses", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PDAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schreien \u00fcber mich auch hunderttausendmal,", "tokens": ["Schrei\u00b7en", "\u00fc\u00b7ber", "mich", "auch", "hun\u00b7dert\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Froh wollt ich seyn, wenns dir und mir wohlgeht,", "tokens": ["Froh", "wollt", "ich", "seyn", ",", "wenns", "dir", "und", "mir", "wohl\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Obschon mein treues Herz in Trauren steht.", "tokens": ["Ob\u00b7schon", "mein", "treu\u00b7es", "Herz", "in", "Trau\u00b7ren", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Gute Nacht, gute Nacht! Frau Nachtigall", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "!", "Frau", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "punct", "wor