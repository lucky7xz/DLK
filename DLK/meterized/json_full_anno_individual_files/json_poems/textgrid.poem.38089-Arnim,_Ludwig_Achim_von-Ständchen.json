{"textgrid.poem.38089": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "St\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liegst du schon in sanfter Ruh", "tokens": ["Liegst", "du", "schon", "in", "sanf\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und thust dein schwarzbraun Aeuglein zu,", "tokens": ["Und", "thust", "dein", "schwarz\u00b7braun", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die zarte Gliederlein", "tokens": ["Und", "die", "zar\u00b7te", "Glie\u00b7der\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl in ein Federbett gewickelt ein.", "tokens": ["Wohl", "in", "ein", "Fe\u00b7der\u00b7bett", "ge\u00b7wi\u00b7ckelt", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4lder, Felder schweigen still,", "tokens": ["W\u00e4l\u00b7der", ",", "Fel\u00b7der", "schwei\u00b7gen", "still", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und niemand ist der mit mir sprechen will,", "tokens": ["Und", "nie\u00b7mand", "ist", "der", "mit", "mir", "spre\u00b7chen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Alle Fl\u00fc\u00df haben ihren Lauf,", "tokens": ["Al\u00b7le", "Fl\u00fc\u00df", "ha\u00b7ben", "ih\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und niemand ist, der mit mir bleibet auf.", "tokens": ["Und", "nie\u00b7mand", "ist", ",", "der", "mit", "mir", "blei\u00b7bet", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Heut hab ich die Wach allhier,", "tokens": ["Heut", "hab", "ich", "die", "Wach", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sch\u00f6nste vor deiner verschlo\u00dfnen Th\u00fcr,", "tokens": ["Sch\u00f6ns\u00b7te", "vor", "dei\u00b7ner", "ver\u00b7schlo\u00df\u00b7nen", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sonn und Mond, dazu das Firmament,", "tokens": ["Sonn", "und", "Mond", ",", "da\u00b7zu", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PAV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schaun wie mein junges Herz vor Liebe brennt.", "tokens": ["Schaun", "wie", "mein", "jun\u00b7ges", "Herz", "vor", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du nicht die Seufzer schallen,", "tokens": ["H\u00f6rst", "du", "nicht", "die", "Seuf\u00b7zer", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nste vor deinem Schlafk\u00e4mmerlein fallen,", "tokens": ["Sch\u00f6ns\u00b7te", "vor", "dei\u00b7nem", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Stehest du nicht auf und l\u00e4ssest mich nicht ein,", "tokens": ["Ste\u00b7hest", "du", "nicht", "auf", "und", "l\u00e4s\u00b7sest", "mich", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie k\u00f6nntest du so unbarmherzig seyn.", "tokens": ["Wie", "k\u00f6nn\u00b7test", "du", "so", "un\u00b7barm\u00b7her\u00b7zig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Harfenklang und Saitenspiel,", "tokens": ["Har\u00b7fen\u00b7klang", "und", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich lassen spielen so oft und viel,", "tokens": ["Hab", "ich", "las\u00b7sen", "spie\u00b7len", "so", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ich hab es lassen spielen so oft und viel,", "tokens": ["Ich", "hab", "es", "las\u00b7sen", "spie\u00b7len", "so", "oft", "und", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So da\u00df mir keine Saite mehr klingen will.", "tokens": ["So", "da\u00df", "mir", "kei\u00b7ne", "Sai\u00b7te", "mehr", "klin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Berg und H\u00fcgel auch dieses Thal,", "tokens": ["Berg", "und", "H\u00fc\u00b7gel", "auch", "die\u00b7ses", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PDAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schreien \u00fcber mich auch hunderttausendmal,", "tokens": ["Schrei\u00b7en", "\u00fc\u00b7ber", "mich", "auch", "hun\u00b7dert\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Froh wollt ich seyn, wenns dir und mir wohlgeht,", "tokens": ["Froh", "wollt", "ich", "seyn", ",", "wenns", "dir", "und", "mir", "wohl\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Obschon mein treues Herz in Trauren steht.", "tokens": ["Ob\u00b7schon", "mein", "treu\u00b7es", "Herz", "in", "Trau\u00b7ren", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gute Nacht, gute Nacht! Frau Nachtigall", "tokens": ["Gu\u00b7te", "Nacht", ",", "gu\u00b7te", "Nacht", "!", "Frau", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$.", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In dem Thal, tausendmal, \u00fcberall,", "tokens": ["In", "dem", "Thal", ",", "tau\u00b7send\u00b7mal", ",", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "$,", "ADV", "$,"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Gr\u00fc\u00dfe sie aus meinem Herzensgrund,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "sie", "aus", "mei\u00b7nem", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus meinem Herzen, mit deinem Mund.", "tokens": ["Aus", "mei\u00b7nem", "Her\u00b7zen", ",", "mit", "dei\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "H\u00f6rst du wohl den Schu\u00df hier fallen,", "tokens": ["H\u00f6rst", "du", "wohl", "den", "Schu\u00df", "hier", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nste vor dem Schlafk\u00e4mmerlein schallen,", "tokens": ["Sch\u00f6ns\u00b7te", "vor", "dem", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ach warum lie\u00dfest du mich nicht herein,", "tokens": ["Ach", "wa\u00b7rum", "lie\u00b7\u00dfest", "du", "mich", "nicht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "PRF", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Konntest ach so unbarmherzig seyn.", "tokens": ["Konn\u00b7test", "ach", "so", "un\u00b7barm\u00b7her\u00b7zig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Geht es dir wohl, so denke an mich,", "tokens": ["Geht", "es", "dir", "wohl", ",", "so", "den\u00b7ke", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Geht es dir \u00fcbel, so kr\u00e4nket es mich", "tokens": ["Geht", "es", "dir", "\u00fc\u00b7bel", ",", "so", "kr\u00e4n\u00b7ket", "es", "mich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Froh wollt ich seyn, wenns dir und mir wohlgeht,", "tokens": ["Froh", "wollt", "ich", "seyn", ",", "wenns", "dir", "und", "mir", "wohl\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Obgleich mein treues Herz in Blute steht.", "tokens": ["Ob\u00b7gleich", "mein", "treu\u00b7es", "Herz", "in", "Blu\u00b7te", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Liegst du schon in sanfter Ruh", "tokens": ["Liegst", "du", "schon", "in", "sanf\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und thust dein schwarzbraun Aeuglein zu,", "tokens": ["Und", "thust", "dein", "schwarz\u00b7braun", "A\u00b7e\u00b7u\u00b7glein", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die zarte Gliederlein", "tokens": ["Und", "die", "zar\u00b7te", "Gli