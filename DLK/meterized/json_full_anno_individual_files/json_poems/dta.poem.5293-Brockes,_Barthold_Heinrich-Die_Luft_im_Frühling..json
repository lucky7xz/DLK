{"dta.poem.5293": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Luft im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kalte Luft, die um uns schwebet, und welche sich mit", "tokens": ["Die", "kal\u00b7te", "Luft", ",", "die", "um", "uns", "schwe\u00b7bet", ",", "und", "wel\u00b7che", "sich", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "KON", "PRELS", "PRF", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "uns zugleich", "tokens": ["uns", "zu\u00b7gleich"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Der Licht- und Lebens-Qvelle n\u00e4hert, und in der hellen", "tokens": ["Der", "Licht", "und", "Le\u00b7bens\u00b7Q\u00b7vel\u00b7le", "n\u00e4\u00b7hert", ",", "und", "in", "der", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sonnen Reich", "tokens": ["Son\u00b7nen", "Reich"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Zugleich mit uns gef\u00fchret wird, versp\u00fchrt derselben war-", "tokens": ["Zu\u00b7gleich", "mit", "uns", "ge\u00b7f\u00fch\u00b7ret", "wird", ",", "ver\u00b7sp\u00fchrt", "der\u00b7sel\u00b7ben", "wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "men Kraft", "tokens": ["men", "Kraft"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "(wodurch sie alles labt und n\u00e4hret) so sanft belebend\u2019 Ei-", "tokens": ["(", "wo\u00b7durch", "sie", "al\u00b7les", "labt", "und", "n\u00e4h\u00b7ret", ")", "so", "sanft", "be\u00b7le\u00b7bend'", "Ei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$(", "ADV", "ADJD", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "genschaft", "tokens": ["gen\u00b7schaft"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Am erst- und meisten; wird durchdrungen, wird warm,", "tokens": ["Am", "er\u00b7st", "und", "meis\u00b7ten", ";", "wird", "durch\u00b7drun\u00b7gen", ",", "wird", "warm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "VVFIN", "$.", "VAFIN", "VVPP", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "verd\u00fcnnt und ausgespannt:", "tokens": ["ver\u00b7d\u00fcnnt", "und", "aus\u00b7ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die groben D\u00fcnste theilen sich, man sieht sie hier und", "tokens": ["Die", "gro\u00b7ben", "D\u00fcns\u00b7te", "thei\u00b7len", "sich", ",", "man", "sieht", "sie", "hier", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "PPER", "ADV", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "dort zerst\u00fccket", "tokens": ["dort", "zer\u00b7st\u00fc\u00b7cket"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "In Wolcken von verschiedner Art, in nicht zu zehlenden Fi-", "tokens": ["In", "Wol\u00b7cken", "von", "ver\u00b7schied\u00b7ner", "Art", ",", "in", "nicht", "zu", "zeh\u00b7len\u00b7den", "Fi"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "PTKNEG", "PTKZU", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.14": {"text": "guren,", "tokens": ["gu\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Die theils bekannt, theils unbekannt,", "tokens": ["Die", "theils", "be\u00b7kannt", ",", "theils", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und die das Licht im holden Fr\u00fchling viel sch\u00f6ner noch, als", "tokens": ["Und", "die", "das", "Licht", "im", "hol\u00b7den", "Fr\u00fch\u00b7ling", "viel", "sch\u00f6\u00b7ner", "noch", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ART", "NN", "APPRART", "ADJA", "NN", "ADV", "ADJD", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "sonsten schm\u00fccket.", "tokens": ["sons\u00b7ten", "schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Sie theilen sich bald hier, bald dort, und lassen durch der", "tokens": ["Sie", "thei\u00b7len", "sich", "bald", "hier", ",", "bald", "dort", ",", "und", "las\u00b7sen", "durch", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "KON", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wolcken Schleyer", "tokens": ["Wol\u00b7cken", "Schle\u00b7yer"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Der reinen Sonnen Glantz und Feuer,", "tokens": ["Der", "rei\u00b7nen", "Son\u00b7nen", "Glantz", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Dort, durch derselben duncklen Schwall des reinen Him-", "tokens": ["Dort", ",", "durch", "der\u00b7sel\u00b7ben", "dunck\u00b7len", "Schwall", "des", "rei\u00b7nen", "Him"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "mels tieffe H\u00f6hn", "tokens": ["mels", "tief\u00b7fe", "H\u00f6hn"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "In einem hellen, heitern, reinen, mehr als Sapphirnen", "tokens": ["In", "ei\u00b7nem", "hel\u00b7len", ",", "hei\u00b7tern", ",", "rei\u00b7nen", ",", "mehr", "als", "Sap\u00b7phir\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,", "ADV", "KOUS", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.24": {"text": "Blauen sehn,", "tokens": ["Blau\u00b7en", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.25": {"text": "Doch sieht man jetzt zuweilen auch sich manchen d\u00fcnnen", "tokens": ["Doch", "sieht", "man", "jetzt", "zu\u00b7wei\u00b7len", "auch", "sich", "man\u00b7chen", "d\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "ADV", "PRF", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Duft erheben,", "tokens": ["Duft", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Und, gleichsam seegelnd, hin und her in neu durchstrahlten", "tokens": ["Und", ",", "gleich\u00b7sam", "see\u00b7gelnd", ",", "hin", "und", "her", "in", "neu", "durch\u00b7strahl\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "VVPP", "$,", "ADV", "KON", "ADV", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "L\u00fcften schweben", "tokens": ["L\u00fcf\u00b7ten", "schwe\u00b7ben"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}}}}}