{"dta.poem.11106": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft der Amaranthe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steh, pilgram! wer du bist, und schaue diesen stein,", "tokens": ["Steh", ",", "pilg\u00b7ram", "!", "wer", "du", "bist", ",", "und", "schau\u00b7e", "die\u00b7sen", "stein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich Venus selbst, ich bins, die ihn hieher getragen.", "tokens": ["Ich", "Ve\u00b7nus", "selbst", ",", "ich", "bins", ",", "die", "ihn", "hie\u00b7her", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,", "PPER", "VAFIN", "$,", "PRELS", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fragst du: Wer ruht allhier? So la\u00df dir dieses sagen:", "tokens": ["Fragst", "du", ":", "Wer", "ruht", "all\u00b7hier", "?", "So", "la\u00df", "dir", "die\u00b7ses", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWS", "VVFIN", "ADV", "$.", "ADV", "VVIMP", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ich nicht Venus w\u00e4r, so m\u00fcste sie es seyn.", "tokens": ["Wenn", "ich", "nicht", "Ve\u00b7nus", "w\u00e4r", ",", "so", "m\u00fcs\u00b7te", "sie", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}