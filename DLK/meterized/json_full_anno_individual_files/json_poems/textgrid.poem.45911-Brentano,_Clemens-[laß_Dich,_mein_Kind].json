{"textgrid.poem.45911": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[la\u00df Dich, mein Kind]", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Dich, mein Kind den Tadel nicht verf\u00fchren,", "tokens": ["La\u00df", "Dich", ",", "mein", "Kind", "den", "Ta\u00b7del", "nicht", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vertrau wenn du ihn hast, dem guten Sinn,", "tokens": ["Ver\u00b7trau", "wenn", "du", "ihn", "hast", ",", "dem", "gu\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PPER", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sprich: Nur weil ich nicht unsterblich bin", "tokens": ["Und", "sprich", ":", "Nur", "weil", "ich", "nicht", "uns\u00b7terb\u00b7lich", "bin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ADV", "KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Will die Vers\u00f6hnung liebend mir geb\u00fchren.", "tokens": ["Will", "die", "Ver\u00b7s\u00f6h\u00b7nung", "lie\u00b7bend", "mir", "ge\u00b7b\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn Gottes Hand sie kann uns pl\u00f6tzlich r\u00fchren,", "tokens": ["