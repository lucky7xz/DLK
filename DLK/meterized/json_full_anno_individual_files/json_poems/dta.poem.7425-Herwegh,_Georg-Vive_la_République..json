{"dta.poem.7425": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vive la R\u00e9publique.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Berg an Berg und Brand an Brand", "tokens": ["Berg", "an", "Berg", "und", "Brand", "an", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lodern hier zusammen;", "tokens": ["Lo\u00b7dern", "hier", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Welch ein Gl\u00fchen! \u2014 ha! so stand", "tokens": ["Welch", "ein", "Gl\u00fc\u00b7hen", "!", "ha", "!", "so", "stand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$.", "$(", "ITJ", "$.", "ADV", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ilion einst in Flammen.", "tokens": ["I\u00b7li\u00b7on", "einst", "in", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ein versinkend K\u00f6nigshaus", "tokens": ["Ein", "ver\u00b7sin\u00b7kend", "K\u00f6\u00b7nigs\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Raucht vor meinem Blicke,", "tokens": ["Raucht", "vor", "mei\u00b7nem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ich ruf' ins Land hinaus:", "tokens": ["Und", "ich", "ruf'", "ins", "Land", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heil'ge Gluten, reiner Schnee,", "tokens": ["Heil'\u00b7ge", "Glu\u00b7ten", ",", "rei\u00b7ner", "Schnee", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Golden Freiheitkissen,", "tokens": ["Gol\u00b7den", "Frei\u00b7heit\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Abendglanzumstralter See,", "tokens": ["A\u00b7bend\u00b7glan\u00b7zums\u00b7tral\u00b7ter", "See", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schluchten, wild zerrissen \u2014", "tokens": ["Schluch\u00b7ten", ",", "wild", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df im Schweizerlandrevier", "tokens": ["Da\u00df", "im", "Schwei\u00b7zer\u00b7land\u00b7re\u00b7vier"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich kein Nacken b\u00fccke!", "tokens": ["Sich", "kein", "Na\u00b7cken", "b\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kaiser ist der B\u00fcrger hier;", "tokens": ["Kai\u00b7ser", "ist", "der", "B\u00fcr\u00b7ger", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine Phalanx stehet fest,", "tokens": ["Ei\u00b7ne", "Pha\u00b7lanx", "ste\u00b7het", "fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest und ohne Wanken,", "tokens": ["Fest", "und", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und an Euren Alpen me\u00dft", "tokens": ["Und", "an", "Eu\u00b7ren", "Al\u00b7pen", "me\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euere Gedanken!", "tokens": ["Eu\u00b7e\u00b7re", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Eurer ", "tokens": ["Eu\u00b7rer"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Ward Euch vom Geschicke;", "tokens": ["Ward", "Euch", "vom", "Ge\u00b7schi\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Blumen um die Schl\u00e4fe her", "tokens": ["Blu\u00b7men", "um", "die", "Schl\u00e4\u00b7fe", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen Eure H\u00f6hen,", "tokens": ["Stei\u00b7gen", "Eu\u00b7re", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Frisch, wie Venus aus dem Meer,", "tokens": ["Frisch", ",", "wie", "Ve\u00b7nus", "aus", "dem", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Auf aus Euren Seeen;", "tokens": ["Auf", "aus", "Eu\u00b7ren", "Se\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da\u00df aus Deinem Jungfernkranz", "tokens": ["Da\u00df", "aus", "Dei\u00b7nem", "Jung\u00b7fern\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Man kein R\u00f6schen knicke,", "tokens": ["Man", "kein", "R\u00f6\u00b7schen", "kni\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Schweizerin, h\u00fct' ihn wohl beim Tanz!", "tokens": ["Schwei\u00b7ze\u00b7rin", ",", "h\u00fct'", "ihn", "wohl", "beim", "Tanz", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Auf die Felsen wollte Gott", "tokens": ["Auf", "die", "Fel\u00b7sen", "woll\u00b7te", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Kirche bauen;", "tokens": ["Sei\u00b7ne", "Kir\u00b7che", "bau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Vor den Felsen soll dem Spott", "tokens": ["Vor", "den", "Fel\u00b7sen", "soll", "dem", "Spott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Feinde grauen!", "tokens": ["Sei\u00b7ner", "Fein\u00b7de", "grau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Zwischen hier und zwischen dort", "tokens": ["Zwi\u00b7schen", "hier", "und", "zwi\u00b7schen", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "APPR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gibt's nur Eine Br\u00fccke:", "tokens": ["Gibt's", "nur", "Ei\u00b7ne", "Br\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Freiheit, o du Felsenwort!", "tokens": ["Frei\u00b7heit", ",", "o", "du", "Fel\u00b7sen\u00b7wort", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}