{"dta.poem.4181": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Fl\u00e4che des Meeres im Sturm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die weite Wasser-Welt f\u00e4ngt an, wenns st\u00fcrmt, sich in", "tokens": ["Die", "wei\u00b7te", "Was\u00b7ser\u00b7Welt", "f\u00e4ngt", "an", ",", "wenns", "st\u00fcrmt", ",", "sich", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "VVFIN", "$,", "PRF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die H\u00f6h' zu b\u00e4umen,", "tokens": ["die", "H\u00f6h'", "zu", "b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und anfangs schwach, bald f\u00fcrchterlich und wild, zu wallen", "tokens": ["Und", "an\u00b7fangs", "schwach", ",", "bald", "f\u00fcrch\u00b7ter\u00b7lich", "und", "wild", ",", "zu", "wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und zu sch\u00e4umen;", "tokens": ["und", "zu", "sch\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ein ungest\u00fchmes, heisres, rauh-verwirrt- und schreckendes", "tokens": ["Ein", "un\u00b7ge\u00b7st\u00fch\u00b7mes", ",", "heis\u00b7res", ",", "rauh\u00b7ver\u00b7wirr\u00b7t", "und", "schre\u00b7cken\u00b7des"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "TRUNC", "KON", "ADJA"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Get\u00f6n", "tokens": ["Ge\u00b7t\u00f6n"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "F\u00e4ngt unten, bey, auch \u00fcber uns, und \u00fcberall an, zu", "tokens": ["F\u00e4ngt", "un\u00b7ten", ",", "bey", ",", "auch", "\u00fc\u00b7ber", "uns", ",", "und", "\u00fc\u00b7be\u00b7rall", "an", ",", "zu"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "$,", "ADV", "APPR", "PPER", "$,", "KON", "ADV", "PTKVZ", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "entstehn.", "tokens": ["ent\u00b7stehn", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Ein f\u00fcrchterlich- und wildes Sausen, ein Rauschen, Krachen,", "tokens": ["Ein", "f\u00fcrch\u00b7ter\u00b7lich", "und", "wil\u00b7des", "Sau\u00b7sen", ",", "ein", "Rau\u00b7schen", ",", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN", "$,", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Heulen, Br\u00fcllen,", "tokens": ["Heu\u00b7len", ",", "Br\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "F\u00e4ngt an, das dunkel-braune Reich der L\u00fcfte, nah und fern,", "tokens": ["F\u00e4ngt", "an", ",", "das", "dun\u00b7kel\u00b7brau\u00b7ne", "Reich", "der", "L\u00fcf\u00b7te", ",", "nah", "und", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "zu f\u00fcllen,", "tokens": ["zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Worein fast schwarzes gre\u00dflich Grau, zugleich mit unter-", "tokens": ["Wo\u00b7rein", "fast", "schwar\u00b7zes", "gre\u00df\u00b7lich", "Grau", ",", "zu\u00b7gleich", "mit", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "ADJD", "NN", "$,", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mischt, zu sehn;", "tokens": ["mischt", ",", "zu", "sehn", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Durch Strick\u2019 und Masten bricht sodann, bey einem scharfen", "tokens": ["Durch", "Strick'", "und", "Mas\u00b7ten", "bricht", "so\u00b7dann", ",", "bey", "ei\u00b7nem", "schar\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "ADV", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schnauben, Brausen,", "tokens": ["Schnau\u00b7ben", ",", "Brau\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Bey einem f\u00fcrchterlichen Krachen, und fast unleidlich starkem", "tokens": ["Bey", "ei\u00b7nem", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Kra\u00b7chen", ",", "und", "fast", "un\u00b7leid\u00b7lich", "star\u00b7kem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+---+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Sausen,", "tokens": ["Sau\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Ein helles, so durchdringendes, als stark- und widriges", "tokens": ["Ein", "hel\u00b7les", ",", "so", "durch\u00b7drin\u00b7gen\u00b7des", ",", "als", "stark", "und", "wid\u00b7ri\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJA", "$,", "KOUS", "TRUNC", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Gepfeif.", "tokens": ["Ge\u00b7pfeif", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Es steht der Flaggen d\u00fcnner Taft, recht wie ein Blech, so fest", "tokens": ["Es", "steht", "der", "Flag\u00b7gen", "d\u00fcn\u00b7ner", "Taft", ",", "recht", "wie", "ein", "Blech", ",", "so", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "und steif,", "tokens": ["und", "steif", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Wenn der so strenge Zug der Luft so Flagg\u2019 als Wimpel", "tokens": ["Wenn", "der", "so", "stren\u00b7ge", "Zug", "der", "Luft", "so", "Flag\u00b7g'", "als", "Wim\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "ART", "NN", "ADV", "NE", "KOUS", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "aufwerts f\u00fchret,", "tokens": ["auf\u00b7werts", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Und sie von allen Seiten pre\u00dft; so l\u00e4\u00dft es wirklich anders", "tokens": ["Und", "sie", "von", "al\u00b7len", "Sei\u00b7ten", "pre\u00dft", ";", "so", "l\u00e4\u00dft", "es", "wirk\u00b7lich", "an\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "nicht,", "tokens": ["nicht", ","], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$,"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Als wenn, da alles in Bewegung, von allen sich kein\u2019 einz\u2019ge", "tokens": ["Als", "wenn", ",", "da", "al\u00b7les", "in", "Be\u00b7we\u00b7gung", ",", "von", "al\u00b7len", "sich", "kein'", "einz'\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "$,", "KOUS", "PIS", "APPR", "NN", "$,", "APPR", "PIAT", "PRF", "PIAT", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "r\u00fchret.", "tokens": ["r\u00fch\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Die Wolken, welche bald im Dunkeln, und bald in einem", "tokens": ["Die", "Wol\u00b7ken", ",", "wel\u00b7che", "bald", "im", "Dun\u00b7keln", ",", "und", "bald", "in", "ei\u00b7nem"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "ADJA", "$,", "KON", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "schnellen Licht,", "tokens": ["schnel\u00b7len", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Den ganzen Kreis der Luft durchstreichen, das graue Firma-", "tokens": ["Den", "gan\u00b7zen", "Kreis", "der", "Luft", "durch\u00b7strei\u00b7chen", ",", "das", "grau\u00b7e", "Fir\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "ment umziehn,", "tokens": ["ment", "um\u00b7ziehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Und bald die Sonne blicken lassen, bald schnell sie wieder", "tokens": ["Und", "bald", "die", "Son\u00b7ne", "bli\u00b7cken", "las\u00b7sen", ",", "bald", "schnell", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "VVINF", "$,", "ADV", "ADJD", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "rauben, fliehn,", "tokens": ["rau\u00b7ben", ",", "fliehn", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Gedr\u00e4nget von der St\u00fcrme Grimm, getrieben von der Wut", "tokens": ["Ge\u00b7dr\u00e4n\u00b7get", "von", "der", "St\u00fcr\u00b7me", "Grimm", ",", "ge\u00b7trie\u00b7ben", "von", "der", "Wut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.36": {"text": "der Winde,", "tokens": ["der", "Win\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Und fliegen, schnellen V\u00f6geln gleich, mit strengem Zug, recht", "tokens": ["Und", "flie\u00b7gen", ",", "schnel\u00b7len", "V\u00f6\u00b7geln", "gleich", ",", "mit", "stren\u00b7gem", "Zug", ",", "recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVINF", "$,", "ADJA", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Pfeil-geschwinde.", "tokens": ["Pfeil\u00b7ge\u00b7schwin\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Es scheint das Firmament nicht fest, ja selbst mit ihnen", "tokens": ["Es", "scheint", "das", "Fir\u00b7ma\u00b7ment", "nicht", "fest", ",", "ja", "selbst", "mit", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "fortzugehn,", "tokens": ["fort\u00b7zu\u00b7gehn", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Kaum kann auch ein gesetztes Aug\u2019 es, sonder Angst und", "tokens": ["Kaum", "kann", "auch", "ein", "ge\u00b7setz\u00b7tes", "Aug'", "es", ",", "son\u00b7der", "Angst", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "NN", "PPER", "$,", "KON", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Schwindel, sehn.", "tokens": ["Schwin\u00b7del", ",", "sehn", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Die aufgeth\u00fcrmte Wasser-Wogen, die regen Berge wilder", "tokens": ["Die", "auf\u00b7ge\u00b7th\u00fcrm\u00b7te", "Was\u00b7ser\u00b7Wo\u00b7gen", ",", "die", "re\u00b7gen", "Ber\u00b7ge", "wil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.44": {"text": "Wellen,", "tokens": ["Wel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Entstehn und \u00e4ndern unaufh\u00f6rlich, im schnellen Steigen,", "tokens": ["Ent\u00b7stehn", "und", "\u00e4n\u00b7dern", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", ",", "im", "schnel\u00b7len", "Stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "ihre Stellen.", "tokens": ["ih\u00b7re", "Stel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.47": {"text": "Sie eilen, aus den Tiefen aufwerts, mit schnellem Zug und", "tokens": ["Sie", "ei\u00b7len", ",", "aus", "den", "Tie\u00b7fen", "auf\u00b7werts", ",", "mit", "schnel\u00b7lem", "Zug", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.48": {"text": "Wallen fort,", "tokens": ["Wal\u00b7len", "fort", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.49": {"text": "Da sie die r\u00fcckwerts wallende, mit strengem Dr\u00e4ngen, \u00fcber-", "tokens": ["Da", "sie", "die", "r\u00fcck\u00b7werts", "wal\u00b7len\u00b7de", ",", "mit", "stren\u00b7gem", "Dr\u00e4n\u00b7gen", ",", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.50": {"text": "steigen,", "tokens": ["stei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "Bis da\u00df, in ihrem ", "tokens": ["Bis", "da\u00df", ",", "in", "ih\u00b7rem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "KOUS", "$,", "APPR", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.52": {"text": "h\u00f6chsten Ort", "tokens": ["h\u00f6chs\u00b7ten", "Ort"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.53": {"text": "Zerbersten, sch\u00e4umen, und im Fallen viel tausend krause", "tokens": ["Zer\u00b7bers\u00b7ten", ",", "sch\u00e4u\u00b7men", ",", "und", "im", "Fal\u00b7len", "viel", "tau\u00b7send", "krau\u00b7se"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KON", "APPRART", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.54": {"text": "Wirbel zeigen,", "tokens": ["Wir\u00b7bel", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.55": {"text": "Von gelblich theils, theils braun-theils grau-theils wei\u00dflich", "tokens": ["Von", "gelb\u00b7lich", "theils", ",", "theils", "braun\u00b7theils", "grau\u00b7theils", "wei\u00df\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADV", "$,", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.56": {"text": "schwarzen Linien,", "tokens": ["schwar\u00b7zen", "Li\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+----", "measure": "dactylic.init"}, "line.57": {"text": "Die sich einander Wechsels-weis\u2019 erheben, st\u00fcrzen und ver-", "tokens": ["Die", "sich", "ein\u00b7an\u00b7der", "Wech\u00b7sels\u00b7weis'", "er\u00b7he\u00b7ben", ",", "st\u00fcr\u00b7zen", "und", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "NN", "VVINF", "$,", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.58": {"text": "dringen,", "tokens": ["drin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.59": {"text": "Erzeugen, sich vernichtigen, sich selbst erheben und verschlin-", "tokens": ["Er\u00b7zeu\u00b7gen", ",", "sich", "ver\u00b7nich\u00b7ti\u00b7gen", ",", "sich", "selbst", "er\u00b7he\u00b7ben", "und", "ver\u00b7schlin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRF", "VVPP", "$,", "PRF", "ADV", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.60": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.61": {"text": "Es sieht des Meeres weite Fl\u00e4ch\u2019, es sieht das ganze", "tokens": ["Es", "sieht", "des", "Mee\u00b7res", "wei\u00b7te", "Fl\u00e4ch'", ",", "es", "sieht", "das", "gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Wasser-Reich", "tokens": ["Was\u00b7ser\u00b7Reich"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.63": {"text": "Dem dunkel-grauen Wasser-Bley, ja gleichsam schwarzer", "tokens": ["Dem", "dun\u00b7kel\u00b7grau\u00b7en", "Was\u00b7ser\u00b7Bley", ",", "ja", "gleich\u00b7sam", "schwar\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Seifen, gleich,", "tokens": ["Sei\u00b7fen", ",", "gleich", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.65": {"text": "Wobey der Wasser-Berge Gipfel und ihre wei\u00df besch\u00e4umten", "tokens": ["Wo\u00b7bey", "der", "Was\u00b7ser\u00b7Ber\u00b7ge", "Gip\u00b7fel", "und", "ih\u00b7re", "wei\u00df", "be\u00b7sch\u00e4um\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "PPOSAT", "VVFIN", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.66": {"text": "H\u00f6h'n,", "tokens": ["H\u00f6h'n", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.67": {"text": "Als w\u00e4ren sie beschneyt, zwar wei\u00df, doch noch fast gre\u00dflicher", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "be\u00b7schneyt", ",", "zwar", "wei\u00df", ",", "doch", "noch", "fast", "gre\u00df\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$,", "ADV", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.68": {"text": "zu sehn;", "tokens": ["zu", "sehn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.69": {"text": "Indem sie, durch ihr schreckend Licht, womit sie in die H\u00f6he", "tokens": ["In\u00b7dem", "sie", ",", "durch", "ihr", "schre\u00b7ckend", "Licht", ",", "wo\u00b7mit", "sie", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJD", "NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.70": {"text": "steigen,", "tokens": ["stei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.71": {"text": "Der hohlen Tiefe dunkle Schw\u00e4rze, um so viel deutlicher,", "tokens": ["Der", "hoh\u00b7len", "Tie\u00b7fe", "dunk\u00b7le", "Schw\u00e4r\u00b7ze", ",", "um", "so", "viel", "deut\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,", "KOUI", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.72": {"text": "noch zeigen.", "tokens": ["noch", "zei\u00b7gen", "."], "to