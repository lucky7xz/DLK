{"dta.poem.12123": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will nicht wohnen an der Wasserf\u00e4lle Brausen,", "tokens": ["Ich", "will", "nicht", "woh\u00b7nen", "an", "der", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch wohnen an der schneebedeckten Berge Grausen.", "tokens": ["Noch", "woh\u00b7nen", "an", "der", "schnee\u00b7be\u00b7deck\u00b7ten", "Ber\u00b7ge", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Alles will ich im Vor\u00fcbergehn besehn,", "tokens": ["Das", "Al\u00b7les", "will", "ich", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch meine Wohnung soll in stillen Schatten stehn.", "tokens": ["Doch", "mei\u00b7ne", "Woh\u00b7nung", "soll", "in", "stil\u00b7len", "Schat\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn wol die Seele schwellt Erhabenheit mit Schauer,", "tokens": ["Denn", "wol", "die", "See\u00b7le", "schwellt", "Er\u00b7ha\u00b7ben\u00b7heit", "mit", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Anmuth nur gef\u00e4llt und freut auf l\u00e4ngre Dauer.", "tokens": ["Doch", "An\u00b7muth", "nur", "ge\u00b7f\u00e4llt", "und", "freut", "auf", "l\u00e4ng\u00b7re", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}