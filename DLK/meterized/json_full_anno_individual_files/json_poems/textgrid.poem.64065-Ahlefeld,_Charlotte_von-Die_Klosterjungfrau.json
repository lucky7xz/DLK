"}, "line.4": {"text": "Schwebt ahnend mir die tr\u00fcbe Zukunft vor.", "tokens": ["Schwebt", "ah\u00b7nend", "mir", "die", "tr\u00fc\u00b7be", "Zu\u00b7kunft", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Wo ist die Zelle, dass ich einsam weine?", "tokens": ["Wo", "ist", "die", "Zel\u00b7le", ",", "dass", "ich", "ein\u00b7sam", "wei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sey mir gegr\u00fcsst, Du trauriges Asyl!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", ",", "Du", "trau\u00b7ri\u00b7ges", "A\u00b7syl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach, in der Andacht seeligem Vereine", "tokens": ["Ach", ",", "in", "der", "An\u00b7dacht", "see\u00b7li\u00b7gem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit stiller Wehmuth, lindre mein Gef\u00fchl!", "tokens": ["Mit", "stil\u00b7ler", "Weh\u00b7muth", ",", "lind\u00b7re", "mein", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Eng' ist Dein Raum \u2013 der Sonne warmer Schimmer", "tokens": ["E