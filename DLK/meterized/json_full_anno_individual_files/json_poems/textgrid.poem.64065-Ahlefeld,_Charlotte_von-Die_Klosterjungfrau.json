rd", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meinem Geist der Erde Lust und Qualen,", "tokens": ["In", "mei\u00b7nem", "Geist", "der", "Er\u00b7de", "Lust", "und", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sich die Neigung mit der Pflicht entzweit.", "tokens": ["Wenn", "sich", "die", "Nei\u00b7gung", "mit", "der", "Pflicht", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.28": {"line.1": {"text": "Lass mich den Himmel rein und offen schauen,", "tokens": ["Lass", "mich", "den", "Him\u00b7mel", "rein", "und", "of\u00b7fen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den du bewohnst mit Deiner Engelschaar,", "tokens": ["Den", "du", "be\u00b7wohnst", "mit", "Dei\u00b7ner", "En\u00b7gel\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sende mir das kindliche Vertrauen,", "tokens": ["Und", "sen\u00b7de", "mir", "das", "kind\u00b7li\u00b7che", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die