{"textgrid.poem.64065": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Klosterjungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur wenig rasche, schnell gewagte Schritte,", "tokens": ["Nur", "we\u00b7nig", "ra\u00b7sche", ",", "schnell", "ge\u00b7wag\u00b7te", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und knarrend \u00f6ffnet sich die Pforte hier;", "tokens": ["Und", "knar\u00b7rend", "\u00f6ff\u00b7net", "sich", "die", "Pfor\u00b7te", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist geschehn \u2013 in seine d\u00fcstre Mitte", "tokens": ["Es", "ist", "ge\u00b7schehn", "\u2013", "in", "sei\u00b7ne", "d\u00fcst\u00b7re", "Mit\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fasst mich der Vorhof, wehe, wehe mir!", "tokens": ["Fasst", "mich", "der", "Vor\u00b7hof", ",", "we\u00b7he", ",", "we\u00b7he", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom frohen Leben ewig, ach, geschieden,", "tokens": ["Vom", "fro\u00b7hen", "Le\u00b7ben", "e\u00b7wig", ",", "ach", ",", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$,", "ITJ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umf\u00e4ngt der Jugend schauerliches Grab", "tokens": ["Um\u00b7f\u00e4ngt", "der", "Ju\u00b7gend", "schau\u00b7er\u00b7li\u00b7ches", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich grausend nun, und Freude, Gl\u00fcck und Frieden", "tokens": ["Mich", "grau\u00b7send", "nun", ",", "und", "Freu\u00b7de", ",", "Gl\u00fcck", "und", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "$,", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sinkt abgestorben und verbl\u00fcht hinab.", "tokens": ["Sinkt", "ab\u00b7ge\u00b7stor\u00b7ben", "und", "ver\u00b7bl\u00fcht", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Schl\u00f6sser rasseln \u2013 dieses Himmels Bl\u00e4ue", "tokens": ["Die", "Schl\u00f6s\u00b7ser", "ras\u00b7seln", "\u2013", "die\u00b7ses", "Him\u00b7mels", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$(", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von d\u00fcstern Linden schwermuthsvoll beschr\u00e4nkt,", "tokens": ["Von", "d\u00fcs\u00b7tern", "Lin\u00b7den", "schwer\u00b7muths\u00b7voll", "be\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erregt der Hoffnung Hochgef\u00fchl aufs neue", "tokens": ["Er\u00b7regt", "der", "Hoff\u00b7nung", "Hoch\u00b7ge\u00b7f\u00fchl", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sich so gern in bange Busen senkt.", "tokens": ["Das", "sich", "so", "gern", "in", "ban\u00b7ge", "Bu\u00b7sen", "senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hinweg, hinweg! ach \u00fcber jene Schwelle,", "tokens": ["Hin\u00b7weg", ",", "hin\u00b7weg", "!", "ach", "\u00fc\u00b7ber", "je\u00b7ne", "Schwel\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "XY", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die bebend nun mein matter Fuss betritt,", "tokens": ["Die", "be\u00b7bend", "nun", "mein", "mat\u00b7ter", "Fuss", "be\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da fluthet der Verzweiflung dunkle Welle,", "tokens": ["Da", "flut\u00b7het", "der", "Ver\u00b7zwei\u00b7flung", "dunk\u00b7le", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmt der Hoffnung letzten Schimmer mit.", "tokens": ["Und", "nimmt", "der", "Hoff\u00b7nung", "letz\u00b7ten", "Schim\u00b7mer", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich bin hin\u00fcber, und mit dumpfem Klange", "tokens": ["Ich", "bin", "hin\u00b7\u00fc\u00b7ber", ",", "und", "mit", "dum\u00b7pfem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschliesst sich hinter mir das Gitterthor,", "tokens": ["Ver\u00b7schliesst", "sich", "hin\u00b7ter", "mir", "das", "Git\u00b7ter\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in dem hochgew\u00f6lbten, finstern Gange", "tokens": ["Und", "in", "dem", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten", ",", "fins\u00b7tern", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwebt ahnend mir die tr\u00fcbe Zukunft vor.", "tokens": ["Schwebt", "ah\u00b7nend", "mir", "die", "tr\u00fc\u00b7be", "Zu\u00b7kunft", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo ist die Zelle, dass ich einsam weine?", "tokens": ["Wo", "ist", "die", "Zel\u00b7le", ",", "dass", "ich", "ein\u00b7sam", "wei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sey mir gegr\u00fcsst, Du trauriges Asyl!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7st", ",", "Du", "trau\u00b7ri\u00b7ges", "A\u00b7syl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ach, in der Andacht seeligem Vereine", "tokens": ["Ach", ",", "in", "der", "An\u00b7dacht", "see\u00b7li\u00b7gem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mit stiller Wehmuth, lindre mein Gef\u00fchl!", "tokens": ["Mit", "stil\u00b7ler", "Weh\u00b7muth", ",", "lind\u00b7re", "mein", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Eng' ist Dein Raum \u2013 der Sonne warmer Schimmer", "tokens": ["Eng'", "ist", "Dein", "Raum", "\u2013", "der", "Son\u00b7ne", "war\u00b7mer", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erheitert nie die graue D\u00e4mmerung", "tokens": ["Er\u00b7hei\u00b7tert", "nie", "die", "grau\u00b7e", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die in Dir waltet, ach und nimmer, nimmer", "tokens": ["Die", "in", "Dir", "wal\u00b7tet", ",", "ach", "und", "nim\u00b7mer", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "XY", "KON", "ADV", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Strahlt mir des Mondes Licht Beruhigung.", "tokens": ["Strahlt", "mir", "des", "Mon\u00b7des", "Licht", "Be\u00b7ru\u00b7hi\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Das schmale Fenster, das mit Eisenst\u00e4ben,", "tokens": ["Das", "schma\u00b7le", "Fens\u00b7ter", ",", "das", "mit", "Ei\u00b7sen\u00b7st\u00e4\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00e4ngnissgleich, verschanzt nach Norden schaut,", "tokens": ["Ge\u00b7f\u00e4ng\u00b7niss\u00b7gleich", ",", "ver\u00b7schanzt", "nach", "Nor\u00b7den", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umschleiern wilde, unfruchtbare Reben,", "tokens": ["Um\u00b7schlei\u00b7ern", "wil\u00b7de", ",", "un\u00b7frucht\u00b7ba\u00b7re", "Re\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wohl oft von heissen Thr\u00e4nen schon bethaut.", "tokens": ["Wohl", "oft", "von", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "schon", "be\u00b7thaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da werd' ich still der L\u00fcfte K\u00fchlung trinken,", "tokens": ["Da", "werd'", "ich", "still", "der", "L\u00fcf\u00b7te", "K\u00fch\u00b7lung", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Tages holdes, mir getr\u00fcbtes Licht,", "tokens": ["Des", "Ta\u00b7ges", "hol\u00b7des", ",", "mir", "ge\u00b7tr\u00fcb\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sinnend weilen, wenn der Sterne Blinken", "tokens": ["Und", "sin\u00b7nend", "wei\u00b7len", ",", "wenn", "der", "Ster\u00b7ne", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich golden in das Blau des Himmels flicht.", "tokens": ["Sich", "gol\u00b7den", "in", "das", "Blau", "des", "Him\u00b7mels", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Denn von dem harten Lager scheucht der Kummer,", "tokens": ["Denn", "von", "dem", "har\u00b7ten", "La\u00b7ger", "scheucht", "der", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der unsichtbar in diesen Mauern thront,", "tokens": ["Der", "un\u00b7sicht\u00b7bar", "in", "die\u00b7sen", "Mau\u00b7ern", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Menschheit h\u00f6chsten Zauber, ach den Schlummer,", "tokens": ["Der", "Menschheit", "h\u00f6chs\u00b7ten", "Zau\u00b7ber", ",", "ach", "den", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Der nicht auf rothgeweinten Augen wohnt.", "tokens": ["Der", "nicht", "auf", "roth\u00b7ge\u00b7wein\u00b7ten", "Au\u00b7gen", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Dein hehres Bild, Du Hochgebenedeite,", "tokens": ["Dein", "heh\u00b7res", "Bild", ",", "Du", "Hoch\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entflammt zur Andacht den umw\u00f6lkten Sinn.", "tokens": ["Ent\u00b7flammt", "zur", "An\u00b7dacht", "den", "um\u00b7w\u00f6lk\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Die Jungfrau, die sich Deinem Dienste weihte,", "tokens": ["Die", "Jung\u00b7frau", ",", "die", "sich", "Dei\u00b7nem", "Diens\u00b7te", "weih\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sinkt betend und ergeben vor Dir hin.", "tokens": ["Sinkt", "be\u00b7tend", "und", "er\u00b7ge\u00b7ben", "vor", "Dir", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O l\u00f6sche Du mit Deiner Blicke Strahlen,", "tokens": ["O", "l\u00f6\u00b7sche", "Du", "mit", "Dei\u00b7ner", "Bli\u00b7cke", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Deines L\u00e4chelns stiller Heiligkeit,", "tokens": ["Mit", "Dei\u00b7nes", "L\u00e4\u00b7chelns", "stil\u00b7ler", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In meinem Geist der Erde Lust und Qualen,", "tokens": ["In", "mei\u00b7nem", "Geist", "der", "Er\u00b7de", "Lust", "und", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sich die Neigung mit der Pflicht entzweit.", "tokens": ["Wenn", "sich", "die", "Nei\u00b7gung", "mi