{"textgrid.poem.41674": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von diesem schrecklichen Gefilde,", "tokens": ["Von", "die\u00b7sem", "schreck\u00b7li\u00b7chen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nie ein sterblich Aug erblickt,", "tokens": ["Das", "nie", "ein", "sterb\u00b7lich", "Aug", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat ein verweht und zart Gebilde", "tokens": ["Hat", "ein", "ver\u00b7weht", "und", "zart", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "VVFIN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch diesen Morgen mich entz\u00fcckt.", "tokens": ["Noch", "die\u00b7sen", "Mor\u00b7gen", "mich", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Schlaf ist reich an Wundertr\u00e4umen!", "tokens": ["Der", "Schlaf", "ist", "reich", "an", "Wun\u00b7der\u00b7tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch einer Laune fremdes Spiel", "tokens": ["Durch", "ei\u00b7ner", "Lau\u00b7ne", "frem\u00b7des", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bannt ich aus den erschauten R\u00e4umen", "tokens": ["Bannt", "ich", "aus", "den", "er\u00b7schau\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Pflanzen regellos Gew\u00fchl.", "tokens": ["Der", "Pflan\u00b7zen", "re\u00b7gel\u00b7los", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Bild, das stolz mein Geist sich malte,", "tokens": ["Im", "Bild", ",", "das", "stolz", "mein", "Geist", "sich", "mal\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfreute sich mein k\u00fchnes Herz", "tokens": ["Er\u00b7freu\u00b7te", "sich", "mein", "k\u00fch\u00b7nes", "Herz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An ewger \u00d6de, die erstrahlte", "tokens": ["An", "ew\u00b7ger", "\u00d6\u00b7de", ",", "die", "er\u00b7strahl\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Wasser, Marmelstein und Erz.", "tokens": ["Von", "Was\u00b7ser", ",", "Mar\u00b7mel\u00b7stein", "und", "Erz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es war ein Babel von Arkaden,", "tokens": ["Es", "war", "ein", "Ba\u00b7bel", "von", "Ar\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein niemals endender Palast,", "tokens": ["Ein", "nie\u00b7mals", "en\u00b7den\u00b7der", "Pa\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Reich an Bassins und an Kaskaden,", "tokens": ["Reich", "an", "Bas\u00b7sins", "und", "an", "Kas\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Schalen matten Golds gefa\u00dft;", "tokens": ["Von", "Scha\u00b7len", "mat\u00b7ten", "Golds", "ge\u00b7fa\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Wasserf\u00e4lle, niederschie\u00dfend", "tokens": ["Und", "Was\u00b7ser\u00b7f\u00e4l\u00b7le", ",", "nie\u00b7der\u00b7schie\u00b7\u00dfend"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich einem Vorhang von Kristall,", "tokens": ["Gleich", "ei\u00b7nem", "Vor\u00b7hang", "von", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sie hingen schwer, ihr Licht ergie\u00dfend,", "tokens": ["Sie", "hin\u00b7gen", "schwer", ",", "ihr", "Licht", "er\u00b7gie\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An steilen Mauern von Metall.", "tokens": ["An", "stei\u00b7len", "Mau\u00b7ern", "von", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Nicht B\u00e4ume sondern Kolonnaden", "tokens": ["Nicht", "B\u00e4u\u00b7me", "son\u00b7dern", "Ko\u00b7lon\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umgaben schlummerstille Seen,", "tokens": ["Um\u00b7ga\u00b7ben", "schlum\u00b7mer\u00b7stil\u00b7le", "Seen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo die gigantischen Najaden", "tokens": ["Wo", "die", "gi\u00b7gan\u00b7ti\u00b7schen", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Frauen sich im Spiegel sehn.", "tokens": ["Wie", "Frau\u00b7en", "sich", "im", "Spie\u00b7gel", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es breiteten sich blaue Teiche", "tokens": ["Es", "brei\u00b7te\u00b7ten", "sich", "blau\u00b7e", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entlang den gr\u00fcn und rosgen Strand,", "tokens": ["Ent\u00b7lang", "den", "gr\u00fcn", "und", "ros\u00b7gen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch tausend nie erme\u00dfne Reiche,", "tokens": ["Durch", "tau\u00b7send", "nie", "er\u00b7me\u00df\u00b7ne", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis an der Erde fernsten Rand.", "tokens": ["Bis", "an", "der", "Er\u00b7de", "ferns\u00b7ten", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es waren nie erschaute Steine", "tokens": ["Es", "wa\u00b7ren", "nie", "er\u00b7schau\u00b7te", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine magisch-fremde Flut,", "tokens": ["Und", "ei\u00b7ne", "ma\u00b7gischfrem\u00b7de", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gewaltge Spiegel, hell vom Scheine", "tokens": ["Ge\u00b7walt\u00b7ge", "Spie\u00b7gel", ",", "hell", "vom", "Schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wunder, die darin geruht.", "tokens": ["Der", "Wun\u00b7der", ",", "die", "da\u00b7rin", "ge\u00b7ruht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Weltstr\u00f6me wie der Ganges flossen", "tokens": ["Welt\u00b7str\u00f6\u00b7me", "wie", "der", "Gan\u00b7ges", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verstummt im Ruhn des \u00c4therblaus,", "tokens": ["Ver\u00b7stummt", "im", "Ruhn", "des", "\u00c4\u00b7ther\u00b7blaus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihrer Urnen Sch\u00e4tze gossen", "tokens": ["Und", "ih\u00b7rer", "Ur\u00b7nen", "Sch\u00e4t\u00b7ze", "gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie in demantne Schl\u00fcnde aus.", "tokens": ["Sie", "in", "de\u00b7mant\u00b7ne", "Schl\u00fcn\u00b7de", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich lie\u00df, des Feeenreichs Erbauer,", "tokens": ["Ich", "lie\u00df", ",", "des", "Fe\u00b7e\u00b7en\u00b7reichs", "Er\u00b7bau\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch eines Tunnels n\u00e4chtgen Gang,", "tokens": ["Durch", "ei\u00b7nes", "Tun\u00b7nels", "n\u00e4cht\u00b7gen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit edelsteingeschm\u00fcckter Mauer,", "tokens": ["Mit", "e\u00b7del\u00b7stein\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Weltmeer gehn, das ich bezwang.", "tokens": ["Das", "Welt\u00b7meer", "gehn", ",", "das", "ich", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Geschliffen, schillernd und gegl\u00e4ttet", "tokens": ["Ge\u00b7schlif\u00b7fen", ",", "schil\u00b7lernd", "und", "ge\u00b7gl\u00e4t\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War selbst der schw\u00e4rzen Farben Nacht,", "tokens": ["War", "selbst", "der", "schw\u00e4r\u00b7zen", "Far\u00b7ben", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stolz prangend in die Flut gebettet,", "tokens": ["Stolz", "pran\u00b7gend", "in", "die", "Flut", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erleuchtet in kristallner Pracht.", "tokens": ["Er\u00b7leuch\u00b7tet", "in", "kris\u00b7tall\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Sonst keine Sterne, keine Flammen", "tokens": ["Sonst", "kei\u00b7ne", "Ster\u00b7ne", ",", "kei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne, selbst am Himmelsrand,", "tokens": ["Der", "Son\u00b7ne", ",", "selbst", "am", "Him\u00b7mels\u00b7rand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dinge all, die wundersamen,", "tokens": ["Die", "Din\u00b7ge", "all", ",", "die", "wun\u00b7der\u00b7sa\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchleuchtete ihr eigner Brand.", "tokens": ["Durch\u00b7leuch\u00b7te\u00b7te", "ihr", "eig\u00b7ner", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und \u00fcber dieser Welt verloren,", "tokens": ["Und", "\u00fc\u00b7ber", "die\u00b7ser", "Welt", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lag \u2013 neuer Schrecken: endlos weit", "tokens": ["Lag", "\u2013", "neu\u00b7er", "Schre\u00b7cken", ":", "end\u00b7los", "weit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$(", "ADJA", "NN", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Auge alles, nichts den Ohren \u2013", "tokens": ["Dem", "Au\u00b7ge", "al\u00b7les", ",", "nichts", "den", "Oh\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "PIS", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schweigen wie die Ewigkeit!", "tokens": ["Ein", "Schwei\u00b7gen", "wie", "die", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Von diesem schrecklichen Gefilde,", "tokens": ["Von", "die\u00b7sem", "schreck\u00b7li\u00b7chen", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das nie ein sterblich Aug erblickt,", "tokens": ["Das", "nie", "ein", "sterb\u00b7lich", "Aug", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat ein verweht und zart Gebilde", "tokens": ["Hat", "ein", "ver\u00b7weht", "und", "zart", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "VVFIN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch diesen Morgen mich entz\u00fcckt.", "tokens": ["Noch", "die\u00b7sen", "Mor\u00b7gen", "mich", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"l