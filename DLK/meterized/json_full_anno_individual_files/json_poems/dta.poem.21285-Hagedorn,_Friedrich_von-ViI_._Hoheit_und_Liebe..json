{"dta.poem.21285": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "ViI .  \n  Hoheit und Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1752", "urn": "urn:nbn:de:kobv:b4-200905199117", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Monarch im Reiche stolzer Thoren,", "tokens": ["Mon\u00b7arch", "im", "Rei\u00b7che", "stol\u00b7zer", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, hohes Gl\u00fcck, verehr ich nicht!", "tokens": ["Dich", ",", "ho\u00b7hes", "Gl\u00fcck", ",", "ver\u00b7ehr", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ward in Phyllis mehr gebohren,", "tokens": ["Mir", "ward", "in", "Phyl\u00b7lis", "mehr", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als alles, was dein Tand verspricht.", "tokens": ["Als", "al\u00b7les", ",", "was", "dein", "Tand", "ver\u00b7spricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Traum der Wachenden, die Ehre,", "tokens": ["Der", "Traum", "der", "Wa\u00b7chen\u00b7den", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sclaven-Stand der Eitelkeit,", "tokens": ["Der", "Scla\u00b7ven\u00b7Stand", "der", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schliesst dein Gefolg an H\u00f6f\u2019 und Heere,", "tokens": ["Schliesst", "dein", "Ge\u00b7folg", "an", "H\u00f6f'", "und", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis es der letzte Schlaf befreyt.", "tokens": ["Bis", "es", "der", "letz\u00b7te", "Schlaf", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Recht, mein Herze zu entz\u00fccken", "tokens": ["Das", "Recht", ",", "mein", "Her\u00b7ze", "zu", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meiner W\u00fcnsche Ziel zu seyn,", "tokens": ["Und", "mei\u00b7ner", "W\u00fcn\u00b7sche", "Ziel", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "R\u00e4um ich nur einer Phyllis Blicken,", "tokens": ["R\u00e4um", "ich", "nur", "ei\u00b7ner", "Phyl\u00b7lis", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Jhrer seltnen Sch\u00f6nheit ein.", "tokens": ["Nur", "Ih\u00b7rer", "selt\u00b7nen", "Sch\u00f6n\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie stolz war ich, Sie zu gewinnen!", "tokens": ["Wie", "stolz", "war", "ich", ",", "Sie", "zu", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch dieser Ruhm verewigt sich.", "tokens": ["Auch", "die\u00b7ser", "Ruhm", "ve\u00b7re\u00b7wigt", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Beneidet Sie, ihr K\u00f6niginnen!", "tokens": ["Be\u00b7nei\u00b7det", "Sie", ",", "ihr", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und, K\u00f6nige! beneidet mich.", "tokens": ["Und", ",", "K\u00f6\u00b7ni\u00b7ge", "!", "be\u00b7nei\u00b7det", "mich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Phyllis, Seele meiner Lieder!", "tokens": ["O", "Phyl\u00b7lis", ",", "See\u00b7le", "mei\u00b7ner", "Lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich reizt kein himmelhoher Flug.", "tokens": ["Mich", "reizt", "kein", "him\u00b7mel\u00b7ho\u00b7her", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich liebest Du, Dich lieb ich wieder.", "tokens": ["Mich", "lie\u00b7best", "Du", ",", "Dich", "lieb", "ich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind wir nicht beyde froh genug?", "tokens": ["Sind", "wir", "nicht", "bey\u00b7de", "froh", "ge\u00b7nug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An treuer Brust, an treuer Seiten", "tokens": ["An", "treu\u00b7er", "Brust", ",", "an", "treu\u00b7er", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Macht uns die Liebe gro\u00df und reich.", "tokens": ["Macht", "uns", "die", "Lie\u00b7be", "gro\u00df", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ach sey, an wahren Z\u00e4rtlichkeiten,", "tokens": ["Ach", "sey", ",", "an", "wah\u00b7ren", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Unendlich jener Taube gleich!", "tokens": ["Un\u00b7end\u00b7lich", "je\u00b7ner", "Tau\u00b7be", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Adler sah die Turteltaube,", "tokens": ["Den", "Ad\u00b7ler", "sah", "die", "Tur\u00b7tel\u00b7tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in der Stille girrt und liebt,", "tokens": ["Die", "in", "der", "Stil\u00b7le", "girrt", "und", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ihn Gewalt und Muth zum Raube", "tokens": ["Wie", "ihn", "Ge\u00b7walt", "und", "Muth", "zum", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In k\u00f6niglichen Thaten \u00fcbt.", "tokens": ["In", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Tha\u00b7ten", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sah ihn Sieg und Ehre finden,", "tokens": ["Sie", "sah", "ihn", "Sieg", "und", "Eh\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Kranich stolz entgegen ziehn,", "tokens": ["Dem", "Kra\u00b7nich", "stolz", "ent\u00b7ge\u00b7gen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich heben, k\u00e4mpfen, \u00fcberwinden,", "tokens": ["Sich", "he\u00b7ben", ",", "k\u00e4mp\u00b7fen", ",", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und alle V\u00f6gel vor ihm fliehn.", "tokens": ["Und", "al\u00b7le", "V\u00f6\u00b7gel", "vor", "ihm", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sprach: Ich will dich nicht beneiden:", "tokens": ["Sie", "sprach", ":", "Ich", "will", "dich", "nicht", "be\u00b7nei\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sey immer gro\u00df und f\u00fcrchterlich.", "tokens": ["Sey", "im\u00b7mer", "gro\u00df", "und", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gepr\u00fcfter Liebe s\u00fcsse Freuden!", "tokens": ["Ge\u00b7pr\u00fcf\u00b7ter", "Lie\u00b7be", "s\u00fcs\u00b7se", "Freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ihr allein begl\u00fccket mich.", "tokens": ["Nur", "ihr", "al\u00b7lein", "be\u00b7gl\u00fc\u00b7cket", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir will ich keinen Sieg erwerben,", "tokens": ["Mir", "will", "ich", "kei\u00b7nen", "Sieg", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als den mein Gatte mir gew\u00e4hrt.", "tokens": ["Als", "den", "mein", "Gat\u00b7te", "mir", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit ihm zu leben und zu sterben", "tokens": ["Mit", "ihm", "zu", "le\u00b7ben", "und", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist alles, was mein Wunsch begehrt.", "tokens": ["Ist", "al\u00b7les", ",", "was", "mein", "Wunsch", "be\u00b7gehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}