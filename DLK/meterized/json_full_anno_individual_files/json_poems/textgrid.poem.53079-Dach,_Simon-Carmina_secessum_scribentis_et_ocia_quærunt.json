": "iambic.tri"}, "line.7": {"text": "Ich setze mich hie nieder", "tokens": ["Ich", "set\u00b7ze", "mich", "hie", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd h\u00f6r' Euch fleissig zu;", "tokens": ["Vnd", "h\u00f6r'", "Euch", "fleis\u00b7sig", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du sanffter West-Wind pfeiffe", "tokens": ["Du", "sanff\u00b7ter", "West\u00b7Wind", "pfeif\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-++--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vnd halte recht den Schlag,", "tokens": ["Vnd", "hal\u00b7te", "recht", "den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df alles seine L\u00e4uffe", "tokens": ["Da\u00df", "al\u00b7les", "sei\u00b7ne", "L\u00e4uf\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geb\u00fchrlich machen mag!", "tokens": ["Ge\u00b7b\u00fchr\u00b7lich", "ma\u00b7chen", "mag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Laub ist g\u00e4ng vnd rege,", "tokens": ["Das", "Laub", "ist", "g\u00e4ng", "vnd", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Lufft ist sanfft vnd still,", "tokens": ["Die", "Lufft", "ist", "sanfft", "vnd", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kein Vogel ist, der tr\u00e4ge", "tokens": ["Kein", "Vo\u00b7gel", "ist", ",", "der", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sich jetzt erweisen will.", "tokens": ["Sich", "jetzt", "er\u00b7wei\u00b7sen", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Indem der Mey behende", "tokens": ["In\u00b7dem", "der", "Mey", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jetzt von vns Abschied nimpt,", "tokens": ["Jetzt", "von", "vns", "Ab\u00b7schied", "nimpt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie allem Thun sein Ende", "tokens": ["Wie", "al\u00b7lem", "Thun", "sein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu haben ist bestimmt:", "tokens": ["Zu", "ha\u00b7ben", "ist", "be\u00b7stimmt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VAINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will ich auff meiner Geigen", "tokens": ["Will", "ich", "auff", "mei\u00b7ner", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im gleichen lustig seyn,", "tokens": ["Im", "glei\u00b7chen", "lus\u00b7tig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sitz' vnter ewren Zweigen", "tokens": ["Sitz'", "vn\u00b7ter", "ew\u00b7ren", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd stimme mit Euch ein.", "tokens": ["Vnd", "stim\u00b7me", "mit", "Euch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wolt jhr darumb mich neiden?", "tokens": ["Wolt", "jhr", "da\u00b7rumb", "mich", "nei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er selbst, Herr Adersbach,", "tokens": ["Er", "selbst", ",", "Herr", "A\u00b7ders\u00b7bach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag es sehr gerne leiden,", "tokens": ["Mag", "es", "sehr", "ger\u00b7ne", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df ich mich fr\u00f6lich mach'", "tokens": ["Da\u00df", "ich", "mich", "fr\u00f6\u00b7lich", "mach'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hie, wo er an dem Pregel", "tokens": ["Hie", ",", "wo", "er", "an", "dem", "Pre\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein Lust-Hau\u00df siehet stehn,", "tokens": ["Sein", "Lust\u00b7Hau\u00df", "sie\u00b7het", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd wo der Russen Segel", "tokens": ["Vnd", "wo", "der", "Rus\u00b7sen", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vorber m\u00fcssen gehn.", "tokens": ["Vor\u00b7ber", "m\u00fcs\u00b7sen", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ich lass', euch B\u00e4ume, ragen", "tokens": ["Ich", "lass'", ",", "euch", "B\u00e4u\u00b7me", ",", "ra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So hoch jhr jmmer k\u00f6nnt,", "tokens": ["So", "hoch", "jhr", "jm\u00b7mer", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd edle Zweige tragen;", "tokens": ["Vnd", "ed\u00b7le", "Zwei\u00b7ge", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dafern jhr mir verg\u00f6nnt,", "tokens": ["Da\u00b7fern", "jhr", "mir", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ich nur mag daneben", "tokens": ["Da\u00df", "ich", "nur", "mag", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch Herren Adersbach", "tokens": ["Auch", "Her\u00b7ren", "A\u00b7ders\u00b7bach"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch vber euch erheben", "tokens": ["Hoch", "vber", "euch", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Bi\u00df an der Sternen Dach.", "tokens": ["Bi\u00df", "an", "der", "Ster\u00b7nen", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gl\u00fcck zu, jhr gr\u00fcnen B\u00e4ume,", "tokens": ["Gl\u00fcck", "zu", ",", "jhr", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ihr Hau\u00df der Sicherheit,", "tokens": ["Ihr", "Hau\u00df", "der", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Vorraht guter Reime,", "tokens": ["Ihr", "Vor\u00b7raht