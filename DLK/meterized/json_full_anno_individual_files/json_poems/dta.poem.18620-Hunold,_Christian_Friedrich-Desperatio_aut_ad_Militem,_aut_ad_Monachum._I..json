{"dta.poem.18620": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Desperatio aut ad Militem, aut  \n ad Monachum.  \n I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bruder Liederlich", "tokens": ["Ein", "Bru\u00b7der", "Lie\u00b7der\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der weder Geld noch Mittel weiter hat/", "tokens": ["Der", "we\u00b7der", "Geld", "noch", "Mit\u00b7tel", "wei\u00b7ter", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wird endlich desperat.", "tokens": ["Wird", "end\u00b7lich", "des\u00b7pe\u00b7rat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was ist nun vorzunehmen?", "tokens": ["Was", "ist", "nun", "vor\u00b7zu\u00b7neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die faule Haut wird sich zu Graben sch\u00e4men", "tokens": ["Die", "fau\u00b7le", "Haut", "wird", "sich", "zu", "Gra\u00b7ben", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kein h\u00e4rter Holtz ist als der Bettelstab.", "tokens": ["Kein", "h\u00e4r\u00b7ter", "Holtz", "ist", "als", "der", "Bet\u00b7tel\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Armuht ist der Wollust Tod und Grab.", "tokens": ["Die", "Ar\u00b7muht", "ist", "der", "Wol\u00b7lust", "Tod", "und", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was f\u00e4ngt er an? Zwey Wege sind zur\u00fccke;", "tokens": ["Was", "f\u00e4ngt", "er", "an", "?", "Zwey", "We\u00b7ge", "sind", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$.", "CARD", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Er sucht zuletzt beym Kalbfell sein Gel\u00fccke;", "tokens": ["Er", "sucht", "zu\u00b7letzt", "beym", "Kalb\u00b7fell", "sein", "Ge\u00b7l\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das Silber ist verthan/", "tokens": ["Das", "Sil\u00b7ber", "ist", "ver\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dr\u00fcm greifft er nun das Eisen an/", "tokens": ["Dr\u00fcm", "greifft", "er", "nun", "das", "Ei\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch wenns der Sauren seyn/", "tokens": ["Doch", "wenns", "der", "Sau\u00b7ren", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "So laufft er in ein Kloster nein.", "tokens": ["So", "laufft", "er", "in", "ein", "Klos\u00b7ter", "nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}