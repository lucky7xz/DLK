{"dta.poem.11287": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Des   Gassendi.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll hier Gassendus seyn? Jhr sucht ihn viel zu fr\u00fch;", "tokens": ["Soll", "hier", "Gas\u00b7sen\u00b7dus", "seyn", "?", "Ihr", "sucht", "ihn", "viel", "zu", "fr\u00fch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "VAINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Es liegt kein leib nicht hier, es sind nur ", "tokens": ["Es", "liegt", "kein", "leib", "nicht", "hier", ",", "es", "sind", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKNEG", "ADV", "$,", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Gassendus lernet hier, es sey nun gantz gewi\u00df:", "tokens": ["Gas\u00b7sen\u00b7dus", "ler\u00b7net", "hier", ",", "es", "sey", "nun", "gantz", "ge\u00b7wi\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Es werd\u2019 ein ", "tokens": ["Es", "werd'", "ein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}}}}}