{"dta.poem.11949": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was dir mislang, wirf weg, wenn du ein Meister bist;", "tokens": ["Was", "dir", "mis\u00b7lang", ",", "wirf", "weg", ",", "wenn", "du", "ein", "Meis\u00b7ter", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn dich's reut, so la\u00df es gut seyn wie es ist.", "tokens": ["Und", "wenn", "dich's", "reut", ",", "so", "la\u00df", "es", "gut", "seyn", "wie", "es", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVIMP", "PPER", "ADJD", "VAINF", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur m\u00fch dich nicht umsonst es bessernd umzuschaffen;", "tokens": ["Nur", "m\u00fch", "dich", "nicht", "um\u00b7sonst", "es", "bes\u00b7sernd", "um\u00b7zu\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKNEG", "VVFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn w\u00e4hrend hier du fugst, wird es dort wieder klaffen.", "tokens": ["Denn", "w\u00e4h\u00b7rend", "hier", "du", "fugst", ",", "wird", "es", "dort", "wie\u00b7der", "klaf\u00b7fen", "."], "token_info": ["word", "word", "wo