{"dta.poem.10213": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( An eine alte Excellenz. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Dir schenkte Hebe einst in tiefe Schalen,", "tokens": ["Dir", "schenk\u00b7te", "He\u00b7be", "einst", "in", "tie\u00b7fe", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du trankst und hast die Reste nicht vergossen,", "tokens": ["Du", "trankst", "und", "hast", "die", "Res\u00b7te", "nicht", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du sahst die Schlacht, den Feind auf Fluchtsandalen,", "tokens": ["Du", "sahst", "die", "Schlacht", ",", "den", "Feind", "auf", "Fluchts\u00b7an\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Mannes Hochkraft strotzt auf Siegesrossen.", "tokens": ["Des", "Man\u00b7nes", "Hoch\u00b7kraft", "strotzt", "auf", "Sie\u00b7ges\u00b7ros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Tagespflicht die Seele, dem Realen,", "tokens": ["Der", "Ta\u00b7ges\u00b7pflicht", "die", "See\u00b7le", ",", "dem", "Re\u00b7a\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat frisch des Lebens Welle dich umflossen.", "tokens": ["Hat", "frisch", "des", "Le\u00b7bens", "Wel\u00b7le", "dich", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nun, Alter, stehst du wei\u00df auf Bergeskahlen,", "tokens": ["Nun", ",", "Al\u00b7ter", ",", "stehst", "du", "wei\u00df", "auf", "Ber\u00b7ges\u00b7kah\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schaust in\u2019s Thal, verdrie\u00dflich und verdrossen.", "tokens": ["Und", "schaust", "in's", "Thal", ",", "ver\u00b7drie\u00df\u00b7lich", "und", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}