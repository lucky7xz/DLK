{"dta.poem.700": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wje der stoltze Schaum der Wellen ", "tokens": ["Wie", "der", "stolt\u00b7ze", "Schaum", "der", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Getrotzt durch grausen Sturm/ vermischt mit wind vnd", "tokens": ["Ge\u00b7trotzt", "durch", "grau\u00b7sen", "Sturm", "/", "ver\u00b7mischt", "mit", "wind", "vnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$(", "VVFIN", "APPR", "NE", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sandt ", "tokens": ["Sandt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jtzt durch die W\u00f6lcken spr\u00fctz't/ jtzt das best\u00fcrtzte Landt ", "tokens": ["Jtzt", "durch", "die", "W\u00f6l\u00b7cken", "spr\u00fctz't", "/", "jtzt", "das", "be\u00b7st\u00fcrtz\u00b7te", "Landt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo die Fischer Netz aufstellen ", "tokens": ["Wo", "die", "Fi\u00b7scher", "Netz", "auf\u00b7stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit brausen \u00fcberschwem\u0301t. wie er/ das Spiel der ", "tokens": ["Mit", "brau\u00b7sen", "\u00fc\u00b7ber\u00b7schwe\u1e3ft", ".", "wie", "er", "/", "das", "Spiel", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ADJD", "$.", "PWAV", "PPER", "$(", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein halb zu scheittert ", "tokens": ["Ein", "halb", "zu", "scheit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKZU", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Bald mit sich in den Abgrundt rei\u00dft/ ", "tokens": ["Bald", "mit", "sich", "in", "den", "Ab\u00b7grundt", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald \u00fcber Klipp\u2019 auf ", "tokens": ["Bald", "\u00fc\u00b7ber", "Klipp'", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "So handelt vn\u00df die herbe Noth.", "tokens": ["So", "han\u00b7delt", "vn\u00df", "die", "her\u00b7be", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Menschen furcht der hartte Todt", "tokens": ["Der", "Men\u00b7schen", "furcht", "der", "hart\u00b7te", "Todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nicht so schrecklich al\u00df das Leben", "tokens": ["Ist", "nicht", "so", "schreck\u00b7lich", "al\u00df", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "(wofern es Leben hei\u00dft) in welchem wir verschmachten/", "tokens": ["(", "wo\u00b7fern", "es", "Le\u00b7ben", "hei\u00dft", ")", "in", "wel\u00b7chem", "wir", "ver\u00b7schmach\u00b7ten", "/"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NN", "VVFIN", "$(", "APPR", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bi\u00df wir den/ durch viel hohn/ vnd Geisseln vnd verachten/", "tokens": ["Bi\u00df", "wir", "den", "/", "durch", "viel", "hohn", "/", "vnd", "Geis\u00b7seln", "vnd", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$(", "APPR", "PIAT", "NN", "$(", "KON", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von hier verjagten Geist auf geben.", "tokens": ["Von", "hier", "ver\u00b7jag\u00b7ten", "Geist", "auf", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ach! m\u00f6cht vn\u00df Rettung doch erquicken:", "tokens": ["Ach", "!", "m\u00f6cht", "vn\u00df", "Ret\u00b7tung", "doch", "er\u00b7qui\u00b7cken", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach! m\u00f6cht vn\u00df der doch h\u00fclffe schicken", "tokens": ["Ach", "!", "m\u00f6cht", "vn\u00df", "der", "doch", "h\u00fclf\u00b7fe", "schi\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "APPR", "ART", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der sich in Sion hat verliebet", "tokens": ["Der", "sich", "in", "Si\u00b7on", "hat", "ver\u00b7lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NE", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Vnd bricht was Jsrael betr\u00fcbet.", "tokens": ["Vnd", "bricht", "was", "Js\u00b7rael", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}