{"textgrid.poem.55355": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ins Einzelne", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit vielen Jahren hab ich still", "tokens": ["Seit", "vie\u00b7len", "Jah\u00b7ren", "hab", "ich", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu eurem Tun geschwiegen,", "tokens": ["Zu", "eu\u00b7rem", "Tun", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sich am Tag und Tageswill", "tokens": ["Das", "sich", "am", "Tag", "und", "Ta\u00b7ges\u00b7will"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4llig mag vergn\u00fcgen.", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "mag", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr denkt, woher der Wind auch weht", "tokens": ["Ihr", "denkt", ",", "wo\u00b7her", "der", "Wind", "auch", "weht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Schaden und Gewinne,", "tokens": ["Zu", "Scha\u00b7den", "und", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "mete