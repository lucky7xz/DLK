{"dta.poem.23493": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von rc. beeden  \n Fr\u00e4wlin rc. Agnes vnd  \n Anna Hertzoginen zu Wir-  \n temberg rc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sjh wie anmutig/ sch\u00f6n vnd rein/", "tokens": ["Sjh", "wie", "an\u00b7mu\u00b7tig", "/", "sch\u00f6n", "vnd", "rein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit wie vngewohnlichem schein", "tokens": ["Mit", "wie", "vn\u00b7ge\u00b7wohn\u00b7li\u00b7chem", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Jetzuud die Morgenr\u00f6ht aufgehet/", "tokens": ["Jet\u00b7zu\u00b7ud", "die", "Mor\u00b7gen\u00b7r\u00f6ht", "auf\u00b7ge\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Vnd alles mit lust \u00fcbersehet", "tokens": ["Vnd", "al\u00b7les", "mit", "lust", "\u00fc\u00b7ber\u00b7se\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aber nein. So h\u00fcpsch w\u00e4r sie nicht/", "tokens": ["A\u00b7ber", "nein", ".", "So", "h\u00fcpsch", "w\u00e4r", "sie", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Sie thet dan solche sch\u00f6nheit borgen", "tokens": ["Sie", "thet", "dan", "sol\u00b7che", "sch\u00f6n\u00b7heit", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von Anna/ deren angesicht", "tokens": ["Von", "An\u00b7na", "/", "de\u00b7ren", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$(", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist sch\u00f6ner dan der sch\u00f6nste morgen.", "tokens": ["Ist", "sch\u00f6\u00b7ner", "dan", "der", "sch\u00f6ns\u00b7te", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}