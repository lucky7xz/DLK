{"textgrid.poem.54043": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wenn der b\u00f6se Feind kommt, kriechen alle", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn der b\u00f6se Feind kommt, kriechen alle", "tokens": ["Und", "wenn", "der", "b\u00f6\u00b7se", "Feind", "kommt", ",", "krie\u00b7chen", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "rasch hinter diesen breiten, alten Mann . . .", "tokens": ["rasch", "hin\u00b7ter", "die\u00b7sen", "brei\u00b7ten", ",", "al\u00b7ten", "Mann", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedeckt sehn sie auf die Krawalle", "tokens": ["Ge\u00b7deckt", "sehn", "sie", "auf", "die", "Kra\u00b7wal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "und freun sich, da\u00df man ihnen da nichts machen kann.", "tokens": ["und", "freun", "sich", ",", "da\u00df", "man", "ih\u00b7nen", "da", "nichts", "ma\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PIS", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie sch\u00fctzen nicht. Sie lassen sich nur sch\u00fctzen.", "tokens": ["Sie", "sch\u00fct\u00b7zen", "nicht", ".", "Sie", "las\u00b7sen", "sich", "nur", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie starren wie behext: \u00bbDie Augen links!\u00ab", "tokens": ["Sie", "star\u00b7ren", "wie", "be\u00b7hext", ":", "\u00bb", "Die", "Au\u00b7gen", "links", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVFIN", "$.", "$(", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "dann ziehn sie \u00fcbers Ohr die Zipfelm\u00fctzen \u2013", "tokens": ["dann", "ziehn", "sie", "\u00fc\u00b7bers", "Ohr", "die", "Zip\u00b7fel\u00b7m\u00fct\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "es l\u00e4chelt stumm die Paragraphen-Sphinx.", "tokens": ["es", "l\u00e4\u00b7chelt", "stumm", "die", "Pa\u00b7ra\u00b7gra\u00b7phen\u00b7\u00b7S\u00b7phinx", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Sie sch\u00fctzen nicht. Sie machen t\u00e4glich Pleite.", "tokens": ["Sie", "sch\u00fct\u00b7zen", "nicht", ".", "Sie", "ma\u00b7chen", "t\u00e4g\u00b7lich", "Plei\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure