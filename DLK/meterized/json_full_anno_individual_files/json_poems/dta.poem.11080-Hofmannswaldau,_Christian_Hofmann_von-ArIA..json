{"dta.poem.11080": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "ArIA.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein verh\u00e4ngni\u00df! soll ich brennen,", "tokens": ["Mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "!", "soll", "ich", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und doch ohne flammen seyn?", "tokens": ["Und", "doch", "oh\u00b7ne", "flam\u00b7men", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird man nicht die asche kennen,", "tokens": ["Wird", "man", "nicht", "die", "asc\u00b7he", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man hertzen \u00e4schert ein?", "tokens": ["Wo", "man", "hert\u00b7zen", "\u00e4sc\u00b7hert", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVIZU", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Ich bin kranck am liebes-fieber,", "tokens": ["Ich", "bin", "kranck", "am", "lie\u00b7bes\u00b7fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPRART", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "So ich doch verschweigen soll;", "tokens": ["So", "ich", "doch", "ver\u00b7schwei\u00b7gen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Geht der mund nicht dessen \u00fcber,", "tokens": ["Geht", "der", "mund", "nicht", "des\u00b7sen", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "PDS", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wessen unser hertze voll?", "tokens": ["Wes\u00b7sen", "un\u00b7ser", "hert\u00b7ze", "voll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich liebe, kan ich sehen,", "tokens": ["Was", "ich", "lie\u00b7be", ",", "kan", "ich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mu\u00df thun, als s\u00e4h ichs nicht.", "tokens": ["Und", "mu\u00df", "thun", ",", "als", "s\u00e4h", "ichs", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$,", "KOUS", "ADJD", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mein gem\u00fcth darff nicht gestehen,", "tokens": ["Mein", "ge\u00b7m\u00fcth", "darff", "nicht", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was aus meinen augen bricht.", "tokens": ["Was", "aus", "mei\u00b7nen", "au\u00b7gen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Uber k\u00e4lte mu\u00df ich klagen,", "tokens": ["U\u00b7ber", "k\u00e4l\u00b7te", "mu\u00df", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn mich qu\u00e4let feuers-gluth;", "tokens": ["Wenn", "mich", "qu\u00e4\u00b7let", "feu\u00b7er\u00b7sgluth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch ein bloser blick wird sagen,", "tokens": ["Doch", "ein", "blo\u00b7ser", "blick", "wird", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich besteh aus fleisch und blut.", "tokens": ["Ich", "be\u00b7steh", "aus", "fleisch", "und", "blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber wie wird meine liebe", "tokens": ["A\u00b7ber", "wie", "wird", "mei\u00b7ne", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch was str\u00e4fliches versucht!", "tokens": ["Durch", "was", "str\u00e4f\u00b7li\u00b7ches", "ver\u00b7sucht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich bin Adams kind und riebe,", "tokens": ["Ich", "bin", "A\u00b7dams", "kind", "und", "rie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Greiffe nach verbotner frucht.", "tokens": ["Greif\u00b7fe", "nach", "ver\u00b7bot\u00b7ner", "frucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer ist starck zu widerstreben,", "tokens": ["Wer", "ist", "starck", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die sch\u00f6nheit auf uns dringt?", "tokens": ["Wenn", "die", "sch\u00f6n\u00b7heit", "auf", "uns", "dringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der die liebligkeit gegeben,", "tokens": ["Der", "die", "lieb\u00b7lig\u00b7keit", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wei\u00df auch, da\u00df dieselbe zwingt.", "tokens": ["Wei\u00df", "auch", ",", "da\u00df", "die\u00b7sel\u00b7be", "zwingt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PDAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}