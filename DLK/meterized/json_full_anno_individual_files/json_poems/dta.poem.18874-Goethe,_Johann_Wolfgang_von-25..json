{"dta.poem.18874": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Reiche den Becher dann", "tokens": ["Rei\u00b7che", "den", "Be\u00b7cher", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "O! Sawad Ben Amre:", "tokens": ["O", "!", "Sa\u00b7wad", "Ben", "Am\u00b7re", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "NE", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn mein K\u00f6rper um des Oheims willen", "tokens": ["Denn", "mein", "K\u00f6r\u00b7per", "um", "des", "O\u00b7heims", "wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist eine grosse Wunde.", "tokens": ["Ist", "ei\u00b7ne", "gros\u00b7se", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}