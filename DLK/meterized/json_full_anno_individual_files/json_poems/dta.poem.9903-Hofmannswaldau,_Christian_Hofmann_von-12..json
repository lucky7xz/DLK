{"dta.poem.9903": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man vertraut uns solche dinge die sonst keiner wi\u00dfe\u0303 soll/ (wol/", "tokens": ["Man", "ver\u00b7traut", "uns", "sol\u00b7che", "din\u00b7ge", "die", "sonst", "kei\u00b7ner", "wi\u00df\u1ebd", "soll", "/", "(", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "ART", "ADV", "PIS", "VVINF", "VMFIN", "$(", "$(", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Weil nichts kom\u0303t aus unserm munde/ traut ein ieder uns gar", "tokens": ["Weil", "nichts", "kom\u0303t", "aus", "un\u00b7serm", "mun\u00b7de", "/", "traut", "ein", "ie\u00b7der", "uns", "gar"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "VVFIN", "ART", "PIS", "PPER", "ADV"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Bey dem hof und in der stadt schm\u00fcckt man uns mit gold und", "tokens": ["Bey", "dem", "hof", "und", "in", "der", "stadt", "schm\u00fcckt", "man", "uns", "mit", "gold", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "PIS", "PRF", "APPR", "NN", "KON"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "steinen/", "tokens": ["stei\u00b7nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da gesang und saiten-spiel uns viel angenehmer scheinen.", "tokens": ["Da", "ge\u00b7sang", "und", "sai\u00b7ten\u00b7spiel", "uns", "viel", "an\u00b7ge\u00b7neh\u00b7mer", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+----+-+-+-", "measure": "anapaest.init"}}}}}