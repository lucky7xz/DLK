{"textgrid.poem.26336": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der graue Geist", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen leuchtet voll Vertrauen,", "tokens": ["Der", "Mor\u00b7gen", "leuch\u00b7tet", "voll", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00f6hen friedlich sich beschauen.", "tokens": ["Die", "H\u00f6\u00b7hen", "fried\u00b7lich", "sich", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort auf dem Bergkamm", "tokens": ["Dort", "auf", "dem", "Berg\u00b7kamm"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf dem frischbetauten Rasen", "tokens": ["Auf", "dem", "frischbe\u00b7tau\u00b7ten", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drei blanke K\u00fche ernst geruhsam grasen,", "tokens": ["Drei", "blan\u00b7ke", "K\u00fc\u00b7he", "ernst", "ge\u00b7ruh\u00b7sam", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stehn aufgerichtet k\u00fchn,", "tokens": ["Stehn", "auf\u00b7ge\u00b7rich\u00b7tet", "k\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "$,"], "me