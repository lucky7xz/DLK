{"textgrid.poem.56005": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Alchimist", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsam verl\u00e4chelnd schob der Laborant", "tokens": ["Selt\u00b7sam", "ver\u00b7l\u00e4\u00b7chelnd", "schob", "der", "La\u00b7bo\u00b7rant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "den Kolben fort, der halbberuhigt rauchte.", "tokens": ["den", "Kol\u00b7ben", "fort", ",", "der", "halb\u00b7be\u00b7ru\u00b7higt", "rauch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wu\u00dfte jetzt, was er noch brauchte,", "tokens": ["Er", "wu\u00df\u00b7te", "jetzt", ",", "was", "er", "noch", "brauch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "damit der sehr erlauchte Gegenstand", "tokens": ["da\u00b7mit", "der", "sehr", "er\u00b7lauch\u00b7te", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "da drin entst\u00e4nde. Zeiten brauchte er,", "tokens": ["da", "drin", "ent\u00b7st\u00e4n\u00b7de", ".", "Zei\u00b7ten", "brauch\u00b7te", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$.", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Jahrtausende f\u00fcr sich und diese Birne", "tokens": ["Jahr\u00b7tau\u00b7sen\u00b7de", "f\u00fcr", "sich", "und", "die\u00b7se", "Bir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in der es brodelte; im Hirn Gestirne", "tokens": ["in", "der", "es", "bro\u00b7del\u00b7te", ";", "im", "Hirn", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$.", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und im Bewu\u00dftsein mindestens das Meer.", "tokens": ["und", "im", "Be\u00b7wu\u00df\u00b7tsein", "min\u00b7des\u00b7tens", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Ungeheuere, das er gewollt,", "tokens": ["Das", "Un\u00b7ge\u00b7heu\u00b7e\u00b7re", ",", "das", "er", "ge\u00b7wollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VMPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "er lie\u00df es los in dieser Nacht. Es kehrte", "tokens": ["er", "lie\u00df", "es", "los", "in", "die\u00b7ser", "Nacht", ".", "Es", "kehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zur\u00fcck zu Gott und in sein altes Ma\u00df;", "tokens": ["zu\u00b7r\u00fcck", "zu", "Gott", "und", "in", "sein", "al\u00b7tes", "Ma\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "er aber, lallend wie ein Trunkenbold,", "tokens": ["er", "a\u00b7ber", ",", "lal\u00b7lend", "wie", "ein", "Trun\u00b7ken\u00b7bold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "lag \u00fcber dem Geheimfach und begehrte", "tokens": ["lag", "\u00fc\u00b7ber", "dem", "Ge\u00b7heim\u00b7fach", "und", "be\u00b7gehr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den Brocken Gold, den er besa\u00df.", "tokens": ["den", "Bro\u00b7cken", "Gold", ",", "den", "er", "be\u00b7sa\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seltsam verl\u00e4chelnd schob der Laborant", "tokens": ["Selt\u00b7sam", "ver\u00b7l\u00e4\u00b7chelnd", "schob", "der", "La\u00b7bo\u00b7rant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "den Kolben fort, der halbberuhigt rauchte.", "tokens": ["den", "Kol\u00b7ben", "fort", ",", "der", "halb\u00b7be\u00b7ru\u00b7higt", "rauch\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "meas