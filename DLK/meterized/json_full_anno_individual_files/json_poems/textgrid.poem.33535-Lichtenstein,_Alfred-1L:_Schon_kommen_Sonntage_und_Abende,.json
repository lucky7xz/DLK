{"textgrid.poem.33535": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schon kommen Sonntage und Abende,", "genre": "verse", "period": "N.A.", "pub_year": 1902, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon kommen Sonntage und Abende,", "tokens": ["Schon", "kom\u00b7men", "Sonn\u00b7ta\u00b7ge", "und", "A\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In denen ich ganz leer und lustlos schreite,", "tokens": ["In", "de\u00b7nen", "ich", "ganz", "leer", "und", "lust\u00b7los", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ganz gl\u00e4sern bin, zum Spa\u00df mit Hunden spiele,", "tokens": ["Ganz", "gl\u00e4\u00b7sern", "bin", ",", "zum", "Spa\u00df", "mit", "Hun\u00b7den", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "$,", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach, oder kleine Steine, die ich fand,", "tokens": ["Ach", ",", "o\u00b7der", "klei\u00b7ne", "Stei\u00b7ne", ",", "die", "ich", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "M\u00fchsam und sinnlos durch die Stra\u00dfen schleife.", "tokens": ["M\u00fch\u00b7sam", "und", "sinn\u00b7los", "durch", "die", "Stra\u00b7\u00dfen", "schlei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Oft steh ich auch an meinen Fenstern faul herum,", "tokens": ["Oft", "steh", "ich", "auch", "an", "mei\u00b7nen", "Fens\u00b7tern", "faul", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Unschl\u00fcssig: soll ich nun in Bierlokalen", "tokens": ["Un\u00b7schl\u00fcs\u00b7sig", ":", "soll", "ich", "nun", "in", "Bier\u00b7lo\u00b7ka\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Kameraden runden Stumpfsinn pflegen,", "tokens": ["Mit", "Ka\u00b7me\u00b7ra\u00b7den", "run\u00b7den", "Stumpf\u00b7sinn", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In flinken Kinos meine m\u00fcden", "tokens": ["In", "flin\u00b7ken", "Ki\u00b7nos", "mei\u00b7ne", "m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Elenden Stunden t\u00f6ten und zum Zeitvertreib", "tokens": ["E\u00b7len\u00b7den", "Stun\u00b7den", "t\u00f6\u00b7ten", "und", "zum", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "KON", "APPRART", "NN"], "me