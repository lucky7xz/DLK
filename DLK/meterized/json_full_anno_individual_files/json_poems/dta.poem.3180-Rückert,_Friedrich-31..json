{"dta.poem.3180": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was willst du mit der Welt? Du kannst sie nicht durchmessen,", "tokens": ["Was", "willst", "du", "mit", "der", "Welt", "?", "Du", "kannst", "sie", "nicht", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dein enges Herz sie nicht zusammenpressen.", "tokens": ["Und", "in", "dein", "en\u00b7ges", "Herz", "sie", "nicht", "zu\u00b7sam\u00b7men\u00b7pres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du l\u00f6sest sie nicht auf, der R\u00e4thsel sind zu viele,", "tokens": ["Du", "l\u00f6\u00b7sest", "sie", "nicht", "auf", ",", "der", "R\u00e4th\u00b7sel", "sind", "zu", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "ART", "NN", "VAFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch lenkest ihren Lauf, sie rennt nach eignem Ziele.", "tokens": ["Noch", "len\u00b7kest", "ih\u00b7ren", "Lauf", ",", "sie", "rennt", "nach", "eig\u00b7nem", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohlauf, so viel du kannst, mit Lieb' und Geist zu fassen,", "tokens": ["Wohl\u00b7auf", ",", "so", "viel", "du", "kannst", ",", "mit", "Lieb'", "und", "Geist", "zu", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was du nicht begreifst, dahin gestellt zu lassen.", "tokens": ["Und", "was", "du", "nicht", "be\u00b7greifst", ",", "da\u00b7hin", "ge\u00b7stellt", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVFIN", "$,", "PAV", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie Kr\u00e4mer ihre Waar', auch deine sollst du tauschen,", "tokens": ["Wie", "Kr\u00e4\u00b7mer", "ih\u00b7re", "Waa\u00b7r'", ",", "auch", "dei\u00b7ne", "sollst", "du", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Versenden Liebesgr\u00fc\u00df', und der Erwidrung lauschen.", "tokens": ["Ver\u00b7sen\u00b7den", "Lie\u00b7bes\u00b7gr\u00fc\u00df'", ",", "und", "der", "Er\u00b7wid\u00b7rung", "lau\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Ich sende diesen Gru\u00df, und sage nicht, wohin?", "tokens": ["Ich", "sen\u00b7de", "die\u00b7sen", "Gru\u00df", ",", "und", "sa\u00b7ge", "nicht", ",", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wissen m\u00f6cht' ich, ob ich dort willkommen bin.", "tokens": ["Doch", "wis\u00b7sen", "m\u00f6cht'", "ich", ",", "ob", "ich", "dort", "will\u00b7kom\u00b7men", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}