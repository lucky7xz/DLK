{"textgrid.poem.34869": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnachtw\u00e4chter mit langen Fortschrittsbeinen,", "tokens": ["\u00bb", "nacht\u00b7w\u00e4ch\u00b7ter", "mit", "lan\u00b7gen", "Fort\u00b7schritts\u00b7bei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du kommst so verst\u00f6rt einhergerannt!", "tokens": ["Du", "kommst", "so", "ver\u00b7st\u00f6rt", "ein\u00b7her\u00b7ge\u00b7rannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie geht es daheim den lieben Meinen,", "tokens": ["Wie", "geht", "es", "da\u00b7heim", "den", "lie\u00b7ben", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist schon befreit das Vaterland?\u00ab", "tokens": ["Ist", "schon", "be\u00b7freit", "das", "Va\u00b7ter\u00b7land", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbvortrefflich geht es, der stille Segen,", "tokens": ["\u00bb", "vor\u00b7treff\u00b7lich", "geht", "es", ",", "der", "stil\u00b7le", "Se\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wuchert im sittlich geh\u00fcteten Haus,", "tokens": ["Er", "wu\u00b7chert", "im", "sitt\u00b7lich", "ge\u00b7h\u00fc\u00b7te\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und ruhig und sicher, auf friedlichen Wegen,", "tokens": ["Und", "ru\u00b7hig", "und", "si\u00b7cher", ",", "auf", "fried\u00b7li\u00b7chen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Entwickelt sich Deutschland von innen heraus.", "tokens": ["Ent\u00b7wi\u00b7ckelt", "sich", "Deutschland", "von", "in\u00b7nen", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht oberfl\u00e4chlich wie Frankreich bl\u00fcht es,", "tokens": ["Nicht", "o\u00b7berf\u00b7l\u00e4ch\u00b7lich", "wie", "Fran\u00b7kreich", "bl\u00fcht", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Freiheit das \u00e4u\u00dfere Leben bewegt;", "tokens": ["Wo", "Frei\u00b7heit", "das", "\u00e4u\u00b7\u00dfe\u00b7re", "Le\u00b7ben", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nur in der Tiefe des Gem\u00fctes", "tokens": ["Nur", "in", "der", "Tie\u00b7fe", "des", "Ge\u00b7m\u00fc\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein deutscher Mann die Freiheit tr\u00e4gt.", "tokens": ["Ein", "deut\u00b7scher", "Mann", "die", "Frei\u00b7heit", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Dom zu K\u00f6llen wird vollendet,", "tokens": ["Der", "Dom", "zu", "K\u00f6l\u00b7len", "wird", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Hohenzollern verdanken wir das;", "tokens": ["Den", "Ho\u00b7hen\u00b7zol\u00b7lern", "ver\u00b7dan\u00b7ken", "wir", "das", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDS", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Habsburg hat auch dazu gespendet,", "tokens": ["Habs\u00b7burg", "hat", "auch", "da\u00b7zu", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wittelsbach schickt Fensterglas.", "tokens": ["Ein", "Wit\u00b7tels\u00b7bach", "schickt", "Fens\u00b7ter\u00b7glas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Konstitution, die Freiheitsgesetze,", "tokens": ["Die", "Kons\u00b7ti\u00b7tu\u00b7ti\u00b7on", ",", "die", "Frei\u00b7heits\u00b7ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sind uns versprochen, wir haben das Wort,", "tokens": ["Sie", "sind", "uns", "ver\u00b7spro\u00b7chen", ",", "wir", "ha\u00b7ben", "das", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und K\u00f6nigsworte, das sind Sch\u00e4tze,", "tokens": ["Und", "K\u00f6\u00b7nigs\u00b7wor\u00b7te", ",", "das", "sind", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie tief im Rhein der Niblungshort.", "tokens": ["Wie", "tief", "im", "Rhein", "der", "Nib\u00b7lungs\u00b7hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der freie Rhein, der Brutus der Fl\u00fcsse,", "tokens": ["Der", "frei\u00b7e", "Rhein", ",", "der", "Bru\u00b7tus", "der", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wird uns nimmermehr geraubt!", "tokens": ["Er", "wird", "uns", "nim\u00b7mer\u00b7mehr", "ge\u00b7raubt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Holl\u00e4nder binden ihm die F\u00fc\u00dfe,", "tokens": ["Die", "Hol\u00b7l\u00e4n\u00b7der", "bin\u00b7den", "ihm", "die", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Schwyzer halten fest sein Haupt.", "tokens": ["Die", "Schwy\u00b7zer", "hal\u00b7ten", "fest", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch eine Flotte will Gott uns bescheren,", "tokens": ["Auch", "ei\u00b7ne", "Flot\u00b7te", "will", "Gott", "uns", "be\u00b7sche\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die patriotische \u00dcberkraft", "tokens": ["Die", "pat\u00b7ri\u00b7o\u00b7ti\u00b7sche", "\u00dc\u00b7ber\u00b7kraft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wird lustig rudern auf deutschen Galeeren;", "tokens": ["Wird", "lus\u00b7tig", "ru\u00b7dern", "auf", "deut\u00b7schen", "Ga\u00b7lee\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Festungsstrafe wird abgeschafft.", "tokens": ["Die", "Fes\u00b7tungs\u00b7stra\u00b7fe", "wird", "ab\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Es bl\u00fcht der Lenz, es platzen die Schoten,", "tokens": ["Es", "bl\u00fcht", "der", "Lenz", ",", "es", "plat\u00b7zen", "die", "Scho\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir atmen frei in der freien Natur!", "tokens": ["Wir", "at\u00b7men", "frei", "in", "der", "frei\u00b7en", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wird uns der ganze Verlag verboten,", "tokens": ["Und", "wird", "uns", "der", "gan\u00b7ze", "Ver\u00b7lag", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So schwindet am Ende von selbst die Zensur.\u00ab", "tokens": ["So", "schwin\u00b7det", "am", "En\u00b7de", "von", "selbst", "die", "Zen\u00b7sur", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbnachtw\u00e4chter mit langen Fortschrittsbeinen,", "tokens": ["\u00bb", "nacht\u00b7w\u00e4ch\u00b7ter", "mit", "lan\u00b7gen", "Fort\u00b7schritts\u00b7bei\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "