{"textgrid.poem.24107": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Spinne", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Augen waren n\u00e4chten aufgethan,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "wa\u00b7ren", "n\u00e4ch\u00b7ten", "auf\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Starr im tiefen Traume, einem Riesenplan.", "tokens": ["Starr", "im", "tie\u00b7fen", "Trau\u00b7me", ",", "ei\u00b7nem", "Rie\u00b7sen\u00b7plan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Eine Ebene war es unerme\u00dflich weit,", "tokens": ["Ei\u00b7ne", "E\u00b7be\u00b7ne", "war", "es", "un\u00b7er\u00b7me\u00df\u00b7lich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und mein Auge sahe die Unendlichkeit.", "tokens": ["Und", "mein", "Au\u00b7ge", "sa\u00b7he", "die", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "War wie Blei so grau, war wie Blei so schwer,", "tokens": ["War", "wie", "Blei", "so", "grau", ",", "war", "wie", "Blei", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"