{"textgrid.poem.35367": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Scheidens Angstruf ist von meinem Munde", "tokens": ["Des", "Schei\u00b7dens", "Angs\u00b7truf", "ist", "von", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verbannt und l\u00e4ngst zerrissen das Gewebe", "tokens": ["Ver\u00b7bannt", "und", "l\u00e4ngst", "zer\u00b7ris\u00b7sen", "das", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der tr\u00fcgerischen Tr\u00e4ume; doch ich bebe,", "tokens": ["Der", "tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "Tr\u00e4u\u00b7me", ";", "doch", "ich", "be\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "O Mutter Erde, vor der letzten Stunde.", "tokens": ["O", "Mut\u00b7ter", "Er\u00b7de", ",", "vor", "der", "letz\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin geknickt in meinem letzten Stolze,", "tokens": ["Ich", "bin", "ge\u00b7knickt", "in", "mei\u00b7nem", "letz\u00b7ten", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil du zur\u00fcckverlangst, was dir entsprossen;", "tokens": ["Weil", "du", "zu\u00b7r\u00fcck\u00b7ver\u00b7langst", ",", "was", "dir", "ent\u00b7spros\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weil du den Keim, der langsam aufgeschossen,", "tokens": ["Weil", "du", "den", "Keim", ",", "der", "lang\u00b7sam", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Langsam verderben mu\u00dft in faulem Holze;", "tokens": ["Lang\u00b7sam", "ver\u00b7der\u00b7ben", "mu\u00dft", "in", "fau\u00b7lem", "Hol\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Weil du die kalte Form in lose Fetzen", "tokens": ["Weil", "du", "die", "kal\u00b7te", "Form", "in", "lo\u00b7se", "Fet\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zerlegst, den welken K\u00f6rper, keusch verh\u00fcllt;", "tokens": ["Zer\u00b7legst", ",", "den", "wel\u00b7ken", "K\u00f6r\u00b7per", ",", "keusch", "ver\u00b7h\u00fcllt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Weil Zorn und Scham, weil Grausen mich erf\u00fcllt", "tokens": ["Weil", "Zorn", "und", "Scham", ",", "weil", "Grau\u00b7sen", "mich", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "KOUS", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Vor eines Grabes Ekel und Entsetzen;", "tokens": ["Vor", "ei\u00b7nes", "Gra\u00b7bes", "E\u00b7kel", "und", "Ent\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und weil ich \u2013 o des namenlosen Jammers! \u2013", "tokens": ["Und", "weil", "ich", "\u2013", "o", "des", "na\u00b7men\u00b7lo\u00b7sen", "Jam\u00b7mers", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "$(", "FM", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "An jene denke, die man fortgetragen,", "tokens": ["An", "je\u00b7ne", "den\u00b7ke", ",", "die", "man", "fort\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Als, nach den Donnerschl\u00e4gen eines Hammers,", "tokens": ["Als", ",", "nach", "den", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", "ei\u00b7nes", "Ham\u00b7mers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie dennoch ruhig auf den Sp\u00e4nen lagen", "tokens": ["Sie", "den\u00b7noch", "ru\u00b7hig", "auf", "den", "Sp\u00e4\u00b7nen", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und endlich aus der Ohnmacht sich erhuben,", "tokens": ["Und", "end\u00b7lich", "aus", "der", "Ohn\u00b7macht", "sich", "er\u00b7hu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nachdem die Stricke schon emporgeflogen,", "tokens": ["Nach\u00b7dem", "die", "Stri\u00b7cke", "schon", "em\u00b7por\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Nachdem der Schaufeln Arbeit schon vollzogen,", "tokens": ["Nach\u00b7dem", "der", "Schau\u00b7feln", "Ar\u00b7beit", "schon", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und dann die N\u00e4gel in die S\u00e4rge gruben. \u2013", "tokens": ["Und", "dann", "die", "N\u00e4\u00b7gel", "in", "die", "S\u00e4r\u00b7ge", "gru\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ich will nicht weiter sinnen. \u2013 Sei zerst\u00f6rt,", "tokens": ["Ich", "will", "nicht", "wei\u00b7ter", "sin\u00b7nen", ".", "\u2013", "Sei", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$.", "$(", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Bild der Verzweiflung, kehre niemals wieder! \u2013", "tokens": ["Bild", "der", "Ver\u00b7zwei\u00b7flung", ",", "keh\u00b7re", "nie\u00b7mals", "wie\u00b7der", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "Ich lege Pinsel und Palette nieder,", "tokens": ["Ich", "le\u00b7ge", "Pin\u00b7sel", "und", "Pa\u00b7let\u00b7te", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wenn mein Gedanke dich heraufbeschw\u00f6rt.", "tokens": ["Wenn", "mein", "Ge\u00b7dan\u00b7ke", "dich", "her\u00b7auf\u00b7be\u00b7schw\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Entweiche, grauenhafter Gast, vor dessen", "tokens": ["Ent\u00b7wei\u00b7che", ",", "grau\u00b7en\u00b7haf\u00b7ter", "Gast", ",", "vor", "des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Gorgonen-Antlitz meine Pulse stocken!", "tokens": ["Gor\u00b7go\u00b7nen\u00b7Ant\u00b7litz", "mei\u00b7ne", "Pul\u00b7se", "sto\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Verschlinge, Grab, die hingeworfnen Brocken", "tokens": ["Ver\u00b7schlin\u00b7ge", ",", "Grab", ",", "die", "hin\u00b7ge\u00b7worf\u00b7nen", "Bro\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Nur dann, wenn sie von F\u00e4ulnis angefressen!", "tokens": ["Nur", "dann", ",", "wenn", "sie", "von", "F\u00e4ul\u00b7nis", "an\u00b7ge\u00b7fres\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Es w\u00e4re besser, himmelan zu lodern,", "tokens": ["Es", "w\u00e4\u00b7re", "bes\u00b7ser", ",", "him\u00b7me\u00b7lan", "zu", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Gereinigt und vertilgt durch Feuerbr\u00e4nde,", "tokens": ["Ge\u00b7rei\u00b7nigt", "und", "ver\u00b7tilgt", "durch", "Feu\u00b7er\u00b7br\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Statt eingesperrt in eines Sarges W\u00e4nde", "tokens": ["Statt", "ein\u00b7ge\u00b7sperrt", "in", "ei\u00b7nes", "Sar\u00b7ges", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "In feuchter Erde langsam zu vermodern.", "tokens": ["In", "feuch\u00b7ter", "Er\u00b7de", "lang\u00b7sam", "zu", "ver\u00b7mo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Gemildert wird des Scheidens Bitterkeit,", "tokens": ["Ge\u00b7mil\u00b7dert", "wird", "des", "Schei\u00b7dens", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Der Hinterbliebnen Schmerz, wenn Ueberreste", "tokens": ["Der", "Hin\u00b7ter\u00b7blieb\u00b7nen", "Schmerz", ",", "wenn", "Ue\u00b7ber\u00b7res\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Der Menschen in dem ew'gen Sch\u00f6pfungsfeste", "tokens": ["Der", "Men\u00b7schen", "in", "dem", "ew'\u00b7gen", "Sch\u00f6p\u00b7fungs\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Verbrennen, von der Erde Last befreit.", "tokens": ["Ver\u00b7bren\u00b7nen", ",", "von", "der", "Er\u00b7de", "Last", "be\u00b7freit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Es mag die Nachwelt ihre Toten taufen,", "tokens": ["Es", "mag", "die", "Nach\u00b7welt", "ih\u00b7re", "To\u00b7ten", "tau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Wie sichs allein geb\u00fchrt, im Flammenbade;", "tokens": ["Wie", "sichs", "al\u00b7lein", "ge\u00b7b\u00fchrt", ",", "im", "Flam\u00b7men\u00b7ba\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVPP", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Denn eines reinen Glaubens letzte Gnade", "tokens": ["Denn", "ei\u00b7nes", "rei\u00b7nen", "Glau\u00b7bens", "letz\u00b7te", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Ist nicht das Grab, es ist der Scheiterhaufen.", "tokens": ["Ist", "nicht", "das", "Grab", ",", "es", "ist", "der", "Schei\u00b7ter\u00b7hau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Scheidens Angstruf ist von meinem Munde", "tokens": ["Des", "Schei\u00b7dens", "Angs\u00b7truf", "ist", "von", "mei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verbannt und l\u00e4ngst zerrissen das Gewebe", "tokens": ["Ver\u00b7bannt", "und", "l\u00e4ngst", "zer\u00b7ris\u00b7sen", "das", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der tr\u00fcgerischen Tr\u00e4ume; doch ich bebe,", "tokens": ["Der", "tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "Tr\u00e4u\u00b7me", ";", "doch", "ich", "be\