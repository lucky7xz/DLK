{"textgrid.poem.43024": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "12. Nachtphantasien", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah mich in Tr\u00e4umen der Mitternacht", "tokens": ["Ich", "sah", "mich", "in", "Tr\u00e4u\u00b7men", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verlassen und verachtet!", "tokens": ["Ver\u00b7las\u00b7sen", "und", "ver\u00b7ach\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Auges milde Glut und Pracht,", "tokens": ["Des", "Au\u00b7ges", "mil\u00b7de", "Glut", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das liebend einst \u00fcber mir gewacht,", "tokens": ["Das", "lie\u00b7bend", "einst", "\u00fc\u00b7ber", "mir", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich sah es von Ha\u00df umnachtet!", "tokens": ["Ich", "sah", "es", "von", "Ha\u00df", "um\u00b7nach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mir malte der irre Gedankenflug", "tokens": ["Mir", "mal\u00b7te", "der", "ir\u00b7re", "Ge\u00b7dan\u00b7ken\u00b7flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gestalten bleich und tr\u00fcbe!", "tokens": ["Ge\u00b7stal\u00b7ten", "bleich", "und", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah einen finstern Leichenzug;", "tokens": ["Ich", "sah", "ei\u00b7nen", "fins\u00b7tern", "Lei\u00b7chen\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Leiche, die man vor\u00fcbertrug,", "tokens": ["Die", "Lei\u00b7che", ",", "die", "man", "vor\u00b7\u00fc\u00b7bert\u00b7rug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "War uns're gestorbene Liebe.", "tokens": ["War", "un\u00b7s'\u00b7re", "ge\u00b7stor\u00b7be\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Entflieh', du gespenstische Mitternacht!", "tokens": ["Ent\u00b7flieh'", ",", "du", "ge\u00b7spens\u00b7ti\u00b7sche", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entflieht, ihr blassen Gestalten!", "tokens": ["Ent\u00b7flieht", ",", "ihr", "blas\u00b7sen", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis der selige, fr\u00f6hliche Tag erwacht,", "tokens": ["Bis", "der", "se\u00b7li\u00b7ge", ",", "fr\u00f6h\u00b7li\u00b7che", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bis Leben und Liebe mit frischer Macht", "tokens": ["Bis", "Le\u00b7ben", "und", "Lie\u00b7be", "mit", "fri\u00b7scher", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mich jauchzend umschlungen halten.", "tokens": ["Mich", "jauch\u00b7zend", "um\u00b7schlun\u00b7gen", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie liebt' ich die sch\u00f6ne, heilige Nacht,", "tokens": ["Wie", "liebt'", "ich", "die", "sch\u00f6\u00b7ne", ",", "hei\u00b7li\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn die b\u00f6sen Tr\u00e4ume nicht w\u00e4ren!", "tokens": ["Wenn", "die", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", "nicht", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unheimlicher Geister wilde Jagd", "tokens": ["Un\u00b7heim\u00b7li\u00b7cher", "Geis\u00b7ter", "wil\u00b7de", "Jagd"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verfolgt mich, bis ich, vom Schlaf erwacht,", "tokens": ["Ver\u00b7folgt", "mich", ",", "bis", "ich", ",", "vom", "Schlaf", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mich bade in hei\u00dfen Z\u00e4hren.", "tokens": ["Mich", "ba\u00b7de", "in", "hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich f\u00fchl' mich allein in der weiten Welt;", "tokens": ["Ich", "f\u00fchl'", "mich", "al\u00b7lein", "in", "der", "wei\u00b7ten", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was ich liebe, ist fremd und ferne!", "tokens": ["Was", "ich", "lie\u00b7be", ",", "ist", "fremd", "und", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJA", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da scheint mir der Mond am Himmelszelt", "tokens": ["Da", "scheint", "mir", "der", "Mond", "am", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein sp\u00e4hender Lauscher hingestellt,", "tokens": ["Ein", "sp\u00e4\u00b7hen\u00b7der", "Lau\u00b7scher", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Spione die ewigen Sterne!", "tokens": ["Und", "Spi\u00b7o\u00b7ne", "die", "e\u00b7wi\u00b7gen", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich liebe die Nacht; ich liebe die Nacht!", "tokens": ["Ich", "lie\u00b7be", "die", "Nacht", ";", "ich", "lie\u00b7be", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch nicht die einsame, tr\u00fcbe!", "tokens": ["Doch", "nicht", "die", "ein\u00b7sa\u00b7me", ",", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, die aus seligen Augen lacht,", "tokens": ["Nein", ",", "die", "aus", "se\u00b7li\u00b7gen", "Au\u00b7gen", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "In flammender Pracht, in Zaubermacht,", "tokens": ["In", "flam\u00b7men\u00b7der", "Pracht", ",", "in", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die heilige Nacht der Liebe.", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Nacht", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Es mahne der Tod mich, der finst're, bleiche,", "tokens": ["Es", "mah\u00b7ne", "der", "Tod", "mich", ",", "der", "finst'\u00b7re", ",", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "An das Leben, das lichte, das reiche,", "tokens": ["An", "das", "Le\u00b7ben", ",", "das", "lich\u00b7te", ",", "das", "rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "An den heitern Genius der Welt!", "tokens": ["An", "den", "hei\u00b7tern", "Ge\u00b7nius", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "D'rum hab' ich ein kn\u00f6chern Beingerippe,", "tokens": ["D'\u00b7rum", "hab'", "ich", "ein", "kn\u00f6\u00b7chern", "Bein\u00b7ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit Crucifix und drohender Hippe,", "tokens": ["Mit", "Cru\u00b7ci\u00b7fix", "und", "dro\u00b7hen\u00b7der", "Hip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In meiner Zelle aufgestellt.", "tokens": ["In", "mei\u00b7ner", "Zel\u00b7le", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Fest schau' ich es an bei Mondenscheine,", "tokens": ["Fest", "schau'", "ich", "es", "an", "bei", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPR", "APPR", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn ich in verzweifeltem Schmerze weine,", "tokens": ["Wenn", "ich", "in", "ver\u00b7zwei\u00b7fel\u00b7tem", "Schmer\u00b7ze", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein k\u00e4mpfendes Kind der k\u00e4mpfenden Zeit!", "tokens": ["Ein", "k\u00e4mp\u00b7fen\u00b7des", "Kind", "der", "k\u00e4mp\u00b7fen\u00b7den", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann tauml' ich empor in wildem Entz\u00fccken,", "tokens": ["Dann", "tauml'", "ich", "em\u00b7por", "in", "wil\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das Leben noch einmal an's Herz zu dr\u00fccken,", "tokens": ["Das", "Le\u00b7ben", "noch", "ein\u00b7mal", "an's", "Herz", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bevor es vernichtendem Tode geweiht!", "tokens": ["Be\u00b7vor", "es", "ver\u00b7nich\u00b7ten\u00b7dem", "To\u00b7de", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, k\u00fchlen in frischen Lebensfluthen", "tokens": ["Ja", ",", "k\u00fch\u00b7len", "in", "fri\u00b7schen", "Le\u00b7bens\u00b7flut\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will ich der lodernden Seele Gluten!", "tokens": ["Will", "ich", "der", "lo\u00b7dern\u00b7den", "See\u00b7le", "Glu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich will vor S\u00fcnde und Kreuz bewahrt,", "tokens": ["Ich", "will", "vor", "S\u00fcn\u00b7de", "und", "Kreuz", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stark durch des eigenen Geistes Ringen,", "tokens": ["Stark", "durch", "des", "ei\u00b7ge\u00b7nen", "Geis\u00b7tes", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Mich aus Fesseln und Banden schwingen", "tokens": ["Mich", "aus", "Fes\u00b7seln", "und", "Ban\u00b7den", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Auf zu begeisterter Himmelfahrt!", "tokens": ["Auf", "zu", "be\u00b7geis\u00b7ter\u00b7ter", "Him\u00b7mel\u00b7fahrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Ich sah mich in Tr\u00e4umen der Mitternacht", "tokens": ["Ich", "sah", "mich", "in", "Tr\u00e4u\u00b7men", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Verlassen und verachtet!", "tokens": ["Ver\u00b7las\u00b7sen", "und", "ver\u00b7ach\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Auges milde Glut und Pracht,", "tokens": ["Des", "Au\u00b7ges", "mil\u00b7de", "Glut", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das liebend einst \u00fcber mir gewacht,", "tokens": ["Das", "lie\u00b7bend", "einst", "\u00fc\u00b7ber", "mir", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich sah es von Ha\u00df umnachtet!", "tokens": ["Ich", "sah", "es", "von", "Ha\u00df", "um\u00b7nach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Mir malte der irre Gedankenflug", "tokens": ["Mir", "mal\u00b7te", "der", "ir\u00b7re", "Ge\u00b7dan\u00b7ken\u00b7flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gestalten bleich und tr\u00fcbe!", "tokens": ["Ge\u00b7stal\u00b7ten", "bleich", "und", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah einen finstern Leichenzug;", "tokens": ["Ich", "sah", "ei\u00b7nen", "fins\u00b7tern", "Lei\u00b7chen\u00b7zug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Leiche, die man vor\u00fcbertrug,", "tokens": ["Die", "Lei\u00b7che", ",", "die", "man", "vor\u00b7\u00fc\u00b7bert\u00b7rug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "War uns're gestorbene Liebe.", "tokens": ["War", "un\u00b7s'\u00b7re", "ge\u00b7stor\u00b7be\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Entflieh', du gespenstische Mitternacht!", "tokens": ["Ent\u00b7flieh'", ",", "du", "ge\u00b7spens\u00b7ti\u00b7sche", "Mit\u00b7ter\u00b7nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Entflieht, ihr blassen Gestalten!", "tokens": ["Ent\u00b7flieht", ",", "ihr", "blas\u00b7sen", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bis der selige, fr\u00f6hliche Tag erwacht,", "tokens": ["Bis", "der", "se\u00b7li\u00b7ge", ",", "fr\u00f6h\u00b7li\u00b7che", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bis Leben und Liebe mit frischer Macht", "tokens": ["Bis", "Le\u00b7ben", "und", "Lie\u00b7be", "mit", "fri\u00b7scher", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mich jauchzend umschlungen halten.", "tokens": ["Mich", "jauch\u00b7zend", "um\u00b7schlun\u00b7gen", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Wie liebt' ich die sch\u00f6ne, heilige Nacht,", "tokens": ["Wie", "liebt'", "ich", "die", "sch\u00f6\u00b7ne", ",", "hei\u00b7li\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn die b\u00f6sen Tr\u00e4ume nicht w\u00e4ren!", "tokens": ["Wenn", "die", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", "nicht", "w\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKNEG", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Unheimlicher Geister wilde Jagd", "tokens": ["Un\u00b7heim\u00b7li\u00b7cher", "Geis\u00b7ter", "wil\u00b7de", "Jagd"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verfolgt mich, bis ich, vom Schlaf erwacht,", "tokens": ["Ver\u00b7folgt", "mich", ",", "bis", "ich", ",", "vom", "Schlaf", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mich bade in hei\u00dfen Z\u00e4hren.", "tokens": ["Mich", "ba\u00b7de", "in", "hei\u00b7\u00dfen", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Ich f\u00fchl' mich allein in der weiten Welt;", "tokens": ["Ich", "f\u00fchl'", "mich", "al\u00b7lein", "in", "der", "wei\u00b7ten", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was ich liebe,