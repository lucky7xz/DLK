{"dta.poem.16451": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "112.  In der Sonnen ists gut seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer in der Sonnen ist/ dem mangelt nicht das Licht/", "tokens": ["Wer", "in", "der", "Son\u00b7nen", "ist", "/", "dem", "man\u00b7gelt", "nicht", "das", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "$(", "ART", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das dem/ der ausser jhr verjrret geht/ gebricht/", "tokens": ["Das", "dem", "/", "der", "aus\u00b7ser", "jhr", "ver\u00b7jr\u00b7ret", "geht", "/", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ART", "$(", "ART", "ADJA", "PPER", "VVFIN", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}