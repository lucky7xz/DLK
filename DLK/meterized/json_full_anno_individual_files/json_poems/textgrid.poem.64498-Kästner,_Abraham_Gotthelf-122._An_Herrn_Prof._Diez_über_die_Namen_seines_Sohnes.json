{"textgrid.poem.64498": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "122. An Herrn Prof. Diez \u00fcber die Namen seines Sohnes", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragt, seine Namen ihm zu deuten.", "tokens": ["Fragt", ",", "sei\u00b7ne", "Na\u00b7men", "ihm", "zu", "deu\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Giebst du zum Schutzgott ihm den Borrom\u00e4us an?", "tokens": ["Giebst", "du", "zum", "Schutz\u00b7gott", "ihm", "den", "Bor\u00b7ro\u00b7m\u00e4\u00b7us", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ART", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Soll ihn Assisens Seraph leiten?", "tokens": ["Soll", "ihn", "As\u00b7si\u00b7sens", "Se\u00b7raph", "lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "NE", "VVINF", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Nein! zeig' ihm einen ", "tokens": ["Nein", "!", "zeig'", "ihm", "ei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "PPER", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ihn zu besiegen war nur ", "tokens": ["Ihn", "zu", "be\u00b7sie\u00b7gen", "war", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "VAFIN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die waren gro\u00df an Geist,