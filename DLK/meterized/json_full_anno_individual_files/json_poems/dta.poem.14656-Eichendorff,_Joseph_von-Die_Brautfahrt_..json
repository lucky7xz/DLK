{"dta.poem.14656": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und er tr\u00e4gt die s\u00fc\u00dfe Beute", "tokens": ["Und", "er", "tr\u00e4gt", "die", "s\u00fc\u00b7\u00dfe", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubelnd aus dem Schlo\u00df auf's Schiff,", "tokens": ["Ju\u00b7belnd", "aus", "dem", "Schlo\u00df", "auf's", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drunten harren seine Leute,", "tokens": ["Drun\u00b7ten", "har\u00b7ren", "sei\u00b7ne", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sto\u00dfen froh vom Felsenriff;", "tokens": ["Sto\u00b7\u00dfen", "froh", "vom", "Fel\u00b7sen\u00b7riff", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}