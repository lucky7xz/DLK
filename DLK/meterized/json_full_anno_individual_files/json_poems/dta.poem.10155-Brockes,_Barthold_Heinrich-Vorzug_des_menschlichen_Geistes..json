{"dta.poem.10155": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vorzug des menschlichen Geistes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwege, lieber Mensch, es sind ja unsre Seelen", "tokens": ["Er\u00b7we\u00b7ge", ",", "lie\u00b7ber", "Mensch", ",", "es", "sind", "ja", "uns\u00b7re", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "NN", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von andrer Art, als Holtz und Stein;", "tokens": ["Von", "an\u00b7drer", "Art", ",", "als", "Holtz", "und", "Stein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da, von des Sch\u00f6pfers Creaturen", "tokens": ["Da", ",", "von", "des", "Sch\u00f6p\u00b7fers", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Recht wunder-wunderbare Spuren", "tokens": ["Recht", "wun\u00b7der\u00b7wun\u00b7der\u00b7ba\u00b7re", "Spu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In ihnen anzutreffen seyn.", "tokens": ["In", "ih\u00b7nen", "an\u00b7zu\u00b7tref\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVIZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sprich nicht, da\u00df das Gesicht der Thiere", "tokens": ["Sprich", "nicht", ",", "da\u00df", "das", "Ge\u00b7sicht", "der", "Thie\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denselben Eindruck auch versp\u00fcre:", "tokens": ["Den\u00b7sel\u00b7ben", "Ein\u00b7druck", "auch", "ver\u00b7sp\u00fc\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn, ob es, leider! wahr, da\u00df sich zu dieser Zeit", "tokens": ["Denn", ",", "ob", "es", ",", "lei\u00b7der", "!", "wahr", ",", "da\u00df", "sich", "zu", "die\u00b7ser", "Zeit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "ADV", "$.", "ADJD", "$,", "KOUS", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In den Betrachtungen der Creaturen Pracht,", "tokens": ["In", "den", "Be\u00b7trach\u00b7tun\u00b7gen", "der", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Die Menschheit, durch Unachtsamkeit,", "tokens": ["Die", "Menschheit", ",", "durch", "U\u00b7nacht\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "So wie das Vieh, fast g\u00e4ntzlich f\u00fchllos macht;", "tokens": ["So", "wie", "das", "Vieh", ",", "fast", "g\u00e4ntz\u00b7lich", "f\u00fchl\u00b7los", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So da\u00df es leider noch die Frage:", "tokens": ["So", "da\u00df", "es", "lei\u00b7der", "noch", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ob ich mit gr\u00f6sserm Nechte sage,", "tokens": ["Ob", "ich", "mit", "gr\u00f6s\u00b7serm", "Nech\u00b7te", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df Menschen GOttes-Werck mit Hund-und Katzen-", "tokens": ["Da\u00df", "Men\u00b7schen", "Got\u00b7tes\u00b7\u00b7Werck", "mit", "Hun\u00b7d\u00b7und", "Kat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wie? oder da\u00df die Hund\u2019 und Katzen das, was sch\u00f6n,", "tokens": ["Wie", "?", "o\u00b7der", "da\u00df", "die", "Hund'", "und", "Kat\u00b7zen", "das", ",", "was", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "KOUS", "ART", "NN", "KON", "NN", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit Menschen-Augen sehn?", "tokens": ["Mit", "Men\u00b7schen\u00b7Au\u00b7gen", "sehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Weil eine Blindheit ja der andern gleicht,", "tokens": ["Weil", "ei\u00b7ne", "Blind\u00b7heit", "ja", "der", "an\u00b7dern", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und unsere der ihrigen nicht weicht;", "tokens": ["Und", "un\u00b7se\u00b7re", "der", "ih\u00b7ri\u00b7gen", "nicht", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ART", "PPOSS", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "So ist es doch in unsrer Macht,", "tokens": ["So", "ist", "es", "doch", "in", "uns\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In der Gesch\u00f6pffe Wunder-Pracht,", "tokens": ["In", "der", "Ge\u00b7sch\u00f6pf\u00b7fe", "Wun\u00b7der\u00b7Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Denjenigen, der sie hervorgebracht,", "tokens": ["Den\u00b7je\u00b7ni\u00b7gen", ",", "der", "sie", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zu schmecken, und zu sehn, zu f\u00fchlen und zu h\u00f6ren,", "tokens": ["Zu", "schme\u00b7cken", ",", "und", "zu", "sehn", ",", "zu", "f\u00fch\u00b7len", "und", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auch Jhn, durch Gegen-Lieb, und frohen Danck, zu ehren;", "tokens": ["Auch", "Jhn", ",", "durch", "Ge\u00b7gen\u00b7Lieb", ",", "und", "fro\u00b7hen", "Danck", ",", "zu", "eh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "APPR", "NN", "$,", "KON", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das jenen untersagt.", "tokens": ["Das", "je\u00b7nen", "un\u00b7ter\u00b7sagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Ach la\u00dft uns denn ie mehr und mehr", "tokens": ["Ach", "la\u00dft", "uns", "denn", "ie", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des schwartzen Undancks Laster meiden!", "tokens": ["Des", "schwart\u00b7zen", "Un\u00b7dancks", "Las\u00b7ter", "mei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Uns von den Thieren unterscheiden!", "tokens": ["Uns", "von", "den", "Thie\u00b7ren", "un\u00b7ter\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und unsrer Seelen Eigenschaft,", "tokens": ["Und", "uns\u00b7rer", "See\u00b7len", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die ihr von GOTT verliehne Krafft,", "tokens": ["Die", "ihr", "von", "GoTT", "ver\u00b7lieh\u00b7ne", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Durch das betrachten und das dencken,", "tokens": ["Durch", "das", "be\u00b7trach\u00b7ten", "und", "das", "den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "PDS", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Auf GOTTES Creatur zu lencken,", "tokens": ["Auf", "GoT\u00b7TES", "Crea\u00b7tur", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Mit froher Andacht, uns bestreben!", "tokens": ["Mit", "fro\u00b7her", "An\u00b7dacht", ",", "uns", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Um GOTT dadurch, was GOTTES ist, zu geben.", "tokens": ["Um", "GoTT", "da\u00b7durch", ",", "was", "GoT\u00b7TES", "ist", ",", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NE", "PAV", "$,", "PRELS", "NE", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und zwar \u00fcm so viel mehr, als wir", "tokens": ["Und", "zwar", "\u00fcm", "so", "viel", "mehr", ",", "als", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADV", "ADV", "ADV", "$,", "KOUS", "PPER"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "In der Gesch\u00f6pf\u2019, allein durch Jhn gewirckten Zier,", "tokens": ["In", "der", "Ge\u00b7sch\u00f6pf'", ",", "al\u00b7lein", "durch", "Jhn", "ge\u00b7wirck\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wenn wir sie mit Vernunft ergr\u00fcnden,", "tokens": ["Wenn", "wir", "sie", "mit", "Ver\u00b7nunft", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Den grossen Sch\u00f6pfer selber finden.", "tokens": ["Den", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "sel\u00b7ber", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}