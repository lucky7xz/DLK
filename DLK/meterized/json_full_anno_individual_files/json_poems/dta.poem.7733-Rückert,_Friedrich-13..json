{"dta.poem.7733": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Gott gelangst du nicht im Wachen noch im Traum;", "tokens": ["Zu", "Gott", "ge\u00b7langst", "du", "nicht", "im", "Wa\u00b7chen", "noch", "im", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist im Weltraum nicht, noch im Gedankenraum.", "tokens": ["Er", "ist", "im", "Welt\u00b7raum", "nicht", ",", "noch", "im", "Ge\u00b7dan\u00b7ken\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PTKNEG", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du kannst die Grenze nicht des Denkens \u00fcberschreiten,", "tokens": ["Du", "kannst", "die", "Gren\u00b7ze", "nicht", "des", "Den\u00b7kens", "\u00fc\u00b7bersc\u00b7hrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch stehend an der Grenz', hin\u00fcber sehn vom weiten.", "tokens": ["Doch", "ste\u00b7hend", "an", "der", "Grenz'", ",", "hin\u00b7\u00fc\u00b7ber", "sehn", "vom", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie dein Auge sieht, was du nicht kannst ergreifen,", "tokens": ["Und", "wie", "dein", "Au\u00b7ge", "sieht", ",", "was", "du", "nicht", "kannst", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kann dein h\u00f6hrer Sinn ins Undenkbare streifen.", "tokens": ["So", "kann", "dein", "h\u00f6h\u00b7rer", "Sinn", "ins", "Un\u00b7denk\u00b7ba\u00b7re", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}