{"textgrid.poem.60242": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mepsantus gieng zu seinem Weibe,", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mepsantus gieng zu seinem Weibe,", "tokens": ["Me\u00b7ps\u00b7an\u00b7tus", "gieng", "zu", "sei\u00b7nem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mit geschwollnem kranken Leibe", "tokens": ["Die", "mit", "ge\u00b7schwoll\u00b7nem", "kran\u00b7ken", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon manche Nacht und manchen Tag", "tokens": ["Schon", "man\u00b7che", "Nacht", "und", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einem bangen Bette lag;", "tokens": ["Auf", "ei\u00b7nem", "ban\u00b7gen", "Bet\u00b7te", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie seufzte: Schatz, bey meinem bittern Leiden", "tokens": ["Sie", "seufz\u00b7te", ":", "Schatz", ",", "bey", "mei\u00b7nem", "bit\u00b7tern", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Will mir der Arzt die Schenkel noch zerschneiden;", "tokens": ["Will", "mir", "der", "Arzt", "die", "Schen\u00b7kel", "noch", "zer\u00b7schnei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Waden sind schon wundenvoll", "tokens": ["Die", "Wa\u00b7den", "sind", "schon", "wun\u00b7den\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vom spanschen Fliegenbisse,", "tokens": ["Vom", "span\u00b7schen", "Flie\u00b7gen\u00b7bis\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und ach, der b\u00f6se Doctor sagt,", "tokens": ["Und", "ach", ",", "der", "b\u00f6\u00b7se", "Doc\u00b7tor", "sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df ich den Schnitt noch \u00fcberstehen m\u00fcsse.", "tokens": ["Da\u00df", "ich", "den", "Schnitt", "noch", "\u00fc\u00b7bers\u00b7te\u00b7hen", "m\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Bin ich nicht schon genug geplagt?", "tokens": ["Bin", "ich", "nicht", "schon", "ge\u00b7nug", "ge\u00b7plagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Was aber thut man nicht sein Leben zu erhalten?", "tokens": ["Was", "a\u00b7ber", "thut", "man", "nicht", "sein", "Le\u00b7ben", "zu", "er\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PIS", "PTKNEG", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Hier nahm der freundliche Mepsant", "tokens": ["Hier", "nahm", "der", "freund\u00b7li\u00b7che", "Me\u00b7psant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die kranke Gattinn bey der Hand,", "tokens": ["Die", "kran\u00b7ke", "Gat\u00b7tinn", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und bat sie, lasse dir die Schenkel nicht zerspalten!", "tokens": ["Und", "bat", "sie", ",", "las\u00b7se", "dir", "die", "Schen\u00b7kel", "nicht", "zer\u00b7spal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Aerzte martern nur den schwachen K\u00f6rperbau,", "tokens": ["Die", "A\u00b7erz\u00b7te", "mar\u00b7tern", "nur", "den", "schwa\u00b7chen", "K\u00f6r\u00b7per\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Der wie der Tag hat abgenommen.", "tokens": ["Der", "wie", "der", "Tag", "hat", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Stirb lieber, stirb geschwind, o meine gute Frau,", "tokens": ["Stirb", "lie\u00b7ber", ",", "stirb", "ge\u00b7schwind", ",", "o", "mei\u00b7ne", "gu\u00b7te", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADJD", "$,", "FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich schw\u00f6re dir bald nachzukommen.", "tokens": ["Ich", "schw\u00f6\u00b7re", "dir", "bald", "nach\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Er sprachs, und seine Kranke litt", "tokens": ["Er", "sprachs", ",", "und", "sei\u00b7ne", "Kran\u00b7ke", "litt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Kein Wasserzapfen, keinen Schnitt;", "tokens": ["Kein", "Was\u00b7ser\u00b7zap\u00b7fen", ",", "kei\u00b7nen", "Schnitt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie qu\u00e4lte sich noch sieben Wochen,", "tokens": ["Sie", "qu\u00e4l\u00b7te", "sich", "noch", "sie\u00b7ben", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und starb, und endete die Noth.", "tokens": ["Und", "starb", ",", "und", "en\u00b7de\u00b7te", "die", "Noth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Doch, Freundinn, kurz darauf erschien der blasse Tod", "tokens": ["Doch", ",", "Freun\u00b7dinn", ",", "kurz", "da\u00b7rauf", "er\u00b7schien", "der", "blas\u00b7se", "Tod"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "ADJD", "PAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dem Mann im Traum, und sprach:", "tokens": ["Dem", "Mann", "im", "Traum", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Mepsant, du hast versprochen,", "tokens": ["Me\u00b7psant", ",", "du", "hast", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Der lieben Frau bald nachzugehn;", "tokens": ["Der", "lie\u00b7ben", "Frau", "bald", "nach\u00b7zu\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Hast du den Tag schon auserlesen?", "tokens": ["Hast", "du", "den", "Tag", "schon", "au\u00b7ser\u00b7le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ich werde keinen Spa\u00df verstehn.", "tokens": ["Ich", "wer\u00b7de", "kei\u00b7nen", "Spa\u00df", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Tod, rief Mepsant, es ist ja nicht mein Ernst gewesen,", "tokens": ["Tod", ",", "rief", "Me\u00b7psant", ",", "es", "ist", "ja", "nicht", "mein", "Ernst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wollte nur das Doctorlohn,", "tokens": ["Ich", "woll\u00b7te", "nur", "das", "Doc\u00b7tor\u00b7lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Das viele Geld ersparen.", "tokens": ["Das", "vie\u00b7le", "Geld", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Ich bin ein muntrer Mann von zwey und vierzig Jahren,", "tokens": ["Ich", "bin", "ein", "mun\u00b7trer", "Mann", "von", "zwey", "und", "vier\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und w\u00fcnsche mir nichts weniger, als schon", "tokens": ["Und", "w\u00fcn\u00b7sche", "mir", "nichts", "we\u00b7ni\u00b7ger", ",", "als", "schon"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Dem Weibe nachzufahren.", "tokens": ["Dem", "Wei\u00b7be", "nach\u00b7zu\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mepsantus gieng zu seinem Weibe,", "tokens": ["Me\u00b7ps\u00b7an\u00b7tus", "gieng", "zu", "sei\u00b7nem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die mit geschwollnem kranken Leibe", "tokens": ["Die", "mit", "ge\u00b7schwoll\u00b7nem", "kran\u00b7ken", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon manche Nacht und manchen Tag", "tokens": ["Schon", "man\u00b7che", "Nacht", "und", "man\u00b7chen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einem bangen Bette lag;", "tokens": ["Auf", "ei\u00b7nem", "ban\u00b7gen", "Bet\u00b7te", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie seufzte: Schatz, bey meinem bittern Leiden", "tokens": ["Sie", "seufz\u00b7te", ":", "Schatz", ",", "bey", "mei\u00b7nem", "bit\u00b7tern", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure":