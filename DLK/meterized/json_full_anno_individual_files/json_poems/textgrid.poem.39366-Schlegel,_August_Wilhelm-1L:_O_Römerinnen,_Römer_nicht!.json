{"textgrid.poem.39366": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O R\u00f6merinnen, R\u00f6mer nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O R\u00f6merinnen, R\u00f6mer nicht!", "tokens": ["O", "R\u00f6\u00b7me\u00b7rin\u00b7nen", ",", "R\u00f6\u00b7mer", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist die Heldenkraft der Ahnen?", "tokens": ["Wo", "ist", "die", "Hel\u00b7den\u00b7kraft", "der", "Ah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verge\u00dfen habt ihr Namen, Ruhm und Pflicht", "tokens": ["Ver\u00b7ge\u00b7\u00dfen", "habt", "ihr", "Na\u00b7men", ",", "Ruhm", "und", "Pflicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tief err\u00f6then jene gro\u00dfen Manen.", "tokens": ["Und", "tief", "er\u00b7r\u00f6\u00b7then", "je\u00b7ne", "gro\u00b7\u00dfen", "Ma\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du, ewig nun zu eigner Schmach!", "tokens": ["Du", ",", "e\u00b7wig", "nun", "zu", "eig\u00b7ner", "Schmach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leb wohl, o Stadt der sieben H\u00fcgel!", "tokens": ["Leb", "wohl", ",", "o", "Stadt", "der", "sie\u00b7ben", "H\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald st\u00fcrzt dein \u00f6des Pantheon uns nach,", "tokens": ["Bald", "st\u00fcrzt", "dein", "\u00f6\u00b7des", "Pan\u00b7the\u00b7on", "uns", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns aber leiht der Sieg die leichten Fl\u00fcgel.", "tokens": ["Uns", "a\u00b7ber", "leiht", "der", "Sieg", "die", "leich\u00b7ten", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom Himmel lockten uns herab", "tokens": ["Vom", "Him\u00b7mel", "lock\u00b7ten", "uns", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die freundlich bildenden Hellenen;", "tokens": ["Die", "freund\u00b7lich", "bil\u00b7den\u00b7den", "Hel\u00b7le\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von uns empfangend was ihr Sinn uns gab,", "tokens": ["Von", "uns", "emp\u00b7fan\u00b7gend", "was", "ihr", "Sinn", "uns", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verm\u00e4hlten sie das G\u00f6ttliche dem Sch\u00f6nen.", "tokens": ["Ver\u00b7m\u00e4hl\u00b7ten", "sie", "das", "G\u00f6tt\u00b7li\u00b7che", "dem", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als Freiheit mit der Tugend starb,", "tokens": ["Als", "Frei\u00b7heit", "mit", "der", "Tu\u00b7gend", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erloschen Hellas Rosenschimmer,", "tokens": ["Er\u00b7lo\u00b7schen", "Hel\u00b7las", "Ro\u00b7sen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und St\u00e4rke, die ein grauses Recht erwarb,", "tokens": ["Und", "St\u00e4r\u00b7ke", ",", "die", "ein", "grau\u00b7ses", "Recht", "er\u00b7warb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stie\u00df den Altar der holden Kunst in Tr\u00fcmmer.", "tokens": ["Stie\u00df", "den", "Al\u00b7tar", "der", "hol\u00b7den", "Kunst", "in", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Doch sahn wir, im Vernichten gro\u00df,", "tokens": ["Doch", "sahn", "wir", ",", "im", "Ver\u00b7nich\u00b7ten", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Lust der W\u00f6lfin Pfleges\u00f6hne;", "tokens": ["Mit", "Lust", "der", "W\u00f6l\u00b7fin", "Pfle\u00b7ge\u00b7s\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir theilten gern der hohen Roma Loo\u00df,", "tokens": ["Wir", "theil\u00b7ten", "gern", "der", "ho\u00b7hen", "Ro\u00b7ma", "Loo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie uns Herrscher \u00fcber Herrscher kr\u00f6ne.", "tokens": ["Da\u00df", "sie", "uns", "Herr\u00b7scher", "\u00fc\u00b7ber", "Herr\u00b7scher", "kr\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In der Triumphe Prachtgew\u00fchl", "tokens": ["In", "der", "Tri\u00b7um\u00b7phe", "Pracht\u00b7ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfieng Quirinus seine Br\u00fcder;", "tokens": ["Emp\u00b7fi\u00b7eng", "Qui\u00b7ri\u00b7nus", "sei\u00b7ne", "Br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Kapitol ward ein Olymp: da fiel", "tokens": ["Das", "Ka\u00b7pi\u00b7tol", "ward", "ein", "O\u00b7lymp", ":", "da", "fiel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor seinem Raub der Sieger betend nieder.", "tokens": ["Vor", "sei\u00b7nem", "Raub", "der", "Sie\u00b7ger", "be\u00b7tend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zeus Adler flog dem Heer voran,", "tokens": ["Zeus", "Ad\u00b7ler", "flog", "dem", "Heer", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er thronte, der Gesetze W\u00e4chter \u2013", "tokens": ["Er", "thron\u00b7te", ",", "der", "Ge\u00b7set\u00b7ze", "W\u00e4ch\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Senat und Volk ward Einem unterthan,", "tokens": ["Se\u00b7nat", "und", "Volk", "ward", "Ei\u00b7nem", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schnell entartet sanken die Geschlechter.", "tokens": ["Und", "schnell", "ent\u00b7ar\u00b7tet", "san\u00b7ken", "die", "Ge\u00b7schlech\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Wild schw\u00e4rmten an der Tiber Strand", "tokens": ["Wild", "schw\u00e4rm\u00b7ten", "an", "der", "Ti\u00b7ber", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nordens Riesen, sie zu strafen.", "tokens": ["Des", "Nor\u00b7dens", "Rie\u00b7sen", ",", "sie", "zu", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Knechten und Barbaren abgewandt", "tokens": ["Von", "Knech\u00b7ten", "und", "Bar\u00b7ba\u00b7ren", "ab\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schien ein Jahrtausend unsre Macht zu schlafen,", "tokens": ["Schien", "ein", "Jahr\u00b7tau\u00b7send", "uns\u00b7re", "Macht", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Bis mit dem jungen Morgenroth", "tokens": ["Bis", "mit", "dem", "jun\u00b7gen", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entwohnte Hymnen uns umwallen.", "tokens": ["Ent\u00b7wohn\u00b7te", "Hym\u00b7nen", "uns", "um\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es war die Kunst, die Huldigung uns bot,", "tokens": ["Es", "war", "die", "Kunst", ",", "die", "Hul\u00b7di\u00b7gung", "uns", "bot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und neue Feste, neue Tempelhallen.", "tokens": ["Und", "neu\u00b7e", "Fes\u00b7te", ",", "neu\u00b7e", "Tem\u00b7pel\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Auf! fe\u00dfle jetzt uns noch, o Rom,", "tokens": ["Auf", "!", "fe\u00df\u00b7le", "jetzt", "uns", "noch", ",", "o", "Rom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ADV", "PPER", "ADV", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch gro\u00dfe Thaten, sch\u00f6ne Werke!", "tokens": ["Durch", "gro\u00b7\u00dfe", "Tha\u00b7ten", ",", "sch\u00f6\u00b7ne", "Wer\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Versank auf ewig in der Zeiten Strom", "tokens": ["Ver\u00b7sank", "auf", "e\u00b7wig", "in", "der", "Zei\u00b7ten", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Bilder Geist so wie der Helden St\u00e4rke?", "tokens": ["Der", "Bil\u00b7der", "Geist", "so", "wie", "der", "Hel\u00b7den", "St\u00e4r\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "In dumpfer Tr\u00e4gheit Nebelduft", "tokens": ["In", "dum\u00b7pfer", "Tr\u00e4g\u00b7heit", "Ne\u00b7bel\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Prahlst du mit g\u00e4hnenden Ruinen;", "tokens": ["Prahlst", "du", "mit", "g\u00e4h\u00b7nen\u00b7den", "Ru\u00b7i\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der Vorwelt eingesunkne Gruft,", "tokens": ["Du", "bist", "der", "Vor\u00b7welt", "ein\u00b7ge\u00b7sunk\u00b7ne", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo weder Rosen bl\u00fchn, noch Lorbeern gr\u00fcnen.", "tokens": ["Wo", "we\u00b7der", "Ro\u00b7sen", "bl\u00fchn", ",", "noch", "Lor\u00b7beern", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "NN", "VVINF", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ihr aber, die ihr, siegberauscht,", "tokens": ["Ihr", "a\u00b7ber", ",", "die", "ihr", ",", "sieg\u00b7be\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ausoniens myrtumkr\u00e4nzte Fluren", "tokens": ["Au\u00b7so\u00b7ni\u00b7ens", "myr\u00b7tum\u00b7kr\u00e4nz\u00b7te", "Flu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gleich eurem Rhodan wogend \u00fcberrauscht,", "tokens": ["Gleich", "eu\u00b7rem", "Rho\u00b7dan", "wo\u00b7gend", "\u00fc\u00b7berr\u00b7auscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und einem Brennus folgt auf Brennus Spuren!", "tokens": ["Und", "ei\u00b7nem", "Bren\u00b7nus", "folgt", "auf", "Bren\u00b7nus", "Spu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Ruft uns mit reiner Opferglut,", "tokens": ["Ruft", "uns", "mit", "rei\u00b7ner", "Op\u00b7fer\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So soll euch unsre Huld belohnen.", "tokens": ["So", "soll", "euch", "uns\u00b7re", "Huld", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein ", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und wollt ", "tokens": ["Und", "wollt"], "token_info": ["word", "word"], "pos": ["KON", "VMFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.14": {"line.1": {"text": "Habt ihr f\u00fcr uns ein Heiligthum?", "tokens": ["H