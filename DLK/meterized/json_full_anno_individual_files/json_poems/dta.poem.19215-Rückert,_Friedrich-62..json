{"dta.poem.19215": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Maulwurf ist nicht blind, gegeben hat ihm nur", "tokens": ["Der", "Maul\u00b7wurf", "ist", "nicht", "blind", ",", "ge\u00b7ge\u00b7ben", "hat", "ihm", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$,", "VVPP", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein kleines Auge, wie ers brauchet, die Natur,", "tokens": ["Ein", "klei\u00b7nes", "Au\u00b7ge", ",", "wie", "ers", "brau\u00b7chet", ",", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mit welchem er wird sehn soviel als er bedarf,", "tokens": ["Mit", "wel\u00b7chem", "er", "wird", "sehn", "so\u00b7viel", "als", "er", "be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "VVINF", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im unterirdischen Palast, den er entwarf,", "tokens": ["Im", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Pa\u00b7last", ",", "den", "er", "ent\u00b7warf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und Staub ins Auge wird ihm desto minder fallen,", "tokens": ["Und", "Staub", "ins", "Au\u00b7ge", "wird", "ihm", "des\u00b7to", "min\u00b7der", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn w\u00fchlend er empor wirft die gew\u00f6lbten Hallen.", "tokens": ["Wenn", "w\u00fch\u00b7lend", "er", "em\u00b7por", "wirft", "die", "ge\u00b7w\u00f6lb\u00b7ten", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PTKVZ", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den Regenwurm, den er mit andern Sinnen sucht,", "tokens": ["Den", "Re\u00b7gen\u00b7wurm", ",", "den", "er", "mit", "an\u00b7dern", "Sin\u00b7nen", "sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Braucht er nicht zu ersp\u00e4hn, nicht schnell ist dessen Flucht.", "tokens": ["Braucht", "er", "nicht", "zu", "er\u00b7sp\u00e4hn", ",", "nicht", "schnell", "ist", "des\u00b7sen", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "PTKNEG", "ADJD", "VAFIN", "PDS", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.5": {"line.1": {"text": "Und wird in warmer Nacht er aus dem Boden steigen,", "tokens": ["Und", "wird", "in", "war\u00b7mer", "Nacht", "er", "aus", "dem", "Bo\u00b7den", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch seinem kleinen Stern wird sich der Himmel zeigen;", "tokens": ["Auch", "sei\u00b7nem", "klei\u00b7nen", "Stern", "wird", "sich", "der", "Him\u00b7mel", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und ohne da\u00df ers wei\u00df, nimmt er mit sich hinnieder", "tokens": ["Und", "oh\u00b7ne", "da\u00df", "ers", "wei\u00df", ",", "nimmt", "er", "mit", "sich", "hin\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "KOUS", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PRF", "ADV"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch einen Stral, und w\u00fchlt vergn\u00fcgt im Dunkeln wieder.", "tokens": ["Auch", "ei\u00b7nen", "Stral", ",", "und", "w\u00fchlt", "ver\u00b7gn\u00fcgt", "im", "Dun\u00b7keln", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}