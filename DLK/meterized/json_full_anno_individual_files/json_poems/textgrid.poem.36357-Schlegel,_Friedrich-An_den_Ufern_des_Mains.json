{"textgrid.poem.36357": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Ufern des Mains", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier, wo um weinbekr\u00e4nzte H\u00fcgel", "tokens": ["Hier", ",", "wo", "um", "wein\u00b7be\u00b7kr\u00e4nz\u00b7te", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Strom sich schlingt,", "tokens": ["Der", "Strom", "sich", "schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sanft gleitend, wie des Schwanes Fl\u00fcgel,", "tokens": ["Sanft", "glei\u00b7tend", ",", "wie", "des", "Schwa\u00b7nes", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrischend durch die Wiesen dringt,", "tokens": ["Er\u00b7fri\u00b7schend", "durch", "die", "Wie\u00b7sen", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Schiffleins stille Bahn gezogen", "tokens": ["Des", "Schif\u00b7fleins", "stil\u00b7le", "Bahn", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf schlangengleich gewundnen Wogen", "tokens": ["Auf", "schlan\u00b7gen\u00b7gleich", "ge\u00b7wund\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich um die Berge schwingt;", "tokens": ["Sich", "um", "die", "Ber\u00b7ge", "schwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier, wo im fruchtbegabten Tale", "tokens": ["Hier", ",", "wo", "im", "frucht\u00b7be\u00b7gab\u00b7ten", "Ta\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Rebe Kraft,", "tokens": ["Der", "Re\u00b7be", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gen\u00e4hrt vom starken Sonnenstrahle,", "tokens": ["Ge\u00b7n\u00e4hrt", "vom", "star\u00b7ken", "Son\u00b7nen\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So goldnen Weines Trank erschafft,", "tokens": ["So", "gold\u00b7nen", "Wei\u00b7nes", "Trank", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der einst die Enkel noch erheitert,", "tokens": ["Der", "einst", "die", "En\u00b7kel", "noch", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Liedern ihre Brust erweitert,", "tokens": ["Zu", "Lie\u00b7dern", "ih\u00b7re", "Brust", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Mut der Sorg' entrafft;", "tokens": ["Den", "Mut", "der", "Sor\u00b7g'", "en\u00b7trafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo froh gesinnt die deutschen Franken,", "tokens": ["Wo", "froh", "ge\u00b7sinnt", "die", "deut\u00b7schen", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Kraft und Lust,", "tokens": ["Voll", "Kraft", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am schwachen Tr\u00fcbsinn nie erkranken,", "tokens": ["Am", "schwa\u00b7chen", "Tr\u00fcb\u00b7sinn", "nie", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00f6hlich des freien Muts bewu\u00dft;", "tokens": ["Fr\u00f6h\u00b7lich", "des", "frei\u00b7en", "Muts", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Wie einz'le Blumen auf den Fluren,", "tokens": ["Wie", "einz'\u00b7le", "Blu\u00b7men", "auf", "den", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zeigend der alten Sitte Spuren,", "tokens": ["Zei\u00b7gend", "der", "al\u00b7ten", "Sit\u00b7te", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Der alten Deutschen Lust;", "tokens": ["Der", "al\u00b7ten", "Deut\u00b7schen", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier r\u00fchrten mutig linde Lieder", "tokens": ["Hier", "r\u00fchr\u00b7ten", "mu\u00b7tig", "lin\u00b7de", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir an das Herz,", "tokens": ["Mir", "an", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die alten Str\u00f6me brachen wieder", "tokens": ["Die", "al\u00b7ten", "Str\u00f6\u00b7me", "bra\u00b7chen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor, und es verschwand der Schmerz.", "tokens": ["Her\u00b7vor", ",", "und", "es", "ver\u00b7schwand", "der", "Schmerz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was sanft im Lied ergossen weinet,", "tokens": ["Was", "sanft", "im", "Lied", "er\u00b7gos\u00b7sen", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPRART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Starrt schweigend innen sonst versteinet,", "tokens": ["Starrt", "schwei\u00b7gend", "in\u00b7nen", "sonst", "ver\u00b7stei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie kaltes grauses Erz.", "tokens": ["Wie", "kal\u00b7tes", "grau\u00b7ses", "Erz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch, gleitend auf des Liedes Wellen,", "tokens": ["Doch", ",", "glei\u00b7tend", "auf", "des", "Lie\u00b7des", "Wel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird alles mild.", "tokens": ["Wird", "al\u00b7les", "mild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Oft spiegelt sich in diesen Quellen", "tokens": ["Oft", "spie\u00b7gelt", "sich", "in", "die\u00b7sen", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne und der Sterne Bild;", "tokens": ["Die", "Son\u00b7ne", "und", "der", "Ster\u00b7ne", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fort wie des Lebens Schiff gezogen", "tokens": ["Fort", "wie", "des", "Le\u00b7bens", "Schiff", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist auch des Ungl\u00fccks Sturm entflogen,", "tokens": ["Ist", "auch", "des", "Un\u00b7gl\u00fccks", "Sturm", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine Zeit mehr wild.", "tokens": ["Und", "kei\u00b7ne", "Zeit", "mehr", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl mu\u00df ein ew'ger Fr\u00fchling gr\u00fcnen", "tokens": ["Wohl", "mu\u00df", "ein", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem sel'gen Mann,", "tokens": ["Dem", "sel'\u00b7gen", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der seines Herzens nur erk\u00fchnen", "tokens": ["Der", "sei\u00b7nes", "Her\u00b7zens", "nur", "er\u00b7k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich den Freund verb\u00fcnden kann.", "tokens": ["Und", "sich", "den", "Freund", "ver\u00b7b\u00fcn\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Euch Wellen gr\u00fc\u00df' ich drum des Maines,", "tokens": ["Euch", "Wel\u00b7len", "gr\u00fc\u00df'", "ich", "drum", "des", "Mai\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gar oft gedenkend des Vereines,", "tokens": ["Gar", "oft", "ge\u00b7den\u00b7kend", "des", "Ver\u00b7ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der sch\u00f6ner dort begann.", "tokens": ["Der", "sch\u00f6\u00b7ner", "dort", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hier, wo um weinbekr\u00e4nzte H\u00fcgel", "tokens": ["Hier", ",", "wo", "um", "wein\u00b7be\u00b7kr\u00e4nz\u00b7te", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Strom sich schlingt,", "tokens": ["Der", "Strom", "sich", "schlingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sanft gleitend, wie des Schwanes Fl\u00fcgel,", "tokens": ["Sanft", "glei\u00b7tend", ",", "wie", "des", "Schwa\u00b7nes", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrischend durch die Wiese