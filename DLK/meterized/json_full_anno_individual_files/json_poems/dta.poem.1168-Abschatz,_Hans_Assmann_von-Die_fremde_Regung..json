{"dta.poem.1168": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die fremde Regung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Im Mittel aller Lust/ die Gl\u00fcck und Zeit mir geben/", "tokens": ["Im", "Mit\u00b7tel", "al\u00b7ler", "Lust", "/", "die", "Gl\u00fcck", "und", "Zeit", "mir", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$(", "ART", "NN", "KON", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan ich ohn Silvien nicht fr\u00f6lich leben;", "tokens": ["Kan", "ich", "ohn", "Sil\u00b7vi\u00b7en", "nicht", "fr\u00f6\u00b7lich", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn ich bey ihr bin/ so spielet um mein Hertz", "tokens": ["Und", "wenn", "ich", "bey", "ihr", "bin", "/", "so", "spie\u00b7let", "um", "mein", "Hertz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$(", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein angenehmer Schmertz.", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mer", "Schmertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Sinn f\u00fchlt sich gereizt von unbekandtem Triebe/", "tokens": ["Mein", "Sinn", "f\u00fchlt", "sich", "ge\u00b7reizt", "von", "un\u00b7be\u00b7kand\u00b7tem", "Trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich such/ und treffe sie doch ohne Furcht nicht an.", "tokens": ["Ich", "such", "/", "und", "tref\u00b7fe", "sie", "doch", "oh\u00b7ne", "Furcht", "nicht", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wofern ein Mensch iemahls unwissend lieben kan/", "tokens": ["Wo\u00b7fern", "ein", "Mensch", "ie\u00b7mahls", "un\u00b7wis\u00b7send", "lie\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So glaub ich/ da\u00df ich liebe.", "tokens": ["So", "glaub", "ich", "/", "da\u00df", "ich", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}