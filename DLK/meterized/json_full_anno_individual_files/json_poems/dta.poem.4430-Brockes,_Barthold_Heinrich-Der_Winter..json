{"dta.poem.4430": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser GOtt! ich kann mit Freuden,", "tokens": ["Gros\u00b7ser", "Gott", "!", "ich", "kann", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch beym Frost, die Augen weiden", "tokens": ["Auch", "beym", "Frost", ",", "die", "Au\u00b7gen", "wei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der wei\u00df beschney\u2019ten Welt,", "tokens": ["In", "der", "wei\u00df", "be\u00b7schney'\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die, wenn\u2019s klar ist oder schneyet,", "tokens": ["Die", ",", "wenn's", "klar", "ist", "o\u00b7der", "schne\u00b7yet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mich auf beyde Weis\u2019 erfreuet,", "tokens": ["Mich", "auf", "bey\u00b7de", "Weis'", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, zu Deinem Ruhm, gef\u00e4ll\u2019t.", "tokens": ["Und", ",", "zu", "Dei\u00b7nem", "Ruhm", ",", "ge\u00b7f\u00e4ll'", "t."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "abbreviation"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "$,", "XY", "XY"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}