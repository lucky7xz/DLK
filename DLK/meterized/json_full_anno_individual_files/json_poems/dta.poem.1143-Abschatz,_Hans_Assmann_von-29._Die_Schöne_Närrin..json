{"dta.poem.1143": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "29. Die Sch\u00f6ne N\u00e4rrin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6ne N\u00e4rrin du/ dein ausgela\u00dfner Geist ", "tokens": ["Du", "sch\u00f6\u00b7ne", "N\u00e4r\u00b7rin", "du", "/", "dein", "aus\u00b7ge\u00b7la\u00df\u00b7ner", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat mir die Sinnen auch nach Tichter Art entz\u00fcckt.", "tokens": ["Hat", "mir", "die", "Sin\u00b7nen", "auch", "nach", "Tich\u00b7ter", "Art", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach w\u00e4r ich deinen Ruhm zu preisen so geschickt/", "tokens": ["Ach", "w\u00e4r", "ich", "dei\u00b7nen", "Ruhm", "zu", "prei\u00b7sen", "so", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als du der Welt mit Recht ein kluges Wunder heist.", "tokens": ["Als", "du", "der", "Welt", "mit", "Recht", "ein", "klu\u00b7ges", "Wun\u00b7der", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie frey ist doch dein Sinn/ der sich mit Macht entreist", "tokens": ["Wie", "frey", "ist", "doch", "dein", "Sinn", "/", "der", "sich", "mit", "Macht", "en\u00b7treist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Fesseln/ damit ihn der sch\u00f6ne Leib bestrickt/", "tokens": ["Den", "Fes\u00b7seln", "/", "da\u00b7mit", "ihn", "der", "sch\u00f6\u00b7ne", "Leib", "be\u00b7strickt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du andre Pythias/ aus deinen Augen blickt", "tokens": ["Du", "and\u00b7re", "Py\u00b7thi\u00b7as", "/", "aus", "dei\u00b7nen", "Au\u00b7gen", "blickt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NE", "$(", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Cupidens Feuer/ wie die Brust Apollen weist.", "tokens": ["Cu\u00b7pi\u00b7dens", "Feu\u00b7er", "/", "wie", "die", "Brust", "A\u00b7pol\u00b7len", "weist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "KOKOM", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Hier ist kein falscher Schein/ noch blinder Worte Kunst/", "tokens": ["Hier", "ist", "kein", "fal\u00b7scher", "Schein", "/", "noch", "blin\u00b7der", "Wor\u00b7te", "Kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$(", "ADV", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dein Hertze/ wie dein Leib ist ohne Schminck und Dunst/", "tokens": ["Dein", "Hert\u00b7ze", "/", "wie", "dein", "Leib", "ist", "oh\u00b7ne", "Schminck", "und", "Dunst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$(", "KOKOM", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Geitz/ Ehrsucht/ Traurigkeit sind bey dir eingestellt;", "tokens": ["Geitz", "/", "Ehr\u00b7sucht", "/", "Trau\u00b7rig\u00b7keit", "sind", "bey", "dir", "ein\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch die manch kluger Kopff Vernunfft und Witz verloh-", "tokens": ["Durch", "die", "manch", "klu\u00b7ger", "Kopff", "Ver\u00b7nunfft", "und", "Witz", "ver\u00b7loh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "NN", "KON", "NN", "TRUNC"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "ren. ", "tokens": ["ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Was Wunder/ wenn du gleich mein\u2019 Einfalt machst zum", "tokens": ["Was", "Wun\u00b7der", "/", "wenn", "du", "gleich", "mein'", "Ein\u00b7falt", "machst", "zum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Thoren? ", "tokens": ["Tho\u00b7ren", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Wie manchem Narren folgt ein grosses Theil der Welt!", "tokens": ["Wie", "man\u00b7chem", "Nar\u00b7ren", "folgt", "ein", "gros\u00b7ses", "Theil", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}