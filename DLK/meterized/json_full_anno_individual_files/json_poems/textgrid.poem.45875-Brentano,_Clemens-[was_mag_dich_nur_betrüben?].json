{"textgrid.poem.45875": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[was mag dich nur betr\u00fcben?]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mag dich nur betr\u00fcben?", "tokens": ["Was", "mag", "dich", "nur", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du so traurig denkst.", "tokens": ["Da\u00df", "du", "so", "trau\u00b7rig", "denkst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft wohl Bu\u00dfe \u00fcben,", "tokens": ["Du", "mu\u00dft", "wohl", "Bu\u00b7\u00dfe", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil du die Blicke senkst.", "tokens": ["Weil", "du", "die", "Bli\u00b7cke", "senkst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie durch die stillen Wiesen", "tokens": ["Wie", "durch", "die", "stil\u00b7len", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die B\u00e4chlein murmelnd gehn,", "tokens": ["Die", "B\u00e4ch\u00b7lein", "mur\u00b7melnd", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-