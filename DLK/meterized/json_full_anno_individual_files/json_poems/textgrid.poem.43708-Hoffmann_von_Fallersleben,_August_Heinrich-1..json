{"textgrid.poem.43708": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Freude, wenn wir heute", "tokens": ["Wel\u00b7che", "Freu\u00b7de", ",", "wenn", "wir", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieh'n das gro\u00dfe Netz ans Land,", "tokens": ["Zieh'n", "das", "gro\u00b7\u00dfe", "Netz", "ans", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es liegt vor uns die Beute", "tokens": ["Und", "es", "liegt", "vor", "uns", "die", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausgebreitet auf dem Sand!", "tokens": ["Aus\u00b7ge\u00b7brei\u00b7tet", "auf", "dem", "Sand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche Fische bei einander!", "tokens": ["Wel\u00b7che", "Fi\u00b7sche", "bei", "ein\u00b7an\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klein' und gro\u00df' und allerlei!", "tokens": ["Klein'", "und", "gro\u00df'", "und", "al\u00b7ler\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aal, Karausche, Barsch und Zander,", "tokens": ["Aal", ",", "Ka\u00b7rau\u00b7sche", ",", "Barsch", "und", "Zan\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brachse, Karpfe, Hecht und Schlei.", "tokens": ["Brach\u00b7se", ",", "Karp\u00b7fe", ",", "Hecht", "und", "Schlei", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun, so la\u00dft das Netz uns senken", "tokens": ["Nun", ",", "so", "la\u00dft", "das", "Netz", "uns", "sen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Wasser tief hinein,", "tokens": ["In", "das", "Was\u00b7ser", "tief", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Fische, eh' wir's denken,", "tokens": ["Und", "die", "Fi\u00b7sche", ",", "eh'", "wir's", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden sie gefangen sein.", "tokens": ["Wer\u00b7den", "sie", "ge\u00b7fan\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--