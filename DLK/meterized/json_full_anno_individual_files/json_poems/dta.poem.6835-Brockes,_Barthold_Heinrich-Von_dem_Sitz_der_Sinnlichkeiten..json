{"dta.poem.6835": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann eine Lauten-Sait' ein weiser WEISE r\u00fchret;", "tokens": ["Wann", "ei\u00b7ne", "Lau\u00b7ten\u00b7Sait'", "ein", "wei\u00b7ser", "WeI\u00b7SE", "r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird der Schlag, wodurch er sie belebt,", "tokens": ["So", "wird", "der", "Schlag", ",", "wo\u00b7durch", "er", "sie", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er sich bis ans Ohr erhebt,", "tokens": ["Wenn", "er", "sich", "bis", "ans", "Ohr", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch unsrer Nerven Gang bi\u00df ins Gehirn gef\u00fchret.", "tokens": ["Durch", "uns\u00b7rer", "Ner\u00b7ven", "Gang", "bi\u00df", "ins", "Ge\u00b7hirn", "ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Nerven nun sowol, als auch die Sait\u2019,", "tokens": ["Die", "Ner\u00b7ven", "nun", "so\u00b7wol", ",", "als", "auch", "die", "Sait'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist blo\u00df Materie, auf gleiche Art bewegt,", "tokens": ["Ist", "blo\u00df", "Ma\u00b7te\u00b7rie", ",", "auf", "glei\u00b7che", "Art", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Die beyd\u2019 ein gleiches Zittern regt.", "tokens": ["Die", "beyd'", "ein", "glei\u00b7ches", "Zit\u00b7tern", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PIS", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Allein heisst dies Empfindlichkeit?", "tokens": ["Al\u00b7lein", "heisst", "dies", "Emp\u00b7find\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sind sie nun im Gehirn; so regt sich zwar", "tokens": ["Sind", "sie", "nun", "im", "Ge\u00b7hirn", ";", "so", "regt", "sich", "zwar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "$.", "ADV", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Desselben graues Marck, das, z\u00e4rter als ein Haar,", "tokens": ["Des\u00b7sel\u00b7ben", "grau\u00b7es", "Marck", ",", "das", ",", "z\u00e4r\u00b7ter", "als", "ein", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "PDS", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In kleinen F\u00e4den liegt, viel sanffter: doch de\u00dfwegen", "tokens": ["In", "klei\u00b7nen", "F\u00e4\u00b7den", "liegt", ",", "viel", "sanff\u00b7ter", ":", "doch", "de\u00df\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADJD", "$.", "ADV", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vermag man ihm mit Recht nichts sinnlichs beyzulegen.", "tokens": ["Ver\u00b7mag", "man", "ihm", "mit", "Recht", "nichts", "sinn\u00b7lichs", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "PIS", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}