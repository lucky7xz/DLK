{"textgrid.poem.62751": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als in Thessaliens Gefilden", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als in Thessaliens Gefilden", "tokens": ["Als", "in", "Thes\u00b7sa\u00b7li\u00b7ens", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Apoll, der Exulant, voll Menschenfreundlichkeit", "tokens": ["A\u00b7poll", ",", "der", "E\u00b7xul\u00b7ant", ",", "voll", "Men\u00b7schen\u00b7freund\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "ADJD", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Noch Gott im h\u00e4nfnen Sch\u00e4ferkleid,", "tokens": ["Noch", "Gott", "im", "h\u00e4nf\u00b7nen", "Sch\u00e4\u00b7fer\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bem\u00fchet war die Hirten umzubilden;", "tokens": ["Be\u00b7m\u00fc\u00b7het", "war", "die", "Hir\u00b7ten", "um\u00b7zu\u00b7bil\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sein harmonischer Gesang", "tokens": ["Da", "sein", "har\u00b7mo\u00b7ni\u00b7scher", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und seiner Fl\u00f6te Zauberklang", "tokens": ["Und", "sei\u00b7ner", "Fl\u00f6\u00b7te", "Zau\u00b7ber\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die entz\u00fcckten Fluren hallte,", "tokens": ["Durch", "die", "ent\u00b7z\u00fcck\u00b7ten", "Flu\u00b7ren", "hall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und, angespornt von einem neuen Trieb,", "tokens": ["Und", ",", "an\u00b7ge\u00b7spornt", "von", "ei\u00b7nem", "neu\u00b7en", "Trieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der J\u00fcngling auch sich eine Fl\u00f6te hieb;", "tokens": ["Der", "J\u00fcng\u00b7ling", "auch", "sich", "ei\u00b7ne", "Fl\u00f6\u00b7te", "hieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das M\u00e4dchen Ph\u00f6bus Hymnen lallte:", "tokens": ["Das", "M\u00e4d\u00b7chen", "Ph\u00f6\u00b7bus", "Hym\u00b7nen", "lall\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In jener ersten goldnen Zeit", "tokens": ["In", "je\u00b7ner", "ers\u00b7ten", "gold\u00b7nen", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sa\u00df einst Apoll im Schoo\u00dfe junger Myrthen", "tokens": ["Sa\u00df", "einst", "A\u00b7poll", "im", "Schoo\u00b7\u00dfe", "jun\u00b7ger", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "NE", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Und blies und sang, behorcht von einem kleinen Hirten,", "tokens": ["Und", "blies", "und", "sang", ",", "be\u00b7horcht", "von", "ei\u00b7nem", "klei\u00b7nen", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein hohes Lied von der Zufriedenheit.", "tokens": ["Ein", "ho\u00b7hes", "Lied", "von", "der", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Knabe f\u00fchlt. Wer m\u00fc\u00dfte da nicht f\u00fchlen?", "tokens": ["Der", "Kna\u00b7be", "f\u00fchlt", ".", "Wer", "m\u00fc\u00df\u00b7te", "da", "nicht", "f\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Berauscht von dem, was er empfand,", "tokens": ["Be\u00b7rauscht", "von", "dem", ",", "was", "er", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ergriff er schnell den Ph\u00f6bus bey der Hand,", "tokens": ["Er\u00b7griff", "er", "schnell", "den", "Ph\u00f6\u00b7bus", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und bat: o la\u00df mich auch ein Liedchen spielen!", "tokens": ["Und", "bat", ":", "o", "la\u00df", "mich", "auch", "ein", "Lied\u00b7chen", "spie\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "FM", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der holde Gott reicht ihm das Rohr,", "tokens": ["Der", "hol\u00b7de", "Gott", "reicht", "ihm", "das", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der Knabe h\u00fcpft und lacht, und f\u00e4hrt damit zum Munde;", "tokens": ["Der", "Kna\u00b7be", "h\u00fcpft", "und", "lacht", ",", "und", "f\u00e4hrt", "da\u00b7mit", "zum", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Schon bl\u00e4st und fingert er bald eine halbe Stunde;", "tokens": ["Schon", "bl\u00e4st", "und", "fin\u00b7gert", "er", "bald", "ei\u00b7ne", "hal\u00b7be", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein es kommt kein Lied hervor.", "tokens": ["Al\u00b7lein", "es", "kommt", "kein", "Lied", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wie schauerte bey diesem Meisterst\u00fccke", "tokens": ["Wie", "schau\u00b7er\u00b7te", "bey", "die\u00b7sem", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Des guten Ph\u00f6bus zartes Ohr!", "tokens": ["Des", "gu\u00b7ten", "Ph\u00f6\u00b7bus", "zar\u00b7tes", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Zuletzt nahm er mit v\u00e4terlichem Blicke", "tokens": ["Zu\u00b7letzt", "nahm", "er", "mit", "v\u00e4\u00b7ter\u00b7li\u00b7chem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "Das allzulang entweihte Rohr", "tokens": ["Das", "all\u00b7zu\u00b7lang", "ent\u00b7weih\u00b7te", "Rohr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Aus der verwegnen Hand zur\u00fccke.", "tokens": ["Aus", "der", "ver\u00b7weg\u00b7nen", "Hand", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Beym Pan; das Ding ist schwer, so schwur der kleine Mann.", "tokens": ["Beym", "Pan", ";", "das", "Ding", "ist", "schwer", ",", "so", "schwur", "der", "klei\u00b7ne", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ey nun, er brauchte nicht zu schw\u00f6ren;", "tokens": ["Ey", "nun", ",", "er", "brauch\u00b7te", "nicht", "zu", "schw\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Doch, fuhr er kindisch fort, was ich nicht kann,", "tokens": ["Doch", ",", "fuhr", "er", "kin\u00b7disch", "fort", ",", "was", "ich", "nicht", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "O Ph\u00f6bus, das mu\u00dft du mich lehren.", "tokens": ["O", "Ph\u00f6\u00b7bus", ",", "das", "mu\u00dft", "du", "mich", "leh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PDS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Als in Thessaliens Gefilden", "tokens": ["Als", "in", "Thes\u00b7sa\u00b7li\u00b7ens", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Apoll, der Exulant, voll Menschenfreundlichkeit", "tokens": ["A\u00b7poll", ",", "der", "E\u00b7xul\u00b7ant", ",", "voll", "Men\u00b7schen\u00b7freund\