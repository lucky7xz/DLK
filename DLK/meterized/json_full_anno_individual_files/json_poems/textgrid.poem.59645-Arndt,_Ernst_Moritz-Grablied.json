{"textgrid.poem.59645": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Grablied", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht nun hin und grabt mein Grab,", "tokens": ["Geht", "nun", "hin", "und", "grabt", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich bin des Wanderns m\u00fcde,", "tokens": ["Denn", "ich", "bin", "des", "Wan\u00b7derns", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Erde scheid' ich ab,", "tokens": ["Von", "der", "Er\u00b7de", "scheid'", "ich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mir ruft des Himmels Friede,", "tokens": ["Denn", "mir", "ruft", "des", "Him\u00b7mels", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mir ruft die s\u00fc\u00dfe Ruh'", "tokens": ["Denn", "mir", "ruft", "die", "s\u00fc\u00b7\u00dfe", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den Engeln droben zu.", "tokens": ["Von", "den", "En\u00b7geln", "dro\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht nun hin und grabt mein Grab,", "tokens": ["Geht", "nun", "hin", "und", "grabt", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Lauf hab' ich vollendet,", "tokens": ["Mei\u00b7nen", "Lauf", "hab'", "ich", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lege nun den Wanderstab", "tokens": ["Le\u00b7ge", "nun", "den", "Wan\u00b7der\u00b7stab"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin, wo alles Ird'sche endet,", "tokens": ["Hin", ",", "wo", "al\u00b7les", "Ird'\u00b7sche", "en\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lege selbst mich nun hinein", "tokens": ["Le\u00b7ge", "selbst", "mich", "nun", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ADV", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In das Bette sonder Pein.", "tokens": ["In", "das", "Bet\u00b7te", "son\u00b7der", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was soll ich hienieden noch", "tokens": ["Was", "soll", "ich", "hien\u00b7ie\u00b7den", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem dunklen Tale machen?", "tokens": ["In", "dem", "dunk\u00b7len", "Ta\u00b7le", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn wie m\u00e4chtig stolz und hoch", "tokens": ["Denn", "wie", "m\u00e4ch\u00b7tig", "stolz", "und", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wir auch stellen unsre Sachen,", "tokens": ["Wir", "auch", "stel\u00b7len", "uns\u00b7re", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df es doch wie Sand zergehn,", "tokens": ["Mu\u00df", "es", "doch", "wie", "Sand", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann die Winde dr\u00fcber wehn.", "tokens": ["Wann", "die", "Win\u00b7de", "dr\u00fc\u00b7ber", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Darum, Erde, fahre wohl!", "tokens": ["Da\u00b7rum", ",", "Er\u00b7de", ",", "fah\u00b7re", "wohl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich nun in Frieden scheiden,", "tokens": ["La\u00df", "mich", "nun", "in", "Frie\u00b7den", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Hoffnung ach! ist hohl,", "tokens": ["Dei\u00b7ne", "Hoff\u00b7nung", "ach", "!", "ist", "hohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ITJ", "$.", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Freuden werden Leiden,", "tokens": ["Dei\u00b7ne", "Freu\u00b7den", "wer\u00b7den", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Sch\u00f6nheit Unbestand:", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "Un\u00b7be\u00b7stand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles Wahn und Trug und Tand.", "tokens": ["Al\u00b7les", "Wahn", "und", "Trug", "und", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Darum letzte gute Nacht,", "tokens": ["Da\u00b7rum", "letz\u00b7te", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonn' und Mond und liebe Sterne!", "tokens": ["Sonn'", "und", "Mond", "und", "lie\u00b7be", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fahret wohl mit eurer Pracht!", "tokens": ["Fah\u00b7ret", "wohl", "mit", "eu\u00b7rer", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn ich reis' in weite Ferne,", "tokens": ["Denn", "ich", "reis'", "in", "wei\u00b7te", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Reise hin zu jenem Glanz,", "tokens": ["Rei\u00b7se", "hin", "zu", "je\u00b7nem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Worin ihr erbleichet ganz.", "tokens": ["Wo\u00b7rin", "ihr", "er\u00b7blei\u00b7chet", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihr, die nun in Trauern geht,", "tokens": ["Ihr", ",", "die", "nun", "in", "Trau\u00b7ern", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahret wohl, ihr lieben Freunde!", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "lie\u00b7ben", "Freun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was von oben niederweht,", "tokens": ["Was", "von", "o\u00b7ben", "nie\u00b7der\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00f6stet froh des Herrn Gemeinde;", "tokens": ["Tr\u00f6s\u00b7tet", "froh", "des", "Herrn", "Ge\u00b7mein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weint nicht ob dem eitlen Schein:", "tokens": ["Weint", "nicht", "ob", "dem", "eit\u00b7len", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Droben nur kann ewig sein.", "tokens": ["Dro\u00b7ben", "nur", "kann", "e\u00b7wig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Weinet nicht, da\u00df nun ich will", "tokens": ["Wei\u00b7net", "nicht", ",", "da\u00df", "nun", "ich", "will"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Welt den Abschied nehmen,", "tokens": ["Von", "der", "Welt", "den", "Ab\u00b7schied", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich aus dem Irrland will,", "tokens": ["Da\u00df", "ich", "aus", "dem", "Irr\u00b7land", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Schatten, aus den Schemen,", "tokens": ["Aus", "den", "Schat\u00b7ten", ",", "aus", "den", "Sche\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Eitlen, aus dem Nichts", "tokens": ["Aus", "dem", "Eit\u00b7len", ",", "aus", "dem", "Nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hin ins Land des ew'gen Lichts.", "tokens": ["Hin", "ins", "Land", "des", "ew'\u00b7gen", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weinet nicht, mein s\u00fc\u00dfes Heil,", "tokens": ["Wei\u00b7net", "nicht", ",", "mein", "s\u00fc\u00b7\u00dfes", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Heiland hab' ich funden,", "tokens": ["Mei\u00b7nen", "Hei\u00b7land", "hab'", "ich", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich habe auch mein Teil", "tokens": ["Und", "ich", "ha\u00b7be", "auch", "mein", "Teil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In den warmen Herzenswunden,", "tokens": ["In", "den", "war\u00b7men", "Her\u00b7zens\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Woraus einst sein frommes Blut", "tokens": ["Wo\u00b7raus", "einst", "sein", "from\u00b7mes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Flo\u00df der ganzen Welt zugut'.", "tokens": ["Flo\u00df", "der", "gan\u00b7zen", "Welt", "zu\u00b7gut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Weint nicht: Mein Erl\u00f6ser lebt,", "tokens": ["Weint", "nicht", ":", "Mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch vom finstern Erdenstaube", "tokens": ["Hoch", "vom", "fins\u00b7tern", "Er\u00b7dens\u00b7tau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hell empor die Hoffnung schwebt,", "tokens": ["Hell", "em\u00b7por", "die", "Hoff\u00b7nung", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Himmelsheld, der Glaube,", "tokens": ["Und", "der", "Him\u00b7mels\u00b7held", ",", "der", "Glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die ewige Liebe spricht:", "tokens": ["Und", "die", "e\u00b7wi\u00b7ge", "Lie\u00b7be", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Geht nun hin und grabt mein Grab,", "tokens": ["Geht", "nun", "hin", "und", "grabt", "mein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich bin des Wanderns m\u00fcde,", "tokens": ["Denn", "ich", "bin", "des", "Wan\u00b7derns", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Erde scheid' ich ab,", "tokens": ["Von", "der", "Er\u00b7de", "scheid'", "ich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn mir ruft des Himmels Friede,", "tokens": ["Denn", "mir", "ruft", "des", "Him\u00b7mels", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mir ruft die s\u00fc\u00dfe Ruh'", "tokens": ["Denn", "mir", "ruft", "die", "s\u00fc\u00b7\u00dfe", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure":