{"textgrid.poem.62448": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hans von Sagan war gehei\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans von Sagan war gehei\u00dfen", "tokens": ["Hans", "von", "Sa\u00b7gan", "war", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst ein fr\u00f6hlicher Gesell,", "tokens": ["Einst", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der im sch\u00f6nen Lande Preu\u00dfen", "tokens": ["Der", "im", "sch\u00f6\u00b7nen", "Lan\u00b7de", "Preu\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat gefochten k\u00fchn und schnell.", "tokens": ["Hat", "ge\u00b7foch\u00b7ten", "k\u00fchn", "und", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo vom alten B\u00fcrgerwesen", "tokens": ["Wo", "vom", "al\u00b7ten", "B\u00fcr\u00b7ger\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viel der goldnen Worte stehn,", "tokens": ["Viel", "der", "gold\u00b7nen", "Wor\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Chronik ist zu lesen,", "tokens": ["In", "der", "Chro\u00b7nik", "ist", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er lie\u00df die Fahnen wehn.", "tokens": ["Wie", "er", "lie\u00df", "die", "Fah\u00b7nen", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ordensfahne war gesunken", "tokens": ["Or\u00b7dens\u00b7fah\u00b7ne", "war", "ge\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Feinde drangen an,", "tokens": ["Und", "die", "Fein\u00b7de", "dran\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hans von Sagan muthestrunken,", "tokens": ["Hans", "von", "Sa\u00b7gan", "mu\u00b7thes\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stand ein rechter Landwehrmann.", "tokens": ["Stand", "ein", "rech\u00b7ter", "Land\u00b7wehr\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wu\u00dfte tapfer dich zu schirmen,", "tokens": ["Wu\u00df\u00b7te", "tap\u00b7fer", "dich", "zu", "schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nigsberg, du gute Stadt,", "tokens": ["K\u00f6\u00b7nigs\u00b7berg", ",", "du", "gu\u00b7te", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in Br\u00fccken und an Th\u00fcrmen", "tokens": ["Die", "in", "Br\u00fc\u00b7cken", "und", "an", "Th\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Sch\u00f6nheit Reichthum hat.", "tokens": ["Und", "an", "Sch\u00f6n\u00b7heit", "Reicht\u00b7hum", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In zwei schmalen dunkeln Gassen,", "tokens": ["In", "zwei", "schma\u00b7len", "dun\u00b7keln", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des alten Pregels Rand,", "tokens": ["An", "des", "al\u00b7ten", "Pre\u00b7gels", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Buden winkt gelassen", "tokens": ["Aus", "den", "Bu\u00b7den", "winkt", "ge\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarzes Aug' und wei\u00dfe Hand.", "tokens": ["Schwar\u00b7zes", "Aug'", "und", "wei\u00b7\u00dfe", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbgr\u00fc\u00df euch Gott, ihr sch\u00f6nen M\u00e4dchen,", "tokens": ["\u00bb", "gr\u00fc\u00df", "euch", "Gott", ",", "ihr", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft mich weilen vor der Fahrt,", "tokens": ["La\u00dft", "mich", "wei\u00b7len", "vor", "der", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will euch n\u00e4h'n mit festem Dr\u00e4htchen", "tokens": ["Will", "euch", "n\u00e4h'n", "mit", "fes\u00b7tem", "Dr\u00e4ht\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Pantoffeln weich und zart.\u00ab", "tokens": ["Die", "Pan\u00b7tof\u00b7feln", "weich", "und", "zart", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Oben wohnen die Studenten,", "tokens": ["O\u00b7ben", "woh\u00b7nen", "die", "Stu\u00b7den\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sitzen bei dem schwachen Licht,", "tokens": ["Sit\u00b7zen", "bei", "dem", "schwa\u00b7chen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir nicht Studenten kennten!", "tokens": ["Wenn", "wir", "nicht", "Stu\u00b7den\u00b7ten", "kenn\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In die Kammer la\u00dft sie nicht.", "tokens": ["In", "die", "Kam\u00b7mer", "la\u00dft", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schneider sind zum Spott erlesen", "tokens": ["Schnei\u00b7der", "sind", "zum", "Spott", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem weiten deutschen Land.", "tokens": ["In", "dem", "wei\u00b7ten", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schneiderwerk und Schneiderwesen", "tokens": ["Schnei\u00b7der\u00b7werk", "und", "Schnei\u00b7der\u00b7we\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dienet auch zu Putz und Tand.", "tokens": ["Die\u00b7net", "auch", "zu", "Putz", "und", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Schusterarbeit h\u00e4lt die Proben", "tokens": ["Schus\u00b7ter\u00b7ar\u00b7beit", "h\u00e4lt", "die", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Reise und im Feld,", "tokens": ["Auf", "der", "Rei\u00b7se", "und", "im", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schusterarbeit mu\u00df man loben,", "tokens": ["Schus\u00b7ter\u00b7ar\u00b7beit", "mu\u00df", "man", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die das Herz gesund erh\u00e4lt.", "tokens": ["Die", "das", "Herz", "ge\u00b7sund", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schneider will den Leib umfahen,", "tokens": ["Schnei\u00b7der", "will", "den", "Leib", "um\u00b7fa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schuster kniet vor seinem Kind,", "tokens": ["Schus\u00b7ter", "kniet", "vor", "sei\u00b7nem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich in Z\u00fcchten ihr zu nahen,", "tokens": ["Sich", "in", "Z\u00fcch\u00b7ten", "ihr", "zu", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt er immerdar gesinnt.", "tokens": ["Bleibt", "er", "im\u00b7mer\u00b7dar", "ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Darum sie zum Tanze f\u00fchren", "tokens": ["Da\u00b7rum", "sie", "zum", "Tan\u00b7ze", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPRART", "NN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Darf er am Johannisfest,", "tokens": ["Darf", "er", "am", "Jo\u00b7han\u00b7nis\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der sch\u00f6nste Fu\u00df sich sp\u00fcren", "tokens": ["Wo", "der", "sch\u00f6ns\u00b7te", "Fu\u00df", "sich", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gern im sch\u00f6nsten Schuhe l\u00e4\u00dft.", "tokens": ["Gern", "im", "sch\u00f6ns\u00b7ten", "Schu\u00b7he", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Hans von Sagan war gehei\u00dfen", "tokens": ["Hans", "von", "Sa\u00b7gan", "war", "ge\u00b7hei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NE", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst ein muthiger Gesell,", "tokens": ["Einst", "ein", "mut\u00b7hi\u00b7ger", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der allhier im Lande Preu\u00dfen", "tokens": ["Der", "all\u00b7hier", "im", "Lan\u00b7de", "Preu\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hat gefochten k\u00fchn und schnell.", "tokens": ["Hat", "ge\u00b7foch\u00b7ten", "k\u00fchn", "und", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "In der Vorstadt mit der Fahne", "tokens": ["In", "der", "Vor\u00b7stadt", "mit", "der", "Fah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Brunnen steht sein Bild,", "tokens": ["Auf", "dem", "Brun\u00b7nen", "steht", "sein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Preu\u00dfenm\u00e4dchen, gelt, ich ahne,", "tokens": ["Preu\u00b7\u00dfen\u00b7m\u00e4d\u00b7chen", ",", "gelt", ",", "ich", "ah\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein freies Herz erf\u00fcllt.", "tokens": ["Was", "dein", "frei\u00b7es", "Herz", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Horch die hellen Trommeln schallen,", "tokens": ["Horch", "die", "hel\u00b7len", "Trom\u00b7meln", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau', das junge Volk erwacht,", "tokens": ["Schau'", ",", "das", "jun\u00b7ge", "Volk", "er\u00b7wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig aus des Remters Hallen", "tokens": ["Freu\u00b7dig", "aus", "des", "Rem\u00b7ters", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieh' ich in die blut'ge Schlacht.", "tokens": ["Zieh'", "ich", "in", "die", "blut'\u00b7ge", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Lebe wohl Gesellenlade,", "tokens": ["Le\u00b7be", "wohl", "Ge\u00b7sel\u00b7len\u00b7la\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein werther Aeltermann,", "tokens": ["Du", "mein", "wert\u00b7her", "A\u00b7el\u00b7ter\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Meistertochter, deren Gnade", "tokens": ["Meis\u00b7ter\u00b7t