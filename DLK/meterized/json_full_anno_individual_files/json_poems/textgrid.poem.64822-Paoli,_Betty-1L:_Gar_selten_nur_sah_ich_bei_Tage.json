{"textgrid.poem.64822": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gar selten nur sah ich bei Tage", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gar selten nur sah ich bei Tage", "tokens": ["Gar", "sel\u00b7ten", "nur", "sah", "ich", "bei", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dich, meinen Herrn!", "tokens": ["Dich", ",", "mei\u00b7nen", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da hielt der Erde Lust und Plage", "tokens": ["Da", "hielt", "der", "Er\u00b7de", "Lust", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich von mir fern;", "tokens": ["Dich", "von", "mir", "fern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und immer bist du nur gekommen,", "tokens": ["Und", "im\u00b7mer", "bist", "du", "nur", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn fern am Horizont erglommen", "tokens": ["Wenn", "fern", "am", "Ho\u00b7ri\u00b7zont", "er\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-