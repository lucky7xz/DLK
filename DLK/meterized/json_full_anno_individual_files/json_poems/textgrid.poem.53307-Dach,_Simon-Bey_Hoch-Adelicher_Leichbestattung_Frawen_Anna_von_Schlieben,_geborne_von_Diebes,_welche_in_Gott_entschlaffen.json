r", "so", ",", "jhr", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begrabt mich Christlich, als jhr thut,", "tokens": ["Be\u00b7grabt", "mich", "Christ\u00b7lich", ",", "als", "jhr", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd tr\u00f6stet nachmals ewren Muth,", "tokens": ["Vnd", "tr\u00f6s\u00b7tet", "nach\u00b7mals", "ew\u00b7ren", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sterbet mit der Zeit nicht minder.", "tokens": ["Ihr", "ster\u00b7bet", "mit", "der", "Zeit", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denckt an den Tod, seyd allzeit wach!", "tokens": ["Denckt", "an", "den", "Tod", ",", "seyd", "all\u00b7zeit", "wach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ich reise vor, jhr folget nach.", "tokens": ["Ich", "rei\u00b7se", "vor", ",", "jhr", "fol\u00b7get", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}