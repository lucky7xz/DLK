{"textgrid.poem.53307": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey Hoch-Adelicher Leichbestattung Frawen Anna von Schlieben, geborne von Diebes, welche in Gott entschlaffen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv, O getrewe Mutter, Erde,", "tokens": ["Dv", ",", "O", "ge\u00b7tre\u00b7we", "Mut\u00b7ter", ",", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am allerbesten ist es doch,", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "ist", "es", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auf des schweren Alters Joch", "tokens": ["Da\u00df", "auf", "des", "schwe\u00b7ren", "Al\u00b7ters", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Mensch in dich verscharret werde,", "tokens": ["Ein", "Mensch", "in", "dich", "ver\u00b7schar\u00b7ret", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schlaffe stoltz vnd vngeschreckt", "tokens": ["Vnd", "schlaf\u00b7fe", "stoltz", "vnd", "vn\u00b7ge\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis jhn der J\u00fcngste Tag erweckt.", "tokens": ["Bis", "jhn", "der", "J\u00fcngs\u00b7te", "Tag", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Welt kan vns nicht ewig haben,", "tokens": ["Die", "Welt", "kan", "vns", "nicht", "e\u00b7wig", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird noch vnser endlich satt,", "tokens": ["Sie", "wird", "noch", "vn\u00b7ser", "end\u00b7lich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind verdrie\u00dflich, alt vnd matt.", "tokens": ["Wir", "sind", "ver\u00b7drie\u00df\u00b7lich", ",", "alt", "vnd", "matt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was bessers ist, als seyn begraben,", "tokens": ["Was", "bes\u00b7sers", "ist", ",", "als", "seyn", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "KOUS", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd r\u00e4umen Welt vnd jhre Pein", "tokens": ["Vnd", "r\u00e4u\u00b7men", "Welt", "vnd", "jhre", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Der Nachfahrt, vnserm Samen ein?", "tokens": ["Der", "Nach\u00b7fahrt", ",", "vn\u00b7serm", "Sa\u00b7men", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du birgst das Wohnhau\u00df vnsrer Seelen,", "tokens": ["Du", "birgst", "das", "Wohn\u00b7hau\u00df", "vns\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Leichnam, tieff in deinen Schos,", "tokens": ["Den", "Leich\u00b7nam", ",", "tieff", "in", "dei\u00b7nen", "Schos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ruht er aller Sorgen los", "tokens": ["Da", "ruht", "er", "al\u00b7ler", "Sor\u00b7gen", "los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den geheiligt-stillen H\u00f6len,", "tokens": ["In", "den", "ge\u00b7hei\u00b7lig\u00b7tstil\u00b7len", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis jhm der grosse Seelen-Hirt", "tokens": ["Bis", "jhm", "der", "gros\u00b7se", "See\u00b7len\u00b7Hirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch aus den Wolcken ruffen wird:", "tokens": ["Hoch", "aus", "den", "Wol\u00b7cken", "ruf\u00b7fen", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Todten, findet euch nun wieder,", "tokens": ["Ihr", "Tod\u00b7ten", ",", "fin\u00b7det", "euch", "nun", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kompt, werdet vor Gericht geschawt!", "tokens": ["Kompt", ",", "wer\u00b7det", "vor", "Ge\u00b7richt", "ge\u00b7schawt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wirstu, was dir anvertrawt,", "tokens": ["Da", "wirs\u00b7tu", ",", "was", "dir", "an\u00b7ver\u00b7trawt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Haut, Fleisch, Gebein vnd alle Glieder", "tokens": ["Haut", ",", "Fleisch", ",", "Ge\u00b7bein", "vnd", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vns wieder geben also wol", "tokens": ["Vns", "wie\u00b7der", "ge\u00b7ben", "al\u00b7so", "wol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auch kein Zahn gebrechen sol.", "tokens": ["Da\u00df", "auch", "kein", "Zahn", "ge\u00b7bre\u00b7chen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich war zu leben gantz verdrossen,", "tokens": ["Ich", "war", "zu", "le\u00b7ben", "gantz", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wol mir, da\u00df ich gestorben bin!", "tokens": ["Wol", "mir", ",", "da\u00df", "ich", "ge\u00b7stor\u00b7ben", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Im Himmel schwebt mein Geist vnd Sinn,", "tokens": ["Im", "Him\u00b7mel", "schwebt", "mein", "Geist", "vnd", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hie sprech ich meine Blutsgenossen;", "tokens": ["Hie", "sprech", "ich", "mei\u00b7ne", "Bluts\u00b7ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der H\u00f6chste selbst ertheilt sich mir", "tokens": ["Der", "H\u00f6chs\u00b7te", "selbst", "er\u00b7theilt", "sich", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In aller Pracht vnd heilgen Zier.", "tokens": ["In", "al\u00b7ler", "Pracht", "vnd", "heil\u00b7gen", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du, Erde, die mich erst geboren", "tokens": ["Du", ",", "Er\u00b7de", ",", "die", "mich", "erst", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd wol gen\u00e4hrt, nimbst mein Gebein", "tokens": ["Vnd", "wol", "ge\u00b7n\u00e4hrt", ",", "nimbst", "mein", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meinen Kindern zu dir ein,", "tokens": ["Von", "mei\u00b7nen", "Kin\u00b7dern", "zu", "dir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hie bleibt kein Nagel mir verlohren,", "tokens": ["Hie", "bleibt", "kein", "Na\u00b7gel", "mir", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wiewol nur \u00fcber wenig Jahr", "tokens": ["Wie\u00b7wol", "nur", "\u00fc\u00b7ber", "we\u00b7nig", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vmb mich ist weder Haut noch Haar.", "tokens": ["Vmb", "mich", "ist", "we\u00b7der", "Haut", "noch", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was weint jhr so, jhr lieben Kinder!", "tokens": ["Was", "weint", "jhr", "so", ",", "jhr", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begrabt mich Christlich, als jhr thut,", "tokens": ["Be\u00b7grabt", "mich", "Christ\u00b7lich", ",", "als", "jhr", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd tr\u00f6stet nachmals ewren Muth,", "tokens": ["Vnd", "tr\u00f6s\u00b7tet", "nach\u00b7mals", "ew\u00b7ren", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sterbet mit der Zeit nicht minder.", "tokens": ["Ihr", "ster\u00b7bet", "mit", "der", "Zeit", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denckt an den Tod, seyd allzeit wach!", "tokens": ["Denckt", "an", "den", "Tod", ",", "seyd", "all\u00b7zeit", "wach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ich reise vor, jhr folget nach.", "tokens": ["Ich", "rei\u00b7se", "vor", ",", "jhr", "fol\u00b7get", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dv, O getrewe Mutter, Erde,", "tokens": ["Dv", ",", "O", "ge\u00b7tre\u00b7we", "Mut\u00b7ter", ",", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am allerbesten ist es doch,", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "ist", "es", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df auf des schweren Alters Joch", "tokens": ["Da\u00df", "auf", "des", "schwe\u00b7ren", "Al\u00b7ters", "Joch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Mensch in dich verscharret werde,", "tokens": ["Ein", "Mensch", "in", "dich", "ver\u00b7schar\u00b7ret", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schlaffe stoltz vnd vngeschreckt", "tokens": ["Vnd", "schlaf\u00b7fe", "stoltz", "vnd", "vn\u00b7ge\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis jhn der J\u00fcngste Tag erweckt.", "tokens": ["Bis", "jhn", "der", "J\u00fcngs\u00b7te", "Tag", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Welt kan vns nicht ewig haben,", "tokens": ["Die", "Welt", "kan", "vns", "nicht", "e\u00b7wig", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird noch vnser endlich satt,", "tokens": ["Sie", "wird", "noch", "vn\u00b7ser", "end\u00b7lich", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind verdrie\u00dflich, alt vnd matt.", "tokens": ["Wir", "sind", "ver\u00b7drie\u00df\u00b7lich", ",", "alt", "vnd", "matt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was bessers ist, als seyn begraben,", "tokens": ["Was", "bes\u00b7sers", "ist", ",", "als", "seyn", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "KOUS", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd r\u00e4umen Welt vnd jhre Pein", "tokens": ["Vnd", "r\u00e4u\u00b7men", "Welt", "vnd", "jhre", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Der Nachfahrt, vnserm Samen ein?", "tokens": ["Der", "Nach\u00b7fahrt", ",", "vn\u00b7serm", "Sa\u00b7men", "ein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du birgst das Wohnhau\u00df vnsrer Seelen,", "tokens": ["Du", "birgst", "das", "Wohn\u00b7hau\u00df", "vns\u00b7rer", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Leichnam, tieff in deinen Schos,", "tokens": ["Den", "Leich\u00b7nam", ",", "tieff", "in", "dei\u00b7nen", "Schos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da ruht er aller Sorgen los", "tokens": ["Da", "ruht", "er", "al\u00b7ler", "Sor\u00b7gen", "los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den geheiligt-stillen H\u00f6len,", "tokens": ["In", "den", "ge\u00b7hei\u00b7lig\u00b7tstil\u00b7len", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis jhm der grosse Seelen-Hirt", "tokens": ["Bis", "jhm", "der", "gros\u00b7se", "See\u00b7len\u00b7Hirt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hoch aus den Wolcken ruffen wird:", "tokens": ["Hoch", "aus", "den", "Wol\u00b7cken", "ruf\u00b7fen", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihr Todten, findet euch nun wieder,", "tokens": ["Ihr", "Tod\u00b7ten", ",", "fin\u00b7det", "euch", "nun", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kompt, werdet vor Gericht geschawt!", "tokens": ["Kompt", ",", "wer\u00b7det", "vor", "Ge\u00b7richt", "ge\u00b7schawt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wirstu, was dir anvertrawt,", "tokens": ["Da", "wirs\u00b7tu", ",", "was", "dir", "an\u00b7ver\u00b7trawt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Haut, Fleisch, Gebein vnd alle Glieder", "tokens": ["Haut", ",", "Fleisch", ",", "Ge\u00b7bein", "vnd", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vns wieder geben also wol", "tokens": ["Vns", "wie\u00b7der", "ge\u00b7ben", "al\u00b7so", "wol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df auch kein Zahn gebrechen sol.", "tokens": ["Da\u00df", "auch", "kein", "Zahn", "ge\u00b7bre\u00b7chen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich war zu l