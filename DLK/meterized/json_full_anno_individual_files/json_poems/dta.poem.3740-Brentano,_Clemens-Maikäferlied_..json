{"dta.poem.3740": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Maik\u00e4ferlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "T\u00fcrkenm\u00e4nnchen, flieg hinweg,               ", "tokens": ["T\u00fcr\u00b7ken\u00b7m\u00e4nn\u00b7chen", ",", "flieg", "hin\u00b7weg", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Weiber mit den Stangen,", "tokens": ["Die", "Wei\u00b7ber", "mit", "den", "Stan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollen dich empfangen.", "tokens": ["Wol\u00b7len", "dich", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "T\u00fcrkenweibchen flieg hinweg,", "tokens": ["T\u00fcr\u00b7ken\u00b7weib\u00b7chen", "flieg", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die M\u00e4nner mit den Spiessen,", "tokens": ["Die", "M\u00e4n\u00b7ner", "mit", "den", "Spies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wollen dich erschie\u00dfen.", "tokens": ["Wol\u00b7len", "dich", "er\u00b7schie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Flieg in den Himmel,", "tokens": ["Flieg", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Bring mirn Sack voll K\u00fcmmel,", "tokens": ["Bring", "mirn", "Sack", "voll", "K\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Tunk ich meinen Weck hinein,", "tokens": ["Tunk", "ich", "mei\u00b7nen", "Weck", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Bei dem rothen k\u00fchlen Wein.", "tokens": ["Bei", "dem", "ro\u00b7then", "k\u00fch\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}