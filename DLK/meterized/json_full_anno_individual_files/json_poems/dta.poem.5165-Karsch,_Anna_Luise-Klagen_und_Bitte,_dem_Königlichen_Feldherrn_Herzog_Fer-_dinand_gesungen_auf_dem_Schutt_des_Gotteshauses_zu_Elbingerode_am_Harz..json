{"dta.poem.5165": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Klagen und Bitte,  \n dem K\u00f6niglichen Feldherrn Herzog Fer-  \n dinand gesungen auf dem Schutt des  \n Gotteshauses zu Elbingerode  \n am Harz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Held! der in tapfrer Hand verdeckte Keile tr\u00e4gt,\nBi\u00df schlangenf\u00f6rmig hin und wieder\nSein Blitz die Luft durchf\u00e4hrt, und schnell zur Erde nieder\nDes stolzen Frankreichs Fahnen schl\u00e4gt.", "tokens": ["Held", "!", "der", "in", "tapf\u00b7rer", "Hand", "ver\u00b7deck\u00b7te", "Kei\u00b7le", "tr\u00e4gt", ",", "Bi\u00df", "schlan\u00b7gen\u00b7f\u00f6r\u00b7mig", "hin", "und", "wie\u00b7der", "Sein", "Blitz", "die", "Luft", "durch\u00b7f\u00e4hrt", ",", "und", "schnell", "zur", "Er\u00b7de", "nie\u00b7der", "Des", "stol\u00b7zen", "Fran\u00b7kreichs", "Fah\u00b7nen", "schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "APPR", "ADJA", "NN", "VVFIN", "PIS", "VVFIN", "$,", "KOUS", "ADJD", "PTKVZ", "KON", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,", "KON", "ADJD", "APPRART", "NN", "PTKVZ", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-+-+-+--+-+-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}}}}}