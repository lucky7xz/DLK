{"textgrid.poem.58536": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "574. Die Schule der Weisheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ist gelehrt,", "tokens": ["Der", "ist", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer nichts f\u00fcr sich begehrt,", "tokens": ["Wer", "nichts", "f\u00fcr", "sich", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nichts will sein bei allen,", "tokens": ["Wer", "nichts", "will", "sein", "bei", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "PPOSAT", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer sich auch selbst nicht will gefallen,", "tokens": ["Wer", "sich", "auch", "selbst", "nicht", "will", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "ADV", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer in sich selbst nichts find't,", "tokens": ["Wer", "in", "sich", "selbst", "nichts", "find't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als da\u00df er wie ein kleines Kind", "tokens": ["Als", "da\u00df", "er", "wie", "ein", "klei\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sonst nichts will wissen, nichts will denken,", "tokens": ["Sonst", "nichts", "will", "wis\u00b7sen", ",", "nichts", "will", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "VVINF", "$,", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als in sein Nichts sich zu ersenken.", "tokens": ["Als", "in", "sein", "Nichts", "sich", "zu", "er\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "PIS", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O sch\u00f6nes Nichts,", "tokens": ["O", "sch\u00f6\u00b7nes", "Nichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Du F\u00fclle alles Lichts,", "tokens": ["Du", "F\u00fcl\u00b7le", "al\u00b7les", "Lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Du Sonne voller Klarheit,", "tokens": ["Du", "Son\u00b7ne", "vol\u00b7ler", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Du Brunnen aller reinen Wahrheit,", "tokens": ["Du", "Brun\u00b7nen", "al\u00b7ler", "rei\u00b7nen", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Verborgnes Winkelein,", "tokens": ["Ver\u00b7borg\u00b7nes", "Win\u00b7kel\u00