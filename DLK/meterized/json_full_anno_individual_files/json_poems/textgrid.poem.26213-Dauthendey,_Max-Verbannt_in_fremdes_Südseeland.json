{"textgrid.poem.26213": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Verbannt in fremdes S\u00fcdseeland", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verbannt in fremdes S\u00fcdseeland,", "tokens": ["Ver\u00b7bannt", "in", "frem\u00b7des", "S\u00fcd\u00b7see\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ich von ferne schauen,", "tokens": ["Mu\u00df", "ich", "von", "fer\u00b7ne", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie t\u00e4glich sich die Leichenberge Europas h\u00f6her bauen,", "tokens": ["Wie", "t\u00e4g\u00b7lich", "sich", "die", "Lei\u00b7chen\u00b7ber\u00b7ge", "Eu\u00b7ro\u00b7pas", "h\u00f6\u00b7her", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wie t\u00e4glich Heldentaten, gro\u00dfe, tausendfach geschehen,", "tokens": ["Wie", "t\u00e4g\u00b7lich", "Hel\u00b7den\u00b7ta\u00b7ten", ",", "gro\u00b7\u00dfe", ",", "tau\u00b7send\u00b7fach", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,", "ADJA", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Wie kleine B\u00e4che hochgef\u00fcllt mit Blut jetzt gehen,", "tokens": ["Wie", "klei\u00b7ne", "B\u00e4\u00b7che", "hoch\u00b7ge\u00b7f\