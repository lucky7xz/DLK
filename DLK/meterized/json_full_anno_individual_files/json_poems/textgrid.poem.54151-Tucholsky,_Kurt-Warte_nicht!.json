 "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADV", "KON", "ADV", "$."], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Kopf hoch! Es gibt einen Spruch,", "tokens": ["Kopf", "hoch", "!", "Es", "gibt", "ei\u00b7nen", "Spruch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "der strahlt \u00fcber allen Sorgen:", "tokens": ["der", "strahlt", "\u00fc\u00b7ber", "al\u00b7len", "Sor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Warte nicht zu lange,", "tokens": ["War\u00b7te", "nicht", "zu", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKA", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "warte nicht zu lang!", "tokens": ["war\u00b7te", "nicht", "zu", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Lausch deinem innern Klange,", "tokens": ["Lausch", "dei\u00b7nem", "in\u00b7nern", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "die Zeit geht ihren Gang.", "tokens": ["die", "Zeit", "geht", "ih\u00b7ren", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Jeder hat im Leben", "tokens": ["Je\u00b7der", "hat", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "eine Melodie . . .", "tokens": ["ei\u00b7ne", "Me\u00b7lo\u00b7die", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$.", "$.", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Und was du dir nicht selber nimmst,", "tokens": ["Und", "was", "du", "dir", "nicht", "sel\u00b7ber", "nimmst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "das erreichst du nie \u2013!", "tokens": ["das", "er\u00b7reichst", "du", "nie", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Du, junge Arbeiterin,", "tokens": ["Du", ",", "jun\u00b7ge", "Ar\u00b7bei\u00b7te\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "liebst einen, der dich liebt.", "tokens": ["liebst", "ei\u00b7nen", ",", "der", "dich", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sollst du ihn nehmen? Ist er ein Gewinn?", "tokens": ["Sollst", "du", "ihn", "neh\u00b7men", "?", "Ist", "er", "ein", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat er zu geben, wenn er gibt?", "tokens": ["Hat", "er", "zu", "ge\u00b7ben", ",", "wenn", "er", "gibt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mach reinen Tisch und entscheide dich \u2013", "tokens": ["Mach", "rei\u00b7nen", "Tisch", "und", "ent\u00b7schei\u00b7de", "dich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "VVFIN", "PPER", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "S\u00fcden oder Norden!", "tokens": ["S\u00fc\u00b7den", "o\u00b7der", "Nor\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ja oder Nein! \u2013 aber bleib nicht stehn,", "tokens": ["Ja", "o\u00b7der", "Nein", "!", "\u2013", "a\u00b7ber", "bleib", "nicht", "stehn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KON", "PTKANT", "$.", "$(", "ADV", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "noch keine ist j\u00fcnger geworden . . .", "tokens": ["noch", "kei\u00b7ne", "ist", "j\u00fcn\u00b7ger", "ge\u00b7wor\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PIAT", "VAFIN", "ADJD", "VAPP", "$.", "$.", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Warte nicht zu lange,", "tokens": ["War\u00b7te", "nicht", "zu", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKA", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "warte nicht z