{"dta.poem.5961": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht da den Wirbel an, in dessen grossem Creise", "tokens": ["Seht", "da", "den", "Wir\u00b7bel", "an", ",", "in", "des\u00b7sen", "gros\u00b7sem", "Crei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erde, Sonne, Mond, nebst andrer Kugeln-Heer,", "tokens": ["Die", "Er\u00b7de", ",", "Son\u00b7ne", ",", "Mond", ",", "nebst", "an\u00b7drer", "Ku\u00b7geln\u00b7Heer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Saturnus, Venus, Mars, Mercur und Jupiter", "tokens": ["Sa\u00b7tur\u00b7nus", ",", "Ve\u00b7nus", ",", "Mars", ",", "Mer\u00b7cur", "und", "Ju\u00b7pi\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NE", "KON", "NN"], "meter": "-+--++-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In einem Raum, der fl\u00fc\u00dfig, circkelweise", "tokens": ["In", "ei\u00b7nem", "Raum", ",", "der", "fl\u00fc\u00b7\u00dfig", ",", "cir\u00b7ckel\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Best\u00e4ndig vorwerts gehn,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "vor\u00b7werts", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wovon wir einige bald hell- bald dunckler sehn.", "tokens": ["Wo\u00b7von", "wir", "ei\u00b7ni\u00b7ge", "bald", "hell", "bald", "dunck\u00b7ler", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "TRUNC", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}