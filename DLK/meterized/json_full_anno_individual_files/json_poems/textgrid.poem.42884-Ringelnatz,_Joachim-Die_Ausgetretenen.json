{"textgrid.poem.42884": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Ausgetretenen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freifrau Berta von Sade,", "tokens": ["Die", "Frei\u00b7frau", "Ber\u00b7ta", "von", "Sa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die hielt sich auf ihrem Schlo\u00df", "tokens": ["Die", "hielt", "sich", "auf", "ih\u00b7rem", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der M\u00e4nnerretirade", "tokens": ["In", "der", "M\u00e4n\u00b7ner\u00b7re\u00b7ti\u00b7ra\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen L\u00f6wen, so gro\u00df wie ein Ponyro\u00df.", "tokens": ["Ei\u00b7nen", "L\u00f6\u00b7wen", ",", "so", "gro\u00df", "wie", "ein", "Po\u00b7ny\u00b7ro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Bei ihren ber\u00fcchtigten, tollen,", "tokens": ["Bei", "ih\u00b7ren", "be\u00b7r\u00fcch\u00b7tig\u00b7ten", ",", "tol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Staubaufwirbelnden Gastm\u00e4hlern sollen", "tokens": ["Staub\u00b7auf