{"dta.poem.2952": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "151.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df am Busch und sah hervor ein H\u00e4slein schlupfen,", "tokens": ["Ich", "sa\u00df", "am", "Busch", "und", "sah", "her\u00b7vor", "ein", "H\u00e4s\u00b7lein", "schlup\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das fieng im Abendschein sein Gr\u00e4slein an zu rupfen.", "tokens": ["Das", "fi\u00b7eng", "im", "A\u00b7bend\u00b7schein", "sein", "Gr\u00e4s\u00b7lein", "an", "zu", "rup\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die L\u00f6ffel reckt' es hoch, und schob die Augen gl\u00e4sern", "tokens": ["Die", "L\u00f6f\u00b7fel", "reckt'", "es", "hoch", ",", "und", "schob", "die", "Au\u00b7gen", "gl\u00e4\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umher, sobald ein Hauch sich regte in den Gr\u00e4sern.", "tokens": ["Um\u00b7her", ",", "so\u00b7bald", "ein", "Hauch", "sich", "reg\u00b7te", "in", "den", "Gr\u00e4\u00b7sern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mich ward es nicht gewahr, und sah nicht die Gefahr,", "tokens": ["Mich", "ward", "es", "nicht", "ge\u00b7wahr", ",", "und", "sah", "nicht", "die", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht weil ich ihm verdeckt, nur weil ich reglos war.", "tokens": ["Nicht", "weil", "ich", "ihm", "ver\u00b7deckt", ",", "nur", "weil", "ich", "reg\u00b7los", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PPER", "VVPP", "$,", "ADV", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da dacht' ich: o Natur, was dachte dein Verstand,", "tokens": ["Da", "dacht'", "ich", ":", "o", "Na\u00b7tur", ",", "was", "dach\u00b7te", "dein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "FM", "NN", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als deiner Sch\u00f6pferhand sich dies Gesch\u00f6pf entwand?", "tokens": ["Als", "dei\u00b7ner", "Sch\u00f6p\u00b7fer\u00b7hand", "sich", "dies", "Ge\u00b7sch\u00f6pf", "ent\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Begabt mit jedem Sinn, mit jedem blind und taub,", "tokens": ["Be\u00b7gabt", "mit", "je\u00b7dem", "Sinn", ",", "mit", "je\u00b7dem", "blind", "und", "taub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorm Feinde rasch zur Flucht, doch stets des Feindes Raub.", "tokens": ["Vorm", "Fein\u00b7de", "rasch", "zur", "Flucht", ",", "doch", "stets", "des", "Fein\u00b7des", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPRART", "NN", "$,", "ADV", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es lockt der Abendschein aus dumpfem Wald hervor,", "tokens": ["Es", "lockt", "der", "A\u00b7bend\u00b7schein", "aus", "dum\u00b7pfem", "Wald", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Zittern gras't's und blickt vom Futter nicht empor.", "tokens": ["Mit", "Zit\u00b7tern", "gras't's", "und", "blickt", "vom", "Fut\u00b7ter", "nicht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "VVFIN", "APPRART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich blick' empor zu Gott und dank' ihm diese Gabe,", "tokens": ["Ich", "blick'", "em\u00b7por", "zu", "Gott", "und", "dank'", "ihm", "die\u00b7se", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "NN", "KON", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich nicht wie das Thier vorm Tod zu zittern habe.", "tokens": ["Da\u00df", "ich", "nicht", "wie", "das", "Thier", "vorm", "Tod", "zu", "zit\u00b7tern", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "KOKOM", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}