{"textgrid.poem.57595": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Flugsand", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lange, gelblichgraue D\u00fcne,", "tokens": ["Du", "lan\u00b7ge", ",", "gelb\u00b7lich\u00b7grau\u00b7e", "D\u00fc\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du weites, graulichblaues Meer,", "tokens": ["Du", "wei\u00b7tes", ",", "grau\u00b7lich\u00b7blau\u00b7es", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es zuckt um meine ernste Miene", "tokens": ["Es", "zuckt", "um", "mei\u00b7ne", "erns\u00b7te", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der D\u00fcnenhafer hin und her;", "tokens": ["Der", "D\u00fc\u00b7nen\u00b7ha\u00b7fer", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stilleinsamkeit, du spendest s\u00fc\u00dfe", "tokens": ["Stil\u00b7lein\u00b7sam\u00b7keit", ",", "du", "spen\u00b7dest", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gef\u00fchle, lang nicht mehr gekannt,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", ",", "lang", "nicht", "mehr", "ge\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich recke mich, auf meine F\u00fc\u00dfe", "tokens": ["Ich", "re\u00b7cke", "mich", ",", "auf", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Rinnt leis herab der gelbe Sand.", "tokens": ["Rinnt", "leis", "her\u00b7ab", "der", "gel\u00b7be", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du gelber Sand, woher getrieben", "tokens": ["Du", "gel\u00b7ber", "Sand", ",", "wo\u00b7her", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dich des Windes Leidenschaft?", "tokens": ["Hat", "dich", "des", "Win\u00b7des", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin du f\u00e4llst, da mu\u00df zerstieben,", "tokens": ["Wo\u00b7hin", "du", "f\u00e4llst", ",", "da", "mu\u00df", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verwelken, d\u00f6rren Saft und Kraft;", "tokens": ["Ver\u00b7wel\u00b7ken", ",", "d\u00f6r\u00b7ren", "Saft", "und", "Kraft", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sind die St\u00e4dte, handelspr\u00e4chtig,", "tokens": ["Wo", "sind", "die", "St\u00e4d\u00b7te", ",", "han\u00b7del\u00b7spr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gelegen an der Ostsee Strand,", "tokens": ["Ge\u00b7le\u00b7gen", "an", "der", "Ost\u00b7see", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es schrie der Nordwind, todesm\u00e4chtig,", "tokens": ["Es", "schrie", "der", "Nord\u00b7wind", ",", "to\u00b7des\u00b7m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dr\u00fcber fiel der gelbe Sand.", "tokens": ["Und", "dr\u00fc\u00b7ber", "fiel", "der", "gel\u00b7be", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es stand noch gestern, wo ich liege,", "tokens": ["Es", "stand", "noch", "ge\u00b7stern", ",", "wo", "ich", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der M\u00f6we Nest, ein kleines Gl\u00fcck,", "tokens": ["Der", "M\u00f6\u00b7we", "Nest", ",", "ein", "klei\u00b7nes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sucht die heuumkr\u00e4nzte Wiege", "tokens": ["Es", "sucht", "die", "heu\u00b7um\u00b7kr\u00e4nz\u00b7te", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergebens heut' mein scharfer Blick;", "tokens": ["Ver\u00b7ge\u00b7bens", "heut'", "mein", "schar\u00b7fer", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach ihrem Neste schreit die M\u00f6we", "tokens": ["Nach", "ih\u00b7rem", "Nes\u00b7te", "schreit", "die", "M\u00f6\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Strand zu Land, von Land zu Strand,", "tokens": ["Von", "Strand", "zu", "Land", ",", "von", "Land", "zu", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es reckte sich der gelbe L\u00f6we", "tokens": ["Es", "reck\u00b7te", "sich", "der", "gel\u00b7be", "L\u00f6\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und dr\u00fcber fiel der gelbe Sand.", "tokens": ["Und", "dr\u00fc\u00b7ber", "fiel", "der", "gel\u00b7be", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Altpreu\u00dfens Helden, die vor Tagen", "tokens": ["Alt\u00b7preu\u00b7\u00dfens", "Hel\u00b7den", ",", "die", "vor", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Einst friedlich dieses Land bebaut,", "tokens": ["Einst", "fried\u00b7lich", "die\u00b7ses", "Land", "be\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ordensritter, die erschlagen", "tokens": ["Die", "Or\u00b7dens\u00b7rit\u00b7ter", ",", "die", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Friedvolk unter Psalmenlaut,", "tokens": ["Das", "Fried\u00b7volk", "un\u00b7ter", "Psal\u00b7men\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Pommern, Polen und nach Jahren", "tokens": ["Die", "Pom\u00b7mern", ",", "Po\u00b7len", "und", "nach", "Jah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NE", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Napoleon, als sein Grab er fand,", "tokens": ["Na\u00b7po\u00b7le\u00b7on", ",", "als", "sein", "Grab", "er", "fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Wohin sind alle sie gefahren?", "tokens": ["Wo\u00b7hin", "sind", "al\u00b7le", "sie", "ge\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Stillschweigen. Dar\u00fcber liegt der Sand.", "tokens": ["Still\u00b7schwei\u00b7gen", ".", "Da\u00b7r\u00fc\u00b7ber", "liegt", "der", "Sand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Auch ich, noch jetzt so lebensmunter,", "tokens": ["Auch", "ich", ",", "noch", "jetzt", "so", "le\u00b7bens\u00b7mun\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Plan zu k\u00fchn, kein Wunsch zu schwer,", "tokens": ["Kein", "Plan", "zu", "k\u00fchn", ",", "kein", "Wunsch", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Westen steigt der Tod herunter,", "tokens": ["Von", "Wes\u00b7ten", "steigt", "der", "Tod", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ruck, ein Sto\u00df, ich bin nicht mehr;", "tokens": ["Ein", "Ruck", ",", "ein", "Sto\u00df", ",", "ich", "bin", "nicht", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und all' mein Jauchzen, all' mein Klagen,", "tokens": ["Und", "all'", "mein", "Jauch\u00b7zen", ",", "all'", "mein", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Traum, schon morgen unbekannt,", "tokens": ["Ein", "Traum", ",", "schon", "mor\u00b7gen", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Schaffen, Dichten, Tun und Sagen,", "tokens": ["Mein", "Schaf\u00b7fen", ",", "Dich\u00b7ten", ",", "Tun", "und", "Sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es rollt dar\u00fcber gelber Sand.", "tokens": ["Es", "rollt", "da\u00b7r\u00fc\u00b7ber", "gel\u00b7ber", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du lange, gelblichgraue D\u00fcne,", "tokens": ["Du", "lan\u00b7ge", ",", "gelb\u00b7lich\u00b7grau\u00b7e", "D\u00fc\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du weites, graulichblaues Meer,", "tokens": ["Du", "wei\u0