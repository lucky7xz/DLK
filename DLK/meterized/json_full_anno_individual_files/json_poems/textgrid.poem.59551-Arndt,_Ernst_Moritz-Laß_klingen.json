{"textgrid.poem.59551": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "La\u00df klingen", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe ein Liedel, juchheisa! juchhei!", "tokens": ["Ich", "sin\u00b7ge", "ein", "Lie\u00b7del", ",", "juch\u00b7hei\u00b7sa", "!", "juch\u00b7hei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ITJ", "$.", "FM", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es s\u00e4useln die L\u00fcfte, es locket der Mai,", "tokens": ["Es", "s\u00e4u\u00b7seln", "die", "L\u00fcf\u00b7te", ",", "es", "lo\u00b7cket", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Quellen, sie rieseln mit lustigem Klang,", "tokens": ["Die", "Quel\u00b7len", ",", "sie", "rie\u00b7seln", "mit", "lus\u00b7ti\u00b7gem", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die B\u00e4che, sie spielen und fl\u00f6ten Gesang.", "tokens": ["Die", "B\u00e4\u00b7che", ",", "sie", "spie\u00b7len", "und", "fl\u00f6\u00b7ten", "Ge\u00b7sang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "O liebliche Rosen, o Lilien wei\u00df!", "tok