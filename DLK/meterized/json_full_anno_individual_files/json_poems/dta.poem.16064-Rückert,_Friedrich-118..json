{"dta.poem.16064": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "118.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zunft und Vernunft, mein Sohn, sind leider zweierlei,", "tokens": ["Zunft", "und", "Ver\u00b7nunft", ",", "mein", "Sohn", ",", "sind", "lei\u00b7der", "zwei\u00b7er\u00b7lei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "PIS", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch unsre Aufgab' ist, zu einigen die Zwei.", "tokens": ["Doch", "uns\u00b7re", "Auf\u00b7gab'", "ist", ",", "zu", "ei\u00b7ni\u00b7gen", "die", "Zwei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "APPR", "PIAT", "ART", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Sohn, in keiner Zunft ist die Vernunft zwar z\u00fcnftig,", "tokens": ["Mein", "Sohn", ",", "in", "kei\u00b7ner", "Zunft", "ist", "die", "Ver\u00b7nunft", "zwar", "z\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch seyn soll die Vernunft in jeder Zunft vern\u00fcnftig.", "tokens": ["Doch", "seyn", "soll", "die", "Ver\u00b7nunft", "in", "je\u00b7der", "Zunft", "ver\u00b7n\u00fcnf\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON