{"textgrid.poem.51259": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Weinjahr", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fcstet die Kelter, die Kufen und Tonnen,", "tokens": ["R\u00fcs\u00b7tet", "die", "Kel\u00b7ter", ",", "die", "Ku\u00b7fen", "und", "Ton\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Denn es vergl\u00fchet ein seltenes Jahr!", "tokens": ["Denn", "es", "ver\u00b7gl\u00fc\u00b7het", "ein", "sel\u00b7te\u00b7nes", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schon naht der Herbst, und es glastet die Sonne,", "tokens": ["Schon", "naht", "der", "Herbst", ",", "und", "es", "glas\u00b7tet", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie sie geglastet den Sommer entlang!", "tokens": ["Wie", "sie", "ge\u00b7glas\u00b7tet", "den", "Som\u00b7mer", "ent\u00b7lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "H\u00f6rt, im Gebirge, was Zeichen geschehen!", "tokens": ["H\u00f6rt", ",", "im", "Ge\u00b7bir\u00b7ge", ",", "was", "Zei\u00b7chen", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "$,", "PWS", "NN", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gletscher, sie ebben wie Meere zur\u00fcck,", "tokens": ["Glet\u00b7scher", ",", "sie", "eb\u00b7ben", "wie", "Mee\u00b7re", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ihre blaugr\u00fcnen Gew\u00f6lbe zerschmelzen,", "tokens": ["Ih\u00b7re", "blau\u00b7gr\u00fc\u00b7nen", "Ge\u00b7w\u00f6l\u00b7be", "zer\u00b7schmel\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Grotten und Spalten so tief und so k\u00fchl!", "tokens": ["Grot\u00b7ten", "und", "Spal\u00b7ten", "so", "tief", "und", "so", "k\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Trocken enth\u00fcllen sich felsige Gr\u00fcnde,", "tokens": ["Tro\u00b7cken", "ent\u00b7h\u00fcl\u00b7len", "sich", "fel\u00b7si\u00b7ge", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die seit Jahrtausenden keiner geschaut,", "tokens": ["Die", "seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "kei\u00b7ner", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und aus der tiefsten und engsten der Kl\u00fcfte", "tokens": ["Und", "aus", "der", "tiefs\u00b7ten", "und", "engs\u00b7ten", "der", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "KON", "VVFIN", "ART", "NN"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Leuchten gebleichte Gebeine herauf.", "tokens": ["Leuch\u00b7ten", "ge\u00b7bleich\u00b7te", "Ge\u00b7bei\u00b7ne", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Knochen des riesigen Vorweltsb\u00e4ren", "tokens": ["Kno\u00b7chen", "des", "rie\u00b7si\u00b7gen", "Vor\u00b7welts\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Liegen gebrochen wie spr\u00f6des Glas,", "tokens": ["Lie\u00b7gen", "ge\u00b7bro\u00b7chen", "wie", "spr\u00f6\u00b7des", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aber dazwischen die Rippen und R\u00f6hren", "tokens": ["A\u00b7ber", "da\u00b7zwi\u00b7schen", "die", "Rip\u00b7pen", "und", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Eines in Waffen verschollenen Manns.", "tokens": ["Ei\u00b7nes", "in", "Waf\u00b7fen", "ver\u00b7schol\u00b7le\u00b7nen", "Manns", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und die verrostete Panzerschale,", "tokens": ["Und", "die", "ver\u00b7ros\u00b7te\u00b7te", "Pan\u00b7zer\u00b7scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auch ein zerfressenes spanisches Schwert", "tokens": ["Auch", "ein", "zer\u00b7fres\u00b7se\u00b7nes", "spa\u00b7ni\u00b7sches", "Schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "K\u00fcnden den Krieger aus traurigen Tagen", "tokens": ["K\u00fcn\u00b7den", "den", "Krie\u00b7ger", "aus", "trau\u00b7ri\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Einer in Leiden zerkl\u00fcfteten Welt.", "tokens": ["Ei\u00b7ner", "in", "Lei\u00b7den", "zer\u00b7kl\u00fcf\u00b7te\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch mit den s\u00e4mtlichen Z\u00e4hnen gezieret", "tokens": ["Noch", "mit", "den", "s\u00e4mt\u00b7li\u00b7chen", "Z\u00e4h\u00b7nen", "ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Starren die Kiefer im r\u00e4umigen Helm,", "tokens": ["Star\u00b7ren", "die", "Kie\u00b7fer", "im", "r\u00e4u\u00b7mi\u00b7gen", "Helm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Gleich einem Spielzeug neben des wilden", "tokens": ["Gleich", "ei\u00b7nem", "Spiel\u00b7zeug", "ne\u00b7ben", "des", "wil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "B\u00e4ren gewaltigem Kopfgestell.", "tokens": ["B\u00e4\u00b7ren", "ge\u00b7wal\u00b7ti\u00b7gem", "Kopf\u00b7ge\u00b7stell", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Sehet! unb\u00e4ndig schwellen die Trauben", "tokens": ["Se\u00b7het", "!", "un\u00b7b\u00e4n\u00b7dig", "schwel\u00b7len", "die", "Trau\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "\u2013 R\u00fcstet die Kelter und r\u00fcstet den Krug! \u2013", "tokens": ["\u2013", "R\u00fcs\u00b7tet", "die", "Kel\u00b7ter", "und", "r\u00fcs\u00b7tet", "den", "Krug", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Jegliche Beer eine sonnige Klause,", "tokens": ["Jeg\u00b7li\u00b7che", "Beer", "ei\u00b7ne", "son\u00b7ni\u00b7ge", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Drinnen ein Glutelf brauet die Flut!", "tokens": ["Drin\u00b7nen", "ein", "Glu\u00b7telf", "brau\u00b7et", "die", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Zwei friedlose Gesellen, schlafen", "tokens": ["Zwei", "fried\u00b7lo\u00b7se", "Ge\u00b7sel\u00b7len", ",", "schla\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["CARD", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jene, in ewigen Frieden entr\u00fcckt;", "tokens": ["Je\u00b7ne", ",", "in", "e\u00b7wi\u00b7gen", "Frie\u00b7den", "ent\u00b7r\u00fcckt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber die Wut und das W\u00e4hnen und Wagen", "tokens": ["A\u00b7ber", "die", "Wut", "und", "das", "W\u00e4h\u00b7nen", "und", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "H\u00e4lt noch die duldenden L\u00fcfte erf\u00fcllt.", "tokens": ["H\u00e4lt", "noch", "die", "dul\u00b7den\u00b7den", "L\u00fcf\u00b7te", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "R\u00fcstet die Tonnen! Umfanget den starken", "tokens": ["R\u00fcs\u00b7tet", "die", "Ton\u00b7nen", "!", "Um\u00b7fan\u00b7get", "den", "star\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Reisigen Wein mit eisernem Band!", "tokens": ["Rei\u00b7si\u00b7gen", "Wein", "mit", "ei\u00b7ser\u00b7nem", "Band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "M\u00e4nnern zerbricht er den st\u00e4mmigsten Nacken,", "tokens": ["M\u00e4n\u00b7nern", "zer\u00b7bricht", "er", "den", "st\u00e4m\u00b7migs\u00b7ten", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "St\u00fcrzet sie j\u00e4hlings in Jammer und Qual!", "tokens": ["St\u00fcr\u00b7zet", "sie", "j\u00e4h\u00b7lings", "in", "Jam\u00b7mer", "und", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.10": {"line.1": {"text": "F\u00fcllet die Kr\u00fcge, doch trinket den Frieden,", "tokens": ["F\u00fcl\u00b7let", "die", "Kr\u00fc\u00b7ge", ",", "doch", "trin\u00b7ket", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Trinket das Licht, das dem Himmel entstrahlt!", "tokens": ["Trin\u00b7ket", "das", "Licht", ",", "das", "dem", "Him\u00b7mel", "ent\u00b7strahlt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Bindet die Herzen mit eisernem Willen,", "tokens": ["Bin\u00b7det", "die", "Her\u00b7zen", "mit", "ei\u00b7ser\u00b7nem", "Wil\u00b7len", ","], "to