{"dta.poem.4071": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wanderer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Einsam stand ich und sah in die afrikanischen d\u00fcrren", "tokens": ["Ein\u00b7sam", "stand", "ich", "und", "sah", "in", "die", "af\u00b7ri\u00b7ka\u00b7ni\u00b7schen", "d\u00fcr\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ebnen hinaus; vom Olymp regnete Feuer herab.", "tokens": ["Eb\u00b7nen", "hin\u00b7aus", ";", "vom", "O\u00b7lymp", "reg\u00b7ne\u00b7te", "Feu\u00b7er", "her\u00b7ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "APPRART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Fernhin schlich das hagre Gebirg, wie ein wandelnd", "tokens": ["Fern\u00b7hin", "schlich", "das", "hag\u00b7re", "Ge\u00b7birg", ",", "wie", "ein", "wan\u00b7delnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "VVPP"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gerippe,", "tokens": ["Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Hohl und einsam und kahl blickt' aus der H\u00f6he", "tokens": ["Hohl", "und", "ein\u00b7sam", "und", "kahl", "blickt'", "aus", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "sein Haupt.", "tokens": ["sein", "Haupt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Ach! nicht sprang, mit erfrischendem Gr\u00fcn, der", "tokens": ["Ach", "!", "nicht", "sprang", ",", "mit", "er\u00b7fri\u00b7schen\u00b7dem", "Gr\u00fcn", ",", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "PTKNEG", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "PRELS"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "schattende Wald hier", "tokens": ["schat\u00b7ten\u00b7de", "Wald", "hier"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "In die s\u00e4uselnde Luft \u00fcppig und herrlich empor,", "tokens": ["In", "die", "s\u00e4u\u00b7seln\u00b7de", "Luft", "\u00fcp\u00b7pig", "und", "herr\u00b7lich", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "B\u00e4che st\u00fcrzten hier nicht in melodischem Fall vom", "tokens": ["B\u00e4\u00b7che", "st\u00fcrz\u00b7ten", "hier", "nicht", "in", "me\u00b7lo\u00b7di\u00b7schem", "Fall", "vom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "APPRART"], "meter": "+-+--+---+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Gebirge,", "tokens": ["Ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Durch das bl\u00fchende Thal schlingend den silbernen", "tokens": ["Durch", "das", "bl\u00fc\u00b7hen\u00b7de", "Thal", "schlin\u00b7gend", "den", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "ART", "NN"], "meter": "+-+--+---+--", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Strom,", "tokens": ["Strom", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Keiner Heerde verging am pl\u00e4tschernden Brunnen", "tokens": ["Kei\u00b7ner", "Heer\u00b7de", "ver\u00b7ging", "am", "pl\u00e4t\u00b7schern\u00b7den", "Brun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "der Mittag,", "tokens": ["der", "Mit\u00b7tag", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Freundlich aus B\u00e4umen hervor blickte kein wirth-", "tokens": ["Freund\u00b7lich", "aus", "B\u00e4u\u00b7men", "her\u00b7vor", "blick\u00b7te", "kein", "wir\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "VVFIN", "PIAT", "TRUNC"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "liches Dach.", "tokens": ["li\u00b7ches", "Dach", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Unter dem Strauche sa\u00df ein ernster Vogel ge-", "tokens": ["Un\u00b7ter", "dem", "Strau\u00b7che", "sa\u00df", "ein", "erns\u00b7ter", "Vo\u00b7gel", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NE", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.19": {"text": "sanglos,", "tokens": ["sang\u00b7los", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Aengstig und eilend flohn wandernde St\u00f6rche", "tokens": ["A\u00b7engs\u00b7tig", "und", "ei\u00b7lend", "flohn", "wan\u00b7dern\u00b7de", "St\u00f6r\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.21": {"text": "vorbei.", "tokens": ["vor\u00b7bei", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Nicht um Wasser rief ich dich an, Natur, in der", "tokens": ["Nicht", "um", "Was\u00b7ser", "rief", "ich", "dich", "an", ",", "Na\u00b7tur", ",", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.23": {"text": "W\u00fcste,", "tokens": ["W\u00fcs\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Wassers bewahrte mir traulich das fromme Kamel,", "tokens": ["Was\u00b7sers", "be\u00b7wahr\u00b7te", "mir", "trau\u00b7lich", "das", "from\u00b7me", "Ka\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.25": {"text": "Um der Haine Gesang, um Gestalten und Farben", "tokens": ["Um", "der", "Hai\u00b7ne", "Ge\u00b7sang", ",", "um", "Ge\u00b7stal\u00b7ten", "und", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "KOUI", "NN", "KON", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "des Lebens", "tokens": ["des", "Le\u00b7bens"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Bat ich, vom lieblichen Glanz heimischer Fluren", "tokens": ["Bat", "ich", ",", "vom", "lieb\u00b7li\u00b7chen", "Glanz", "hei\u00b7mi\u00b7scher", "Flu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "verw\u00f6hnt.", "tokens": ["ver\u00b7w\u00f6hnt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Aber ich bat umsonst; du erschienst mir feurig und", "tokens": ["A\u00b7ber", "ich", "bat", "um\u00b7sonst", ";", "du", "er\u00b7schienst", "mir", "feu\u00b7rig", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADJD", "KON"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.30": {"text": "herrlich,", "tokens": ["herr\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Aber ich hatte dich einst g\u00f6ttlicher, sch\u00f6ner gesehn.", "tokens": ["A\u00b7ber", "ich", "hat\u00b7te", "dich", "einst", "g\u00f6tt\u00b7li\u00b7cher", ",", "sch\u00f6\u00b7ner", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "ADJA", "$,", "ADJD", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Auch den Eispol hab' ich besucht; wie ein starren-", "tokens": ["Auch", "den", "Eis\u00b7pol", "hab'", "ich", "be\u00b7sucht", ";", "wie", "ein", "star\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "ART", "TRUNC"], "meter": "+-+-+--++-+-", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "des Chaos", "tokens": ["des", "Chaos"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.34": {"text": "Th\u00fcrmte das Meer sich da schrecklich zum Him-", "tokens": ["Th\u00fcrm\u00b7te", "das", "Meer", "sich", "da", "schreck\u00b7lich", "zum", "Him"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "ADJD", "APPRART", "TRUNC"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.35": {"text": "mel empor.", "tokens": ["mel", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.36": {"text": "Todt in der H\u00fclle von Schnee schlief hier das", "tokens": ["Todt", "in", "der", "H\u00fcl\u00b7le", "von", "Schnee", "schlief", "hier", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "ADV", "ART"], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}, "line.37": {"text": "gefesselte Leben,", "tokens": ["ge\u00b7fes\u00b7sel\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Und der eiserne Schlaf harrte des Tages umsonst.", "tokens": ["Und", "der", "ei\u00b7ser\u00b7ne", "Schlaf", "harr\u00b7te", "des", "Ta\u00b7ges", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Ach! nicht schlang um die Erde den w\u00e4rmenden", "tokens": ["Ach", "!", "nicht", "schlang", "um", "die", "Er\u00b7de", "den", "w\u00e4r\u00b7men\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.40": {"text": "Arm der Olymp hier,", "tokens": ["Arm", "der", "O\u00b7lymp", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Wie Pygmalions Arm um die Geliebte sich schlang.", "tokens": ["Wie", "Pyg\u00b7ma\u00b7li\u00b7ons", "Arm", "um", "die", "Ge\u00b7lieb\u00b7te", "sich", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.42": {"text": "Hier bewegt' er ihr nicht mit dem Sonnenblicke", "tokens": ["Hier", "be\u00b7wegt'", "er", "ihr", "nicht", "mit", "dem", "Son\u00b7nen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.43": {"text": "den Busen,", "tokens": ["den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "Und in Regen und Thau sprach er nicht freundlich", "tokens": ["Und", "in", "Re\u00b7gen", "und", "Thau", "sprach", "er", "nicht", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PTKNEG", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.45": {"text": "zu ihr.", "tokens": ["zu", "ihr", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$."], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "Mutter Erde! rief ich, du bist zur Wittwe ge-", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "!", "rief", "ich", ",", "du", "bist", "zur", "Witt\u00b7we", "ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "APPRART", "NN", "TRUNC"], "meter": "+-+-+--+-++-", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "worden,", "tokens": ["wor\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VAPP", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "D\u00fcrftig und kinderlos lebst du in langsamer Zeit.", "tokens": ["D\u00fcrf\u00b7tig", "und", "kin\u00b7der\u00b7los", "lebst", "du", "in", "lang\u00b7sa\u00b7mer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.49": {"text": "Nichts zu erzeugen und nichts zu pflegen in sor-", "tokens": ["Nichts", "zu", "er\u00b7zeu\u00b7gen", "und", "nichts", "zu", "pfle\u00b7gen", "in", "sor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PTKZU", "VVINF", "KON", "PIS", "PTKZU", "VVINF", "APPR", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.50": {"text": "gender Liebe,", "tokens": ["gen\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.51": {"text": "Alternd im Kinde sich nicht wiederzusehn, ist", "tokens": ["Al\u00b7ternd", "im", "Kin\u00b7de", "sich", "nicht", "wie\u00b7der\u00b7zu\u00b7sehn", ",", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPRART", "NN", "PRF", "PTKNEG", "VVINF", "$,", "VAFIN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.52": {"text": "der Tod.", "tokens": ["der", "Tod", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.53": {"text": "Aber vielleicht erwarmst du dereinst am Strale des", "tokens": ["A\u00b7ber", "viel\u00b7leicht", "er\u00b7warmst", "du", "de\u00b7reinst", "am", "Stra\u00b7le", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.54": {"text": "Himmels,", "tokens": ["Him\u00b7mels", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.55": {"text": "Aus dem d\u00fcrftigen Schlaf schmeichelt sein Odem", "tokens": ["Aus", "dem", "d\u00fcrf\u00b7ti\u00b7gen", "Schlaf", "schmei\u00b7chelt", "sein", "O\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.56": {"text": "dich auf;", "tokens": ["dich", "auf", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.57": {"text": "Und, wie ein Samenkorn, durchbrichst du die", "tokens": ["Und", ",", "wie", "ein", "Sa\u00b7men\u00b7korn", ",", "durch\u00b7brichst",