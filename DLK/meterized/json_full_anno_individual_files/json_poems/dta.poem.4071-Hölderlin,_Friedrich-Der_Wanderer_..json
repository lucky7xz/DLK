r Kindheit,", "tokens": ["der", "Kind\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.151": {"text": "Und mit sanfter Gewalt h\u00f6her und weiter mich", "tokens": ["Und", "mit", "sanf\u00b7ter", "Ge\u00b7walt", "h\u00f6\u00b7her", "und", "wei\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "KON", "ADV", "PPER"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.152": {"text": "triebst,", "tokens": ["triebst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.153": {"text": "Mildere Sonne! zu dir kehr' ich getreuer und weiser,", "tokens": ["Mil\u00b7de\u00b7re", "Son\u00b7ne", "!", "zu", "dir", "kehr'", "ich", "ge\u00b7treu\u00b7er", "und", "wei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.154": {"text": "Friedlich zu werden, und froh unter den Blumen", "tokens": ["Fried\u00b7lich", "zu", "wer\u00b7den", ",", "und", "froh", "un\u00b7ter", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VAINF", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.155": {"text": "zu ruhn.", "tokens": ["zu", "ruhn", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}