{"dta.poem.6939": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin werd' ich zuletzt mein k\u00fchnes Werck noch leiten?", "tokens": ["Wo\u00b7hin", "werd'", "ich", "zu\u00b7letzt", "mein", "k\u00fch\u00b7nes", "Werck", "noch", "lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch ein begl\u00fcckter Glantz wird meinen Muht begleiten?", "tokens": ["Welch", "ein", "be\u00b7gl\u00fcck\u00b7ter", "Glantz", "wird", "mei\u00b7nen", "Muht", "be\u00b7glei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu zeigen, und den Grund des Wunderwercks zu finden,", "tokens": ["Zu", "zei\u00b7gen", ",", "und", "den", "Grund", "des", "Wun\u00b7der\u00b7wercks", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf welche Weis\u2019 ein Seyn von daurender Natur,", "tokens": ["Auf", "wel\u00b7che", "Weis'", "ein", "Seyn", "von", "dau\u00b7ren\u00b7der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das sonder Leib, ohn Ort und ohn Figur", "tokens": ["Das", "son\u00b7der", "Leib", ",", "ohn", "Ort", "und", "ohn", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUI", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch f\u00e4hig sey, sich zu verbinden", "tokens": ["Doch", "f\u00e4\u00b7hig", "sey", ",", "sich", "zu", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Handlungen von c\u00f6rperlichen Wesen,", "tokens": ["Mit", "Hand\u00b7lun\u00b7gen", "von", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ver\u00e4nderlich und ungewi\u00df;", "tokens": ["Ver\u00b7\u00e4n\u00b7der\u00b7lich", "und", "un\u00b7ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da, sag ich sp\u00fcrt der Witz die dickste Finsterni\u00df.", "tokens": ["Da", ",", "sag", "ich", "sp\u00fcrt", "der", "Witz", "die", "dicks\u00b7te", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}