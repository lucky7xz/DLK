{"dta.poem.20624": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An die Clio  \n wegen des K\u00f6niges", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die einst zu jedem honigs\u00fc\u00dfen Liede                 ", "tokens": ["Die", "einst", "zu", "je\u00b7dem", "ho\u00b7nig\u00b7s\u00fc\u00b7\u00dfen", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euripides um Beystand rief,", "tokens": ["Eu\u00b7ri\u00b7pi\u00b7des", "um", "Beys\u00b7tand", "rief", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dich, Muse, f\u00fchl ich ganz, jetzt, da mein Tyndaride", "tokens": ["Dich", ",", "Mu\u00b7se", ",", "f\u00fchl", "ich", "ganz", ",", "jetzt", ",", "da", "mein", "Tyn\u00b7da\u00b7ri\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drey hundert N\u00e4chte ruhig schlief.", "tokens": ["Drey", "hun\u00b7dert", "N\u00e4ch\u00b7te", "ru\u00b7hig", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt, da kein kalydonisch Ungeheuer", "tokens": ["Jetzt", ",", "da", "kein", "ka\u00b7ly\u00b7do\u00b7nisch", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Kampfe f\u00fcr Ihn \u00fcbrig ist,", "tokens": ["Zum", "Kamp\u00b7fe", "f\u00fcr", "Ihn", "\u00fcb\u00b7rig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kein verdeckter Ha\u00df; und nicht des Neides Geyer", "tokens": ["Und", "kein", "ver\u00b7deck\u00b7ter", "Ha\u00df", ";", "und", "nicht", "des", "Nei\u00b7des", "Ge\u00b7yer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$.", "KON", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "An Seines Nachbars Leber fri\u00dft;", "tokens": ["An", "Sei\u00b7nes", "Nach\u00b7bars", "Le\u00b7ber", "fri\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und nur noch Alexander gro\u00dfe Todten", "tokens": ["Und", "nur", "noch", "A\u00b7lex\u00b7an\u00b7der", "gro\u00b7\u00dfe", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unruhig macht durch ein Geschrey:", "tokens": ["Un\u00b7ru\u00b7hig", "macht", "durch", "ein", "Ge\u00b7schrey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u201eda\u00df, von des Orients Geschenkbeladnen Bothen", "tokens": ["\u201e", "da\u00df", ",", "von", "des", "O\u00b7rients", "Ge\u00b7schenk\u00b7be\u00b7lad\u00b7nen", "Bo\u00b7then"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "$,", "APPR", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "\u201egegr\u00fc\u00dft Sein Ueberwinder sey;", "tokens": ["\u201e", "ge\u00b7gr\u00fc\u00dft", "Sein", "Ue\u00b7ber\u00b7win\u00b7der", "sey", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eda\u00df von dem Ei\u00dfmeer bis zu Herculs S\u00e4ulen,", "tokens": ["\u201e", "da\u00df", "von", "dem", "Ei\u00df\u00b7meer", "bis", "zu", "Her\u00b7culs", "S\u00e4u\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "APPR", "ART", "NN", "APPR", "APPR", "NE", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u201evom Belt bis zu dem Hellespont,", "tokens": ["\u201e", "vom", "Belt", "bis", "zu", "dem", "Hel\u00b7les\u00b7pont", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edem K\u00f6nig Ruhm erschallt, dem Zevs von seinen Keilen", "tokens": ["\u201e", "dem", "K\u00f6\u00b7nig", "Ruhm", "er\u00b7schallt", ",", "dem", "Zevs", "von", "sei\u00b7nen", "Kei\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "VVPP", "$,", "PRELS", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie blitzbeschwungenste vergont;", "tokens": ["\u201e", "die", "blitz\u00b7be\u00b7schwun\u00b7gens\u00b7te", "ver\u00b7gont", ";"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eden unter seinen Lorbeerkr\u00e4nzen neiden", "tokens": ["\u201e", "den", "un\u00b7ter", "sei\u00b7nen", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", "nei\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201eperikles w\u00fcrde, der Athen", "tokens": ["\u201e", "pe\u00b7rik\u00b7les", "w\u00fcr\u00b7de", ",", "der", "A\u00b7then"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PIS", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eneunmahl verfochten, und in pr\u00e4chtigen Geb\u00e4uden", "tokens": ["\u201e", "neun\u00b7mahl", "ver\u00b7foch\u00b7ten", ",", "und", "in", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7b\u00e4u\u00b7den"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "\u201ehie\u00df seine goldne G\u00f6tter stehn;", "tokens": ["\u201e", "hie\u00df", "sei\u00b7ne", "gold\u00b7ne", "G\u00f6t\u00b7ter", "stehn", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eund in der Hand ein Horn des Ueberflusses", "tokens": ["\u201e", "und", "in", "der", "Hand", "ein", "Horn", "des", "Ue\u00b7ber\u00b7flus\u00b7ses"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201egesch\u00fcttelt auf das Volk herab,", "tokens": ["\u201e", "ge\u00b7sch\u00fct\u00b7telt", "auf", "das", "Volk", "her\u00b7ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eals selbst Minerva die Befehle seines Schlusses", "tokens": ["\u201e", "als", "selbst", "Mi\u00b7ner\u00b7va", "die", "Be\u00b7feh\u00b7le", "sei\u00b7nes", "Schlus\u00b7ses"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADV", "NE", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "\u201ezum Aufbau des Odeons gab,", "tokens": ["\u201e", "zum", "Auf\u00b7bau", "des", "O\u00b7deons", "gab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eund einer Burg, die nach drey tausend Tagen", "tokens": ["\u201e", "und", "ei\u00b7ner", "Burg", ",", "die", "nach", "drey", "tau\u00b7send", "Ta\u00b7gen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "NN", "$,", "PRELS", "APPR", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201enoch dem Pallast nicht \u00e4hnlich schien,", "tokens": ["\u201e", "noch", "dem", "Pal\u00b7last", "nicht", "\u00e4hn\u00b7lich", "schien", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u201ezu welchem ", "tokens": ["\u201e", "zu", "wel\u00b7chem"], "token_info": ["punct", "word", "word"], "pos": ["$(", "APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "\u201evon Starken, die zu Felde ziehn,", "tokens": ["\u201e", "von", "Star\u00b7ken", ",", "die", "zu", "Fel\u00b7de", "ziehn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201ewenn gegen stolzerhobne Feindes Stirne", "tokens": ["\u201e", "wenn", "ge\u00b7gen", "stol\u00b7zer\u00b7hob\u00b7ne", "Fein\u00b7des", "Stir\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201eder Held die Waffen nehmen hei\u00dft,", "tokens": ["\u201e", "der", "Held", "die", "Waf\u00b7fen", "neh\u00b7men", "hei\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eden jeder liebt, und den sein gl\u00fcckliches Gestirne", "tokens": ["\u201e", "den", "je\u00b7der", "liebt", ",", "und", "den", "sein", "gl\u00fcck\u00b7li\u00b7ches", "Ge\u00b7stir\u00b7ne"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "PIS", "VVFIN", "$,", "KON", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ehervorgebracht mit diesem Geist,", "tokens": ["\u201e", "her\u00b7vor\u00b7ge\u00b7bracht", "mit", "die\u00b7sem", "Geist", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201eder unerschrocken bliebe, wenn Typhonen", "tokens": ["\u201e", "der", "un\u00b7er\u00b7schro\u00b7cken", "blie\u00b7be", ",", "wenn", "Ty\u00b7pho\u00b7nen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201ebest\u00fcrmen wollten seinen Sitz,", "tokens": ["\u201e", "be\u00b7st\u00fcr\u00b7men", "woll\u00b7ten", "sei\u00b7nen", "Sitz", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eso fest wie der Olymp, auf dem die G\u00f6tter wohnen,", "tokens": ["\u201e", "so", "fest", "wie", "der", "O\u00b7lymp", ",", "auf", "dem", "die", "G\u00f6t\u00b7ter", "woh\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "\u201ebesch\u00fctzt genug durch ihren Blitz.\u201e", "tokens": ["\u201e", "be\u00b7sch\u00fctzt", "ge\u00b7nug", "durch", "ih\u00b7ren", "Blitz", ".", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}