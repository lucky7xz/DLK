{"dta.poem.5914": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Materie nun dergestalt erreget,", "tokens": ["Wenn", "die", "Ma\u00b7te\u00b7rie", "nun", "der\u00b7ge\u00b7stalt", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und so getheilet ist; Wo der, so sie beweget,", "tokens": ["Und", "so", "ge\u00b7thei\u00b7let", "ist", ";", "Wo", "der", ",", "so", "sie", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$.", "PWAV", "ART", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dieselbe Ordnung h\u00e4lt; entstehen allgemach,", "tokens": ["Die\u00b7sel\u00b7be", "Ord\u00b7nung", "h\u00e4lt", ";", "ent\u00b7ste\u00b7hen", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Formen Staffelweis: und die besondern Dinge", "tokens": ["Die", "For\u00b7men", "Staf\u00b7fel\u00b7weis", ":", "und", "die", "be\u00b7son\u00b7dern", "Din\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verf\u00fcgen nach und nach", "tokens": ["Ver\u00b7f\u00fc\u00b7gen", "nach", "und", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sich all an ihren Ort.", "tokens": ["Sich", "all", "an", "ih\u00b7ren", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Zeigt uns allhier so fort", "tokens": ["Zeigt", "uns", "all\u00b7hier", "so", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Den Grund der Welt und ihren Ur-Stoff an.", "tokens": ["Den", "Grund", "der", "Welt", "und", "ih\u00b7ren", "Ur\u00b7Stoff", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die kleine zarte C\u00f6rperlein,", "tokens": ["Die", "klei\u00b7ne", "zar\u00b7te", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die aus den abgebrochnen Ecken", "tokens": ["Die", "aus", "den", "ab\u00b7ge\u00b7broch\u00b7nen", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Entstehen und erzeuget seyn,", "tokens": ["Ent\u00b7ste\u00b7hen", "und", "er\u00b7zeu\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}