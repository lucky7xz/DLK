{"textgrid.poem.34910": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mohrenk\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Exil der Alpuxarren", "tokens": ["Ins", "E\u00b7xil", "der", "Al\u00b7pu\u00b7xar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zog der junge Mohrenk\u00f6nig;", "tokens": ["Zog", "der", "jun\u00b7ge", "Moh\u00b7ren\u00b7k\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweigsam und das Herz voll Kummer", "tokens": ["Schweig\u00b7sam", "und", "das", "Herz", "voll", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritt er an des Zuges Spitze.", "tokens": ["Ritt", "er", "an", "des", "Zu\u00b7ges", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter ihm auf hohen Zeltern", "tokens": ["Hin\u00b7ter", "ihm", "auf", "ho\u00b7hen", "Zel\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder auch in g\u00fcldnen S\u00e4nften", "tokens": ["O\u00b7der", "auch", "in", "g\u00fcld\u00b7nen", "S\u00e4nf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sa\u00dfen seines Hauses Frauen;", "tokens": ["Sa\u00b7\u00dfen", "sei\u00b7nes", "Hau\u00b7ses", "Frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwarze M\u00e4gde tr\u00e4gt das Maultier.", "tokens": ["Schwar\u00b7ze", "M\u00e4g\u00b7de", "tr\u00e4gt", "das", "Maul\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hundert treue Diener folgen", "tokens": ["Hun\u00b7dert", "treu\u00b7e", "Die\u00b7ner", "fol\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf arabisch edlen Rappen;", "tokens": ["Auf", "a\u00b7ra\u00b7bisch", "ed\u00b7len", "Rap\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Stolze G\u00e4ule, doch die Reiter", "tokens": ["Stol\u00b7ze", "G\u00e4u\u00b7le", ",", "doch", "die", "Rei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ngen schlottrig in den S\u00e4tteln.", "tokens": ["H\u00e4n\u00b7gen", "schlott\u00b7rig", "in", "den", "S\u00e4t\u00b7teln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Zimbel, keine Pauke,", "tokens": ["Kei\u00b7ne", "Zim\u00b7bel", ",", "kei\u00b7ne", "Pau\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein Gesangeslaut ert\u00f6nte;", "tokens": ["Kein", "Ge\u00b7san\u00b7ges\u00b7laut", "er\u00b7t\u00f6n\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur des Maultiers Silbergl\u00f6ckchen", "tokens": ["Nur", "des", "Maul\u00b7tiers", "Sil\u00b7ber\u00b7gl\u00f6ck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wimmern schmerzlich in der Stille.", "tokens": ["Wim\u00b7mern", "schmerz\u00b7lich", "in", "der", "Stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf der H\u00f6he, wo der Blick", "tokens": ["Auf", "der", "H\u00f6\u00b7he", ",", "wo", "der", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ins Duero-Tal hinabschweift,", "tokens": ["Ins", "Du\u00b7er\u00b7o\u00b7Tal", "hin\u00b7ab\u00b7schweift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Zinnen von Granada", "tokens": ["Und", "die", "Zin\u00b7nen", "von", "Gra\u00b7na\u00b7da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sichtbar sind zum letzten Male:", "tokens": ["Sicht\u00b7bar", "sind", "zum", "letz\u00b7ten", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dorten stieg vom Pferd der K\u00f6nig", "tokens": ["Dor\u00b7ten", "stieg", "vom", "Pferd", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und betrachtete die Stadt,", "tokens": ["Und", "be\u00b7trach\u00b7te\u00b7te", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im Abendlichte gl\u00e4nzte,", "tokens": ["Die", "im", "A\u00b7bend\u00b7lich\u00b7te", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie geschm\u00fcckt mit Gold und Purpur.", "tokens": ["Wie", "ge\u00b7schm\u00fcckt", "mit", "Gold", "und", "Pur\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber, Allah! Welch ein Anblick!", "tokens": ["A\u00b7ber", ",", "Al\u00b7lah", "!", "Welch", "ein", "An\u00b7blick", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$.", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Statt des vielgeliebten Halbmonds,", "tokens": ["Statt", "des", "viel\u00b7ge\u00b7lieb\u00b7ten", "Halb\u00b7monds", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Prangen Spaniens Kreuz und Fahnen", "tokens": ["Pran\u00b7gen", "Spa\u00b7ni\u00b7ens", "Kreuz", "und", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Auf den T\u00fcrmen der Alhambra.", "tokens": ["Auf", "den", "T\u00fcr\u00b7men", "der", "Al\u00b7hamb\u00b7ra", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach, bei diesem Anblick brachen", "tokens": ["Ach", ",", "bei", "die\u00b7sem", "An\u00b7blick", "bra\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus des K\u00f6nigs Brust die Seufzer,", "tokens": ["Aus", "des", "K\u00f6\u00b7nigs", "Brust", "die", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4nen \u00fcberstr\u00f6mten pl\u00f6tzlich", "tokens": ["Tr\u00e4\u00b7nen", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7ten", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Sturzbach seine Wangen.", "tokens": ["Wie", "ein", "Sturz\u00b7bach", "sei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "D\u00fcster von dem hohen Zelter", "tokens": ["D\u00fcs\u00b7ter", "von", "dem", "ho\u00b7hen", "Zel\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut' herab des K\u00f6nigs Mutter,", "tokens": ["Schaut'", "her\u00b7ab", "des", "K\u00f6\u00b7nigs", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaut' auf ihres Sohnes Jammer,", "tokens": ["Schaut'", "auf", "ih\u00b7res", "Soh\u00b7nes", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie schalt ihn stolz und bitter.", "tokens": ["Und", "sie", "schalt", "ihn", "stolz", "und", "bit\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbboabdil el Chico\u00ab, sprach sie,", "tokens": ["\u00bb", "boab\u00b7dil", "el", "Chi\u00b7co", "\u00ab", ",", "sprach", "sie", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbwie ein Weib beweinst du jetzo", "tokens": ["\u00bb", "wie", "ein", "Weib", "be\u00b7weinst", "du", "jet\u00b7zo"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Jene Stadt, die du nicht wu\u00dftest", "tokens": ["Je\u00b7ne", "Stadt", ",", "die", "du", "nicht", "wu\u00df\u00b7test"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu verteid'gen wie ein Mann.\u00ab", "tokens": ["Zu", "ver\u00b7tei\u00b7d'\u00b7gen", "wie", "ein", "Mann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "Als des K\u00f6nigs liebste Kebsin", "tokens": ["Als", "des", "K\u00f6\u00b7nigs", "liebs\u00b7te", "Keb\u00b7sin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Solche harte Rede h\u00f6rte,", "tokens": ["Sol\u00b7che", "har\u00b7te", "Re\u00b7de", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrzte sie aus ihrer S\u00e4nfte", "tokens": ["St\u00fcrz\u00b7te", "sie", "aus", "ih\u00b7rer", "S\u00e4nf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und umhalste den Gebieter.", "tokens": ["Und", "um\u00b7hals\u00b7te", "den", "Ge\u00b7bie\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "\u00bbboabdil el Chico\u00ab, sprach sie,", "tokens": ["\u00bb", "boab\u00b7dil", "el", "Chi\u00b7co", "\u00ab", ",", "sprach", "sie", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "$(", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbtr\u00f6ste dich, mein Hei\u00dfgeliebter,", "tokens": ["\u00bb", "tr\u00f6s\u00b7te", "dich", ",", "mein", "Hei\u00df\u00b7ge\u00b7lieb\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Abgrund deines Elends", "tokens": ["Aus", "dem", "Ab\u00b7grund", "dei\u00b7nes", "E\u00b7lends"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht hervor ein sch\u00f6ner Lorbeer.", "tokens": ["Bl\u00fcht", "her\u00b7vor", "ein", "sch\u00f6\u00b7ner", "Lor\u00b7beer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nicht allein der Triumphator,", "tokens": ["Nicht", "al\u00b7lein", "der", "Tri\u00b7um\u00b7pha\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Nicht allein der sieggekr\u00f6nte", "tokens": ["Nicht", "al\u00b7lein", "der", "sieg\u00b7ge\u00b7kr\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "G\u00fcnstling jener blinden G\u00f6ttin,", "tokens": ["G\u00fcnst\u00b7ling", "je\u00b7ner", "blin\u00b7den", "G\u00f6t\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch der blut'ge Sohn des Ungl\u00fccks,", "tokens": ["Auch", "der", "blut'\u00b7ge", "Sohn", "des", "Un\u00b7gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Auch der heldenm\u00fct'ge K\u00e4mpfer,", "tokens": ["Auch", "der", "hel\u00b7den\u00b7m\u00fct'\u00b7ge", "K\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dem ungeheuren Schicksal", "tokens": ["Der", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterlag, wird ewig leben", "tokens": ["Un\u00b7ter\u00b7lag", ",", "wird", "e\u00b7wig", "le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Menschen Angedenken.\u00ab", "tokens": ["In", "der", "Men\u00b7schen", "An\u00b7ge\u00b7den\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbberg des letzten Mohrenseufzers\u00ab", "tokens": ["\u00bb", "berg", "des", "letz\u00b7ten", "Moh\u00b7ren\u00b7seuf\u00b7zers", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft bis auf den heut'gen Tag", "tokens": ["Hei\u00dft", "bis", "auf", "den", "heut'\u00b7gen", "Tag"], 