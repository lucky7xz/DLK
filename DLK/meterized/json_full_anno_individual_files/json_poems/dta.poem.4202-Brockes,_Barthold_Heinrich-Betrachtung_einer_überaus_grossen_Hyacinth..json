{"dta.poem.4202": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung  \n einer \u00fcberaus grossen Hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie reich, wie g\u00fctig die Natur, wie auf so manche Art", "tokens": ["Wie", "reich", ",", "wie", "g\u00fc\u00b7tig", "die", "Na\u00b7tur", ",", "wie", "auf", "so", "man\u00b7che", "Art"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "und Weise,", "tokens": ["und", "Wei\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mit ihren holden Fr\u00fchlings-Sch\u00e4tzen,", "tokens": ["Mit", "ih\u00b7ren", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, Welcher sie gemacht, zum Preise,", "tokens": ["Dem", ",", "Wel\u00b7cher", "sie", "ge\u00b7macht", ",", "zum", "Prei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAT", "PPER", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sich bem\u00fcht uns zu ergetzen,", "tokens": ["Sie", "sich", "be\u00b7m\u00fcht", "uns", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bemerkt\u2019 ich j\u00fcngst, fast ausser mir,", "tokens": ["Be\u00b7merkt'", "ich", "j\u00fcngst", ",", "fast", "aus\u00b7ser", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als ich die Balsam- reiche Zier", "tokens": ["Als", "ich", "die", "Bal\u00b7sam", "rei\u00b7che", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "TRUNC", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von einer recht besonders grossen, gef\u00fcllten Hyacinth", "tokens": ["Von", "ei\u00b7ner", "recht", "be\u00b7son\u00b7ders", "gros\u00b7sen", ",", "ge\u00b7f\u00fcll\u00b7ten", "Hya\u00b7cin\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "erblickte,", "tokens": ["er\u00b7blick\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Wovon sich eine jede Glocke mit zwey und vierzig Bl\u00e4ttern", "tokens": ["Wo\u00b7von", "sich", "ei\u00b7ne", "je\u00b7de", "Glo\u00b7cke", "mit", "zwey", "und", "vier\u00b7zig", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "PIAT", "NN", "APPR", "CARD", "KON", "CARD", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "schm\u00fcckte,", "tokens": ["schm\u00fcck\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "An deren \u00e4ussern harten Kelch man eine glatte Festigkeit,", "tokens": ["An", "de\u00b7ren", "\u00e4us\u00b7sern", "har\u00b7ten", "Kelch", "man", "ei\u00b7ne", "glat\u00b7te", "Fes\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "ADJA", "NN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Und fast dieselbige Figur der lieblichen Granaten-Bl\u00fcht,", "tokens": ["Und", "fast", "die\u00b7sel\u00b7bi\u00b7ge", "Fi\u00b7gur", "der", "lieb\u00b7li\u00b7chen", "Gra\u00b7na\u00b7ten\u00b7B\u00b7l\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Doch gl\u00e4nzend-wei\u00df mit gr\u00fcnen Ecken, mit fast erstaunten", "tokens": ["Doch", "gl\u00e4n\u00b7zen\u00b7dwei\u00df", "mit", "gr\u00fc\u00b7nen", "E\u00b7cken", ",", "mit", "fast", "er\u00b7staun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Augen sieht,", "tokens": ["Au\u00b7gen", "sieht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "In deren Silber-reinen Bl\u00e4ttern ein zartes r\u00f6htlich Feuer", "tokens": ["In", "de\u00b7ren", "Sil\u00b7ber\u00b7rei\u00b7nen", "Bl\u00e4t\u00b7tern", "ein", "zar\u00b7tes", "r\u00f6ht\u00b7lich", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "NN", "ART", "ADJA", "ADJD", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.17": {"text": "gl\u00fcht;", "tokens": ["gl\u00fcht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Dem auch die allersch\u00f6nste Haut der Rosen-farbnen Jugend", "tokens": ["Dem", "auch", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Haut", "der", "Ro\u00b7sen\u00b7fa\u00b7rb\u00b7nen", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.19": {"text": "weicht,", "tokens": ["weicht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Und das an Glanz dem reinsten Schmelz, an Farben zarter", "tokens": ["Und", "das", "an", "Glanz", "dem", "reins\u00b7ten", "Schmelz", ",", "an", "Far\u00b7ben", "zar\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN", "ART", "ADJA", "NN", "$,", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Pfirsch-Bl\u00fcht gleicht.", "tokens": ["Pfir\u00b7schB\u00b7l\u00fcht", "gleicht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Ein gr\u00fcner Schmelz bedeckt\u2019 die Stengel. Noch sah ich eine", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Schmelz", "be\u00b7deckt'", "die", "Sten\u00b7gel", ".", "Noch", "sah", "ich", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "ja so sch\u00f6n,", "tokens": ["ja", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Und ja so gro\u00df, so Bl\u00e4tter-reich, nur mit dem Unterscheid", "tokens": ["Und", "ja", "so", "gro\u00df", ",", "so", "Bl\u00e4t\u00b7ter\u00b7reich", ",", "nur", "mit", "dem", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "allein,", "tokens": ["al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Da\u00df, wie der ersten Bl\u00e4tter r\u00f6htlich, die andern blau gef\u00e4rbet", "tokens": ["Da\u00df", ",", "wie", "der", "ers\u00b7ten", "Bl\u00e4t\u00b7ter", "r\u00f6ht\u00b7lich", ",", "die", "an\u00b7dern", "blau", "ge\u00b7f\u00e4r\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "ADJA", "NN", "ADJD", "$,", "PRELS", "PIS", "ADJD", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.27": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "Und recht wie Porcellain gesprenget.", "tokens": ["Und", "recht", "wie", "Por\u00b7cel\u00b7lain", "ge\u00b7spren\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Mich r\u00fchrete die Balsam-Kraft, so sich aus allen beyden", "tokens": ["Mich", "r\u00fch\u00b7re\u00b7te", "die", "Bal\u00b7sam\u00b7Kraft", ",", "so", "sich", "aus", "al\u00b7len", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "drenget.", "tokens": ["dren\u00b7get", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Es d\u00fcnstet\u2019 aus den vollen Kelchen ein recht ambrirter", "tokens": ["Es", "d\u00fcns\u00b7tet'", "aus", "den", "vol\u00b7len", "Kel\u00b7chen", "ein", "recht", "am\u00b7br\u00b7ir\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-++-++-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Balsam-Schwall,", "tokens": ["Bal\u00b7sam\u00b7Schwall", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und f\u00fcllete nicht eine Nase, den ganzen Luft-Kreis \u00fcberall.", "tokens": ["Und", "f\u00fcl\u00b7le\u00b7te", "nicht", "ei\u00b7ne", "Na\u00b7se", ",", "den", "gan\u00b7zen", "Luft\u00b7Kreis", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Hierinn ist ein so s\u00e4urlich Wesen mit einem s\u00fcsseren", "tokens": ["Hie\u00b7rinn", "ist", "ein", "so", "s\u00e4ur\u00b7lich", "We\u00b7sen", "mit", "ei\u00b7nem", "s\u00fcs\u00b7se\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADV", "ADJD", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "vermischet,", "tokens": ["ver\u00b7mi\u00b7schet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Da\u00df es mit Wollust den Geruch, mit Anmuht unser Hirn", "tokens": ["Da\u00df", "es", "mit", "Wol\u00b7lust", "den", "Ge\u00b7ruch", ",", "mit", "An\u00b7muht", "un\u00b7ser", "Hirn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "$,", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "erfrischet,", "tokens": ["er\u00b7fri\u00b7schet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Und mit der k\u00fchlen Lieblichkeit ins Innerste der Seele", "tokens": ["Und", "mit", "der", "k\u00fch\u00b7len", "Lieb\u00b7lich\u00b7keit", "ins", "In\u00b7ners\u00b7te", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "dringet,", "tokens": ["drin\u00b7get", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Woraus Bewundrung, Dank und Andacht, und Preis, und", "tokens": ["Wo\u00b7raus", "Be\u00b7wund\u00b7rung", ",", "Dank", "und", "An\u00b7dacht", ",", "und", "Preis", ",", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "NN", "$,", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Lob in ihr entspringet.", "tokens": ["Lob", "in", "ihr", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Ich hielte sie bald vor die Nase, bald sah ich ihren Schim-", "tokens": ["Ich", "hiel\u00b7te", "sie", "bald", "vor", "die", "Na\u00b7se", ",", "bald", "sah", "ich", "ih\u00b7ren", "Schim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "mer an,", "tokens": ["mer", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Wodurch ich endlich dieses Lied, zu ihres Sch\u00f6pfers Ruhm,", "tokens": ["Wo\u00b7durch", "ich", "end\u00b7lich", "die\u00b7ses", "Lied", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PDAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "begann:", "tokens": ["be\u00b7gann", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wie herrlich sind doch Deine Werke,", "tokens": ["Wie", "herr\u00b7lich", "sind", "doch", "Dei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Brunnquell aller Herrlichkeit!", "tokens": ["O", "Brunn\u00b7quell", "al\u00b7ler", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, gieb doch, da\u00df ich jederzeit,", "tokens": ["Ach", ",", "gieb", "doch", ",", "da\u00df", "ich", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "ADV", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Deiner Vollenkommenheit", "tokens": ["Von", "Dei\u00b7ner", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wunder, \u00fcberall bemerke!", "tokens": ["Die", "Wun\u00b7der", ",", "\u00fc\u00b7be\u00b7rall", "be\u00b7mer\u00b7ke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ach, st\u00e4rke meiner Seelen Augen,", "tokens": ["Ach", ",", "st\u00e4r\u00b7ke", "mei\u00b7ner", "See\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie, durch ihren sch\u00f6nen Dunst,", "tokens": ["Da\u00df", "sie", ",", "durch", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Dunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Dich verh\u00fcllt, die Weisheit, Gunst", "tokens": ["Der", "Dich", "ver\u00b7h\u00fcllt", ",", "die", "Weis\u00b7heit", ",", "Gunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und Deine Macht zu schauen taugen!", "tokens": ["Und", "Dei\u00b7ne", "Macht", "zu", "schau\u00b7en", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch la\u00df uns, mit ger\u00fchrter Seelen,", "tokens": ["Auch", "la\u00df", "uns", ",", "mit", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dein huldreich Wirken gern erzehlen!", "tokens": ["Dein", "huld\u00b7reich", "Wir\u00b7ken", "gern", "er\u00b7zeh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}