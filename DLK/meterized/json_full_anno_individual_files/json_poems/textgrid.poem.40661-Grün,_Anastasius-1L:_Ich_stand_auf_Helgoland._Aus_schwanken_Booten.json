{"textgrid.poem.40661": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stand auf Helgoland. Aus schwanken Booten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf Helgoland. Aus schwanken Booten", "tokens": ["Ich", "stand", "auf", "Hel\u00b7go\u00b7land", ".", "Aus", "schwan\u00b7ken", "Boo\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam neuer G\u00e4ste Schaar zum Strand geschritten;", "tokens": ["Kam", "neu\u00b7er", "G\u00e4s\u00b7te", "Schaar", "zum", "Strand", "ge\u00b7schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da rief mir's zu: \u00bbDein Freund hat ausgelitten!", "tokens": ["Da", "rief", "mir's", "zu", ":", "\u00bb", "Dein", "Freund", "hat", "aus\u00b7ge\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tod l\u00f6ste mild den dunkeln Lebensknoten.\u00ab \u2013 \u2013", "tokens": ["Tod", "l\u00f6s\u00b7te", "mild", "den", "dun\u00b7keln", "Le\u00b7bens\u00b7kno\u00b7ten", ".", "\u00ab", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fcrwahr, der d\u00fcstre Fels in Meeresmitten", "tokens": ["F\u00fcr\u00b7wahr", ",", "der", "d\u00fcst\u00b7re", "Fels", "in", "Mee\u00b7res\u00b7mit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVFIN", "NE", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ort ist's, recht zu denken dieses Todten!", "tokens": ["Ein", "Ort", "ist's", ",", "recht", "zu", "den\u00b7ken", "die\u00b7ses", "Tod\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und solcher Kunde k\u00f6nnt ihr bessern Boten", "tokens": ["Und", "sol\u00b7cher", "Kun\u00b7de", "k\u00f6nnt", "ihr", "bes\u00b7sern", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als sein geliebtes Meer wohl nicht erbitten;", "tokens": ["Als", "sein", "ge\u00b7lieb\u00b7tes", "Meer", "wohl", "nicht", "er\u00b7bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die\u00df Nordmeer, das umw\u00f6lkt, in Trauerschleiern,", "tokens": ["Die\u00df", "Nord\u00b7meer", ",", "das", "um\u00b7w\u00f6lkt", ",", "in", "Trau\u00b7er\u00b7schlei\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PDS", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Klaggest\u0