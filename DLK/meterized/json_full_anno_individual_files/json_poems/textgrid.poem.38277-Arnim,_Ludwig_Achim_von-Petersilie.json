{"textgrid.poem.38277": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Petersilie", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hab ich meinem Sch\u00e4tzlein zu Leide gethan?", "tokens": ["Was", "hab", "ich", "mei\u00b7nem", "Sch\u00e4tz\u00b7lein", "zu", "Lei\u00b7de", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es geht wohl bey mir her, und sieht mich nicht an;", "tokens": ["Es", "geht", "wohl", "bey", "mir", "her", ",", "und", "sieht", "mich", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es schl\u00e4gt seine Augen wohl unter sich,", "tokens": ["Es", "schl\u00e4gt", "sei\u00b7ne", "Au\u00b7gen", "wohl", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PRF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sieht einen andern Schatz wohl lieber als mich.", "tokens": ["Und", "sieht", "ei\u00b7nen", "an\u00b7dern", "Schatz", "wohl", "lie\u00b7ber", "als", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Petersilie, das edle gr\u00fcne Kraut!", "tokens": ["Pe\u00b7ter\u00b7si\u00b7lie", ",", "das", "ed\u00b7le", "gr\u00fc\u00b7ne", "Kraut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Was hab ich meinem Sch\u00e4tzelein so vieles vertraut;", "tokens": ["Was", "hab", "ich", "mei\u00b7nem", "Sch\u00e4t\u00b7zel\u00b7ein", "so", "vie\u00b7les", "ver\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Vieles Vertrauen thut selten gut,", "tokens": ["Vie\u00b7les", "Ver\u00b7trau\u00b7en", "thut", "sel\u00b7ten", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "So w\u00fcnsch ich meinem Sch\u00e4tzelein alles Guts.", "tokens": ["So", "w\u00fcnsch", "ich", "mei\u00b7nem", "Sch\u00e4t\u00b7zel\u00b7ein", "al\u00b7les", "Guts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Alles Guts und noch vielmehr,", "tokens": ["Al\u00b7les", "Guts", "und", "noch", "viel\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach wenn ich nur ein St\u00fcndelein bei meinem Sch\u00e4tzgen w\u00e4r;", "tokens": ["Ach", "wenn", "ich", "nur", "ein", "St\u00fcn\u00b7del\u00b7ein", "bei", "mei\u00b7nem", "Sch\u00e4tz\u00b7gen", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein Viertelst\u00fcndchen zwey und drey,", "tokens": ["Ein", "Vier\u00b7tel\u00b7st\u00fcnd\u00b7chen", "zwey", "und", "drey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "KON", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ich mit meinem Schatz zufrieden sey.", "tokens": ["Da\u00b7mit", "ich", "mit", "mei\u00b7nem", "Schatz", "zu\u00b7frie\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Was hab ich meinem Sch\u00e4tzlein zu Leide gethan?", "tokens": ["Was", "hab", "ich", "mei\u00b7nem", "Sch\u00e4tz\u00b7lein", "zu", "Lei\u00b7de", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es geht wohl bey mir her, und sieht mich nicht an;", "tokens": ["Es", "geht", "wohl", "bey", "mir", "her", ",", "und", "sieht", "mich", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Es schl\u00e4gt seine Augen wohl unter sich,", "tokens": ["Es", "schl\u00e4gt", "sei\u00b7ne", "Au