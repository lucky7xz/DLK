{"textgrid.poem.46936": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steigt ein Geist, umh\u00fcllt von blankem Stahle,", "tokens": ["Es", "steigt", "ein", "Geist", ",", "um\u00b7h\u00fcllt", "von", "blan\u00b7kem", "Stah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Friedrichs Geist, der in der Jahre sieben", "tokens": ["Des", "Fried\u00b7richs", "Geist", ",", "der", "in", "der", "Jah\u00b7re", "sie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einst that die Wunder, die er selbst beschrieben,", "tokens": ["Einst", "that", "die", "Wun\u00b7der", ",", "die", "er", "selbst", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er steigt empor aus seines Grabes Male", "tokens": ["Er", "steigt", "em\u00b7por", "aus", "sei\u00b7nes", "Gra\u00b7bes", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und spricht: \u00bbEs schwankt in dunkler Hand die Schale,", "tokens": ["Und", "spricht", ":", "\u00bb", "Es", "schwankt", "in", "dunk\u00b7ler", "Hand", "die", "Scha\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Reiche w\u00e4gt, und meins ward schnell zerrieben.", "tokens": ["Die", "Rei\u00b7che", "w\u00e4gt", ",", "und", "meins", "ward", "schnell", "zer\u00b7rie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "KON", "NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Seit ich entschlief, war niemand wach geblieben;", "tokens": ["Seit", "ich", "ent\u00b7schlief", ",", "war", "nie\u00b7mand", "wach", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Ro\u00dfbachs Ruhm ging unter in der Saale.", "tokens": ["Und", "Ro\u00df\u00b7bachs", "Ruhm", "ging", "un\u00b7ter", "in", "der", "Saa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer weckt mich heut und will mir Rach' erstreiten?", "tokens": ["Wer", "weckt", "mich", "heut", "und", "will", "mir", "Rach'", "er\u00b7strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "KON", "VMFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ich sehe Helden, da\u