{"textgrid.poem.63823": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Sophie, Gro\u00dfherzogin von Sachsen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schatten f\u00e4llt in dieser Tage Glanz,", "tokens": ["Ein", "Schat\u00b7ten", "f\u00e4llt", "in", "die\u00b7ser", "Ta\u00b7ge", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Wohllaut stirbt in herber Dissonanz.", "tokens": ["Ihr", "Wohl\u00b7laut", "stirbt", "in", "her\u00b7ber", "Dis\u00b7so\u00b7nanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der junge Fr\u00fchling, lachend, wangenrot,", "tokens": ["Der", "jun\u00b7ge", "Fr\u00fch\u00b7ling", ",", "la\u00b7chend", ",", "wan\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erschrickt erblassend vor dem dunklen Tod.", "tokens": ["Er\u00b7schrickt", "er\u00b7blas\u00b7send", "vor", "dem", "dunk\u00b7len", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwei Augen schlossen sich, die ihre Welt", "tokens": ["Zwei", "Au\u00b7gen", "schlos\u00b7sen", "sich", ",", "die", "ih\u00b7re", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PRF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erw\u00e4rmt mit sanftem Leuchten und erhellt.", "tokens": ["Er\u00b7w\u00e4rmt", "mit", "sanf\u00b7tem", "Leuch\u00b7ten", "und", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Herz steht still, das treu blieb immerdar", "tokens": ["Ein", "Herz", "steht", "still", ",", "das", "treu", "blieb", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich selbst und dem, was wert der Liebe war.", "tokens": ["Sich", "selbst", "und", "dem", ",", "was", "wert", "der", "Lie\u00b7be", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ART", "$,", "PWS", "VVFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Geist ging in die ew'ge Klarheit ein,", "tokens": ["Ein", "Geist", "ging", "in", "die", "ew'\u00b7ge", "Klar\u00b7heit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Wahrheit sucht' und feind war allem Schein,", "tokens": ["Der", "Wahr\u00b7heit", "sucht'", "und", "feind", "war", "al\u00b7lem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VAFIN", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Begl\u00fcckt, das Wort der Dichter zu verstehn,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "das", "Wort", "der", "Dich\u00b7ter", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihnen wandelnd auf der Menschheit H\u00f6hn.", "tokens": ["Mit", "ih\u00b7nen", "wan\u00b7delnd", "auf", "der", "Menschheit", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Denn ob am Thron auch ihre Wiege stand,", "tokens": ["Denn", "ob", "am", "Thron", "auch", "ih\u00b7re", "Wie\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Geist nur adelt, hat sie fr\u00fch erkannt.", "tokens": ["Da\u00df", "Geist", "nur", "a\u00b7delt", ",", "hat", "sie", "fr\u00fch", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Geist und ein Herz, das ernst und fromm sich weiht", "tokens": ["Geist", "und", "ein", "Herz", ",", "das", "ernst", "und", "fromm", "sich", "weiht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "PRF", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem Priesteramte reiner Menschlichkeit.", "tokens": ["Dem", "Pries\u00b7ter\u00b7am\u00b7te", "rei\u00b7ner", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer stillt um sie die Trauer, schmerzverzagt,", "tokens": ["Wer", "stillt", "um", "sie", "die", "Trau\u00b7er", ",", "schmerz\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um die ihr Land als um die Mutter klagt", "tokens": ["Um", "die", "ihr", "Land", "als", "um", "die", "Mut\u00b7ter", "klagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPOSAT", "NN", "KOKOM", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und weithin \u00fcber seiner Grenzen Ring,", "tokens": ["Und", "weit\u00b7hin", "\u00fc\u00b7ber", "sei\u00b7ner", "Gren\u00b7zen", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer ihres Geists je einen Hauch empfing!", "tokens": ["Wer", "ih\u00b7res", "Geists", "je", "ei\u00b7nen", "Hauch", "emp\u00b7fing", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Doch schwand sie auch f\u00fcr immer unserm Blick,", "tokens": ["Doch", "schwand", "sie", "auch", "f\u00fcr", "im\u00b7mer", "un\u00b7serm", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bewegten Herzens preis' ich das Geschick,", "tokens": ["Be\u00b7weg\u00b7ten", "Her\u00b7zens", "preis'", "ich", "das", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Das in der F\u00fcrstentochter offenbart", "tokens": ["Das", "in", "der", "F\u00fcrs\u00b7ten\u00b7toch\u00b7ter", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hoheit mit reinstem Frauenwert gepaart,", "tokens": ["Ho\u00b7heit", "mit", "reins\u00b7tem", "Frau\u00b7en\u00b7wert", "ge\u00b7paart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.13": {"line.1": {"text": "Ein segensreich erhabnes Menschenbild,", "tokens": ["Ein", "se\u00b7gens\u00b7reich", "er\u00b7hab\u00b7nes", "Men\u00b7schen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "So klug wie g\u00fctig, so gerecht wie mild.", "tokens": ["So", "klug", "wie", "g\u00fc\u00b7tig", ",", "so", "ge\u00b7recht", "wie", "mild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mir aber, schreitend hier am Seegestad,", "tokens": ["Mir", "a\u00b7ber", ",", "schrei\u00b7tend", "hier", "am", "See\u00b7ge\u00b7stad", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begegnet sie auf einsam stillem Pfad", "tokens": ["Be\u00b7geg\u00b7net", "sie", "auf", "ein\u00b7sam", "stil\u00b7lem", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und sieht mit ihrem hellen Blick mich an,", "tokens": ["Und", "sieht", "mit", "ih\u00b7rem", "hel\u00b7len", "Blick", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der jedem wohltat, der ihr durfte nahn.", "tokens": ["Der", "je\u00b7dem", "wohl\u00b7tat", ",", "der", "ihr", "durf\u00b7te", "nahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "ADJA", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.16": {"line.1": {"text": "Als spr\u00e4che sie: wie magst du klagen nur,", "tokens": ["Als", "spr\u00e4\u00b7che", "sie", ":", "wie", "magst", "du", "kla\u00b7gen", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "$.", "PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verdunkelt sei dir nun die Lenzesflur?", "tokens": ["Ver\u00b7dun\u00b7kelt", "sei", "dir", "nun", "die", "Len\u00b7zes\u00b7flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Mich nahm ein ew'ger Geistesfr\u00fchling auf,", "tokens": ["Mich", "nahm", "ein", "ew'\u00b7ger", "Geis\u00b7tes\u00b7fr\u00fch\u00b7ling", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erhaben ob der Jahreszeiten Lauf,", "tokens": ["Er\u00b7ha\u00b7ben", "ob", "der", "Jah\u00b7res\u00b7zei\u00b7ten", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Und wie du siehst am Hang dort immergr\u00fcn", "tokens": ["Und", "wie", "du", "siehst", "am", "Hang", "dort", "im\u00b7mer\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Scheinlos, doch fr\u00fcchtereich, den Lorbeer bl\u00fchn,", "tokens": ["Schein\u00b7los", ",", "doch", "fr\u00fcch\u00b7ter\u00b7eich", ",", "den", "Lor\u00b7beer", "bl\u00fchn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.19": {"line.1": {"text": "Ein Sinnbild edlen Ruhms, der fortbesteht,", "tokens": ["Ein", "Sinn\u00b7bild", "ed\u00b7len", "Ruhms", ",", "der", "fort\u00b7be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob er auch bunten Farbenprunk verschm\u00e4ht,", "tokens": ["Ob", "er", "auch", "bun\u00b7ten", "Far\u00b7ben\u00b7prunk", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "So la\u00df mich in dir leben fort und fort", "tokens": ["So", "la\u00df", "mich", "in", "dir", "le\u00b7ben", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und klage nicht um mich mit bangem Wort.", "tokens": ["Und", "kla\u00b7ge", "nicht", "um", "mich", "mit", "ban\u00b7gem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "Nur im Begl\u00fccken f\u00fchlt' ich wahres Gl\u00fcck,", "tokens": ["Nur", "im", "Be\u00b7gl\u00fc\u00b7cken", "f\u00fchlt'", "ich", "wah\u00b7res", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ruf in Freuden dir mein Bild zur\u00fcck,", "tokens": ["So", "ruf", "in", "Freu\u00b7den", "dir", "mein", "Bild", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Und in den Stunden, wo die Seele rein,", "tokens": ["Und", "in", "den", "Stun\u00b7den", ",", "wo", "die", "See\u00b7le", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Ew'gen sich erhebt, gedenke mein!", "tokens": ["Zum", "Ew'\u00b7gen", "sich", "er\u00b7hebt", ",", "ge\u00b7den\u00b7ke", "mein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PRF", "VVFIN", "$,", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.23": {"line.1": {"text": "Ein Schatten f\u00e4llt in dieser Tage Glanz,", "tokens": ["Ein", "Schat\u00b7ten", "f\u00e4llt", "in", "die\u00b7ser", "Ta\u00b7ge", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Wohllaut stirbt in herber Dissonanz.", "tokens": ["Ihr", "Wohl\u00b7laut", "stirbt", "in", "her\u00b7ber", "Dis\u00b7so\u00b7nanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "Der junge Fr\u00fchling, lachend, wangenrot,", "tokens": ["Der", "jun\u00b7ge", "Fr\u00fch\u00b7ling", ",", "la\u00b7chend", ",", "wan\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"tex