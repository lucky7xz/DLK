{"dta.poem.19676": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Selbstgef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, wie mirs ist,               ", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mirs", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin nicht krank und bin nicht gesund.", "tokens": ["Ich", "bin", "nicht", "krank", "und", "bin", "nicht", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich bin blessirt und hab keine Wund.", "tokens": ["Ich", "bin", "bles\u00b7sirt", "und", "hab", "kei\u00b7ne", "Wund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df nicht, wie mirs ist,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mirs", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich th\u00e4t gern essen und geschmeckt mir nichts,", "tokens": ["Ich", "th\u00e4t", "gern", "es\u00b7sen", "und", "ge\u00b7schmeckt", "mir", "nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "KON", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich hab ein Geld und gilt mir nichts.", "tokens": ["Ich", "hab", "ein", "Geld", "und", "gilt", "mir", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df nicht, wie mirs ist,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mirs", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab sogar kein Schnupftaback,", "tokens": ["Ich", "hab", "so\u00b7gar", "kein", "Schnupf\u00b7ta\u00b7back", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hab kein Kreutzer Geld im Sack.", "tokens": ["Und", "hab", "kein", "Kreut\u00b7zer", "Geld", "im", "Sack", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nicht, wie mirs ist,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mirs", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "NE", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Heirathen th\u00e4t ich auch schon gern,", "tokens": ["Hei\u00b7ra\u00b7then", "th\u00e4t", "ich", "auch", "schon", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kann aber Kinderschrein nicht h\u00f6rn.", "tokens": ["Kann", "a\u00b7ber", "Kin\u00b7der\u00b7schrein", "nicht", "h\u00f6rn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df nicht, wie mir ist,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "mir", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ich hab erst heut den Doktor gefragt,", "tokens": ["Ich", "hab", "erst", "heut", "den", "Dok\u00b7tor", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der hat mirs unters Gesicht gesagt.", "tokens": ["Der", "hat", "mirs", "un\u00b7ters", "Ge\u00b7sicht", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df wohl, was dir ist,", "tokens": ["Ich", "wei\u00df", "wohl", ",", "was", "dir", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Narr bist du gewi\u00df;", "tokens": ["Ein", "Narr", "bist", "du", "ge\u00b7wi\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun wei\u00df ich wie mir ist!", "tokens": ["Nun", "wei\u00df", "ich", "wie", "mir", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}