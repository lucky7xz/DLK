{"textgrid.poem.61109": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9. Villa bei Melanes auf Naxos", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich der Garten in Duft und D\u00e4mmerung h\u00fcllt! Der Orangen", "tokens": ["Wie", "sich", "der", "Gar\u00b7ten", "in", "Duft", "und", "D\u00e4m\u00b7me\u00b7rung", "h\u00fcllt", "!", "Der", "O\u00b7ran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "---+--+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Saftige Wipfel verstreun liebliches Dunkel umher.", "tokens": ["Saf\u00b7ti\u00b7ge", "Wip\u00b7fel", "ver\u00b7streun", "lieb\u00b7li\u00b7ches", "Dun\u00b7kel", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weithin streckt sich der Pinie Dach, aus Silberoliven", "tokens": ["Weit\u00b7hin", "streckt", "sich", "der", "Pi\u00b7nie", "Dach", ",", "aus", "Sil\u00b7be\u00b7ro\u00b7li\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,", "APPR", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Heben das s\u00e4uselnde Haupt schlanke Zupressen empor.", "tokens": ["He\u00b7ben", "das", "s\u00e4u\u00b7seln\u00b7de", "Haupt", "schlan\u00b7ke", "Zu\u00b7pres\u00b7sen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Durch Weinlauben hinauf f\u00fchrt stattlich zur Villa die Treppe,", "tokens": ["Durch", "Wein\u00b7lau\u00b7ben", "hin\u00b7auf", "f\u00fchrt", "statt\u00b7lich", "zur", "Vil\u00b7la", "die", "Trep\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-++--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Aber des freundlichen Baus weite Gem\u00e4cher sind leer.", "tokens": ["A\u00b7ber", "des", "freund\u00b7li\u00b7chen", "Baus", "wei\u00b7te", "Ge\u00b7m\u00e4\u00b7cher", "sind", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "K\u00f6nnt' ich doch hier, entfernt von der Welt, mit der Jugendgeliebten", "tokens": ["K\u00f6nnt'", "ich", "doch", "hier", ",", "ent\u00b7fernt", "von", "der", "Welt", ",", "mit", "der", "Ju\u00b7gend\u00b7ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "ADJD", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Einmal gr\u00fc\u00dfen den Lenz, wann er mit Bl\u00fcten sich schm\u00fcckt,", "tokens": ["Ein\u00b7mal", "gr\u00fc\u00b7\u00dfen", "den", "Lenz", ",", "wann", "er", "mit", "Bl\u00fc\u00b7ten", "sich", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-+-+-