{"textgrid.poem.35012": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morphine", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df ist die \u00c4hnlichkeit der beiden sch\u00f6nen", "tokens": ["Gro\u00df", "ist", "die", "\u00c4hn\u00b7lich\u00b7keit", "der", "bei\u00b7den", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "J\u00fcnglingsgestalten, ob der eine gleich", "tokens": ["J\u00fcng\u00b7lings\u00b7ge\u00b7stal\u00b7ten", ",", "ob", "der", "ei\u00b7ne", "gleich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ART", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Viel bl\u00e4sser als der andre, auch viel strenger,", "tokens": ["Viel", "bl\u00e4s\u00b7ser", "als", "der", "and\u00b7re", ",", "auch", "viel", "stren\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "$,", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fast m\u00f6cht ich sagen: viel vornehmer aussieht", "tokens": ["Fast", "m\u00f6cht", "ich", "sa\u00b7gen", ":", "viel", "vor\u00b7neh\u00b7mer", "aus\u00b7sieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Als jener andre, welcher mich vertraulich", "tokens": ["Als", "je\u00b7ner", "and\u00b7re", ",", "wel\u00b7cher", "mich", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In seine Arme schlo\u00df \u2013 Wie lieblich sanft", "tokens": ["In", "sei\u00b7ne", "Ar\u00b7me", "schlo\u00df", "\u2013", "Wie", "lieb\u00b7lich", "sanft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "PWAV", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "War dann sein L\u00e4cheln, und sein Blick wie selig!", "tokens": ["War", "dann", "sein", "L\u00e4\u00b7cheln", ",", "und", "sein", "Blick", "wie", "se\u00b7lig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "PPOSAT", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann mocht es wohl geschehn, da\u00df seines Hauptes", "tokens": ["Dann", "mocht", "es", "wohl", "ge\u00b7schehn", ",", "da\u00df", "sei\u00b7nes", "Haup\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mohnblumenkranz auch meine Stirn ber\u00fchrte", "tokens": ["Mohn\u00b7blu\u00b7men\u00b7kranz", "auch", "mei\u00b7ne", "Stirn", "be\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und seltsam duftend allen Schmerz verscheuchte", "tokens": ["Und", "selt\u00b7sam", "duf\u00b7tend", "al\u00b7len", "Schmerz", "ver\u00b7scheuch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Aus meiner Seel' \u2013 Doch solche Linderung,", "tokens": ["Aus", "mei\u00b7ner", "Seel'", "\u2013", "Doch", "sol\u00b7che", "Lin\u00b7de\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie dauert kurze Zeit; genesen g\u00e4nzlich", "tokens": ["Sie", "dau\u00b7ert", "kur\u00b7ze", "Zeit", ";", "ge\u00b7ne\u00b7sen", "g\u00e4nz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$.", "VVPP", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Kann ich nur dann, wenn seine Fackel senkt", "tokens": ["Kann", "ich", "nur", "dann", ",", "wenn", "sei\u00b7ne", "Fa\u00b7ckel", "senkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der andre Bruder' der so ernst und bleich. \u2013", "tokens": ["Der", "and\u00b7re", "Bru\u00b7der'", "der", "so", "ernst", "und", "bleich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Gut ist der Schlaf, der Tod ist besser \u2013 freilich", "tokens": ["Gut", "ist", "der", "Schlaf", ",", "der", "Tod", "ist", "bes\u00b7ser", "\u2013", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$(