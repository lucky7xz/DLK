{"textgrid.poem.31594": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grosm\u00fctigkeit pflegt st\u00e4ts was grosses zu verlangen /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grosm\u00fctigkeit pflegt st\u00e4ts was grosses zu verlangen /", "tokens": ["Gros\u00b7m\u00fc\u00b7tig\u00b7keit", "pflegt", "st\u00e4ts", "was", "gros\u00b7ses", "zu", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PWS", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wie Jason / der sich auf dem Meer zu fahren wagt /", "tokens": ["wie", "Ja\u00b7son", "/", "der", "sich", "auf", "dem", "Meer", "zu", "fah\u00b7ren", "wagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "PRELS", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "und auch das g\u00fcldne Fell durch M\u00fch und Flei\u00df erjagt.", "tokens": ["und", "auch", "das", "g\u00fcld\u00b7ne", "Fell", "durch", "M\u00fch", "und", "Flei\u00df", "er\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ist der Tugend Art / was grosses unterfangen.", "tokens": ["Es", "ist", "der", "Tu\u00b7gend", "Art", "/", "was", "gros\u00b7ses", "un\u00b7ter\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"