{"textgrid.poem.66224": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Chronos", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schnell, o Chronos, rollet dein Wagen,", "tokens": ["Wie", "schnell", ",", "o", "Chro\u00b7nos", ",", "rol\u00b7let", "dein", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "FM", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von st\u00fcrmenden Winden getragen,", "tokens": ["Von", "st\u00fcr\u00b7men\u00b7den", "Win\u00b7den", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch dein weites Gebiet!", "tokens": ["Durch", "dein", "wei\u00b7tes", "Ge\u00b7biet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es rasseln und donnern die R\u00e4der,", "tokens": ["Es", "ras\u00b7seln", "und", "don\u00b7nern", "die", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Durch den weichenden Aether,", "tokens": ["Durch", "den", "wei\u00b7chen\u00b7den", "A\u00b7e\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Da\u00df die Axe gl\u00fcht.", "tokens": ["Da\u00df", "die", "A\u00b7xe", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hoch stehst du mit herrschendem Blicke,", "tokens": ["Hoch", "stehst", "du", "mit", "herr\u00b7schen\u00b7dem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Das Sandglas in der Hand;", "tokens": ["Das", "Sand\u00b7glas", "in", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein Sturmwind treibt dein Gewand", "tokens": ["Ein", "Sturm\u00b7wind", "treibt", "dein", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Und dein Haupthaar, wie Wolken, zur\u00fccke.", "tokens": ["Und", "dein", "Haupt\u00b7haar", ",", "wie", "Wol\u00b7ken", ",", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "--++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "K\u00f6nigreiche fallen, wenn dein Scepter winkt", "tokens": ["K\u00f6\u00b7nig\u00b7rei\u00b7che", "fal\u00b7len", ",", "wenn", "dein", "Scep\u00b7ter", "winkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Und das Felsenhaus des Tyrannen sinkt.", "tokens": ["Und", "das", "Fel\u00b7sen\u00b7haus", "des", "Ty\u00b7ran\u00b7nen", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Unter deinem Wagen winken Wiegen,", "tokens": ["Un\u00b7ter", "dei\u00b7nem", "Wa\u00b7gen", "win\u00b7ken", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Wo mit morgenr\u00f6thlichen Z\u00fcgen", "tokens": ["Wo", "mit", "mor\u00b7gen\u00b7r\u00f6th\u00b7li\u00b7chen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "K\u00fcnftige Geschlechter liegen.", "tokens": ["K\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7schlech\u00b7ter", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Aber auch der Berg des Todes ragt", "tokens": ["A\u00b7ber", "auch", "der", "Berg", "des", "To\u00b7des", "ragt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Hoch empor \u2013 wo mit verwilderter Geberde", "tokens": ["Hoch", "em\u00b7por", "\u2013", "wo", "mit", "ver\u00b7wil\u00b7der\u00b7ter", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$(", "PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Auf losgeschaufelter Erde", "tokens": ["Auf", "los\u00b7ge\u00b7schau\u00b7fel\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.19": {"text": "Die Verwesung \u2013 ach! an Menschenknochen nagt.", "tokens": ["Die", "Ver\u00b7we\u00b7sung", "\u2013", "ach", "!", "an", "Men\u00b7schen\u00b7kno\u00b7chen", "nagt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ITJ", "$.", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Oft ers\u00e4uft der Nachwelt bessere Geschlechter", "tokens": ["Oft", "er\u00b7s\u00e4uft", "der", "Nach\u00b7welt", "bes\u00b7se\u00b7re", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.21": {"text": "Der Zeiten aufgeschwollner Flu\u00df \u2013", "tokens": ["Der", "Zei\u00b7ten", "auf\u00b7ge\u00b7schwoll\u00b7ner", "Flu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und es heulen deine T\u00f6chter,", "tokens": ["Und", "es", "heu\u00b7len", "dei\u00b7ne", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Grauer Archipelagus.", "tokens": ["Grau\u00b7er", "Ar\u00b7chi\u00b7pe\u00b7la\u00b7gus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Dorten an der Felsenwand", "tokens": ["Dor\u00b7ten", "an", "der", "Fel\u00b7sen\u00b7wand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Ringt ein Greis die welke Hand", "tokens": ["Ringt", "ein", "Greis", "die", "wel\u00b7ke", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Auf dem nahen Grabe.", "tokens": ["Auf", "dem", "na\u00b7hen", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "R\u00f6chelnd seufzt er auf: Ich habe,", "tokens": ["R\u00f6\u00b7chelnd", "seufzt", "er", "auf", ":", "Ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Chronos, deinen Werth verkannt \u2013", "tokens": ["Chro\u00b7nos", ",", "dei\u00b7nen", "Werth", "ver\u00b7kannt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und der goldnen Stunde Gabe", "tokens": ["Und", "der", "gold\u00b7nen", "Stun\u00b7de", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ach! \u2013 entsetzlich angewandt.", "tokens": ["Ach", "!", "\u2013", "ent\u00b7setz\u00b7lich", "an\u00b7ge\u00b7wandt", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "$(", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Und ein M\u00e4dchen, ausgeweint und hager,", "tokens": ["Und", "ein", "M\u00e4d\u00b7chen", ",", "aus\u00b7ge\u00b7weint", "und", "ha\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "W\u00e4lzt um Mitternacht sich auf ihrem Lager,", "tokens": ["W\u00e4lzt", "um", "Mit\u00b7ter\u00b7nacht", "sich", "auf", "ih\u00b7rem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Jammernd, da\u00df ein B\u00f6sewicht sie betrog", "tokens": ["Jam\u00b7mernd", ",", "da\u00df", "ein", "B\u00f6\u00b7se\u00b7wicht", "sie", "be\u00b7trog"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "Und ihr Schutzgeist ", "tokens": ["Und", "ihr", "Schutz\u00b7geist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Der Weise, der in stiller Nacht", "tokens": ["Der", "Wei\u00b7se", ",", "der", "in", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Vom Mond bescheint am Gitter wacht,", "tokens": ["Vom", "Mond", "be\u00b7scheint", "am", "Git\u00b7ter", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "H\u00f6rt, Chronos, deinen Wagen rollen \u2013", "tokens": ["H\u00f6rt", ",", "Chro\u00b7nos", ",", "dei\u00b7nen", "Wa\u00b7gen", "rol\u00b7len", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Dann z\u00e4hlt er jeden Augenblick", "tokens": ["Dann", "z\u00e4hlt", "er", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und kehrt mit feuervollem Blick", "tokens": ["Und", "kehrt", "mit", "feu\u00b7er\u00b7vol\u00b7lem", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Zur Tugend und zur Pflicht zur\u00fcck.", "tokens": ["Zur", "Tu\u00b7gend", "und", "zur", "Pflicht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und du \u2013 du lispelst ihm den himmelvollen,", "tokens": ["Und", "du", "\u2013", "du", "lis\u00b7pelst", "ihm", "den", "him\u00b7mel\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Den gro\u00dfen Trost ins Ohr:", "tokens": ["Den", "gro\u00b7\u00dfen", "Trost", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Heil dem, der keinen Tag verlor.", "tokens": ["Heil", "dem", ",", "der", "kei\u00b7nen", "Tag", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure