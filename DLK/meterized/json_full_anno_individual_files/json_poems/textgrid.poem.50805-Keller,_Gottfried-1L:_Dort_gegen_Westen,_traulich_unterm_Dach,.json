{"textgrid.poem.50805": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort gegen Westen, traulich unterm Dach,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort gegen Westen, traulich unterm Dach,", "tokens": ["Dort", "ge\u00b7gen", "Wes\u00b7ten", ",", "trau\u00b7lich", "un\u00b7term", "Dach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liegt hoch und abgeschieden das Gemach,", "tokens": ["Liegt", "hoch", "und", "ab\u00b7ge\u00b7schie\u00b7den", "das", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das sich des Hauses T\u00f6chter jederzeit", "tokens": ["Das", "sich", "des", "Hau\u00b7ses", "T\u00f6ch\u00b7ter", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum stillen Allerheiligsten geweiht.", "tokens": ["Zum", "stil\u00b7len", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}}, "stanza.2": {"line.1": {"text": "Es ist ein eng und niedrig K\u00e4mmerlein", "tokens": ["Es", "ist", "ein", "eng", "und", "nied\u00b7rig", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit runden Scheiben und uraltem Schrein,", "tokens": ["Mit", "run\u00b7den", "Schei\u00b7ben", "und", "ur\u00b7al\u00b7tem", "Schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drin B\u00e4nder, Kettlein, Herzchen aller Art", "tokens": ["Drin", "B\u00e4n\u00b7der", ",", "Ket\u00b7tlein", ",", "Herz\u00b7chen", "al\u00b7ler", "Art"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In mannigfachen K\u00e4stlein wohlverwahrt.", "tokens": ["In", "man\u00b7nig\u00b7fa\u00b7chen", "K\u00e4st\u00b7lein", "wohl\u00b7ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Am Fenster steht das Spinnrad und davor", "tokens": ["Am", "Fens\u00b7ter", "steht", "das", "Spinn\u00b7rad", "und", "da\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "KON", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der zartgepflegte bunte Blumenflor,", "tokens": ["Der", "zart\u00b7ge\u00b7pfleg\u00b7te", "bun\u00b7te", "Blu\u00b7men\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gelbveiglein, Nelken, Rosen ohne End,", "tokens": ["Gelb\u00b7veig\u00b7lein", ",", "Nel\u00b7ken", ",", "Ro\u00b7sen", "oh\u00b7ne", "End", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und wie man all das liebe Zeug benennt!", "tokens": ["Und", "wie", "man", "all", "das", "lie\u00b7be", "Zeug", "be\u00b7nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PIAT", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Manch n\u00e4chtlich Lied hat hier heraufget\u00f6nt,", "tokens": ["Manch", "n\u00e4cht\u00b7lich", "Lied", "hat", "hier", "her\u00b7auf\u00b7ge\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und diese Fensterlein sind dran gew\u00f6hnt,", "tokens": ["Und", "die\u00b7se", "Fens\u00b7ter\u00b7lein", "sind", "dran", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ger\u00e4uschlos blinkend, heimlich aufzugehn,", "tokens": ["Ge\u00b7r\u00e4uschlos", "blin\u00b7kend", ",", "heim\u00b7lich", "auf\u00b7zu\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Ge\u00f6ffnet ganze N\u00e4chte durch zu stehn.", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "gan\u00b7ze", "N\u00e4ch\u00b7te", "durch", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und manche Leiter wurde aufget\u00fcrmt", "tokens": ["Und", "man\u00b7che", "Lei\u00b7ter", "wur\u00b7de", "auf\u00b7ge\u00b7t\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auf die Liebeswarte k\u00fchn gest\u00fcrmt;", "tokens": ["Und", "auf", "die", "Lie\u00b7bes\u00b7war\u00b7te", "k\u00fchn", "ge\u00b7st\u00fcrmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob stets das Rosengitter widerstand,", "tokens": ["Ob", "stets", "das", "Ro\u00b7sen\u00b7git\u00b7ter", "wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Geh\u00f6rt zu den Geheimnissen im Land.", "tokens": ["Ge\u00b7h\u00f6rt", "zu", "den", "Ge\u00b7heim\u00b7nis\u00b7sen", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auch jetzt ist eine Leiter angelegt,", "tokens": ["Auch", "jetzt", "ist", "ei\u00b7ne", "Lei\u00b7ter", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die einen Schwarm geschw\u00e4rzter M\u00e4nner tr\u00e4gt;", "tokens": ["Die", "ei\u00b7nen", "Schwarm", "ge\u00b7schw\u00e4rz\u00b7ter", "M\u00e4n\u00b7ner", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im roten Mantel st\u00fcrmet in die T\u00fcr", "tokens": ["Im", "ro\u00b7ten", "Man\u00b7tel", "st\u00fcr\u00b7met", "in", "die", "T\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Freiersmann mit flammendem Panier.", "tokens": ["Ein", "Frei\u00b7ers\u00b7mann", "mit", "flam\u00b7men\u00b7dem", "Pa\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und vor ihm f\u00e4hrt ein Kn\u00e4uel, wirr und kraus,", "tokens": ["Und", "vor", "ihm", "f\u00e4hrt", "ein", "Kn\u00e4u\u00b7el", ",", "wirr", "und", "kraus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erschreckter Liebesg\u00f6tter fliehend aus;", "tokens": ["Er\u00b7schreck\u00b7ter", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "flie\u00b7hend", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das flattert irrend in der Fr\u00fchlingsluft,", "tokens": ["Das", "flat\u00b7tert", "ir\u00b7rend", "in", "der", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verfliegend wie verbrannter Ambraduft.", "tokens": ["Ver\u00b7flie\u00b7gend", "wie", "ver\u00b7brann\u00b7ter", "Am\u00b7bra\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das ganze Fensterg\u00e4rtlein st\u00fcrzt herab", "tokens": ["Das", "gan\u00b7ze", "Fens\u00b7ter\u00b7g\u00e4rt\u00b7lein", "st\u00fcrzt", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und findet in der Glut sein feurig Grab.", "tokens": ["Und", "fin\u00b7det", "in", "der", "Glut", "sein", "feu\u00b7rig", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob all die stille, sch\u00f6ne Liebeswelt", "tokens": ["Ob", "all", "die", "stil\u00b7le", ",", "sch\u00f6\u00b7ne", "Lie\u00b7bes\u00b7welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wohl rettungslos damit in Asche f\u00e4llt?", "tokens": ["Wohl", "ret\u00b7tungs\u00b7los", "da\u00b7mit", "in", "A\u00b7sche", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Mir ist nicht bang; ist neu das Haus erbaut,", "tokens": ["Mir", "ist", "nicht", "bang", ";", "ist", "neu", "das", "Haus", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta