{"dta.poem.5265": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch einige Betrachtungen der Bl\u00e4tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da die glatten, saftgen Aeste", "tokens": ["Da", "die", "glat\u00b7ten", ",", "saft\u00b7gen", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sich so nett, verwirrt und feste", "tokens": ["Sich", "so", "nett", ",", "ver\u00b7wirrt", "und", "fes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieblich in einander schrencken;", "tokens": ["Lieb\u00b7lich", "in", "ein\u00b7an\u00b7der", "schren\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnschet der vergn\u00fcgte Blick", "tokens": ["W\u00fcn\u00b7schet", "der", "ver\u00b7gn\u00fcg\u00b7te", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fast nicht wiederum zur\u00fcck,", "tokens": ["Fast", "nicht", "wie\u00b7de\u00b7rum", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sucht sich tieffer zu versencken.", "tokens": ["Sucht", "sich", "tief\u00b7fer", "zu", "ver\u00b7sen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn er glaubt in ihren Gr\u00fcnden,", "tokens": ["Denn", "er", "glaubt", "in", "ih\u00b7ren", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwischen denen glatten Rinden,", "tokens": ["Zwi\u00b7schen", "de\u00b7nen", "glat\u00b7ten", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Stets ein neues Gr\u00fcn zu finden.", "tokens": ["Stets", "ein", "neu\u00b7es", "Gr\u00fcn", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00fcrcklich wird er auch gewahr,", "tokens": ["W\u00fcrck\u00b7lich", "wird", "er", "auch", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wie der gr\u00fcnen Knospen Schaar", "tokens": ["Wie", "der", "gr\u00fc\u00b7nen", "Knos\u00b7pen", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Das, womit sie sich erf\u00fcllen,", "tokens": ["Das", ",", "wo\u00b7mit", "sie", "sich", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Jhre Bl\u00e4tter, zu enth\u00fcllen,", "tokens": ["Ih\u00b7re", "Bl\u00e4t\u00b7ter", ",", "zu", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und das r\u00f6htlich braune Gr\u00fcn", "tokens": ["Und", "das", "r\u00f6ht\u00b7lich", "brau\u00b7ne", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Auszuschm\u00fccken sich bem\u00fchn.", "tokens": ["Aus\u00b7zu\u00b7schm\u00fc\u00b7cken", "sich", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Wenn alsdann, bey heiterm Wetter,", "tokens": ["Wenn", "als\u00b7dann", ",", "bey", "hei\u00b7term", "Wet\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Durch den zarten Leib der Bl\u00e4tter,", "tokens": ["Durch", "den", "zar\u00b7ten", "Leib", "der", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Das entw\u00f6lckte Sonnen-Licht", "tokens": ["Das", "ent\u00b7w\u00f6lck\u00b7te", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Hin und wieder lieblich dringt,", "tokens": ["Hin", "und", "wie\u00b7der", "lieb\u00b7lich", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Sich mit ihnen gleichsam gattet,", "tokens": ["Sich", "mit", "ih\u00b7nen", "gleich\u00b7sam", "gat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und durchstrahlet; so entspringt", "tokens": ["Und", "durch\u00b7strah\u00b7let", ";", "so", "ent\u00b7springt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.22": {"text": "Ein fast brennend Gr\u00fcn, zumahl", "tokens": ["Ein", "fast", "bren\u00b7nend", "Gr\u00fcn", ",", "zu\u00b7mahl"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADJD", "NN", "$,", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da, wo selbst der Grund beschattet", "tokens": ["Da", ",", "wo", "selbst", "der", "Grund", "be\u00b7schat\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ADV", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Und der Sonnen heller Strahl", "tokens": ["Und", "der", "Son\u00b7nen", "hel\u00b7ler", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Hie und da ein Blatt allein", "tokens": ["Hie", "und", "da", "ein", "Blatt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Trift, verkl\u00e4rt, illuminiret.", "tokens": ["Trift", ",", "ver\u00b7kl\u00e4rt", ",", "il\u00b7lu\u00b7mi\u00b7ni\u00b7ret", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Durch den Farben-reichen Schein", "tokens": ["Durch", "den", "Fa\u00b7rben\u00b7rei\u00b7chen", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Wird im Aug\u2019 ein Hertz ger\u00fchret;", "tokens": ["Wird", "im", "Aug'", "ein", "Hertz", "ge\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}