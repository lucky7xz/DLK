{"textgrid.poem.37034": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Gottheit", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist es Sch\u00f6pfer, der durch einen Wink", "tokens": ["Du", "bist", "es", "Sch\u00f6p\u00b7fer", ",", "der", "durch", "ei\u00b7nen", "Wink"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zahllose Welten schuff,", "tokens": ["Zahl\u00b7lo\u00b7se", "Wel\u00b7ten", "schuff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Rosen um des Morgens Stirne band,", "tokens": ["Der", "Ro\u00b7sen", "um", "des", "Mor\u00b7gens", "Stir\u00b7ne", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und um des Abends Haupt", "tokens": ["Und", "um", "des", "A\u00b7bends", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Kranz von Gold. Dich lobet die Natur,", "tokens": ["Den", "Kranz", "von", "Gold", ".", "Dich", "lo\u00b7bet", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das bunte Veilchenthal,", "tokens": ["Das", "bun\u00b7te", "Veil\u00b7chen\u00b7thal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Morgenr\u00f6the, die ihr junges Bild", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ",", "die", "ihr", "jun\u00b7ges", "Bild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In jede Welle pr\u00e4gt.", "tokens": ["In", "je\u00b7de", "Wel\u00b7le", "pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dich lobt der Westwind, der die gr\u00fcne Fluth", "tokens": ["Dich", "lobt", "der", "West\u00b7wind", ",", "der", "die", "gr\u00fc\u00b7ne", "Fluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Saaten kr\u00e4uselt, Dich", "tokens": ["Der", "Saa\u00b7ten", "kr\u00e4u\u00b7selt", ",", "Dich"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhebt die Windsbraut, die den Eichenwald", "tokens": ["Er\u00b7hebt", "die", "Winds\u00b7braut", ",", "die", "den", "Ei\u00b7chen\u00b7wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entwurzelt und zerrei\u00dft.", "tokens": ["Ent\u00b7wur\u00b7zelt", "und", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dich lobt der Donner, der am Himmel rollt,", "tokens": ["Dich", "lobt", "der", "Don\u00b7ner", ",", "der", "am", "Him\u00b7mel", "rollt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und rothe Blitze spr\u00fcht,", "tokens": ["Und", "ro\u00b7the", "Blit\u00b7ze", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wolkenbruch, der auf die Wiese f\u00e4llt,", "tokens": ["Der", "Wol\u00b7ken\u00b7bruch", ",", "der", "auf", "die", "Wie\u00b7se", "f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sie in Meer verkehrt.", "tokens": ["Und", "sie", "in", "Meer", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dich loben Flocken, die das gr\u00fcne Haar", "tokens": ["Dich", "lo\u00b7ben", "Flo\u00b7cken", ",", "die", "das", "gr\u00fc\u00b7ne", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des finstren Tannenhayns", "tokens": ["Des", "finst\u00b7ren", "Tan\u00b7nen\u00b7hayns"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Silber \u00fcberstreun, und wei\u00dfen Flor", "tokens": ["Mit", "Sil\u00b7ber", "\u00fc\u00b7bers\u00b7treun", ",", "und", "wei\u00b7\u00dfen", "Flor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um die Geb\u00fcsche ziehn.", "tokens": ["Um", "die", "Ge\u00b7b\u00fc\u00b7sche", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Die ganze Erde ist Dein Lobgesang,", "tokens": ["Die", "gan\u00b7ze", "Er\u00b7de", "ist", "Dein", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Hymn' auf Hymne steigt", "tokens": ["Und", "Hymn'", "auf", "Hym\u00b7ne", "steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Deinem Thron empor, wo Du im Licht,", "tokens": ["Zu", "Dei\u00b7nem", "Thron", "em\u00b7por", ",", "wo", "Du", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie im Gezelte, wohnst.", "tokens": ["Wie", "im", "Ge\u00b7zel\u00b7te", ",", "wohnst", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verstummest du allein, du Mensch? Erwach", "tokens": ["Ver\u00b7stum\u00b7mest", "du", "al\u00b7lein", ",", "du", "Mensch", "?", "Er\u00b7wach"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Schlummer, der dich dr\u00fcckt,", "tokens": ["Vom", "Schlum\u00b7mer", ",", "der", "dich", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wirf deine Blicke rings umher, und sieh", "tokens": ["Wirf", "dei\u00b7ne", "Bli\u00b7cke", "rings", "um\u00b7her", ",", "und", "sieh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADV", "PTKVZ", "$,", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunder seiner Macht.", "tokens": ["Die", "Wun\u00b7der", "sei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Prei\u00df deinen Sch\u00f6pfer, wenn der fr\u00fche Hahn", "tokens": ["Prei\u00df", "dei\u00b7nen", "Sch\u00f6p\u00b7fer", ",", "wenn", "der", "fr\u00fc\u00b7he", "Hahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Morgen ausposaunt,", "tokens": ["Den", "Mor\u00b7gen", "aus\u00b7po\u00b7saunt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erheb ihn, wenn das milde Abendroth", "tokens": ["Er\u00b7heb", "ihn", ",", "wenn", "das", "mil\u00b7de", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die H\u00fcgelspitzen mahlt.", "tokens": ["Die", "H\u00fc\u00b7gel\u00b7spit\u00b7zen", "mahlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Erheb ihn, wenn die Nacht der See entsteigt,", "tokens": ["Er\u00b7heb", "ihn", ",", "wenn", "die", "Nacht", "der", "See", "ent\u00b7steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ihr Gewand, durchwebt", "tokens": ["Und", "ihr", "Ge\u00b7wand", ",", "durch\u00b7webt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit goldnen Sternen, und mit Mondenschein,", "tokens": ["Mit", "gold\u00b7nen", "Ster\u00b7nen", ",", "und", "mit", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rund um den Himmel wirft. \u2013", "tokens": ["Rund", "um", "den", "Him\u00b7mel", "wirft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Prei\u00df ihn durch Hymnen, heiliges Gef\u00fchl", "tokens": ["Prei\u00df", "ihn", "durch", "Hym\u00b7nen", ",", "hei\u00b7li\u00b7ges", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ergreife dich, wenn du", "tokens": ["Er\u00b7grei\u00b7fe", "dich", ",", "wenn", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Sch\u00f6pfer denkst, der dich ins Leben rief,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "denkst", ",", "der", "dich", "ins", "Le\u00b7ben", "rief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Gott voll Gnad und Huld.", "tokens": ["Den", "Gott", "voll", "Gnad", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du bist es Sch\u00f6pfer, der durch einen Wink", "tokens": ["Du", "bist", "es", "Sch\u00f6p\u00b7fer", ",", "der", "durch", "ei\u00b7nen", "Wink"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zahllose Welten schuff,", "tokens": ["Zahl\u00b7lo\u00b7se", "Wel\u00b7ten", "schuff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Rosen um des Morgens Stirne band,", "tokens": ["Der", "Ro\u00b7sen", "um", "des", "Mor\u00b7gens", "Stir\u00b7ne", "band", ","], "token_info":