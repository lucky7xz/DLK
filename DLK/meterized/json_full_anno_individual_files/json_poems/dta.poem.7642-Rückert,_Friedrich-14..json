{"dta.poem.7642": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragest, wo und wie im Land du wohnen sollest,", "tokens": ["Du", "fra\u00b7gest", ",", "wo", "und", "wie", "im", "Land", "du", "woh\u00b7nen", "sol\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "KON", "KOKOM", "APPRART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du des Menschen Zweck und Gl\u00fcck erreichen wollest.", "tokens": ["Wenn", "du", "des", "Men\u00b7schen", "Zweck", "und", "Gl\u00fcck", "er\u00b7rei\u00b7chen", "wol\u00b7lest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohn' unter Himmelklar auf selbstbegr\u00fcnter Flur,", "tokens": ["Wohn'", "un\u00b7ter", "Him\u00b7mel\u00b7klar", "auf", "selbst\u00b7be\u00b7gr\u00fcn\u00b7ter", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ruhend im Vollgenu\u00df am Busen der Natur.", "tokens": ["Ru\u00b7hend", "im", "Voll\u00b7ge\u00b7nu\u00df", "am", "Bu\u00b7sen", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wohn' auf bebautem Feld, wo, was man pflanzte, sprie\u00dft,", "tokens": ["Wohn'", "auf", "be\u00b7bau\u00b7tem", "Feld", ",", "wo", ",", "was", "man", "pflanz\u00b7te", ",", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PWAV", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In F\u00fclle, die sie schafft, die Arbeit sich genie\u00dft.", "tokens": ["In", "F\u00fcl\u00b7le", ",", "die", "sie", "schafft", ",", "die", "Ar\u00b7beit", "sich", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wohn' in belebter Stadt, wo eins das andre regt,", "tokens": ["Wohn'", "in", "be\u00b7leb\u00b7ter", "Stadt", ",", "wo", "eins", "das", "and\u00b7re", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "PWAV", "PIS", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bild' und la\u00df bilden dich, bewegend und bewegt.", "tokens": ["Bild'", "und", "la\u00df", "bil\u00b7den", "dich", ",", "be\u00b7we\u00b7gend", "und", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVIMP", "VVFIN", "PPER", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wohn' in der W\u00fcste, wo Natur- und Menschenweben", "tokens": ["Wohn'", "in", "der", "W\u00fcs\u00b7te", ",", "wo", "Na\u00b7tur", "und", "Men\u00b7schen\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PWAV", "TRUNC", "KON", "NN"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Dich beides nicht ber\u00fchrt, um dir und Gott zu leben.", "tokens": ["Dich", "bei\u00b7des", "nicht", "be\u00b7r\u00fchrt", ",", "um", "dir", "und", "Gott", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PTKNEG", "VVPP", "$,", "KOUI", "PPER", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo du auch wohnen magst, da kannst du seyn und bleiben", "tokens": ["Wo", "du", "auch", "woh\u00b7nen", "magst", ",", "da", "kannst", "du", "seyn", "und", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mensch, und Menschliches so oder anders treiben.", "tokens": ["Ein", "Mensch", ",", "und", "Menschli\u00b7ches", "so", "o\u00b7der", "an\u00b7ders", "trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}