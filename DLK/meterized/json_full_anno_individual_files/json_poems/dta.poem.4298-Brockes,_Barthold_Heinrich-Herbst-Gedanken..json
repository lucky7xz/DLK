{"dta.poem.4298": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Gedanken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da im entw\u00f6lkten Sonnenschein", "tokens": ["Da", "im", "ent\u00b7w\u00f6lk\u00b7ten", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut\u2019 alles, was mein Auge siehet,", "tokens": ["Heut'", "al\u00b7les", ",", "was", "mein", "Au\u00b7ge", "sie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einst so sch\u00f6n, noch einst so rein,", "tokens": ["Noch", "einst", "so", "sch\u00f6n", ",", "noch", "einst", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00e4rbt, und gleichsam gl\u00e4nzt und gl\u00fchet;", "tokens": ["Ge\u00b7f\u00e4rbt", ",", "und", "gleich\u00b7sam", "gl\u00e4nzt", "und", "gl\u00fc\u00b7het", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erweg\u2019 ich die so sch\u00f6ne Welt,", "tokens": ["Er\u00b7weg'", "ich", "die", "so", "sch\u00f6\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als mir vom Sch\u00f6pfer vorgestellt,", "tokens": ["Als", "mir", "vom", "Sch\u00f6p\u00b7fer", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und la\u00df das, was an ihr so sch\u00f6n,", "tokens": ["Und", "la\u00df", "das", ",", "was", "an", "ihr", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDS", "$,", "PRELS", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir, durch das Aug\u2019, ans Herze geh\u2019n,", "tokens": ["Mir", ",", "durch", "das", "Aug'", ",", "ans", "Her\u00b7ze", "geh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und recht in meine Seele dringen;", "tokens": ["Und", "recht", "in", "mei\u00b7ne", "See\u00b7le", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da denn die aufgebrachten Triebe,", "tokens": ["Da", "denn", "die", "auf\u00b7ge\u00b7brach\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Des Sch\u00f6pfers Weisheit, Macht und Liebe,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Voll Lust und Andacht zu besingen,", "tokens": ["Voll", "Lust", "und", "An\u00b7dacht", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mit lieblicher Gewalt mich zwingen.", "tokens": ["Mit", "lieb\u00b7li\u00b7cher", "Ge\u00b7walt", "mich", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Herbst will uns mit seinen Sch\u00e4tzen,", "tokens": ["Der", "Herbst", "will", "uns", "mit", "sei\u00b7nen", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem mit seiner Fruchtbarkeit", "tokens": ["Nach\u00b7dem", "mit", "sei\u00b7ner", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schw\u00fchle Sommer uns erfreut,", "tokens": ["Der", "schw\u00fch\u00b7le", "Som\u00b7mer", "uns", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf eine neue Weis\u2019 ergetzen.", "tokens": ["Auf", "ei\u00b7ne", "neu\u00b7e", "Weis'", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es \u00e4ndert die Natur zwar itzo die Gestalt,", "tokens": ["Es", "\u00e4n\u00b7dert", "die", "Na\u00b7tur", "zwar", "it\u00b7zo", "die", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da Berge, Th\u00e4ler, Feld und Wald", "tokens": ["Da", "Ber\u00b7ge", ",", "Th\u00e4\u00b7ler", ",", "Feld", "und", "Wald"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einer andern Farb\u2019 erscheinen;", "tokens": ["In", "ei\u00b7ner", "an\u00b7dern", "Fa\u00b7rb'", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Allein, man darf darum nicht meynen,", "tokens": ["Al\u00b7lein", ",", "man", "darf", "da\u00b7rum", "nicht", "mey\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PIS", "VMFIN", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df selbe darum minder sch\u00f6n,", "tokens": ["Da\u00df", "sel\u00b7be", "da\u00b7rum", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "PAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn wir sie nur bedachtsam seh\u2019n.", "tokens": ["Wenn", "wir", "sie", "nur", "be\u00b7dacht\u00b7sam", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das itzo fast nicht abzuseh\u2019nde, und gleichsam Grenzen-", "tokens": ["Das", "it\u00b7zo", "fast", "nicht", "ab\u00b7zu\u00b7seh'n\u00b7de", ",", "und", "gleich\u00b7sam", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "PTKNEG", "ADJA", "$,", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "lose Feld,", "tokens": ["lo\u00b7se", "Feld", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nachdem es \u00fcberall gepfl\u00fcgt, bes\u00e4t, geeget und bestellt,", "tokens": ["Nach\u00b7dem", "es", "\u00fc\u00b7be\u00b7rall", "ge\u00b7pfl\u00fcgt", ",", "be\u00b7s\u00e4t", ",", "gee\u00b7get", "und", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "ADJD", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Zeigt itzt, in seiner ebnen, sanften und dunkel-braunen flachen", "tokens": ["Zeigt", "itzt", ",", "in", "sei\u00b7ner", "eb\u00b7nen", ",", "sanf\u00b7ten", "und", "dun\u00b7kel\u00b7brau\u00b7nen", "fla\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJA", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Bl\u00f6sse,", "tokens": ["Bl\u00f6s\u00b7se", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Den Blicken, die dar\u00fcber gleiten, nur fein\u2019 und klein\u2019 geegte", "tokens": ["Den", "Bli\u00b7cken", ",", "die", "da\u00b7r\u00fc\u00b7ber", "glei\u00b7ten", ",", "nur", "fein'", "und", "klein'", "geeg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVINF", "$,", "ADV", "ADJD", "KON", "ADJD", "ADJA"], "meter": "-+--+--+--+-++-", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Kl\u00f6sse.", "tokens": ["Kl\u00f6s\u00b7se", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es ist itzt einem weiten, leeren und aufger\u00e4umten Schauplatz", "tokens": ["Es", "ist", "itzt", "ei\u00b7nem", "wei\u00b7ten", ",", "lee\u00b7ren", "und", "auf\u00b7ge\u00b7r\u00e4um\u00b7ten", "Schau\u00b7platz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "gleich,", "tokens": ["gleich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Worauf der Landmann albereit, an einer neuen Hoffnung", "tokens": ["Wo\u00b7rauf", "der", "Land\u00b7mann", "al\u00b7be\u00b7reit", ",", "an", "ei\u00b7ner", "neu\u00b7en", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "reich,", "tokens": ["reich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "In einer Segen-reichen Saat, die es mit g\u00fcldnen Aehren", "tokens": ["In", "ei\u00b7ner", "Se\u00b7gen\u00b7rei\u00b7chen", "Saat", ",", "die", "es", "mit", "g\u00fcld\u00b7nen", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "schm\u00fccket,", "tokens": ["schm\u00fc\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Die noch unsichtbare Ver\u00e4ndrung im Geiste schon vorher", "tokens": ["Die", "noch", "un\u00b7sicht\u00b7ba\u00b7re", "Ver\u00b7\u00e4n\u00b7drung", "im", "Geis\u00b7te", "schon", "vor\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPOSAT", "NN", "APPRART", "NN", "ADV", "ADV"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "erblicket.", "tokens": ["er\u00b7bli\u00b7cket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}