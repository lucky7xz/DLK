en schilt!", "tokens": ["da\u00df", "er", "die", "stren\u00b7ge", "Pracht", "der", "fre\u00b7chen", "Wel\u00b7len", "schilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprich auch, da\u00df \u00c4ol stracks sein leichtes Volk verbanne,", "tokens": ["Sprich", "auch", ",", "da\u00df", "\u00c4\u00b7ol", "stracks", "sein", "leich\u00b7tes", "Volk", "ver\u00b7ban\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "darmit es nicht auf uns mit Sturm' und Wetter billt!", "tokens": ["dar\u00b7mit", "es", "nicht", "auf", "uns", "mit", "Sturm'", "und", "Wet\u00b7ter", "billt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf Doris, Doris auf mit tausent Najadinnen!", "tokens": ["Auf", "Do\u00b7ris", ",", "Do\u00b7ris", "auf", "mit", "tau\u00b7sent", "Na\u00b7ja\u00b7din\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "APPR", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der g\u00fcnstige Nordwest wird unsers Aufbruchs innen.", "tokens": ["Der", "g\u00fcns\u00b7ti\u00b7ge", "Nord\u00b7west", "wird", "un\u00b7sers", "Auf\u00b7bruchs", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "