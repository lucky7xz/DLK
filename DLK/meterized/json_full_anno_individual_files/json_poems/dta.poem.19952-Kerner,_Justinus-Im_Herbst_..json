{"dta.poem.19952": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh' nur, du Sonne, zieh'", "tokens": ["Zieh'", "nur", ",", "du", "Son\u00b7ne", ",", "zieh'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Eilend von hier, von hier!", "tokens": ["Ei\u00b7lend", "von", "hier", ",", "von", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADV", "$,", "APPR", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Auf da\u00df Ihr W\u00e4rme komm'", "tokens": ["Auf", "da\u00df", "Ihr", "W\u00e4r\u00b7me", "komm'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einzig von mir.", "tokens": ["Ein\u00b7zig", "von", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Welkt nur, ihr Blumen, welkt!", "tokens": ["Welkt", "nur", ",", "ihr", "Blu\u00b7men", ",", "welkt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Schweigt nur, ihr V\u00f6gelein!", "tokens": ["Schweigt", "nur", ",", "ihr", "V\u00f6\u00b7ge\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf da\u00df Ihr sing' und bl\u00fch'", "tokens": ["Auf", "da\u00df", "Ihr", "sing'", "und", "bl\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "KON", "XY"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich nur allein.", "tokens": ["Ich", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}