{"dta.poem.23786": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Des  Autoris  Gedancken in seiner letzten  \n Kranckheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Blut und L\u00fcste sch\u00e4umen/", "tokens": ["Wenn", "Blut", "und", "L\u00fcs\u00b7te", "sch\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So st\u00e4rcke meinen Geist/", "tokens": ["So", "st\u00e4r\u00b7cke", "mei\u00b7nen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er sich auch im Tr\u00e4umen", "tokens": ["Da\u00df", "er", "sich", "auch", "im", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Satans Netze rei\u00dft.", "tokens": ["Aus", "Sa\u00b7tans", "Net\u00b7ze", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hilff f\u00fcr mein Bestes sorgen/", "tokens": ["Hilff", "f\u00fcr", "mein", "Bes\u00b7tes", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ver\u00e4ndre meinen Sinn/", "tokens": ["Ver\u00b7\u00e4nd\u00b7re", "mei\u00b7nen", "Sinn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mache/ da\u00df ich morgen", "tokens": ["Und", "ma\u00b7che", "/", "da\u00df", "ich", "mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein neu Gesch\u00f6pffe bin.", "tokens": ["Ein", "neu", "Ge\u00b7sch\u00f6pf\u00b7fe", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich seh\u2019 das Licht verschwinden/", "tokens": ["Ich", "seh'", "das", "Licht", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die tr\u00fcbe Nacht bricht ein/", "tokens": ["Die", "tr\u00fc\u00b7be", "Nacht", "bricht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach HErr/ la\u00df meine S\u00fcnden", "tokens": ["Ach", "Herr", "/", "la\u00df", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$(", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch mit verschwunden seyn/", "tokens": ["Auch", "mit", "ver\u00b7schwun\u00b7den", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVPP", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Streich\u2019 sie aus deinem Buche/", "tokens": ["Streich'", "sie", "aus", "dei\u00b7nem", "Bu\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das mich zum Schuldner macht/", "tokens": ["Das", "mich", "zum", "Schuld\u00b7ner", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und rette mich vom Fluche/", "tokens": ["Und", "ret\u00b7te", "mich", "vom", "Flu\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der mir war zugedacht.", "tokens": ["Der", "mir", "war", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Wenn heut mein Ziel der Jahre", "tokens": ["Wenn", "heut", "mein", "Ziel", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein letzter Abend ist/", "tokens": ["Mein", "letz\u00b7ter", "A\u00b7bend", "ist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wolan! wann ich nur fahre", "tokens": ["Wo\u00b7lan", "!", "wann", "ich", "nur", "fah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo deine Wohnung ist.", "tokens": ["Wo", "dei\u00b7ne", "Woh\u00b7nung", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "(wo du/ mein Vater/ bist.)", "tokens": ["(", "wo", "du", "/", "mein", "Va\u00b7ter", "/", "bist", ".", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PPER", "$(", "PPOSAT", "NN", "$(", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch soll ich l\u00e4nger leben/", "tokens": ["Doch", "soll", "ich", "l\u00e4n\u00b7ger", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So la\u00df den festen Schlu\u00df", "tokens": ["So", "la\u00df", "den", "fes\u00b7ten", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mir stets f\u00fcr Augen schweben/", "tokens": ["Mir", "stets", "f\u00fcr", "Au\u00b7gen", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df ich einst scheiden mu\u00df!", "tokens": ["Da\u00df", "ich", "einst", "schei\u00b7den", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}