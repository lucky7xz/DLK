{"textgrid.poem.61313": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An Klara", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du vom Teppich Salomos", "tokens": ["Hast", "du", "vom", "Tep\u00b7pich", "Sa\u00b7lo\u00b7mos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00f6rt die wundervolle Sage,", "tokens": ["Ge\u00b7h\u00f6rt", "die", "wun\u00b7der\u00b7vol\u00b7le", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dran in kristallner Grotte Scho\u00df", "tokens": ["Dran", "in", "kris\u00b7tall\u00b7ner", "Grot\u00b7te", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die Geister woben drei\u00dfig Tage?", "tokens": ["Die", "Geis\u00b7ter", "wo\u00b7ben", "drei\u00b7\u00dfig", "Ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer ihn betrat mit Zauberwort,", "tokens": ["Wer", "ihn", "be\u00b7trat", "mit", "Zau\u00b7ber\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den trug er durch die L\u00fcfte fort,", "tokens": ["Den", "trug", "er", "durch", "die", "L\u00fcf\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein schwebend Schifflein rastlos fliegend,", "tokens": ["Ein", "schwe\u00b7bend", "Schif\u00b7flein", "rast\u00b7los", "flie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In blauer \u00c4therflut sich wiegend.", "tokens": ["In", "blau\u00b7er", "\u00c4\u00b7ther\u00b7flut", "sich", "wie\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin nicht K\u00f6nig Salomo,", "tokens": ["Ich", "bin", "nicht", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf dessen Wink D\u00e4monen schreiten:", "tokens": ["Auf", "des\u00b7sen", "Wink", "D\u00e4\u00b7mo\u00b7nen", "schrei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum mu\u00dft' ich selber still und froh", "tokens": ["Drum", "mu\u00dft'", "ich", "sel\u00b7ber", "still", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlummerteppich dir bereiten;", "tokens": ["Den", "Schlum\u00b7mer\u00b7tep\u00b7pich", "dir", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch hat auch hier ein Geist von oben,", "tokens": ["Doch", "hat", "auch", "hier", "ein", "Geist", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe hat mit dran gewoben.", "tokens": ["Die", "Lie\u00b7be", "hat", "mit", "dran", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und sieh, mich d\u00fcnkt, da\u00df Liebeskraft", "tokens": ["Und", "sieh", ",", "mich", "d\u00fcnkt", ",", "da\u00df", "Lie\u00b7bes\u00b7kraft"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wohl fast noch s\u00fc\u00dfre Wunder schafft.", "tokens": ["Wohl", "fast", "noch", "s\u00fc\u00df\u00b7re", "Wun\u00b7der", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Denn wenn du tagesmatt die Glieder", "tokens": ["Denn", "wenn", "du", "ta\u00b7ges\u00b7matt", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Geh\u00fcllt in dies Gewebe kaum,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "dies", "Ge\u00b7we\u00b7be", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDS", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So kommen leise zu dir nieder", "tokens": ["So", "kom\u00b7men", "lei\u00b7se", "zu", "dir", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die stillen Knaben Schlaf und Traum,", "tokens": ["Die", "stil\u00b7len", "Kna\u00b7ben", "Schlaf", "und", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit lindem, k\u00fchlem Fl\u00fcgelschlagen", "tokens": ["Mit", "lin\u00b7dem", ",", "k\u00fch\u00b7lem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ins Reich der M\u00e4rchen dich zu tragen.", "tokens": ["Ins", "Reich", "der", "M\u00e4r\u00b7chen", "dich", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da klingt's im Ohr dir wie ein Lied;", "tokens": ["Da", "klingt's", "im", "Ohr", "dir", "wie", "ein", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Nebel rei\u00dft \u2013 dein Auge sieht,", "tokens": ["Ein", "Ne\u00b7bel", "rei\u00dft", "\u2013", "dein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Befreit von jeder dumpfen H\u00fclle,", "tokens": ["Be\u00b7freit", "von", "je\u00b7der", "dum\u00b7pfen", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Erschlossen aller Wunder F\u00fclle.", "tokens": ["Er\u00b7schlos\u00b7sen", "al\u00b7ler", "Wun\u00b7der", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was war, was ist, was kommen will,", "tokens": ["Was", "war", ",", "was", "ist", ",", "was", "kom\u00b7men", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWS", "VAFIN", "$,", "PWS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schaust du zugleich; die Zeit steht still.", "tokens": ["Schaust", "du", "zu\u00b7gleich", ";", "die", "Zeit", "steht", "still", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Bei Fr\u00fchlingsbl\u00fcten gl\u00e4nzt im Laube", "tokens": ["Bei", "Fr\u00fch\u00b7lings\u00b7bl\u00fc\u00b7ten", "gl\u00e4nzt", "im", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die goldne Frucht, die gl\u00fchnde Traube;", "tokens": ["Die", "gold\u00b7ne", "Frucht", ",", "die", "gl\u00fchn\u00b7de", "Trau\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Das Wissen der erfahrnen Brust", "tokens": ["Das", "Wis\u00b7sen", "der", "er\u00b7fahr\u00b7nen", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Verschmilzt mit reinster Jugendlust;", "tokens": ["Ver\u00b7schmilzt", "mit", "reins\u00b7ter", "Ju\u00b7gend\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Du sp\u00fcrst im Herzen s\u00fc\u00dferschrocken", "tokens": ["Du", "sp\u00fcrst", "im", "Her\u00b7zen", "s\u00fc\u00b7\u00dfer\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der fr\u00fchsten Liebesahnung Glanz,", "tokens": ["Der", "fr\u00fchs\u00b7ten", "Lie\u00b7be\u00b7sah\u00b7nung", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und doch in deines Kindes Locken", "tokens": ["Und", "doch", "in", "dei\u00b7nes", "Kin\u00b7des", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Dr\u00fcckst wonnig du den Myrtenkranz \u2013", "tokens": ["Dr\u00fcckst", "won\u00b7nig", "du", "den", "Myr\u00b7ten\u00b7kranz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Geliebte, Mutter, Kind zugleich", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "Mut\u00b7ter", ",", "Kind", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Bist du unendlich froh und reich.", "tokens": ["Bist", "du", "un\u00b7end\u00b7lich", "froh", "und", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und webt der Traum auch immer nicht", "tokens": ["Und", "webt", "der", "Traum", "auch", "im\u00b7mer", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solch unergr\u00fcndlich s\u00fc\u00df Gedicht,", "tokens": ["Solch", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "s\u00fc\u00df", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wei\u00df er doch mit Elfenh\u00e4nden", "tokens": ["So", "wei\u00df", "er", "doch", "mit", "El\u00b7fen\u00b7h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Willkommne Gabe stets zu spenden:", "tokens": ["Will\u00b7komm\u00b7ne", "Ga\u00b7be", "stets", "zu", "spen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Winters Schnee und rauher Luft", "tokens": ["In", "Win\u00b7ters", "Schnee", "und", "rau\u00b7her", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umspielt er dich mit Veilchenduft;", "tokens": ["Um\u00b7spielt", "er", "dich", "mit", "Veil\u00b7chen\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er weht dir in des Sommers Schw\u00fcle", "tokens": ["Er", "weht", "dir", "in", "des", "Som\u00b7mers", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ums Haupt mit gr\u00fcner Waldesk\u00fchle;", "tokens": ["Ums", "Haupt", "mit", "gr\u00fc\u00b7ner", "Wal\u00b7des\u00b7k\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Lieben bringt er dir ins Haus,", "tokens": ["Die", "Lie\u00b7ben", "bringt", "er", "dir", "ins", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von denen dich die Welt geschieden;", "tokens": ["Von", "de\u00b7nen", "dich", "die", "Welt", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Erquickung gie\u00dft er, gie\u00dfet Frieden", "tokens": ["Er\u00b7quic\u00b7kung", "gie\u00dft", "er", ",", "gie\u00b7\u00dfet", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Auf deine Wimpern l\u00e4chelnd aus,", "tokens": ["Auf", "dei\u00b7ne", "Wim\u00b7pern", "l\u00e4\u00b7chelnd", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und will die Brust die Sorge pressen,", "tokens": ["Und", "will", "die", "Brust", "die", "Sor\u00b7ge", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Er schafft ein wundervoll Vergessen.", "tokens": ["Er", "schafft", "ein", "wun\u00b7der\u00b7voll", "Ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das ist's, was ich in mir gedacht,", "tokens": ["Das", "ist's", ",", "was", "ich", "in", "mir", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als ich das Werk f\u00fcr dich vollbracht;", "tokens": ["Als", "ich", "das", "Werk", "f\u00fcr", "dich", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wirst du, holde Schl\u00e4ferin,", "tokens": ["Und", "wirst", "du", ",", "hol\u00b7de", "Schl\u00e4\u00b7fe\u00b7rin", ","], "token_info