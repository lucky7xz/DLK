{"dta.poem.20692": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die K\u00f6nigl. Hof-Bauadministration  \n  wegen  \n ein paar geschenkter eiserner  \n Spahr\u00f6fen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzeihung von der K\u00f6niglichen                 ", "tokens": ["Ver\u00b7zei\u00b7hung", "von", "der", "K\u00f6\u00b7nig\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Administration, bitt\u2019 ich,", "tokens": ["Ad\u00b7mi\u00b7nist\u00b7ra\u00b7ti\u00b7on", ",", "bitt'", "ich", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Weil mancher Tag schon fortgewichen,", "tokens": ["Weil", "man\u00b7cher", "Tag", "schon", "fort\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch des Winters L\u00e4nge sich", "tokens": ["Und", "auch", "des", "Win\u00b7ters", "L\u00e4n\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So nach und nach hinweg geschlichen,", "tokens": ["So", "nach", "und", "nach", "hin\u00b7weg", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh die dankbare Karschin sich", "tokens": ["Eh", "die", "dank\u00b7ba\u00b7re", "Kar\u00b7schin", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Mit gro\u00dfem Dank hat abgefunden", "tokens": ["Mit", "gro\u00b7\u00dfem", "Dank", "hat", "ab\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr ein paar Oefchen, ihr geschenkt.", "tokens": ["F\u00fcr", "ein", "paar", "O\u00b7e\u00b7fchen", ",", "ihr", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sie z\u00e4hlte gar viel kranke Stunden,", "tokens": ["Sie", "z\u00e4hl\u00b7te", "gar", "viel", "kran\u00b7ke", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "War halb schon aus der Welt gelenkt", "tokens": ["War", "halb", "schon", "aus", "der", "Welt", "ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In andre nicht bekannte Welten,", "tokens": ["In", "and\u00b7re", "nicht", "be\u00b7kann\u00b7te", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wo man Bestrafung und Vergelten", "tokens": ["Wo", "man", "Be\u00b7stra\u00b7fung", "und", "Ver\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "F\u00fcr gut\u2019 und b\u00f6se That empf\u00e4ngt \u2014", "tokens": ["F\u00fcr", "gut'", "und", "b\u00f6\u00b7se", "That", "emp\u00b7f\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "W\u00e4r nicht mein Geist von seltner St\u00e4rke,", "tokens": ["W\u00e4r", "nicht", "mein", "Geist", "von", "selt\u00b7ner", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Er w\u00e4re l\u00e4ngst hinweggedr\u00e4ngt,", "tokens": ["Er", "w\u00e4\u00b7re", "l\u00e4ngst", "hin\u00b7weg\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Denn schwach sind nur die Au\u00dfenwerke,", "tokens": ["Denn", "schwach", "sind", "nur", "die", "Au\u00b7\u00dfen\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie werden wahrlich keinen Schmaus", "tokens": ["Sie", "wer\u00b7den", "wahr\u00b7lich", "kei\u00b7nen", "Schmaus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr irgend einen Grabwurm geben,", "tokens": ["F\u00fcr", "ir\u00b7gend", "ei\u00b7nen", "Grab\u00b7wurm", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Man tr\u00e4gt nur Haut und Bein ins finstre Leichenhaus.", "tokens": ["Man", "tr\u00e4gt", "nur", "Haut", "und", "Bein", "ins", "finst\u00b7re", "Lei\u00b7chen\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich denk es ohne grauses Beben.", "tokens": ["Ich", "denk", "es", "oh\u00b7ne", "grau\u00b7ses", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Warum sollt ich betr\u00fcbt ein Achgeschrey erheben", "tokens": ["Wa\u00b7rum", "sollt", "ich", "be\u00b7tr\u00fcbt", "ein", "Ach\u00b7ge\u00b7schrey", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Beim Anblick meines Bleichgesichts?", "tokens": ["Beim", "An\u00b7blick", "mei\u00b7nes", "Bleich\u00b7ge\u00b7sichts", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kalt ist das Grab, davon empfindet nichts", "tokens": ["Kalt", "ist", "das", "Grab", ",", "da\u00b7von", "emp\u00b7fin\u00b7det", "nichts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PAV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Wesen, welches in mir denket,", "tokens": ["Das", "We\u00b7sen", ",", "wel\u00b7ches", "in", "mir", "den\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Feuer widersprichts \u2014", "tokens": ["Sein", "Feu\u00b7er", "wi\u00b7der\u00b7sprichts"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und da\u00df sich\u2019s dermaleinst an Lethens Ufer tr\u00e4nket;", "tokens": ["Und", "da\u00df", "sich's", "der\u00b7ma\u00b7leinst", "an", "Le\u00b7thens", "U\u00b7fer", "tr\u00e4n\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die\u00df glaube wer da mag und kann,", "tokens": ["Die\u00df", "glau\u00b7be", "wer", "da", "mag", "und", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWS", "ADV", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich nehme diesen Wahn nicht an,", "tokens": ["Ich", "neh\u00b7me", "die\u00b7sen", "Wahn", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil ich durchaus nicht will vergessen,", "tokens": ["Weil", "ich", "durc\u00b7haus", "nicht", "will", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was mir hienieden Guts geschehn;", "tokens": ["Was", "mir", "hien\u00b7ie\u00b7den", "Guts", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil ich auch dort noch will ermessen,", "tokens": ["Weil", "ich", "auch", "dort", "noch", "will", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Welch Auge mich hier gern gesehn,", "tokens": ["Welch", "Au\u00b7ge", "mich", "hier", "gern", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und welche Hand mirs leichter machte", "tokens": ["Und", "wel\u00b7che", "Hand", "mirs", "leich\u00b7ter", "mach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "NE", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Zu wallen auf dem Lebenspfad,", "tokens": ["Zu", "wal\u00b7len", "auf", "dem", "Le\u00b7ben\u00b7spfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wo oft mein Fu\u00df auf Dornen trat \u2014", "tokens": ["Wo", "oft", "mein", "Fu\u00df", "auf", "Dor\u00b7nen", "trat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Selbst da der beste K\u00f6nig dachte,", "tokens": ["Selbst", "da", "der", "bes\u00b7te", "K\u00f6\u00b7nig", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df meine Laufbahn bis ans Ziel,", "tokens": ["Da\u00df", "mei\u00b7ne", "Lauf\u00b7bahn", "bis", "ans", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nun Rosen ohne Dornen brachte,", "tokens": ["Nun", "Ro\u00b7sen", "oh\u00b7ne", "Dor\u00b7nen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Denn so befahls Sein K\u00f6niglich Gef\u00fchl \u2014", "tokens": ["Denn", "so", "be\u00b7fahls", "Sein", "K\u00f6\u00b7nig\u00b7lich", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wie der Befehl ward ausgerichtet,", "tokens": ["Wie", "der", "Be\u00b7fehl", "ward", "aus\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ist Jedermann zur Augenschau,", "tokens": ["Ist", "Je\u00b7der\u00b7mann", "zur", "Au\u00b7gen\u00b7schau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Der auf der neuen Br\u00fccke Bau", "tokens": ["Der", "auf", "der", "neu\u00b7en", "Br\u00fc\u00b7cke", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Sphinx betrachtet, die erdichtet", "tokens": ["Die", "Sphinx", "be\u00b7trach\u00b7tet", ",", "die", "er\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Von gro\u00dfen Fabeldichtern ward.", "tokens": ["Von", "gro\u00b7\u00dfen", "Fa\u00b7bel\u00b7dich\u00b7tern", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Er darf nur rechter Hand sich drehen,", "tokens": ["Er", "darf", "nur", "rech\u00b7ter", "Hand", "sich", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da wird er mit Verwundrungsart", "tokens": ["Da", "wird", "er", "mit", "Ver\u00b7wund\u00b7rungs\u00b7art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Das Eckchen meines Hauses sehen \u2014", "tokens": ["Das", "Eck\u00b7chen", "mei\u00b7nes", "Hau\u00b7ses", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Das ein recht hochgewachsner Mann,", "tokens": ["Das", "ein", "recht", "hoch\u00b7ge\u00b7wachs\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie weiland Potsdams Gardem\u00e4nner,", "tokens": ["Wie", "wei\u00b7land", "Pots\u00b7dams", "Gar\u00b7de\u00b7m\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Mit seinem Arm umspannen kann.", "tokens": ["Mit", "sei\u00b7nem", "Arm", "um\u00b7span\u00b7nen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Indessen lad\u2019 ich meine G\u00f6nner", "tokens": ["In\u00b7des\u00b7sen", "lad'", "ich", "mei\u00b7ne", "G\u00f6n\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und G\u00f6nnerinnen freundlich ein,", "tokens": ["Und", "G\u00f6n\u00b7ne\u00b7rin\u00b7nen", "freund\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Nicht auf sechs Sch\u00fcsseln, nicht auf Wein:", "tokens": ["Nicht", "auf", "sechs", "Sch\u00fcs\u00b7seln", ",", "nicht", "auf", "Wein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "CARD", "NN", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Nein, meine Wohnung nur zu schauen,", "tokens": ["Nein", ",", "mei\u00b7ne", "Woh\u00b7nung", "nur", "zu", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Lad ich Sie ein,", "tokens": ["Lad", "ich", "Sie", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Und kann\u2019s Euch schw\u00f6ren mit Vertrauen,", "tokens": ["Und", "kann's", "Euch", "schw\u00f6\u00b7ren", "mit", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Da\u00df ihre Niedlichkeit Sie reizt,", "tokens": ["Da\u00df", "ih\u00b7re", "Nied\u00b7lich\u00b7keit", "Sie", "reizt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "So wahr mit wenig Glut das Eisen\u00f6fchen heizt.", "tokens": ["So", "wahr", "mit", "we\u00b7nig", "Glut", "das", "Ei\u00b7se\u00b7n\u00f6f\u00b7chen", "heizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}