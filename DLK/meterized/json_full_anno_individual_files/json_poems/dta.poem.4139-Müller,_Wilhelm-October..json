{"dta.poem.4139": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "October.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem k\u00fchlen, klaren Mondenstrahl,", "tokens": ["Auf", "ei\u00b7nem", "k\u00fch\u00b7len", ",", "kla\u00b7ren", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit einem vollen, sch\u00e4umenden Pokal,", "tokens": ["Mit", "ei\u00b7nem", "vol\u00b7len", ",", "sch\u00e4u\u00b7men\u00b7den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die hei\u00dfe Stirn umweht von frischen Reben.", "tokens": ["Die", "hei\u00b7\u00dfe", "Stirn", "um\u00b7weht", "von", "fri\u00b7schen", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es wogt ein unergr\u00fcndlich tiefes Leben", "tokens": ["Es", "wogt", "ein", "un\u00b7er\u00b7gr\u00fcnd\u00b7lich", "tie\u00b7fes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meiner Beere g\u00fcldenem Krystall:", "tokens": ["In", "mei\u00b7ner", "Bee\u00b7re", "g\u00fcl\u00b7de\u00b7nem", "Krys\u00b7tall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du's entfesseln, la\u00df in hellem Schall", "tokens": ["Willst", "du's", "ent\u00b7fes\u00b7seln", ",", "la\u00df", "in", "hel\u00b7lem", "Schall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "VVIMP", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zwei Bruderbecher an einander beben.", "tokens": ["Zwei", "Bru\u00b7der\u00b7be\u00b7cher", "an", "ein\u00b7an\u00b7der", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und unterth\u00e4nig diesem Zauberklange,", "tokens": ["Und", "un\u00b7ter\u00b7th\u00e4\u00b7nig", "die\u00b7sem", "Zau\u00b7ber\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwingt flugs ein unz\u00e4hlbares Elfenchor", "tokens": ["Schwingt", "flugs", "ein", "un\u00b7z\u00e4hl\u00b7ba\u00b7res", "El\u00b7fen\u00b7chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus Silberperlen sprudelnd sich empor.", "tokens": ["Aus", "Sil\u00b7ber\u00b7per\u00b7len", "spru\u00b7delnd", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Rand umh\u00fcpfen sie in buntem Drange,", "tokens": ["Den", "Rand", "um\u00b7h\u00fcp\u00b7fen", "sie", "in", "bun\u00b7tem", "Dran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Spie\u00df und Degen, Saitenspiel und Kranz,", "tokens": ["Mit", "Spie\u00df", "und", "De\u00b7gen", ",", "Sai\u00b7ten\u00b7spiel", "und", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bockshorn und Eulenohr und Drachenschwanz.", "tokens": ["Bocks\u00b7horn", "und", "Eu\u00b7le\u00b7nohr", "und", "Dra\u00b7chen\u00b7schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}