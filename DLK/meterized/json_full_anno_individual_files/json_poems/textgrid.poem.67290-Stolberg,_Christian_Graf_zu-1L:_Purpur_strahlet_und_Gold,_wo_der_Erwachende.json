pos": ["ART", "APPR", "NN", "VVFIN", "ADJA", "$,", "ADJA"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wellen \u2013 so in des Schwertes", "tokens": ["Wel\u00b7len", "\u2013", "so", "in", "des", "Schwer\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Purpurtriefendem Siegesschmuck", "tokens": ["Pur\u00b7pur\u00b7trie\u00b7fen\u00b7dem", "Sie\u00b7ges\u00b7schmuck"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Gl\u00fchte flammend die Schlacht! Segnend wie Sonnenlicht", "tokens": ["Gl\u00fch\u00b7te", "flam\u00b7mend", "die", "Schlacht", "!", "Seg\u00b7nend", "wie", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "ART", "NN", "$.", "ADJD", "KOKOM", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Warst du, bleibst du; doch ha! warst ein vertilgendes", "tokens": ["Warst", "du", ",", "bleibst", "du", ";", "doch", "ha", "!", "warst", "ein", "ver\u00b7til\u00b7gen\u00b7des"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "PPER", "$.", "ADV", "NE", "$.", "VAFIN", "ART", "ADJA"], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Sturmgewitter: zertr\u00fcmmert", "tokens": ["Sturm\u00b7ge\u00b7wit\u00b7ter", ":", "zer\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Liegt in Scherben der Thon-Kolo\u00df.", "tokens": ["Liegt", "in", "Scher\u00b7ben", "der", "Thon\u00b7Ko\u00b7lo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Pfade bahnetest du hin zu dem Drachennest", "tokens": ["Pfa\u00b7de", "bah\u00b7ne\u00b7test", "du", "hin", "zu", "dem", "Dra\u00b7chen\u00b7nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Unserm Heer, das, Triumph! treibend die fliehenden", "tokens": ["Un\u00b7serm", "Heer", ",", "das", ",", "Tri\u00b7umph", "!", "trei\u00b7bend", "die", "flie\u00b7hen\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PDS", "$,", "NN", "$.", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Horden, pfl\u00fcckend des Lorbeers", "tokens": ["Hor\u00b7den", ",", "pfl\u00fc\u00b7ckend", "des", "Lor\u00b7beers"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Saat, von Siegen zu Siegen flog.", "tokens": ["Saat", ",", "von", "Sie\u00b7gen", "zu", "Sie\u00b7gen", "flog", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Schonung waltete da \u2013 jenes erw\u00e4rmeten", "tokens": ["Scho\u00b7nung", "wal\u00b7te\u00b7te", "da", "\u2013", "je\u00b7nes", "er\u00b7w\u00e4r\u00b7me\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$(", "PDS", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Busen-Gastes gedenkt! \u2013 winkte das Schwert zur\u00fcck,", "tokens": ["Bu\u00b7sen\u00b7Gas\u00b7tes", "ge\u00b7denkt", "!", "\u2013", "wink\u00b7te", "das", "Schwert", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$.", "$(", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das schon streckt' in die Wagschaal", "tokens": ["Das", "schon", "streckt'", "in", "die", "Wag\u00b7schaal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bl\u00fccher: \u00bbWeh den Besiegeten!\u00ab", "tokens": ["Bl\u00fc\u00b7cher", ":", "\u00bb", "Weh", "den", "Be\u00b7sie\u00b7ge\u00b7ten", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "$(", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo, Heroen des Schwarms, war't ihr, vergoldete", "tokens": ["Wo", ",", "He\u00b7roen", "des", "Schwarms", ",", "wa\u00b7r't", "ihr", ",", "ver\u00b7gol\u00b7de\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Spie\u00dfgesellen? Ihr fielt, wie vom ger\u00fcttelten", "tokens": ["Spie\u00df\u00b7ge\u00b7sel\u00b7len", "?", "Ihr", "fielt", ",", "wie", "vom", "ge\u00b7r\u00fct\u00b7tel\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "APPRART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Dornstrauch regnen die K\u00e4fer,", "tokens": ["Dorn\u00b7strauch", "reg\u00b7nen", "die", "K\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So vom schwindenden G\u00f6tzen ab.", "tokens": ["So", "vom", "schwin\u00b7den\u00b7den", "G\u00f6t\u00b7zen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Fodr' in tobender Wuth nun die zerst\u00e4ubeten", "tokens": ["Fodr'", "in", "to\u00b7ben\u00b7der", "Wuth", "nun", "die", "zer\u00b7st\u00e4u\u00b7be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Legionen zur\u00fcck, gegen des Inselchens", "tokens": ["Le\u00b7gi\u00b7o\u00b7nen", "zu\u00b7r\u00fcck", ",", "ge\u00b7gen", "des", "In\u00b7sel\u00b7chens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Felsen schmetternd die Kronen-", "tokens": ["Fel\u00b7sen", "schmet\u00b7ternd", "die", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "TRUNC"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Scheitel, Elba's Augustulus!", "tokens": ["Schei\u00b7tel", ",", "El\u00b7ba's", "Au\u00b7gus\u00b7tu\u00b7lus", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Purpur strahlet und Gold, wo der Erwachende", "tokens": ["Pur\u00b7pur", "strah\u00b7let", "und", "Gold", ",", "wo", "der", "Er\u00b7wa\u00b7chen\u00b7de"], "token