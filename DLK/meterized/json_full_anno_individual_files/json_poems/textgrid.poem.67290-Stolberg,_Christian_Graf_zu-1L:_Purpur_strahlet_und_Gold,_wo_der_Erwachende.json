{"textgrid.poem.67290": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Purpur strahlet und Gold, wo der Erwachende", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Purpur strahlet und Gold, wo der Erwachende", "tokens": ["Pur\u00b7pur", "strah\u00b7let", "und", "Gold", ",", "wo", "der", "Er\u00b7wa\u00b7chen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Scheucht die D\u00e4mmerung, schwebt trunken in Wonn' empor;", "tokens": ["Scheucht", "die", "D\u00e4m\u00b7me\u00b7rung", ",", "schwebt", "trun\u00b7ken", "in", "Wonn'", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Heil dir, du mit dem Sieger-", "tokens": ["Heil", "dir", ",", "du", "mit", "dem", "Sie\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kranze, herbstlicher G\u00f6ttersohn!", "tokens": ["Kran\u00b7ze", ",", "herbst\u00b7li\u00b7cher", "G\u00f6t\u00b7ter\u00b7sohn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Jahrstag Leipzigs, begr\u00fc\u00dft sei du mit Musensang,", "tokens": ["Jahr\u00b7stag", "Leip\u00b7zigs", ",", "be\u00b7gr\u00fc\u00dft", "sei", "du", "mit", "Mu\u00b7sen\u00b7sang", ","], "