{"dta.poem.2913": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Seele Saiten, wann sie dir am feinsten sind", "tokens": ["Der", "See\u00b7le", "Sai\u00b7ten", ",", "wann", "sie", "dir", "am", "feins\u00b7ten", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "PPER", "PPER", "APPRART", "ADJA", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestimmt, o h\u00fcte sie vorm allerkleinsten Wind!", "tokens": ["Ge\u00b7stimmt", ",", "o", "h\u00fc\u00b7te", "sie", "vorm", "al\u00b7ler\u00b7kleins\u00b7ten", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "FM", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn auch ein solcher kann verstimmen dann die Saiten,", "tokens": ["Denn", "auch", "ein", "sol\u00b7cher", "kann", "ver\u00b7stim\u00b7men", "dann", "die", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PIS", "VMFIN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ohne Eindruck sonst dar\u00fcber w\u00fcrde gleiten.", "tokens": ["Der", "oh\u00b7ne", "Ein\u00b7druck", "sonst", "da\u00b7r\u00fc\u00b7ber", "w\u00fcr\u00b7de", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "PAV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn der Begeisterung Erwachen schauernd sp\u00fcrt", "tokens": ["Wenn", "der", "Be\u00b7geis\u00b7te\u00b7rung", "Er\u00b7wa\u00b7chen", "schau\u00b7ernd", "sp\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ADJD", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der Geist, f\u00fchlt unsanft er von Ird'schem sich ber\u00fchrt;", "tokens": ["Der", "Geist", ",", "f\u00fchlt", "un\u00b7sanft", "er", "von", "Ird'\u00b7schem", "sich", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "PPER", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So da\u00df der Andacht Glut oft, nebenaus vom Zug", "tokens": ["So", "da\u00df", "der", "An\u00b7dacht", "Glut", "oft", ",", "ne\u00b7be\u00b7naus", "vom", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "ADV", "$,", "APPR", "APPRART", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der Luft gewendet, wild in Zornesflamm' ausschlug.", "tokens": ["Der", "Luft", "ge\u00b7wen\u00b7det", ",", "wild", "in", "Zor\u00b7nes\u00b7flamm'", "aus\u00b7schlug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht nur dem Altar ist sein Opfer dann entzogen,", "tokens": ["Nicht", "nur", "dem", "Al\u00b7tar", "ist", "sein", "Op\u00b7fer", "dann", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du selber f\u00fchlest um die Stille dich betrogen.", "tokens": ["Du", "sel\u00b7ber", "f\u00fch\u00b7lest", "um", "die", "Stil\u00b7le", "dich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}