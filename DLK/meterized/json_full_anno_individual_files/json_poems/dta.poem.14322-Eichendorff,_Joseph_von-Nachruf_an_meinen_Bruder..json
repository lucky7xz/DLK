{"dta.poem.14322": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, da\u00df auch wir schliefen!", "tokens": ["Ach", ",", "da\u00df", "auch", "wir", "schlie\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die bl\u00fchenden Tiefen,", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Str\u00f6me, die Auen", "tokens": ["Die", "Str\u00f6\u00b7me", ",", "die", "Au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So heimlich aufschauen,", "tokens": ["So", "heim\u00b7lich", "auf\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Als ob sie all' riefen:", "tokens": ["Als", "ob", "sie", "all'", "rie\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "\u201edein Bruder ist todt!", "tokens": ["\u201e", "dein", "Bru\u00b7der", "ist", "todt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Unter Rosen roth", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "roth"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ach, da\u00df wir auch schliefen!\u201c", "tokens": ["Ach", ",", "da\u00df", "wir", "auch", "schlie\u00b7fen", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}