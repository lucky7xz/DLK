{"textgrid.poem.32699": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stille Nacht, o sei gegr\u00fc\u00dfet!", "tokens": ["Stil\u00b7le", "Nacht", ",", "o", "sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "FM", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verr\u00e4tst die Seufzer nicht,", "tokens": ["Du", "ver\u00b7r\u00e4tst", "die", "Seuf\u00b7zer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die stumme Z\u00e4hre flie\u00dfet", "tokens": ["Und", "die", "stum\u00b7me", "Z\u00e4h\u00b7re", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unverhohlner vom Gesicht,", "tokens": ["Un\u00b7ver\u00b7hohl\u00b7ner", "vom", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier will ich, im Mondenscheine,", "tokens": ["Hier", "will", "ich", ",", "im", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf den werten Auen gehn,", "tokens": ["Auf", "den", "wer\u00b7ten", "Au\u00b7en", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo ich ihn so oft alleine", "tokens": ["Wo", "ich", "ihn", "so", "oft", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blumenkr\u00e4nze winden sehn.", "tokens": ["Blu\u00b7men\u00b7kr\u00e4n\u00b7ze", "win\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alle Blumen will ich pfl\u00fccken,", "tokens": ["Al\u00b7le", "Blu\u00b7men", "will", "ich", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er mir zur\u00fccke lie\u00df;", "tokens": ["Die", "er", "mir", "zu\u00b7r\u00fc\u00b7cke", "lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will damit den Busen schm\u00fccken,", "tokens": ["Will", "da\u00b7mit", "den", "Bu\u00b7sen", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den sein Auge seufzen hie\u00df.", "tokens": ["Den", "sein", "Au\u00b7ge", "seuf\u00b7zen", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! an seinem Busen bl\u00fchtet", "tokens": ["Ach", "!", "an", "se