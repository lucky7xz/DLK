{"textgrid.poem.62854": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Kroton starb ein armer Greis.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Kroton starb ein armer Greis.", "tokens": ["In", "Kro\u00b7ton", "starb", "ein", "ar\u00b7mer", "Greis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Freund, ein alter Pudel, harrte", "tokens": ["Sein", "Freund", ",", "ein", "al\u00b7ter", "Pu\u00b7del", ",", "harr\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fest bey ihm aus: den Todesschwei\u00df", "tokens": ["Fest", "bey", "ihm", "aus", ":", "den", "To\u00b7des\u00b7schwei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Leckt er ihm vom Gesicht. Man scharrte", "tokens": ["Leckt", "er", "ihm", "vom", "Ge\u00b7sicht", ".", "Man", "scharr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "PIS", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Den Leichnam ein. Der fromme Hund", "tokens": ["Den", "Leich\u00b7nam", "ein", ".", "Der", "from\u00b7me", "Hund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line