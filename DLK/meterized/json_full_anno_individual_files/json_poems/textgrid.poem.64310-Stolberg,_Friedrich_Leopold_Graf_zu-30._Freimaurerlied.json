{"textgrid.poem.64310": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "30. Freimaurerlied", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wackre Br\u00fcder, stimmet an!", "tokens": ["Wack\u00b7re", "Br\u00fc\u00b7der", ",", "stim\u00b7met", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf! begr\u00fc\u00dft den braven Mann,", "tokens": ["Auf", "!", "be\u00b7gr\u00fc\u00dft", "den", "bra\u00b7ven", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der in unsern freien Orden", "tokens": ["Der", "in", "un\u00b7sern", "frei\u00b7en", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eben aufgenommen worden;", "tokens": ["E\u00b7ben", "auf\u00b7ge\u00b7nom\u00b7men", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nicht wei\u00df, wie ihm geschah,", "tokens": ["Der", "nicht", "wei\u00df", ",", "wie", "ihm", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ob der Wunder, die er sah!", "tokens": ["Ob", "der", "Wun\u00b7der", ",", "die", "er", "sah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lieber Bruder, freue dich!", "tokens": ["Lie\u00b7ber", "Bru\u00b7der", ",", "freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir auch freun uns inniglich!", "tokens": ["Wir", "auch", "freun", "uns", "in\u00b7nig\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So du als ein Maurer handelst,", "tokens": ["So", "du", "als", "ein", "Mau\u00b7rer", "han\u00b7delst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Auf der Weisheit Pfade wandelst;", "tokens": ["Auf", "der", "Weis\u00b7heit", "Pfa\u00b7de", "wan\u00b7delst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fcllet, mit der Zeiten Lauf,", "tokens": ["H\u00fcl\u00b7let", ",", "mit", "der", "Zei\u00b7ten", "Lauf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Neue Wahrheit dir sich auf.", "tokens": ["Neu\u00b7e", "Wahr\u00b7heit", "dir", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Senke, Bruder, nicht den Blick", "tokens": ["Sen\u00b7ke", ",", "Bru\u00b7der", ",", "nicht", "den", "Blick"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Finsternis zur\u00fcck!", "tokens": ["In", "die", "Fins\u00b7ter\u00b7nis", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Forsche tiefer in die Wahrheit!", "tokens": ["For\u00b7sche", "tie\u00b7fer", "in", "die", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der D\u00e4mmrung geh zur Klarheit!", "tokens": ["Von", "der", "D\u00e4mm\u00b7rung", "geh", "zur", "Klar\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandle sicher! strauchle nicht!", "tokens": ["Wand\u00b7le", "si\u00b7cher", "!", "strauch\u00b7le", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis du fleugst von Licht zu Licht!", "tokens": ["Bis", "du", "fleugst", "von", "Licht", "zu", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei getrost, und achte nicht,", "tokens": ["Sei", "ge\u00b7trost", ",", "und", "ach\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KON", "ADJA", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Thor und Heuchler spricht,", "tokens": ["Was", "der", "Thor", "und", "Heuch\u00b7ler", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, die uns im Finstern richten,", "tokens": ["Sie", ",", "die", "uns", "im", "Fins\u00b7tern", "rich\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "L\u00fcgen an die Wahrheit dichten!", "tokens": ["L\u00fc\u00b7gen", "an", "die", "Wahr\u00b7heit", "dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was gehn einen braven Mann", "tokens": ["Was", "gehn", "ei\u00b7nen", "bra\u00b7ven", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Alle Splitterrichter an?", "tokens": ["Al\u00b7le", "Split\u00b7ter\u00b7rich\u00b7ter", "an", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6re, was die Weisheit spricht:", "tokens": ["H\u00f6\u00b7re", ",", "was", "die", "Weis\u00b7heit", "spricht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbthue Recht, und zittre nicht!\u00ab", "tokens": ["\u00bb", "thue", "Recht", ",", "und", "zitt\u00b7re", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "NN", "$,", "KON", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ihm tausend Feinde dr\u00e4uen,", "tokens": ["Ob", "ihm", "tau\u00b7send", "Fein\u00b7de", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Redliche nichts scheuen;", "tokens": ["Wird", "der", "Red\u00b7li\u00b7che", "nichts", "scheu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weichet weder links noch rechts,", "tokens": ["Wei\u00b7chet", "we\u00b7der", "links", "noch", "rechts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlt sich g\u00f6ttliches Geschlechts!", "tokens": ["F\u00fchlt", "sich", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7schlechts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bruder, gieb uns deine Hand", "tokens": ["Bru\u00b7der", ",", "gieb", "uns", "dei\u00b7ne", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Freundschaft Unterpfand!", "tokens": ["Uns\u00b7rer", "Freund\u00b7schaft", "Un\u00b7ter\u00b7pfand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser B\u00fcndnis zu erneuen,", "tokens": ["Un\u00b7ser", "B\u00fcnd\u00b7nis", "zu", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll sich unser Bruder freuen!", "tokens": ["Soll", "sich", "un\u00b7ser", "Bru\u00b7der", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Maurer, schenkt die Gl\u00e4ser voll!", "tokens": ["Mau\u00b7rer", ",", "schenkt", "die", "Gl\u00e4\u00b7ser", "voll", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Trinkt auf unsers Bruders Wohl!", "tokens": ["Trinkt", "auf", "un\u00b7sers", "Bru\u00b7ders", "Wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wackre Br\u00fcder, stimmet an!", "tokens": ["Wack\u00b7re", "Br\u00fc\u00b7der", ",", "stim\u00b7met", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf! begr\u00fc\u00dft den braven Mann,", "tokens": ["Auf", "!", "be\u00b7gr\u00fc\u00dft", "den", "bra\u00b7ven", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART"