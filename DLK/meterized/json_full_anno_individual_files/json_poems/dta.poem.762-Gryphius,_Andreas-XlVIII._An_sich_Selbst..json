{"dta.poem.762": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.  An sich Selbst.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr grawet vor mir selbst/ mir zittern alle glieder ", "tokens": ["Mjr", "gra\u00b7wet", "vor", "mir", "selbst", "/", "mir", "zit\u00b7tern", "al\u00b7le", "glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ADV", "$(", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ich die Lipp\u2019 vnd Na\u00df\u2019 vnd beyder Augen kluft/", "tokens": ["Wenn", "ich", "die", "Lipp'", "vnd", "Na\u00df'", "vnd", "bey\u00b7der", "Au\u00b7gen", "kluft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die blindt vom wachen sindt/ des Athems schwere ", "tokens": ["Die", "blindt", "vom", "wa\u00b7chen", "sindt", "/", "des", "A\u00b7thems", "schwe\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "VVINF", "VAFIN", "$(", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Luft", "tokens": ["Luft"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Betracht/ vnd die nun schon erstorbnen Augen-lieder:", "tokens": ["Be\u00b7tracht", "/", "vnd", "die", "nun", "schon", "ers\u00b7torb\u00b7nen", "Au\u00b7gen\u00b7lie\u00b7der", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die zunge/ schwartz vom brandt felt mit den worten nieder/", "tokens": ["Die", "zun\u00b7ge", "/", "schwartz", "vom", "brandt", "felt", "mit", "den", "wor\u00b7ten", "nie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "APPRART", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd lalt ich weis nicht was; die m\u00fcde Seele ruft/", "tokens": ["Vnd", "lalt", "ich", "weis", "nicht", "was", ";", "die", "m\u00fc\u00b7de", "See\u00b7le", "ruft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "PTKNEG", "PWS", "$.", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem grossen Tr\u00f6ster zu/ das Fleisch reucht nach der gruft/", "tokens": ["Dem", "gros\u00b7sen", "Tr\u00f6s\u00b7ter", "zu", "/", "das", "Fleisch", "reucht", "nach", "der", "gruft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$(", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die \u00e4rtzte lassen mich/ die schmertzen kommen wieder/", "tokens": ["Die", "\u00e4rtz\u00b7te", "las\u00b7sen", "mich", "/", "die", "schmert\u00b7zen", "kom\u00b7men", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$(", "ART", "ADJA", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein C\u00f6rper ist nicht mehr als adern/ fell/ vnd bein.", "tokens": ["Mein", "C\u00f6r\u00b7per", "ist", "nicht", "mehr", "als", "a\u00b7dern", "/", "fell", "/", "vnd", "bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "KOKOM", "VVINF", "$(", "ADJD", "$(", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das sitzen ist mein todt/ das liegen meine pein.", "tokens": ["Das", "sit\u00b7zen", "ist", "mein", "todt", "/", "das", "lie\u00b7gen", "mei\u00b7ne", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJD", "$(", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die schenckel haben selbst nun tr\u00e4ger wol von n\u00f6then! ", "tokens": ["Die", "schen\u00b7ckel", "ha\u00b7ben", "selbst", "nun", "tr\u00e4\u00b7ger", "wol", "von", "n\u00f6\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "ADV", "ADJD", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was ist der hohe ruhm/ vnd jugendt/ ehr vnd kunst?", "tokens": ["Was", "ist", "der", "ho\u00b7he", "ruhm", "/", "vnd", "ju\u00b7gendt", "/", "ehr", "vnd", "kunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "NN", "$(", "NN", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn diese stunde kompt; wird alles rauch vnd dunst.", "tokens": ["Wenn", "die\u00b7se", "stun\u00b7de", "kompt", ";", "wird", "al\u00b7les", "rauch", "vnd", "dunst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "VVFIN", "$.", "VAFIN", "PIS", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd eine noth mu\u00df vns mit allem vorsatz t\u00f6dten.", "tokens": ["Vnd", "ei\u00b7ne", "noth", "mu\u00df", "vns", "mit", "al\u00b7lem", "vor\u00b7satz", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "APPR", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}