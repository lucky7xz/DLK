{"textgrid.poem.55168": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndliches Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid, o Geister des Hains, o seid, ihr Nymphen des Flusses,", "tokens": ["Seid", ",", "o", "Geis\u00b7ter", "des", "Hains", ",", "o", "seid", ",", "ihr", "Nym\u00b7phen", "des", "Flus\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "$,", "FM", "NN", "ART", "NN", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eurer Entfernten gedenk, eueren Nahen zur Lust!", "tokens": ["Eu\u00b7rer", "Ent\u00b7fern\u00b7ten", "ge\u00b7denk", ",", "eu\u00b7e\u00b7ren", "Na\u00b7hen", "zur", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Weihend feierten sie im stillen die l\u00e4ndlichen Feste;", "tokens": ["Wei\u00b7hend", "fei\u00b7er\u00b7ten", "sie", "im", "stil\u00b7len", "die", "l\u00e4nd\u00b7li\u00b7chen", "Fes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wir, dem gebahnten Pfad folgend, beschleichen das Gl\u00fcck.", "tokens": ["Wir", ",", "dem", "ge\u00b7bahn\u00b7ten", "Pfad", "fol\u00b7gend", ",", "be\u00b7schlei\u00b7chen", "das", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Amor wohne mit uns, es macht der himmlische Knabe", "tokens": ["A\u00b7mor", "woh\u00b7ne", "mit", "uns", ",", "es", "macht", "der", "himm\u00b7li\u00b7sche", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Gegenw\u00e4rtige lieb und die Entfernten euch nah.", "tokens": ["Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ge", "lieb", "und", "die", "Ent\u00b7fern\u00b7ten", "euch", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Seid, o Geister des Hains, o seid, ihr Nymphen des Flusses,", "tokens": ["Seid", ",", "o", "Geis\u00b7ter", "des", "Hains", ",", "o", "seid", ",", "ihr", "Nym\u00b7phen", "des", "Flus\u00b7ses", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "$,", "FM", "NN", "ART", "NN", "$,", "FM", "FM", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eurer Entfernten gedenk, eueren Nahen zur Lust!", "tokens": ["Eu\u00b7rer", "Ent\u00b7fern\u00b7ten", "ge\u00b7denk", ",", "eu\u00b7e\u00b7ren", "Na\u00b7hen", "zur", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}