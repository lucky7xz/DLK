{"dta.poem.20604": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Verg\u00e4nglichkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Je, \u2019s isch nit anderst, lueg mi a, wie", "tokens": ["Je", ",", "'s", "isch", "nit", "an\u00b7derst", ",", "lu\u00b7eg", "mi", "a", ",", "wie"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PPER", "ADJD", "PTKNEG", "ADV", "$,", "FM", "FM", "FM", "$,", "PWAV"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "d\u2019 witt!", "tokens": ["d'", "witt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Isch Basel nit e sch\u00f6ni tolli Stadt?", "tokens": ["Isch", "Ba\u00b7sel", "nit", "e", "sch\u00f6\u00b7ni", "tol\u00b7li", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2019s sin H\u00fcser drinn, \u2019s isch mengi Chilche nit", "tokens": ["'s", "sin", "H\u00fc\u00b7ser", "drinn", ",", "'s", "isch", "men\u00b7gi", "Chil\u00b7che", "nit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PPER", "ADJD", "ADJA", "NN", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "so gro\u00df, und Chilche, \u2019s sin in mengem", "tokens": ["so", "gro\u00df", ",", "und", "Chil\u00b7che", ",", "'s", "sin", "in", "men\u00b7gem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "NN", "$,", "PPER", "VAFIN", "APPR", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dorf", "tokens": ["Dorf"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "nit so viel H\u00fcser. \u2019s isch e Volchspiel, \u2019s", "tokens": ["nit", "so", "viel", "H\u00fc\u00b7ser", ".", "'s", "isch", "e", "Volchs\u00b7piel", ",", "'s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "$.", "PPER", "ADJD", "ADJA", "NN", "$,", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "wohnt", "tokens": ["wohnt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "e Richthum drinn, und menge brave Her,", "tokens": ["e", "Richt\u00b7hum", "drinn", ",", "und", "men\u00b7ge", "bra\u00b7ve", "Her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "und meuge, woni gchennt ha, lit scho lang", "tokens": ["und", "meu\u00b7ge", ",", "wo\u00b7ni", "gchennt", "ha", ",", "lit", "scho", "lang"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "VVFIN", "NE", "$,", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "im Chr\u00fctz-Gang hinterm M\u00fcnster-Platz", "tokens": ["im", "Chr\u00fctz\u00b7Gang", "hin\u00b7term", "M\u00fcns\u00b7ter\u00b7Platz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "und schloft.", "tokens": ["und", "schloft", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "\u2019s ", "tokens": ["'s"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Stund,", "tokens": ["Stund", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "goht Basel au ins Grab, und streckt no do", "tokens": ["goht", "Ba\u00b7sel", "au", "ins", "Grab", ",", "und", "streckt", "no", "do"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und d\u00f6rt e Glied zum Boden us, e Joch,", "tokens": ["und", "d\u00f6rt", "e", "Glied", "zum", "Bo\u00b7den", "us", ",", "e", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "APPRART", "NN", "NE", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "en alte Thurn, e Giebel-Wand; es wachst", "tokens": ["en", "al\u00b7te", "Thurn", ",", "e", "Gie\u00b7bel\u00b7Wand", ";", "es", "wachst"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["FM", "ADJA", "NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "do Holder druf, do B\u00fcechli, Tanne d\u00f6rt,", "tokens": ["do", "Hol\u00b7der", "druf", ",", "do", "B\u00fcech\u00b7li", ",", "Tan\u00b7ne", "d\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "PAV", "$,", "KOUS", "NE", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "und Moos und Farn, und Reiger sitze", "tokens": ["und", "Moos", "und", "Farn", ",", "und", "Rei\u00b7ger", "sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "$,", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "druf \u2014", "tokens": ["druf"], "token_info": ["word", "punct"], "pos": ["PAV", "$("], "meter": "-", "measure": "single.down"}, "line.21": {"text": "\u2019s isch schad derf\u00fcr! \u2014 und sin bis d\u00f6rthi", "tokens": ["'s", "isch", "schad", "der\u00b7f\u00fcr", "!", "und", "sin", "bis", "d\u00f6r\u00b7thi"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "PAV", "$.", "$(", "KON", "NN", "APPR", "NE"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "d\u2019 L\u00fct", "tokens": ["d'", "L\u00fct"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "so n\u00e4rsch wie jez, se g\u00f6hn au Gspenster um,", "tokens": ["so", "n\u00e4rsch", "wie", "jez", ",", "se", "g\u00f6hn", "au", "Gs\u00b7pens\u00b7ter", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "der Sulger, wo die arme Bettel-L\u00fct", "tokens": ["der", "Sul\u00b7ger", ",", "wo", "die", "ar\u00b7me", "Bet\u00b7tel\u00b7L\u00fct"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "vergelstert het, der Lippi L\u00e4ppeli,", "tokens": ["ver\u00b7gels\u00b7tert", "het", ",", "der", "Lip\u00b7pi", "L\u00e4p\u00b7pe\u00b7li", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.26": {"text": "und was weis tch, wer meh. Was sto\u00dfisch", "tokens": ["und", "was", "weis", "tch", ",", "wer", "meh", ".", "Was", "sto\u00b7\u00dfisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "PTKVZ", "VVFIN", "$,", "PWS", "VVFIN", "$.", "PWS", "ADJD"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "mi?", "tokens": ["mi", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}}}}}