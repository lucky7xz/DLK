{"dta.poem.11082": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die gezwungene heyrath.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein leben ist mir abgesagt,", "tokens": ["Mein", "le\u00b7ben", "ist", "mir", "ab\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und mu\u00df den tod umfassen:", "tokens": ["Ich", "soll", "und", "mu\u00df", "den", "tod", "um\u00b7fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Celinde hat mich angeklagt,", "tokens": ["Ce\u00b7lin\u00b7de", "hat", "mich", "an\u00b7ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und den proce\u00df mir machen lassen,", "tokens": ["Und", "den", "pro\u00b7ce\u00df", "mir", "ma\u00b7chen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich die freyheit ihr entf\u00fchrt,", "tokens": ["Weil", "ich", "die", "frey\u00b7heit", "ihr", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sie ber\u00fchrt", "tokens": ["Und", "sie", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Fast mehr, als mir zu thun geb\u00fchrt.", "tokens": ["Fast", "mehr", ",", "als", "mir", "zu", "thun", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich solte zwar, man rieth es mir,", "tokens": ["Ich", "sol\u00b7te", "zwar", ",", "man", "rieth", "es", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PIS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von hier nach Flandern appelliren,", "tokens": ["Von", "hier", "nach", "Flan\u00b7dern", "ap\u00b7pel\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Celinde m\u00f6cht alsdenn allhier", "tokens": ["Ce\u00b7lin\u00b7de", "m\u00f6cht", "als\u00b7denn", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir dorthin processe f\u00fchren;", "tokens": ["Mit", "mir", "dor\u00b7thin", "pro\u00b7ces\u00b7se", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein ich war etwas zu frey,", "tokens": ["Al\u00b7lein", "ich", "war", "et\u00b7was", "zu", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und blieb dabey,", "tokens": ["Und", "blieb", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da\u00df ich die unschuld selber sey.", "tokens": ["Da\u00df", "ich", "die", "un\u00b7schuld", "sel\u00b7ber", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun ist mein ende diese nacht,", "tokens": ["Nun", "ist", "mein", "en\u00b7de", "die\u00b7se", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da man mich \u00e4rmsten will begraben;", "tokens": ["Da", "man", "mich", "\u00e4rms\u00b7ten", "will", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein testament ist schon gemacht:", "tokens": ["Mein", "tes\u00b7ta\u00b7ment", "ist", "schon", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Celinde soll die erbschafft haben", "tokens": ["Ce\u00b7lin\u00b7de", "soll", "die", "erb\u00b7schafft", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So, wie bey mir es steht und liegt.", "tokens": ["So", ",", "wie", "bey", "mir", "es", "steht", "und", "liegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ist vergn\u00fcgt,", "tokens": ["Sie", "ist", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Weil sie mein gantz verm\u00f6gen kriegt.", "tokens": ["Weil", "sie", "mein", "gantz", "ver\u00b7m\u00f6\u00b7gen", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! seht, nun f\u00fchrt man mich hinaus:", "tokens": ["Ach", "!", "seht", ",", "nun", "f\u00fchrt", "man", "mich", "hin\u00b7aus", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "$,", "ADV", "VVFIN", "PIS", "PRF", "APZR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier bringt man mich, den armen s\u00fcnder.", "tokens": ["Hier", "bringt", "man", "mich", ",", "den", "ar\u00b7men", "s\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich zieh, wie die gebadte mau\u00df;", "tokens": ["Ich", "zieh", ",", "wie", "die", "ge\u00b7bad\u00b7te", "mau\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es lachen dr\u00fcber alt\u2019 und kinder.", "tokens": ["Es", "la\u00b7chen", "dr\u00fc\u00b7ber", "alt'", "und", "kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der mantel h\u00e4ngt mir wunderlich,", "tokens": ["Der", "man\u00b7tel", "h\u00e4ngt", "mir", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zolckert sich,", "tokens": ["Und", "zol\u00b7ckert", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich aber bin selbst b\u00f6\u00df\u2019 auf mich.", "tokens": ["Ich", "a\u00b7ber", "bin", "selbst", "b\u00f6\u00df'", "auf", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df als wie ein Jude gehn,", "tokens": ["Ich", "mu\u00df", "als", "wie", "ein", "Ju\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein pfaffe geht mir an der seiten;", "tokens": ["Kein", "pfaf\u00b7fe", "geht", "mir", "an", "der", "sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jm urthel h\u00f6rt ich dieses stehn:", "tokens": ["Jm", "ur\u00b7thel", "h\u00f6rt", "ich", "die\u00b7ses", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Celinde soll dich nur begleiten.", "tokens": ["Ce\u00b7lin\u00b7de", "soll", "dich", "nur", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der folge nach, wohin sie winckt,", "tokens": ["Der", "fol\u00b7ge", "nach", ",", "wo\u00b7hin", "sie", "winckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da sie\u2019s bed\u00fcnckt,", "tokens": ["Da", "sie's", "be\u00b7d\u00fcnckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da man fein sanffte niedersinckt.", "tokens": ["Da", "man", "fein", "sanff\u00b7te", "nie\u00b7der\u00b7sinckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun h\u00f6rt die art des todes an:", "tokens": ["Nun", "h\u00f6rt", "die", "art", "des", "to\u00b7des", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll und soll mich lassen s\u00e4cken.", "tokens": ["Ich", "soll", "und", "soll", "mich", "las\u00b7sen", "s\u00e4\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Celinde weist den marter-plan,", "tokens": ["Ce\u00b7lin\u00b7de", "weist", "den", "mar\u00b7ter\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will mich selbst ins s\u00e4ckgen stecken.", "tokens": ["Und", "will", "mich", "selbst", "ins", "s\u00e4ck\u00b7gen", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie rufft mir zu: Ins s\u00e4ckgen nein!", "tokens": ["Sie", "rufft", "mir", "zu", ":", "Ins", "s\u00e4ck\u00b7gen", "nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VVFIN", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ist zwar klein;", "tokens": ["Es", "ist", "zwar", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch must du hier gesacket seyn.", "tokens": ["Doch", "must", "du", "hier", "ge\u00b7sa\u00b7cket", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}