{"textgrid.poem.44400": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Tag brach an mit mildem Schein]", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tag brach an mit mildem Schein,", "tokens": ["Der", "Tag", "brach", "an", "mit", "mil\u00b7dem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sangen \u2013 im K\u00e4fig \u2013 die V\u00f6gelein,", "tokens": ["Es", "san\u00b7gen", "\u2013", "im", "K\u00e4\u00b7fig", "\u2013", "die", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPRART", "NN", "$(", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es fiel der Tau \u2013 aus der Gie\u00dfkann gesprengt,", "tokens": ["Es", "fiel", "der", "Tau", "\u2013", "aus", "der", "Gie\u00df\u00b7kann", "ge\u00b7sprengt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aufs hei\u00dfe Pflaster, von Menschen gedr\u00e4ngt,", "tokens": ["Aufs", "hei\u00b7\u00dfe", "Pflas\u00b7ter", ",", "von", "Men\u00b7schen", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die sch\u00f6ne Welt noch in Federn lag,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Welt", "noch", "in", "Fe\u00b7dern", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das hei\u00dft: es war just um Mittag,", "tokens": ["Das", "hei\u00dft", ":", "es", "war", "just", "um", "Mit\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Da ritt, so fr\u00fch schon dem Schlummer entrafft,", "tokens": ["Da", "ritt", ",", "so", "fr\u00fch", "schon", "dem", "Schlum\u00b7mer", "en\u00b7trafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Held ", "tokens": ["Held"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Der stets Bem\u00fchte fr\u00fch und sp\u00e4t,", "tokens": ["Der", "stets", "Be\u00b7m\u00fch\u00b7te", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Ritter und Held der Legitimit\u00e4t;", "tokens": ["Der", "Rit\u00b7ter", "und", "Held", "der", "Le\u00b7gi\u00b7ti\u00b7mi\u00b7t\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ihm nach, bewaffnet bis an die Z\u00e4hn,", "tokens": ["Ihm", "nach", ",", "be\u00b7waff\u00b7net", "bis", "an", "die", "Z\u00e4hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sein edler Schildknapp ", "tokens": ["Sein", "ed\u00b7ler", "Schild\u00b7knapp"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Und weit zur\u00fcck, voll Staub und zu Fu\u00df,", "tokens": ["Und", "weit", "zu\u00b7r\u00fcck", ",", "voll", "Staub", "und", "zu", "Fu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "ADJD", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Der Ritter war lieblich anzuschaun,", "tokens": ["Der", "Rit\u00b7ter", "war", "lieb\u00b7lich", "an\u00b7zu\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ein Schrecken der M\u00e4nner, der Wunsch der Fraun,", "tokens": ["Ein", "Schre\u00b7cken", "der", "M\u00e4n\u00b7ner", ",", "der", "Wunsch", "der", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Sein Feld gegen beide \u2013 das Kabinett;", "tokens": ["Sein", "Feld", "ge\u00b7gen", "bei\u00b7de", "\u2013", "das", "Ka\u00b7bi\u00b7nett", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIS", "$(", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Treu bis auf die Eh! der Legitimit\u00e4t.", "tokens": ["Treu", "bis", "auf", "die", "Eh", "!", "der", "Le\u00b7gi\u00b7ti\u00b7mi\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.18": {"text": "Sein Haar zwar etwas schon cendr\u00e9,", "tokens": ["Sein", "Haar", "zwar", "et\u00b7was", "schon", "cen\u00b7dr\u00e9", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch wei\u00df wie Bl\u00fcten, nicht wei\u00df wie Schnee,", "tokens": ["Doch", "wei\u00df", "wie", "Bl\u00fc\u00b7ten", ",", "nicht", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "$,", "PTKNEG", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Ein s\u00fc\u00dfes L\u00e4cheln den Mund entlang,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "L\u00e4\u00b7cheln", "den", "Mund", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sein air so nobel und nonchalant,", "tokens": ["Sein", "air", "so", "no\u00b7bel", "und", "non\u00b7cha\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Was unbedeutend, was wichtig zumeist,", "tokens": ["Was", "un\u00b7be\u00b7deu\u00b7tend", ",", "was", "wich\u00b7tig", "zu\u00b7meist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Handhabt mit aisance sein edler Geist.", "tokens": ["Hand\u00b7habt", "mit", "ai\u00b7san\u00b7ce", "sein", "ed\u00b7ler", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Er ritt derzeit das gro\u00dfe Pferd,", "tokens": ["Er", "ritt", "der\u00b7zeit", "das", "gro\u00b7\u00dfe", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch war es vernagelt, wie man so h\u00f6rt.", "tokens": ["Doch", "war", "es", "ver\u00b7na\u00b7gelt", ",", "wie", "man", "so", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Hart hinter ihm der Knappe ritt,", "tokens": ["Hart", "hin\u00b7ter", "ihm", "der", "Knap\u00b7pe", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein st\u00e4rkster Galopp ein schaukelnder Schritt.", "tokens": ["Sein", "st\u00e4rks\u00b7ter", "Ga\u00b7lopp", "ein", "schau\u00b7keln\u00b7der", "Schritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Grundsatz war von seinem Tun:", "tokens": ["Der", "Grund\u00b7satz", "war", "von", "sei\u00b7nem", "Tun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach vollbrachter Ruh ist gut zu ruhn,", "tokens": ["Nach", "voll\u00b7brach\u00b7ter", "Ruh", "ist", "gut", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Im \u00fcbrigen ein Optimist;", "tokens": ["Im", "\u00fcb\u00b7ri\u00b7gen", "ein", "Op\u00b7ti\u00b7mist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gut ist, was ist und was man i\u00dft.", "tokens": ["Gut", "ist", ",", "was", "ist", "und", "was", "man", "i\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "VAFIN", "KON", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus St\u00f6pselkork sein Panzer war,", "tokens": ["Aus", "St\u00f6p\u00b7sel\u00b7kork", "sein", "Pan\u00b7zer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Schild ne Bratensch\u00fcssel gar,", "tokens": ["Sein", "Schild", "ne", "Bra\u00b7ten\u00b7sch\u00fcs\u00b7sel", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zwei Schl\u00e4uche seine Stiefel gut,", "tokens": ["Zwei", "Schl\u00e4u\u00b7che", "sei\u00b7ne", "Stie\u00b7fel", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Kasseroll sein Eisenhut", "tokens": ["Ein", "Kas\u00b7se\u00b7roll", "sein", "Ei\u00b7sen\u00b7hut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und statt des Sattels kauernd h\u00e4tt", "tokens": ["Und", "statt", "des", "Sat\u00b7tels", "kau\u00b7ernd", "h\u00e4tt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er ausgespreitet ein Federbett.", "tokens": ["Er", "aus\u00b7ge\u00b7sprei\u00b7tet", "ein", "Fe\u00b7der\u00b7bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "All was er trug, der Erdensohn,", "tokens": ["All", "was", "er", "trug", ",", "der", "Er\u00b7den\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "War Privilegium, Invention:", "tokens": ["War", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7um", ",", "In\u00b7ven\u00b7ti\u00b7on", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Sein Helm, zeigt sich ein Wolkenget\u00fcrm,", "tokens": ["Sein", "Helm", ",", "zeigt", "sich", "ein", "Wol\u00b7ken\u00b7ge\u00b7t\u00fcrm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Tat sich voneinander als Regenschirm;", "tokens": ["Tat", "sich", "von\u00b7ein\u00b7an\u00b7der", "als", "Re\u00b7gen\u00b7schirm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Der Speer trug eine Wetterfahn", "tokens": ["Der", "Speer", "trug", "ei\u00b7ne", "Wet\u00b7ter\u00b7fahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und war ein eteignoir daran.", "tokens": ["Und", "war", "ein", "e\u00b7teig\u00b7no\u00b7ir", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADV", "PAV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Vorn prangt an seines Pferdes Bug", "tokens": ["Vorn", "prangt", "an", "sei\u00b7nes", "Pfer\u00b7des", "Bug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ein Sch\u00fcsselkorb und ein Pfropfenzug", "tokens": ["Ein", "Sch\u00fcs\u00b7sel\u00b7korb", "und", "ein", "Pfrop\u00b7fen\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Und rechts und links, wie die M\u00e4hre ging,", "tokens": ["Und", "rechts", "und", "links", ",", "wie", "die", "M\u00e4h\u00b7re", "ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Zur Erde straff eine Treppe hing,", "tokens": ["Zur", "Er\u00b7de", "straff", "ei\u00b7ne", "Trep\u00b7pe", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Dran stieg der Edle auf und ab,", "tokens": ["Dran", "stieg", "der", "Ed\u00b7le", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Wenn sich im Notfall ein Schreck begab.", "tokens": ["Wenn", "sich", "im", "Not\u00b7fall", "ein", "Schreck", "be\u00b7gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Sonst duftet er von W\u00e4ssern sehr,", "tokens": ["Sonst", "duf\u00b7tet", "er", "von", "W\u00e4s\u00b7sern", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Sein Schnupftuch war ein Rosenmeer,", "tokens": ["Sein", "Schnupf\u00b7tuch", "war", "ein", "Ro\u00b7sen\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Selbst unter seines Pferdes Schwanz", "tokens": ["Selbst", "un\u00b7ter", "sei\u00b7nes", "Pfer\u00b7des", "Schwanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["AD