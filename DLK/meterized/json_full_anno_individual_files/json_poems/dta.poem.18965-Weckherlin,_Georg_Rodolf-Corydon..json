{"dta.poem.18965": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Corydon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Odu Cometen schein/", "tokens": ["O\u00b7du", "Co\u00b7me\u00b7ten", "schein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "O harter Kiselstein/", "tokens": ["O", "har\u00b7ter", "Ki\u00b7sel\u00b7stein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nichts kan dein hertz erwaichen/", "tokens": ["Nichts", "kan", "dein", "hertz", "er\u00b7wai\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Ich mu\u00df tods verblaichen", "tokens": ["Vnd", "Ich", "mu\u00df", "tods", "ver\u00b7blai\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch hofnung-lose pein:", "tokens": ["Durch", "hof\u00b7nung\u00b7lo\u00b7se", "pein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darumb so la\u00df vns h\u00f6ren", "tokens": ["Da\u00b7rumb", "so", "la\u00df", "vns", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie meiner schmertzen ehr/", "tokens": ["Wie", "mei\u00b7ner", "schmert\u00b7zen", "ehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd deiner Rawheit lehr", "tokens": ["Vnd", "dei\u00b7ner", "Raw\u00b7heit", "lehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Wald vnd Feld vermehren.", "tokens": ["Die", "Wald", "vnd", "Feld", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}