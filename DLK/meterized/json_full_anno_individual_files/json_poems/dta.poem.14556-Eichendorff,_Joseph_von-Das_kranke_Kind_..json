{"dta.poem.14556": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kranke Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Kinder jauchzen ferne,", "tokens": ["Viel", "Kin\u00b7der", "jauch\u00b7zen", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sch\u00f6n ist's auf der Welt!", "tokens": ["So", "sch\u00f6n", "ist's", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ging' auch spatzieren gerne,", "tokens": ["Ging'", "auch", "spat\u00b7zie\u00b7ren", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch m\u00fcde st\u00fcrzt's im Feld.", "tokens": ["Doch", "m\u00fc\u00b7de", "st\u00fcrzt's", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}