{"textgrid.poem.32454": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "133. Auf den Pfriem", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pfriem ist nicht blo\u00df mein Freund; er ist mein andres Ich.", "tokens": ["Pfriem", "ist", "nicht", "blo\u00df", "mein", "Freund", ";", "er", "ist", "mein", "and\u00b7res", "Ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dies sagt er nicht allein, dies zeigt er meisterlich.", "tokens": ["Dies", "sagt", "er", "nicht", "al\u00b7lein", ",", "dies", "zeigt", "er", "meis\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er steckt in seinen Sack ein Geld, das mit geh\u00f6ret,", "tokens": ["Er", "steckt", "in", "sei\u00b7nen", "Sack", "ein", "Geld", ",", "das", "mit", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und tut mit Dingen gro\u00df, die ihn mein Brief gelehret.", "tokens": ["Und", "tut", "mit", "Din\u00b7gen", "gro\u00df", ",", "die", "ihn", "mein", "Brief", "ge\u00b7leh\u00b7ret", "."], "token_info": ["word", "word", "word"