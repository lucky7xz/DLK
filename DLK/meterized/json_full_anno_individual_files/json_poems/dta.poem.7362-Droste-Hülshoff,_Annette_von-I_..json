{"dta.poem.7362": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Der Anger dampft, es kocht die Ruhr,", "tokens": ["Der", "An\u00b7ger", "dampft", ",", "es", "kocht", "die", "Ruhr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im scharfen Ost die Halme pfeifen,", "tokens": ["Im", "schar\u00b7fen", "Ost", "die", "Hal\u00b7me", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da trabt es sachte durch die Flur,", "tokens": ["Da", "trabt", "es", "sach\u00b7te", "durch", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da taucht es auf wie Nebelstreifen,", "tokens": ["Da", "taucht", "es", "auf", "wie", "Ne\u00b7bel\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da nieder rauscht es in den Flu\u00df,", "tokens": ["Da", "nie\u00b7der", "rauscht", "es", "in", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stemmend gen der Wellen Gu\u00df", "tokens": ["Und", "stem\u00b7mend", "gen", "der", "Wel\u00b7len", "Gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es fliegt der Bug, die Hufe greifen.", "tokens": ["Es", "fliegt", "der", "Bug", ",", "die", "Hu\u00b7fe", "grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Schnauben noch, ein Satz, und frei", "tokens": ["Ein", "Schnau\u00b7ben", "noch", ",", "ein", "Satz", ",", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ro\u00df schwingt seine nassen Flanken,", "tokens": ["Das", "Ro\u00df", "schwingt", "sei\u00b7ne", "nas\u00b7sen", "Flan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder eins, und wieder zwei,", "tokens": ["Und", "wie\u00b7der", "eins", ",", "und", "wie\u00b7der", "zwei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$,", "KON", "ADV", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis f\u00fcnf und zwanzig stehn wie Schranken:", "tokens": ["Bis", "f\u00fcnf", "und", "zwan\u00b7zig", "stehn", "wie", "Schran\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voran, voran durch Haid und Wald,", "tokens": ["Vo\u00b7ran", ",", "vo\u00b7ran", "durch", "Haid", "und", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo sich w\u00fcst das Dickicht ballt,", "tokens": ["Und", "wo", "sich", "w\u00fcst", "das", "Di\u00b7ckicht", "ballt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da brechen knisternd sie die Ranken.", "tokens": ["Da", "bre\u00b7chen", "knis\u00b7ternd", "sie", "die", "Ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Eichenstamm, im Ueberwind,", "tokens": ["Am", "Ei\u00b7chen\u00b7stamm", ",", "im", "Ue\u00b7ber\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um einen Ast den Arm geschlungen,", "tokens": ["Um", "ei\u00b7nen", "Ast", "den", "Arm", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Isenburger steht und sinnt", "tokens": ["Der", "I\u00b7sen\u00b7bur\u00b7ger", "steht", "und", "sinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und naget an Erinnerungen.", "tokens": ["Und", "na\u00b7get", "an", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob er vernimmt, was durch's Gezweig", "tokens": ["Ob", "er", "ver\u00b7nimmt", ",", "was", "durch's", "Ge\u00b7zweig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm Rinkerad, der Ritter bleich,", "tokens": ["Ihm", "Rin\u00b7ke\u00b7rad", ",", "der", "Rit\u00b7ter", "bleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Raunt leise wie mit V\u00f6gelzungen?", "tokens": ["Raunt", "lei\u00b7se", "wie", "mit", "V\u00f6\u00b7gel\u00b7zun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201egraf, fl\u00fcstert es, Graf haltet dicht,", "tokens": ["\u201e", "graf", ",", "fl\u00fcs\u00b7tert", "es", ",", "Graf", "hal\u00b7tet", "dicht", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mich d\u00fcnkt, als woll' es euch beth\u00f6ren;", "tokens": ["Mich", "d\u00fcnkt", ",", "als", "woll'", "es", "euch", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Christi Blute, la\u00dft uns nicht", "tokens": ["Bei", "Chris\u00b7ti", "Blu\u00b7te", ",", "la\u00dft", "uns", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "VVIMP", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heim wie gepeitschte Hunde kehren!", "tokens": ["Heim", "wie", "ge\u00b7peitschte", "Hun\u00b7de", "keh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer hat gefesselt eure Hand,", "tokens": ["Wer", "hat", "ge\u00b7fes\u00b7selt", "eu\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den freien Stegreif euch verrannt?\u201c \u2014", "tokens": ["Den", "frei\u00b7en", "Steg\u00b7reif", "euch", "ver\u00b7rannt", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Isenburg scheint nicht zu h\u00f6ren.", "tokens": ["Der", "I\u00b7sen\u00b7burg", "scheint", "nicht", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "\u201egraf, fl\u00fcstert es, wer war der Mann,", "tokens": ["\u201e", "graf", ",", "fl\u00fcs\u00b7tert", "es", ",", "wer", "war", "der", "Mann", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PWS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem zu dem Kreuz die Rose ", "tokens": ["Dem", "zu", "dem", "Kreuz", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer machte euren Schw\u00e4tzer dann", "tokens": ["Wer", "mach\u00b7te", "eu\u00b7ren", "Schw\u00e4t\u00b7zer", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem eignen Land zum Gaste?", "tokens": ["In", "sei\u00b7nem", "eig\u00b7nen", "Land", "zum", "Gas\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, Graf, wer h\u00f6hnte euer Recht,", "tokens": ["Und", ",", "Graf", ",", "wer", "h\u00f6hn\u00b7te", "eu\u00b7er", "Recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer stempelt euch zum Pfaffenknecht?\u201c \u2014", "tokens": ["Wer", "stem\u00b7pelt", "euch", "zum", "Pfaf\u00b7fen\u00b7knecht", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Isenburg biegt an dem Aste.", "tokens": ["Der", "I\u00b7sen\u00b7burg", "biegt", "an", "dem", "As\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eund wer, wer hat euch zuerkannt,", "tokens": ["\u201e", "und", "wer", ",", "wer", "hat", "euch", "zu\u00b7er\u00b7kannt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "$,", "PWS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im h\u00e4rnen S\u00fcnderhemd zu stehen,", "tokens": ["Im", "h\u00e4r\u00b7nen", "S\u00fcn\u00b7der\u00b7hemd", "zu", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schandekerz' in eurer Hand,", "tokens": ["Die", "Schan\u00b7de\u00b7ker\u00b7z'", "in", "eu\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alte Vetteln anzuflehen", "tokens": ["Und", "al\u00b7te", "Vet\u00b7teln", "an\u00b7zu\u00b7fle\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um Kyrie und Litaney!?\u201c \u2014", "tokens": ["Um", "Ky\u00b7rie", "und", "Li\u00b7ta\u00b7ney", "!?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da krachend bricht der Ast entzwei", "tokens": ["Da", "kra\u00b7chend", "bricht", "der", "Ast", "ent\u00b7zwei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wirbelt in des Sturmes Weben.", "tokens": ["Und", "wir\u00b7belt", "in", "des", "Stur\u00b7mes", "We\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Spricht Isenburg: \u201emein guter Fant,", "tokens": ["Spricht", "I\u00b7sen\u00b7burg", ":", "\u201e", "mein", "gu\u00b7ter", "Fant", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "$(", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und meinst du denn ich sey begraben?", "tokens": ["Und", "meinst", "du", "denn", "ich", "sey", "be\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O la\u00df mich nur in meiner Hand \u2014", "tokens": ["O", "la\u00df", "mich", "nur", "in", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ruhig, still, ich h\u00f6re traben!\u201c", "tokens": ["Doch", "ru\u00b7hig", ",", "still", ",", "ich", "h\u00f6\u00b7re", "tra\u00b7ben", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "PPER", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie stehen lauschend, vorgebeugt;", "tokens": ["Sie", "ste\u00b7hen", "lau\u00b7schend", ",", "vor\u00b7ge\u00b7beugt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch das Gezweig der Helmbusch steigt", "tokens": ["Durch", "das", "Ge\u00b7zweig", "der", "Helm\u00b7busch", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und flattert dr\u00fcber gleich dem Raben.", "tokens": ["Und", "flat\u00b7tert", "dr\u00fc\u00b7ber", "gleich", "dem", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}