{"textgrid.poem.51024": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Tagelied", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst dich freventlich emanzipieren", "tokens": ["Du", "willst", "dich", "fre\u00b7vent\u00b7lich", "e\u00b7man\u00b7zi\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aufstehn wider mich mit keckem Sinn,", "tokens": ["Und", "auf\u00b7stehn", "wi\u00b7der", "mich", "mit", "ke\u00b7ckem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein rotes M\u00fctzlein und die Z\u00fcgel f\u00fchren,", "tokens": ["Ein", "ro\u00b7tes", "M\u00fctz\u00b7lein", "und", "die", "Z\u00fc\u00b7gel", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sch\u00f6ne kleine Jakobinerin?", "tokens": ["Du", "sch\u00f6\u00b7ne", "klei\u00b7ne", "Ja\u00b7ko\u00b7bi\u00b7ne\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zur Politik nun auch dein W\u00f6rtlein sagen,", "tokens": ["Zur", "Po\u00b7li\u00b7tik", "nun", "auch", "dein", "W\u00f6rt\u00b7lein", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Spindel meidend in den Ratsaal fliehn?", "tokens": ["Die", "Spin\u00b7del", "mei\u00b7dend", "in", "den", "Rat\u00b7saal", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl gar mit wei\u00dfer Hand die Trommel schlagen,", "tokens": ["Wohl", "gar", "mit", "wei\u00b7\u00dfer", "Hand", "die", "Trom\u00b7mel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann einst wir gegen die Tyrannen ziehn?", "tokens": ["Wann", "einst", "wir", "ge\u00b7gen", "die", "Ty\u00b7ran\u00b7nen", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Berufest dich auf meine eignen Lehren", "tokens": ["Be\u00b7ru\u00b7fest", "dich", "auf", "mei\u00b7ne", "eig\u00b7nen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Freiheit, Gleichheit und von Menschenrecht?", "tokens": ["Von", "Frei\u00b7heit", ",", "Gleich\u00b7heit", "und", "von", "Men\u00b7schen\u00b7recht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O la\u00df, mein Kind, mit K\u00fcssen dich bekehren,", "tokens": ["O", "la\u00df", ",", "mein", "Kind", ",", "mit", "K\u00fcs\u00b7sen", "dich", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies eine Mal errietest du mich schlecht!", "tokens": ["Dies", "ei\u00b7ne", "Mal", "er\u00b7rie\u00b7test", "du", "mich", "schlecht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Ketten all, von denen ich entbinden", "tokens": ["Die", "Ket\u00b7ten", "all", ",", "von", "de\u00b7nen", "ich", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "$,", "APPR", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die V\u00f6lker m\u00f6chte, o Geliebte mein!", "tokens": ["Die", "V\u00f6l\u00b7ker", "m\u00f6ch\u00b7te", ",", "o", "Ge\u00b7lieb\u00b7te", "mein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "FM", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als Blumenketten eng dir umzuwinden", "tokens": ["Als", "Blu\u00b7men\u00b7ket\u00b7ten", "eng", "dir", "um\u00b7zu\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird einzig nur mein Tun und Trachten sein.", "tokens": ["Wird", "ein\u00b7zig", "nur", "mein", "Tun", "und", "Trach\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich will dir einen festen Turm erbauen", "tokens": ["Ich", "will", "dir", "ei\u00b7nen", "fes\u00b7ten", "Turm", "er\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drin ein K\u00e4mmerlein von Seide weich;", "tokens": ["Und", "drin", "ein", "K\u00e4m\u00b7mer\u00b7lein", "von", "Sei\u00b7de", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sollst du nur des Himmels Sterne schauen", "tokens": ["Da", "sollst", "du", "nur", "des", "Him\u00b7mels", "Ster\u00b7ne", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich, den Kerkermeister, froh und reich!", "tokens": ["Und", "mich", ",", "den", "Ker\u00b7ker\u00b7meis\u00b7ter", ",", "froh", "und", "reich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nie la\u00df ich dich dein langes Haar beschneiden,", "tokens": ["Nie", "la\u00df", "ich", "dich", "dein", "lan\u00b7ges", "Haar", "be\u00b7schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "PPOSAT", "AD