acht", "und", "will's", "auf", "Er\u00b7den", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VMFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eil ich f\u00fcr dich und mich zum Kampf der Zeit.", "tokens": ["Eil", "ich", "f\u00fcr", "dich", "und", "mich", "zum", "Kampf", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "KON", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Du willst dich freventlich emanzipieren", "tokens": ["Du", "willst", "dich", "fre\u00b7vent\u00b7lich", "e\u00b7man\u00b7zi\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aufstehn wider mich mit keckem Sinn,", "tokens": ["Und", "auf\u00b7stehn", "wi\u00b7der", "mich", "mit", "ke\u00b7ckem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "APPR", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein rotes M\u00fctzlein und die Z\u00fcgel f\u00fchren,", "tokens": ["Ein", "ro\u00b7tes", "M\u00fctz\u00b7lein", "und", "die", "Z\u00fc\u00b7gel", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sch\u00f6ne kleine Jakobinerin?", "tokens": ["Du", "sch\u00f6\u00b7ne", "klei\u00b7ne", "Ja\u00b7ko\u00b7bi\u00b7ne\u00b7rin", "?"], "token_info": ["word", "wor