{"dta.poem.2058": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo von allen andern Thieren wir mit Rechte sagen", "tokens": ["Wo", "von", "al\u00b7len", "an\u00b7dern", "Thie\u00b7ren", "wir", "mit", "Rech\u00b7te", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "k\u00f6nnen,", "tokens": ["k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VMFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df der Sch\u00f6pfer uns in ihnen Lust und Nutzen wollen", "tokens": ["Da\u00df", "der", "Sch\u00f6p\u00b7fer", "uns", "in", "ih\u00b7nen", "Lust", "und", "Nut\u00b7zen", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "PPER", "NN", "KON", "NN", "VMFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "g\u00f6nnen,", "tokens": ["g\u00f6n\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So erfodert es der Hund, da\u00df des Sch\u00f6pfers Huld hie-", "tokens": ["So", "er\u00b7fo\u00b7dert", "es", "der", "Hund", ",", "da\u00df", "des", "Sch\u00f6p\u00b7fers", "Huld", "hie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "NE", "TRUNC"], "meter": "--+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "bey", "tokens": ["bey"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Jmmer mit Vernunft betrachtet und mit Dank geprie-", "tokens": ["Jm\u00b7mer", "mit", "Ver\u00b7nunft", "be\u00b7trach\u00b7tet", "und", "mit", "Dank", "ge\u00b7prie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "KON", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "sen sey.", "tokens": ["sen", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Alle Vortheil von den Hunden sind so gro\u00df, so ungemein,", "tokens": ["Al\u00b7le", "Vor\u00b7theil", "von", "den", "Hun\u00b7den", "sind", "so", "gro\u00df", ",", "so", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Da\u00df sie nie recht zu beschreiben und fast nicht zu z\u00e4hlen seyn.", "tokens": ["Da\u00df", "sie", "nie", "recht", "zu", "be\u00b7schrei\u00b7ben", "und", "fast", "nicht", "zu", "z\u00e4h\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "KON", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Selbst entfernte Nationen, welche nicht dergleichen ha-", "tokens": ["Selbst", "ent\u00b7fern\u00b7te", "Na\u00b7ti\u00b7o\u00b7nen", ",", "wel\u00b7che", "nicht", "derg\u00b7lei\u00b7chen", "ha"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "PIS", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "ben,", "tokens": ["ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Wenn man von den Hunden spricht und von allen ihren", "tokens": ["Wenn", "man", "von", "den", "Hun\u00b7den", "spricht", "und", "von", "al\u00b7len", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "PIAT", "PPOSAT"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Gaben,", "tokens": ["Ga\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "K\u00f6nnen es unm\u00f6glich glauben, da\u00df ein Thier das Haus", "tokens": ["K\u00f6n\u00b7nen", "es", "un\u00b7m\u00f6g\u00b7lich", "glau\u00b7ben", ",", "da\u00df", "ein", "Thier", "das", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "bewacht,", "tokens": ["be\u00b7wacht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Andre G\u00fcter sch\u00fctzt und h\u00fctet, so des Tages als bey Nacht;", "tokens": ["And\u00b7re", "G\u00fc\u00b7ter", "sch\u00fctzt", "und", "h\u00fc\u00b7tet", ",", "so", "des", "Ta\u00b7ges", "als", "bey", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "ART", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Da\u00df es unsere Personen gegen Dieb und R\u00e4uber sch\u00fctzt;", "tokens": ["Da\u00df", "es", "un\u00b7se\u00b7re", "Per\u00b7so\u00b7nen", "ge\u00b7gen", "Dieb", "und", "R\u00e4u\u00b7ber", "sch\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Was verlohren, wieder sucht; uns zum Bratenwenden", "tokens": ["Was", "ver\u00b7loh\u00b7ren", ",", "wie\u00b7der", "sucht", ";", "uns", "zum", "Bra\u00b7ten\u00b7wen\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVPP", "$,", "ADV", "VVFIN", "$.", "PPER", "APPRART", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "n\u00fctzt;", "tokens": ["n\u00fctzt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Da\u00df es wilde Thiere f\u00e4llet, Schafe h\u00fctet, Blinde f\u00fch-", "tokens": ["Da\u00df", "es", "wil\u00b7de", "Thie\u00b7re", "f\u00e4l\u00b7let", ",", "Scha\u00b7fe", "h\u00fc\u00b7tet", ",", "Blin\u00b7de", "f\u00fch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "ret,", "tokens": ["ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "In den allerdicksten W\u00e4ldern das versteckte Wild aufsp\u00fc-", "tokens": ["In", "den", "al\u00b7ler\u00b7dicks\u00b7ten", "W\u00e4l\u00b7dern", "das", "ver\u00b7steck\u00b7te", "Wild", "auf\u00b7sp\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "ret,", "tokens": ["ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.25": {"text": "In die Netze treibet, f\u00e4nget, zu uns bringet, rettet,", "tokens": ["In", "die", "Net\u00b7ze", "trei\u00b7bet", ",", "f\u00e4n\u00b7get", ",", "zu", "uns", "brin\u00b7get", ",", "ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "wehrt,", "tokens": ["wehrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Da\u00df, wenn es erleget ist, es kein andrer Hund versehrt;", "tokens": ["Da\u00df", ",", "wenn", "es", "er\u00b7le\u00b7get", "ist", ",", "es", "kein", "an\u00b7drer", "Hund", "ver\u00b7sehrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$,", "PPER", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "F\u00fcchse w\u00fcrget, Hasen greift, aus der Erde D\u00e4chse", "tokens": ["F\u00fcch\u00b7se", "w\u00fcr\u00b7get", ",", "Ha\u00b7sen", "greift", ",", "aus", "der", "Er\u00b7de", "D\u00e4ch\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "treibt;", "tokens": ["treibt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Ja mit seinem Dienst und Nutzen nicht nur auf der Er-", "tokens": ["Ja", "mit", "sei\u00b7nem", "Dienst", "und", "Nut\u00b7zen", "nicht", "nur", "auf", "der", "Er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "APPR", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.31": {"text": "de bleibt,", "tokens": ["de", "bleibt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}}}}}