{"dta.poem.17823": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "66.  Das zeitliche macht ungeschikt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein/ wie magstu doch die Welt so in dich sauffen?", "tokens": ["Ach", "mein", "/", "wie", "mags\u00b7tu", "doch", "die", "Welt", "so", "in", "dich", "sauf\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "$(", "PWAV", "VMFIN", "ADV", "ART", "NN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst ja ungeschikt den Ehrn Krantz zuerlauffen!", "tokens": ["Du", "wirst", "ja", "un\u00b7ge\u00b7schikt", "den", "Ehrn", "Krantz", "zu\u00b7er\u00b7lauf\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}