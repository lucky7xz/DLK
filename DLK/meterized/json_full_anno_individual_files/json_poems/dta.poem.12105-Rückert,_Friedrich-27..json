{"dta.poem.12105": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' in tiefer Nacht im tiefen Thal gewacht,", "tokens": ["Ich", "hab'", "in", "tie\u00b7fer", "Nacht", "im", "tie\u00b7fen", "Thal", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Fenster staunt' ich an der Berge Macht.", "tokens": ["Und", "aus", "dem", "Fens\u00b7ter", "staunt'", "ich", "an", "der", "Ber\u00b7ge", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein Lispel war im Thal, und in der Sterne Stral", "tokens": ["Kein", "Lis\u00b7pel", "war", "im", "Thal", ",", "und", "in", "der", "Ster\u00b7ne", "Stral"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sahn geisterhaft herab die H\u00e4upter starr und kahl.", "tokens": ["Sahn", "geis\u00b7ter\u00b7haft", "her\u00b7ab", "die", "H\u00e4up\u00b7ter", "starr", "und", "kahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da kam der Nachtluft Zug, und laut ans Ohr mir schlug", "tokens": ["Da", "kam", "der", "Nacht\u00b7luft", "Zug", ",", "und", "laut", "ans", "Ohr", "mir", "schlug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "KON", "ADJD", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Menschenruf, den sie auf lauem Fittig trug.", "tokens": ["Ein", "Men\u00b7schen\u00b7ruf", ",", "den", "sie", "auf", "lau\u00b7em", "Fit\u00b7tig", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer wird es seyn? ein Hirt, der taglang unverirrt", "tokens": ["Wer", "wird", "es", "seyn", "?", "ein", "Hirt", ",", "der", "ta\u00b7glang", "un\u00b7ver\u00b7irrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "VAINF", "$.", "ART", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Herde droben hielt, und mit ihr ruhn jetzt wird.", "tokens": ["Die", "Her\u00b7de", "dro\u00b7ben", "hielt", ",", "und", "mit", "ihr", "ruhn", "jetzt", "wird", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er thut aus voller Brust noch diesen Schrei der Lust,", "tokens": ["Er", "thut", "aus", "vol\u00b7ler", "Brust", "noch", "die\u00b7sen", "Schrei", "der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der Einsamkeit bleibt er sich sein bewust.", "tokens": ["Und", "in", "der", "Ein\u00b7sam\u00b7keit", "bleibt", "er", "sich", "sein", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Empor zum Himmel steigt, wenn rings die Oede schweigt,", "tokens": ["Em\u00b7por", "zum", "Him\u00b7mel", "steigt", ",", "wenn", "rings", "die", "O\u00b7e\u00b7de", "schweigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der Ruf des Menschen, der als Herr der Welt sich zeigt.", "tokens": ["Der", "Ruf", "des", "Men\u00b7schen", ",", "der", "als", "Herr", "der", "Welt", "sich", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "KOUS", "NN", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}