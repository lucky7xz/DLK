{"textgrid.poem.66477": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da sa\u00df bei dir ich wieder, wie vor Zeiten!", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sa\u00df bei dir ich wieder, wie vor Zeiten!", "tokens": ["Da", "sa\u00df", "bei", "dir", "ich", "wie\u00b7der", ",", "wie", "vor", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPER", "ADV", "$,", "PWAV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00e4ren wir aus langem Schlaf erstanden,", "tokens": ["Als", "w\u00e4\u00b7ren", "wir", "aus", "lan\u00b7gem", "Schlaf", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und anders Alles, was wir wiederfanden,", "tokens": ["Und", "an\u00b7ders", "Al\u00b7les", ",", "was", "wir", "wie\u00b7der\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So lie\u00dfen wir zur\u00fcck die Blicke gleiten.", "tokens": ["So", "lie\u00b7\u00dfen", "wir", "zu\u00b7r\u00fcck", "die", "Bli\u00b7cke", "glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein F\u00fchrer war, mich in dein Herz zu leiten,", "tokens": ["Ein", "F\u00fch\u00b7rer", "war", ",", "mich", "in", "dein", "Herz", "zu", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der ernste Schmerzenszug, den tief verstanden", "tokens": ["Der", "erns\u00b7te", "Schmer\u00b7zens\u00b7zug", ",", "den", "tief", "ver\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich las in deinem Antlitz; doch es schwanden,", "tokens": ["Ich", "las", "in", "dei\u00b7nem", "Ant\u00b7litz", ";", "doch", "es", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nein, niemals! deines Auges Herrlichkeiten!", "tokens": ["Nein", ",", "nie\u00b7mals", "!", "dei\u00b7nes", "Au\u00b7ges", "Herr\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was wir erduldet, lie\u00dfen wir verstummen.", "tokens": ["Was", "wir", "er\u00b7dul\u00b7det", ",", "lie\u00b7\u00dfen", "wir", "ver\u00b7stum\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch dachten wir der Zeit, die wir verloren,", "tokens": ["Doch", "dach\u00b7ten", "wir", "der", "Zeit", ",", "die", "wir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sprachen manchen unverge\u00dfnen Namen.", "tokens": ["Und", "spra\u00b7chen", "man\u00b7chen", "un\u00b7ver\u00b7ge\u00df\u00b7nen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da quoll der alten Lieder fernes Summen \u2013", "tokens": ["Da", "quoll", "der", "al\u00b7ten", "Lie\u00b7der", "fer\u00b7nes", "Sum\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir f\u00fchlten neu den alten Schmerz geboren,", "tokens": ["Wir", "f\u00fchl\u00b7ten", "neu", "den", "al\u00b7ten", "Schmerz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sa\u00dfen schweigend bis wir Abschied nahmen.", "tokens": ["Und", "sa\u00b7\u00dfen", "schwei\u00b7gend", "bis", "wir", "Ab\u00b7schied", "nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da sa\u00df bei dir ich wieder, wie vor Zeiten!", "tokens": ["Da", "sa\u00df", "bei", "dir", "ich", "wie\u00b7der", ",", "wie", "vor", "Zei\u00b7ten", "!"