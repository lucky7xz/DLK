{"textgrid.poem.61137": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Morgenwanderung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer recht in Freuden wandern will,", "tokens": ["Wer", "recht", "in", "Freu\u00b7den", "wan\u00b7dern", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NN", "VVFIN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der geh' der Sonn' entgegen;", "tokens": ["Der", "geh'", "der", "Sonn'", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist der Wald so kirchenstill,", "tokens": ["Da", "ist", "der", "Wald", "so", "kir\u00b7chen\u00b7still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein L\u00fcftchen mag sich regen;", "tokens": ["Kein", "L\u00fcft\u00b7chen", "mag", "sich", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch sind nicht die Lerchen wach,", "tokens": ["Noch", "sind", "nicht", "die", "Ler\u00b7chen", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur im hohen Gras der Bach", "tokens": ["Nur", "im", "ho\u00b7hen", "Gras", "der", "Bach"], "token_info": ["word", "word", "word", "word", "word", "wor