{"textgrid.poem.51033": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe dich mit l\u00e4ssig sichrer Hand", "tokens": ["Ich", "se\u00b7he", "dich", "mit", "l\u00e4s\u00b7sig", "sich\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schulterlinien einer G\u00f6ttin schreiben,", "tokens": ["Die", "Schul\u00b7ter\u00b7li\u00b7ni\u00b7en", "ei\u00b7ner", "G\u00f6t\u00b7tin", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dazu den Hohn um deine Lippen treiben:", "tokens": ["Da\u00b7zu", "den", "Hohn", "um", "dei\u00b7ne", "Lip\u00b7pen", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bb's ist nichts dahinter!\u00ab oder: \u00bbeitler Tand!\u00ab", "tokens": ["\u00bb", "'s", "ist", "nichts", "da\u00b7hin\u00b7ter", "!", "\u00ab", "o\u00b7der", ":", "\u00bb", "eit\u00b7ler", "Tand", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "PAV", "$.", "$(", "KON", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Seh dich zuhinterst an der Schenke Wand", "tokens": ["Seh", "dich", "zu\u00b7hin\