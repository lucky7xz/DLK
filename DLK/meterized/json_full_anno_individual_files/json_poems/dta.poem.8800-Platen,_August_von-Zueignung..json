{"dta.poem.8800": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die Farb' an Farbe dir das Haupt umflicht,", "tokens": ["Die", "Fa\u00b7rb'", "an", "Far\u00b7be", "dir", "das", "Haupt", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Magst du mir danken bald und bald verzeihen,", "tokens": ["Magst", "du", "mir", "dan\u00b7ken", "bald", "und", "bald", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was hier gelungen oder was gebricht:", "tokens": ["Was", "hier", "ge\u00b7lun\u00b7gen", "o\u00b7der", "was", "ge\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "KON", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was k\u00f6nnte ", "tokens": ["Was", "k\u00f6nn\u00b7te"], "token_info": ["word", "word"], "pos": ["PWS", "VMFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Du bist mir selbst ein freundliches Gedicht,", "tokens": ["Du", "bist", "mir", "selbst", "ein", "freund\u00b7li\u00b7ches", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das, wenn der Tr\u00fcbsinn oft ihn l\u00e4hmend z\u00fcgelt,", "tokens": ["Das", ",", "wenn", "der", "Tr\u00fcb\u00b7sinn", "oft", "ihn", "l\u00e4h\u00b7mend", "z\u00fc\u00b7gelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den schweren Muth des Dichters froh befl\u00fcgelt.", "tokens": ["Den", "schwe\u00b7ren", "Muth", "des", "Dich\u00b7ters", "froh", "be\u00b7fl\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und w\u00e4g' ich uns, erscheinst du von uns beyden", "tokens": ["Und", "w\u00e4g'", "ich", "uns", ",", "er\u00b7scheinst", "du", "von", "uns", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "APPR", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Kluge sicher mir und ich der Thor:", "tokens": ["Der", "Klu\u00b7ge", "si\u00b7cher", "mir", "und", "ich", "der", "Thor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich trage nur das Leben und die Leiden,", "tokens": ["Ich", "tra\u00b7ge", "nur", "das", "Le\u00b7ben", "und", "die", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dich aber tr\u00e4gt das Leben selbst empor:", "tokens": ["Dich", "a\u00b7ber", "tr\u00e4gt", "das", "Le\u00b7ben", "selbst", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wer nicht dich liebte, m\u00fc\u00dfte dich beneiden,", "tokens": ["Wer", "nicht", "dich", "lieb\u00b7te", ",", "m\u00fc\u00df\u00b7te", "dich", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PPER", "VVFIN", "$,", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Allein wer z\u00f6ge nicht die Liebe vor?", "tokens": ["Al\u00b7lein", "wer", "z\u00f6\u00b7ge", "nicht", "die", "Lie\u00b7be", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich habe, durch dein Wesen unterrichtet,", "tokens": ["Ich", "ha\u00b7be", ",", "durch", "dein", "We\u00b7sen", "un\u00b7ter\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Den Hafis nachgef\u00fchlt und nachgedichtet.", "tokens": ["Den", "Ha\u00b7fis", "nach\u00b7ge\u00b7f\u00fchlt", "und", "nach\u00b7ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}