{"dta.poem.18147": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Abwechselung der Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Erde die zur Winters Zeit,", "tokens": ["Die", "Er\u00b7de", "die", "zur", "Win\u00b7ters", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Eis bedekt mit Schnee bestreut", "tokens": ["Mit", "Eis", "be\u00b7dekt", "mit", "Schnee", "be\u00b7streut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von rauher K\u00e4lte gantz erfroren,", "tokens": ["Von", "rau\u00b7her", "K\u00e4l\u00b7te", "gantz", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheint gleichsam wieder neu gebohren;", "tokens": ["Scheint", "gleich\u00b7sam", "wie\u00b7der", "neu", "ge\u00b7boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bald der Strahl vom Sonnenlicht,", "tokens": ["So", "bald", "der", "Strahl", "vom", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Schnee zerlekt, das Eis durchbricht:", "tokens": ["Den", "Schnee", "zer\u00b7lekt", ",", "das", "Eis", "durch\u00b7bricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und den erfrornen Schoos der Erden,", "tokens": ["Und", "den", "er\u00b7fror\u00b7nen", "Schoos", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00e4st wieder fruchtbahr, tr\u00e4chtig werden.", "tokens": ["L\u00e4st", "wie\u00b7der", "frucht\u00b7bahr", ",", "tr\u00e4ch\u00b7tig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein GOtt! wie herrlig ist die Welt,", "tokens": ["Mein", "Gott", "!", "wie", "herr\u00b7lig", "ist", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da sich der Fr\u00fchling eingestellt.", "tokens": ["Da", "sich", "der", "Fr\u00fch\u00b7ling", "ein\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Landman fuhr vorher mit Schlitten,", "tokens": ["Der", "Land\u00b7man", "fuhr", "vor\u00b7her", "mit", "Schlit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das Holtz in sein und andre H\u00fctten", "tokens": ["Das", "Holtz", "in", "sein", "und", "and\u00b7re", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zum Unterhalt des Element", "tokens": ["Zum", "Un\u00b7ter\u00b7halt", "des", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Des Feurs, das w\u00e4rmet kocht und brent:", "tokens": ["Des", "Feurs", ",", "das", "w\u00e4r\u00b7met", "kocht", "und", "brent", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Er briet damit auf seinen Heerte,", "tokens": ["Er", "briet", "da\u00b7mit", "auf", "sei\u00b7nen", "Heer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was ihn der vorge Herbst bescherte.", "tokens": ["Was", "ihn", "der", "vor\u00b7ge", "Herbst", "be\u00b7scher\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dies hat er meist als Winterkost,", "tokens": ["Dies", "hat", "er", "meist", "als", "Win\u00b7ter\u00b7kost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit warmer Lust bey rauhen Frost", "tokens": ["Mit", "war\u00b7mer", "Lust", "bey", "rau\u00b7hen", "Frost"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Verzehrt; von Wurtzeln, Kohl und R\u00fcben,", "tokens": ["Ver\u00b7zehrt", ";", "von", "Wurt\u00b7zeln", ",", "Kohl", "und", "R\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ist nunmehr wenig \u00fcbrig blieben.", "tokens": ["Ist", "nun\u00b7mehr", "we\u00b7nig", "\u00fcb\u00b7rig", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Gott! der dem Menschen Futter schafft,", "tokens": ["Gott", "!", "der", "dem", "Men\u00b7schen", "Fut\u00b7ter", "schafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Giebt wiederum den Lande Krafft", "tokens": ["Giebt", "wie\u00b7de\u00b7rum", "den", "Lan\u00b7de", "Krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Damit der Saame den man streuet,", "tokens": ["Da\u00b7mit", "der", "Saa\u00b7me", "den", "man", "streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Jm G\u00e4rten Paradies gedeiet.", "tokens": ["Jm", "G\u00e4r\u00b7ten", "Pa\u00b7ra\u00b7dies", "ge\u00b7dei\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ja! seiner Vorsicht Wundermacht,", "tokens": ["Ja", "!", "sei\u00b7ner", "Vor\u00b7sicht", "Wun\u00b7der\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hat Blumen schon hervorgebracht", "tokens": ["Hat", "Blu\u00b7men", "schon", "her\u00b7vor\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Ich sahe j\u00fcngst die Maienblume,", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", "die", "Mai\u00b7en\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und forschte zu des H\u00f6chsten Ruhme,", "tokens": ["Und", "forschte", "zu", "des", "H\u00f6chs\u00b7ten", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Warum so fr\u00fch dies Bl\u00fcmelein,", "tokens": ["Wa\u00b7rum", "so", "fr\u00fch", "dies", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sich in dem Garten stellte ein?", "tokens": ["Sich", "in", "dem", "Gar\u00b7ten", "stell\u00b7te", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mir deucht es zeiget diese Bl\u00fcte,", "tokens": ["Mir", "deucht", "es", "zei\u00b7get", "die\u00b7se", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wie zu uns spricht des H\u00f6chsten G\u00fcte:", "tokens": ["Wie", "zu", "uns", "spricht", "des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Jhr Kinder! grabet, pflantzet, streut,", "tokens": ["Ihr", "Kin\u00b7der", "!", "gra\u00b7bet", ",", "pflant\u00b7zet", ",", "streut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Die Erde ist zum Wuchs bereit,", "tokens": ["Die", "Er\u00b7de", "ist", "zum", "Wuchs", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Zur Probe sind der Blumen Fr\u00fcchte,", "tokens": ["Zur", "Pro\u00b7be", "sind", "der", "Blu\u00b7men", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Schon da, vor euren Angesichte.", "tokens": ["Schon", "da", ",", "vor", "eu\u00b7ren", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}