{"textgrid.poem.47257": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bei Sonnenuntergang", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr' wohl, o goldne Sonne,", "tokens": ["Fahr'", "wohl", ",", "o", "gold\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du gehst zu deiner Ruh';", "tokens": ["Du", "gehst", "zu", "dei\u00b7ner", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und voll von deiner Wonne", "tokens": ["Und", "voll", "von", "dei\u00b7ner", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gehn mir die Augen zu.", "tokens": ["Gehn", "mir", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schwer sind die Augenlider,", "tokens": ["Schwer", "sind", "die", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du nimmst das Lied mit fort.", "tokens": ["Du", "nimmst", "das", "Lied", "mit", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fahr' wohl! wir sehn uns wieder", "tokens": ["Fahr'", "wohl", "!", "wir", "sehn", "uns", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier unten oder dort.", "tokens": ["Hier", "un\u00b7ten", "o\u00b7der", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier unten, wann sich wieder", "tokens": ["Hier", "un\u00b7ten", ",", "wann", "sich", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dies Haupt vom Schlaf erhob;", "tokens": ["Dies", "Haupt", "vom", "Schlaf", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann blickest du hernieder", "tokens": ["Dann", "bli\u00b7ckest", "du", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und freuest dich darob.", "tokens": ["Und", "freu\u00b7est", "dich", "da\u00b7rob", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und tr\u00e4gt des Tods Gefieder", "tokens": ["Und", "tr\u00e4gt", "des", "Tods", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich statt des Traums empor,", "tokens": ["Mich", "st