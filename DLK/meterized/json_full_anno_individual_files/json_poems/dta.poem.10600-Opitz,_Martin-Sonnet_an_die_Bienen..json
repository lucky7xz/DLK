{"dta.poem.10600": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet an die Bienen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Honigv\u00f6gelein/ die jhr von den Violen/", "tokens": ["Ihr", "Ho\u00b7nig\u00b7v\u00f6\u00b7ge\u00b7lein", "/", "die", "jhr", "von", "den", "Vi\u00b7o\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Rosen abgemeit den wunders\u00fcssen Safft/", "tokens": ["Vnd", "Ro\u00b7sen", "ab\u00b7ge\u00b7meit", "den", "wun\u00b7der\u00b7s\u00fcs\u00b7sen", "Safft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die jhr dem gr\u00fcnen Klee entzogen seine Krafft/", "tokens": ["Die", "jhr", "dem", "gr\u00fc\u00b7nen", "Klee", "ent\u00b7zo\u00b7gen", "sei\u00b7ne", "Krafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die jhr das sch\u00f6ne Feldt so offt vnd viel bestohlen/", "tokens": ["Die", "jhr", "das", "sch\u00f6\u00b7ne", "Feldt", "so", "offt", "vnd", "viel", "be\u00b7stoh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Feldt einwohnerin/ was wollet jhr doch holen", "tokens": ["Ihr", "Feldt", "ein\u00b7woh\u00b7ne\u00b7rin", "/", "was", "wol\u00b7let", "jhr", "doch", "ho\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das/ so euch noch zur Zeit hat wenig nutz geschafft/", "tokens": ["Das", "/", "so", "euch", "noch", "zur", "Zeit", "hat", "we\u00b7nig", "nutz", "ge\u00b7schafft", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "ADV", "APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil jhr mit Dienstbarkeit de\u00df Menschen seit behafft/", "tokens": ["Weil", "jhr", "mit", "Dienst\u00b7bar\u00b7keit", "de\u00df", "Men\u00b7schen", "seit", "be\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd jhnen mehrentheils das Honig m\u00fcsset Zollen?", "tokens": ["Vnd", "jh\u00b7nen", "meh\u00b7ren\u00b7theils", "das", "Ho\u00b7nig", "m\u00fcs\u00b7set", "Zol\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NN", "VMFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kompt/ kompt zu meinem Lieb/ auff jhren Rosenmundt/", "tokens": ["Kompt", "/", "kompt", "zu", "mei\u00b7nem", "Lieb", "/", "auff", "jhren", "Ro\u00b7sen\u00b7mundt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der mir mein kranckes Hertz gantz jnniglich verwundt/", "tokens": ["Der", "mir", "mein", "kran\u00b7ckes", "Hertz", "gantz", "jn\u00b7nig\u00b7lich", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da solt jhr Himmelspei\u00df vnd vberfl\u00fcssig brechen:", "tokens": ["Da", "solt", "jhr", "Him\u00b7mel\u00b7spei\u00df", "vnd", "vberf\u00b7l\u00fcs\u00b7sig", "bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.12": {"text": "Wann aber jemandt ja sich vnderstehen kundt", "tokens": ["Wann", "a\u00b7ber", "je\u00b7mandt", "ja", "sich", "vn\u00b7der\u00b7ste\u00b7hen", "kundt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "ADV", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr vbel anzuthun/ dem sollet jhr zur stundt", "tokens": ["Ihr", "vbel", "an\u00b7zu\u00b7thun", "/", "dem", "sol\u00b7let", "jhr", "zur", "stundt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVIZU", "$(", "PDS", "VMFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "F\u00fcr Honig Galle sein/ vnd jhn zu tode stechen.", "tokens": ["F\u00fcr", "Ho\u00b7nig", "Gal\u00b7le", "sein", "/", "vnd", "jhn", "zu", "to\u00b7de", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAINF", "$(", "KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}