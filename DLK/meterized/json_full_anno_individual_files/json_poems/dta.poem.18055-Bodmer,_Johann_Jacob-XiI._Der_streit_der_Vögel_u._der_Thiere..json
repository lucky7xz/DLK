{"dta.poem.18055": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  Der streit der V\u00f6gel u. der Thiere.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor diesem ist ein schwerer Streit gewesen,", "tokens": ["Vor", "die\u00b7sem", "ist", "ein", "schwe\u00b7rer", "Streit", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darinnen kaum der zehnde Mann genesen.", "tokens": ["Da\u00b7rin\u00b7nen", "kaum", "der", "zehn\u00b7de", "Mann", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Thiere foderten f\u00fcr sich und ihr Geschlecht", "tokens": ["Die", "Thie\u00b7re", "fo\u00b7der\u00b7ten", "f\u00fcr", "sich", "und", "ihr", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "KON", "PPOSAT", "NN"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Das Land, als ihr geb\u00fchrend Recht.", "tokens": ["Das", "Land", ",", "als", "ihr", "ge\u00b7b\u00fch\u00b7rend", "Recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott, sagten sie, hat uns das Land gegeben,", "tokens": ["Gott", ",", "sag\u00b7ten", "sie", ",", "hat", "uns", "das", "Land", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf dem wir sollten gehn und leben;", "tokens": ["Auf", "dem", "wir", "soll\u00b7ten", "gehn", "und", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und was es bringt und tr\u00e4gt geh\u00f6ret uns zur Speise.", "tokens": ["Und", "was", "es", "bringt", "und", "tr\u00e4gt", "ge\u00b7h\u00f6\u00b7ret", "uns", "zur", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die V\u00f6gel sprachen gleicherweise", "tokens": ["Die", "V\u00f6\u00b7gel", "spra\u00b7chen", "glei\u00b7cher\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das Erdreich an; die Erde nebst der Luft;", "tokens": ["Das", "Er\u00b7dreich", "an", ";", "die", "Er\u00b7de", "nebst", "der", "Luft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Die beyde w\u00e4ren nur f\u00fcr sie gemacht.", "tokens": ["Die", "bey\u00b7de", "w\u00e4\u00b7ren", "nur", "f\u00fcr", "sie", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "F\u00fcr diese wollten sie in einer strengen Schlacht", "tokens": ["F\u00fcr", "die\u00b7se", "woll\u00b7ten", "sie", "in", "ei\u00b7ner", "stren\u00b7gen", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr Leben an die Thiere wagen.", "tokens": ["Ihr", "Le\u00b7ben", "an", "die", "Thie\u00b7re", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Daher erhub sich nun ein t\u00f6dlich-schwerer Krieg.", "tokens": ["Da\u00b7her", "er\u00b7hub", "sich", "nun", "ein", "t\u00f6d\u00b7lich\u00b7schwe\u00b7rer", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man stritt mit Heldenmuth um Ehr, Besitz und Rechte,", "tokens": ["Man", "stritt", "mit", "Hel\u00b7den\u00b7muth", "um", "Ehr", ",", "Be\u00b7sitz", "und", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit Schilde, Spie\u00df u. Schwerdt, so Rittersleut als Knechte.", "tokens": ["Mit", "Schil\u00b7de", ",", "Spie\u00df", "u.", "Schwerdt", ",", "so", "Rit\u00b7ters\u00b7leut", "als", "Knech\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "abbreviation", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "NE", "$,", "ADV", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Man jagt und ward gejagt, mit ungewissem Sieg;", "tokens": ["Man", "jagt", "und", "ward", "ge\u00b7jagt", ",", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Sieg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VAFIN", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bis da\u00df ein grosser Ri\u00df im Vogelheer geschah.", "tokens": ["Bis", "da\u00df", "ein", "gros\u00b7ser", "Ri\u00df", "im", "Vo\u00b7gel\u00b7heer", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sobald als die\u00df die Fledermaus ersah,", "tokens": ["So\u00b7bald", "als", "die\u00df", "die", "Fle\u00b7der\u00b7maus", "er\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PDS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verlie\u00df sie heimlich ihre Schaaren.", "tokens": ["Ver\u00b7lie\u00df", "sie", "heim\u00b7lich", "ih\u00b7re", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie mischt sich unter die, die ihre Feinde waren.", "tokens": ["Sie", "mischt", "sich", "un\u00b7ter", "die", ",", "die", "ih\u00b7re", "Fein\u00b7de", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie folgt des feigen Hertzens Rath,", "tokens": ["Sie", "folgt", "des", "fei\u00b7gen", "Hert\u00b7zens", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und flieht da man sie n\u00f6thig hat.", "tokens": ["Und", "flieht", "da", "man", "sie", "n\u00f6\u00b7thig", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PIS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Inzwischen steht mit st\u00fcrmendem Gefieder", "tokens": ["I\u00b7nzwi\u00b7schen", "steht", "mit", "st\u00fcr\u00b7men\u00b7dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Der Adler vor den Ri\u00df, erg\u00e4ntzt die L\u00fccke wieder.", "tokens": ["Der", "Ad\u00b7ler", "vor", "den", "Ri\u00df", ",", "er\u00b7g\u00e4ntzt", "die", "L\u00fc\u00b7cke", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er schrie die V\u00f6gel an, gab ihnen Hertz und Muth,", "tokens": ["Er", "schrie", "die", "V\u00f6\u00b7gel", "an", ",", "gab", "ih\u00b7nen", "Hertz", "und", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie Ajax that, und noch ein k\u00fchner thut.", "tokens": ["Wie", "A\u00b7jax", "that", ",", "und", "noch", "ein", "k\u00fch\u00b7ner", "thut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,", "KON", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Sie w\u00fcrgten ihm mit scharffen Schn\u00e4beln nach.", "tokens": ["Sie", "w\u00fcrg\u00b7ten", "ihm", "mit", "scharf\u00b7fen", "Schn\u00e4\u00b7beln", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dadurch verkehret sich des Krieges Gl\u00fccke,", "tokens": ["Da\u00b7durch", "ver\u00b7keh\u00b7ret", "sich", "des", "Krie\u00b7ges", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Es wendet sich der Sieg von seiner Flucht.", "tokens": ["Es", "wen\u00b7det", "sich", "der", "Sieg", "von", "sei\u00b7ner", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Mit ihm fliegt auch die Fledermaus zur\u00fccke.", "tokens": ["Mit", "ihm", "fliegt", "auch", "die", "Fle\u00b7der\u00b7maus", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Sie kam zu der verdienten Straffe,", "tokens": ["Sie", "kam", "zu", "der", "ver\u00b7dien\u00b7ten", "Straf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Die V\u00f6gel machten sie gantz nackt und blo\u00df;", "tokens": ["Die", "V\u00f6\u00b7gel", "mach\u00b7ten", "sie", "gantz", "nackt", "und", "blo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sie gaben ihr manch harten Puff und Sto\u00df.", "tokens": ["Sie", "ga\u00b7ben", "ihr", "manch", "har\u00b7ten", "Puff", "und", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Dazu ward ihr zur Busse das gegeben,", "tokens": ["Da\u00b7zu", "ward", "ihr", "zur", "Bus\u00b7se", "das", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPRART", "NN", "ART", "VVPP", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Sie sollte Nachts ihr sch\u00e4ndlich Leben", "tokens": ["Sie", "soll\u00b7te", "Nachts", "ihr", "sch\u00e4nd\u00b7lich", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Speise zu versorgen fliegen;", "tokens": ["Mit", "Spei\u00b7se", "zu", "ver\u00b7sor\u00b7gen", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Des Tages sollte sie im Finstern liegen.", "tokens": ["Des", "Ta\u00b7ges", "soll\u00b7te", "sie", "im", "Fins\u00b7tern", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}