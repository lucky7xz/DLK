{"dta.poem.19223": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Anfang war das Licht, ein goldner Aetherduft,", "tokens": ["Im", "An\u00b7fang", "war", "das", "Licht", ",", "ein", "gold\u00b7ner", "A\u00b7e\u00b7ther\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der wollte anders seyn, und ward sein Andres, Luft.", "tokens": ["Der", "woll\u00b7te", "an\u00b7ders", "seyn", ",", "und", "ward", "sein", "And\u00b7res", ",", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VAINF", "$,", "KON", "VAFIN", "PPOSAT", "PIS", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die L\u00fcfte strebten sich mit Sehnsucht auszudehnen,", "tokens": ["Die", "L\u00fcf\u00b7te", "streb\u00b7ten", "sich", "mit", "Sehn\u00b7sucht", "aus\u00b7zu\u00b7deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nieder flossen sie in Wasser wie in Thr\u00e4nen.", "tokens": ["Und", "nie\u00b7der", "flos\u00b7sen", "sie", "in", "Was\u00b7ser", "wie", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Wasser gohr vor Lust und zeugete den Schaum,", "tokens": ["Das", "Was\u00b7ser", "gohr", "vor", "Lust", "und", "zeu\u00b7ge\u00b7te", "den", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ward verdichtet Schlamm, und trug dann Gras und Baum.", "tokens": ["Da", "ward", "ver\u00b7dich\u00b7tet", "Schlamm", ",", "und", "trug", "dann", "Gras", "und", "Baum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "NN", "$,", "KON", "VVFIN", "ADV", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Schlammerd' aber schlo\u00df sich fest in sich hinein,", "tokens": ["Die", "Schlam\u00b7merd'", "a\u00b7ber", "schlo\u00df", "sich", "fest", "in", "sich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ward im Innersten verh\u00e4rtet Erz und Stein.", "tokens": ["Und", "ward", "im", "In\u00b7ners\u00b7ten", "ver\u00b7h\u00e4r\u00b7tet", "Erz", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Der Stein erregte sich, und schlug hervor das Feuer,", "tokens": ["Der", "Stein", "er\u00b7reg\u00b7te", "sich", ",", "und", "schlug", "her\u00b7vor", "das", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ward im Tiefen bald ein Herrscher ungeheuer.", "tokens": ["Das", "ward", "im", "Tie\u00b7fen", "bald", "ein", "Herr\u00b7scher", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Erst glaubt' es ewig sich, am Ende fiel ihm bei,", "tokens": ["Erst", "glaubt'", "es", "e\u00b7wig", "sich", ",", "am", "En\u00b7de", "fiel", "ihm", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "$,", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df es von Anfang nur das Licht gewesen sei.", "tokens": ["Da\u00df", "es", "von", "An\u00b7fang", "nur", "das", "Licht", "ge\u00b7we\u00b7sen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und es beschlo\u00df die Welt von unten auf zu treiben,", "tokens": ["Und", "es", "be\u00b7schlo\u00df", "die", "Welt", "von", "un\u00b7ten", "auf", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Licht von oben her; so wird die Sch\u00f6pfung bleiben.", "tokens": ["Wie", "Licht", "von", "o\u00b7ben", "her", ";", "so", "wird", "die", "Sch\u00f6p\u00b7fung", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADV", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}