{"dta.poem.13799": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Bruder meines Lebens,", "tokens": ["Dem", "Bru\u00b7der", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der, fern, mit mir zusammen,", "tokens": ["Der", ",", "fern", ",", "mit", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei denn aus Herzensgrunde", "tokens": ["Sei", "denn", "aus", "Her\u00b7zens\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das erste Glas gebracht!", "tokens": ["Das", "ers\u00b7te", "Glas", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich brauch' ihn nicht zu nennen,", "tokens": ["Ich", "brauch'", "ihn", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er aber wird mich kennen.", "tokens": ["Er", "a\u00b7ber", "wird", "mich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Viel Land trennt uns vergebens,", "tokens": ["Viel", "Land", "trennt", "uns", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm soll dies Wort, die Stunde,", "tokens": ["Ihm", "soll", "dies", "Wort", ",", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Durch alle Adern flammen,", "tokens": ["Durch", "al\u00b7le", "A\u00b7dern", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie ich an Ihn gedacht!", "tokens": ["Wie", "ich", "an", "Ihn", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}