{"textgrid.poem.47022": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ist nicht sch\u00f6n, kann ich dir redlich sagen,", "tokens": ["Du", "ist", "nicht", "sch\u00f6n", ",", "kann", "ich", "dir", "red\u00b7lich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist nicht sch\u00f6n, ob rot gleich ist die Wange", "tokens": ["Du", "bist", "nicht", "sch\u00f6n", ",", "ob", "rot", "gleich", "ist", "die", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "ADJD", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und blau das Aug' und braun das Haar, das lange,", "tokens": ["Und", "blau", "das", "Aug'", "und", "braun", "das", "Haar", ",", "das", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel sch\u00f6n're sah ich schon in meinen Tagen.", "tokens": ["Viel", "sch\u00f6n'\u00b7re", "sah", "ich", "schon", "in", "mei\u00b7nen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und da\u00df ich so in Wohl- und Wehbehagen,", "tokens": ["Und", "da\u00df", "ich", "so", "in", "Wohl", "und", "Weh\u00b7be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht zu-, nicht abw\u00e4rts k\u00f6nnend, an dir hange,", "tokens": ["Nicht", "zu", ",", "nicht", "ab\u00b7w\u00e4rts", "k\u00f6n\u00b7nend", ",", "an", "dir", "han\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "TRUNC", "$,", "PTKNEG", "ADV", "VVPP", "$,", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht deine Sch\u00f6nheit ist die goldne Spange;", "tokens": ["Nicht", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", "die", "gold\u00b7ne", "Span\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eherne, die ich mu\u00df k\u00fcssend nagen,", "tokens": ["Die", "e\u00b7her\u00b7ne", ",", "die", "ich", "mu\u00df", "k\u00fcs\u00b7send", "na\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dein Trotz ist es, dein starrer Sinn und steifer,", "tokens": ["Dein", "Trotz", "ist", "es", ",", "dein", "star\u00b7rer", "Sinn", "und", "stei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "PPO