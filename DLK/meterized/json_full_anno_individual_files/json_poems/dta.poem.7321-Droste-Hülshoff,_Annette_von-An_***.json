{"dta.poem.7321": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "An  ***", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Wort, und w\u00e4r' es scharf wie Stahles Klinge,", "tokens": ["Kein", "Wort", ",", "und", "w\u00e4r'", "es", "scharf", "wie", "Stah\u00b7les", "Klin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "VAFIN", "PPER", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll trennen, was in tausend F\u00e4den Eins,", "tokens": ["Soll", "tren\u00b7nen", ",", "was", "in", "tau\u00b7send", "F\u00e4\u00b7den", "Eins", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PRELS", "APPR", "CARD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00e4chtig kein Gedanke, da\u00df er dringe", "tokens": ["So", "m\u00e4ch\u00b7tig", "kein", "Ge\u00b7dan\u00b7ke", ",", "da\u00df", "er", "drin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIAT", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verg\u00e4llend in den Becher reinen Weins;", "tokens": ["Ver\u00b7g\u00e4l\u00b7lend", "in", "den", "Be\u00b7cher", "rei\u00b7nen", "Weins", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Leben ist so kurz, das Gl\u00fcck so selten,", "tokens": ["Das", "Le\u00b7ben", "ist", "so", "kurz", ",", "das", "Gl\u00fcck", "so", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So gro\u00dfes Kleinod, einmal sein statt gelten!", "tokens": ["So", "gro\u00b7\u00dfes", "Klei\u00b7nod", ",", "ein\u00b7mal", "sein", "statt", "gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hat das Geschick uns, wie in frevlem Witze,", "tokens": ["Hat", "das", "Ge\u00b7schick", "uns", ",", "wie", "in", "frev\u00b7lem", "Wit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf feindlich starre Pole gleich erh\u00f6ht,", "tokens": ["Auf", "feind\u00b7lich", "star\u00b7re", "Po\u00b7le", "gleich", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wisse, dort, dort auf der Scheidung Spitze", "tokens": ["So", "wis\u00b7se", ",", "dort", ",", "dort", "auf", "der", "Schei\u00b7dung", "Spit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "$,", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herrscht, K\u00f6nig \u00fcber Alle, der Magnet,", "tokens": ["Herrscht", ",", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "Al\u00b7le", ",", "der", "Mag\u00b7net", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "PIS", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht fr\u00e4gt er ob ihn Fels und Strom gef\u00e4hrde,", "tokens": ["Nicht", "fr\u00e4gt", "er", "ob", "ihn", "Fels", "und", "Strom", "ge\u00b7f\u00e4hr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Stral f\u00e4hrt mitten er durchs Herz der Erde.", "tokens": ["Ein", "Stral", "f\u00e4hrt", "mit\u00b7ten", "er", "durchs", "Herz", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Blick' in mein Auge \u2014 ist es nicht das deine,", "tokens": ["Blick'", "in", "mein", "Au\u00b7ge", "ist", "es", "nicht", "das", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$(", "VAFIN", "PPER", "PTKNEG", "ART", "PPOSAT", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ist nicht mein Z\u00fcrnen selber deinem gleich?", "tokens": ["Ist", "nicht", "mein", "Z\u00fcr\u00b7nen", "sel\u00b7ber", "dei\u00b7nem", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du l\u00e4chelst \u2014 und dein L\u00e4cheln ist das meine,", "tokens": ["Du", "l\u00e4\u00b7chelst", "und", "dein", "L\u00e4\u00b7cheln", "ist", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "An gleicher Lust und gleichem Sinnen reich;", "tokens": ["An", "glei\u00b7cher", "Lust", "und", "glei\u00b7chem", "Sin\u00b7nen", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wor\u00fcber alle Lippen freundlich scherzen,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "al\u00b7le", "Lip\u00b7pen", "freund\u00b7lich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wir f\u00fchlen Heilger es im eignen Herzen.", "tokens": ["Wir", "f\u00fch\u00b7len", "Heil\u00b7ger", "es", "im", "eig\u00b7nen", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Pollux und Castor, \u2014 wechselnd Gl\u00fchn und Bleichen,", "tokens": ["Pol\u00b7lux", "und", "Cas\u00b7tor", ",", "wech\u00b7selnd", "Gl\u00fchn", "und", "Blei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "$(", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Einen Licht geraubt dem Andern nur,", "tokens": ["Des", "Ei\u00b7nen", "Licht", "ge\u00b7raubt", "dem", "An\u00b7dern", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und doch der allerfr\u00f6mmsten Treue Zeichen. \u2014", "tokens": ["Und", "doch", "der", "al\u00b7ler\u00b7fr\u00b7\u00f6mms\u00b7ten", "Treu\u00b7e", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So reiche mir die Hand, mein Dioskur!", "tokens": ["So", "rei\u00b7che", "mir", "die", "Hand", ",", "mein", "Dios\u00b7kur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mag erneuern sich die holde Mythe,", "tokens": ["Und", "mag", "er\u00b7neu\u00b7ern", "sich", "die", "hol\u00b7de", "My\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo \u00fcberm Helm die Zwillingsflamme gl\u00fchte.", "tokens": ["Wo", "\u00fc\u00b7berm", "Helm", "die", "Zwil\u00b7lings\u00b7flam\u00b7me", "gl\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}