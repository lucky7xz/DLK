{"dta.poem.12346": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Ringlein und F\u00e4hnlein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor Tags ich h\u00f6rt, in Liebes Port, wohl diese\nWort               ", "tokens": ["Vor", "Tags", "ich", "h\u00f6rt", ",", "in", "Lie\u00b7bes", "Port", ",", "wohl", "die\u00b7se", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,", "APPR", "NE", "NE", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von W\u00e4chters Mund erklingen:", "tokens": ["Von", "W\u00e4ch\u00b7ters", "Mund", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eist Jemand je, verborgen hie, der achte wie", "tokens": ["\u201e", "ist", "Je\u00b7mand", "je", ",", "ver\u00b7bor\u00b7gen", "hie", ",", "der", "ach\u00b7te", "wie"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "PIS", "ADV", "$,", "VVPP", "ADV", "$,", "ART", "ADJA", "KOKOM"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eer m\u00f6g' hindannen sprengen,", "tokens": ["\u201e", "er", "m\u00f6g'", "hin\u00b7dan\u00b7nen", "spren\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eder Tag gar hell, will kommen schnell,", "tokens": ["\u201e", "der", "Tag", "gar", "hell", ",", "will", "kom\u00b7men", "schnell", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "ADJD", "$,", "VMFIN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ewer liebend ruht, in Frauen Hut,", "tokens": ["\u201e", "wer", "lie\u00b7bend", "ruht", ",", "in", "Frau\u00b7en", "Hut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJD", "VVFIN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ela\u00df bald das Bett erkalten.\u201c", "tokens": ["\u201e", "la\u00df", "bald", "das", "Bett", "er\u00b7kal\u00b7ten", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201edas Firmament, schnell und behend, von Orient,", "tokens": ["\u201e", "das", "Fir\u00b7ma\u00b7ment", ",", "schnell", "und", "be\u00b7hend", ",", "von", "O\u00b7rient", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eim weissen Schein herpranget,", "tokens": ["\u201e", "im", "weis\u00b7sen", "Schein", "her\u00b7pran\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201ef\u00fcrwahr ich sag', aus gr\u00fcnem Hag, der Lerchen Schlag,", "tokens": ["\u201e", "f\u00fcr\u00b7wahr", "ich", "sag'", ",", "aus", "gr\u00fc\u00b7nem", "Hag", ",", "der", "Ler\u00b7chen", "Schlag", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eden jungen Tag empfanget.", "tokens": ["\u201e", "den", "jun\u00b7gen", "Tag", "emp\u00b7fan\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edrum eil' vom Ort, wer noch im Hort", "tokens": ["\u201e", "drum", "eil'", "vom", "Ort", ",", "wer", "noch", "im", "Hort"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PAV", "VVFIN", "APPRART", "NN", "$,", "PWS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eder Liebe sey, eh Jammers-Schrei", "tokens": ["\u201e", "der", "Lie\u00b7be", "sey", ",", "eh", "Jam\u00b7mer\u00b7sSchrei"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eden Muth ihm m\u00f6g zerspalten.\u201c", "tokens": ["\u201e", "den", "Muth", "ihm", "m\u00f6g", "zer\u00b7spal\u00b7ten", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des W\u00e4chters Kund in Herzensgrund mich tief ver-", "tokens": ["Des", "W\u00e4ch\u00b7ters", "Kund", "in", "Her\u00b7zens\u00b7grund", "mich", "tief", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und all mein Freud zerst\u00f6ret,", "tokens": ["Und", "all", "mein", "Freud", "zer\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Lichtes Neid, will da\u00df ich scheid, h\u00f6r s\u00fc\u00dfe Maid,", "tokens": ["Des", "Lich\u00b7tes", "Neid", ",", "will", "da\u00df", "ich", "scheid", ",", "h\u00f6r", "s\u00fc\u00b7\u00dfe", "Maid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie will vor Leid nicht h\u00f6ren!", "tokens": ["Sie", "will", "vor", "Leid", "nicht", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich zu mir schm\u00fcckt, gar sch\u00e4mlich blickt,", "tokens": ["Sich", "zu", "mir", "schm\u00fcckt", ",", "gar", "sch\u00e4m\u00b7lich", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht mehr schlief, gar schnell ich rief:", "tokens": ["Und", "nicht", "mehr", "schlief", ",", "gar", "schnell", "ich", "rief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201each Gott, wir han verschlafen!\u201c", "tokens": ["\u201e", "ach", "Gott", ",", "wir", "han", "ver\u00b7schla\u00b7fen", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "NN", "$,", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zur Hand sich ragt, die werthe Magd, hierauf sie", "tokens": ["Zur", "Hand", "sich", "ragt", ",", "die", "wert\u00b7he", "Magd", ",", "hier\u00b7auf", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "NN", "$,", "PAV", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u201egut W\u00e4chter la\u00df dein Schimpfen!", "tokens": ["\u201e", "gut", "W\u00e4ch\u00b7ter", "la\u00df", "dein", "Schimp\u00b7fen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eum alle Welt, den Tag nicht meld, eh da\u00df das Feld", "tokens": ["\u201e", "um", "al\u00b7le", "Welt", ",", "den", "Tag", "nicht", "meld", ",", "eh", "da\u00df", "das", "Feld"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PIAT", "NN", "$,", "ART", "NN", "PTKNEG", "VVFIN", "$,", "KOUS", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ein k\u00fchlem Thau thut glimmen.", "tokens": ["\u201e", "in", "k\u00fch\u00b7lem", "Thau", "thut", "glim\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201edie Zeit ist klein, da\u00df ich und mein", "tokens": ["\u201e", "die", "Zeit", "ist", "klein", ",", "da\u00df", "ich", "und", "mein"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "KON", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201egeselle gut, hie han geruht", "tokens": ["\u201e", "ge\u00b7sel\u00b7le", "gut", ",", "hie", "han", "ge\u00b7ruht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201ein ehrenreicher Wonne.\u201c", "tokens": ["\u201e", "in", "eh\u00b7ren\u00b7rei\u00b7cher", "Won\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der W\u00e4chter sprach: \u201eFrau thu zur Sach, denn", "tokens": ["Der", "W\u00e4ch\u00b7ter", "sprach", ":", "\u201e", "Frau", "thu", "zur", "Sach", ",", "denn"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "VVFIN", "APPRART", "NN", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ehat k\u00fchler Thau umgeben,", "tokens": ["\u201e", "hat", "k\u00fch\u00b7ler", "Thau", "um\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eseit du nun hast ein fremden Gast, so hab nicht Rast,", "tokens": ["\u201e", "seit", "du", "nun", "hast", "ein", "frem\u00b7den", "Gast", ",", "so", "hab", "nicht", "Rast", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ehei\u00df' ihn von dannen streben.", "tokens": ["\u201e", "hei\u00df'", "ihn", "von", "dan\u00b7nen", "stre\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201eich seh manch Thier in dem Revier", "tokens": ["\u201e", "ich", "seh", "manch", "Thier", "in", "dem", "Re\u00b7vier"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201evon Hohl zu Hohl ja schl\u00fcpfen wohl,", "tokens": ["\u201e", "von", "Hohl", "zu", "Hohl", "ja", "schl\u00fcp\u00b7fen", "wohl", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "APPR", "NN", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201edas zeiget mir die Sonne.\u201c", "tokens": ["\u201e", "das", "zei\u00b7get", "mir", "die", "Son\u00b7ne", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erst ward zur Stund, uns Jammer kund im Freu-", "tokens": ["Erst", "ward", "zur", "Stund", ",", "uns", "Jam\u00b7mer", "kund", "im", "Freu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$,", "PPER", "NN", "PTKVZ", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da wir den Tag ansahen,", "tokens": ["Da", "wir", "den", "Tag", "an\u00b7sa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl Mund an Mund, gar s\u00fc\u00df verwundt im Ku\u00df ge-", "tokens": ["Wohl", "Mund", "an", "Mund", ",", "gar", "s\u00fc\u00df", "ver\u00b7wundt", "im", "Ku\u00df", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "NN", "$,", "ADV", "ADJD", "VVPP", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sund,", "tokens": ["sund", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und liebliches Umfahen,", "tokens": ["Und", "lieb\u00b7li\u00b7ches", "Um\u00b7fa\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ward Liebes-Scherz in Scheidens-Schmerz,", "tokens": ["Ward", "Lie\u00b7bes\u00b7Scherz", "in", "Schei\u00b7den\u00b7sSchmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gar treu getheilt und schnell ereilt.", "tokens": ["Gar", "treu", "ge\u00b7theilt", "und", "schnell", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach edle Frucht du weiblich Zucht, hin auf die", "tokens": ["Ach", "ed\u00b7le", "Frucht", "du", "weib\u00b7lich", "Zucht", ",", "hin", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "PPER", "ADJD", "NN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ich mich leider kehren,", "tokens": ["Mu\u00df", "ich", "mich", "lei\u00b7der", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott durch sein G\u00fct, dir wohl beh\u00fct dein rein Gem\u00fcth,", "tokens": ["Gott", "durch", "sein", "G\u00fct", ",", "dir", "wohl", "be\u00b7h\u00fct", "dein", "rein", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Heil m\u00f6g er dir mehren,", "tokens": ["Dein", "Heil", "m\u00f6g", "er", "dir", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "F\u00fcrwahr ich will, bis an mein Ziel,", "tokens": ["F\u00fcr\u00b7wahr", "ich", "will", ",", "bis", "an", "mein", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Diener seyn, Gnad! Fraue mein,", "tokens": ["Dein", "Die\u00b7ner", "seyn", ",", "Gnad", "!", "Frau\u00b7e", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "