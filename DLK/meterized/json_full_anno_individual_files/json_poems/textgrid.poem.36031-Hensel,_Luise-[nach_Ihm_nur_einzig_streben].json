{"textgrid.poem.36031": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[nach Ihm nur einzig streben]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Ihm nur einzig streben,", "tokens": ["Nach", "Ihm", "nur", "ein\u00b7zig", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Treu seinen Willen tun,", "tokens": ["Treu", "sei\u00b7nen", "Wil\u00b7len", "tun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr ihn allein nur leben,", "tokens": ["F\u00fcr", "ihn", "al\u00b7lein", "nur", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seinem Scho\u00dfe ruhn \u2013", "tokens": ["In", "sei\u00b7nem", "Scho\u00b7\u00dfe", "ruhn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df kein and'res Wissen,", "tokens": ["Ich", "wei\u00df", "kein", "an\u00b7d'\u00b7res", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich will kein and'res Heil;", "tokens": ["Ich", "will", "kein", "an\u00b7d'\u00b7res", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "$."], "meter": "