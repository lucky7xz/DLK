{"dta.poem.12452": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Von Volksliedern .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Potz tausend, schaut fort l\u00e4uft die Katz,", "tokens": ["Potz", "tau\u00b7send", ",", "schaut", "fort", "l\u00e4uft", "die", "Katz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "PTKVZ", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh Plasl lauf, halts auf,", "tokens": ["Geh", "Plasl", "lauf", ",", "halts", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ein jeder Mensch hat seinen Schatz,", "tokens": ["Ein", "je\u00b7der", "Mensch", "hat", "sei\u00b7nen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Lebenslauf.", "tokens": ["In", "die\u00b7sem", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als d' Jungfer noch ein Jungfer war,", "tokens": ["Als", "d'", "Jung\u00b7fer", "noch", "ein", "Jung\u00b7fer", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Hats keine mehr seyn m\u00f6gen,", "tokens": ["Hats", "kei\u00b7ne", "mehr", "seyn", "m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich wust es alles auf ein Haar,", "tokens": ["Ich", "wust", "es", "al\u00b7les", "auf", "ein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr Pelz der hing voll Regen.", "tokens": ["Ihr", "Pelz", "der", "hing", "voll", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}