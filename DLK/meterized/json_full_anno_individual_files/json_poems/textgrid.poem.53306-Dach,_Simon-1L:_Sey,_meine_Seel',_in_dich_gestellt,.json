{"textgrid.poem.53306": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sey, meine Seel', in dich gestellt,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey, meine Seel', in dich gestellt,", "tokens": ["Sey", ",", "mei\u00b7ne", "Seel'", ",", "in", "dich", "ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beruff zusammen die Gedancken,", "tokens": ["Be\u00b7ruff", "zu\u00b7sam\u00b7men", "die", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Thue einig die\u00df, nimb vor die Welt,", "tokens": ["Thue", "ei\u00b7nig", "die\u00df", ",", "nimb", "vor", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PDS", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchsuch jhr Wesen, Thun vnd Wancken,", "tokens": ["Durch\u00b7such", "jhr", "We\u00b7sen", ",", "Thun", "vnd", "Wan\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schaw ob sie auch was anders sey", "tokens": ["Schaw", "ob", "sie", "auch", "was", "an\u00b7ders", "sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "ADV", "PWS", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Eitelheit vnd Triegerey.", "tokens": ["Als", "Ei\u00b7tel\u00b7heit", "vnd", "Trie\u00b7ge\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergn\u00fcget Ehre meinen Sinn?", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "Eh\u00b7re", "mei\u00b7nen", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je gr\u00f6sser' Ehr', je gr\u00f6ssre Plagen.", "tokens": ["Je", "gr\u00f6s\u00b7ser'", "Ehr'", ",", "je", "gr\u00f6ss\u00b7re", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gro\u00df Gut? wie reich ich jmmer bin,", "tokens": ["Gro\u00df", "Gut", "?", "wie", "reich", "ich", "jm\u00b7mer", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$.", "PWAV", "ADJD", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wil ich doch noch mehr erjagen.", "tokens": ["So", "wil", "ich", "doch", "noch", "mehr", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wollust Thun? von jhrer Macht", "tokens": ["Der", "Wol\u00b7lust", "Thun", "?", "von", "jhrer", "Macht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wird Leib vnd Seele durchgebracht.", "tokens": ["Wird", "Leib", "vnd", "See\u00b7le", "durch\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gesund vnd frisch von Leibe seyn?", "tokens": ["Ge\u00b7sund", "vnd", "frisch", "von", "Lei\u00b7be", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hilfft es mir, wann ich mu\u00df alten?", "tokens": ["Was", "hilfft", "es", "mir", ",", "wann", "ich", "mu\u00df", "al\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VMFIN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Summa: Arbeit, M\u00fch vnd Pein", "tokens": ["In", "Sum\u00b7ma", ":", "Ar\u00b7beit", ",", "M\u00fch", "vnd", "Pein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$.", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht man in allen Dingen walten.", "tokens": ["Sieht", "man", "in", "al\u00b7len", "Din\u00b7gen", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd w\u00e4re ja was ausser Noht,", "tokens": ["Vnd", "w\u00e4\u00b7re", "ja", "was", "aus\u00b7ser", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So frisst vns s\u00e4mptlich doch der Tod.", "tokens": ["So", "frisst", "vns", "s\u00e4mpt\u00b7lich", "doch", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie gro\u00df wir sind, wie sch\u00f6n, wie zart,", "tokens": ["Wie", "gro\u00df", "wir", "sind", ",", "wie", "sch\u00f6n", ",", "wie", "zart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So eilt er mit vns nach der Erden;", "tokens": ["So", "eilt", "er", "mit", "vns", "nach", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was wir durch allen Flei\u00df erspahrt,", "tokens": ["Was", "wir", "durch", "al\u00b7len", "Flei\u00df", "er\u00b7spahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df andern hinderlassen werden.", "tokens": ["Mu\u00df", "an\u00b7dern", "hin\u00b7der\u00b7las\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hie hilfft kein Diamanten Thor,", "tokens": ["Hie", "hilfft", "kein", "Di\u00b7a\u00b7man\u00b7ten", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Schloss, kein Fel\u00df, kein Hochmuth vor.", "tokens": ["Kein", "Schloss", ",", "kein", "Fel\u00df", ",", "kein", "Hoch\u00b7muth", "vor", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zeuch, J\u00fcngling, du nach Wei\u00dfheit aus,", "tokens": ["Zeuch", ",", "J\u00fcng\u00b7ling", ",", "du", "nach", "Wei\u00df\u00b7heit", "aus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd h\u00e4rt durch Arbeit deine Jugend,", "tokens": ["Vnd", "h\u00e4rt", "durch", "Ar\u00b7beit", "dei\u00b7ne", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm wieder heim, erf\u00fcll dein Hau\u00df", "tokens": ["Komm", "wie\u00b7der", "heim", ",", "er\u00b7f\u00fcll", "dein", "Hau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ruhm vnd adelicher Tugend,", "tokens": ["Mit", "Ruhm", "vnd", "a\u00b7de\u00b7li\u00b7cher", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd du, wend alle deine Macht", "tokens": ["Vnd", "du", ",", "wend", "al\u00b7le", "dei\u00b7ne", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff Waffen vnd auff k\u00fchne Schlacht.", "tokens": ["Auff", "Waf\u00b7fen", "vnd", "auff", "k\u00fch\u00b7ne", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimb du den Handel vor die Hand,", "tokens": ["Nimb", "du", "den", "Han\u00b7del", "vor", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeuch \u00fcber Meer, reis' allenthalben:", "tokens": ["Zeuch", "\u00fc\u00b7ber", "Meer", ",", "reis'", "al\u00b7len\u00b7thal\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd du ergreiff den Liebes Stand,", "tokens": ["Vnd", "du", "er\u00b7greiff", "den", "Lie\u00b7bes", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schm\u00fcck deinen Leib mit Seid' vnd Salben,", "tokens": ["Schm\u00fcck", "dei\u00b7nen", "Leib", "mit", "Seid'", "vnd", "Sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd wisst daneben aller seit:", "tokens": ["Vnd", "wisst", "da\u00b7ne\u00b7ben", "al\u00b7ler", "seit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die\u00df alles w\u00e4hret kurtze Zeit.", "tokens": ["Die\u00df", "al\u00b7les", "w\u00e4h\u00b7ret", "kurt\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn jhr dahin gestrecket liegt,", "tokens": ["Wenn", "jhr", "da\u00b7hin", "ge\u00b7stre\u00b7cket", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblasst vnd h\u00e4\u00dflich anzuschawen,", "tokens": ["Er\u00b7blasst", "vnd", "h\u00e4\u00df\u00b7lich", "an\u00b7zu\u00b7scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da\u00df die Verwesung euch besiegt", "tokens": ["Da\u00df", "die", "Ver\u00b7we\u00b7sung", "euch", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd jederman mu\u00df vor euch grawen,", "tokens": ["Vnd", "je\u00b7der\u00b7man", "mu\u00df", "vor", "euch", "gra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mein sagt, was Nutzes euch doch giebt", "tokens": ["Mein", "sagt", ",", "was", "Nut\u00b7zes", "euch", "doch", "giebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "$,", 