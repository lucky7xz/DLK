, "line.6": {"text": "Die nichts als schn\u00f6des Ding getahn?", "tokens": ["Die", "nichts", "als", "schn\u00f6\u00b7des", "Ding", "ge\u00b7tahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kehrt vmb, es ist sehr hohe Zeit!", "tokens": ["Kehrt", "vmb", ",", "es", "ist", "sehr", "ho\u00b7he", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt augenblicklich euch zu Sinnen,", "tokens": ["F\u00fchrt", "au\u00b7gen\u00b7blick\u00b7lich", "euch", "zu", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie fl\u00fcchtig jhr sampt allem seyd,", "tokens": ["Wie", "fl\u00fcch\u00b7tig", "jhr", "sampt", "al\u00b7lem", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht Gott durch Busse zu gewinnen,", "tokens": ["Sucht", "Gott", "durch", "Bus\u00b7se", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd liebt den Nechsten wie man sol,", "tokens": ["Vnd", "liebt", "den", "Nechs\u00b7ten", "wie", "man", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "PIS", "VMFIN", "$,"], "meter": "-+-+-