{"textgrid.poem.62465": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn alle untreu werden,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn alle untreu werden,", "tokens": ["Wenn", "al\u00b7le", "un\u00b7treu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleib' ich euch doch treu,", "tokens": ["So", "bleib'", "ich", "euch", "doch", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df immer noch auf Erden", "tokens": ["Da\u00df", "im\u00b7mer", "noch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr euch ein Streiter sei.", "tokens": ["F\u00fcr", "euch", "ein", "Strei\u00b7ter", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gef\u00e4hrten meiner Jugend,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7ten", "mei\u00b7ner", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Bilder be\u00dfrer Zeit,", "tokens": ["Ihr", "Bil\u00b7der", "be\u00df\u00b7rer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die mich zu M\u00e4nne