{"dta.poem.21270": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Hymen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.57", "en:0.42"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Ly&#228;ens und Cytherens Sohn,", "tokens": ["Ly", "&#228;", "ens", "und", "Cy\u00b7the\u00b7rens", "Sohn", ","], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "KON", "NE", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Im sch\u00f6nsten Rausch geboren,", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Rausch", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott Hymen, der du dir zum Thron", "tokens": ["Gott", "Hy\u00b7men", ",", "der", "du", "dir", "zum", "Thron"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Hochzeitbett erkohren!", "tokens": ["Das", "Hoch\u00b7zeit\u00b7bett", "er\u00b7koh\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir fleht der sorgenvolle Greis:", "tokens": ["Dir", "fleht", "der", "sor\u00b7gen\u00b7vol\u00b7le", "Greis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Stifter der Geschlechter,", "tokens": ["O", "Stif\u00b7ter", "der", "Ge\u00b7schlech\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm, was ich nicht zu sch\u00fctzen weiss,", "tokens": ["Nimm", ",", "was", "ich", "nicht", "zu", "sch\u00fct\u00b7zen", "weiss", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mir die grossen T\u00f6chter!", "tokens": ["Nimm", "mir", "die", "gros\u00b7sen", "T\u00f6ch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dir schm\u00fcckt das fromme M\u00e4dchen sich", "tokens": ["Dir", "schm\u00fcckt", "das", "from\u00b7me", "M\u00e4d\u00b7chen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey seinem Morgenliede;", "tokens": ["Bey", "sei\u00b7nem", "Mor\u00b7gen\u00b7lie\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der weise J\u00fcngling hofft auf dich,", "tokens": ["Der", "wei\u00b7se", "J\u00fcng\u00b7ling", "hofft", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des falschen Amors m\u00fcde.", "tokens": ["Des", "fal\u00b7schen", "A\u00b7mors", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dich rufen junge Wittwen an", "tokens": ["Dich", "ru\u00b7fen", "jun\u00b7ge", "Witt\u00b7wen", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im hochbetr\u00fcbten Schleyer;", "tokens": ["Im", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ten", "Schle\u00b7yer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Im Flohr bekennt der Trauermann", "tokens": ["Im", "Flohr", "be\u00b7kennt", "der", "Trau\u00b7er\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir sein gewaltig Feuer.", "tokens": ["Dir", "sein", "ge\u00b7wal\u00b7tig", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du, mehr als andre G\u00f6tter werth,", "tokens": ["Du", ",", "mehr", "als", "and\u00b7re", "G\u00f6t\u00b7ter", "werth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "KOUS", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir flehen auch die Prinzen:", "tokens": ["Dir", "fle\u00b7hen", "auch", "die", "Prin\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erf\u00fclle, was der Krieg geleert,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", ",", "was", "der", "Krieg", "ge\u00b7leert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erf\u00fcll\u2019 uns die Provinzen!", "tokens": ["Er\u00b7f\u00fcll'", "uns", "die", "Pro\u00b7vin\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O! wenn dich noch ein Opferschmaus", "tokens": ["O", "!", "wenn", "dich", "noch", "ein", "Op\u00b7fer\u00b7schmaus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab vom Himmel ziehet:", "tokens": ["Her\u00b7ab", "vom", "Him\u00b7mel", "zie\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So komm in meines Leukons Haus,", "tokens": ["So", "komm", "in", "mei\u00b7nes", "Leu\u00b7kons", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der am Altare knieet!", "tokens": ["Der", "am", "Al\u00b7ta\u00b7re", "kni\u00b7e\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O komm! zwey Ring\u2019 an Einer Hand,", "tokens": ["O", "komm", "!", "zwey", "Ring'", "an", "Ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und um die Schl\u00e4fe Myrthen,", "tokens": ["Und", "um", "die", "Schl\u00e4\u00b7fe", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und um den Arm ein goldnes Band,", "tokens": ["Und", "um", "den", "Arm", "ein", "gold\u00b7nes", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Knie der Braut zu g\u00fcrten,", "tokens": ["Das", "Knie", "der", "Braut", "zu", "g\u00fcr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die, wann von Wein und Liebe voll,", "tokens": ["Die", ",", "wann", "von", "Wein", "und", "Lie\u00b7be", "voll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gast zu viel begehret,", "tokens": ["Ein", "Gast", "zu", "viel", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sie doch etwas missen soll,", "tokens": ["Und", "sie", "doch", "et\u00b7was", "mis\u00b7sen", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am liebsten Band entbehret.", "tokens": ["Am", "liebs\u00b7ten", "Band", "ent\u00b7beh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Schaar der trunknen R\u00e4uber theilt", "tokens": ["Die", "Schaar", "der", "trunk\u00b7nen", "R\u00e4u\u00b7ber", "theilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in die goldne Beute:", "tokens": ["Sich", "in", "die", "gold\u00b7ne", "Beu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie flieht indess, der Liebling eilt,", "tokens": ["Sie", "flieht", "in\u00b7dess", ",", "der", "Lieb\u00b7ling", "eilt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebt ihr das Geleite.", "tokens": ["Und", "giebt", "ihr", "das", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}