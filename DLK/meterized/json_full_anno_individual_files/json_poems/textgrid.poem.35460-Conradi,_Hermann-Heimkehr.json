{"textgrid.poem.35460": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst so brav, da ich dich einst verlassen, \u2013", "tokens": ["Du", "warst", "so", "brav", ",", "da", "ich", "dich", "einst", "ver\u00b7las\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich die Heimat mied.", "tokens": ["Da", "ich", "die", "Hei\u00b7mat", "mied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heut kehr' ich heim ... Der Sturm durchgellt die Gassen,", "tokens": ["Heut", "kehr'", "ich", "heim", "...", "Der", "Sturm", "durch\u00b7gellt", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jauchzt sein Rebellenlied ...", "tokens": ["Jauchzt", "sein", "Re\u00b7bel\u00b7len\u00b7lied", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Doch durch das Windgeheul dr\u00f6hnt mir entgegen", "tokens": ["Doch", "durch", "das", "Wind\u00b7ge\u00b7heul", "dr\u00f6hnt", "mir", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein r\u00fcdes Zechgebr\u00fcll ...", "tokens": ["Ein", "r\u00fc\u00b7des", "Zech\u00b7ge\u00b7br\u00fcll", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich geh' ihm nach auf den verschneiten Wegen", "tokens": ["Ich", "geh'", "ihm", "nach", "auf", "den", "ver\u00b7schnei\u00b7ten", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-----+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und lausche still ...", "tokens": ["Und", "lau\u00b7sche", "still", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich st\u00f6\u00dft sich siedendhei\u00df zum Herzen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "st\u00f6\u00dft", "sich", "sie\u00b7dend\u00b7hei\u00df", "zum", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein st\u00fcrmisch Blut ...", "tokens": ["Mein", "st\u00fcr\u00b7misch", "Blut", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich sehe dich ... im Blutlicht flackernder Kerzen", "tokens": ["Ich", "se\u00b7he", "dich", "...", "im", "Blut\u00b7licht", "fla\u00b7ckern\u00b7der", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Inmitten Bubenbrut ...", "tokens": ["In\u00b7mit\u00b7ten", "Bu\u00b7ben\u00b7brut", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Maria, du? ... Zur\u00fcck nun, Heimatskehrer!", "tokens": ["Ma\u00b7ria", ",", "du", "?", "...", "Zu\u00b7r\u00fcck", "nun", ",", "Hei\u00b7mats\u00b7keh\u00b7rer", "!"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPER", "$.", "$(", "NN", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Was z\u00f6gerst du?", "tokens": ["Was", "z\u00f6\u00b7gerst", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Welt, die weite Welt ist nirgends leerer \u2013", "tokens": ["Die", "Welt", ",", "die", "wei\u00b7te", "Welt", "ist", "nir\u00b7gends", "lee\u00b7rer", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schreit' zu \u2013 schreit' zu! ...", "tokens": ["Schreit'", "zu", "\u2013", "schreit'", "zu", "!", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PTKZU", "$(", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Du warst so brav, da ich dich einst verlassen, \u2013", "tokens": ["Du", "warst", "so", "brav", ",", "da", "ich", "dich", "einst", "ver\u00b7las\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS