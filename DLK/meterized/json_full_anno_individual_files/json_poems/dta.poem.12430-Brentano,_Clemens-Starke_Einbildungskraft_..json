{"dta.poem.12430": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Starke Einbildungskraft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen. Hast gesagt du willst mich nehmen,               ", "tokens": ["M\u00e4d\u00b7chen", ".", "Hast", "ge\u00b7sagt", "du", "willst", "mich", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "VVPP", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sobald der Sommer kommt:,:", "tokens": ["So\u00b7bald", "der", "Som\u00b7mer", "kommt", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sommer ist gekommen,", "tokens": ["Der", "Som\u00b7mer", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast mich nicht genommen,", "tokens": ["Du", "hast", "mich", "nicht", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Geh Buble, geh nehm mich! Gelt ja", "tokens": ["Geh", "Bub\u00b7le", ",", "geh", "nehm", "mich", "!", "Gelt", "ja"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "VVFIN", "VVFIN", "PPER", "$.", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du nimmst mich noch.", "tokens": ["Du", "nimmst", "mich", "noch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bube. Wie soll ich dich denn nehmen,               ", "tokens": ["Bu\u00b7be", ".", "Wie", "soll", "ich", "dich", "denn", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und wenn ich dich schon hab:,:", "tokens": ["Und", "wenn", "ich", "dich", "schon", "hab", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VAFIN", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn wenn ich halt an dich gedenk,", "tokens": ["Denn", "wenn", "ich", "halt", "an", "dich", "ge\u00b7denk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wenn ich halt an dich gedenk,", "tokens": ["Denn", "wenn", "ich", "halt", "an", "dich", "ge\u00b7denk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mein ich, so mein ich, ich mein,", "tokens": ["So", "mein", "ich", ",", "so", "mein", "ich", ",", "ich", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "$,", "ADV", "PPOSAT", "PPER", "$,", "PPER", "PPOSAT", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ich w\u00e4r bey dir.", "tokens": ["Ich", "w\u00e4r", "bey", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}