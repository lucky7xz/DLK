{"textgrid.poem.51029": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, das ist der alte Kirchhof,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, das ist der alte Kirchhof,", "tokens": ["Ja", ",", "das", "ist", "der", "al\u00b7te", "Kirch\u00b7hof", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in blauer Flut sich spiegelt,", "tokens": ["Der", "in", "blau\u00b7er", "Flut", "sich", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Offen steht sein morsches Gitter,", "tokens": ["Of\u00b7fen", "steht", "sein", "mor\u00b7sches", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand ist, der es verriegelt!", "tokens": ["Nie\u00b7mand", "ist", ",", "der", "es", "ver\u00b7rie\u00b7gelt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier der kleine Berg voll Rosen,", "tokens": ["Hier", "der", "klei\u00b7ne", "Berg", "voll", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dicht und \u00fcppig aufgesprossen:", "tokens": ["Dicht", "und", "\u00fcp\u00b7pig", "auf\u00b7ge\u00b7spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drunter liegt die wei\u00dfe Lilie,", "tokens": ["Drun\u00b7ter", "liegt", "die", "wei\u00b7\u00dfe", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine Sage schon, verschlossen.", "tokens": ["Ei\u00b7ne", "Sa\u00b7ge", "schon", ",", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um die Sage, um ein M\u00e4rchen,", "tokens": ["Um", "die", "Sa\u00b7ge", ",", "um", "ein", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Tod hab ich geworben,", "tokens": ["Um", "den", "Tod", "hab", "ich", "ge\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so sei mein treues Hoffen", "tokens": ["Und", "so", "sei", "mein", "treu\u00b7es", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrhin tot und abgestorben!", "tokens": ["F\u00fcr\u00b7hin", "tot", "und", "ab\u00b7ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd rei\u00df ich aus dem Busen", "tokens": ["Zit\u00b7ternd", "rei\u00df", "ich", "aus", "dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch die letzten zarten Bl\u00fcten,", "tokens": ["Noch", "die", "letz\u00b7ten", "zar\u00b7ten", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gebe sie dem toten Liebchen", "tokens": ["Ge\u00b7be", "sie", "dem", "to\u00b7ten", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis zum J\u00fcngsten Tag zu h\u00fcten!", "tokens": ["Bis", "zum", "J\u00fcng\u00b7sten", "Tag", "zu", "h\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schwarzer G\u00e4rtner, Grabespfleger,", "tokens": ["Schwar\u00b7zer", "G\u00e4rt\u00b7ner", ",", "Gra\u00b7bes\u00b7pfle\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df, o la\u00df das Grab verwildern!", "tokens": ["La\u00df", ",", "o", "la\u00df", "das", "Grab", "ver\u00b7wil\u00b7dern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "FM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine wermutbittern Schauer", "tokens": ["Sei\u00b7ne", "wer\u00b7mut\u00b7bit\u00b7tern", "Schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll kein Lenz mehr freundlich mildern!", "tokens": ["Soll", "kein", "Lenz", "mehr", "freund\u00b7lich", "mil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Binde nicht mehr diese Zweige,", "tokens": ["Bin\u00b7de", "nicht", "mehr", "die\u00b7se", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4nke nicht mehr diese Rosen!", "tokens": ["Tr\u00e4n\u00b7ke", "nicht", "mehr", "die\u00b7se", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit dem verdorrten Kranze", "tokens": ["Und", "mit", "dem", "ver\u00b7dorr\u00b7ten", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mag der kalte Nordwind kosen!", "tokens": ["Mag", "der", "kal\u00b7te", "Nord\u00b7wind", "ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gegen Morgen, gegen Morgen", "tokens": ["Ge\u00b7gen", "Mor\u00b7gen", ",", "ge\u00b7gen", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau ich trotzig in die Sonne:", "tokens": ["Schau", "ich", "trot\u00b7zig", "in", "die", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ergl\u00e4nzt sie wild und feurig,", "tokens": ["Wie", "er\u00b7gl\u00e4nzt", "sie", "wild", "und", "feu\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd in Gewitterwonne!", "tokens": ["L\u00e4\u00b7chelnd", "in", "Ge\u00b7wit\u00b7ter\u00b7won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fchn gewappnet um die Heldin", "tokens": ["K\u00fchn", "ge\u00b7wapp\u00b7net", "um", "die", "Hel\u00b7din"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich die Wetterwolken scharen,", "tokens": ["Sich", "die", "Wet\u00b7ter\u00b7wol\u00b7ken", "scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie auf weitem Ozeane", "tokens": ["Wie", "auf", "wei\u00b7tem", "O\u00b7zea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Drohende Armaden fahren!", "tokens": ["Dro\u00b7hen\u00b7de", "Ar\u00b7ma\u00b7den", "fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Vor mir liegt das rauhe Leben,", "tokens": ["Vor", "mir", "liegt", "das", "rau\u00b7he", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt die Zeit die hohen Wogen,", "tokens": ["Schl\u00e4gt", "die", "Zeit", "die", "ho\u00b7hen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kreist die Welt mit ihren Sternen;", "tokens": ["Kreist", "die", "Welt", "mit", "ih\u00b7ren", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutig bin ich ausgezogen,", "tokens": ["Mu\u00b7tig", "bin", "ich", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Biete Stirn und Herz den St\u00fcrmen,", "tokens": ["Bie\u00b7te", "Stirn", "und", "Herz", "den", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasse meine Wimpel wehen,", "tokens": ["Las\u00b7se", "mei\u00b7ne", "Wim\u00b7pel", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beim Kreuzen ruhlos denk ich", "tokens": ["Und", "beim", "Kreu\u00b7zen", "ruh\u00b7los", "denk", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kaum noch an ein Wiedersehen!", "tokens": ["Kaum", "noch", "an", "ein", "Wie\u00b7der\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, das ist der alte Kirchhof,", "tokens": ["Ja", ",", "das", "ist", "der", "al\u00b7te", "Kirch\u00b7hof", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in blauer Flut sich spiegelt,", "tokens": ["Der", "in", "blau\u00b7er", "Flut", "sich", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Offen steht sein morsches Gitter,", "tokens": ["Of\u00b7fen", "steht", "sein", "mor\u00b7sches", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand ist, der es verriegelt!", "tokens": ["Nie\u00b7mand", "ist", ",", "der", "es", "ver\u00b7rie\u00b7gelt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier der kleine Berg voll Rosen,", "tokens": ["Hier", "der", "klei\u00b7ne", "Berg", "voll", "Ro\u00b7sen", ","], "token_info": ["word", "wo