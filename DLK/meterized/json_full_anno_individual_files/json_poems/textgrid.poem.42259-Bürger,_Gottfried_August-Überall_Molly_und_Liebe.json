{"textgrid.poem.42259": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcberall Molly und Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In die Nacht der Tannen oder Eichen,", "tokens": ["In", "die", "Nacht", "der", "Tan\u00b7nen", "o\u00b7der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die das Kind der Freude schauernd flieht,", "tokens": ["Die", "das", "Kind", "der", "Freu\u00b7de", "schau\u00b7ernd", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Such' ich oft, von Kummer abgem\u00fcht,", "tokens": ["Such'", "ich", "oft", ",", "von", "Kum\u00b7mer", "ab\u00b7ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus der Welt Gerassel wegzuschleichen.", "tokens": ["Aus", "der", "Welt", "Ge\u00b7ras\u00b7sel", "weg\u00b7zu\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "K\u00f6nnt' ich nur, wie allem Meinesgleichen,", "tokens": ["K\u00f6nnt'", "ich", "nur", ",", "wie", "al\u00b7lem", "Mei\u00b7nes\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PWAV", "PIS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch sogar der Wildnis, die mich sieht,", "tokens": ["Auch", "so\u00b7gar", "der", "Wild\u00b7nis", ",", "die", "mich", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Sinn zu neuer Arbeit zieht,", "tokens": ["Und", "den", "Sinn", "zu", "neu\u00b7er", "Ar\u00b7beit", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bis ins Nichts hinein zur Ruh' entweichen!", "tokens": ["Bis", "ins", "Nichts", "hin\u00b7ein", "zur", "Ruh'", "ent\u00b7wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "PIS", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dennoch ist so heimlich kein Revier,", "tokens": ["Den\u00b7noch", "ist", "so", "heim\u00b7lich", "kein", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ist auch nicht ein Felsenspalt so \u00f6de,", "tokens": ["Ist", "auch", "nicht", "ein", "Fel\u00b7sen\u00b7spalt", "so", "\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df mich nicht, wie \u00fcberall, auch hier", "tokens": ["Da\u00df", "mich", "nicht", ",", "wie", "\u00fc\u00b7be\u00b7rall", ",", "auch", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "$,", "PWAV", "ADV", "$,", "ADV", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Liebe, die Verfolgerin, befehde;", "tokens": ["Lie\u00b7be", ",", "die", "Ver\u00b7fol\u00b7ge\u00b7rin", ",", "be\u00b7feh\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df nicht ich mit ihr von Molly rede,", "tokens": ["Da\u00df", "nicht", "ich", "mit", "ihr", "von", "Mol\u00b7ly", "re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPER", "APPR", "PPOSAT", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oder sie, die Schw\u00e4tzerin, mit mir.", "tokens": ["O\u00b7der", "sie", ",", "die", "Schw\u00e4t\u00b7ze\u00b7rin", ",", "mit", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "In die Nacht der Tannen oder Eichen,", "tokens": ["In", "die", "Nacht", "der", "Tan\u00b7nen", "o\u00b7der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die das Kind der Freude schauernd flieht,", "tokens": ["Die", "das", "Kind", "der", "Freu\u00b7de", "schau\u00b7ernd", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "p