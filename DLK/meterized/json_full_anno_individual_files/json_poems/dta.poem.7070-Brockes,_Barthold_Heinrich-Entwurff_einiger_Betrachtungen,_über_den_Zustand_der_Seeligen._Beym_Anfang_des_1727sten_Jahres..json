{"dta.poem.7070": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie werden sie sich freu\u2019n, wenn sie ermessen,", "tokens": ["Wie", "wer\u00b7den", "sie", "sich", "freu'n", ",", "wenn", "sie", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie manchen Augenblick, wie manche frohe Stunden,", "tokens": ["Wie", "man\u00b7chen", "Au\u00b7gen\u00b7blick", ",", "wie", "man\u00b7che", "fro\u00b7he", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "(die sie, wol leider! hier nicht einst empfunden,", "tokens": ["(", "die", "sie", ",", "wol", "lei\u00b7der", "!", "hier", "nicht", "einst", "emp\u00b7fun\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "$,", "ADV", "ADV", "$.", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Mangel schuldiger Aufmercksamkeit", "tokens": ["Aus", "Man\u00b7gel", "schul\u00b7di\u00b7ger", "Auf\u00b7merck\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wenigstens sie schnell vergessen)", "tokens": ["Und", "we\u00b7nigs\u00b7tens", "sie", "schnell", "ver\u00b7ges\u00b7sen", ")"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sch\u00f6pffer sie allhier vergn\u00fcgt erleben lassen.", "tokens": ["Der", "Sch\u00f6pf\u00b7fer", "sie", "all\u00b7hier", "ver\u00b7gn\u00fcgt", "er\u00b7le\u00b7ben", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem sie dann vollkommen werden fassen,", "tokens": ["In\u00b7dem", "sie", "dann", "voll\u00b7kom\u00b7men", "wer\u00b7den", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df keiner was verdient, da\u00df alles, was man habe", "tokens": ["Da\u00df", "kei\u00b7ner", "was", "ver\u00b7dient", ",", "da\u00df", "al\u00b7les", ",", "was", "man", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PWS", "VVFIN", "$,", "KOUS", "PIS", "$,", "PRELS", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Empfangen und gethan, allein des Sch\u00f6pfers Gabe.", "tokens": ["Emp\u00b7fan\u00b7gen", "und", "ge\u00b7than", ",", "al\u00b7lein", "des", "Sch\u00f6p\u00b7fers", "Ga\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}