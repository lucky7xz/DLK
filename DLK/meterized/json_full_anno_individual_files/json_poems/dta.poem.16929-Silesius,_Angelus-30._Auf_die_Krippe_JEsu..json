{"dta.poem.16929": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "30.  Auf die Krippe JEsu.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt da\u00df wehrte Kind/ der Jungfrau erste Blum/", "tokens": ["Hier", "liegt", "da\u00df", "wehr\u00b7te", "Kind", "/", "der", "Jung\u00b7frau", "ers\u00b7te", "Blum", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Engel Freud und Lust/ der Menschen Prei\u00df un\u0304 Ruhm.", "tokens": ["Der", "En\u00b7gel", "Freud", "und", "Lust", "/", "der", "Men\u00b7schen", "Prei\u00df", "un\u0304", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sol Er dein Heyland seyn/ und dich zu GOtt erheben/", "tokens": ["Sol", "Er", "dein", "Hey\u00b7land", "seyn", "/", "und", "dich", "zu", "Gott", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$(", "KON", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mustu nicht sehr weit von seiner Krippe leben.", "tokens": ["So", "mus\u00b7tu", "nicht", "sehr", "weit", "von", "sei\u00b7ner", "Krip\u00b7pe", "le\