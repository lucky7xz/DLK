{"dta.poem.5397": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sahe j\u00fcngst, im Herbst, von B\u00e4umen die Bl\u00e4tter fal-", "tokens": ["Ich", "sa\u00b7he", "j\u00fcngst", ",", "im", "Herbst", ",", "von", "B\u00e4u\u00b7men", "die", "Bl\u00e4t\u00b7ter", "fa\u00b7l"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPRART", "NN", "$,", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "len, und erbleichen,", "tokens": ["len", ",", "und", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVINF", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ich dachte: sollte man niht B\u00e4ume mit Wasser-K\u00fcnsten", "tokens": ["Ich", "dach\u00b7te", ":", "soll\u00b7te", "man", "niht", "B\u00e4u\u00b7me", "mit", "Was\u00b7ser\u00b7K\u00fcns\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VMFIN", "PIS", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "fast vergleichen?", "tokens": ["fast", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Indem der Erden-Saft in ihnen, in Bl\u00e4ttern, bald sich", "tokens": ["In\u00b7dem", "der", "Er\u00b7den\u00b7Saft", "in", "ih\u00b7nen", ",", "in", "Bl\u00e4t\u00b7tern", ",", "bald", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "$,", "APPR", "NN", "$,", "ADV", "PRF"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "aufw\u00e4rts lenckt,", "tokens": ["auf\u00b7w\u00e4rts", "lenckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bald sich, in eben diesen B\u00e4umen, zur Herbst-Zeit wieder", "tokens": ["Bald", "sich", ",", "in", "e\u00b7ben", "die\u00b7sen", "B\u00e4u\u00b7men", ",", "zur", "Herbs\u00b7tZeit", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PRF", "$,", "APPR", "ADV", "PDAT", "NN", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "abwerts senckt,", "tokens": ["ab\u00b7werts", "senckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Um abermahl, zu unserm Nutz, allm\u00e4hlig in die H\u00f6h\u2019 zu", "tokens": ["Um", "a\u00b7ber\u00b7mahl", ",", "zu", "un\u00b7serm", "Nutz", ",", "all\u00b7m\u00e4h\u00b7lig", "in", "die", "H\u00f6h'", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "steigen,", "tokens": ["stei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und denn aufs neu, zu rechter Zeit, sich abermahl herab", "tokens": ["Und", "denn", "aufs", "neu", ",", "zu", "rech\u00b7ter", "Zeit", ",", "sich", "a\u00b7ber\u00b7mahl", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJD", "$,", "APPR", "ADJA", "NN", "$,", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "zu neigen.", "tokens": ["zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ach, s\u00e4he, zu des Sch\u00f6pfers Ehren, mit froher See-", "tokens": ["Ach", ",", "s\u00e4\u00b7he", ",", "zu", "des", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ",", "mit", "fro\u00b7her", "See"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "len, iedermann,", "tokens": ["len", ",", "ie\u00b7der\u00b7mann", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "In ehrerbietigster Verwundrung, doch diesen grossen", "tokens": ["In", "ehr\u00b7er\u00b7bie\u00b7tigs\u00b7ter", "Ver\u00b7wund\u00b7rung", ",", "doch", "die\u00b7sen", "gros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "PDAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Kreis-Lauf an!", "tokens": ["Kreis\u00b7Lauf", "an", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.17": {"text": "Ach, ehrte man doch dessen Allmacht, der stets im Nord,", "tokens": ["Ach", ",", "ehr\u00b7te", "man", "doch", "des\u00b7sen", "All\u00b7macht", ",", "der", "stets", "im", "Nord", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PIS", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "S\u00fcd, Ost und West", "tokens": ["S\u00fcd", ",", "Ost", "und", "West"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Dergleichen Wasser-K\u00fcnst\u2019 in B\u00e4umen voll Anmuth vor", "tokens": ["Derg\u00b7lei\u00b7chen", "Was\u00b7ser\u00b7K\u00fcnst'", "in", "B\u00e4u\u00b7men", "voll", "An\u00b7muth", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NN", "APPR", "NN", "ADJD", "NN", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "sich spielen l\u00e4ss't!", "tokens": ["sich", "spie\u00b7len", "l\u00e4ss't", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}