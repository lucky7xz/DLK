{"textgrid.poem.35567": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Unterwegs", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Abend trugen sie den Fremden in die Totenkammer;", "tokens": ["Am", "A\u00b7bend", "tru\u00b7gen", "sie", "den", "Frem\u00b7den", "in", "die", "To\u00b7ten\u00b7kam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ein Duft von Teer; das leise Rauschen roter Platanen;", "tokens": ["Ein", "Duft", "von", "Teer", ";", "das", "lei\u00b7se", "Rau\u00b7schen", "ro\u00b7ter", "Pla\u00b7ta\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der dunkle Flug der Dohlen; am Platz zog eine Wache auf.", "tokens": ["Der", "dunk\u00b7le", "Flug", "der", "Doh\u00b7len", ";", "am", "Platz", "zog", "ei\u00b7ne", "Wa\u00b7che", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Die Sonne ist in schwarze Linnen gesunken; immer wieder kehrt dieser vergangene Abend.", "tokens": ["Die", "Son\u00b7ne", "ist", "in", "schwar\u00b7ze", "Lin\u00b7nen", "ge\u00b7sun\u00b7ken", ";", "im\u00b7mer", "wie\u00b7der", "kehrt", "die\u00b7ser", "ver\u00b7gan\u00b7ge\u00b7ne", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$.", "ADV", "ADV", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+---+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Im Nebenzimmer spielt die Schwester eine Sonate von Schubert.", "tokens": ["Im", "Ne\u00b7ben\u00b7zim\u00b7mer", "spielt", "die", "Schwes\u00b7ter", "ei\u00b7ne", "So\u00b7na\u00b7te", "von", "Schu\u00b7bert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Sehr leise sinkt ihr L\u00e4cheln in den verfallenen Brunnen,", "tokens": ["Sehr", "lei\u00b7se", "sinkt", "ihr", "L\u00e4\u00b7cheln", "in", "den", "ver\u00b7fal\u00b7le\u00b7nen", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Der bl\u00e4ulich in der D\u00e4mmerung rauscht. O, wie alt ist unser Geschlecht.", "tokens": ["Der", "bl\u00e4u\u00b7lich", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "rauscht", ".", "O", ",", "wie", "alt", "ist", "un\u00b7ser", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$.", "NE", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--++-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Jemand fl\u00fcstert drunten im Garten; jemand hat diesen schwarzen Himmel verlassen.", "tokens": ["Je\u00b7mand", "fl\u00fcs\u00b7tert", "drun\u00b7ten", "im", "Gar\u00b7ten", ";", "je\u00b7mand", "hat", "die\u00b7sen", "schwar\u00b7zen", "Him\u00b7mel", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$.", "PIS", "VAFIN", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+----+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Auf der Kommode duften \u00c4pfel. Gro\u00dfmutter z\u00fcndet goldene Kerzen an.", "tokens": ["Auf", "der", "Kom\u00b7mo\u00b7de", "duf\u00b7ten", "\u00c4p\u00b7fel", ".", "Gro\u00df\u00b7mut\u00b7ter", "z\u00fcn\u00b7det", "gol\u00b7de\u00b7ne", "Ker\u00b7zen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$.", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+--+-+--+-+", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "O, wie mild ist der Herbst. Leise klingen unsere Schritte im alten Park", "tokens": ["O", ",", "wie", "mild", "ist", "der", "Herbst", ".", "Lei\u00b7se", "klin\u00b7gen", "un\u00b7se\u00b7re", "Schrit\u00b7te", "im", "al\u00b7ten", "Park"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--++-+-+--+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Unter hohen B\u00e4umen. O, wie ernst ist das hyazinthene Antlitz der D\u00e4mmerung.", "tokens": ["Un\u00b7ter", "ho\u00b7hen", "B\u00e4u\u00b7men", ".", "O", ",", "wie", "ernst", "ist", "das", "hy\u00b7a\u00b7zin\u00b7the\u00b7ne", "Ant\u00b7litz", "der", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NE", "$,", "PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+--+--+--", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Der blaue Quell zu deinen F\u00fc\u00dfen, geheimnisvoll die rote Stille deines Munds,", "tokens": ["Der", "blau\u00b7e", "Quell", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ",", "ge\u00b7heim\u00b7nis\u00b7voll", "die", "ro\u00b7te", "Stil\u00b7le", "dei\u00b7nes", "Munds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Umd\u00fcstert vom Schlummer des Laubs, dem dunklen Gold verfallener Sonnenblumen.", "tokens": ["Um\u00b7d\u00fcs\u00b7tert", "vom", "Schlum\u00b7mer", "des", "Laubs", ",", "dem", "dunk\u00b7len", "Gold", "ver\u00b7fal\u00b7le\u00b7ner", "Son\u00b7nen\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Deine Lider sind schwer von Mohn und tr\u00e4umen leise auf meiner Stirne.", "tokens": ["Dei\u00b7ne", "Li\u00b7der", "sind", "schwer", "von", "Mohn", "und", "tr\u00e4u\u00b7men", "lei\u00b7se", "auf", "mei\u00b7ner", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Sanfte Glocken durchzittern die Brust. Eine blaue Wolke", "tokens": ["Sanf\u00b7te", "Glo\u00b7cken", "durch\u00b7zit\u00b7tern", "die", "Brust", ".", "Ei\u00b7ne", "blau\u00b7e", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Ist dein Antlitz auf mich gesunken in der D\u00e4mmerung.", "tokens": ["Ist", "dein", "Ant\u00b7litz", "auf", "mich", "ge\u00b7sun\u00b7ken", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Ein Lied zur Guitarre, das in einer fremden Schenke erklingt,", "tokens": ["Ein", "Lied", "zur", "Gui\u00b7tar\u00b7re", ",", "das", "in", "ei\u00b7ner", "frem\u00b7den", "Schen\u00b7ke", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+--+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Die wilden Hollunderb\u00fcsche dort, ein lang vergangener Novembertag,", "tokens": ["Die", "wil\u00b7den", "Hol\u00b7lun\u00b7der\u00b7b\u00fc\u00b7sche", "dort", ",", "ein", "lang", "ver\u00b7gan\u00b7ge\u00b7ner", "No\u00b7vem\u00b7ber\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Vertraute Schritte auf der d\u00e4mmernden Stiege, der Anblick gebr\u00e4unter Balken,", "tokens": ["Ver\u00b7trau\u00b7te", "Schrit\u00b7te", "auf", "der", "d\u00e4m\u00b7mern\u00b7den", "Stie\u00b7ge", ",", "der", "An\u00b7blick", "ge\u00b7br\u00e4un\u00b7ter", "Bal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Ein offenes Fenster, an dem ein s\u00fc\u00dfes Hoffen zur\u00fcckblieb \u2013", "tokens": ["Ein", "of\u00b7fe\u00b7nes", "Fens\u00b7ter", ",", "an", "dem", "ein", "s\u00fc\u00b7\u00dfes", "Hof\u00b7fen", "zu\u00b7r\u00fcck\u00b7blieb", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "Uns\u00e4glich ist das alles, o Gott, da\u00df man ersch\u00fcttert ins Knie bricht.", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "ist", "das", "al\u00b7les", ",", "o", "Gott", ",", "da\u00df", "man", "er\u00b7sch\u00fct\u00b7tert", "ins", "Knie", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "PIS", "$,", "FM", "NN", "$,", "KOUS", "PIS", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "O, wie dunkel ist diese Nacht. Eine purpurne Flamme", "tokens": ["O", ",", "wie", "dun\u00b7kel", "ist", "die\u00b7se", "Nacht", ".", "Ei\u00b7ne", "pur\u00b7pur\u00b7ne", "Flam\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "PDAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Erlosch an meinem Mund. In der Stille", "tokens": ["Er\u00b7losch", "an", "mei\u00b7nem", "Mund", ".", "In", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erstirbt der bangen Seele einsames Saitenspiel.", "tokens": ["Er\u00b7stirbt", "der", "ban\u00b7gen", "See\u00b7le", "ein\u00b7sa\u00b7mes", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "La\u00df, wenn trunken von Wein das Haupt in die Gosse sinkt.", "tokens": ["La\u00df", ",", "wenn", "trun\u00b7ken", "von", "Wein", "das", "Haupt", "in", "die", "Gos\u00b7se", "sinkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "ADJD", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Am Abend trugen sie den Fremden in die Totenkammer;", "tokens": ["Am", "A\u00b7bend", "tru\u00b7gen", "sie", "den", "Frem\u00b7den", "in", "die", "To\u00b7ten\u00b7kam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ein Duft von Teer; das leise Rauschen roter Platanen;", "