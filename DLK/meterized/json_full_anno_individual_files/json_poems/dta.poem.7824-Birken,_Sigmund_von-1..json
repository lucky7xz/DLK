{"dta.poem.7824": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.57", "sv:0.42"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Nymfe! steh doch stille!", "tokens": ["Sch\u00f6\u00b7ne", "Nym\u00b7fe", "!", "steh", "doch", "stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "steh! ich bin dein Feind ja nit!", "tokens": ["steh", "!", "ich", "bin", "dein", "Feind", "ja", "nit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Hirtinn/ das Gebr\u00fclle", "tokens": ["Ei\u00b7ne", "Hir\u00b7tinn", "/", "das", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eines L\u00f6wens/ also flieht.", "tokens": ["ei\u00b7nes", "L\u00f6\u00b7wens", "/", "al\u00b7so", "flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "also fliehen feige Tauben/", "tokens": ["al\u00b7so", "flie\u00b7hen", "fei\u00b7ge", "Tau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wann sie will der Habicht rauben.", "tokens": ["wann", "sie", "will", "der", "Ha\u00b7bicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schafe also sich entziehen", "tokens": ["Scha\u00b7fe", "al\u00b7so", "sich", "ent\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "vor dem Wolf/ dem Schreckengast.", "tokens": ["vor", "dem", "Wolf", "/", "dem", "Schre\u00b7cken\u00b7gast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$(", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes gerne will entfliehen", "tokens": ["Je\u00b7des", "ger\u00b7ne", "will", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seinem Feinde/ den es hasst.", "tokens": ["sei\u00b7nem", "Fein\u00b7de", "/", "den", "es", "hasst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe/ diesen Weg mir weiset;", "tokens": ["Lie\u00b7be", "/", "die\u00b7sen", "Weg", "mir", "wei\u00b7set", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe/ mich dir folgen heiset.", "tokens": ["Lie\u00b7be", "/", "mich", "dir", "fol\u00b7gen", "hei\u00b7set", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nymfe! dich vom Laufen kehre!", "tokens": ["Nym\u00b7fe", "!", "dich", "vom", "Lau\u00b7fen", "keh\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ach! ich f\u00fcrchte/ da\u00df ein Fall", "tokens": ["ach", "!", "ich", "f\u00fcrch\u00b7te", "/", "da\u00df", "ein", "Fall"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$(", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "deinen sch\u00f6nen Leib versehre.", "tokens": ["dei\u00b7nen", "sch\u00f6\u00b7nen", "Leib", "ver\u00b7seh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dornen sind hier \u00fcberall:", "tokens": ["Dor\u00b7nen", "sind", "hier", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schad es w\u00e4r/ da\u00df ihre Spitzen", "tokens": ["Schad", "es", "w\u00e4r", "/", "da\u00df", "ih\u00b7re", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "solten solche Glieder ritzen.", "tokens": ["sol\u00b7ten", "sol\u00b7che", "Glie\u00b7der", "rit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich mag dir nit machen Schmerzen.", "tokens": ["Ich", "mag", "dir", "nit", "ma\u00b7chen", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lauf nit so mit strenger Macht!", "tokens": ["Lauf", "nit", "so", "mit", "stren\u00b7ger", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schrofen m\u00f6chten mit dir scherzen.", "tokens": ["Schro\u00b7fen", "m\u00f6ch\u00b7ten", "mit", "dir", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilst du fliehen/ fliehe sacht!", "tokens": ["Wilst", "du", "flie\u00b7hen", "/", "flie\u00b7he", "sacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$(", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich/ auf da\u00df du m\u00f6gst verschnaufen/", "tokens": ["Ich", "/", "auf", "da\u00df", "du", "m\u00f6gst", "ver\u00b7schnau\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "will auch so geschwind nit laufen.", "tokens": ["will", "auch", "so", "ge\u00b7schwind", "nit", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}