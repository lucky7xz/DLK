{"textgrid.poem.48415": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Auf H. Georg Glogers Med. Cand. seliges Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Liebster, was bedeut das ungewohnte R\u00f6cheln,", "tokens": ["O", "Liebs\u00b7ter", ",", "was", "be\u00b7deut", "das", "un\u00b7ge\u00b7wohn\u00b7te", "R\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Furcht der hei\u00dfen Brust, der matten Lungen Fecheln,", "tokens": ["die", "Furcht", "der", "hei\u00b7\u00dfen", "Brust", ",", "der", "mat\u00b7ten", "Lun\u00b7gen", "Fe\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das so geschwinde keicht? Ach! wo, wo l\u00e4\u00dft du dich,", "tokens": ["das", "so", "ge\u00b7schwin\u00b7de", "keicht", "?", "Ach", "!", "wo", ",", "wo", "l\u00e4\u00dft", "du", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$.", "ITJ", "$.", "PWAV", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dein' Augen, deinen Mund, und was noch mehr, wo mich?", "tokens": ["dein'", "Au\u00b7gen", ",", "dei\u00b7nen", "Mund", ",", "und", "was", "noch", "mehr", ",", "wo", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "PWS", "ADV", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "mich, deinen andern Dich? So bistu nun geflogen,", "tokens": ["mich", ",", "dei\u00b7nen", "an\u00b7dern", "Dich", "?", "So", "bis\u00b7tu", "nun", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "PPER", "$.", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "du sch\u00f6ne Seele du, und l\u00e4\u00dft unnachgezogen", "tokens": ["du", "sch\u00f6\u00b7ne", "See\u00b7le", "du", ",", "und", "l\u00e4\u00dft", "un\u00b7nach\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "KON", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "den Leib, dein sch\u00f6nes Kleid, das mit so sch\u00f6ner Pracht", "tokens": ["den", "Leib", ",", "dein", "sch\u00f6\u00b7nes", "Kleid", ",", "das", "mit", "so", "sch\u00f6\u00b7ner", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der Tugend war gest\u00fcckt und sauber ausgemacht!", "tokens": ["der", "Tu\u00b7gend", "war", "ge\u00b7st\u00fcckt", "und", "sau\u00b7ber", "aus\u00b7ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Mund, den Venus selbst in ihre Nectar tauchet,", "tokens": ["Du", "Mund", ",", "den", "Ve\u00b7nus", "selbst", "in", "ih\u00b7re", "Nec\u00b7tar", "tau\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und dem die Gratien ihr Holdsein eingehauchet,", "tokens": ["und", "dem", "die", "Gra\u00b7ti\u00b7en", "ihr", "Hold\u00b7sein", "ein\u00b7ge\u00b7hau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ihr Augen, die ihr mich durch euer freundlich Sehn", "tokens": ["ihr", "Au\u00b7gen", ",", "die", "ihr", "mich", "durch", "eu\u00b7er", "freund\u00b7lich", "Sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "zur Gegenliebe zwingt, nun ists um euch geschehn", "tokens": ["zur", "Ge\u00b7gen\u00b7lie\u00b7be", "zwingt", ",", "nun", "ists", "um", "euch", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "und auch um euren mich! Vor hab' ich finden k\u00f6nnen", "tokens": ["und", "auch", "um", "eu\u00b7ren", "mich", "!", "Vor", "hab'", "ich", "fin\u00b7den", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "PPER", "$.", "APPR", "VAFIN", "PPER", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "noch meinen Landsman, dich, du Labsal meiner Sinnen!", "tokens": ["noch", "mei\u00b7nen", "Lands\u00b7man", ",", "dich", ",", "du", "Lab\u00b7sal", "mei\u00b7ner", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "PPER", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Freund zwar, hoff' ich wol, mir anzutreffen ist:", "tokens": ["Ein", "Freund", "zwar", ",", "hoff'", "ich", "wol", ",", "mir", "an\u00b7zu\u00b7tref\u00b7fen", "ist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVIZU", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "so einer nimmermehr, wie du gewesen bist.", "tokens": ["so", "ei\u00b7ner", "nim\u00b7mer\u00b7mehr", ",", "wie", "du", "ge\u00b7we\u00b7sen", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$,", "PWAV", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "An dir hab' ich gehabt, ach! ach! gehabt den Zeugen", "tokens": ["An", "dir", "hab'", "ich", "ge\u00b7habt", ",", "ach", "!", "ach", "!", "ge\u00b7habt", "den", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VAPP", "$,", "ITJ", "$.", "XY", "$.", "VAPP", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "von meiner Poesie, wie sehr sie umzubeugen", "tokens": ["von", "mei\u00b7ner", "Poe\u00b7sie", ",", "wie", "sehr", "sie", "um\u00b7zu\u00b7beu\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "ADV", "PPER", "VVIZU"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.19": {"text": "der hagre Neid erk\u00fchnt, wie schlim er auf sie sieht!", "tokens": ["der", "hag\u00b7re", "Neid", "er\u00b7k\u00fchnt", ",", "wie", "schlim", "er", "auf", "sie", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PWAV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch dich verlacht' ich ihn: du hubst mir das Gem\u00fct'", "tokens": ["Durch", "dich", "ver\u00b7lacht'", "ich", "ihn", ":", "du", "hubst", "mir", "das", "Ge\u00b7m\u00fct'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "ie mehr zum Ewigsein. Apollo war mir g\u00fcnstig,", "tokens": ["ie", "mehr", "zum", "E\u00b7wig\u00b7sein", ".", "A\u00b7pol\u00b7lo", "war", "mir", "g\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$.", "NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "der Musicant' und Arzt, weil du mich machtest br\u00fcnstig", "tokens": ["der", "Mu\u00b7si\u00b7cant'", "und", "Arzt", ",", "weil", "du", "mich", "mach\u00b7test", "br\u00fcns\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "zu seiner doppeln Kunst. Die freie Meditrin", "tokens": ["zu", "sei\u00b7ner", "dop\u00b7peln", "Kunst", ".", "Die", "frei\u00b7e", "Me\u00b7di\u00b7trin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "verweiste mich durch dich zu ihrem Tempel hin", "tokens": ["ver\u00b7weis\u00b7te", "mich", "durch", "dich", "zu", "ih\u00b7rem", "Tem\u00b7pel", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und hie\u00df mich ihren Freund. Wo werd' ich nun gelassen,", "tokens": ["und", "hie\u00df", "mich", "ih\u00b7ren", "Freund", ".", "Wo", "werd'", "ich", "nun", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "weil du mich so verl\u00e4\u00dft? Wie auf den rauhen Gassen", "tokens": ["weil", "du", "mich", "so", "ver\u00b7l\u00e4\u00dft", "?", "Wie", "auf", "den", "rau\u00b7hen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "$.", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "des b\u00f6sen Oceans ein schwacher Nachen wankt,", "tokens": ["des", "b\u00f6\u00b7sen", "O\u00b7ceans", "ein", "schwa\u00b7cher", "Na\u00b7chen", "wankt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "der keinen Bootsknecht hat, da\u00df er den Port erlangt,", "tokens": ["der", "kei\u00b7nen", "Boots\u00b7knecht", "hat", ",", "da\u00df", "er", "den", "Port", "er\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "sch\u00f6pft Wasser, tauchet ein: also gehts meinem Kahne,", "tokens": ["sch\u00f6pft", "Was\u00b7ser", ",", "tau\u00b7chet", "ein", ":", "al\u00b7so", "gehts", "mei\u00b7nem", "Kah\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "der nun Kunst holen soll. Ich bin auf wilder Bahne,", "tokens": ["der", "nun", "Kunst", "ho\u00b7len", "soll", ".", "Ich", "bin", "auf", "wil\u00b7der", "Bah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVINF", "VMFIN", "$.", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "mein Ruder ist entzwei, mein Anker bleibt im Stich',", "tokens": ["mein", "Ru\u00b7der", "ist", "ent\u00b7zwei", ",", "mein", "An\u00b7ker", "bleibt", "im", "Stich'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "im bodenlosen Grund'. O du mein selber Ich!", "tokens": ["im", "bo\u00b7den\u00b7lo\u00b7sen", "Grund'", ".", "O", "du", "mein", "sel\u00b7ber", "Ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "NE", "PPOSAT", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Alles und mein Nichts, ach Liebster! war dein Name,", "tokens": ["Mein", "Al\u00b7les", "und", "mein", "Nichts", ",", "ach", "Liebs\u00b7ter", "!", "war", "dein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "KON", "PPOSAT", "NN", "$,", "XY", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.34": {"text": "der's wol auch bleiben wird, so lang' ein K\u00f6rnlein Same", "tokens": ["der's", "wol", "auch", "blei\u00b7ben", "wird", ",", "so", "lang'", "ein", "K\u00f6rn\u00b7lein", "Sa\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "VVINF", "VAFIN", "$,", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "der Seelen in mir bleibt! Die Faust erstarret mir,", "tokens": ["der", "See\u00b7len", "in", "mir", "bleibt", "!", "Die", "Faust", "er\u00b7star\u00b7ret", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "die Tr\u00e4nen schwemmen aus die Dinte vom Papier'.", "tokens": ["die", "Tr\u00e4\u00b7nen", "schwem\u00b7men", "aus", "die", "Din\u00b7te", "vom", "Pa\u00b7pier'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich kan, ich kan nicht mehr! So nim doch hin, mein Leben,", "tokens": ["Ich", "kan", ",", "ich", "kan", "nicht", "mehr", "!", "So", "nim", "doch", "hin", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "$.", "ADV", "VVIMP", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "den Ku\u00df, den letzten Ku\u00df, den ohne Wiedergeben", "tokens": ["den", "Ku\u00df", ",", "den", "letz\u00b7ten", "Ku\u00df", ",", "den", "oh\u00b7ne", "Wie\u00b7der\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "(ach w\u00e4rs auch vor geschehn!) ich setz' auf deinen Mund,", "tokens": ["(", "ach", "w\u00e4rs", "auch", "vor", "ge\u00b7schehn", "!", ")", "ich", "setz'", "auf", "dei\u00b7nen", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "VAFIN", "ADV", "APPR", "VVPP", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "auf deinen kalten Mund! Di\u00df ist der letzte Bund:", "tokens": ["auf", "dei\u00b7nen", "kal\u00b7ten", "Mund", "!", "Di\u00df", "ist", "der", "letz\u00b7te", "Bund", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "so bleib' ich dir verm\u00e4hlt! So ewig Flemings Buhlen,", "tokens": ["so", "bleib'", "ich", "dir", "ver\u00b7m\u00e4hlt", "!", "So", "e\u00b7wig", "Fle\u00b7mings", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "$.", "ADV", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "die zarte Poesie, wird sein in Ph\u00f6bus Schulen,", "tokens": ["die", "zar\u00b7te", "Poe\u00b7sie", ",", "wird", "sein", "in", "Ph\u00f6\u00b7bus", "Schu\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "so soll dein herzer Nam' an allen W\u00e4nden stehn,", "tokens": ["so", "soll", "dein", "her\u00b7zer", "Nam'", "an", "al\u00b7len", "W\u00e4n\u00b7den", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "und mit der Ewigkeit mein Gloger untergehn!", "tokens": ["und", "mit", "der", "E\u00b7wig\u00b7keit", "mein", "Glo\u00b7ger", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Liebster, was bedeut das ungewohnte R\u00f6cheln,", "tokens": ["O", "Liebs\u00b7ter", ",", "was", "be\u00b7deut", "das", "un\u00b7ge\u00b7wohn\u00b7te", "R\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Furcht der hei\u00dfen Brust, der matten Lungen Fecheln,", "tokens": ["die", "Furcht", "der", "hei\u00b7\u00dfen", "Brust", ",", "der", "mat\u00b7ten", "Lun\u00b7gen", "Fe\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das so geschwinde keicht? Ach! wo, wo l\u00e4\u00dft du dich,", "tokens": ["das", "so", "ge\u00b7schwin\u00b7de", "keicht", "?", "Ach", "!", "wo", ",", "wo", "l\u00e4\u00dft", "du", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$.", "ITJ", "$.", "PWAV", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dein' Augen, deinen Mund, und was noch mehr, wo mich?", "tokens": ["dein'", "Au\u00b7gen", ",", "dei\u00b7nen", "Mund", ",", "und", "was", "noch", "mehr", ",", "wo", "mich", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "PWS", "ADV", "ADV", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "mich, deinen andern Dich? So bistu nun geflogen,", "tokens": ["mich", ",", "dei\u00b7nen", "an\u00b7dern", "Dich", "?", "So", "bis\u00b7tu", "nun", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "PPER", "$.", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "du sch\u00f6ne Seele du, und l\u00e4\u00dft unnachgezogen", "tokens": ["du", "sch\u00f6\u00b7ne", "See\u00b7le", "du", ",", "und", "l\u00e4\u00dft", "un\u00b7nach\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,", "KON", "VVFIN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "den Leib, dein sch\u00f6nes Kleid, das mit so sch\u00f6ner Pracht", "tokens": ["den", "Leib", ",", "dein", "sch\u00f6\u00b7nes", "Kleid", ",", "das", "mit", "so", "sch\u00f6\u00b7ner", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der Tugend war gest\u00fcckt und sauber ausgemacht!", "tokens": ["der", "Tu\u00b7gend", "war", "ge\u00b7st\u00fcckt", "und", "sau\u00b7ber", "aus\u00b7ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Mund, den Venus s