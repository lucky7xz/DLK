", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "im bodenlosen Grund'. O du mein selber Ich!", "tokens": ["im", "bo\u00b7den\u00b7lo\u00b7sen", "Grund'", ".", "O", "du", "mein", "sel\u00b7ber", "Ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "NE", "PPOSAT", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Alles und mein Nichts, ach Liebster! war dein Name,", "tokens": ["Mein", "Al\u00b7les", "und", "mein", "Nichts", ",", "ach", "Liebs\u00b7ter", "!", "war", "dein", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "KON", "PPOSAT", "NN", "$,", "XY", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.34": {"text": "der's wol auch bleiben wird, so lang' ein K\u00f6rnlein Same", "tokens": ["der's", "wol", "auch", "blei\u00b7ben", "wird", ",", "so", "lang'", "ein", "K\u00f6rn\u00b7lein", "Sa\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "VVINF", "VAFIN", "$,", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "der Seelen in mir bleibt! Die Faust erstarret mir,", "tokens": ["der", "See\u00b7len", "in", "mir", "bleibt", "!", "Die", "Faust", "er\u00b7star\u00b7ret", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "die Tr\u00e4nen schwemmen aus die Dinte vom Papier'.", "tokens": ["die", "Tr\u00e4\u00b7nen", "schwem\u00b7men", "aus", "die", "Din\u00b7te", "vom", "Pa\u00b7pier'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich kan, ich kan nicht mehr! So nim doch hin, mein Leben,", "tokens": ["Ich", "kan", ",", "ich", "kan", "nicht", "mehr", "!", "So", "nim", "doch", "hin", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "$.", "ADV", "VVIMP", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "den Ku\u00df, den letzten Ku\u00df, den ohne Wiedergeben", "tokens": ["den", "Ku\u00df", ",", "den", "letz\u00b7ten", "Ku\u00df", ",", "den", "oh\u00b7ne", "Wie\u00b7der\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "(ach w\u00e4rs auch vor geschehn!) ich setz' auf deinen Mund,", "tokens": ["(", "ach", "w\u00e4rs", "auch", "vor", "ge\u00b7schehn", "!", ")", "ich", "setz'", "auf", "dei\u00b7nen", "Mund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "VAFIN", "ADV", "APPR", "VVPP", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {