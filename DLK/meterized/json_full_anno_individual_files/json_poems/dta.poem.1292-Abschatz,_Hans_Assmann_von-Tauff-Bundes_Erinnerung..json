{"dta.poem.1292": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff-Bundes Erinnerung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wer wei\u00df das Element des Wassers zu entbehren?", "tokens": ["Wer", "wei\u00df", "das", "E\u00b7le\u00b7ment", "des", "Was\u00b7sers", "zu", "ent\u00b7beh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es tr\u00e4ncket/ kocht und heilt: Wenn heisser Sonnen", "tokens": ["Es", "tr\u00e4n\u00b7cket", "/", "kocht", "und", "heilt", ":", "Wenn", "heis\u00b7ser", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "$.", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Arbeit Last und Schwei\u00df den Gliedern wehe thut/", "tokens": ["Der", "Ar\u00b7beit", "Last", "und", "Schwei\u00df", "den", "Glie\u00b7dern", "we\u00b7he", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df uns Labung di\u00df und neue Krafft gewehren/", "tokens": ["So", "mu\u00df", "uns", "La\u00b7bung", "di\u00df", "und", "neu\u00b7e", "Krafft", "ge\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "PDS", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df giebt der Speise Safft/ aus wie viel edlen Brunnen", "tokens": ["Di\u00df", "giebt", "der", "Spei\u00b7se", "Safft", "/", "aus", "wie", "viel", "ed\u00b7len", "Brun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$(", "PTKVZ", "KOKOM", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "K\u00f6mmt krancker Glieder Heyl mit Bad und Tranck gerun-", "tokens": ["K\u00f6mmt", "kran\u00b7cker", "Glie\u00b7der", "Heyl", "mit", "Bad", "und", "Tranck", "ge\u00b7run"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "NN", "APPR", "NE", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Di\u00df n\u00e4hret/ was uns n\u00e4hrt/ gewehrt auff unsre Tische/", "tokens": ["Di\u00df", "n\u00e4h\u00b7ret", "/", "was", "uns", "n\u00e4hrt", "/", "ge\u00b7wehrt", "auff", "uns\u00b7re", "Ti\u00b7sche", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWS", "PPER", "VVFIN", "$(", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm manch fremdes Volck vor Fleisch und Brod er-", "tokens": ["Was", "ihm", "manch", "frem\u00b7des", "Volck", "vor", "Fleisch", "und", "Brod", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PIAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was selbst die Erde nicht in so viel Arten zehlt/", "tokens": ["Was", "selbst", "die", "Er\u00b7de", "nicht", "in", "so", "viel", "Ar\u00b7ten", "zehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PTKNEG", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu unsrem Unterhalt und Uberflu\u00df/ die Fische.", "tokens": ["Zu", "uns\u00b7rem", "Un\u00b7ter\u00b7halt", "und", "U\u00b7ber\u00b7flu\u00df", "/", "die", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wollen wir den Nutz des Wassers recht ergr\u00fcnden/", "tokens": ["Doch", "wol\u00b7len", "wir", "den", "Nutz", "des", "Was\u00b7sers", "recht", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird sich aus GOttes Wort noch bessre Probe finden:", "tokens": ["Wird", "sich", "aus", "Got\u00b7tes", "Wort", "noch", "bess\u00b7re", "Pro\u00b7be", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Naemans Aussatz ward in Jordans Flutt geheilet/", "tokens": ["Nae\u00b7mans", "Aus\u00b7satz", "ward", "in", "Jor\u00b7dans", "Flutt", "ge\u00b7hei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPR", "NE", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ein heilsam Wasser war in GOttes Volck bekandt/", "tokens": ["Ein", "heil\u00b7sam", "Was\u00b7ser", "war", "in", "Got\u00b7tes", "Volck", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPR", "NN", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zur Reinigung gesprengt von Hohen-Priesters Hand.", "tokens": ["Zur", "Rei\u00b7ni\u00b7gung", "ge\u00b7sprengt", "von", "Ho\u00b7hen\u00b7Pries\u00b7ters", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von wem Bethe\u00dfdens Teich am ersten ward ereilet/", "tokens": ["Von", "wem", "Be\u00b7the\u00df\u00b7dens", "Teich", "am", "ers\u00b7ten", "ward", "er\u00b7ei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "APPRART", "ADJA", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn ihn von oben her des Engels Hand ber\u00fchret/", "tokens": ["Wenn", "ihn", "von", "o\u00b7ben", "her", "des", "En\u00b7gels", "Hand", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "APZR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der hat sich aller Plag\u2019 und Siechthums frey gesp\u00fcret.", "tokens": ["Der", "hat", "sich", "al\u00b7ler", "Plag'", "und", "Siech\u00b7thums", "frey", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "PIAT", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des H\u00f6chsten Wasser-Kunst ist h\u00f6her noch gestiegen/", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Was\u00b7ser\u00b7Kunst", "ist", "h\u00f6\u00b7her", "noch", "ge\u00b7stie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Bilder zeigten uns im Schatten Wercke dar/", "tokens": ["Die", "Bil\u00b7der", "zeig\u00b7ten", "uns", "im", "Schat\u00b7ten", "Wer\u00b7cke", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was von des HErren Gunst f\u00fcr uns bereitet war/", "tokens": ["Was", "von", "des", "Her\u00b7ren", "Gunst", "f\u00fcr", "uns", "be\u00b7rei\u00b7tet", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was in der Tauffe Brunn f\u00fcr Seelen-Sch\u00e4tze liegen/", "tokens": ["Was", "in", "der", "Tauf\u00b7fe", "Brunn", "f\u00fcr", "See\u00b7len\u00b7Sch\u00e4t\u00b7ze", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun Christus/ GOttes Sohn/ das Wasser selbst geweyhet/", "tokens": ["Nun", "Chris\u00b7tus", "/", "Got\u00b7tes", "Sohn", "/", "das", "Was\u00b7ser", "selbst", "ge\u00b7wey\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "NN", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unsre Tauffe durch die seine benedeyet.", "tokens": ["Und", "uns\u00b7re", "Tauf\u00b7fe", "durch", "die", "sei\u00b7ne", "be\u00b7ne\u00b7de\u00b7yet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.5": {"line.1": {"text": "Wer wei\u00df di\u00df Sacrament des Wassers zu entbehren/", "tokens": ["Wer", "wei\u00df", "di\u00df", "Sa\u00b7cra\u00b7ment", "des", "Was\u00b7sers", "zu", "ent\u00b7beh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Wassers/ dem das Wort des H\u00f6chsten                             beygesellt/", "tokens": ["Des", "Was\u00b7sers", "/", "dem", "das", "Wort", "des", "H\u00f6chs\u00b7ten", "bey\u00b7ge\u00b7sellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Viel Wunder-Kr\u00e4ffte schenckt: Wir kommen auff die Welt", "tokens": ["Viel", "Wun\u00b7der\u00b7Kr\u00e4ff\u00b7te", "schenckt", ":", "Wir", "kom\u00b7men", "auff", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ohnm\u00e4chtig uns der Macht der Raub-Fisch zu erwehren/", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "uns", "der", "Macht", "der", "Raub\u00b7Fisch", "zu", "er\u00b7weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Als Schleyen/ die im Koth der schn\u00f6den Erb-Schuld                             stecken/", "tokens": ["Als", "Schle\u00b7yen", "/", "die", "im", "Koth", "der", "schn\u00f6\u00b7den", "Er\u00b7bSchuld", "ste\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die das Netze soll des H\u00f6llen-Fischers                             decken.", "tokens": ["Und", "die", "das", "Net\u00b7ze", "soll", "des", "H\u00f6l\u00b7len\u00b7Fi\u00b7schers", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Uns wird durch dieses Bad verneute Krafft gegeben/", "tokens": ["Uns", "wird", "durch", "die\u00b7ses", "Bad", "ver\u00b7neu\u00b7te", "Krafft", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NE", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Wachsthum in dem Geist/ uns macht die Quelle rein/", "tokens": ["Und", "Wach\u00b7sthum", "in", "dem", "Geist", "/", "uns", "macht", "die", "Quel\u00b7le", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wir vom S\u00fcnden-Wust f\u00fcr GOtt gesaubert seyn/", "tokens": ["Da\u00df", "wir", "vom", "S\u00fcn\u00b7den\u00b7Wust", "f\u00fcr", "Gott", "ge\u00b7sau\u00b7bert", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nicht in Dienstbarkeit der H\u00f6lle d\u00f6rfften kleben;", "tokens": ["Und", "nicht", "in", "Dienst\u00b7bar\u00b7keit", "der", "H\u00f6l\u00b7le", "d\u00f6rff\u00b7ten", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den Heil-Brunn gabst du mir/ O GOTT/ auch zu genies-", "tokens": ["Den", "Heil\u00b7Brunn", "gabst", "du", "mir", "/", "O", "GoTT", "/", "auch", "zu", "ge\u00b7ni\u00b7es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$(", "NE", "NE", "$(", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df drau\u00df best\u00e4ndigs Heyl und Segen auff mich fliessen.", "tokens": ["La\u00df", "drau\u00df", "be\u00b7st\u00e4n\u00b7digs", "Heyl", "und", "Se\u00b7gen", "auff", "mich", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}