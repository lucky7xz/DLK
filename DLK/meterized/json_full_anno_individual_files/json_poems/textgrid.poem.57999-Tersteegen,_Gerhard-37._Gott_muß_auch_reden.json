{"textgrid.poem.57999": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "37. Gott mu\u00df auch reden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast Gott immerhin so viele Ding' zu sagen,", "tokens": ["Du", "hast", "Gott", "im\u00b7mer\u00b7hin", "so", "vie\u00b7le", "Ding'", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald h\u00e4ttst du dieses gern, bald kommst du jenes klagen;", "tokens": ["Bald", "h\u00e4ttst", "du", "die\u00b7ses", "gern", ",", "bald", "kommst", "du", "je\u00b7nes", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "ADV", "$,", "ADV", "VVFIN", "PPER", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "La\u00df ihm doch auch 'was Raum, schweig ihm ein wenig still", "tokens": ["La\u00df", "ihm", "doch", "auch", "'was", "Raum", ",", "schweig", "ihm", "ein", "we\u00b7nig", "still"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "NE", "NN", "$,", "VVFIN", "PPER", "ART", "PIS", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und merk, was er dir sagt und von dir haben will!", "tokens": ["Und", "merk", ",", "was", "er", "dir", "sagt", "und", "von", "dir", "ha\u00b7ben", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "p