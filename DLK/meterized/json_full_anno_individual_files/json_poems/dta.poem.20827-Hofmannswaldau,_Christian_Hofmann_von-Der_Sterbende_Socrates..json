{"dta.poem.20827": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wann das Verh\u00e4ngni\u00df wird gedr\u00fcckt", "tokens": ["Wann", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "wird", "ge\u00b7dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch des Gel\u00fcckes falsche T\u00fccke/", "tokens": ["Durch", "des", "Ge\u00b7l\u00fc\u00b7ckes", "fal\u00b7sche", "T\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und unser Auge wird verr\u00fcckt", "tokens": ["Und", "un\u00b7ser", "Au\u00b7ge", "wird", "ver\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch allzuheisse Sonnenblicke.", "tokens": ["Durch", "all\u00b7zu\u00b7heis\u00b7se", "Son\u00b7nen\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann wir erlernen aus der Pein/", "tokens": ["Wann", "wir", "er\u00b7ler\u00b7nen", "aus", "der", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df des Gestirnes falscher Schein", "tokens": ["Da\u00df", "des", "Ge\u00b7stir\u00b7nes", "fal\u00b7scher", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht m\u00fcde wird uns zu verderben.", "tokens": ["Nicht", "m\u00fc\u00b7de", "wird", "uns", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wann unser Geist uns st\u00fcndlich plagt/", "tokens": ["Wann", "un\u00b7ser", "Geist", "uns", "st\u00fcnd\u00b7lich", "plagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und tausend Schrecken in uns jagt/", "tokens": ["Und", "tau\u00b7send", "Schre\u00b7cken", "in", "uns", "jagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Darff man durch eigne Hand nicht sterben?", "tokens": ["Darff", "man", "durch", "eig\u00b7ne", "Hand", "nicht", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was darff die Noth von fremden H\u00e4nden", "tokens": ["Was", "darff", "die", "Noth", "von", "frem\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In k\u00fcnfftig seyn von mir gewand/", "tokens": ["In", "k\u00fcnff\u00b7tig", "seyn", "von", "mir", "ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VAINF", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wann heute noch mir meine Hand", "tokens": ["Wann", "heu\u00b7te", "noch", "mir", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Kan tausend Angst und Leiden enden.", "tokens": ["Kan", "tau\u00b7send", "Angst", "und", "Lei\u00b7den", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "CARD", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}