{"dta.poem.5174": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Vorbitte f\u00fcr einer armen Witwe  \n an das Dohmcapitul zu Halberstadt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Seyd mir gegr\u00fc\u00dft, ihr Herren von dem Dohm!", "tokens": ["Seyd", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "Her\u00b7ren", "von", "dem", "Dohm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ehrw\u00fcrdiger, als ehedem zu Rom,", "tokens": ["Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "als", "e\u00b7he\u00b7dem", "zu", "Rom", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Consul und Senat die V\u00f6lker auszur\u00fcsten", "tokens": ["Der", "Con\u00b7sul", "und", "Se\u00b7nat", "die", "V\u00f6l\u00b7ker", "aus\u00b7zu\u00b7r\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ins Capitol geeilt, um Nachbarn zu verw\u00fcsten;", "tokens": ["Ins", "Ca\u00b7pi\u00b7tol", "ge\u00b7eilt", ",", "um", "Nach\u00b7barn", "zu", "ver\u00b7w\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seyd mir gesegneter, als Herren zu Paris,", "tokens": ["Seyd", "mir", "ge\u00b7se\u00b7gne\u00b7ter", ",", "als", "Her\u00b7ren", "zu", "Pa\u00b7ris", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADJD", "$,", "KOUS", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Die Ludewig versammlen lie\u00df,", "tokens": ["Die", "Lu\u00b7de\u00b7wig", "ver\u00b7samm\u00b7len", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im grossen Parlament, um alles Volk zu sch\u00e4tzen,", "tokens": ["Im", "gros\u00b7sen", "Par\u00b7la\u00b7ment", ",", "um", "al\u00b7les", "Volk", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUI", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Krieg mit Nachdruck fortzusetzen;", "tokens": ["Den", "Krieg", "mit", "Nach\u00b7druck", "fort\u00b7zu\u00b7set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Heil sey euch! mehr, als in dem kalten Norden", "tokens": ["Heil", "sey", "euch", "!", "mehr", ",", "als", "in", "dem", "kal\u00b7ten", "Nor\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$.", "ADV", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Den St\u00e4nden, die gemacht, da\u00df Friedrichs naher Freund", "tokens": ["Den", "St\u00e4n\u00b7den", ",", "die", "ge\u00b7macht", ",", "da\u00df", "Fried\u00b7richs", "na\u00b7her", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,", "KOUS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In seiner Schwester Arm, ein Feind", "tokens": ["In", "sei\u00b7ner", "Schwes\u00b7ter", "Arm", ",", "ein", "Feind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Trotz seines Herzens ist geworden;", "tokens": ["Trotz", "sei\u00b7nes", "Her\u00b7zens", "ist", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich gr\u00fc\u00df euch mit dem besten Gru\u00df,", "tokens": ["Ich", "gr\u00fc\u00df", "euch", "mit", "dem", "bes\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Der einem Menschen-Freund geb\u00fchret;", "tokens": ["Der", "ei\u00b7nem", "Men\u00b7schen\u00b7Freund", "ge\u00b7b\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und meine Muse bringt vor euch ein Weib gef\u00fchret,", "tokens": ["Und", "mei\u00b7ne", "Mu\u00b7se", "bringt", "vor", "euch", "ein", "Weib", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vom Grabe hergewankt, worauf sie weinen mu\u00df.", "tokens": ["Vom", "Gra\u00b7be", "her\u00b7ge\u00b7wankt", ",", "wo\u00b7rauf", "sie", "wei\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihr Mann war euer Syndicus!", "tokens": ["Ihr", "Mann", "war", "eu\u00b7er", "Syn\u00b7di\u00b7cus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er starb im Sommer seiner Jahre,", "tokens": ["Er", "starb", "im", "Som\u00b7mer", "sei\u00b7ner", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Noch langes Leben sah\u2019 aus seinem Angesicht;", "tokens": ["Noch", "lan\u00b7ges", "Le\u00b7ben", "sah'", "aus", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ihn brach der Tod, wie Sturm die g\u2019rade Tanne bricht,", "tokens": ["Ihn", "brach", "der", "Tod", ",", "wie", "Sturm", "die", "g'\u00b7ra\u00b7de", "Tan\u00b7ne", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Er fiel! die Wittwe raufte nicht,", "tokens": ["Er", "fiel", "!", "die", "Witt\u00b7we", "rauf\u00b7te", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Bey seiner ihr zu fr\u00fchen Bahre", "tokens": ["Bey", "sei\u00b7ner", "ihr", "zu", "fr\u00fc\u00b7hen", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Aus tobendem Gef\u00fchl die Haare,", "tokens": ["Aus", "to\u00b7ben\u00b7dem", "Ge\u00b7f\u00fchl", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sie schlug sich nicht an ihre Brust;", "tokens": ["Sie", "schlug", "sich", "nicht", "an", "ih\u00b7re", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Nein, von zu grossen Schmerz durchdrungen", "tokens": ["Nein", ",", "von", "zu", "gros\u00b7sen", "Schmerz", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "War sie erstarrt, sich selber nicht bewu\u00dft,", "tokens": ["War", "sie", "er\u00b7starrt", ",", "sich", "sel\u00b7ber", "nicht", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PRF", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Denn ihr Verlust war nicht mit Zungen", "tokens": ["Denn", "ihr", "Ver\u00b7lust", "war", "nicht", "mit", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Genug zu klagen, ach! die stumme Traurigkeit", "tokens": ["Ge\u00b7nug", "zu", "kla\u00b7gen", ",", "ach", "!", "die", "stum\u00b7me", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ward noch von Dichtern nicht gesungen,", "tokens": ["Ward", "noch", "von", "Dich\u00b7tern", "nicht", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Von Rednern nicht gesagt. Sie ist ein nagend Leid", "tokens": ["Von", "Red\u00b7nern", "nicht", "ge\u00b7sagt", ".", "Sie", "ist", "ein", "na\u00b7gend", "Leid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$.", "PPER", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und bricht gewaltiger hervor nach kurzer Zeit;", "tokens": ["Und", "bricht", "ge\u00b7wal\u00b7ti\u00b7ger", "her\u00b7vor", "nach", "kur\u00b7zer", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sie str\u00f6mt in ganzer Thr\u00e4nen-Quelle,", "tokens": ["Sie", "str\u00f6mt", "in", "gan\u00b7zer", "Thr\u00e4\u00b7nen\u00b7Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die je ein Auge weinen kann;", "tokens": ["Die", "je", "ein", "Au\u00b7ge", "wei\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Ein nahmenloser Gram liegt vor dem todten Mann", "tokens": ["Ein", "nah\u00b7men\u00b7lo\u00b7ser", "Gram", "liegt", "vor", "dem", "tod\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Am Herzen dieser Frau. Er wird ihr Schlafgeselle;", "tokens": ["Am", "Her\u00b7zen", "die\u00b7ser", "Frau", ".", "Er", "wird", "ihr", "Schlaf\u00b7ge\u00b7sel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Mangel sitzt auf ihrer Schwelle;", "tokens": ["Der", "Man\u00b7gel", "sitzt", "auf", "ih\u00b7rer", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ihn, und acht Kinder sieht sie an,", "tokens": ["Ihn", ",", "und", "acht", "Kin\u00b7der", "sieht", "sie", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So bald die Sonne blickt, die eher noch erscheinet,", "tokens": ["So", "bald", "die", "Son\u00b7ne", "blickt", ",", "die", "e\u00b7her", "noch", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Als diese Mutter Brodt f\u00fcr ihre Kinder hat.", "tokens": ["Als", "die\u00b7se", "Mut\u00b7ter", "Brodt", "f\u00fcr", "ih\u00b7re", "Kin\u00b7der", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Sie netzt den Flachs, indem sie weinet,", "tokens": ["Sie", "netzt", "den", "Flachs", ",", "in\u00b7dem", "sie", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Spinnt, und verkauft das Garn dem H\u00e4ndler in", "tokens": ["Spinnt", ",", "und", "ver\u00b7kauft", "das", "Garn", "dem", "H\u00e4nd\u00b7ler", "in"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "der Stad", "tokens": ["der", "Stad"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "O der Verdienst macht kaum zween kleine Magen", "tokens": ["O", "der", "Ver\u00b7dienst", "macht", "kaum", "zween", "klei\u00b7ne", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "satt!", "tokens": ["satt", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Er kleidet nicht den Sohn, den noch auf ihren Armen", "tokens": ["Er", "klei\u00b7det", "nicht", "den", "Sohn", ",", "den", "noch", "auf", "ih\u00b7ren", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die Amme tragen mu\u00df. Er fodert das Erbarmen", "tokens": ["Die", "Am\u00b7me", "tra\u00b7gen", "mu\u00df", ".", "Er", "fo\u00b7dert", "das", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Mit stammelnder Gewalt von dem, der f\u00fchlen kann.", "tokens": ["Mit", "stam\u00b7meln\u00b7der", "Ge\u00b7walt", "von", "dem", ",", "der", "f\u00fch\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "$,", "PRELS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Seht dies verla\u00dfne Kind, seht diese Mutter an,", "tokens": ["Seht", "dies", "ver\u00b7la\u00df\u00b7ne", "Kind", ",", "seht", "die\u00b7se", "Mut\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "$,", "VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Und gebt ihr einen Theil von dem, was der erworben,", "tokens": ["Und", "gebt", "ihr", "ei\u00b7nen", "Theil", "von", "dem", ",", "was", "der", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "$,", "PRELS", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Der euch gedient, und ihr zu bald gestorben!", "tokens": ["Der", "euch", "ge\u00b7dient", ",", "und", "ihr", "zu", "bald", "ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "KON", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Ihr thuts, ihr seht herab, als wie der Menschen-", "tokens": ["Ihr", "thuts", ",", "ihr", "seht", "her\u00b7ab", ",", "als", "wie", "der", "Men\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,", "KOUS", "KOKOM", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.52": {"text": "freund,", "tokens": ["freund", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.53": {"text": "Ein Engel niedersieht, wenn der Verla\u00df\u2019ne weint.", "tokens": ["Ein", "En\u00b7gel", "nie\u00b7der\u00b7sieht", ",", "wenn", "der", "Ver\u00b7la\u00df'\u00b7ne", "weint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "