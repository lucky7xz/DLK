{"textgrid.poem.36370": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Sehnsucht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flog in Eil vorbei der R\u00e4uber-Adler,", "tokens": ["Es", "flog", "in", "Eil", "vor\u00b7bei", "der", "R\u00e4u\u00b7ber\u00b7Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und scho\u00df hinunter in das Todesmeer.", "tokens": ["Und", "scho\u00df", "hin\u00b7un\u00b7ter", "in", "das", "To\u00b7des\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die strengen W\u00e4chter sind der Frechheit Tadler,", "tokens": ["Die", "stren\u00b7gen", "W\u00e4ch\u00b7ter", "sind", "der", "Frech\u00b7heit", "Tad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getreu bewacht der Hund den rechten Adler,", "tokens": ["Ge\u00b7treu", "be\u00b7wacht", "der", "Hund", "den", "rech\u00b7ten", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Schlange zischt von unten Gift uns her.", "tokens": ["Die", "Schlan\u00b7ge", "zischt", "von", "un\u00b7ten", "Gift", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl dunkle Nacht bedeckt die Welt in Klagen,", "tokens": ["Wohl", "dunk\u00b7le", "Nacht", "be\u00b7deckt", "die", "Welt", "in", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erd' erbebt im ersten Morgenwehn.", "tokens": ["Die", "Erd'", "er\u00b7bebt", "im", "ers\u00b7ten", "Mor\u00b7gen\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O m\u00f6chte endlich doch die Sonne tagen,", "tokens": ["O", "m\u00f6ch\u00b7te", "end\u00b7lich", "doch", "die", "Son\u00b7ne", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hoch der Sieger auf dem Sternenwagen,", "tokens": ["Und", "hoch", "der", "Sie\u00b7ger", "auf", "dem", "Ster\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Glanz das Kreuz am lichten Himmel stehn!", "tokens": ["Im", "Glanz", "das", "Kreuz", "am", "lich\u00b7ten", "Him\u00b7mel", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.pent