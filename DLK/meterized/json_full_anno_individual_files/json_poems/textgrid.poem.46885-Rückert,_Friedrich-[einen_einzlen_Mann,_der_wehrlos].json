{"textgrid.poem.46885": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[einen einzlen Mann, der wehrlos]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen einzlen Mann, der wehrlos", "tokens": ["Ei\u00b7nen", "einz\u00b7len", "Mann", ",", "der", "wehr\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Steht und friedlich angethan,", "tokens": ["Steht", "und", "fried\u00b7lich", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anzugreifen, gilt f\u00fcr ehrlos,", "tokens": ["An\u00b7zu\u00b7grei\u00b7fen", ",", "gilt", "f\u00fcr", "ehr\u00b7los", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch so greift das Ungl\u00fcck an.", "tokens": ["Doch", "so", "greift", "das", "Un\u00b7gl\u00fcck", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von Unf\u00e4llen \u00fcberfallen,", "tokens": ["Von", "Un\u00b7f\u00e4l\u00b7len", "\u00fc\u00b7berf\u00b7al\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann ich mich vor keinem wahren,", "tokens": ["Kann", "ich", "mich", "vor", "kei\u00b7nem", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und soll wahren mich vor allen,", "tokens": ["Und", "soll", "wah\u00b7ren", "mich", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "PRF", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn sie kommen stets in Schaaren.", "tokens": ["Denn", "sie", "kom\u00b7men", "stets", "in", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nur Muth! Denn sie sind feige,", "tokens": ["Doch", "nur", "Muth", "!", "Denn", "sie", "sind", "fei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$.", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Viele Memmen sind kein Mann;", "tokens": ["Vie\u00b7le", "Mem\u00b7men", "sind", "kein", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warst du einer je, so zeige,", "tokens": ["Warst", "du", "ei\u00b7ner", "je", ",", "so", "zei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine Mannheit zeige dann!", "tokens": ["Dei\u00b7ne", "Mann\u00b7heit", "zei\u00b7ge", "dann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf! ob Streich um Streich dich f\u00e4lle,", "tokens": ["Auf", "!", "ob", "Streich", "um", "Streich", "dich", "f\u00e4l\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "KOUS", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Raffe dich empor! und wenn", "tokens": ["Raf\u00b7fe", "dich", "em\u00b7por", "!", "und", "wenn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "KON", "KOUS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie dich stehn sehn, fliehn sie, denn", "tokens": ["Sie", "dich", "stehn", "sehn", ",", "fliehn", "sie", ",", "denn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "VVINF", "VVINF", "$,", "VVFIN", "PPER", "$,", "KON"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Feige sind die Ungl\u00fccksf\u00e4lle.", "tokens": ["Fei\u00b7ge", "sind", "die", "Un\u00b7gl\u00fccks\u00b7f\u00e4l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen einzlen Mann, der wehrlos", "tokens": ["Ei\u00b7nen", "einz\u00b7len", "Mann", ",", "der", "wehr\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Steht und friedlich angethan,", "tokens": ["Steht", "und", "fried\u00b7lich", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "A