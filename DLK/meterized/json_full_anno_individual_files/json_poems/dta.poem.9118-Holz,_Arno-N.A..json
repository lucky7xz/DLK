{"dta.poem.9118": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich \u00f6ffne ein kleines Gitter.", "tokens": ["Ich", "\u00f6ff\u00b7ne", "ein", "klei\u00b7nes", "Git\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4rzgefallnen.", "tokens": ["Die", "M\u00e4rz\u00b7ge\u00b7fall\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ueber den Weg, durch welkes Laub, h\u00fcpfen Schwarzdrosseln,", "tokens": ["Ue\u00b7ber", "den", "Weg", ",", "durch", "wel\u00b7kes", "Laub", ",", "h\u00fcp\u00b7fen", "Schwarz\u00b7dros\u00b7seln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PWAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "um verwitternde Kreuze im Sonnenlicht spielen glitzernde F\u00e4den.", "tokens": ["um", "ver\u00b7wit\u00b7tern\u00b7de", "Kreu\u00b7ze", "im", "Son\u00b7nen\u00b7licht", "spie\u00b7len", "glit\u00b7zern\u00b7de", "F\u00e4\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "--+--+--+--+-+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "In einer Ecke,", "tokens": ["In", "ei\u00b7ner", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u2014 der Epheu blinkt, ich b\u00fccke mich \u2014", "tokens": ["der", "E\u00b7pheu", "blinkt", ",", "ich", "b\u00fc\u00b7cke", "mich"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auf einem Stein, liegen Rosen.", "tokens": ["auf", "ei\u00b7nem", "Stein", ",", "lie\u00b7gen", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "D\u00fcnne Ranken, graues Moos und Thautropfen.", "tokens": ["D\u00fcn\u00b7ne", "Ran\u00b7ken", ",", "grau\u00b7es", "Moos", "und", "Thau\u00b7trop\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Die alten Buchstaben sind kaum mehr zu lesen.", "tokens": ["Die", "al\u00b7ten", "Buch\u00b7sta\u00b7ben", "sind", "kaum", "mehr", "zu", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Mit M\u00fche nur entziffre ich:", "tokens": ["Mit", "M\u00fc\u00b7he", "nur", "ent\u00b7ziff\u00b7re", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}}}}