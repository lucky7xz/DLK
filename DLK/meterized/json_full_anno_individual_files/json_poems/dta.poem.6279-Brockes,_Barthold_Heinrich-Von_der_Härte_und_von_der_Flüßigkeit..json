{"dta.poem.6279": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern ihr Trieb nun nicht gehemmt,", "tokens": ["Wo\u00b7fern", "ihr", "Trieb", "nun", "nicht", "ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wir sie nicht, durch Ufer einged\u00e4mmt,", "tokens": ["Und", "wir", "sie", "nicht", ",", "durch", "U\u00b7fer", "ein\u00b7ge\u00b7d\u00e4mmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "PTKNEG", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ein Gef\u00e4\u00dfe schl\u00fc\u00dfen,", "tokens": ["In", "ein", "Ge\u00b7f\u00e4\u00b7\u00dfe", "schl\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist die Feuchtigkeit stets fertig weg zu fl\u00fcssen.", "tokens": ["So", "ist", "die", "Feuch\u00b7tig\u00b7keit", "stets", "fer\u00b7tig", "weg", "zu", "fl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}