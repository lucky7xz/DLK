{"textgrid.poem.49880": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aufgeschm\u00fcckt ist der Freudensaal.", "tokens": ["Auf\u00b7ge\u00b7schm\u00fcckt", "ist", "der", "Freu\u00b7den\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Lichterhell, bunt, in laulicher Sommernacht", "tokens": ["Lich\u00b7ter\u00b7hell", ",", "bunt", ",", "in", "lau\u00b7li\u00b7cher", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stehet das offene Gartengezelte.", "tokens": ["Ste\u00b7het", "das", "of\u00b7fe\u00b7ne", "Gar\u00b7ten\u00b7ge\u00b7zel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "S\u00e4ulengleich steigen, gepaart,", "tokens": ["S\u00e4u\u00b7len\u00b7gleich", "stei\u00b7gen", ",", "ge\u00b7paart", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Gr\u00fcn-umranket, eherne Schlangen,", "tokens": ["Gr\u00fcn\u00b7um\u00b7ran\u00b7ket", ",", "e\u00b7her\u00b7ne", "Schlan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zw\u00f6lf, mit verschlungenen H\u00e4lsen,", "tokens": ["Zw\u00f6lf", ",", "mit", "ver\u00b7schlun\u00b7ge\u00b7nen", "H\u00e4l\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Tragend und st\u00fctzend das", "tokens": ["Tra\u00b7gend", "und", "st\u00fct\u00b7zend", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ART"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Leicht gegitterte Dach.", "tokens": ["Leicht", "ge\u00b7git\u00b7ter\u00b7te", "Dach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aber die Braut noch wartet verborgen", "tokens": ["A\u00b7ber", "die", "Braut", "noch", "war\u00b7tet", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "VVPP"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In dem K\u00e4mmerlein ihres Hauses.", "tokens": ["In", "dem", "K\u00e4m\u00b7mer\u00b7lein", "ih\u00b7res", "Hau\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.3": {"text": "Endlich bewegt sich der Zug der Hochzeit,", "tokens": ["End\u00b7lich", "be\u00b7wegt", "sich", "der", "Zug", "der", "Hoch\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Fackeln tragend,", "tokens": ["Fa\u00b7ckeln", "tra\u00b7gend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Feierlich stumm.", "tokens": ["Fei\u00b7er\u00b7lich", "stumm", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und in der Mitte,", "tokens": ["Und", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.7": {"text": "Mich an der rechten Hand,", "tokens": ["Mich", "an", "der", "rech\u00b7ten", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schwarz gekleidet, geht einfach die Braut;", "tokens": ["Schwarz", "ge\u00b7klei\u00b7det", ",", "geht", "ein\u00b7fach", "die", "Braut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sch\u00f6ngefaltet ein Scharlachtuch", "tokens": ["Sch\u00f6n\u00b7ge\u00b7fal\u00b7tet", "ein", "Schar\u00b7lach\u00b7tuch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Liegt um den zierlichen Kopf geschlagen.", "tokens": ["Liegt", "um", "den", "zier\u00b7li\u00b7chen", "Kopf", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "L\u00e4chelnd geht sie dahin; das Mahl schon duftet.", "tokens": ["L\u00e4\u00b7chelnd", "geht", "sie", "da\u00b7hin", ";", "das", "Mahl", "schon", "duf\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PAV", "$.", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sp\u00e4ter im L\u00e4rmen des Fests", "tokens": ["Sp\u00e4\u00b7ter", "im", "L\u00e4r\u00b7men", "des", "Fests"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Stahlen wir seitw\u00e4rts uns beide", "tokens": ["Stah\u00b7len", "wir", "seit\u00b7w\u00e4rts", "uns", "bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PIS"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Weg, nach den Schatten des Gartens wandelnd,", "tokens": ["Weg", ",", "nach", "den", "Schat\u00b7ten", "des", "Gar\u00b7tens", "wan\u00b7delnd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo im Geb\u00fcsche die Rosen brannten,", "tokens": ["Wo", "im", "Ge\u00b7b\u00fc\u00b7sche", "die", "Ro\u00b7sen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo der Mondstrahl um Lilien zuckte,", "tokens": ["Wo", "der", "Mond\u00b7strahl", "um", "Li\u00b7li\u00b7en", "zuck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wo die Weymouthsfichte mit schwarzem Haar", "tokens": ["Wo", "die", "Wey\u00b7mouths\u00b7fich\u00b7te", "mit", "schwar\u00b7zem", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den Spiegel des Teiches halb verh\u00e4ngt.", "tokens": ["Den", "Spie\u00b7gel", "des", "Tei\u00b7ches", "halb", "ver\u00b7h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Auf seidnem Rasen dort, ach, Herz am Herzen,", "tokens": ["Auf", "seid\u00b7nem", "Ra\u00b7sen", "dort", ",", "ach", ",", "Herz", "am", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ITJ", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie verschlangen, erstickten meine K\u00fcsse den scheueren Ku\u00df!", "tokens": ["Wie", "ver\u00b7schlan\u00b7gen", ",", "er\u00b7stick\u00b7ten", "mei\u00b7ne", "K\u00fcs\u00b7se", "den", "scheu\u00b7e\u00b7ren", "Ku\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Indes der Springquell, unteilnehmend", "tokens": ["In\u00b7des", "der", "Spring\u00b7quell", ",", "un\u00b7teil\u00b7neh\u00b7mend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An \u00fcberschwenglicher Liebe Gefl\u00fcster,", "tokens": ["An", "\u00fc\u00b7bersc\u00b7hweng\u00b7li\u00b7cher", "Lie\u00b7be", "Ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Sich ewig des eigenen Pl\u00e4tscherns freute;", "tokens": ["Sich", "e\u00b7wig", "des", "ei\u00b7ge\u00b7nen", "Pl\u00e4t\u00b7scherns", "freu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Uns aber neckten von fern und lockten", "tokens": ["Uns", "a\u00b7ber", "neck\u00b7ten", "von", "fern", "und", "lock\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJD", "KON", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Freundliche Stimmen,", "tokens": ["Freund\u00b7li\u00b7che", "Stim\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Fl\u00f6ten und Saiten umsonst.", "tokens": ["Fl\u00f6\u00b7ten", "und", "Sai\u00b7ten", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Erm\u00fcdet lag, zu bald f\u00fcr mein Verlangen,", "tokens": ["Er\u00b7m\u00fc\u00b7det", "lag", ",", "zu", "bald", "f\u00fcr", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das leichte, liebe Haupt auf meinem Scho\u00df.", "tokens": ["Das", "leich\u00b7te", ",", "lie\u00b7be", "Haupt", "auf", "mei\u00b7nem", "Scho\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Spielender Weise mein Aug auf ihres dr\u00fcckend", "tokens": ["Spie\u00b7len\u00b7der", "Wei\u00b7se", "mein", "Aug", "auf", "ih\u00b7res", "dr\u00fc\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPOSAT", "NN", "APPR", "PPOSAT", "VVPP"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "F\u00fchlt ich ein Weilchen die langen Wimpern,", "tokens": ["F\u00fchlt", "ich", "ein", "Weil\u00b7chen", "die", "lan\u00b7gen", "Wim\u00b7pern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bis der Schlaf sie stellte,", "tokens": ["Bis", "der", "Schlaf", "sie", "stell\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Wie Schmetterlingsgefieder auf und nieder gehn.", "tokens": ["Wie", "Schmet\u00b7ter\u00b7lings\u00b7ge\u00b7fie\u00b7der", "auf", "und", "nie\u00b7der", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "KON", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Eh das Fr\u00fchrot schien,", "tokens": ["Eh", "das", "Fr\u00fch\u00b7rot", "schien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eh das L\u00e4mpchen erlosch im Brautgemache,", "tokens": ["Eh", "das", "L\u00e4mp\u00b7chen", "er\u00b7losch", "im", "Braut\u00b7ge\u00b7ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Weckt ich die Schl\u00e4ferin,", "tokens": ["Weckt", "ich", "die", "Schl\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchrte das seltsame Kind in mein Haus ein.", "tokens": ["F\u00fchr\u00b7te", "das", "selt\u00b7sa\u00b7me", "Kind", "in", "mein", "Haus", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Aufgeschm\u00fcckt ist der Freudensaal.", "tokens": ["Auf\u00b7ge\u00b7schm\u00fcckt", "ist", "der", "Freu\u00b7den\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Lichterhell, bunt, in laulicher Sommernacht", "tokens": ["Lich\u00b7ter\u00b7hell", ",", "bunt", ",", "in", "lau\u00b7li\u00b7cher", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Stehet das offene Gartengezelte.", "tokens": ["Ste\u00b7het", "das", "of\u00b7fe\u00b7ne", "Gar\u00b7ten\u00b7ge\u00b7zel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "S\u00e4ulengleich steigen, gepaart,", "tokens": ["S\u00e4u\u00b7len\u00b7gleich", "stei\u00b7gen", ",", "ge\u00b7paart", ","], "token_info": ["word", "word", "punct", "w