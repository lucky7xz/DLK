{"dta.poem.12337": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die mystische Wurzel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Jesse kommt ein Wurzel zart,               ", "tokens": ["Von", "Jes\u00b7se", "kommt", "ein", "Wur\u00b7zel", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daraus ein Zweig von Wunderart,", "tokens": ["Da\u00b7raus", "ein", "Zweig", "von", "Wun\u00b7der\u00b7art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Zweig ein sch\u00f6nes R\u00f6slein bringt,", "tokens": ["Der", "Zweig", "ein", "sch\u00f6\u00b7nes", "R\u00f6s\u00b7lein", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wunderlich vom Zweig entspringt.", "tokens": ["Das", "wun\u00b7der\u00b7lich", "vom", "Zweig", "ent\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Wurzel der Stamm Davids ist,", "tokens": ["Die", "Wur\u00b7zel", "der", "Stamm", "Da\u00b7vids", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "VAFIN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Maria, du das Zweiglein bist,", "tokens": ["Ma\u00b7ria", ",", "du", "das", "Zwei\u00b7glein", "bist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Sohn, die Blum, die sch\u00f6ne Ros,", "tokens": ["Dein", "Sohn", ",", "die", "Blum", ",", "die", "sch\u00f6\u00b7ne", "Ros", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Gott und Mensch in deinem Schos.", "tokens": ["Ist", "Gott", "und", "Mensch", "in", "dei\u00b7nem", "Schos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der heilig' Geist von dir allein,", "tokens": ["Der", "hei\u00b7lig'", "Geist", "von", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschaffen hat das Kindlein fein,", "tokens": ["Er\u00b7schaf\u00b7fen", "hat", "das", "Kin\u00b7dlein", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleichwie die Sonn durch ihre Kraft,", "tokens": ["Gleich\u00b7wie", "die", "Sonn", "durch", "ih\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein von Zweiglein Rosen schafft.", "tokens": ["Al\u00b7lein", "von", "Zwei\u00b7glein", "Ro\u00b7sen", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Wunderwerk! auf einem Stiel", "tokens": ["O", "Wun\u00b7der\u00b7werk", "!", "auf", "ei\u00b7nem", "Stiel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stehn R\u00f6slein und auch Bl\u00e4tter viel,", "tokens": ["Stehn", "R\u00f6s\u00b7lein", "und", "auch", "Bl\u00e4t\u00b7ter", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Wunderwerk! in Gottes Sohn", "tokens": ["O", "Wun\u00b7der\u00b7werk", "!", "in", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind zwei Naturen in Person.", "tokens": ["Sind", "zwei", "Na\u00b7tu\u00b7ren", "in", "Per\u00b7son", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPR", "NE", "$."], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Roth ist die Ros, gr\u00fcn ist das Blat,", "tokens": ["Roth", "ist", "die", "Ros", ",", "gr\u00fcn", "ist", "das", "Blat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Zweiglein gleichwohl beide hat,", "tokens": ["Ein", "Zwei\u00b7glein", "gleich\u00b7wohl", "bei\u00b7de", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Also man zwei Naturen findt,", "tokens": ["Al\u00b7so", "man", "zwei", "Na\u00b7tu\u00b7ren", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein Person in diesem Kind.", "tokens": ["Und", "ein", "Per\u00b7son", "in", "die\u00b7sem", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Zweig! dich ziert die sch\u00f6ne Blum,", "tokens": ["O", "Zweig", "!", "dich", "ziert", "die", "sch\u00f6\u00b7ne", "Blum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ros dir bring", "tokens": ["Die", "Ros", "dir", "bring"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Ros das Zweiglein nicht verstellt,", "tokens": ["Die", "Ros", "das", "Zwei\u00b7glein", "nicht", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Jungfrauschaft dein Kind erh\u00e4lt.", "tokens": ["Dein", "Jung\u00b7frausc\u00b7haft", "dein", "Kind", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}