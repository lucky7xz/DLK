{"dta.poem.20759": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Eine Gesundheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor alten Zeiten war's Gebrauch,               ", "tokens": ["Vor", "al\u00b7ten", "Zei\u00b7ten", "wa\u00b7r's", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Namen weltber\u00fchmter Todten", "tokens": ["Den", "Na\u00b7men", "welt\u00b7be\u00b7r\u00fchm\u00b7ter", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu nennen bei den Gastgeboten.", "tokens": ["Zu", "nen\u00b7nen", "bei", "den", "Gast\u00b7ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen die Gewohnheit auch", "tokens": ["Wir", "wol\u00b7len", "die", "Ge\u00b7wohn\u00b7heit", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bei unserm Tafelrund erneuern.", "tokens": ["Bei", "un\u00b7serm", "Ta\u00b7fel\u00b7rund", "er\u00b7neu\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hebt eure Becher in die H\u00f6h,", "tokens": ["Hebt", "eu\u00b7re", "Be\u00b7cher", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den gro\u00dfen Namen hoch zu feiern:", "tokens": ["Den", "gro\u00b7\u00dfen", "Na\u00b7men", "hoch", "zu", "fei\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Friedrich der Einzige &#8212;             ", "tokens": ["Fried\u00b7rich", "der", "Ein\u00b7zi\u00b7ge", "&#8212;"], "token_info": ["word", "word", "word", "XML_entity"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}