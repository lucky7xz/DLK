{"dta.poem.6106": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De la Pesanteur et de la Lege -  \n rete\u2019, du Flux et Reflux  \n de la Mer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Gluht, die abwerts sch\u00fc\u00df\u2019t, die rollende Planeten,", "tokens": ["Die", "Gluht", ",", "die", "ab\u00b7werts", "sch\u00fc\u00df't", ",", "die", "rol\u00b7len\u00b7de", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zusammt den drohenden Cometen,", "tokens": ["Zu\u00b7sammt", "den", "dro\u00b7hen\u00b7den", "Co\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die C\u00f6rper, so man f\u00fchlt, die Erde sammt dem Meer,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "so", "man", "f\u00fchlt", ",", "die", "Er\u00b7de", "sammt", "dem", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "VVFIN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja alles, was man kennt,", "tokens": ["Ja", "al\u00b7les", ",", "was", "man", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Stammt aus dem dritten Element,", "tokens": ["Stammt", "aus", "dem", "drit\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seiner so verschiednen Mischung her.", "tokens": ["Und", "sei\u00b7ner", "so", "ver\u00b7schied\u00b7nen", "Misc\u00b7hung", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}