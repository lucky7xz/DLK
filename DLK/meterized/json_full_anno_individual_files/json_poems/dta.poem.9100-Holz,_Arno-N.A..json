{"dta.poem.9100": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich schuf Korinth, ich sah das Meer.", "tokens": ["Mich", "schuf", "Ko\u00b7rinth", ",", "ich", "sah", "das", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Tausend Jahre", "tokens": ["Tau\u00b7send", "Jah\u00b7re"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "unter Schutt und Tempeltr\u00fcmmern", "tokens": ["un\u00b7ter", "Schutt", "und", "Tem\u00b7pel\u00b7tr\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lag ich in schwarzer Erde.", "tokens": ["lag", "ich", "in", "schwar\u00b7zer", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwischen roten Disteln im Abendschein weideten Ziegen,", "tokens": ["Zwi\u00b7schen", "ro\u00b7ten", "Dis\u00b7teln", "im", "A\u00b7bend\u00b7schein", "wei\u00b7de\u00b7ten", "Zie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00fcber mein bl\u00fchendes Grab bliesen Hirten.", "tokens": ["\u00fc\u00b7ber", "mein", "bl\u00fc\u00b7hen\u00b7des", "Grab", "blie\u00b7sen", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Tausend Jahre war ich tot.", "tokens": ["Tau\u00b7send", "Jah\u00b7re", "war", "ich", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heut scheint die Sonne, der Himmel lacht, ich lebe!", "tokens": ["Heut", "scheint", "die", "Son\u00b7ne", ",", "der", "Him\u00b7mel", "lacht", ",", "ich", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Im alten Park", "tokens": ["Im", "al\u00b7ten", "Park"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "steh ich nackt aus weissem Marmor.", "tokens": ["steh", "ich", "nackt", "aus", "weis\u00b7sem", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf meine Schultern", "tokens": ["Auf", "mei\u00b7ne", "Schul\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "durch gezacktes Laub", "tokens": ["durch", "ge\u00b7zack\u00b7tes", "Laub"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "fallen zitternde Tupfen.", "tokens": ["fal\u00b7len", "zit\u00b7tern\u00b7de", "Tup\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Meine Augen,", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "weit ge\u00f6ffnet,", "tokens": ["weit", "ge\u00b7\u00f6ff\u00b7net", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "starren auf ein gr\u00fcnes Wasser.", "tokens": ["star\u00b7ren", "auf", "ein", "gr\u00fc\u00b7nes", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "In breiten, \u00fcberh\u00e4ngenden Kastanienbl\u00e4ttern", "tokens": ["In", "brei\u00b7ten", ",", "\u00fc\u00b7ber\u00b7h\u00e4n\u00b7gen\u00b7den", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "spiegelt sich und zuckt", "tokens": ["spie\u00b7gelt", "sich", "und", "zuckt"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "sein Licht.", "tokens": ["sein", "Licht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}