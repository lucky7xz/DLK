{"dta.poem.7341": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von der Jugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie der zitternde Verbannte", "tokens": ["Wie", "der", "zit\u00b7tern\u00b7de", "Ver\u00b7bann\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht an seiner Heimath Gr\u00e4nzen,", "tokens": ["Steht", "an", "sei\u00b7ner", "Hei\u00b7math", "Gr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fcckw\u00e4rts er das Antlitz wendet,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "er", "das", "Ant\u00b7litz", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fcckw\u00e4rts seine Augen gl\u00e4nzen,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "sei\u00b7ne", "Au\u00b7gen", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winde die hin\u00fcber streichen,", "tokens": ["Win\u00b7de", "die", "hin\u00b7\u00fc\u00b7ber", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "V\u00f6gel in der Luft beneidet,", "tokens": ["V\u00f6\u00b7gel", "in", "der", "Luft", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaudernd vor der kleinen Scholle,", "tokens": ["Schau\u00b7dernd", "vor", "der", "klei\u00b7nen", "Schol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die das Land vom Lande scheidet;", "tokens": ["Die", "das", "Land", "vom", "Lan\u00b7de", "schei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Gr\u00e4ber seiner Todten,", "tokens": ["Wie", "die", "Gr\u00e4\u00b7ber", "sei\u00b7ner", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Lebenden, die s\u00fc\u00dfen,", "tokens": ["Sei\u00b7ne", "Le\u00b7ben\u00b7den", ",", "die", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle stehn am Horizonte,", "tokens": ["Al\u00b7le", "stehn", "am", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er mu\u00df sie weinend gr\u00fc\u00dfen;", "tokens": ["Und", "er", "mu\u00df", "sie", "wei\u00b7nend", "gr\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle kleinen Liebessch\u00e4tze,", "tokens": ["Al\u00b7le", "klei\u00b7nen", "Lie\u00b7bes\u00b7sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unerkannt und unempfunden,", "tokens": ["Un\u00b7er\u00b7kannt", "und", "un\u00b7em\u00b7pfun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle ihn wie S\u00fcnden brennen", "tokens": ["Al\u00b7le", "ihn", "wie", "S\u00fcn\u00b7den", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PPER", "KOKOM", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und wie ewig offne Wunden;", "tokens": ["Und", "wie", "e\u00b7wig", "off\u00b7ne", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So an seiner Jugend Scheide", "tokens": ["So", "an", "sei\u00b7ner", "Ju\u00b7gend", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein Herz voll stolzer Tr\u00e4ume,", "tokens": ["Steht", "ein", "Herz", "voll", "stol\u00b7zer", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt in ihre Paradiese", "tokens": ["Blickt", "in", "ih\u00b7re", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Zukunft \u00f6de R\u00e4ume,", "tokens": ["Und", "der", "Zu\u00b7kunft", "\u00f6\u00b7de", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seine Neigungen, verk\u00fcmmert,", "tokens": ["Sei\u00b7ne", "Nei\u00b7gun\u00b7gen", ",", "ver\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Seine Hoffnungen, begraben,", "tokens": ["Sei\u00b7ne", "Hoff\u00b7nun\u00b7gen", ",", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle stehn am Horizonte,", "tokens": ["Al\u00b7le", "stehn", "am", "Ho\u00b7ri\u00b7zon\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollen ihre Thr\u00e4ne haben.", "tokens": ["Wol\u00b7len", "ih\u00b7re", "Thr\u00e4\u00b7ne", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die Jahre die sich langsam,", "tokens": ["Und", "die", "Jah\u00b7re", "die", "sich", "lang\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00fcckisch reihten aus Minuten,", "tokens": ["T\u00fc\u00b7ckisch", "reih\u00b7ten", "aus", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle brechen auf im Herzen,", "tokens": ["Al\u00b7le", "bre\u00b7chen", "auf", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle nun wie Wunden bluten;", "tokens": ["Al\u00b7le", "nun", "wie", "Wun\u00b7den", "blu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der armen kargen Habe,", "tokens": ["Mit", "der", "ar\u00b7men", "kar\u00b7gen", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus so reichem Schacht erbeutet,", "tokens": ["Aus", "so", "rei\u00b7chem", "Schacht", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Muthlos, ein gebrochner Wandrer,", "tokens": ["Muth\u00b7los", ",", "ein", "ge\u00b7broch\u00b7ner", "Wand\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In das fremde Land er schreitet.", "tokens": ["In", "das", "frem\u00b7de", "Land", "er", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und doch ist des Sommers Garbe", "tokens": ["Und", "doch", "ist", "des", "Som\u00b7mers", "Gar\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht geringer als die Bl\u00fcthen,", "tokens": ["Nicht", "ge\u00b7rin\u00b7ger", "als", "die", "Bl\u00fc\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nur in der feuchten Scholle", "tokens": ["Und", "nur", "in", "der", "feuch\u00b7ten", "Schol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kann der frische Keim sich h\u00fcten;", "tokens": ["Kann", "der", "fri\u00b7sche", "Keim", "sich", "h\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber Fels und \u00f6de Fl\u00e4chen", "tokens": ["Ue\u00b7ber", "Fels", "und", "\u00f6\u00b7de", "Fl\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df der Strom, da\u00df er sich breite,", "tokens": ["Mu\u00df", "der", "Strom", ",", "da\u00df", "er", "sich", "brei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es segnet Gottes Rechte", "tokens": ["Und", "es", "seg\u00b7net", "Got\u00b7tes", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uebermorgen so wie heute.", "tokens": ["Ue\u00b7ber\u00b7mor\u00b7gen", "so", "wie", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}