elbst", "ver\u00b7nahm", "in", "stil\u00b7ler", "Nacht", "den", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nachklang schon oft, wie ferner Saiten Schwingen;", "tokens": ["Nach\u00b7klang", "schon", "oft", ",", "wie", "fer\u00b7ner", "Sai\u00b7ten", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "PWAV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir war, als s\u00e4ngen sie von ew'gen Dingen,", "tokens": ["Mir", "war", ",", "als", "s\u00e4n\u00b7gen", "sie", "von", "ew'\u00b7gen", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als h\u00f6rt' ich Gott und seine Wunder preisen.", "tokens": ["Als", "h\u00f6rt'", "ich", "Gott", "und", "sei\u00b7ne", "Wun\u00b7der", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wei\u00df ich auch hienieden lichte Sph\u00e4ren,", "tokens": ["Doch", "wei\u00df", "ich", "auch", "hien\u00b7ie\u00b7den", "lich\u00b7te", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem ew'gen Born der Seligkeit entquollen,", "tokens": ["Dem", "ew'\u00b7gen",