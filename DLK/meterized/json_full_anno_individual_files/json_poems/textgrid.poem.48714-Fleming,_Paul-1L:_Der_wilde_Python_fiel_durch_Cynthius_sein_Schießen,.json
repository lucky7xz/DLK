, "\u00dc\u00b7berw\u00b7in\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und Schreiber auch gehabt. So unser Wild nichts minder,", "tokens": ["und", "Schrei\u00b7ber", "auch", "ge\u00b7habt", ".", "So", "un\u00b7ser", "Wild", "nichts", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAPP", "$.", "ADV", "PPOSAT", "NN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das wahre wilde Wild, das \u00fcber Pest und Gift", "tokens": ["das", "wah\u00b7re", "wil\u00b7de", "Wild", ",", "das", "\u00fc\u00b7ber", "Pest", "und", "Gift"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "und so viel grimmer ist, als st\u00e4rker's ist f\u00fcr allen.", "tokens": ["und", "so", "viel", "grim\u00b7mer", "ist", ",", "als", "st\u00e4r\u00b7ker's", "ist", "f\u00fcr", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "VAFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleich wird des Helden Ruhm und Schreibers Preis erschallen.", "tokens": ["Gleich", "wird", "des", "Hel\u00b7den", "Ruhm", "und", "Schrei\u00b7bers", "Preis", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "KON", "NN", "NN", "VVINF", "