{"textgrid.poem.48714": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der wilde Python fiel durch Cynthius sein Schie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wilde Python fiel durch Cynthius sein Schie\u00dfen,", "tokens": ["Der", "wil\u00b7de", "Py\u00b7thon", "fiel", "durch", "Cyn\u00b7thi\u00b7us", "sein", "Schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alcides schlug beherzt die b\u00f6se Schlange tot,", "tokens": ["Al\u00b7ci\u00b7des", "schlug", "be\u00b7herzt", "die", "b\u00f6\u00b7se", "Schlan\u00b7ge", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der starke Walfisch wird durch Perseus Faust zu Kot',", "tokens": ["der", "star\u00b7ke", "Wal\u00b7fisch", "wird", "durch", "Per\u00b7seus", "Faust", "zu", "Kot'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NE", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf Kadmus Rennen sank das Wild zu seinen F\u00fc\u00dfen.", "tokens": ["auf", "Kad\u00b7mus", "Ren\u00b7nen", "sank", "das", "Wild", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+