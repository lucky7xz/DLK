{"textgrid.poem.65133": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist's denn nicht mehr schon als genug,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's denn nicht mehr schon als genug,", "tokens": ["Ist's", "denn", "nicht", "mehr", "schon", "als", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "ADV", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im eig'nen Glanz dich zu erblicken?", "tokens": ["Im", "eig'\u00b7nen", "Glanz", "dich", "zu", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft du auch noch durch holden Trug,", "tokens": ["Mu\u00dft", "du", "auch", "noch", "durch", "hol\u00b7den", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch s\u00fc\u00dfe T\u00e4uschung uns bestricken? \u2013", "tokens": ["Durch", "s\u00fc\u00b7\u00dfe", "T\u00e4u\u00b7schung", "uns", "be\u00b7stri\u00b7cken", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nein! nicht Trug und T\u00e4uschung nicht,", "tokens": ["Doch", "nein", "!", "nicht", "Trug", "und", "T\u00e4u\u00b7schung", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KO