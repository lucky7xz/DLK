{"textgrid.poem.65926": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "O Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O birg in k\u00fchnem Leben Sonnenstunden,", "tokens": ["O", "birg", "in", "k\u00fch\u00b7nem", "Le\u00b7ben", "Son\u00b7nen\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Wesen bleib um Wonne hold in Sorgen;", "tokens": ["Dein", "We\u00b7sen", "bleib", "um", "Won\u00b7ne", "hold", "in", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne schenkt sich fordernd: borg zu borgen,", "tokens": ["Die", "Son\u00b7ne", "schenkt", "sich", "for\u00b7dernd", ":", "borg", "zu", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVPP", "$.", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df eignen \u00dcberschwang durch Gold umrunden!", "tokens": ["La\u00df", "eig\u00b7nen", "\u00dc\u00b7bersc\u00b7hwang", "durch", "Gold", "um\u00b7run\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie froh gek\u00fchlte Sommermonde munden!", "tokens": ["Wie", "froh", "ge\u00b7k\u00fchl\u00b7te", "Som\u00b7mer\u00b7mon\u00b7de", "mun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Weinesr\u00f6te loht ein stolzer Morgen", "tokens": ["In", "Wei\u00b7nes\u00b7r\u00f6\u00b7te", "loht", "ein", "stol\u00b7zer", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ewigen Erwachtheit traut geborgen:", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Er\u00b7wacht\u00b7heit", "traut", "ge\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Tag in uns singt alte Sonnenkunden.", "tokens": ["Sein", "Tag", "in", "uns", "singt", "al\u00b7te", "Son\u00b7nen\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O Sonne, meine Seele sei ein Garten,", "tokens": ["O", "Son\u00b7ne", ",", "mei\u00b7ne", "See\u00b7le", "sei", "ein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ber\u00fchre sacht der Triebe zartes Spr\u00fchen:", "tokens": ["Be\u00b7r\u00fch\u00b7re", "sacht", "der", "Trie\u00b7be", "zar\u00b7tes", "Spr\u00fc\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"te