{"textgrid.poem.36241": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was w\u00fcnschen und was streben alle Sinnen? \u2013", "tokens": ["Was", "w\u00fcn\u00b7schen", "und", "was", "stre\u00b7ben", "al\u00b7le", "Sin\u00b7nen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "KON", "PWS", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie m\u00f6chten wieder in das All verschweben.", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "wie\u00b7der", "in", "das", "All", "ver\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ist das h\u00f6chste Ziel von allem Streben?", "tokens": ["Was", "ist", "das", "h\u00f6chs\u00b7te", "Ziel", "von", "al\u00b7lem", "Stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es will der Mensch, wenn er verkl\u00e4rt, von hinnen.", "tokens": ["Es", "will", "der", "Mensch", ",", "wenn", "er", "ver\u00b7kl\u00e4rt", ",", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum wollt ihr, sel'gen G\u00f6tter! Dank gewinnen", "tokens": ["Drum", "wollt", "ihr", ",", "sel'\u00b7gen", "G\u00f6t\u00b7ter", "!", "Dank", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem, der hohem Dienste sich ergeben,", "tokens": ["Von", "dem", ",", "der", "ho\u00b7hem", "Diens\u00b7te", "sich", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In heiliger Natur nur lebt sein Leben,", "tokens": ["In", "hei\u00b7li\u00b7ger", "Na\u00b7tur", "nur", "lebt", "sein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So la\u00dft ihn schnell in leichten Duft zerrinnen.", "tokens": ["So", "la\u00dft", "ihn", "schnell", "in", "leich\u00b7ten", "Duft", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es schwebt die Seele gern auf s\u00fc\u00dfen T\u00f6nen,", "tokens": ["Es", "schwebt", "die", "See\u00b7le", "gern", "auf", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lauschet sinnend, was es wohl verk\u00fcnde,", "t