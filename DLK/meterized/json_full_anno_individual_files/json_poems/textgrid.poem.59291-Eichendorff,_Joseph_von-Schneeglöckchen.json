{"textgrid.poem.59291": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Schneegl\u00f6ckchen", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s war doch wie ein leises Singen", "tokens": ["'s", "war", "doch", "wie", "ein", "lei\u00b7ses", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dem Garten heute nacht,", "tokens": ["In", "dem", "Gar\u00b7ten", "heu\u00b7te", "nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie wenn laue L\u00fcfte gingen:", "tokens": ["Wie", "wenn", "lau\u00b7e", "L\u00fcf\u00b7te", "gin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbs\u00fc\u00dfe Gl\u00f6cklein, nun erwacht,", "tokens": ["\u00bb", "s\u00fc\u00b7\u00dfe", "Gl\u00f6c\u00b7klein", ",", "nun", "er\u00b7wacht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die warme Zeit wir bringen,", "tokens": ["Denn", "die", "war\u00b7me", "Zeit", "wir", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh's noch jemand hat gedacht.\u00ab \u2013", "tokens": ["Eh's", "noch", "je\u00b7mand", "hat", "ge\u00b7dacht", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "ADV", "PIS", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "'s war kein Singen, 's war ein K\u00fcssen,", "tokens": ["'s", "war", "kein", "Sin\u00b7gen", ",", "'s", "war", "ein", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "R\u00fchrt' die stillen Gl\u00f6cklein sacht,", "tokens": ["R\u00fchrt'", "die", "stil\u00b7len", "Gl\u00f6c\u00b7klein", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie alle t\u00f6nen m\u00fcssen", "tokens": ["Da\u00df", "sie", "al\u00b7le", "t\u00f6\u00b7nen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Von der k\u00fcnft'gen bunten Pracht.", "tokens": ["Von", "der", "k\u00fcnft'\u00b7gen", "bun\u00b7ten", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ach, sie konnten's nicht erwarten,", "tokens": ["Ach", ",", "sie", "konn\u00b7ten's", "nicht", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Aber wei\u00df vom letzten Schnee", "tokens": ["A\u00b7ber", "wei\u00df", "vom", "letz\u00b7ten", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "War noch immer Feld und Garten,", "tokens": ["War", "noch", "im\u00b7mer", "Feld", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Und sie sanken um vor Weh.", "tokens": ["Und", "sie", "san\u00b7ken", "um", "vor", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "So schon manche Dichter streckten", "tokens": ["So", "schon", "man\u00b7che", "Dich\u00b7ter", "streck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sangesm\u00fcde sich hinab,", "tokens": ["San\u00b7ges\u00b7m\u00fc\u00b7de", "sich", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Und der Fr\u00fchling, den sie weckten,", "tokens": ["Und", "der", "Fr\u00fch\u00b7ling", ",", "den", "sie", "weck\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.18": {"text": "Rauschet \u00fcber ihrem Grab.", "tokens": ["Rau\u00b7schet", "\u00fc\u00b7ber", "ih\u00b7rem", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "tr