{"textgrid.poem.49271": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4fferey von der Nimfen Hercinie", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Monde machte gleich mehr Stunden zu den Tr\u00e4umen,", "tokens": ["Der", "Mon\u00b7de", "mach\u00b7te", "gleich", "mehr", "Stun\u00b7den", "zu", "den", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Stock stundt ohne Wein, das Obst war von den B\u00e4umen,", "tokens": ["Der", "Stock", "stundt", "oh\u00b7ne", "Wein", ",", "das", "Obst", "war", "von", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der strenge Nortwindt nam den P\u00fcschen ihre Zier", "tokens": ["Der", "stren\u00b7ge", "Nort\u00b7windt", "nam", "den", "P\u00fc\u00b7schen", "ih\u00b7re", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und auff die Wage tratt der Scorpion herf\u00fcr.", "tokens": ["Und", "auff", "die", "Wa\u00b7ge", "tratt", "der", "Scor\u00b7pi\u00b7on", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil mein Verhengni\u00df wil unnd lest mir nicht das Gl\u00fccke", "tokens": ["Weil", "mein", "Ver\u00b7heng\u00b7ni\u00df", "wil", "unnd", "lest", "mir", "nicht", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "ADJD", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey dir, mein Augen-Trost, zu leben nur allein,", "tokens": ["Bey", "dir", ",", "mein", "Au\u00b7gen\u00b7Trost", ",", "zu", "le\u00b7ben", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So giebet zwar mein Sinn sich mit Gedult darein,", "tokens": ["So", "gie\u00b7bet", "zwar", "mein", "Sinn", "sich", "mit", "Ge\u00b7dult", "da\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PRF", "APPR", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch sehnt und w\u00fcndtschet er auch st\u00fcndlich sich zur\u00fccke.", "tokens": ["Doch", "sehnt", "und", "w\u00fcndt\u00b7schet", "er", "auch", "st\u00fcnd\u00b7lich", "sich", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist ja lauter nichts, wo diese sch\u00f6ne Blicke,", "tokens": ["Es", "ist", "ja", "lau\u00b7ter", "nichts", ",", "wo", "die\u00b7se", "sch\u00f6\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "PIS", "$,", "PWAV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df Liecht, das mich verblend, de\u00df g\u00fcldenen Haares Schein,", "tokens": ["Di\u00df", "Liecht", ",", "das", "mich", "ver\u00b7blend", ",", "de\u00df", "g\u00fcl\u00b7de\u00b7nen", "Haa\u00b7res", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das mein Gem\u00fcthe bindt, di\u00df Lachen nicht kan seyn,", "tokens": ["Das", "mein", "Ge\u00b7m\u00fc\u00b7the", "bindt", ",", "di\u00df", "La\u00b7chen", "nicht", "kan", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "NN", "PTKNEG", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mund und alles das, wormit ich mich erquicke.", "tokens": ["Der", "Mund", "und", "al\u00b7les", "das", ",", "wor\u00b7mit", "ich", "mich", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "PDS", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Sonne macht mir kalt, der Tag verfinstert mich,", "tokens": ["Die", "Son\u00b7ne", "macht", "mir", "kalt", ",", "der", "Tag", "ver\u00b7fins\u00b7tert", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich geh' und wei\u00df nicht wie, ich geh' und suche dich,", "tokens": ["Ich", "geh'", "und", "wei\u00df", "nicht", "wie", ",", "ich", "geh'", "und", "su\u00b7che", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKNEG", "KOKOM", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wohin du nie gedenckest. Was macht mein treues Lieben?", "tokens": ["Wo\u00b7hin", "du", "nie", "ge\u00b7den\u00b7ckest", ".", "Was", "macht", "mein", "treu\u00b7es", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "$.", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich seh' und finde nichts; der Mangel deiner Zier", "tokens": ["Ich", "seh'", "und", "fin\u00b7de", "nichts", ";", "der", "Man\u00b7gel", "dei\u00b7ner", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIS", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat alles weggeraubt; zwey Dinge sind noch hier:", "tokens": ["Hat", "al\u00b7les", "weg\u00b7ge\u00b7raubt", ";", "zwey", "Din\u00b7ge", "sind", "noch", "hier", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PI