{"textgrid.poem.50065": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "21. Heyraht des alten Chlorus", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Chlorus nam ein Weib,", "tokens": ["Der", "al\u00b7te", "Chlo\u00b7rus", "nam", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begabt mit einem fruchtbarn Leib,", "tokens": ["Be\u00b7gabt", "mit", "ei\u00b7nem", "frucht\u00b7barn", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ward kein Kind ans Licht gebracht,", "tokens": ["Doch", "ward", "kein", "Kind", "ans", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dass dem ", "tokens": ["Dass", "dem"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Das Ding kam ihm verd\u00e4chtig vor,", "tokens": ["Das", "Ding", "kam", "ihm", "ver\u00b7d\u00e4ch\u00b7tig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und er sagt's seinem Freund ins Ohr;", "tokens": ["Und", "er", "sagt's", "sei\u00b7nem", "Freund", "ins", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Freund, der leichtlich mercken kunt',", "tokens": ["Sein", "Freund", ",", "der", "leicht\u00b7lich", "mer\u00b7cken", "kunt'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dass es nicht recht mit Chlorus stund,", "tokens": ["Dass", "es", "nicht", "recht", "mit", "Chlo\u00b7rus", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Dass er schon ", "tokens": ["Dass", "er", "schon"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Sprang ihm mit diesem Trostwort bey:", "tokens": ["Sprang", "ihm", "mit", "die\u00b7sem", "Trost\u00b7wort", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Freund, sagt' er, habe guten Muht,", "tokens": ["Freund", ",", "sagt'", "er", ",", "ha\u00b7be", "gu\u00b7ten", "Muht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und dencke deine Sach' ist gut;", "tokens": ["Und", "den\u00b7cke", "dei\u00b7ne", "Sach'", "ist", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wo du auf Gleichheit stehst, so denk' auf diesen Streich:", "tokens": ["Wo", "du", "auf", "Gleich\u00b7heit", "stehst", ",", "so", "denk'", "auf", "die\u00b7sen", "Streich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Der alte Chlorus nam ein Weib,", "tokens": ["Der", "al\u00b7te", "Chlo\u00b7rus", "nam", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begabt mit einem fruchtbarn Leib,", "tokens": ["Be\u00b7gabt", "mit", "ei\u00b7nem", "frucht\u00b7barn", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ward kein Kind ans Licht gebracht,", "tokens": ["Doch", "ward", "kein", "Kind", "ans", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dass dem ", "tokens": ["Dass", "dem"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Das Ding kam ihm verd\u00e4chtig vor,", "tokens": ["Das", "Ding", "kam", "ihm", "ver\u00b7d\u00e4ch\u00b7tig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER"