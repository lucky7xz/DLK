{"textgrid.poem.39864": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Thau, unverg\u00e4nglicher als Lilienbl\u00fcte!", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thau, unverg\u00e4nglicher als Lilienbl\u00fcte!", "tokens": ["Thau", ",", "un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7cher", "als", "Li\u00b7li\u00b7en\u00b7bl\u00fc\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "An dir erkenn' ich die Geliebte wieder.", "tokens": ["An", "dir", "er\u00b7kenn'", "ich", "die", "Ge\u00b7lieb\u00b7te", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf dieses Kind senkt sich der Himmel nieder,", "tokens": ["Auf", "die\u00b7ses", "Kind", "senkt", "sich", "der", "Him\u00b7mel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den einst ich schaut' in einem Blick der G\u00fcte.", "tokens": ["Den", "einst", "ich", "schaut'", "in", "ei\u00b7nem", "Blick", "der", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht schmerzet Sch\u00f6nheit mich, die l\u00e4ngst vergl\u00fchte,", "tokens": ["Nicht", "schmer\u00b7zet", "Sch\u00f6n\u00b7heit", "mich", ",", "die", "l\u00e4ngst", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "NN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht der geschwundne Bau so holder Glieder;", "tokens": ["Nicht", "der", "ge\u00b7schwund\u00b7ne", "Bau", "so", "hol\u00b7der", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Herzen rauscht ein froher Quell der Lieder,", "tokens": ["Im", "Her\u00b7zen", "rauscht", "ein", "fro\u00b7her", "Quell", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geweihet nie verbl\u00fchendem Gem\u00fcte. \u2013", "tokens": ["Ge\u00b7wei\u00b7het", "nie", "ver\u00b7bl\u00fc\u00b7hen\u00b7dem", "Ge\u00b7m\u00fc\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ger\u00fchrter Thor! wirst du dich nicht besinnen?", "tokens": ["Ge\u00b7r\u00fchr\u00b7ter", "Thor", "!", "wirst", "du", "dich", "nicht", "be\u00b7sin\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Frauen Urbild soll sie dir vertreten,", "tokens": ["Der", "Frau\u00b7en", "Ur\u00b7bild", "soll", "