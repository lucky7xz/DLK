{"textgrid.poem.63869": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl, ich wei\u00df, ihr hohen Alten, was ich fr\u00fch schon euch verdankt,", "tokens": ["Wohl", ",", "ich", "wei\u00df", ",", "ihr", "ho\u00b7hen", "Al\u00b7ten", ",", "was", "ich", "fr\u00fch", "schon", "euch", "ver\u00b7dankt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PWS", "PPER", "ADJD", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da ich, rein mich zu entfalten, mich an euch emporgerankt.", "tokens": ["Da", "ich", ",", "rein", "mich", "zu", "ent\u00b7fal\u00b7ten", ",", "mich", "an", "euch", "em\u00b7por\u00b7ge\u00b7rankt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$,", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn ihr lehrtet schon den Knaben, der Natur getreu zu sein", "tokens": ["Denn", "ihr", "lehr\u00b7tet", "schon", "den", "Kna\u00b7ben", ",", "der", "Na\u00b7tur", "ge\u00b7treu", "zu", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "ADJD", "PTKZU", "VAINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und den sittlichen Gewalten, deren Urbild nie geschwankt,", "tokens": ["Und", "den", "sitt\u00b7li\u00b7chen", "Ge\u00b7wal\u00b7ten", ",", "de\u00b7ren", "Ur\u00b7bild", "nie", "ge\u00b7schwankt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Sch\u00fctztet den Herangereiften vor des Fiebers gift'gem Hauch,", "tokens": ["Sch\u00fctz\u00b7tet", "den", "Her\u00b7an\u00b7ge\u00b7reif\u00b7ten", "vor", "des", "Fie\u00b7bers", "gift'\u00b7gem", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--++-+-+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "So des hitz'gen wie des kalten, dran die Kunst der Zeit gekrankt.", "tokens": ["So", "des", "hitz'\u00b7gen", "wie", "des", "kal\u00b7ten", ",", "dran", "die", "Kunst", "der", "Zeit", "ge\u00b7krankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KOKOM", "ART", "ADJA", "$,", "PAV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Doch der Lehre dann entwachsen, lie\u00df ich meines Genius", "tokens": ["Doch", "der", "Leh\u00b7re", "dann", "ent\u00b7wach\u00b7sen", ",", "lie\u00df", "ich", "mei\u00b7nes", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVINF", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.8": {"text": "Stimme nur im Busen walten, deren Treue nie gewankt,", "tokens": ["Stim\u00b7me", "nur", "im", "Bu\u00b7sen", "wal\u00b7ten", ",", "de\u00b7ren", "Treu\u00b7e", "nie", "ge\u00b7wankt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVINF", "$,", "PRELAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wandert' einsam meines Weges, wenn die Menge z\u00fcgellos,", "tokens": ["Wan\u00b7dert'", "ein\u00b7sam", "mei\u00b7nes", "We\u00b7ges", ",", "wenn", "die", "Men\u00b7ge", "z\u00fc\u00b7gel\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Nie beflissen, Ma\u00df zu halten, andres Werk von mir verlangt.", "tokens": ["Nie", "be\u00b7flis\u00b7sen", ",", "Ma\u00df", "zu", "hal\u00b7ten", ",", "and\u00b7res", "Werk", "von", "mir", "ver\u00b7langt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Und so tr\u00f6st' ich mich, ihr werdet \u00fcberdauern diese Zeit,", "tokens": ["Und", "so", "tr\u00f6st'", "ich", "mich", ",", "ihr", "wer\u00b7det", "\u00fc\u00b7berd\u00b7au\u00b7ern", "die\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Meiner Bildkraft Traumgestalten, die ihr nicht der Zeit entsprangt.", "tokens": ["Mei\u00b7ner", "Bild\u00b7kraft", "Traum\u00b7ge\u00b7stal\u00b7ten", ",", "die", "ihr", "nicht", "der", "Zeit", "ent\u00b7sprangt", "."], "t