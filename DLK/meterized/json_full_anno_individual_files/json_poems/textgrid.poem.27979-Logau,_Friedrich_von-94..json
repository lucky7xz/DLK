{"textgrid.poem.27979": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Damen, die sich gerne schm\u00fcncken,", "tokens": ["Die", "Da\u00b7men", ",", "die", "sich", "ger\u00b7ne", "schm\u00fcn\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die lassen sich wol selbst bed\u00fcncken,", "tokens": ["Die", "las\u00b7sen", "sich", "wol", "selbst", "be\u00b7d\u00fcn\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df wo Natur an ihren Gaben", "tokens": ["Da\u00df", "wo", "Na\u00b7tur", "an", "ih\u00b7ren", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df etwas \u00fcbersehen haben;", "tokens": ["Mu\u00df", "et\u00b7was", "\u00fc\u00b7ber\u00b7se\u00b7hen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum wo man Schmuck und Schm\u00fcncke schauet,", "tokens": ["Drum", "wo", "man", "Schmuck", "und", "Schm\u00fcn\u00b7cke", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PIS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Thut th\u00f