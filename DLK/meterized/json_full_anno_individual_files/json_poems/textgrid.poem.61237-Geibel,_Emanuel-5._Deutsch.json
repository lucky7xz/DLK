{"textgrid.poem.61237": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5. Deutsch", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich an dich gedenke", "tokens": ["Wenn", "ich", "an", "dich", "ge\u00b7den\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei stiller Nacht allein,", "tokens": ["Bei", "stil\u00b7ler", "Nacht", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das geht mir durch die Seele", "tokens": ["Das", "geht", "mir", "durch", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie lichter Mondenschein;", "tokens": ["Wie", "lich\u00b7ter", "Mon\u00b7den\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das geht mir durch die Seele", "tokens": ["Das", "geht", "mir", "durch", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie lieblich Harfenspiel,", "tokens": ["Wie", "lieb\u00b7lich", "Har\u00b7fen\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir ist, ich hatte nimmer", "tokens": ["Mir", "ist", ",", "ich", "hat\u00b7te", "nim\u00b7mer"], "token_info": ["wo