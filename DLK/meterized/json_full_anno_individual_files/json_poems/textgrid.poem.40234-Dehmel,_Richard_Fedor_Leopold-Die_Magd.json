{"textgrid.poem.40234": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Die Magd", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maiblumen bl\u00fchten \u00fcberall,", "tokens": ["Maib\u00b7lu\u00b7men", "bl\u00fch\u00b7ten", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er sah mich an so tr\u00fcb und m\u00fcd, \u2013", "tokens": ["er", "sah", "mich", "an", "so", "tr\u00fcb", "und", "m\u00fcd", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Faulbaum rief die Nachtigall:", "tokens": ["im", "Faul\u00b7baum", "rief", "die", "Nach\u00b7ti\u00b7gall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die Bl\u00fcte ", "tokens": ["die", "Bl\u00fc\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Von D\u00fcften war die Nacht so warm,", "tokens": ["Von", "D\u00fcf\u00b7ten", "war", "die", "Nacht", "so", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie Blut so warm, wie unser Blut,", "tokens": ["wie", "Blut", "so", "warm", ",", "wie", "un\u00b7ser", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und wir so jung und freudenarm, \u2013", "tokens": ["und", "wir", "so", "jung", "und", "freu\u00b7den\u00b7arm", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "das zuckende Lied: Die Glut ", "tokens": ["das", "zu\u00b7cken\u00b7de", "Lied", ":", "Die", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "und Er so treu und mir so gut ...", "tokens": ["und", "Er", "so", "treu", "und", "mir", "so", "gut", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "KON", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In Knospen scho\u00df der wilde Mohn,", "tokens": ["In", "Knos\u00b7pen", "scho\u00df", "der", "wil\u00b7de", "Mohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "es sog die Sonne unsern Schwei\u00df;", "tokens": ["es", "sog", "die", "Son\u00b7ne", "un\u00b7sern", "Schwei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "es wurden rot die Knospen schon,", "tokens": ["es", "wur\u00b7den", "rot", "die", "Knos\u00b7pen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "da wurden meine Wangen wei\u00df.", "tokens": ["da", "wur\u00b7den", "mei\u00b7ne", "Wan\u00b7gen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ums liebe Brot, ums teure Brot", "tokens": ["Ums", "lie\u00b7be", "Brot", ",", "ums", "teu\u00b7re", "Brot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "flo\u00df ", "tokens": ["flo\u00df"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "der wilde Mohn stand feuerrot, \u2013", "tokens": ["der", "wil\u00b7de", "Mohn", "stand", "feu\u00b7er\u00b7rot", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "es war wol fressendes ", "tokens": ["es", "war", "wol", "fres\u00b7sen\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "es ward auch ", "tokens": ["es", "ward", "auch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "und die Sonne stach im Korn ihn tot ...", "tokens": ["und", "die", "Son\u00b7ne", "stach", "im", "Korn", "ihn", "tot", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Die Astern schwankten bleich am Zaun,", "tokens": ["Die", "As\u00b7tern", "schwank\u00b7ten", "bleich", "am", "Zaun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "im feuchten Wind die Traube schwoll;", "tokens": ["im", "feuch\u00b7ten", "Wind", "die", "Trau\u00b7be", "schwoll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Hofe zischelten die Frau'n,", "tokens": ["im", "Ho\u00b7fe", "zi\u00b7schel\u00b7ten", "die", "Frau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "der Apfelbaum hing schwer und voll.", "tokens": ["der", "Ap\u00b7fel\u00b7baum", "hing", "schwer", "und", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es war ein Tag so regensatt,", "tokens": ["Es", "war", "ein", "Tag", "so", "re\u00b7gen\u00b7satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wie einst sein Blick so bla\u00df und matt;", "tokens": ["wie", "einst", "sein", "Blick", "so", "bla\u00df", "und", "matt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die Astern standen braun und na\u00df,", "tokens": ["die", "As\u00b7tern", "stan\u00b7den", "braun", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "vom gelben Blatt der Nebel troff;", "tokens": ["vom", "gel\u00b7ben", "Blatt", "der", "Ne\u00b7bel", "troff", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "da stie\u00df man sie voll Hohn und Ha\u00df,", "tokens": ["da", "stie\u00df", "man", "sie", "voll", "Hohn", "und", "Ha\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "die s\u00fcndige Magd, hinaus vom Hof ...", "tokens": ["die", "s\u00fcn\u00b7di\u00b7ge", "Magd", ",", "hin\u00b7aus", "vom", "Hof", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun bl\u00fcht von ", "tokens": ["Nun", "bl\u00fcht", "von"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "die Thr\u00e4ne friert im schneidenden Wind;", "tokens": ["die", "Thr\u00e4\u00b7ne", "friert", "im", "schnei\u00b7den\u00b7den", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "aus flimmernden Scheiben gl\u00fcht der Schein", "tokens": ["aus", "flim\u00b7mern\u00b7den", "Schei\u00b7ben", "gl\u00fcht", "der", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "des Christbaums auf mein wimmernd Kind.", "tokens": ["des", "Christ\u00b7baums", "auf", "mein", "wim\u00b7mernd", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die hungernden Spatzen bettelnd schrein,", "tokens": ["Die", "hun\u00b7gern\u00b7den", "Spat\u00b7zen", "bet\u00b7telnd", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "vom blanken Dach die Kr\u00e4he kr\u00e4chzt;", "tokens": ["vom", "blan\u00b7ken", "Dach", "die", "Kr\u00e4\u00b7he", "kr\u00e4chzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "am schlaffen Busen zitternd \u00e4chzt", "tokens": ["am", "schlaf\u00b7fen", "Bu\u00b7sen", "zit\u00b7ternd", "\u00e4chzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "mein Kind, und Keiner l\u00e4\u00dft uns ein;", "tokens": ["mein", "Kind", ",", "und", "Kei\u00b7ner", "l\u00e4\u00dft", "uns", "ein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "wie die Worte der Reichen so scharf und weh", "tokens": ["wie", "die", "Wor\u00b7te", "der", "Rei\u00b7chen", "so", "scharf", "und", "weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "KON", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "knirscht unter mir der harte Schnee.", "tokens": ["knirscht", "un\u00b7ter", "mir", "der", "har\u00b7te", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So weh \u2013 oh, bohrt es mir im Ohr:", "tokens": ["So", "weh", "\u2013", "oh", ",", "bohrt", "es", "mir", "im", "Ohr", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$(", "FM", "$,", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du Kind der Schmach! du S\u00fcndenlohn!", "tokens": ["du", "Kind", "der", "Schmach", "!", "du", "S\u00fcn\u00b7den\u00b7lohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dennoch beten sie empor", "tokens": ["Und", "den\u00b7noch", "be\u00b7ten", "sie", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum Sohn der Magd, dem Jungfraunsohn?! \u2013", "tokens": ["zum", "Sohn", "der", "Magd", ",", "dem", "Jung\u00b7fraun\u00b7sohn", "?!", "\u2013"], "token_info": ["word", "word", "word", 