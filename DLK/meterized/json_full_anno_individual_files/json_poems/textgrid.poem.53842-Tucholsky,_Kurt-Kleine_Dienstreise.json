{"textgrid.poem.53842": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kleine Dienstreise", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Frankfurt haben sie eine Brucken geschlagen;", "tokens": ["In", "Frank\u00b7furt", "ha\u00b7ben", "sie", "ei\u00b7ne", "Bru\u00b7cken", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00fcber den Main her\u00fcber tut sie nunmehr ragen.", "tokens": ["\u00fc\u00b7ber", "den", "Main", "her\u00b7\u00fc\u00b7ber", "tut", "sie", "nun\u00b7mehr", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und um sie einzuweihen, haben die Frankfurter eine gro\u00dfe Feier", "tokens": ["Und", "um", "sie", "ein\u00b7zu\u00b7wei\u00b7hen", ",", "ha\u00b7ben", "die", "Frank\u00b7fur\u00b7ter", "ei\u00b7ne", "gro\u00b7\u00dfe", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVIZU", "$,", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "arrangiert \u2013 mit B\u00f6llergel\u00e4ute und Festzug und \u00c4ppelwoi und gedichteter Sangesleier.", "tokens": ["ar\u00b7ran\u00b7giert", "\u2013", "mit", "B\u00f6l\u00b7ler\u00b7ge\u00b7l\u00e4u\u00b7te", "und", "Fest\u00b7zug", "und", "\u00c4p\u00b7pel\u00b7woi", "und", "ge\u00b7dich\u00b7te\u00b7ter", "San\u00b7ges\u00b7lei\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "NN", "KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+--+-+--+--+-+-", "measure": "amphibrach.penta.plus"}, "line.5": {"text": "Gut.", "tokens": ["Gut", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Dazu haben sie auch den diensttuenden Reichspr\u00e4sidenten eingeladen.", "tokens": ["Da\u00b7zu", "ha\u00b7ben", "sie", "auch", "den", "dienst\u00b7tu\u00b7en\u00b7den", "Reich\u00b7spr\u00e4\u00b7si\u00b7den\u00b7ten", "ein\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bei dem steht aber die Stadt Frankfurt nicht recht in Gnaden,", "tokens": ["Bei", "dem", "steht", "a\u00b7ber", "die", "Stadt", "Frank\u00b7furt", "nicht", "recht", "in", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ADV", "ART", "NN", "NE", "PTKNEG", "ADJD", "APPR", "NN", "$,"], "meter": "--+---+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "und so ist er auch zur Mainbr\u00fccken-Feier nicht gekommen.", "tokens": ["und", "so", "ist", "er", "auch", "zur", "Main\u00b7br\u00fc\u00b7cken\u00b7Fei\u00b7er", "nicht", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.9": {"text": "Denn er hat, glaube ich, den Frankfurtern \u00fcbel genommen,", "tokens": ["Denn", "er", "hat", ",", "glau\u00b7be", "ich", ",", "den", "Frank\u00b7fur\u00b7tern", "\u00fc\u00b7bel", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "da\u00df sie \u2013 pfui Deubel! \u2013 Demokraten sein.", "tokens": ["da\u00df", "sie", "\u2013", "pfui", "Deu\u00b7bel", "!", "\u2013", "De\u00b7mo\u00b7kra\u00b7ten", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "NE", "$.", "$(", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und darauf sagte er: \u00bbNein!\u00ab", "tokens": ["Und", "da\u00b7rauf", "sag\u00b7te", "er", ":", "\u00bb", "Nein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Gut.", "tokens": ["Gut", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Die Frankfurter haben sich den Schmerz von der Nase gewischt", "tokens": ["Die", "Frank\u00b7fur\u00b7ter", "ha\u00b7ben", "sich", "den", "Schmerz", "von", "der", "Na\u00b7se", "ge\u00b7wischt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "und machten sich aus der pr\u00e4sidentlichen Abwesenheit weiter nischt.", "tokens": ["und", "mach\u00b7ten", "sich", "aus", "der", "pr\u00e4\u00b7si\u00b7dent\u00b7li\u00b7chen", "Ab\u00b7we\u00b7sen\u00b7heit", "wei\u00b7ter", "nischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "ADV", "PTKNEG", "$."], "meter": "-+--+-+-+--++--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.15": {"text": "der fast", "tokens": ["der", "fast"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "in eine republikanische Demokratenstadt hineinzufallen in die schmerzliche Lage gekommen w\u00e4re?", "tokens": ["in", "ei\u00b7ne", "re\u00b7pub\u00b7li\u00b7ka\u00b7ni\u00b7sche", "De\u00b7mo\u00b7kra\u00b7ten\u00b7stadt", "hin\u00b7ein\u00b7zu\u00b7fal\u00b7len", "in", "die", "schmerz\u00b7li\u00b7che", "La\u00b7ge", "ge\u00b7kom\u00b7men", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+----+-+-+-+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.17": {"text": "In Berlin? Nein. In K\u00f6ln? Nein. In K\u00f6nigsberg? Nein. Vielmehre,", "tokens": ["In", "Ber\u00b7lin", "?", "Nein", ".", "In", "K\u00f6ln", "?", "Nein", ".", "In", "K\u00f6\u00b7nigs\u00b7berg", "?", "Nein", ".", "Viel\u00b7meh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$.", "PTKANT", "$.", "APPR", "NE", "$.", "PTKANT", "$.", "APPR", "NE", "$.", "PTKANT", "$.", "NN", "$,"], "meter": "-+-+-++-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "weil man ihn dorthin zu einer Familienfeier lud:", "tokens": ["weil", "man", "ihn", "dor\u00b7thin", "zu", "ei\u00b7ner", "Fa\u00b7mi\u00b7li\u00b7en\u00b7fei\u00b7er", "lud", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "In Stolp. (Hinterpommern.)", "tokens": ["In", "Stolp", ".", "(", "Hin\u00b7ter\u00b7pom\u00b7mern", ".", ")"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPR", "NE", "$.", "$(", "VVIZU", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Gut.", "tokens": ["Gut", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Ja da \u2013!", "tokens": ["Ja", "da", "\u2013", "!"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PTKANT", "ADV", "$(", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Da gehts noch zu wie in einem altdeutschen Napfkuchen.", "tokens": ["Da", "gehts", "noch", "zu", "wie", "in", "ei\u00b7nem", "alt\u00b7deut\u00b7schen", "Napf\u00b7ku\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Da kann man sich die Republikaner mit der Reichswehrlaterne suchen.", "tokens": ["Da", "kann", "man", "sich", "die", "Re\u00b7pub\u00b7li\u00b7ka\u00b7ner", "mit", "der", "Reichs\u00b7wehr\u00b7la\u00b7ter\u00b7ne", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.24": {"text": "Da kommen noch abends die Honoratioren zusammen,", "tokens": ["Da", "kom\u00b7men", "noch", "a\u00b7bends", "die", "Ho\u00b7no\u00b7ra\u00b7ti\u00b7o\u00b7ren", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "sitzen breit\u00e4hrig da und tun die verfluchte Judenrepublik verdammen.", "tokens": ["sit\u00b7zen", "brei\u00b7t\u00e4h\u00b7rig", "da", "und", "tun", "die", "ver\u00b7fluch\u00b7te", "Ju\u00b7den\u00b7re\u00b7pub\u00b7lik", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.26": {"text": "Da ist noch deutsche Ordnung, Zucht, Sitte und Gottesfurcht in sch\u00f6nem Quartett", "tokens": ["Da", "ist", "noch", "deut\u00b7sche", "Ord\u00b7nung", ",", "Zucht", ",", "Sit\u00b7te", "und", "Got\u00b7tes\u00b7furcht", "in", "sch\u00f6\u00b7nem", "Quar\u00b7tett"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.27": {"text": "und kein Wasserklosett.", "tokens": ["und", "kein", "Was\u00b7ser\u00b7klo\u00b7sett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Dortselbst weihte unser Oepperster ein Stadion ein.", "tokens": ["Dort\u00b7selbst", "weih\u00b7te", "un\u00b7ser", "O\u00b7ep\u00b7per\u00b7ster", "ein", "Sta\u00b7di\u00b7on", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Stolp (Hinterpommern) scheint wichtiger als eine Br\u00fccke \u00fcber den Main,", "tokens": ["Stolp", "(", "Hin\u00b7ter\u00b7pom\u00b7mern", ")", "scheint", "wich\u00b7ti\u00b7ger", "als", "ei\u00b7ne", "Br\u00fc\u00b7cke", "\u00fc\u00b7ber", "den", "Main", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "VVFIN", "ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.30": {"text": "die Nord und S\u00fcd verbindet, eine sch\u00f6ne Allegorie aus Granit.", "tokens": ["die", "Nord", "und", "S\u00fcd", "ver\u00b7bin\u00b7det", ",", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Al\u00b7le\u00b7go\u00b7rie", "aus", "Gra\u00b7nit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.31": {"text": "Jeder, wohin es ihn zieht.", "tokens": ["Je\u00b7der", ",", "wo\u00b7hin", "es", "ihn", "zieht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "In Frankfurt haben sie eine Brucken geschlagen;", "tokens": ["In", "Frank\u00b7furt", "ha\u00b7ben", "sie", "ei\u00b7ne", "Bru\u00b7cken", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00fcber den Main her\u00fcber