{"textgrid.poem.53498": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Einsiedlerschule", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In das Gra\u00df / so lang ich bin", "tokens": ["In", "das", "Gra\u00df", "/", "so", "lang", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "ADV", "ADJD", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einsamb str\u00e4kke ich mich hin.", "tokens": ["Ein\u00b7samb", "str\u00e4k\u00b7ke", "ich", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdie Welt, die ist ein Jammertal\u00ab", "tokens": ["\u00bb", "die", "Welt", ",", "die", "ist", "ein", "Jam\u00b7mer\u00b7tal", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "\u00bbeinsiedelmann ist nicht zu Haus,", "tokens": ["\u00bb", "ein\u00b7sie\u00b7del\u00b7mann", "ist", "nicht", "zu", "Haus", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil es Zeit zu m\u00e4hen.", "tokens": ["Die\u00b7weil", "es", "Zeit", "zu", "m\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah ihn bei der Halde drau\u00df,", "tokens": ["Ich", "sah", "ihn", "bei", "der", "Hal\u00b7de", "drau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei einer Schnittrin \u2013\u00ab", "tokens": ["Bei", "ei\u00b7ner", "Schnit\u00b7trin", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bb14-, 15-, 16-, 17-, 18- Siedelmann,", "tokens": ["\u00bb", "14", ",", "15", ",", "16", ",", "17", ",", "18", "Sie\u00b7del\u00b7mann", ","], "token_info": ["punct", "amount", "punct", "amount", "punct", "amount", "punct", "amount", "punct", "amount", "word", "punct"], "pos": ["$(", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "TRUNC", "$,", "CARD", "NN", "$,"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "19- Siedelmann, 20 Seidel!\u00ab", "tokens": ["Sie\u00b7del\u00b7mann", ",", "20", "Sei\u00b7del", "!", "\u00ab"], "token_info": ["word", "punct", "number", "word", "punct", "punct"], "pos": ["NE", "$,", "CARD", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann verhallten sie, und wir fuhren wieder durch den gr\u00fcnen Wald, nach Haus, in die b\u00f6se, b\u00f6se Welt.", "tokens": ["Dann", "ver\u00b7hall\u00b7ten", "sie", ",", "und", "wir", "fuh\u00b7ren", "wie\u00b7der", "durch", "den", "gr\u00fc\u00b7nen", "Wald", ",", "nach", "Haus", ",", "in", "die", "b\u00f6\u00b7se", ",", "b\u00f6\u00b7se", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word",