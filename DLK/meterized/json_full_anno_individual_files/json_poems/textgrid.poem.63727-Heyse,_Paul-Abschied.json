, "un\u00b7st\u00e4t", "hast'\u00b7ger", "Fu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Thermen, Tempel und Museen getragen,", "tokens": ["Durch", "Ther\u00b7men", ",", "Tem\u00b7pel", "und", "Mu\u00b7se\u00b7en", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er wird, was mir verg\u00f6nnt ward, nie erjagen,", "tokens": ["Er", "wird", ",", "was", "mir", "ver\u00b7g\u00f6nnt", "ward", ",", "nie", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie ganz verstehn Italiens Genius.", "tokens": ["Nie", "ganz", "ver\u00b7stehn", "I\u00b7ta\u00b7li\u00b7ens", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich aber durft' in dieses Volkes Mitte", "tokens": ["Ich", "a\u00b7ber", "durft'", "in", "die\u00b7ses", "Vol\u00b7kes", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Belauschen seines Herzens freien Schlag,", "tokens": ["Be\u00b7lau\u00b7schen", "sei\u00b7nes", "Her\u00b7zens", "frei\u00b7en", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", 