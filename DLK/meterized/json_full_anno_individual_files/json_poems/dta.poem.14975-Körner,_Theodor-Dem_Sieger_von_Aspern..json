{"dta.poem.14975": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Dem Sieger von Aspern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Was der verwegenen Hand gebot in die Saiten zu\nschlagen,", "tokens": ["Was", "der", "ver\u00b7we\u00b7ge\u00b7nen", "Hand", "ge\u00b7bot", "in", "die", "Sai\u00b7ten", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was mein jugendlich Herz tief in Entz\u00fcckung getaucht,", "tokens": ["Was", "mein", "ju\u00b7gend\u00b7lich", "Herz", "tief", "in", "Ent\u00b7z\u00fc\u00b7ckung", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJD", "NN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dieser Begeisterung Sturm, er schlummert nirgends, es", "tokens": ["Die\u00b7ser", "Be\u00b7geis\u00b7te\u00b7rung", "Sturm", ",", "er", "schlum\u00b7mert", "nir\u00b7gends", ",", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "NN", "$,", "PPER", "VVFIN", "ADV", "$,", "PPER"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nie der Brust das Gef\u00fchl, nur dem Gef\u00fchle das Wort.", "tokens": ["Nie", "der", "Brust", "das", "Ge\u00b7f\u00fchl", ",", "nur", "dem", "Ge\u00b7f\u00fch\u00b7le", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Manche schweigen wohl auch, weil die Zeit das Schweigen", "tokens": ["Man\u00b7che", "schwei\u00b7gen", "wohl", "auch", ",", "weil", "die", "Zeit", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Weil der dr\u00e4ngende Tag scheuche den gl\u00fccklichen Muth.", "tokens": ["Weil", "der", "dr\u00e4n\u00b7gen\u00b7de", "Tag", "scheu\u00b7che", "den", "gl\u00fcck\u00b7li\u00b7chen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Aber die Zeit will ich sehn, und den Tag, der gebieten kann,", "tokens": ["A\u00b7ber", "die", "Zeit", "will", "ich", "sehn", ",", "und", "den", "Tag", ",", "der", "ge\u00b7bie\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "KON", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.8": {"text": "Kalt und besonnen zu seyn, wenn mich Entz\u00fcckung durch-", "tokens": ["Kalt", "und", "be\u00b7son\u00b7nen", "zu", "seyn", ",", "wenn", "mich", "Ent\u00b7z\u00fc\u00b7ckung", "durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVINF", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "NN", "TRUNC"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Wenn mein germanischer Stolz sich beugt dem germanischen", "tokens": ["Wenn", "mein", "ger\u00b7ma\u00b7ni\u00b7scher", "Stolz", "sich", "beugt", "dem", "ger\u00b7ma\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der auf dem Altar des Siegs, Funken und Flammen", "tokens": ["Der", "auf", "dem", "Al\u00b7tar", "des", "Siegs", ",", "Fun\u00b7ken", "und", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Darum ri\u00df es mich fort, ich griff in die rauschenden Saiten,", "tokens": ["Da\u00b7rum", "ri\u00df", "es", "mich", "fort", ",", "ich", "griff", "in", "die", "rau\u00b7schen\u00b7den", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Sang es laut, was sich sonst wortlos im Herzen vergrub.", "tokens": ["Sang", "es", "laut", ",", "was", "sich", "sonst", "wort\u00b7los", "im", "Her\u00b7zen", "ver\u00b7grub", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PRELS", "PRF", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Aber der Held verzeihe der armen Kunst seines Barden,", "tokens": ["A\u00b7ber", "der", "Held", "ver\u00b7zei\u00b7he", "der", "ar\u00b7men", "Kunst", "sei\u00b7nes", "Bar\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Die mit frevelndem Muth sich an das H\u00f6chste gewagt.", "tokens": ["Die", "mit", "fre\u00b7veln\u00b7dem", "Muth", "sich", "an", "das", "H\u00f6chs\u00b7te", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Z\u00fcrnt doch der Sturm, der den Donner der brechenden Eiche", "tokens": ["Z\u00fcrnt", "doch", "der", "Sturm", ",", "der", "den", "Don\u00b7ner", "der", "bre\u00b7chen\u00b7den", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.16": {"text": "Drum dem Schilfe nicht, das ihm entgegengerauscht.", "tokens": ["Drum", "dem", "Schil\u00b7fe", "nicht", ",", "das", "ihm", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}