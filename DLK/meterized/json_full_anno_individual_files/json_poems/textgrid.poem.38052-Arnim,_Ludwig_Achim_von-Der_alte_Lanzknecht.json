{"textgrid.poem.38052": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Lanzknecht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl auf ihr Lanzknecht alle,", "tokens": ["Wohl", "auf", "ihr", "Lanz\u00b7knecht", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seyd fr\u00f6hlich, seyd guter Ding,", "tokens": ["Seyd", "fr\u00f6h\u00b7lich", ",", "seyd", "gu\u00b7ter", "Ding", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen Gott den Herren", "tokens": ["Wir", "wol\u00b7len", "Gott", "den", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu den edlen K\u00f6ning,", "tokens": ["Da\u00b7zu", "den", "ed\u00b7len", "K\u00f6\u00b7ning", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er legt uns ein gewaltigen Haufen ins Feld,", "tokens": ["Er", "legt", "uns", "ein", "ge\u00b7wal\u00b7ti\u00b7gen", "Hau\u00b7fen", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Es soll kein Lanzknecht trauren um Geld,", "tokens": ["Es", "soll", "kein", "Lanz\u00b7knecht", "trau\u00b7ren", "um", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Er will uns ehrlich lohnen", "tokens": ["Er", "will", "uns", "ehr\u00b7lich", "loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit St\u00fcwern und Sonnenkronen.", "tokens": ["Mit", "St\u00fc\u00b7wern", "und", "Son\u00b7nen\u00b7kro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Herzog aus Burgunde,", "tokens": ["Der", "Her\u00b7zog", "aus", "Bur\u00b7gun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Derselbig treulose Mann,", "tokens": ["Der\u00b7sel\u00b7big", "treu\u00b7lo\u00b7se", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wollt uns den edlen Franzosen", "tokens": ["Wollt", "uns", "den", "ed\u00b7len", "Fran\u00b7zo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sch\u00e4ndlich verrathen han,", "tokens": ["Sch\u00e4nd\u00b7lich", "ver\u00b7ra\u00b7then", "han", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das schaffet Gott durch seine G\u00fct,", "tokens": ["Das", "schaf\u00b7fet", "Gott", "durch", "sei\u00b7ne", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott woll uns den edlen K\u00f6nig beh\u00fct,", "tokens": ["Gott", "woll", "uns", "den", "ed\u00b7len", "K\u00f6\u00b7nig", "be\u00b7h\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Er ist ein edler Herre,", "tokens": ["Er", "ist", "ein", "ed\u00b7ler", "Her\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wir dienen ihm allzeit gerne.", "tokens": ["Wir", "die\u00b7nen", "ihm", "all\u00b7zeit", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Beym Bauren mu\u00df ich dreschen", "tokens": ["Beym", "Bau\u00b7ren", "mu\u00df", "ich", "dre\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und essen saure Milch,", "tokens": ["Und", "es\u00b7sen", "sau\u00b7re", "Milch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beym K\u00f6nig trag ich volle Fleschen,", "tokens": ["Beym", "K\u00f6\u00b7nig", "trag", "ich", "vol\u00b7le", "Fle\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beym Bauren ein groben Zwilch,", "tokens": ["Beym", "Bau\u00b7ren", "ein", "gro\u00b7ben", "Zwilch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Beym K\u00f6nig tret ich ganz tapfer ins Feld,", "tokens": ["Beym", "K\u00f6\u00b7nig", "tret", "ich", "ganz", "tap\u00b7fer", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zieh daher als ein freyer Held,", "tokens": ["Zieh", "da\u00b7her", "als", "ein", "frey\u00b7er", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Zerhauen und zerschnitten,", "tokens": ["Zer\u00b7hau\u00b7en", "und", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nach adelichen Sitten.", "tokens": ["Nach", "a\u00b7de\u00b7li\u00b7chen", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Es soll kein Lanzknecht garten", "tokens": ["Es", "soll", "kein", "Lanz\u00b7knecht", "gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor eines Bauren Haus,", "tokens": ["Vor", "ei\u00b7nes", "Bau\u00b7ren", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn er mu\u00df rotten und harken,", "tokens": ["Denn", "er", "mu\u00df", "rot\u00b7ten", "und", "har\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ihm der Schwei\u00df bricht aus,", "tokens": ["Da\u00df", "ihm", "der", "Schwei\u00df", "bricht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu das Mark in seim Gebein;", "tokens": ["Da\u00b7zu", "das", "Mark", "in", "seim", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel lieber diene ich dem K\u00f6nig allein,", "tokens": ["Viel", "lie\u00b7ber", "die\u00b7ne", "ich", "dem", "K\u00f6\u00b7nig", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Denn einem reichen Bauren,", "tokens": ["Denn", "ei\u00b7nem", "rei\u00b7chen", "Bau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er giebt uns das Geld mit Trauren.", "tokens": ["Er", "giebt", "uns", "das", "Geld", "mit", "Trau\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Der uns dies neue Liedlein sang", "tokens": ["Der", "uns", "dies", "neu\u00b7e", "Lied\u00b7lein", "sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PDS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von neuem gesungen hat,", "tokens": ["Von", "neu\u00b7em", "ge\u00b7sun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das hat gethan ein Lanzknecht gut,", "tokens": ["Das", "hat", "ge\u00b7than", "ein", "Lanz\u00b7knecht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gelegen vor mancher Stadt,", "tokens": ["Ist", "ge\u00b7le\u00b7gen", "vor", "man\u00b7cher", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "In mancher Feldschlacht ist er gewesen,", "tokens": ["In", "man\u00b7cher", "Feld\u00b7schlacht", "ist", "er", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In vielen St\u00fcrmen hat er genesen,", "tokens": ["In", "vie\u00b7len", "St\u00fcr\u00b7men", "hat", "er", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Dem edlen K\u00f6nig zu Ehren,", "tokens": ["Dem", "ed\u00b7len", "K\u00f6\u00b7nig", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sein Lob ist weit und ferne.", "tokens": ["Sein", "Lob", "ist", "weit", "und", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl auf ihr Lanzknecht alle,", "tokens": ["Wohl", "auf", "ihr", "Lanz\u00b7knecht", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seyd fr\u00f6hlich, seyd guter Ding,", "tokens": ["Seyd", "fr\u00f6h\u00b7lich", ",", "seyd", "gu\u00b7ter", "Ding", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen Gott den Herren", "tokens": ["Wir", "wol\u00b7len", "Gott", "den", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu den edlen K\u00f6ning,", "tokens": ["Da\u00b7zu", "den", "ed\u00b7len", "K\u00f6\u00b7ning", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er legt uns ein gewaltigen Haufen ins Feld,", "tokens": ["Er", "legt", "uns", "ein", "ge\u00b7wal\u00b7ti\u00b7gen", "Hau\u00b7fen", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "po