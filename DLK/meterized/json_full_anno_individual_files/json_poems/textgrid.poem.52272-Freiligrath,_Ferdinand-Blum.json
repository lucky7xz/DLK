"}, "line.3": {"text": "Ein Kind mit breiter, offner Stirn, ein Kind von heller Lunge,", "tokens": ["Ein", "Kind", "mit", "brei\u00b7ter", ",", "off\u00b7ner", "Stirn", ",", "ein", "Kind", "von", "hel\u00b7ler", "Lun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ein pr\u00e4chtig Proletarierkind, ein derber K\u00fcferjunge.", "tokens": ["Ein", "pr\u00e4ch\u00b7tig", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7kind", ",", "ein", "der\u00b7ber", "K\u00fc\u00b7fer\u00b7jun\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Er schrie, da\u00df in der Werkstatt rings des Vaters Tonnen hallten;", "tokens": ["Er", "schrie", ",", "da\u00df", "in", "der", "Werk\u00b7statt", "rings", "des", "Va\u00b7ters", "Ton\u00b7nen", "hall\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Die Mutter hat mit L\u00e4cheln ihn an ihre Brust gehalten;", "tokens": ["Die", "Mut\u00b7ter", "hat", "mit", "L\u00e4\u00b7cheln", "ihn", "an", "ih\u00b7re", "Brust", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "An ihrer Brust, auf ihrem Arm hat sie ihn eingesungen: \u2013", "tokens": ["An", "ih\u00b7rer", "Brust", ",", "auf", "ih\u00b7rem", "Arm", "hat", "sie", "ihn", "ein\u00b7ge\u00b7sun\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Es ist zu K\u00f6ln das Wiegenlied des Knaben hell erklungen.", "tokens": ["Es", "ist", "zu", "K\u00f6ln", "das", "Wie\u00b7gen\u00b7lied", "des", "Kna\u00b7ben", "hell", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Und heut in diesem selben K\u00f6ln zum Wehn des Winterwindes", "tokens": ["Und", "heut", "in", "die\u00b7sem", "sel\u00b7ben", "K\u00f6ln", "zum", "Wehn", "des", "Win\u00b7ter\u00b7win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und zu der Orgel Brausen schallt das Grablied dieses Kindes.", "tokens": ["Und", "zu", "der", "Or\u00b7gel", "Brau\u00b7sen", "schallt", "das", "Grab\u00b7lied", "die\u00b7ses", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Nicht singt die \u00dcberlebende, die Mutter es dem Sohne:", "tokens": ["Nicht", "singt",