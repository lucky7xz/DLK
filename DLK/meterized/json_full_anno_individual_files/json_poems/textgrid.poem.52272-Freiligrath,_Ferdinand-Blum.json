{"textgrid.poem.52272": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Blum", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor zweiundvierzig Jahren war's, da hat mit Macht geschrien", "tokens": ["Vor", "zwei\u00b7und\u00b7vier\u00b7zig", "Jah\u00b7ren", "wa\u00b7r's", ",", "da", "hat", "mit", "Macht", "ge\u00b7schri\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "$,", "ADV", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein siebent\u00e4gig K\u00f6lner Kind auf seiner Mutter Knien;", "tokens": ["Ein", "sie\u00b7ben\u00b7t\u00e4\u00b7gig", "K\u00f6l\u00b7ner", "Kind", "auf", "sei\u00b7ner", "Mut\u00b7ter", "Kni\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Ein Kind mit breiter, offner Stirn, ein Kind von heller Lunge,", "tokens": ["Ein", "Kind", "mit", "brei\u00b7ter", ",", "off\u00b7ner", "Stirn", ",", "ein", "Kind", "von", "hel\u00b7ler", "Lun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ein pr\u00e4chtig Proletarierkind, ein derber K\u00fcferjunge.", "tokens": ["Ein", "pr\u00e4ch\u00b7tig", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7er\u00b7kind", ",", "ein", "der\u00b7ber", "K\u00fc\u00b7fer\u00b7jun\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Er schrie, da\u00df in der Werkstatt rings des Vaters Tonnen hallten;", "tokens": ["Er", "schrie", ",", "da\u00df", "in", "der", "Werk\u00b7statt", "rings", "des", "Va\u00b7ters", "Ton\u00b7nen", "hall\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Die Mutter hat mit L\u00e4cheln ihn an ihre Brust gehalten;", "tokens": ["Die", "Mut\u00b7ter", "hat", "mit", "L\u00e4\u00b7cheln", "ihn", "an", "ih\u00b7re", "Brust", "ge\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "An ihrer Brust, auf ihrem Arm hat sie ihn eingesungen: \u2013", "tokens": ["An", "ih\u00b7rer", "Brust", ",", "auf", "ih\u00b7rem", "Arm", "hat", "sie", "ihn", "ein\u00b7ge\u00b7sun\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Es ist zu K\u00f6ln das Wiegenlied des Knaben hell erklungen.", "tokens": ["Es", "ist", "zu", "K\u00f6ln", "das", "Wie\u00b7gen\u00b7lied", "des", "Kna\u00b7ben", "hell", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Und heut in diesem selben K\u00f6ln zum Wehn des Winterwindes", "tokens": ["Und", "heut", "in", "die\u00b7sem", "sel\u00b7ben", "K\u00f6ln", "zum", "Wehn", "des", "Win\u00b7ter\u00b7win\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und zu der Orgel Brausen schallt das Grablied dieses Kindes.", "tokens": ["Und", "zu", "der", "Or\u00b7gel", "Brau\u00b7sen", "schallt", "das", "Grab\u00b7lied", "die\u00b7ses", "Kin\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Nicht singt die \u00dcberlebende, die Mutter es dem Sohne:", "tokens": ["Nicht", "singt", "die", "\u00dc\u00b7berl\u00b7e\u00b7ben\u00b7de", ",", "die", "Mut\u00b7ter", "es", "dem", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Das ganze schmerzbewegte K\u00f6ln singt es mit festem Tone.", "tokens": ["Das", "gan\u00b7ze", "schmerz\u00b7be\u00b7weg\u00b7te", "K\u00f6ln", "singt", "es", "mit", "fes\u00b7tem", "To\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Vor deinem Gott, du graues Haupt, ausstr\u00f6me deinen Jammer!", "tokens": ["Vor", "dei\u00b7nem", "Gott", ",", "du", "grau\u00b7es", "Haupt", ",", "aus\u00b7str\u00f6\u00b7me", "dei\u00b7nen", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Auch ich bin seine Mutter, Weib! Ich und noch eine Hohe \u2013", "tokens": ["Auch", "ich", "bin", "sei\u00b7ne", "Mut\u00b7ter", ",", "Weib", "!", "Ich", "und", "noch", "ei\u00b7ne", "Ho\u00b7he", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$.", "PPER", "KON", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Ich und die Revolution, die grimme, lichterlohe!", "tokens": ["Ich", "und", "die", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", ",", "die", "grim\u00b7me", ",", "lich\u00b7ter\u00b7lo\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Bleib du daheim mit deinem Schmerz! Wir wahren seine Ehre \u2013", "tokens": ["Bleib", "du", "da\u00b7heim", "mit", "dei\u00b7nem", "Schmerz", "!", "Wir", "wah\u00b7ren", "sei\u00b7ne", "Eh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Des Robert Requiem singt K\u00f6ln, das revolution\u00e4re!", "tokens": ["Des", "Ro\u00b7bert", "Re\u00b7qui\u00b7em", "singt", "K\u00f6ln", ",", "das", "re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7n\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VVFIN", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "So redet K\u00f6ln! Und Orgelsturm entquillt dem Kirchenchore,", "tokens": ["So", "re\u00b7det", "K\u00f6ln", "!", "Und", "Or\u00b7gel\u00b7sturm", "ent\u00b7quillt", "dem", "Kir\u00b7chen\u00b7cho\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es stehn die S\u00e4ulen des Altars umh\u00fcllt mit Trauerflore,", "tokens": ["Es", "stehn", "die", "S\u00e4u\u00b7len", "des", "Al\u00b7tars", "um\u00b7h\u00fcllt", "mit", "Trau\u00b7er\u00b7flo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die Kerzen werfen matten Schein, die Weihrauchwolken ziehen,", "tokens": ["Die", "Ker\u00b7zen", "wer\u00b7fen", "mat\u00b7ten", "Schein", ",", "die", "Weih\u00b7rauch\u00b7wol\u00b7ken", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und tausend Augen werden na\u00df bei Neukomms Melodien.", "tokens": ["Und", "tau\u00b7send", "Au\u00b7gen", "wer\u00b7den", "na\u00df", "bei", "Neu\u00b7komms", "Me\u00b7lo\u00b7dien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "ADJD", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "So ehrt die treue Vaterstadt des Tonnenbinders Knaben \u2013", "tokens": ["So", "ehrt", "die", "treu\u00b7e", "Va\u00b7ter\u00b7stadt", "des", "Ton\u00b7nen\u00b7bin\u00b7ders", "Kna\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ihn, den die Schergen der Gewalt zu Wien gemordert haben,", "tokens": ["Ihn", ",", "den", "die", "Scher\u00b7gen", "der", "Ge\u00b7walt", "zu", "Wi\u00b7en", "ge\u00b7mor\u00b7dert", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "APPR", "NE", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Ihn, der sich seinen Lebensweg, den steilen und den rauhen,", "tokens": ["Ihn", ",", "der", "sich", "sei\u00b7nen", "Le\u00b7bens\u00b7weg", ",", "den", "stei\u00b7len", "und", "den", "rau\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PRF", "PPOSAT", "NN", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Auf bis zu Frankfurts Parlament mit starker Hand gehauen!", "tokens": ["Auf", "bis", "zu", "Frank\u00b7furts", "Par\u00b7la\u00b7ment", "mit", "star\u00b7ker", "Hand", "ge\u00b7hau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NE", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "(dort auch, was er allst\u00fcndlich war, ein Wackrer, kein Verr\u00e4ter!) \u2013", "tokens": ["(", "dort", "auch", ",", "was", "er", "all\u00b7st\u00fcnd\u00b7lich", "war", ",", "ein", "Wack\u00b7rer", ",", "kein", "Ver\u00b7r\u00e4\u00b7ter", "!", ")", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "$,", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Was greift ihr zu den Schwertern nicht, ihr Singer und ihr Beter?", "tokens": ["Was", "greift", "ihr", "zu", "den", "Schwer\u00b7tern", "nicht", ",", "ihr", "Sin\u00b7ger", "und", "ihr", "Be\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Was werdet ihr Posaunen nicht, ihr ehrnen Orgeltuben,", "tokens": ["Was", "wer\u00b7det", "ihr", "Po\u00b7sau\u00b7nen", "nicht", ",", "ihr", "ehr\u00b7nen", "Or\u00b7gel\u00b7tu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Den J\u00fcngsten Tag ins Ohr zu schrein den Henkern und den Buben?", "tokens": ["Den", "J\u00fcng\u00b7sten", "Tag", "ins", "Ohr", "zu", "schrein", "den", "Hen\u00b7kern", "und", "den", "Bu\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKA", "ADJD", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Den Henkern, die ihn hingestreckt auf der Brigittenaue \u2013", "tokens": ["Den", "Hen\u00b7kern", ",", "die", "ihn", "hin\u00b7ge\u00b7streckt", "auf", "der", "Bri\u00b7git\u00b7te\u00b7nau\u00b7e", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Auf festen Knien lag er da im ersten Morgentaue!", "tokens": ["Auf", "fes\u00b7ten", "Kni\u00b7en", "lag", "er", "da", "im", "ers\u00b7ten", "Mor\u00b7gen\u00b7tau\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "Dann sank er hin \u2013 hin in sein Blut \u2013 lautlos! \u2013 heut vor acht Tagen!", "tokens": ["Dann", "sank", "er", "hin", "\u2013", "hin", "in", "sein", "Blut", "\u2013", "laut\u00b7los", "!", "\u2013", "heut", "vor", "acht", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "ADV", "APPR", "PPOSAT", "NN", "$(", "ADJD", "$.", "$(", "ADV", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+++-+-", "measure": "unknown.measure.octa.plus"}, "line.16": {"text": "Zwei Kugeln haben ihm die Brust, eine das Haupt zerschlagen!", "tokens": ["Zwei", "Ku\u00b7geln", "ha\u00b7ben", "ihm", "die", "Brust", ",", "ei\u00b7ne", "das", "Haupt", "zer\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Ja, ruhig hat man ihn gemacht: \u2013 er liegt in seiner Truhe!", "tokens": ["Ja", ",", "ru\u00b7hig", "hat", "man", "ihn", "ge\u00b7macht", ":", "\u2013", "er", "liegt", "in", "sei\u00b7ner", "Tru\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PIS", "PPER", "VVPP", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "So schall' ihm denn ein Requiem, ein Lied der ew'gen Ruhe!", "tokens": ["So", "schall'", "ihm", "denn", "ein", "Re\u00b7qui\u00b7em", ",", "ein", "Lied", "der", "ew'\u00b7gen", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ruh' ihm, der uns die Unruh' hat als Erbteil hinterlassen: \u2013", "tokens": ["Ruh'", "ihm", ",", "der", "uns", "die", "Un\u00b7ruh'", "hat", "als", "Erb\u00b7teil", "hin\u00b7ter\u00b7las\u00b7sen", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "KOKOM", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Mir, als heut im Tempel stand in den bewegten Massen,", "tokens": ["Mir", ",", "als", "heut", "im", "Tem\u00b7pel", "stand", "in", "den", "be\u00b7weg\u00b7ten", "Mas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "APPRART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Mir war's, als h\u00f6rt' ich durch den Sturm der T\u00f6ne ein Geraune:", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "h\u00f6rt'", "ich", "durch", "den", "Sturm", "der", "T\u00f6\u00b7ne", "ein", "Ge\u00b7rau\u00b7ne", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+---+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Du, rechte mit der Stunde nicht! die Orgel ", "tokens": ["Du", ",", "rech\u00b7te", "mit", "der", "Stun\u00b7de", "nicht", "!", "die", "Or\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "APPR", "ART", "NN", "PTKNEG", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Denn nichts als Kampf und wieder Kampf entringt sich diesen Tagen!", "tokens": ["Denn", "nichts", "als", "Kampf", "und", "wie\u00b7der", "Kampf", "ent\u00b7ringt", "sich", "die\u00b7sen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "NN", "KON", "ADV", "NN", "VVFIN", "PRF", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ein Requiem ist Rache nicht, ein Requiem nicht S\u00fchne \u2013", "tokens": ["Ein", "Re\u00b7qui\u00b7em", "ist", "Ra\u00b7che", "nicht", ",", "ein", "Re\u00b7qui\u00b7em", "nicht", "S\u00fch\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "PTKNEG", "$,", "ART", "NN", "PTKNEG", "NN", "$("], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Bald aber steht die R\u00e4cherin auf schwarzbehangner B\u00fchne!", "tokens": ["Bald", "a\u00b7ber", "steht", "die", "R\u00e4\u00b7che\u00b7rin", "auf", "schwarz\u00b7be\u00b7hang\u00b7ner", "B\u00fch\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Die dunkelrote R\u00e4cherin! Mit Blut bespritzt und Z\u00e4hren,", "tokens": ["Die", "dun\u00b7kel\u00b7ro\u00b7te", "R\u00e4\u00b7che\u00b7rin", "!", "Mit", "Blut", "be\u00b7spritzt", "und", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Wird sie und soll und mu\u00df sie sich in Permanenz erkl\u00e4ren!", "tokens": ["Wird", "sie", "und", "soll", "und", "mu\u00df", "sie", "sich", "in", "Per\u00b7ma\u00b7nenz", "er\u00b7kl\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KON", "VMFIN", "KON", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Du rufst sie nicht, die R\u00e4cherin, doch wird die Zeit sie bringen!", "tokens": ["Du", "rufst", "sie", "nicht", ",", "die", "R\u00e4\u00b7che\u00b7rin", ",", "doch", "wird", "die", "Zeit", "sie", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Weh allen, denen schuldlos Blut klebt an den Henkerh\u00e4nden!", "tokens": ["Weh", "al\u00b7len", ",", "de\u00b7nen", "schuld\u00b7los", "Blut", "klebt", "an", "den", "Hen\u00b7ker\u00b7h\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$,", "PRELS", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Vor zweiundvierzig Jahren war's, da hat mit Macht geschrien", "tokens": ["Vor", "zwei\u00b7und\u00b7vier\u00b7zig", "Jah\u00b7ren", "wa\u00b7r's", ",", "da", "hat", "mit", "Macht", "ge\u00b7schri\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VAFIN", "$,", "ADV", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein siebent\u00e4gig K\u00f6lner Kind auf seiner Mutter Knien!", "tokens": ["Ein", "sie\u00b7ben\u00b7t\u00e4\u00b7gig", "K\u00f6l\u00b7ner", "Kind", "auf", "sei\u00b7ner", "Mut\u00b7ter", "Kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-