{"dta.poem.21334": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund die Freiheit kommt mit Spiel und Tanz,", "tokens": ["\u201e", "und", "die", "Frei\u00b7heit", "kommt", "mit", "Spiel", "und", "Tanz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit der Fahne, der wei\u00df-blau-rothen;", "tokens": ["Mit", "der", "Fah\u00b7ne", ",", "der", "wei\u00df\u00b7blau\u00b7ro\u00b7then", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "--+-+-++-", "measure": "anapaest.init"}, "line.3": {"text": "Vielleicht holt sie sogar aus dem Grab", "tokens": ["Viel\u00b7leicht", "holt", "sie", "so\u00b7gar", "aus", "dem", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den Bonaparte, den Todten!\u201c", "tokens": ["Den", "Bo\u00b7na\u00b7par\u00b7te", ",", "den", "Tod\u00b7ten", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach Gott! die Ritter sind immer noch hier,", "tokens": ["Ach", "Gott", "!", "die", "Rit\u00b7ter", "sind", "im\u00b7mer", "noch", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und manche dieser G\u00e4uche,", "tokens": ["Und", "man\u00b7che", "die\u00b7ser", "G\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die spindeld\u00fcrre gekommen in\u2019s Land,", "tokens": ["Die", "spin\u00b7del\u00b7d\u00fcr\u00b7re", "ge\u00b7kom\u00b7men", "in's", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die haben jetzt dicke B\u00e4uche.", "tokens": ["Die", "ha\u00b7ben", "jetzt", "di\u00b7cke", "B\u00e4u\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die blassen Canaillen, die ausgesehn", "tokens": ["Die", "blas\u00b7sen", "Ca\u00b7nail\u00b7len", ",", "die", "aus\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie Liebe, Glauben und Hoffen,", "tokens": ["Wie", "Lie\u00b7be", ",", "Glau\u00b7ben", "und", "Hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben seitdem in unserm Wein", "tokens": ["Sie", "ha\u00b7ben", "seit\u00b7dem", "in", "un\u00b7serm", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich rothe Nasen gesoffen \u2013 \u2013 \u2013", "tokens": ["Sich", "ro\u00b7the", "Na\u00b7sen", "ge\u00b7sof\u00b7fen", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PRF", "ADJA", "NN", "VVPP", "$(", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und die Freiheit hat sich den Fu\u00df verrenkt,", "tokens": ["Und", "die", "Frei\u00b7heit", "hat", "sich", "den", "Fu\u00df", "ver\u00b7renkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Kann nicht mehr springen und st\u00fcrmen;", "tokens": ["Kann", "nicht", "mehr", "sprin\u00b7gen", "und", "st\u00fcr\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Trikolore in Paris", "tokens": ["Die", "Tri\u00b7ko\u00b7lo\u00b7re", "in", "Pa\u00b7ris"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut traurig herab von den Th\u00fcrmen.", "tokens": ["Schaut", "trau\u00b7rig", "her\u00b7ab", "von", "den", "Th\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}