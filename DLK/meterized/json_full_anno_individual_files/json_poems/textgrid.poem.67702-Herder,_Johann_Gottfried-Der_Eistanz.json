{"textgrid.poem.67702": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Eistanz", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir schweben, wir wallen auf hallendem Meer,", "tokens": ["Wir", "schwe\u00b7ben", ",", "wir", "wal\u00b7len", "auf", "hal\u00b7len\u00b7dem", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf Silberkrystallen dahin und daher;", "tokens": ["Auf", "Sil\u00b7ber\u00b7krys\u00b7tal\u00b7len", "da\u00b7hin", "und", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "KON", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Stahl ist uns Fittig, der Himmel das Dach,", "tokens": ["Der", "Stahl", "ist", "uns", "Fit\u00b7tig", ",", "der", "Him\u00b7mel", "das", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die L\u00fcfte sind heilig und schweben uns nach.", "tokens": ["Die", "L\u00fcf\u00b7te", "sind", "hei\u00b7lig", "und", "schwe\u00b7ben", "uns", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So gleiten wir, Br\u00fcder, mit fr\u00f6hlichem Sinn", "tokens": ["So", "glei\u00b7ten", "wir", ",", "Br\u00fc\u00b7der", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf eherner Tiefe das Leben dahin.", "tokens": ["Auf", "e\u00b7her\u00b7ner", "Tie\u00b7fe", "das", "Le\u00b7ben", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wer w\u00f6lbte Dich oben, Du goldenes Haus,", "tokens": ["Wer", "w\u00f6lb\u00b7te", "Dich", "o\u00b7ben", ",", "Du", "gol\u00b7de\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und legte den Boden mit Demant uns aus", "tokens": ["Und", "leg\u00b7te", "den", "Bo\u00b7den", "mit", "De\u00b7mant", "uns", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "PPER", "PTKVZ"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gab uns den fl\u00fcchtigen Funken im Stahl,", "tokens": ["Und", "gab", "uns", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Fun\u00b7ken", "im", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu tanzen, zu schweben im himmlischen Saal?", "tokens": ["Zu", "tan\u00b7zen", ",", "zu", "schwe\u00b7ben", "im", "himm\u00b7li\u00b7schen", "Saal", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So schweben wir, Br\u00fcder, mit fr\u00f6hlichem Sinn", "tokens": ["So", "schwe\u00b7ben", "wir", ",", "Br\u00fc\u00b7der", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Im himmlischen Saale das Leben dahin.", "tokens": ["Im", "himm\u00b7li\u00b7schen", "Saa\u00b7le", "das", "Le\u00b7ben", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Da stand sie, die Sonne, in D\u00fcfte geh\u00fcllt,", "tokens": ["Da", "stand", "sie", ",", "die", "Son\u00b7ne", ",", "in", "D\u00fcf\u00b7te", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da rauchen die Berge, da schwebet ihr Bild,", "tokens": ["Da", "rau\u00b7chen", "die", "Ber\u00b7ge", ",", "da", "schwe\u00b7bet", "ihr", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da ging sie danieder, und siehe, der Mond,", "tokens": ["Da", "ging", "sie", "da\u00b7nie\u00b7der", ",", "und", "sie\u00b7he", ",", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "KON", "VVIMP", "$,", "ART", "NN", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie silbern er \u00fcber und unter uns wohnt!", "tokens": ["Wie", "sil\u00b7bern", "er", "\u00fc\u00b7ber", "und", "un\u00b7ter", "uns", "wohnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "So wallen wir, Br\u00fcder, mit fr\u00f6hlichem Sinn", "tokens": ["So", "wal\u00b7len", "wir", ",", "Br\u00fc\u00b7der", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Durch Mond und durch Sonne das Leben dahin.", "tokens": ["Durch", "Mond", "und", "durch", "Son\u00b7ne", "das", "Le\u00b7ben", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Seht auf nun, da brennen im himmlischen Meer", "tokens": ["Seht", "auf", "nun", ",", "da", "bren\u00b7nen", "im", "himm\u00b7li\u00b7schen", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "$,", "ADV", "VVINF", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Funken und brennen im Frost um uns her.", "tokens": ["Die", "Fun\u00b7ken", "und", "bren\u00b7nen", "im", "Frost", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVINF", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der oben den Himmel mit Sonnen besteckt,", "tokens": ["Der", "o\u00b7ben", "den", "Him\u00b7mel", "mit", "Son\u00b7nen", "be\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Hat's unten mit Blumen des Frostes gedeckt.", "tokens": ["Hat's", "un\u00b7ten", "mit", "Blu\u00b7men", "des", "Fros\u00b7tes", "ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir gleiten, o Br\u00fcder, mit fr\u00f6hlichem Sinn", "tokens": ["Wir", "glei\u00b7ten", ",", "o", "Br\u00fc\u00b7der", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf Sternengefilden das Leben dahin.", "tokens": ["Auf", "Ster\u00b7nen\u00b7ge\u00b7fil\u00b7den", "das", "Le\u00b7ben", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Er macht' uns ger\u00e4umig den luftigen Saal", "tokens": ["Er", "macht'", "uns", "ge\u00b7r\u00e4u\u00b7mig", "den", "luf\u00b7ti\u00b7gen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und gab uns in N\u00f6then die F\u00fc\u00dfe von Stahl", "tokens": ["Und", "gab", "uns", "in", "N\u00f6\u00b7then", "die", "F\u00fc\u00b7\u00dfe", "von", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und gab uns im Froste das w\u00e4rmende Herz,", "tokens": ["Und", "gab", "uns", "im", "Fros\u00b7te", "das", "w\u00e4r\u00b7men\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zu stehn auf den Fluthen, zu schweben im Scherz.", "tokens": ["Zu", "stehn", "auf", "den", "Flut\u00b7hen", ",", "zu", "schwe\u00b7ben", "im", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Wir streben, o Br\u00fcder, mit ehernem Sinn", "tokens": ["Wir", "stre\u00b7ben", ",", "o", "Br\u00fc\u00b7der", ",", "mit", "e\u00b7her\u00b7nem", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf Fluthen und Abgrund das Leben dahin.", "tokens": ["Auf", "Flut\u00b7hen", "und", "Ab\u00b7grund", "das", "Le\u00b7ben", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Da kommt sie, die G\u00f6ttin, und schwebet, ein Schwan,", "tokens": ["Da", "kommt", "sie", ",", "die", "G\u00f6t\u00b7tin", ",", "und", "schwe\u00b7bet", ",", "ein", "Schwan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In lieblichen Wellen hinab und hinan,", "tokens": ["In", "lieb\u00b7li\u00b7chen", "Wel\u00b7len", "hin\u00b7ab", "und", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gestalt wie der Juno, mit rosigem Knie;", "tokens": ["Ge\u00b7stalt", "wie", "der", "Ju\u00b7no", ",", "mit", "ro\u00b7si\u00b7gem", "Knie", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die L\u00fcfte, sie f\u00fchlen, sie tragen sie.", "tokens": ["Die", "L\u00fcf\u00b7te", ",", "sie", "f\u00fch\u00b7len", ",", "sie", "tra\u00b7gen", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Im Schimmer des Mondes, im schweigenden Tanz,", "tokens": ["Im", "Schim\u00b7mer", "des", "Mon\u00b7des", ",", "im", "schwei\u00b7gen\u00b7den", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie flie\u00dfet ihr Schleier, wie schwebet ihr Kranz!", "tokens": ["Wie", "flie\u00b7\u00dfet", "ihr", "Schlei\u00b7er", ",", "wie", "schwe\u00b7bet", "ihr", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die liebenden Sterne, sie sanken hinab", "tokens": ["Die", "lie\u00b7ben\u00b7den", "Ster\u00b7ne", ",", "sie", "san\u00b7ken", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zum Schleier, zum Kranz, der sie liebend umgab.", "tokens": ["Zum", "Schlei\u00b7er", ",", "zum", "Kranz", ",", "der", "sie", "lie\u00b7bend", "um\u00b7gab", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Sie schwebte vor\u00fcber, da klang sie den Stahl,", "tokens": ["Sie", "schweb\u00b7te", "vor\u00b7\u00fc\u00b7ber", ",", "da", "klang", "sie", "den", "Stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da klangen und sangen im himmlischen Saal", "tokens": ["Da", "klan\u00b7gen", "und", "san\u00b7gen", "im", "himm\u00b7li\u00b7schen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Sterne; da hat sich err\u00f6thend ihr Bild \u2013", "tokens": ["Die", "Ster\u00b7ne", ";", "da", "hat", "sich", "er\u00b7r\u00f6\u00b7thend", "ihr", "Bild", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VAFIN", "PRF", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wohin dort? \u2013 in silberne D\u00fcfte geh\u00fcllt.", "tokens": ["Wo\u00b7hin", "dort", "?", "\u2013", "in", "sil\u00b7ber\u00b7ne", "D\u00fcf\u00b7te", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "$(", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Wir schweben, wir wallen auf hallendem Meer,", "tokens": ["Wir", "schwe\u00b7ben", ",", "wir", "wal\u00b7len", "auf", "hal\u00b7len\u00b7dem", "Meer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf Silberkrystallen dahin und daher;", "tokens": ["Auf", "Sil\u00b7ber\u00b7krys\u00b7tal\u00b7len", "da\u00b7hin", "und", "da\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PAV", "KON", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Stahl ist uns Fittig, der Himmel das Dach,", "tokens": ["Der", "Stahl", "ist", "uns", "Fit\u00b7tig", ",", "der", "Him\u00b7mel", "das", "Dach", ","], "token_info": ["