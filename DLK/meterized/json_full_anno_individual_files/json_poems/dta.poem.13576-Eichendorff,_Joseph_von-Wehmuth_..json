{"dta.poem.13576": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wehmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es waren zwei junge Grafen", "tokens": ["Es", "wa\u00b7ren", "zwei", "jun\u00b7ge", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verliebt bis in den Tod,", "tokens": ["Ver\u00b7liebt", "bis", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die konnten nicht ruh'n noch schlafen", "tokens": ["Die", "konn\u00b7ten", "nicht", "ruh'n", "noch", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVFIN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis an den Morgen roth.", "tokens": ["Bis", "an", "den", "Mor\u00b7gen", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}