{"textgrid.poem.52211": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ein Brief", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war ein lustig Ziehen", "tokens": ["Das", "war", "ein", "lus\u00b7tig", "Zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Reisen durch die Welt!", "tokens": ["Und", "Rei\u00b7sen", "durch", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war ein Fackelspr\u00fchen", "tokens": ["Das", "war", "ein", "Fa\u00b7ckel\u00b7spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Z\u00fcrich bis zum Belt!", "tokens": ["Von", "Z\u00fc\u00b7rich", "bis", "zum", "Belt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aus Herzen und aus K\u00fcchen", "tokens": ["Aus", "Her\u00b7zen", "und", "aus", "K\u00fc\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stieg Weihrauch dir empor;", "tokens": ["Stieg", "Weih\u00b7rauch", "dir", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Pelotons von Tafelspr\u00fcchen", "tokens": ["Pe\u00b7lo\u00b7tons", "von", "Ta\u00b7fel\u00b7spr\u00fc\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schlugen knatternd an dein Ohr!", "tokens": ["Schlu\u00b7gen", "knat\u00b7ternd", "an", "dein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein neuer Held Sankt J\u00fcrgen", "tokens": ["Ein", "neu\u00b7er", "Held", "Sankt", "J\u00fcr\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Deutschland zogst du frei,", "tokens": ["Durch", "Deutschland", "zogst", "du", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Im Fluge zu erw\u00fcrgen", "tokens": ["Im", "Flu\u00b7ge", "zu", "er\u00b7w\u00fcr\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Molch der Tyrannei!", "tokens": ["Den", "Molch", "der", "Ty\u00b7ran\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie kommt es, da\u00df der Grause", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "der", "Grau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch z\u00fcngelt ungescheut?", "tokens": ["Noch", "z\u00fcn\u00b7gelt", "un\u00b7ge\u00b7scheut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verpa\u00dftest du beim Schmause", "tokens": ["Ver\u00b7pa\u00df\u00b7test", "du", "beim", "Schmau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vielleicht die rechte Zeit?", "tokens": ["Viel\u00b7leicht", "die", "rech\u00b7te", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du trotziger Diktator,", "tokens": ["Du", "trot\u00b7zi\u00b7ger", "Dik\u00b7ta\u00b7tor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie bald zerbrach dein Stab!", "tokens": ["Wie", "bald", "zer\u00b7brach", "dein", "Stab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dahin der Agitator,", "tokens": ["Da\u00b7hin", "der", "A\u00b7git\u00b7a\u00b7tor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und \u00fcbrig nur \u2013 der Schwab'!", "tokens": ["Und", "\u00fcb\u00b7rig", "nur", "\u2013", "der", "Schwab'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$(", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verwelkt schon deine Blume!", "tokens": ["Ver\u00b7welkt", "schon", "dei\u00b7ne", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Kranz, o Freund, h\u00e4ngt schief!", "tokens": ["Dein", "Kranz", ",", "o", "Freund", ",", "h\u00e4ngt", "schief", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du schriebst dem eignen Ruhme,", "tokens": ["Du", "schriebst", "dem", "eig\u00b7nen", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach, den Uriasbrief!", "tokens": ["Ach", ",", "den", "U\u00b7rias\u00b7brief", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nun k\u00f6nnen sie dich b\u00e4nd'gen,", "tokens": ["Nun", "k\u00f6n\u00b7nen", "sie", "dich", "b\u00e4n\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Philister und Zelot:", "tokens": ["Phi\u00b7lis\u00b7ter", "und", "Ze\u00b7lot", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbda habt ihr den Lebend'gen!", "tokens": ["\u00bb", "da", "habt", "ihr", "den", "Le\u00b7ben\u00b7d'\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er schlug sich selber tot!\u00ab", "tokens": ["Er", "schlug", "sich", "sel\u00b7ber", "tot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wen Ruhmeskleider zieren,", "tokens": ["Wen", "Ruh\u00b7mes\u00b7klei\u00b7der", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der h\u00fcte sie, wie Schnee!", "tokens": ["Der", "h\u00fc\u00b7te", "sie", ",", "wie", "Schnee", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wahr ist es: Renommieren", "tokens": ["Wahr", "ist", "es", ":", "Re\u00b7nom\u00b7mie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verdirbt die Renommee!", "tokens": ["Ver\u00b7dirbt", "die", "Re\u00b7nom\u00b7mee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer sagt, er st\u00e4nde Wache", "tokens": ["Wer", "sagt", ",", "er", "st\u00e4n\u00b7de", "Wa\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrs Recht, der halte Stich,", "tokens": ["F\u00fcrs", "Recht", ",", "der", "hal\u00b7te", "Stich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gebe statt der Sache", "tokens": ["Und", "ge\u00b7be", "statt", "der", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht immer nur sein Ich!", "tokens": ["Nicht", "im\u00b7mer", "nur", "sein", "Ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PPOSAT", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schwinge, wo f\u00fcrs Ganze", "tokens": ["Der", "schwin\u00b7ge", ",", "wo", "f\u00fcrs", "Gan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man ernste Speere bricht,", "tokens": ["Man", "erns\u00b7te", "Spee\u00b7re", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ruhmredig nicht die Lanze,", "tokens": ["Ruhm\u00b7re\u00b7dig", "nicht", "die", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mit der die Hoffart ficht!", "tokens": ["Mit", "der", "die", "Hof\u00b7fart", "ficht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer so mit Wein der Ehren", "tokens": ["Wer", "so", "mit", "Wein", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfangen ward, wie du,", "tokens": ["Emp\u00b7fan\u00b7gen", "ward", ",", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie mocht' er den bet\u00f6ren,", "tokens": ["Wie", "mocht'", "er", "den", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trank auch ein Volk ihm zu?", "tokens": ["Trank", "auch", "ein", "Volk", "ihm", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Schmach, im Rausch zu fallen,", "tokens": ["O", "Schmach", ",", "im", "Rausch", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In H\u00e4nden noch den Krug!", "tokens": ["In", "H\u00e4n\u00b7den", "noch", "den", "Krug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Berauscht sich zu erlallen", "tokens": ["Be\u00b7rauscht", "sich", "zu", "er\u00b7lal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des L\u00e4cherlichen Fluch!", "tokens": ["Des", "L\u00e4\u00b7cher\u00b7li\u00b7chen", "Fluch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das ist's \u2013 Wohl wird geschlagen", "tokens": ["Das", "ist's", "\u2013", "Wohl", "wird", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Held im Kriegsgew\u00fchl;", "tokens": ["Ein", "Held", "im", "Kriegs\u00b7ge\u00b7w\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In alt und neuen Tagen", "tokens": ["In", "alt", "und", "neu\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schritt mancher ins Exil;", "tokens": ["Schritt", "man\u00b7cher", "ins", "E\u00b7xil", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch rings im Volksget\u00fcmmel", "tokens": ["Doch", "rings", "im", "Volks\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein H\u00f6hnen und kein Groll:", "tokens": ["Kein", "H\u00f6h\u00b7nen", "und", "kein", "Groll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Stern erlosch am Himmel \u2013", "tokens": ["Sein", "Stern", "er\u00b7losch", "am", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch rein und w\u00fcrdevoll!", "tokens": ["Doch", "rein", "und", "w\u00fcr\u00b7de\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Freiheit rang die H\u00e4nde,", "tokens": ["Die", "Frei\u00b7heit", "rang", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da seine band der Strick!", "tokens": ["Da", "sei\u00b7ne", "band", "der", "Strick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie tote Fackelbr\u00e4nde", "tokens": ["Wie", "to\u00b7te", "Fa\u00b7ckel\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freunde d\u00fcstrer Blick!", "tokens": ["Der", "Freun\u00b7de", "d\u00fcst\u00b7rer", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ringsum Gewitterstirnen,", "tokens": ["Ring\u00b7sum", "Ge\u00b7wit\u00b7ters\u00b7tir\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Rings Murmeln durchs Visier,", "tokens": ["Rings", "Mur\u00b7meln", "durchs", "Vi\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ringsum verhaltnes Z\u00fcrnen \u2013", "tokens": ["Ring\u00b7sum", "ver\u00b7halt\u00b7nes", "Z\u00fcr\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "O, st\u00e4nd' es so mit dir!", "tokens": ["O", ",", "st\u00e4nd'", "es", "so", "mit", "dir", "!"], "token_info": ["word", "punct", "word", "word", "