ic.tri"}, "line.4": {"text": "Den Molch der Tyrannei!", "tokens": ["Den", "Molch", "der", "Ty\u00b7ran\u00b7nei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie kommt es, da\u00df der Grause", "tokens": ["Wie", "kommt", "es", ",", "da\u00df", "der", "Grau\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch z\u00fcngelt ungescheut?", "tokens": ["Noch", "z\u00fcn\u00b7gelt", "un\u00b7ge\u00b7scheut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verpa\u00dftest du beim Schmause", "tokens": ["Ver\u00b7pa\u00df\u00b7test", "du", "beim", "Schmau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vielleicht die rechte Zeit?", "tokens": ["Viel\u00b7leicht", "die", "rech\u00b7te", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Du trotziger Diktator,", "tokens": ["Du", "trot\u00b7zi\u00b7ger", "Dik\u00b7ta\u00b7tor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie bald zerbrach dein Stab!", "tokens": ["Wie", "bald", "zer\u00b7brach", "dein", "Stab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dahin der Agitator,", "tokens": ["Da\u00b7hin", "der", "A\u00b7git\u00b7a\u00b7tor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und \u00fcbrig nur \u2013 der Schwab'!", "tokens": ["Und", "\u00fcb\u00b7rig", "nur", "\u2013", "der", "Schwab'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$(", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verwelkt schon deine Blume!", "tokens": ["Ver\u00b7welkt", "schon", "dei\u00b7ne", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Kranz, o Freund, h\u00e4ngt schief!", "tokens": ["Dein", "Kranz", ",", "o", "Freund", ",", "h\u00e4ngt", "schief", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du schriebst dem eignen Ruhme,", "tokens": ["Du", "schriebst", "dem", "eig\u00b7nen", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach, den Uriasbrief!", "tokens": ["Ach", ",", "den", "U\u00b7rias\u00b7brief", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Nun k\u00f6nnen sie dich b\u00e4nd'gen,", "tokens": ["Nun", "k\u00f6n\u00b7nen", "sie", "dich", "b\u00e4n\u00b7d'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Philister und Zelot:", "tokens": ["Phi\u00b7lis\u00b7ter", "und", "Ze\u00b7lot", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbda habt ihr den Lebend'gen!", "tokens": ["\u00bb", "da", "habt", "ihr", "den", "Le\u00b7ben\u00b7d'\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er schlug sich selber tot!\u00ab", "tokens": ["Er", "schlug", "sich", "sel\u00b7ber", "tot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wen Ruhmeskleider zieren,", "tokens": ["Wen", "Ruh\u00b7mes\u00b7klei\u00b7der", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der h\u00fcte sie, wie Schnee!", "tokens": ["Der", "h\u00fc\u00b7te", "sie", ",", "wie", "Schnee", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wahr ist es: Renommieren", "tokens": ["Wahr", "ist", "es", ":", "Re\u00b7nom\u00b7mie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verdirbt die Renommee!", "tokens": ["Ver\u00b7dirbt", "die", "Re\u00b7nom\u00b7mee", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wer sagt, er st\u00e4nde Wache", "tokens": ["Wer", "sagt", ",", "er", "st\u00e4n\u00b7de", "Wa\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrs Recht, der halte Stich,", "tokens": ["F\u00fcrs", "Recht", ",", "der", "hal\u00b7te", "Stich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gebe statt der Sache", "tokens": ["Und", "ge\u00b7be", "statt", "der", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht immer nur sein Ich!", "tokens": ["Nicht", "im\u00b7mer", "nur", "sein", "Ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PPOSAT", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schwinge, wo f\u00fcrs Ganze", "tokens": ["Der", "schwin\u00b7ge", ",", "wo", "f\u00fcrs", "Gan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man ernste Speere bricht,", "tokens": ["Man", "erns\u00b7te", "Spee\u00b7re", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ruhmredig nicht die Lanze,", "tokens": ["Ruhm\u00b7re\u00b7dig", "nicht", "die", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Mit der die Hoffart ficht!", "tokens": ["Mit", "der", "die", "Hof\u00b7fart", "ficht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wer so mit Wein der Ehren", "tokens": ["Wer", "so", "mit", "Wein", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Empfangen ward, wie du,", "tokens": ["Emp\u00b7fan\u00b7gen", "ward", ",", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie mocht' er den bet\u00f6ren,", "tokens": ["Wie", "mocht'", "er", "den", "be\u00b7t\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trank auch ein Volk ihm zu?", "tokens": ["Trank", "auch", "ein", "Volk", "ihm", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Schmach, im Rausch zu fallen,", "tokens": ["O", "Schmach", ",", "im", "Rausch", "zu", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In H\u00e4nden noch den Krug!", "tokens": ["In", "H\u00e4n\u00b7den", "noch", "den", "Krug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Berauscht sich zu erlallen", "tokens": ["Be\u00b7rauscht", "sich", "zu", "er\u00b7lal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des L\u00e4cherlichen Fluch!", "tokens": ["Des", "L\u00e4\u00b7cher\u00b7li\u00b7chen", "Fluch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Das ist's \u2013 Wohl wird geschlagen", "tokens": ["Das", "ist's", "\u2013", "Wohl", "wird", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$(", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Held im Kriegsgew\u00fchl;", "tokens": ["Ein", "Held", "im", "Kriegs\u00b7ge\u00b7w\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In alt und neuen Tagen", "tokens": ["In", "alt", "und", "neu\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schritt mancher ins Exil;", "tokens": ["Schritt", "man\u00b7cher", "ins", "E\u00b7xil", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch rings im Volksget\u00fcmmel", "tokens": ["Doch", "rings", "im", "Volks\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein H\u00f6hnen und kein Groll:", "tokens": ["Kein", "H\u00f6h\u00b7nen", "und", "kein", "Groll", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Stern erlosch am Himmel \u2013", "tokens": ["Sein", "Stern", "er\u00b7losch", "am", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch rein und w\u00fcrdevoll!", "tokens": ["Doch", "rein", "und", "w\u00fcr\u00b7de\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Die Freiheit rang die H\u00e4nde,", "tokens": ["Die", "Frei\u00b7heit", "rang", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da seine band der Strick!", "tokens": ["Da", "sei\u00b7ne", "band", "der", "Strick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie tote Fackelbr\u00e4nde", "tokens": ["Wie", "to\u00b7te", "Fa\u00b7ckel\u00b7br\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freunde d\u00fcstrer Blick!", "tokens": ["Der", "Freun\u00b7de", "d\u00fcst\u00b7rer", "Blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ringsum Gewitterstirnen,", "tokens": ["Ring\u00b7sum", "Ge\u00b7wit\u00b7ters\u00b7tir\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Rings Murmeln durchs Visier,", "tokens": ["Rings", "Mur\u00b7meln", "durchs", "Vi\u00b7sier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ringsum verhaltnes Z\u00fcrnen \u2013", "tokens": ["Ring\u00b7sum", "ver\u00b7halt\u00b7nes", "Z\u00fcr\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "O, st\u00e4nd' es so mit dir!", "tokens": ["O", ",", "st\u00e4nd'", "es", "so", "mit", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Dir folgt, wie plumpen Schnittern,", "tokens": ["Dir", "folgt", ",", "wie", "plum\u00b7pen", "Schnit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Rauschen, h\u00f6rbar kaum;", "tokens": ["Ein", "Rau\u00b7schen", ",", "h\u00f6r\u00b7bar", "kaum", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der Triebe Zittern", "tokens": ["Das", "ist", "der", "Trie\u00b7be", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am jungen Freiheitsbaum!", "tokens": ["Am", "jun\u00b7gen", "Frei\u00b7heits\u00b7baum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Knospen und der Triebe,", "tokens": ["Der", "Knos\u00b7pen", "und", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die freudig ihn geschm\u00fcckt!", "tokens": ["Die", "freu\u00b7dig", "ihn", "ge\u00b7schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die, ach, mit ", "tokens": ["Die", ",", "ach", ",", "mit"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ART", "$,", "ITJ", "$,", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Du alle fast geknickt!", "tokens": ["Du", "al\u00b7le", "fast", "ge\u00b7knickt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "So ziehst du! \u2013 Was ich sagte,", "tokens": ["So", "ziehst", "du", "!", "\u2013", "Was", "ich", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl klingt es schonungslos!", "tokens": ["Wohl", "klingt", "es", "scho\u00b7nungs\u00b7los", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wer uns Arndt verklagte,", "tokens": ["Doch", "wer", "uns", "Arndt", "ver\u00b7klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zog selber sich das Los!", "tokens": ["Zog", "sel\u00b7ber", "sich", "das", "Los", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du nanntest den alten Riesen", "tokens": ["Du", "nann\u00b7test", "den", "al\u00b7ten", "Rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu alt zu dieser Frist?", "tokens": ["Zu", "alt", "zu", "die\u00b7ser", "Frist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du hast uns nur bewiesen,", "tokens": ["Du", "hast", "uns", "nur", "be\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df du zu jung noch bist!", "tokens": ["Da\u00df", "du", "zu", "jung", "noch", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Zieh hin, \u2013 doch um zu kehren!", "tokens": ["Zieh", "hin", ",", "\u2013", "doch", "um", "zu", "keh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "$(", "ADV", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Freiheit kann verzeihn!", "tokens": ["Die", "Frei\u00b7heit", "kann", "ver\u00b7zeihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bring' ein die alten Ehren,", "tokens": ["Bring'", "ein", "die", "al\u00b7ten", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Liedern bring' sie ein!", "tokens": ["Mit", "Lie\u00b7dern", "bring'", "sie", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Dichtung Goldstandarte,", "tokens": ["Der", "Dich\u00b7tung", "Gold\u00b7stand\u00b7ar\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "La\u00df wehn sie, doppeltreich: \u2013", "tokens": ["La\u00df", "wehn", "sie", ",", "dop\u00b7pel\u00b7treich", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Poet, wetz' aus die Scharte,", "tokens": ["Po\u00b7et", ",", "wetz'", "aus", "die", "Schar\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wetz' aus den Schwabenstreich!", "tokens": ["Wetz'", "aus", "den", "Schwa\u00b7ben\u00b7streich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}