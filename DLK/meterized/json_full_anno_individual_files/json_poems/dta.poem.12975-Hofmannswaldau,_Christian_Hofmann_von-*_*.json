{"dta.poem.12975": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ausw\u00fcrffe der natur! ihr unbeseelten st\u00fccke,", "tokens": ["Aus\u00b7w\u00fcrf\u00b7fe", "der", "na\u00b7tur", "!", "ihr", "un\u00b7be\u00b7seel\u00b7ten", "st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist euer gl\u00fccke nicht des gr\u00f6sten neides werth?", "tokens": ["Ist", "eu\u00b7er", "gl\u00fc\u00b7cke", "nicht", "des", "gr\u00f6s\u00b7ten", "nei\u00b7des", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zeit, die alles sonst verschlimmert und verzehrt,", "tokens": ["Die", "zeit", ",", "die", "al\u00b7les", "sonst", "ver\u00b7schlim\u00b7mert", "und", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erwirbet euch, nicht uns, das sch\u00e4tzbarste gel\u00fccke.", "tokens": ["Er\u00b7wir\u00b7bet", "euch", ",", "nicht", "uns", ",", "das", "sch\u00e4tz\u00b7bars\u00b7te", "ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PTKNEG", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Was hebt man theurer auf, als einen alten stein?", "tokens": ["Was", "hebt", "man", "theu\u00b7rer", "auf", ",", "als", "ei\u00b7nen", "al\u00b7ten", "stein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der schnelle wandrer bleibt bey schutt und grause stehen:", "tokens": ["Der", "schnel\u00b7le", "wand\u00b7rer", "bleibt", "bey", "schutt", "und", "grau\u00b7se", "ste\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein halb vermodert bild l\u00e4st ihn so bald nicht gehen;", "tokens": ["Ein", "halb", "ver\u00b7mo\u00b7dert", "bild", "l\u00e4st", "ihn", "so", "bald", "nicht", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unser alter tr\u00e4gt nichts als verachtung ein.", "tokens": ["Und", "un\u00b7ser", "al\u00b7ter", "tr\u00e4gt", "nichts", "als", "ver\u00b7ach\u00b7tung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "PIS", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}