{"dta.poem.9569": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie lie\u00df ihn im finstern mit einer laterne  \n zu sich holen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll die leuchte doch/ die du mir zugesandt?", "tokens": ["Was", "soll", "die", "leuch\u00b7te", "doch", "/", "die", "du", "mir", "zu\u00b7ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "VVFIN", "ADV", "$(", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewi\u00df/ statt ihrer kan Cupido mir schon dienen;", "tokens": ["Ge\u00b7wi\u00df", "/", "statt", "ih\u00b7rer", "kan", "Cu\u00b7pi\u00b7do", "mir", "schon", "die\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "APPR", "PPOSAT", "VMFIN", "NE", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn ob er gleich stockblind sein lebetag erschienen/", "tokens": ["Denn", "ob", "er", "gleich", "stock\u00b7blind", "sein", "le\u00b7be\u00b7tag", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "F\u00fchrt er/ als gleitsmann/ mich doch sicher bey der hand,", "tokens": ["F\u00fchrt", "er", "/", "als", "gleits\u00b7mann", "/", "mich", "doch", "si\u00b7cher", "bey", "der", "hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOKOM", "ADV", "$(", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zudem/ er scheut das licht und will nicht seyn erkant", "tokens": ["Zu\u00b7dem", "/", "er", "scheut", "das", "licht", "und", "will", "nicht", "seyn", "er\u00b7kant"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$(", "PPER", "VVFIN", "ART", "NN", "KON", "VMFIN", "PTKNEG", "VAINF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An der entbl\u00f6\u00dften haut/ und den verliebten minen;", "tokens": ["An", "der", "ent\u00b7bl\u00f6\u00df\u00b7ten", "haut", "/", "und", "den", "ver\u00b7lieb\u00b7ten", "mi\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum wird er sich auch nie/ als nur des nachts/ erk\u00fchnen/", "tokens": ["Drum", "wird", "er", "sich", "auch", "nie", "/", "als", "nur", "des", "nachts", "/", "er\u00b7k\u00fch\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "$(", "KOKOM", "ADV", "ART", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn dunckle schatten sind/ zu streichen durch das land.", "tokens": ["Wenn", "dunck\u00b7le", "schat\u00b7ten", "sind", "/", "zu", "strei\u00b7chen", "durch", "das", "land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "VAFIN", "$(", "PTKZU", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil nun mein wille wird nach seinem wunsch getrieben/", "tokens": ["Weil", "nun", "mein", "wil\u00b7le", "wird", "nach", "sei\u00b7nem", "wunsch", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So mu\u00df ich mit ihm fort ohn eintzigen bedacht.", "tokens": ["So", "mu\u00df", "ich", "mit", "ihm", "fort", "ohn", "eint\u00b7zi\u00b7gen", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "PTKVZ", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich folg ihm endlich auch gar gern und mit belieben:", "tokens": ["Ich", "folg", "ihm", "end\u00b7lich", "auch", "gar", "gern", "und", "mit", "be\u00b7lie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gnug! wenn er mich nur f\u00fchrt zu dich und deiner pracht.", "tokens": ["Gnug", "!", "wenn", "er", "mich", "nur", "f\u00fchrt", "zu", "dich", "und", "dei\u00b7ner", "pracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was hat es denn vor noth/ laternen anzuz\u00fcnden/", "tokens": ["Was", "hat", "es", "denn", "vor", "noth", "/", "la\u00b7ter\u00b7nen", "an\u00b7zu\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "$(", "VVINF", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wann ich die sonne selbst in dir/ mein licht/ kan finden.", "tokens": ["Wann", "ich", "die", "son\u00b7ne", "selbst", "in", "dir", "/", "mein", "licht", "/", "kan", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "PPER", "$(", "PPOSAT", "NN", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}