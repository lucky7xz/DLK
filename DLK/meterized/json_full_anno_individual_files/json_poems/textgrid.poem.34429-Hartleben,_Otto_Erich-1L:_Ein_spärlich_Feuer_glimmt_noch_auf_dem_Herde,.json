{"textgrid.poem.34429": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein sp\u00e4rlich Feuer glimmt noch auf dem Herde,", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sp\u00e4rlich Feuer glimmt noch auf dem Herde,", "tokens": ["Ein", "sp\u00e4r\u00b7lich", "Feu\u00b7er", "glimmt", "noch", "auf", "dem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der einzge schwache Schein \u2013 ein Licht", "tokens": ["Es", "ist", "der", "einz\u00b7ge", "schwa\u00b7che", "Schein", "\u2013", "ein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "w\u00e4r viel zu theuer. In dem engen Raum,", "tokens": ["w\u00e4r", "viel", "zu", "theu\u00b7er", ".", "In", "dem", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dess Decke du mit deinen H\u00e4nden greifst,", "tokens": ["dess", "De\u00b7cke", "du", "mit", "dei\u00b7nen", "H\u00e4n\u00b7den", "greifst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ist \u00fcbelriechende, verdorbne Luft.", "tokens": ["ist", "\u00fc\u00b7bel\u00b7rie\u00b7chen\u00b7de", ",", "ver\u00b7dorb\u00b7ne", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Man hat die Fenster nicht ge\u00f6ffnet, denn", "tokens": ["Man", "hat", "die", "Fens\u00b7ter", "nicht", "ge\u00b7\u00f6ff\u00b7net", ",", "denn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "die W\u00e4rme muss man halten, ach, die theure W\u00e4rme!", "tokens": ["die", "W\u00e4r\u00b7me", "muss", "man", "hal\u00b7ten", ",", "ach", ",", "die", "theu\u00b7re", "W\u00e4r\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVINF", "$,", "ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dort an der Wand, dem Herde gegen\u00fcber,", "tokens": ["Dort", "an", "der", "Wand", ",", "dem", "Her\u00b7de", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da steht ein schmutzges Bett, und h\u00e4sslich St\u00f6hnen", "tokens": ["da", "steht", "ein", "schmutz\u00b7ges", "Bett", ",", "und", "h\u00e4ss\u00b7lich", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kommt von dort her aus einer kranken Brust.", "tokens": ["kommt", "von", "dort", "her", "aus", "ei\u00b7ner", "kran\u00b7ken", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am blinden Fenster, das im Winde klappert,", "tokens": ["Am", "blin\u00b7den", "Fens\u00b7ter", ",", "das", "im", "Win\u00b7de", "klap\u00b7pert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "der draussen durch des Dorfes Gassen f\u00e4hrt,", "tokens": ["der", "draus\u00b7sen", "durch", "des", "Dor\u00b7fes", "Gas\u00b7sen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "sitzt eine Frau und strickt. Sie h\u00f6rt das St\u00f6hnen", "tokens": ["sitzt", "ei\u00b7ne", "Frau", "und", "strickt", ".", "Sie", "h\u00f6rt", "das", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "des Kranken nicht und sieht die Spiele nicht", "tokens": ["des", "Kran\u00b7ken", "nicht", "und", "sieht", "die", "Spie\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "KON", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "der Kinder, die zu ihren F\u00fcssen kauern \u2013", "tokens": ["der", "Kin\u00b7der", ",", "die", "zu", "ih\u00b7ren", "F\u00fcs\u00b7sen", "kau\u00b7ern", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "sie sitzt und strickt, das Haupt nach vorn geneigt ...", "tokens": ["sie", "sitzt", "und", "strickt", ",", "das", "Haupt", "nach", "vorn", "ge\u00b7neigt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich dr\u00f6hnt es auf dem rohen Pflaster:", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "dr\u00f6hnt", "es", "auf", "dem", "ro\u00b7hen", "Pflas\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "herangerollt in vornehm schnellem Trab", "tokens": ["her\u00b7an\u00b7ge\u00b7rollt", "in", "vor\u00b7nehm", "schnel\u00b7lem", "Trab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "kommt ein Coup\u00e9. Die gn\u00e4dge Frau steigt aus,", "tokens": ["kommt", "ein", "Coup\u00e9", ".", "Die", "gn\u00e4d\u00b7ge", "Frau", "steigt", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "der Diener reisst die Th\u00fcr der H\u00fctte auf,", "tokens": ["der", "Die\u00b7ner", "reisst", "die", "Th\u00fcr", "der", "H\u00fct\u00b7te", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "so dass ein scharfer Luftzug bis ans Bett", "tokens": ["so", "dass", "ein", "schar\u00b7fer", "Luft\u00b7zug", "bis", "ans", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "des Kranken f\u00e4hrt, und von der Schwelle tritt", "tokens": ["des", "Kran\u00b7ken", "f\u00e4hrt", ",", "und", "von", "der", "Schwel\u00b7le", "tritt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ein sch\u00f6nes, junges Weib, die Frau vom Schloss.", "tokens": ["ein", "sch\u00f6\u00b7nes", ",", "jun\u00b7ges", "Weib", ",", "die", "Frau", "vom", "Schloss", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit leisen Schritten kommt sie n\u00e4her, winkt", "tokens": ["Mit", "lei\u00b7sen", "Schrit\u00b7ten", "kommt", "sie", "n\u00e4\u00b7her", ",", "winkt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Frau, die sich erhob, und fl\u00fcsternd fragt sie:", "tokens": ["der", "Frau", ",", "die", "sich", "er\u00b7hob", ",", "und", "fl\u00fcs\u00b7ternd", "fragt", "sie", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie geht es ihm? \u2013 Sie weist zum Bett nach hinten.", "tokens": ["Wie", "geht", "es", "ihm", "?", "\u2013", "Sie", "weist", "zum", "Bett", "nach", "hin\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$.", "$(", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr offnes, freudenreiches Antlitz gl\u00e4nzt,", "tokens": ["Ihr", "off\u00b7nes", ",", "freu\u00b7den\u00b7rei\u00b7ches", "Ant\u00b7litz", "gl\u00e4nzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "verkl\u00e4rt vom Gotteshauche reinen Mitleids.", "tokens": ["ver\u00b7kl\u00e4rt", "vom", "Got\u00b7tes\u00b7hau\u00b7che", "rei\u00b7nen", "Mit\u00b7leids", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Ach, gn\u00e4dge Frau, das ist nu schon so lang!", "tokens": ["Ach", ",", "gn\u00e4d\u00b7ge", "Frau", ",", "das", "ist", "nu", "schon", "so", "lang", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PDS", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Statt dass er f\u00fcr die Kleinen und f\u00fcr mich", "tokens": ["Statt", "dass", "er", "f\u00fcr", "die", "Klei\u00b7nen", "und", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "APPR", "ART", "NN", "KON", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "arbeitet, muss er selbst erhalten werden,", "tokens": ["ar\u00b7bei\u00b7tet", ",", "muss", "er", "selbst", "er\u00b7hal\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und besser werden kann er doch nicht wieder,", "tokens": ["und", "bes\u00b7ser", "wer\u00b7den", "kann", "er", "doch", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wenn er nu man nicht so lange machte ...", "tokens": ["und", "wenn", "er", "nu", "man", "nicht", "so", "lan\u00b7ge", "mach\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "PTKNEG", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein rauhes Wimmern unterbricht das Weib,", "tokens": ["Ein", "rau\u00b7hes", "Wim\u00b7mern", "un\u00b7ter\u00b7bricht", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "der Kranke hat sich selbst emporgerichtet:", "tokens": ["der", "Kran\u00b7ke", "hat", "sich", "selbst", "em\u00b7por\u00b7ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lass gut sein, Mutter ... und die eine Hand", "tokens": ["Lass", "gut", "sein", ",", "Mut\u00b7ter", "...", "und", "die", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "VAINF", "$,", "NN", "$(", "KON", "ART", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "streckt er, wie um Erbarmen flehend aus.", "tokens": ["streckt", "er", ",", "wie", "um", "Er\u00b7bar\u00b7men", "fle\u00b7hend", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Noch einmal ruft er mit gebrochenem Hauch:", "tokens": ["Noch", "ein\u00b7mal", "ruft", "er", "mit", "ge\u00b7bro\u00b7che\u00b7nem", "Hauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Lass gut sein, Mutter ... und dann sinkt er hin \u2013", "tokens": ["Lass", "gut", "sein", ",", "Mut\u00b7ter", "...", "und", "dann", "sinkt", "er", "hin", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAINF", "$,", "NN", "$(", "KON", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und in das Haus der Armen trat der Tod.", "tokens": ["und", "in", "das", "Haus", "der", "Ar\u00b7men", "trat", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein sp\u00e4rlich Feuer glimmt noch auf dem Herde,", "tokens": ["Ein", "sp\u00e4r\u00b7lich", "Feu\u00b7er", "glimmt", "noch", "auf", "dem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist der einzge schwache Schein \u2013 ein Licht", "tokens": ["Es", "ist", "der", "einz\u00b7ge", "schwa\u00b7che", "Schein", "\u2013", "ein", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "w\u00e4r viel zu theuer. In dem engen Raum,", "tokens": ["w\u00e4r", "viel", "zu", "theu\u00b7er", ".", "In", "dem", "en\u00b7gen", "Raum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dess Decke du mit deinen H\u00e4nden greifst,", "tokens": ["dess", "De\u00b7cke", "du", "mit", "dei\u00b7nen", "H\u00e4n\u00b7den", "greifst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ist \u00fcbelriechende, verdorbne Luft.", "tokens": ["ist", "\u00fc\u00b7bel\u00b7rie\u00b7chen\u00b7de", ",", "ver\u00b7dorb\u00b7ne", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "$,", "ADJA", "