{"dta.poem.18421": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Duc de Luxemburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man saget/ da\u00df der geist der h\u00f6llen mit dir ist/", "tokens": ["Man", "sa\u00b7get", "/", "da\u00df", "der", "geist", "der", "h\u00f6l\u00b7len", "mit", "dir", "ist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wiewohl du vor dich selbst ein kleiner satan bist;", "tokens": ["Wie\u00b7wohl", "du", "vor", "dich", "selbst", "ein", "klei\u00b7ner", "sa\u00b7tan", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gelingt dirs nun einmal/ was wunder ist es dann?", "tokens": ["Ge\u00b7lingt", "dirs", "nun", "ein\u00b7mal", "/", "was", "wun\u00b7der", "ist", "es", "dann", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "$(", "PWS", "ADJA", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwey teuffel streiten ja bey dir f\u00fcr einen mann.", "tokens": ["Zwey", "teuf\u00b7fel", "strei\u00b7ten", "ja", "bey", "dir", "f\u00fcr", "ei\u00b7nen", "mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}