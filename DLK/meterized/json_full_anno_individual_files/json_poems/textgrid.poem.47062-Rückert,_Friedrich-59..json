{"textgrid.poem.47062": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, der du anklopfst gar nicht leise", "tokens": ["Wer", "bist", "du", ",", "der", "du", "an\u00b7klopfst", "gar", "nicht", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "ADV", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An meine Fenster mit dem Fl\u00fcgelschlage,", "tokens": ["An", "mei\u00b7ne", "Fens\u00b7ter", "mit", "dem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O ungest\u00fcmer Nachtdurchwandler, sage,", "tokens": ["O", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Nacht\u00b7durch\u00b7wand\u00b7ler", ",", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der du die Locken mir behauchst mit Eise?", "tokens": ["Der", "du", "die", "Lo\u00b7cken", "mir", "be\u00b7hauchst", "mit", "Ei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbein Nordwind bin ich und bin auf der Reise;", "tokens": ["\u00bb", "ein", "Nord\u00b7wind", "bin", "ich", "und", "bin", "auf", "der", "Rei\u00b7se", ";"], "token_info": ["punc