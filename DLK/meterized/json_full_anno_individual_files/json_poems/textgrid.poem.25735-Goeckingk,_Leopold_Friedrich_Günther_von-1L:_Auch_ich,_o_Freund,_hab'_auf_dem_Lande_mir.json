{"textgrid.poem.25735": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch ich, o Freund, hab' auf dem Lande mir", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich, o Freund, hab' auf dem Lande mir", "tokens": ["Auch", "ich", ",", "o", "Freund", ",", "hab'", "auf", "dem", "Lan\u00b7de", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinem letzten Deut ein kleines Ohnesorge", "tokens": ["Mit", "mei\u00b7nem", "letz\u00b7ten", "Deut", "ein", "klei\u00b7nes", "Oh\u00b7nes\u00b7or\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zurecht gebaut. Mein Leben gleichet hier", "tokens": ["Zu\u00b7recht", "ge\u00b7baut", ".", "Mein", "Le\u00b7ben", "glei\u00b7chet", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So ziemlich meiner Nachbarin, der ", "tokens": ["So", "ziem\u00b7lich", "mei\u00b7ner", "Nach\u00b7ba\u00b7rin", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar kennt man ihren Namen schon nicht mehr,", "tokens": ["Zwar", "kennt", "man", "ih\u00b7ren", "Na\u00b7men", "schon", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Betritt ein Wanderer nur Schwarzburgs nahe Grenzen,", "tokens": ["Be\u00b7tritt", "ein", "Wan\u00b7de\u00b7rer", "nur", "Schwarz\u00b7burgs", "na\u00b7he", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar dr\u00fcckt kein Schiff mit Kaufmannsgut sie schwer,", "tokens": ["Zwar", "dr\u00fcckt", "kein", "Schiff", "mit", "Kauf\u00b7manns\u00b7gut", "sie", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zwar sieht man kaum im Mondenschein' sie gl\u00e4nzen.", "tokens": ["Zwar", "sieht", "man", "kaum", "im", "Mon\u00b7den\u00b7schein'", "sie", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auch geht nur der an ihren Kieselstrand,", "tokens": ["Auch", "geht", "nur", "der", "an", "ih\u00b7ren", "Kie\u00b7sel\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wer sanftes Rauschen liebt von felsenvoller Welle,", "tokens": ["Wer", "sanf\u00b7tes", "Rau\u00b7schen", "liebt", "von", "fel\u00b7sen\u00b7vol\u00b7ler", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Freud' am Spiel' mit ihren Kindern fand,", "tokens": ["Und", "Freud'", "am", "Spiel'", "mit", "ih\u00b7ren", "Kin\u00b7dern", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dem tr\u00e4gen Krebs, der lustigen Forelle.", "tokens": ["Dem", "tr\u00e4\u00b7gen", "Krebs", ",", "der", "lus\u00b7ti\u00b7gen", "Fo\u00b7rel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch wuchs durch sie schon manche Weide gro\u00df,", "tokens": ["Doch", "wuchs", "durch", "sie", "schon", "man\u00b7che", "Wei\u00b7de", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und gab dem armen Lamm' im Pelze, k\u00fchlen Schatten;", "tokens": ["Und", "gab", "dem", "ar\u00b7men", "Lamm'", "im", "Pel\u00b7ze", ",", "k\u00fch\u00b7len", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und Wiesen trugen Klee durch sie, die Moos", "tokens": ["Und", "Wie\u00b7sen", "tru\u00b7gen", "Klee", "durch", "sie", ",", "die", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "NN", "APPR", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und Disteln sonst und Dorn getragen hatten.", "tokens": ["Und", "Dis\u00b7teln", "sonst", "und", "Dorn", "ge\u00b7tra\u00b7gen", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So wei\u00df von mir kein Wort die n\u00e4chste Stadt,", "tokens": ["So", "wei\u00df", "von", "mir", "kein", "Wort", "die", "n\u00e4chs\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PIAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "So dr\u00fcckt nicht Lein und Korn und Wolle meine Speicher,", "tokens": ["So", "dr\u00fcckt", "nicht", "Lein", "und", "Korn", "und", "Wol\u00b7le", "mei\u00b7ne", "Spei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "NN", "KON", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und die zu Bettlern viel' gestickt schon hat,", "tokens": ["Und", "die", "zu", "Bett\u00b7lern", "viel'", "ge\u00b7stickt", "schon", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADV", "VVPP", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Doch mancher J\u00fcngling scheut durch mich den Schlag", "tokens": ["Doch", "man\u00b7cher", "J\u00fcng\u00b7ling", "scheut", "durch", "mich", "den", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Des St\u00e4rkern nicht, mit ihm f\u00fcr Unschuld Kampf zu wagen;", "tokens": ["Des", "St\u00e4r\u00b7kern", "nicht", ",", "mit", "ihm", "f\u00fcr", "Un\u00b7schuld", "Kampf", "zu", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "APPR", "PPER", "APPR", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und liegt, durch mich, nicht da, wo ", "tokens": ["Und", "liegt", ",", "durch", "mich", ",", "nicht", "da", ",", "wo"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPER", "$,", "PTKNEG", "ADV", "$,", "PWAV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Itzt ", "tokens": ["Itzt"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "O fl\u00f6sse doch mein Leben ganz die Stadt,", "tokens": ["O", "fl\u00f6s\u00b7se", "doch", "mein", "Le\u00b7ben", "ganz", "die", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Bis alles Irren hier ein Ende hat,", "tokens": ["Bis", "al\u00b7les", "Ir\u00b7ren", "hier", "ein", "En\u00b7de", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Von den Socraten gl\u00fccklich nur gepriesen!", "tokens": ["Von", "den", "So\u00b7cra\u00b7ten", "gl\u00fcck\u00b7lich", "nur", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Dann \u2013 \u2013 Aber halt! du steigst zu hoch empor!", "tokens": ["Dann", "\u2013", "\u2013", "A\u00b7ber", "halt", "!", "du", "steigst", "zu", "hoch", "em\u00b7por", "!"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "$(", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Zur\u00fcck du k\u00fchner Wunsch! du darfst noch nicht entfliegen.", "tokens": ["Zu\u00b7r\u00fcck", "du", "k\u00fch\u00b7ner", "Wunsch", "!", "du", "darfst", "noch", "nicht", "ent\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$.", "PPER", "PAV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn w\u00e4re nicht selbst Socrates ein Thor,", "tokens": ["Denn", "w\u00e4\u00b7re", "nicht", "selbst", "So\u00b7cra\u00b7tes", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "NE", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Da\u00df er nicht lebte, blo\u00df f\u00fcr sein Vergn\u00fcgen?", "tokens": ["Da\u00df", "er", "nicht", "leb\u00b7te", ",", "blo\u00df", "f\u00fcr", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Athen mit Achselzucken nicht verlie\u00df,", "tokens": ["A\u00b7then", "mit", "Ach\u00b7sel\u00b7zu\u00b7cken", "nicht", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.33": {"text": "Wo ihn die Schlangen der Verl\u00e4umdung rund umzischten?", "tokens": ["Wo", "ihn", "die", "Schlan\u00b7gen", "der", "Ver\u00b7l\u00e4um\u00b7dung", "rund", "um\u00b7zischten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den S\u00f6hnen noch den Werth der Tugend prie\u00df,", "tokens": ["Den", "S\u00f6h\u00b7nen", "noch", "den", "Werth", "der", "Tu\u00b7gend", "prie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Inde\u00df schon Gift die V\u00e4ter f\u00fcr ihn mischten?", "tokens": ["In\u00b7de\u00df", "schon", "Gift", "die", "V\u00e4\u00b7ter", "f\u00fcr", "ihn", "mischten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Wohlan! Auch ich will unter Menschen alt,", "tokens": ["Wo\u00b7hlan", "!", "Auch", "ich", "will", "un\u00b7ter", "Men\u00b7schen", "alt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "PPER", "VMFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Im Kleinen, Socrates, und grau mit Ehren werden.", "tokens": ["Im", "Klei\u00b7nen", ",", "So\u00b7cra\u00b7tes", ",", "und", "grau", "mit", "Eh\u00b7ren", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NE", "$,", "KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "Dann aber hin, wo die Schalmey erschallt!", "tokens": ["Dann", "a\u00b7ber", "hin", ",", "wo", "die", "Schal\u00b7mey", "er\u00b7schallt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Hinaus zu den friedsamen L\u00e4mmerherden!", "tokens": ["Hin\u00b7aus", "zu", "den", "fried\u00b7sa\u00b7men", "L\u00e4m\u00b7mer\u00b7her\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Auch ich, o Freund, hab' auf dem Lande mir", "tokens": ["Auch", "ich", ",", "o", "Freund", ",", "hab'", "auf", "dem", "Lan\u00b7de", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meinem letzten Deut ein kleines Ohnesorge", "tokens": ["Mit", "mei\u00b7nem", "letz\u00b7ten", "Deut", "ein", "klei\u00b7nes", "Oh\u00b7nes\u00b7or\u00b7ge"], "token_info": ["word", "word", "word", "word", "w