{"textgrid.poem.39088": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vernehmt, ihr Leute fern und nah,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernehmt, ihr Leute fern und nah,", "tokens": ["Ver\u00b7nehmt", ",", "ihr", "Leu\u00b7te", "fern", "und", "nah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die grausige Historia,", "tokens": ["die", "grau\u00b7si\u00b7ge", "His\u00b7to\u00b7ria", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "die j\u00fcngstens sich begeben hat", "tokens": ["die", "j\u00fcng\u00b7stens", "sich", "be\u00b7ge\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PRF", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zu D\u00fcsseldorf der guten Stadt;", "tokens": ["zu", "D\u00fcs\u00b7sel\u00b7dorf", "der", "gu\u00b7ten", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "vernehmt es all und merkt euch wohl:", "tokens": ["ver\u00b7nehmt", "es", "all", "und", "merkt", "euch", "wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Narrheit ist jetzt Monopol.", "tokens": ["Die", "Nar\u00b7rheit", "ist", "jetzt", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu D\u00fcsseldorf, bei n\u00e4cht'ger Zeit,", "tokens": ["Zu", "D\u00fcs\u00b7sel\u00b7dorf", ",", "bei", "n\u00e4cht'\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "h\u00f6rt' man ein L\u00e4uten weit und breit,", "tokens": ["h\u00f6rt'", "man", "ein", "L\u00e4u\u00b7ten", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "von tausend Glocken, hell und fein,", "tokens": ["von", "tau\u00b7send", "Glo\u00b7cken", ",", "hell", "und", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "von tausend Gl\u00e4sern, voll und rein \u2013", "tokens": ["von", "tau\u00b7send", "Gl\u00e4\u00b7sern", ",", "voll", "und", "rein", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir h\u00f6rten's noch, ich denke wohl,", "tokens": ["Wir", "h\u00f6r\u00b7ten's", "noch", ",", "ich", "den\u00b7ke", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "w\u00e4r' nicht die Narrheit Monopol.", "tokens": ["w\u00e4r'", "nicht", "die", "Nar\u00b7rheit", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es klang herauf aus Kellers Grund,", "tokens": ["Es", "klang", "her\u00b7auf", "aus", "Kel\u00b7lers", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie Festgesang von Zechermund,", "tokens": ["wie", "Fest\u00b7ge\u00b7sang", "von", "Ze\u00b7cher\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "klang durch die L\u00fcfte keck und frei,", "tokens": ["klang", "durch", "die", "L\u00fcf\u00b7te", "keck", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wie Mummenschanz und Schelmerei \u2013", "tokens": ["wie", "Mum\u00b7men\u00b7schanz", "und", "Schel\u00b7me\u00b7rei", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es kl\u00e4nge noch, ich denke wohl,", "tokens": ["Es", "kl\u00e4n\u00b7ge", "noch", ",", "ich", "den\u00b7ke", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "w\u00e4r' nicht die Narrheit Monopol.", "tokens": ["w\u00e4r'", "nicht", "die", "Nar\u00b7rheit", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und aus der Tiefe wunderbar,", "tokens": ["Und", "aus", "der", "Tie\u00b7fe", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie Freiheitsatem hell und klar,", "tokens": ["wie", "Frei\u00b7heit\u00b7sa\u00b7tem", "hell", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hinauf, hinab den gr\u00fcnen Strand,", "tokens": ["hin\u00b7auf", ",", "hin\u00b7ab", "den", "gr\u00fc\u00b7nen", "Strand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die goldne Freude flog durchs Land \u2013", "tokens": ["die", "gold\u00b7ne", "Freu\u00b7de", "flog", "durchs", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war vordem, versteht mich wohl,", "tokens": ["Das", "war", "vor\u00b7dem", ",", "ver\u00b7steht", "mich", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "eh' noch die Freiheit Monopol.", "tokens": ["eh'", "noch", "die", "Frei\u00b7heit", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das h\u00f6rt der Pascha in Berlin,", "tokens": ["Das", "h\u00f6rt", "der", "Pa\u00b7scha", "in", "Ber\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "und wie er's h\u00f6rt, verdrie\u00dft es ihn:", "tokens": ["und", "wie", "er's", "h\u00f6rt", ",", "ver\u00b7drie\u00dft", "es", "ihn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbauf, auf, Gensd'arm, und wehr dem Ding,", "tokens": ["\u00bb", "auf", ",", "auf", ",", "Gens\u00b7d'\u00b7arm", ",", "und", "wehr", "dem", "Ding", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "NN", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "wozu w\u00e4r' ich der Bodelschwingh?!\u00ab", "tokens": ["wo\u00b7zu", "w\u00e4r'", "ich", "der", "Bo\u00b7del\u00b7schwingh", "?!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er dacht', und daran dacht' er wohl,", "tokens": ["Er", "dacht'", ",", "und", "da\u00b7ran", "dacht'", "er", "wohl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Narrheit w\u00e4r' sein Monopol.", "tokens": ["die", "Nar\u00b7rheit", "w\u00e4r'", "sein", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und alsofort am n\u00e4chsten Tag", "tokens": ["Und", "al\u00b7so\u00b7fort", "am", "n\u00e4chs\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "verk\u00fcndigt ward beim Trommelschlag,", "tokens": ["ver\u00b7k\u00fcn\u00b7digt", "ward", "beim", "Trom\u00b7mel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "da\u00df niemand sich darf unterstehn,", "tokens": ["da\u00df", "nie\u00b7mand", "sich", "darf", "un\u00b7ter\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in Narrenkappen mehr zu gehn:", "tokens": ["in", "Nar\u00b7ren\u00b7kap\u00b7pen", "mehr", "zu", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr guten B\u00fcrger, merkt euch wohl,", "tokens": ["Ihr", "gu\u00b7ten", "B\u00fcr\u00b7ger", ",", "merkt", "euch", "wohl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Narrheit ist jetzt Monopol.", "tokens": ["die", "Nar\u00b7rheit", "ist", "jetzt", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So sitzen wir nun traurig all", "tokens": ["So", "sit\u00b7zen", "wir", "nun", "trau\u00b7rig", "all"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und haben keinen Karneval,", "tokens": ["und", "ha\u00b7ben", "kei\u00b7nen", "Kar\u00b7ne\u00b7val", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wir sitzen all und weinen sehr", "tokens": ["wir", "sit\u00b7zen", "all", "und", "wei\u00b7nen", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und trinken aber desto mehr \u2013", "tokens": ["und", "trin\u00b7ken", "a\u00b7ber", "des\u00b7to", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O da\u00df sie all der Teufel hol',", "tokens": ["O", "da\u00df", "sie", "all", "der", "Teu\u00b7fel", "hol'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PIAT", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Narren samt ihrem Monopol.", "tokens": ["die", "Nar\u00b7ren", "samt", "ih\u00b7rem", "Mo\u00b7no\u00b7pol", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text":