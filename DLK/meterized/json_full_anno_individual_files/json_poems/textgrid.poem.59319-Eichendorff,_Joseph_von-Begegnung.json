{"textgrid.poem.59319": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Begegnung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandert in der Fr\u00fchlingszeit,", "tokens": ["Ich", "wan\u00b7dert", "in", "der", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern auf den Bergen gingen", "tokens": ["Fern", "auf", "den", "Ber\u00b7gen", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Geigenspiel und Singen", "tokens": ["Mit", "Gei\u00b7gen\u00b7spiel", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel lust'ge Hochzeitsleut,", "tokens": ["Viel", "lust'\u00b7ge", "Hoch\u00b7zeits\u00b7leut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das war ein Jauchzen und Klingen!", "tokens": ["Das", "war", "ein", "Jauch\u00b7zen", "und", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Es bl\u00fchte rings in Tal und H\u00f6hn,", "tokens": ["Es", "bl\u00fch\u00b7te", "rings", "in", "Tal", "und", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich konnt vor Lust nicht weitergehn.", "tokens": ["Ich", "konnt", "vor", "Lust", "nicht", "wei\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Dorfe dann auf gr\u00fcner Au", "tokens": ["Am", "Dor\u00b7fe", "dann", "auf", "gr\u00fc\u00b7ner", "Au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begannen sie den Reigen,", "tokens": ["Be\u00b7gan\u00b7nen", "sie", "den", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und durch den Schall der Geigen", "tokens": ["Und", "durch", "den", "Schall", "der", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lacht' laut die junge Frau,", "tokens": ["Lacht'", "laut", "die", "jun\u00b7ge", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Stimmlein klang so eigen,", "tokens": ["Ihr", "Stimm\u00b7lein", "klang", "so", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wu\u00dfte nicht, wie mir geschehn \u2013", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", ",", "wie", "mir", "ge\u00b7schehn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da wandt sie sich in wildem Drehn.", "tokens": ["Da", "wandt", "sie", "sich", "in", "wil\u00b7dem", "Drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es war mein Lieb! 's ist lange her,", "tokens": ["Es", "war", "mein", "Lieb", "!", "'s", "ist", "lan\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie blickt' so ohne Scheue,", "tokens": ["Sie", "blickt'", "so", "oh\u00b7ne", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verloren ist die Treue,", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie kannte mich nicht mehr \u2013", "tokens": ["Sie", "kann\u00b7te", "mich", "nicht", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da jauchzt' und geigt's aufs neue,", "tokens": ["Da", "jauchzt'", "und", "geigt's", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NE", "APPRART", "ADJA", "$,