{"textgrid.poem.43351": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XvIII. An die Geliebte.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Holde, die mein Sehnen", "tokens": ["Du", "Hol\u00b7de", ",", "die", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von fern erregt mit tiefverh\u00fcllten Z\u00fcgen,", "tokens": ["Von", "fern", "er\u00b7regt", "mit", "tief\u00b7ver\u00b7h\u00fcll\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich l\u00e4\u00dft im Traum nur w\u00e4hnen,", "tokens": ["Mich", "l\u00e4\u00dft", "im", "Traum", "nur", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr himmlisch Bild zu schauen,", "tokens": ["Ihr", "himm\u00b7lisch", "Bild", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn am sch\u00f6nen Tag", "tokens": ["Und", "wenn", "am", "sch\u00f6\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In Wonne lachend die Gefilde liegen:", "tokens": ["In", "Won\u00b7ne", "la\u00b7chend", "die", "Ge\u00b7fil\u00b7de", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sag, lebtest du dein Leben", "tokens": ["Sag", ",", "leb\u00b7test", "du", "dein", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schon in der goldnen Zeit, der unschuldsvollen,", "tokens": ["Schon", "in", "der", "gold\u00b7nen", "Zeit", ",", "der", "un\u00b7schulds\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Um heut uns zu umschweben", "tokens": ["Um", "heut", "uns", "zu", "um\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Als Schatten? Oder hat ein neidisch Walten", "tokens": ["Als", "Schat\u00b7ten", "?", "O\u00b7der", "hat", "ein", "nei\u00b7disch", "Wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$.", "NE", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des Schicksals dich der Zukunft vorbehalten?", "tokens": ["Des", "Schick\u00b7sals", "dich", "der", "Zu\u00b7kunft", "vor\u00b7be\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Hoffnung ist geschwunden,", "tokens": ["Die", "Hoff\u00b7nung", "ist", "ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich je zu schau'n im Leben;", "tokens": ["Dich", "je", "zu", "schau'n", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erst dann vielleicht, wenn h\u00fcllenlos mein Geist", "tokens": ["Erst", "dann", "viel\u00b7leicht", ",", "wenn", "h\u00fcl\u00b7len\u00b7los", "mein", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach fremden St\u00e4tten einsam wird entschweben", "tokens": ["Nach", "frem\u00b7den", "St\u00e4t\u00b7ten", "ein\u00b7sam", "wird", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf neuem Pfad. Schon einst im Morgengrauen", "tokens": ["Auf", "neu\u00b7em", "Pfad", ".", "Schon", "einst", "im", "Mor\u00b7gen\u00b7grau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Erdentags mit ungewissem Scheine", "tokens": ["Des", "Er\u00b7den\u00b7tags", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Glaubt' ich, auf dieser rauhen Erde sei'st", "tokens": ["Glaubt'", "ich", ",", "auf", "die\u00b7ser", "rau\u00b7hen", "Er\u00b7de", "sei'st"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PDAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Auch du bestimmt zur Pilgerschaft. Doch fand ich", "tokens": ["Auch", "du", "be\u00b7stimmt", "zur", "Pil\u00b7ger\u00b7schaft", ".", "Doch", "fand", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPRART", "NN", "$.", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nichts Irdisches dir \u00e4hnlich. Wenn auch Eine", "tokens": ["Nichts", "Ir\u00b7di\u00b7sches", "dir", "\u00e4hn\u00b7lich", ".", "Wenn", "auch", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADJA", "PPER", "ADJD", "$.", "KOUS", "ADV", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dir glich' an Z\u00fcgen, an Geberd' und Rede, \u2013", "tokens": ["Dir", "glich'", "an", "Z\u00fc\u00b7gen", ",", "an", "Ge\u00b7berd'", "und", "Re\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "An Reiz und Anmuth \u00fcbertr\u00e4fst du Jede.", "tokens": ["An", "Reiz", "und", "An\u00b7muth", "\u00fc\u00b7bert\u00b7r\u00e4fst", "du", "Je\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn unter all den Leiden,", "tokens": ["Wenn", "un\u00b7ter", "all", "den", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Sterblichen verh\u00e4ngt sind vom Geschick,", "tokens": ["Die", "Sterb\u00b7li\u00b7chen", "ver\u00b7h\u00e4ngt", "sind", "vom", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leibhaft und so wie dich mein Geist getr\u00e4umt", "tokens": ["Leib\u00b7haft", "und", "so", "wie", "dich", "mein", "Geist", "ge\u00b7tr\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "KOKOM", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dich Einer liebt' auf Erden, \u2013 dieses Leben", "tokens": ["Dich", "Ei\u00b7ner", "liebt'", "auf", "Er\u00b7den", ",", "\u2013", "die\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "APPR", "NN", "$,", "$(", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "W\u00e4r' ihm ein sel'ges Gl\u00fcck;", "tokens": ["W\u00e4r'", "ihm", "ein", "sel'\u00b7ges", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich f\u00fchl' es tief: nach Ruhm und Tugend streben", "tokens": ["Ich", "f\u00fchl'", "es", "tief", ":", "nach", "Ruhm", "und", "Tu\u00b7gend", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "W\u00fcrd' ich aufs Neue, wie in junger Zeit,", "tokens": ["W\u00fcrd'", "ich", "aufs", "Neu\u00b7e", ",", "wie", "in", "jun\u00b7ger", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um deiner Liebe willen. Jetzt gew\u00e4hrt", "tokens": ["Um", "dei\u00b7ner", "Lie\u00b7be", "wil\u00b7len", ".", "Jetzt", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$.", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Himmel keine Lindrung meinem Leid.", "tokens": ["Der", "Him\u00b7mel", "kei\u00b7ne", "Lind\u00b7rung", "mei\u00b7nem", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit dir vereinigt w\u00e4re schon hienieden", "tokens": ["Mit", "dir", "ver\u00b7ei\u00b7nigt", "w\u00e4\u00b7re", "schon", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein g\u00f6ttergleiches Dasein mir beschieden.", "tokens": ["Ein", "g\u00f6t\u00b7ter\u00b7glei\u00b7ches", "Da\u00b7sein", "mir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In Th\u00e4lern, wo das Lied", "tokens": ["In", "Th\u00e4\u00b7lern", ",", "wo", "das", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Des flei\u00df'gen Landmanns hinterm Pflug ert\u00f6nt,", "tokens": ["Des", "flei\u00df'\u00b7gen", "Land\u00b7manns", "hin\u00b7term", "Pflug", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sitz' ich versenkt in Sehnen", "tokens": ["Sitz'", "ich", "ver\u00b7senkt", "in", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach meinem Jugendtraum, der nun entflieht.", "tokens": ["Nach", "mei\u00b7nem", "Ju\u00b7gend\u00b7traum", ",", "der", "nun", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und flie\u00dfen auf den H\u00fcgeln meine Thr\u00e4nen,", "tokens": ["Und", "flie\u00b7\u00dfen", "auf", "den", "H\u00fc\u00b7geln", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil meinen Tagen jede Sehnsucht, jede", "tokens": ["Weil", "mei\u00b7nen", "Ta\u00b7gen", "je\u00b7de", "Sehn\u00b7sucht", ",", "je\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Hoffnung entschwand, \u2013 auf einmal, denk' ich dein,", "tokens": ["Hoff\u00b7nung", "ent\u00b7schwand", ",", "\u2013", "auf", "ein\u00b7mal", ",", "denk'", "ich", "dein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "$(", "APPR", "ADV", "$,", "VVFIN", "PPER", "PPOSAT", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Pocht neuerweckt mein Herz. O k\u00f6nnt' ich nur", "tokens": ["Pocht", "neu\u00b7er\u00b7weckt", "mein", "Herz", ".", "O", "k\u00f6nnt'", "ich", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "NE", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "In dieser d\u00fcstern Zeit voll Schmach und Pein", "tokens": ["In", "die\u00b7ser", "d\u00fcs\u00b7tern", "Zeit", "voll", "Schmach", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dein hohes Bild bewahren, das so mild,", "tokens": ["Dein", "ho\u00b7hes", "Bild", "be\u00b7wah\u00b7ren", ",", "das", "so", "mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Obwohl ihm Leben fehlt, die Seele stillt!", "tokens": ["Ob\u00b7wohl", "ihm", "Le\u00b7ben", "fehlt", ",", "die", "See\u00b7le", "stillt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Bist du vielleicht der ew'gen", "tokens": ["Bist", "du", "viel\u00b7leicht", "der", "ew'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ideen eine, der die ew'ge Weisheit", "tokens": ["I\u00b7deen", "ei\u00b7ne", ",", "der", "die", "ew'\u00b7ge", "Weis\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ein sinnliches Gewand nicht wollte geben,", "tokens": ["Ein", "sinn\u00b7li\u00b7ches", "Ge\u00b7wand", "nicht", "woll\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Nicht sie in schwacher H\u00fclle", "tokens": ["Nicht", "sie", "in", "schwa\u00b7cher", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Versto\u00dfen in dies todgeweihte Leben?", "tokens": ["Ver\u00b7sto\u00b7\u00dfen", "in", "dies", "tod\u00b7ge\u00b7weih\u00b7te", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie, oder ward zum Wohnort dir ersehen", "tokens": ["Wie", ",", "o\u00b7der", "ward", "zum", "Wohn\u00b7ort", "dir", "er\u00b7se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "VAFIN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein neu Gestirn aus aller Welten F\u00fclle,", "tokens": ["Ein", "neu", "Ge\u00b7stirn", "aus", "al\u00b7ler", "Wel\u00b7ten", "F\u00fcl\u00b7le", ","], "token_info": ["word