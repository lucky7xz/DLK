{"dta.poem.18832": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Niedergangen ist die Sonne,", "tokens": ["Nie\u00b7der\u00b7gan\u00b7gen", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch im Westen gl\u00e4nzt es immer,", "tokens": ["Doch", "im", "Wes\u00b7ten", "gl\u00e4nzt", "es", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.3": {"text": "Wissen m\u00f6cht\u2019 ich wohl, wie lange", "tokens": ["Wis\u00b7sen", "m\u00f6cht'", "ich", "wohl", ",", "wie", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "PWAV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dauert noch der goldne Schimmer?", "tokens": ["Dau\u00b7ert", "noch", "der", "gold\u00b7ne", "Schim\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst du, Herr, so will ich bleiben,", "tokens": ["Willst", "du", ",", "Herr", ",", "so", "will", "ich", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warten ausser diesen Zelten,", "tokens": ["War\u00b7ten", "aus\u00b7ser", "die\u00b7sen", "Zel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Nacht des Schimmers Herrinn,", "tokens": ["Ist", "die", "Nacht", "des", "Schim\u00b7mers", "Her\u00b7rinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm\u2019 ich gleich es dir zu melden.", "tokens": ["Komm'", "ich", "gleich", "es", "dir", "zu", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ich weiss du liebst das Droben,", "tokens": ["Denn", "ich", "weiss", "du", "liebst", "das", "Dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Unendliche zu schauen,", "tokens": ["Das", "Un\u00b7end\u00b7li\u00b7che", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wenn sie sich einander loben", "tokens": ["Wenn", "sie", "sich", "ein\u00b7an\u00b7der", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Feuer in dem Blauen.", "tokens": ["Je\u00b7ne", "Feu\u00b7er", "in", "dem", "Blau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und das hellste will nur sagen:", "tokens": ["Und", "das", "hells\u00b7te", "will", "nur", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzo gl\u00e4nz\u2019 ich meiner Stelle,", "tokens": ["Jet\u00b7zo", "gl\u00e4nz'", "ich", "mei\u00b7ner", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollte Gott euch mehr betagen,", "tokens": ["Woll\u00b7te", "Gott", "euch", "mehr", "be\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nztet ihr wie ich so helle.", "tokens": ["Gl\u00e4nz\u00b7tet", "ihr", "wie", "ich", "so", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn vor Gott ist alles herrlich,", "tokens": ["Denn", "vor", "Gott", "ist", "al\u00b7les", "herr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eben weil er ist der beste,", "tokens": ["E\u00b7ben", "weil", "er", "ist", "der", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so schl\u00e4ft nun aller Vogel", "tokens": ["Und", "so", "schl\u00e4ft", "nun", "al\u00b7ler", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem gross und kleinen Neste.", "tokens": ["In", "dem", "gross", "und", "klei\u00b7nen", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einer sitzt auch wohl gest\u00e4ngelt", "tokens": ["Ei\u00b7ner", "sitzt", "auch", "wohl", "ge\u00b7st\u00e4n\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den Aesten der Cypresse,", "tokens": ["Auf", "den", "A\u00b7es\u00b7ten", "der", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wo der laue Wind ihn g\u00e4ngelt", "tokens": ["Wo", "der", "lau\u00b7e", "Wind", "ihn", "g\u00e4n\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zu Thaues luft\u2019ger N\u00e4sse.", "tokens": ["Bis", "zu", "Thau\u00b7es", "luft'\u00b7ger", "N\u00e4s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Solches hast du mich gelehret,", "tokens": ["Sol\u00b7ches", "hast", "du", "mich", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder etwas auch dergleichen,", "tokens": ["O\u00b7der", "et\u00b7was", "auch", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich je dir abgeh\u00f6ret", "tokens": ["Was", "ich", "je", "dir", "ab\u00b7ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dem Herzen nicht entweichen.", "tokens": ["Wird", "dem", "Her\u00b7zen", "nicht", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Eule will ich, deinetwegen,", "tokens": ["Eu\u00b7le", "will", "ich", ",", "dei\u00b7net\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kauzen hier auf der Terrasse,", "tokens": ["Kau\u00b7zen", "hier", "auf", "der", "Ter\u00b7ras\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Bis ich erst des Nordgestirnes", "tokens": ["Bis", "ich", "erst", "des", "Nord\u00b7ges\u00b7tir\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwillings-Wendung wohl erpasse.", "tokens": ["Zwil\u00b7lings\u00b7Wen\u00b7dung", "wohl", "er\u00b7pas\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und da wird es Mitternacht seyn,", "tokens": ["Und", "da", "wird", "es", "Mit\u00b7ter\u00b7nacht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "VAINF", "$,"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.2": {"text": "Wo du oft zu fr\u00fch ermunterst,", "tokens": ["Wo", "du", "oft", "zu", "fr\u00fch", "er\u00b7mun\u00b7terst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann wird es eine Pracht seyn,", "tokens": ["Und", "dann", "wird", "es", "ei\u00b7ne", "Pracht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn das All mit mir bewunderst.", "tokens": ["Wenn", "das", "All", "mit", "mir", "be\u00b7wun\u00b7derst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zwar in diesem Duft und Garten", "tokens": ["Zwar", "in", "die\u00b7sem", "Duft", "und", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6net Bulbul ganze N\u00e4chte,", "tokens": ["T\u00f6\u00b7net", "Bul\u00b7bul", "gan\u00b7ze", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch du k\u00f6nntest lange warten", "tokens": ["Doch", "du", "k\u00f6nn\u00b7test", "lan\u00b7ge", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis die Nacht so viel verm\u00f6chte.", "tokens": ["Bis", "die", "Nacht", "so", "viel", "ver\u00b7m\u00f6ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Denn in dieser Zeit der Flora,", "tokens": ["Denn", "in", "die\u00b7ser", "Zeit", "der", "Flo\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Griechen-Volk sie nennet,", "tokens": ["Wie", "das", "Grie\u00b7chen\u00b7Volk", "sie", "nen\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Strohwittwe, die Aurora", "tokens": ["Die", "Stroh\u00b7witt\u00b7we", ",", "die", "Au\u00b7ro\u00b7ra"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist in Hesperus entbrennet.", "tokens": ["Ist", "in", "Hes\u00b7pe\u00b7rus", "ent\u00b7bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Sieh dich um! sie kommt! wie schnelle!", "tokens": ["Sieh", "dich", "um", "!", "sie", "kommt", "!", "wie", "schnel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$.", "PWAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber Blumenfelds Gel\u00e4nge! \u2014", "tokens": ["Ue\u00b7ber", "Blu\u00b7men\u00b7felds", "Ge\u00b7l\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcben hell und dr\u00fcben helle,", "tokens": ["H\u00fc\u00b7ben", "hell", "und", "dr\u00fc\u00b7ben", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja die Nacht kommt ins Gedr\u00e4nge.", "tokens": ["Ja", "die", "Nacht", "kommt", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und auf rothen leichten Solen", "tokens": ["Und", "auf", "ro\u00b7then", "leich\u00b7ten", "So\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn, der mit der Sonn\u2019 entlaufen,", "tokens": ["Ihn", ",", "der", "mit", "der", "Sonn'", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eilt sie irrig einzohohlen;", "tokens": ["Eilt", "sie", "ir\u00b7rig", "ein\u00b7zo\u00b7hoh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlst du nicht ein Liebe-Schnaufen?", "tokens": ["F\u00fchlst", "du", "nicht", "ein", "Lie\u00b7be\u00b7Schnau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Geh nur, lieblichster der S\u00f6hne,", "tokens": ["Geh", "nur", ",", "lieb\u00b7lichs\u00b7ter", "der", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief in\u2019s Innre schliess die Th\u00fcren;", "tokens": ["Tief", "in's", "Inn\u00b7re", "schliess", "die", "Th\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie m\u00f6chte deine Sch\u00f6ne", "tokens": ["Denn", "sie", "m\u00f6ch\u00b7te", "dei\u00b7ne", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als den Hesperus entf\u00fchren.", "tokens": ["Als", "den", "Hes\u00b7pe\u00b7rus", "ent\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}