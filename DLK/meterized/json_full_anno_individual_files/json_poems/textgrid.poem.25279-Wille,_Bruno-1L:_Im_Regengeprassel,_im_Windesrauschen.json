{"textgrid.poem.25279": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Regengeprassel, im Windesrauschen/", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Regengeprassel, im Windesrauschen/", "tokens": ["Im", "Re\u00b7gen\u00b7ge\u00b7pras\u00b7sel", ",", "im", "Win\u00b7des\u00b7rau\u00b7schen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber/", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Immer dem Einen nur mu\u00df ich lauschen:", "tokens": ["Im\u00b7mer", "dem", "Ei\u00b7nen", "nur", "mu\u00df", "ich", "lau\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Wie d\u00fcstere Pilger die Wolken ziehn", "tokens": ["Wie", "d\u00fcs\u00b7te\u00b7re", "Pil\u00b7ger", "die", "Wol\u00b7ken", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wirbelnd des Waldbachs Wellen fliehn", "tokens": ["Wir\u00b7belnd", "des", "Wald\u00b7bachs", "Wel\u00b7len", "fliehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "VVINF"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Aus kahlen Wipfeln h\u00f6r ich es st\u00f6hnen:", "tokens": ["Aus", "kah\u00b7len", "Wip\u00b7feln", "h\u00f6r", "ich", "es", "st\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schaurig ein Echo im Herzen h\u00f6hnen:", "tokens": ["Schau\u00b7rig", "ein", "E\u00b7cho", "im", "Her\u00b7zen", "h\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Da hab ich gehastet, hoffend geharrt;", "tokens": ["Da", "hab", "ich", "ge\u00b7has\u00b7tet", ",", "hof\u00b7fend", "ge\u00b7harrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Fiebertraum hat mich gehetzt und genarrt;", "tokens": ["Fie\u00b7ber\u00b7traum", "hat", "mich", "ge\u00b7hetzt", "und", "ge\u00b7narrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Wie Wasserwirbel mein Leben zerstieben;", "tokens": ["Wie", "Was\u00b7ser\u00b7wir\u00b7bel", "mein", "Le\u00b7ben", "zer\u00b7stie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Treu ist mir nur das Eine geblieben:", "tokens": ["Treu", "ist", "mir", "nur", "das", "Ei\u00b7ne", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Hei, meine Geschwister Regen und Wind!", "tokens": ["Hei", ",", "mei\u00b7ne", "Ge\u00b7schwis\u00b7ter", "Re\u00b7gen", "und", "Wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Bin ja wie ihr des Irrwahns Kind/", "tokens": ["Bin", "ja", "wie", "ihr", "des", "Irr\u00b7wahns", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Einen Reiter seh ich in Wolken traben;", "tokens": ["Ei\u00b7nen", "Rei\u00b7ter", "seh", "ich", "in", "Wol\u00b7ken", "tra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Bist du's, Vor\u00fcber?", "tokens": ["Bist", "du's", ",", "Vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Den hagern Rappen umflattern Raben.", "tokens": ["Den", "ha\u00b7gern", "Rap\u00b7pen", "um\u00b7flat\u00b7tern", "Ra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Nun, dunkler Ritter? Willkommen, Tr\u00f6ster,", "tokens": ["Nun", ",", "dunk\u00b7ler", "Rit\u00b7ter", "?", "Will\u00b7kom\u00b7men", ",", "Tr\u00f6s\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du herbes Vor\u00fcber!", "tokens": ["Du", "her\u00b7bes", "Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mich d\u00fcnkt, ich werde noch dein Erl\u00f6ster,", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "wer\u00b7de", "noch", "dein", "Er\u00b7l\u00f6s\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Wir st\u00fcrmen ein Weilchen noch um die Wette,", "tokens": ["Wir", "st\u00fcr\u00b7men", "ein", "Weil\u00b7chen", "noch", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber/", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und trotten zuletzt an ein friedlich Bette.", "tokens": ["Und", "trot\u00b7ten", "zu\u00b7letzt", "an", "ein", "fried\u00b7lich", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.10": {"line.1": {"text": "Da wirst du die Morgenfanfare blasen,", "tokens": ["Da", "wirst", "du", "die", "Mor\u00b7gen\u00b7fan\u00b7fa\u00b7re", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein Heiland, Vor\u00fcber:", "tokens": ["Mein", "Hei\u00b7land", ",", "Vor\u00b7\u00fc\u00b7ber", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbtr\u00e4umer, nun ist dein Reiten und Rasen", "tokens": ["\u00bb", "tr\u00e4u\u00b7mer", ",", "nun", "ist", "dein", "Rei\u00b7ten", "und", "Ra\u00b7sen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.11": {"line.1": {"text": "Nur immer ins Weite langte dein Hasten:", "tokens": ["Nur", "im\u00b7mer", "ins", "Wei\u00b7te", "lang\u00b7te", "dein", "Has\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber!", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So war dein Leben ein einzig Fasten/", "tokens": ["So", "war", "dein", "Le\u00b7ben", "ein", "ein\u00b7zig", "Fas\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor\u00fcber.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.12": {"line.1": {"text": "Was du im Weiten nicht fandest, die Ruhe/", "tokens": ["Was", "du", "im", "Wei\u00b7ten", "nicht", "fan\u00b7dest", ",", "die", "Ru\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$,", "ART", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor\u00fcber, vor\u00fcber/", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "/"], "to