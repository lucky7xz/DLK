{"dta.poem.10201": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "( Gnadenort. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Den Eichbaum traf der Blitz aus schwarzen L\u00fcften,", "tokens": ["Den", "Eich\u00b7baum", "traf", "der", "Blitz", "aus", "schwar\u00b7zen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schlug in tausend Splitter ihn, der wilde.", "tokens": ["Und", "schlug", "in", "tau\u00b7send", "Split\u00b7ter", "ihn", ",", "der", "wil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "CARD", "NN", "PPER", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcnfhundert Jahr zur\u00fcck: In Waldesgr\u00fcften", "tokens": ["F\u00fcnf\u00b7hun\u00b7dert", "Jahr", "zu\u00b7r\u00fcck", ":", "In", "Wal\u00b7des\u00b7gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "PTKVZ", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umschlo\u00df Marien er im gr\u00fcnen Schilde.", "tokens": ["Um\u00b7schlo\u00df", "Ma\u00b7ri\u00b7en", "er", "im", "gr\u00fc\u00b7nen", "Schil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Dirne, lebensrot, mit derben H\u00fcften,", "tokens": ["Die", "Dir\u00b7ne", ",", "le\u00b7bens\u00b7rot", ",", "mit", "der\u00b7ben", "H\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kniet schluchzend vor dem Muttergottesbilde,", "tokens": ["Kniet", "schluch\u00b7zend", "vor", "dem", "Mut\u00b7ter\u00b7got\u00b7tes\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indess\u2019 der Junker lachend in den Kl\u00fcften", "tokens": ["In\u00b7dess'", "der", "Jun\u00b7ker", "la\u00b7chend", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jagt Seit\u2019 der blassen Herrin, Frau Wulffhilde.", "tokens": ["Jagt", "Seit'", "der", "blas\u00b7sen", "Her\u00b7rin", ",", "Frau", "Wulff\u00b7hil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}