{"textgrid.poem.60149": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich einem Fr\u00fchlings Morgenroth,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich einem Fr\u00fchlings Morgenroth,", "tokens": ["Gleich", "ei\u00b7nem", "Fr\u00fch\u00b7lings", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Freund, gieng er uns auf!", "tokens": ["O", "Freund", ",", "gieng", "er", "uns", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Abscheulicher, grausamer Tod!", "tokens": ["Ab\u00b7scheu\u00b7li\u00b7cher", ",", "grau\u00b7sa\u00b7mer", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "++--+--+", "measure": "prosodiakos"}, "line.4": {"text": "O welch ein Lebenslauf!", "tokens": ["O", "welch", "ein", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er zog aus wilden W\u00fcsteneyn", "tokens": ["Er", "zog", "aus", "wil\u00b7den", "W\u00fcs\u00b7te\u00b7neyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ungl\u00fcckliche; so zieht", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7che", ";", "so", "zieht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus kalter Erde Sonnenschein,", "tokens": ["Aus", "kal\u00b7ter", "Er\u00b7de", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blume, welche bl\u00fcht.", "tokens": ["Die", "Blu\u00b7me", ",", "wel\u00b7che", "bl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aus seiner grossen Seele flog", "tokens": ["Aus", "sei\u00b7ner", "gros\u00b7sen", "See\u00b7le", "flog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein holder Freundschaftsblick,", "tokens": ["Ein", "hol\u00b7der", "Freund\u00b7schafts\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In unsre L\u00e4nder, pl\u00f6zlich zog", "tokens": ["In", "uns\u00b7re", "L\u00e4n\u00b7der", ",", "pl\u00f6z\u00b7lich", "zog"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwarz Sturmgew\u00f6lk zur\u00fcck!", "tokens": ["Schwarz", "Sturm\u00b7ge\u00b7w\u00f6lk", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In unsern Seelen ward es Licht,", "tokens": ["In", "un\u00b7sern", "See\u00b7len", "ward", "es", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAF