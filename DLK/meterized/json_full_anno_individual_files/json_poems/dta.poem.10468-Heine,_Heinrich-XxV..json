{"dta.poem.10468": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre kommen und gehen,", "tokens": ["Die", "Jah\u00b7re", "kom\u00b7men", "und", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Geschlechter steigen in's Grab,", "tokens": ["Ge\u00b7schlech\u00b7ter", "stei\u00b7gen", "in's", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch nimmer vergeht die Liebe,", "tokens": ["Doch", "nim\u00b7mer", "ver\u00b7geht", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die ich im Herzen hab'.", "tokens": ["Die", "ich", "im", "Her\u00b7zen", "hab'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur einmal noch m\u00f6cht' ich dich sehen,", "tokens": ["Nur", "ein\u00b7mal", "noch", "m\u00f6cht'", "ich", "dich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sinken vor dir auf's Knie,", "tokens": ["Und", "sin\u00b7ken", "vor", "dir", "auf's", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sterbend zu dir sprechen:", "tokens": ["Und", "ster\u00b7bend", "zu", "dir", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Madame, ich liebe Sie!", "tokens": ["Ma\u00b7da\u00b7me", ",", "ich", "lie\u00b7be", "Sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}