{"textgrid.poem.46165": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Christian von Braunschweig reim", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichtum, geilheit, stolz und pracht", "tokens": ["Reich\u00b7tum", ",", "geil\u00b7heit", ",", "stolz", "und", "pracht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "hat die pfaffen so verbeizet", "tokens": ["hat", "die", "pfaf\u00b7fen", "so", "ver\u00b7bei\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und gelehrter laster macht", "tokens": ["und", "ge\u00b7lehr\u00b7ter", "las\u00b7ter", "macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hat sie so weit aus gereizet,", "tokens": ["hat", "sie", "so", "weit", "aus", "ge\u00b7rei\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "VVPP", "$,"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.5": {"text": "da\u00df verkehret sie jetzt seind", "tokens": ["da\u00df", "ver\u00b7keh\u00b7ret", "sie", "jetzt", "seind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gottes feind, des teufels freind.", "tokens": ["got\u00b7tes", "feind", ",", "des", "teu\u00b7fels", "freind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr ehrgeiz, des teufels kunst,", "tokens": ["Ihr", "ehr\u00b7geiz", ",", "des", "teu\u00b7fels", "kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "die sich stets bei ihnen findet,", "tokens": ["die", "sich", "stets", "bei", "ih\u00b7nen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "hat nunmehr ein solche brunst", "tokens": ["hat", "nun\u00b7mehr", "ein", "sol\u00b7che", "brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "allenthalben angez\u00fcndet,", "tokens": ["al\u00b7len\u00b7thal\u00b7ben", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df wir all verloren seind,", "tokens": ["da\u00df", "wir", "all", "ver\u00b7lo\u00b7ren", "seind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "es helf dan ein guter freind.", "tokens": ["es", "helf", "dan", "ein", "gu\u00b7ter", "freind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Gut und k\u00fchn der freind mu\u00df sein,", "tokens": ["Gut", "und", "k\u00fchn", "der", "freind", "mu\u00df", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df er uns nicht la\u00df verzagen,", "tokens": ["da\u00df", "er", "uns", "nicht", "la\u00df", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df er m\u00f6g die wilde schwein,", "tokens": ["da\u00df", "er", "m\u00f6g", "die", "wil\u00b7de", "schwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die man pfaffen hei\u00dft, verjagen;", "tokens": ["die", "man", "pfaf\u00b7fen", "hei\u00dft", ",", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "er mu\u00df sein der pfaffen feind,", "tokens": ["er", "mu\u00df", "sein", "der", "pfaf\u00b7fen", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gottes freind und got sein freind.", "tokens": ["got\u00b7tes", "freind", "und", "got", "sein", "freind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doller bischof, du bist er!", "tokens": ["Dol\u00b7ler", "bi\u00b7schof", ",", "du", "bist", "er", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "nim doch unsre not zu herzen.", "tokens": ["nim", "doch", "uns\u00b7re", "not", "zu", "her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "junger held, um gottes ehr", "tokens": ["jun\u00b7ger", "held", ",", "um", "got\u00b7tes", "ehr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUI", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "la\u00df die pfaffen nicht mehr scherzen!", "tokens": ["la\u00df", "die", "pfaf\u00b7fen", "nicht", "mehr", "scher\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wilt du, da\u00df got bleib dein freind,", "tokens": ["wilt", "du", ",", "da\u00df", "got", "bleib", "dein", "freind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "so bleib du der pfaffen feind.", "tokens": ["so", "bleib", "du", "der", "pfaf\u00b7fen", "feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df dich ihre heuchlerei,", "tokens": ["La\u00df", "dich", "ih\u00b7re", "heuch\u00b7le\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schw\u00e4tzen, schw\u00f6ren, beten, liegen;", "tokens": ["schw\u00e4t\u00b7zen", ",", "schw\u00f6\u00b7ren", ",", "be\u00b7ten", ",", "lie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "la\u00df dich ihre gauklerei", "tokens": ["la\u00df", "dich", "ih\u00b7re", "gauk\u00b7le\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und verdammen nicht betriegen!", "tokens": ["und", "ver\u00b7dam\u00b7men", "nicht", "be\u00b7trie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "bleib du nur der pfaffen feind,", "tokens": ["bleib", "du", "nur", "der", "pfaf\u00b7fen", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "auf da\u00df got auch bleib dein freind!", "tokens": ["auf", "da\u00df", "got", "auch", "bleib", "dein", "freind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NE", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hast du schon vil m\u00fch und not,", "tokens": ["Hast", "du", "schon", "vil", "m\u00fch", "und", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "solt du schon dein blut vergie\u00dfen;", "tokens": ["solt", "du", "schon", "dein", "blut", "ver\u00b7gie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "m\u00fcssen endlich doch mit spot", "tokens": ["m\u00fcs\u00b7sen", "end\u00b7lich", "doch", "mit", "spot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihren fuchsbalg sie einb\u00fc\u00dfen;", "tokens": ["ih\u00b7ren", "fuchs\u00b7balg", "sie", "ein\u00b7b\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVIZU", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "niemand ist der pfaffen feind,", "tokens": ["nie\u00b7mand", "ist", "der", "pfaf\u00b7fen", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "der nicht got zu seinem freind.", "tokens": ["der", "nicht", "got", "zu", "sei\u00b7nem", "freind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Diese teufels freind und schand", "tokens": ["Die\u00b7se", "teu\u00b7fels", "freind", "und", "schand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "nach verdienst bald zu belohnen,", "tokens": ["nach", "ver\u00b7dienst", "bald", "zu", "be\u00b7loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "so la\u00df deine rechte hand", "tokens": ["so", "la\u00df", "dei\u00b7ne", "rech\u00b7te", "hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "keines pfaffen freinds verschonen:", "tokens": ["kei\u00b7nes", "pfaf\u00b7fen", "freinds", "ver\u00b7scho\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "dan du got zu deinem freind,", "tokens": ["dan", "du", "got", "zu", "dei\u00b7nem", "freind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wan du aller pfaffen feind.", "tokens": ["wan", "du", "al\u00b7ler", "pfaf\u00b7fen", "feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Kom, den pfaffen, doller held,", "tokens": ["Kom", ",", "den", "pfaf\u00b7fen", ",", "dol\u00b7ler", "held", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "nach geb\u00fchr zu widerstehen", "tokens": ["nach", "ge\u00b7b\u00fchr", "zu", "wi\u00b7der\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3"