{"dta.poem.20140": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n An den  \n \u00e4dlen Sprossenden  \n als Er  \n Seinen Historischen Lustgarten  \n heraus gab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Der Dafnen stoltzes Haupt/ verliehrt ihr\ngr\u00fcnes Haar", "tokens": ["Der", "Daf\u00b7nen", "stolt\u00b7zes", "Haupt", "/", "ver\u00b7liehrt", "ihr", "gr\u00fc\u00b7nes", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In keiner Winterzeit. Der Nord kan nicht", "tokens": ["In", "kei\u00b7ner", "Win\u00b7ter\u00b7zeit", ".", "Der", "Nord", "kan", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "ART", "NN", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhr\u2019 immer-frische Pracht. So wei\u00df von", "tokens": ["Ih\u00b7r'", "im\u00b7mer\u00b7fri\u00b7sche", "Pracht", ".", "So", "wei\u00df", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "APPR"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Herr Neumark euer Ruhm/ Er sprosset immer-", "tokens": ["Herr", "Neu\u00b7mark", "eu\u00b7er", "Ruhm", "/", "Er", "spros\u00b7set", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Und gr\u00fcnet sch\u00f6n hervor/ Er ist schon einverleibt", "tokens": ["Und", "gr\u00fc\u00b7net", "sch\u00f6n", "her\u00b7vor", "/", "Er", "ist", "schon", "ein\u00b7ver\u00b7leibt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$(", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der grauen Ewigkeit. In diesem sch\u00f6nen Garte\u0303", "tokens": ["Der", "grau\u00b7en", "E\u00b7wig\u00b7keit", ".", "In", "die\u00b7sem", "sch\u00f6\u00b7nen", "Gar\u00b7t\u1ebd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "W\u00e4chst Er erst recht empor. Was man pflegt", "tokens": ["W\u00e4chst", "Er", "erst", "recht", "em\u00b7por", ".", "Was", "man", "pflegt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$.", "PWS", "PIS", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Was so wird eingepflantzt/ die\u00df ewiglich hekleibt/", "tokens": ["Was", "so", "wird", "ein\u00b7ge\u00b7pflantzt", "/", "die\u00df", "e\u00b7wig\u00b7lich", "he\u00b7kleibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "VVPP", "$(", "PDS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nunmer untergeht. Macht \u00e4dler G\u00e4rtner", "tokens": ["Und", "nun\u00b7mer", "un\u00b7ter\u00b7geht", ".", "Macht", "\u00e4d\u00b7ler", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$.", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Uns solche Gartenlust/ so k\u00f6n\u0303en wir vertreiben/", "tokens": ["Uns", "sol\u00b7che", "Gar\u00b7ten\u00b7lust", "/", "so", "k\u00f6\u00f1en", "wir", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$(", "ADV", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Die lange Traurigkeit/ und Euch wird ewig", "tokens": ["Die", "lan\u00b7ge", "Trau\u00b7rig\u00b7keit", "/", "und", "Euch", "wird", "e\u00b7wig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ein wol-verdientes Lob/ ein steter Ruhm und", "tokens": ["Ein", "wol\u00b7ver\u00b7dien\u00b7tes", "Lob", "/", "ein", "ste\u00b7ter", "Ruhm", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}