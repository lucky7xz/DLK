{"dta.poem.1411": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mehl-Thau fremder Schuld klebt allen Fr\u00fcchten an/", "tokens": ["Der", "Mehl\u00b7\u00b7T\u00b7hau", "frem\u00b7der", "Schuld", "klebt", "al\u00b7len", "Fr\u00fcch\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Nachdem der Eltern Mund den k\u00fchnen Bi\u00df gethan/", "tokens": ["Nach\u00b7dem", "der", "El\u00b7tern", "Mund", "den", "k\u00fch\u00b7nen", "Bi\u00df", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch/ der die Kelter von den V\u00f6lckern tritt allein/", "tokens": ["Doch", "/", "der", "die", "Kel\u00b7ter", "von", "den", "V\u00f6l\u00b7ckern", "tritt", "al\u00b7lein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der durch sein eignes Blutt bespritzet sein Gewand/", "tokens": ["Der", "durch", "sein", "eig\u00b7nes", "Blutt", "be\u00b7sprit\u00b7zet", "sein", "Ge\u00b7wand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Macht sie durch Wein und Blutt/ durch Wort und Wasser", "tokens": ["Macht", "sie", "durch", "Wein", "und", "Blutt", "/", "durch", "Wort", "und", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "ADJD", "$(", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "rein/", "tokens": ["rein", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Der/ Pathe/ reicht auch dir die milde Gnaden-Hand:", "tokens": ["Der", "/", "Pa\u00b7the", "/", "reicht", "auch", "dir", "die", "mil\u00b7de", "Gna\u00b7den\u00b7Hand", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NE", "$(", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dir ist vor Seel und Leib ein solches Bad bereit/", "tokens": ["Dir", "ist", "vor", "Seel", "und", "Leib", "ein", "sol\u00b7ches", "Bad", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "PIAT", "NE", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Daraus dir quellen wird Gl\u00fcck/ Heyl und Seligkeit!", "tokens": ["Da\u00b7raus", "dir", "quel\u00b7len", "wird", "Gl\u00fcck", "/", "Heyl", "und", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VAFIN", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}