{"textgrid.poem.39773": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An die W\u00e4nde einer Bergkapelle angeschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Heiligen, die dich bewohnen,", "tokens": ["Wie", "die", "Hei\u00b7li\u00b7gen", ",", "die", "dich", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Selig heiter steht dein milder Bau,", "tokens": ["Se\u00b7lig", "hei\u00b7ter", "steht", "dein", "mil\u00b7der", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie herabgesenkt aus bessern Zonen", "tokens": ["Wie", "her\u00b7ab\u00b7ge\u00b7senkt", "aus", "bes\u00b7sern", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Adelst du die abgeschiedne Au.", "tokens": ["A\u00b7delst", "du", "die", "ab\u00b7ge\u00b7schied\u00b7ne", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Unter dir das irdische Gewimmel,", "tokens": ["Un\u00b7ter", "dir", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ueber dir des Himmels ew'ge Ruh;", "tokens": ["Ue\u00b7ber", "dir", "des", "Him\u00b7mels", "ew'\u00b7ge", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und du schwebest zwischen Erd' und Himmel,", "tokens": ["Und", "du", "schwe\u00b7best", "zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "L\u00e4chelst freundlich beiden zu.", "tokens": ["L\u00e4\u00b7chelst", "freund\u00b7lich", "bei\u00b7den", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen Platz nur hast du von der Erde", "tokens": ["Ei\u00b7nen", "Platz", "nur", "hast", "du", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aufgenommen in dein still Gebiet,", "tokens": ["Auf\u00b7ge\u00b7nom\u00b7men", "in", "dein", "still", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Einen, dessen traurige Geberde", "tokens": ["Ei\u00b7nen", ",", "des\u00b7sen", "trau\u00b7ri\u00b7ge", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede lebensfrohe Menge flieht.", "tokens": ["Je\u00b7de", "le\u00b7bens\u00b7fro\u00b7he", "Men\u00b7ge", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo die schwarzen Kreuze deutend stehen,", "tokens": ["Wo", "die", "schwar\u00b7zen", "Kreu\u00b7ze", "deu\u00b7tend", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo der Boden ahnungsvoll sich schwellt,", "tokens": ["Wo", "der", "Bo\u00b7den", "ah\u00b7nungs\u00b7voll", "sich", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Willst du tief ein ernstes Feld bes\u00e4en,", "tokens": ["Willst", "du", "tief", "ein", "erns\u00b7tes", "Feld", "be\u00b7s\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "F\u00fcr den Himmel, nicht die Welt.", "tokens": ["F\u00fcr", "den", "Him\u00b7mel", ",", "nicht", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieblich blicket nach dem Feld der Leichen", "tokens": ["Lieb\u00b7lich", "bli\u00b7cket", "nach", "dem", "Feld", "der", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den Fenstern dein Marienbild,", "tokens": ["Aus", "den", "Fens\u00b7tern", "dein", "Ma\u00b7ri\u00b7en\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich flehe zu der Lebensreichen:", "tokens": ["Und", "ich", "fle\u00b7he", "zu", "der", "Le\u00b7bens\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mir auch l\u00e4chle, Jungfrau, zart und mild!", "tokens": ["Mir", "auch", "l\u00e4ch\u00b7le", ",", "Jung\u00b7frau", ",", "zart", "und", "mild", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Vielen hast du Trost und Heil geboten,", "tokens": ["Vie\u00b7len", "hast", "du", "Trost", "und", "Heil", "ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und gelindert manchen herben Schmerz:", "tokens": ["Und", "ge\u00b7lin\u00b7dert", "man\u00b7chen", "her\u00b7ben", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bist du eine Pflegerin der Todten,", "tokens": ["Bist", "du", "ei\u00b7ne", "Pfle\u00b7ge\u00b7rin", "der", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wecke denn mein sterbend Herz!", "tokens": ["We\u00b7cke", "denn", "mein", "ster\u00b7bend", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebeskrank wird Liebe nur es heilen:", "tokens": ["Lie\u00b7bes\u00b7krank", "wird", "Lie\u00b7be", "nur", "es", "hei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Liebes\u00fcberschwenglichkeit", "tokens": ["Dei\u00b7ne", "Lie\u00b7be\u00b7s\u00fc\u00b7bersc\u00b7hweng\u00b7lich\u00b7keit"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kannst du sie mit einer Jungfrau theilen,", "tokens": ["Kannst", "du", "sie", "mit", "ei\u00b7ner", "Jung\u00b7frau", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hold wie du, und z\u00fcchtig und geweiht?", "tokens": ["Hold", "wie", "du", ",", "und", "z\u00fcch\u00b7tig", "und", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "$,", "KON", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Gieb ihr nur von deiner, deiner Liebe,", "tokens": ["Gieb", "ihr", "nur", "von", "dei\u00b7ner", ",", "dei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schenk' ihr nur die neigungsvolle Ruh,", "tokens": ["Schenk'", "ihr", "nur", "die", "nei\u00b7gungs\u00b7vol\u00b7le", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Da\u00df sie so in ihrem reinen Triebe", "tokens": ["Da\u00df", "sie", "so", "in", "ih\u00b7rem", "rei\u00b7nen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Auf mich niederschau', wie du!", "tokens": ["Auf", "mich", "nie\u00b7der\u00b7schau'", ",", "wie", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber du, geheiligte Kapelle,", "tokens": ["A\u00b7ber", "du", ",", "ge\u00b7hei\u00b7lig\u00b7te", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df, o la\u00df mich Einmal nur mit Ihr", "tokens": ["La\u00df", ",", "o", "la\u00df", "mich", "Ein\u00b7mal", "nur", "mit", "Ihr"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "FM", "VVIMP", "PPER", "ADV", "ADV", "APPR", "PPOSAT"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Betend knien auf deiner heil'gen Schwelle,", "tokens": ["Be\u00b7tend", "kni\u00b7en", "auf", "dei\u00b7ner", "heil'\u00b7gen", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Vor der aufgethanen Himmelsth\u00fcr!", "tokens": ["Vor", "der", "auf\u00b7ge\u00b7tha\u00b7nen", "Him\u00b7mel\u00b7sth\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00e4llt von ihren gottdurchdrungnen Blicken", "tokens": ["F\u00e4llt", "von", "ih\u00b7ren", "gott\u00b7durch\u00b7drung\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Einer liebend dann auf mich \u2013 o nun!", "tokens": ["Ei\u00b7ner", "lie\u00b7bend", "dann", "auf", "mich", "\u2013", "o", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "APPR", "PPER", "$(", "FM", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "La\u00df mich todt, nach himmlischem Entz\u00fccken,", "tokens": ["La\u00df", "mich", "todt", ",", "nach", "himm\u00b7li\u00b7schem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Unter deinen Kreuzen ruhn!", "tokens": ["Un\u00b7ter", "dei\u00b7nen", "Kreu\u00b7zen", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie die Heiligen, die dich bewohnen,", "tokens": ["Wie", "die", "Hei\u00b7li\u00b7gen", ",", "die", "dich", "be\u00b7woh\u00b7nen", ","], "token_inf