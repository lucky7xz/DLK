{"dta.poem.8182": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Morgens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast genug der Ruh';", "tokens": ["Du", "hast", "ge\u00b7nug", "der", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und setz' dein zierlich F\u00fc\u00dfchen", "tokens": ["Und", "setz'", "dein", "zier\u00b7lich", "F\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Behende in den Schuh!", "tokens": ["Be\u00b7hen\u00b7de", "in", "den", "Schuh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun sch\u00fcttle von der Stirne", "tokens": ["Nun", "sch\u00fctt\u00b7le", "von", "der", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tr\u00e4ume blasse Spur!", "tokens": ["Der", "Tr\u00e4u\u00b7me", "blas\u00b7se", "Spur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das goldene Gestirne", "tokens": ["Das", "gol\u00b7de\u00b7ne", "Ge\u00b7stir\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erleuchtet l\u00e4ngst die Flur.", "tokens": ["Er\u00b7leuch\u00b7tet", "l\u00e4ngst", "die", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Rosen in deinem Garten", "tokens": ["Die", "Ro\u00b7sen", "in", "dei\u00b7nem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sprangen im Sonnenlicht;", "tokens": ["Spran\u00b7gen", "im", "Son\u00b7nen\u00b7licht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie k\u00f6nnen kaum erwarten,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "kaum", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df deine Hand sie bricht.", "tokens": ["Da\u00df", "dei\u00b7ne", "Hand", "sie", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}