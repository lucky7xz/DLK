{"dta.poem.4251": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das andere Fenster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Vorgrund ist allhier zuerst ein Weg, der nach dem", "tokens": ["Jm", "Vor\u00b7grund", "ist", "all\u00b7hier", "zu\u00b7erst", "ein", "Weg", ",", "der", "nach", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Walle f\u00fchret,", "tokens": ["Wal\u00b7le", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und linker Hand sich sanft erh\u00f6ht, mit jungen Linden ausge-", "tokens": ["Und", "lin\u00b7ker", "Hand", "sich", "sanft", "er\u00b7h\u00f6ht", ",", "mit", "jun\u00b7gen", "Lin\u00b7den", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PRF", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NE", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "zieret,", "tokens": ["zie\u00b7ret", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die ich im Fr\u00fchling setzen lassen, darauf ein ", "tokens": ["Die", "ich", "im", "Fr\u00fch\u00b7ling", "set\u00b7zen", "las\u00b7sen", ",", "da\u00b7rauf", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "VVINF", "$,", "PAV", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wall zu seh'n,", "tokens": ["Wall", "zu", "seh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auf welchem Platz noch bey einander metallene drey St\u00fccke", "tokens": ["Auf", "wel\u00b7chem", "Platz", "noch", "bey", "ein\u00b7an\u00b7der", "me\u00b7tal\u00b7le\u00b7ne", "drey", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV", "APPR", "PRF", "ADJA", "CARD", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "steh'n,", "tokens": ["steh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Auf eben so viel Batterien. Der Rand der Brustwehr", "tokens": ["Auf", "e\u00b7ben", "so", "viel", "Bat\u00b7te\u00b7ri\u00b7en", ".", "Der", "Rand", "der", "Brust\u00b7wehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PIAT", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "scheint begrenzet", "tokens": ["scheint", "be\u00b7gren\u00b7zet"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Von der krystallnen Fluht des Grabens, der wie ein reiner", "tokens": ["Von", "der", "krys\u00b7tall\u00b7nen", "Fluht", "des", "Gra\u00b7bens", ",", "der", "wie", "ein", "rei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Spiegel gl\u00e4nzet,", "tokens": ["Spie\u00b7gel", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Und sich vom innern Graben theilt, durch eines langen", "tokens": ["Und", "sich", "vom", "in\u00b7nern", "Gra\u00b7ben", "theilt", ",", "durch", "ei\u00b7nes", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dammes R\u00fccken,", "tokens": ["Dam\u00b7mes", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Den ich bey meiner Zeit erh\u00f6ht, mit Jpern-B\u00e4umen oben", "tokens": ["Den", "ich", "bey", "mei\u00b7ner", "Zeit", "er\u00b7h\u00f6ht", ",", "mit", "Jpern\u00b7B\u00e4u\u00b7men", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "APPR", "NN", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "schm\u00fccken,", "tokens": ["schm\u00fc\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Mit Weyden unten ganz besetzen, mit schlanken Kneyen ", "tokens": ["Mit", "Wey\u00b7den", "un\u00b7ten", "ganz", "be\u00b7set\u00b7zen", ",", "mit", "schlan\u00b7ken", "Kney\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ADV", "VVINF", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "rings umfassen,", "tokens": ["rings", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Sie k\u00fcnstlich in einander flechten, und fest dadurch ver-", "tokens": ["Sie", "k\u00fcnst\u00b7lich", "in", "ein\u00b7an\u00b7der", "flech\u00b7ten", ",", "und", "fest", "da\u00b7durch", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PRF", "VVINF", "$,", "KON", "ADJD", "PAV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "schr\u00e4nken lassen,", "tokens": ["schr\u00e4n\u00b7ken", "las\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "(auf welchem Damm, im holden Gr\u00fcnen, recht zwischen", "tokens": ["(", "auf", "wel\u00b7chem", "Damm", ",", "im", "hol\u00b7den", "Gr\u00fc\u00b7nen", ",", "recht", "zwi\u00b7schen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "APPR", "PWAT", "NN", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.22": {"text": "zweyen Wassern, man", "tokens": ["zwe\u00b7yen", "Was\u00b7sern", ",", "man"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "$,", "PIS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "In einer lieblichen Allee, mit sanftem Schritt, spatzieren", "tokens": ["In", "ei\u00b7ner", "lieb\u00b7li\u00b7chen", "Al\u00b7lee", ",", "mit", "sanf\u00b7tem", "Schritt", ",", "spat\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.24": {"text": "kann.)", "tokens": ["kann", ".", ")"], "token_info": ["word", "punct", "punct"], "pos": ["VMFIN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Des \u00e4ussern Grabens andres Ufer ist auch von hier nicht", "tokens": ["Des", "\u00e4us\u00b7sern", "Gra\u00b7bens", "and\u00b7res", "U\u00b7fer", "ist", "auch", "von", "hier", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "minder sch\u00f6n,", "tokens": ["min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Da ihn sechs kleine G\u00e4rten zieren, voll bunter Bluhmen,", "tokens": ["Da", "ihn", "sechs", "klei\u00b7ne", "G\u00e4r\u00b7ten", "zie\u00b7ren", ",", "voll", "bun\u00b7ter", "Bluh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "ADJA", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-++--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "anzuseh'n.", "tokens": ["an\u00b7zu\u00b7seh'", "n."], "token_info": ["word", "abbreviation"], "pos": ["XY", "XY"], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Die G\u00e4rten grenzen an der Gassen, an welcher neun Geb\u00e4ude", "tokens": ["Die", "G\u00e4r\u00b7ten", "gren\u00b7zen", "an", "der", "Gas\u00b7sen", ",", "an", "wel\u00b7cher", "neun", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "CARD", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.30": {"text": "steh'n,", "tokens": ["steh'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Die denn von Westen, Osten-werts, bis an die grosse Gasse", "tokens": ["Die", "denn", "von", "Wes\u00b7ten", ",", "Os\u00b7ten\u00b7werts", ",", "bis", "an", "die", "gros\u00b7se", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "NE", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "geh'n,", "tokens": ["geh'n", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "So hier sich ins Nord-Osten zieht. Nach vorerwehnten", "tokens": ["So", "hier", "sich", "ins", "Nord\u00b7Os\u00b7ten", "zieht", ".", "Nach", "vo\u00b7rer\u00b7wehn\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "PRF", "APPRART", "NN", "VVFIN", "$.", "APPR", "ADJA"], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "neun Geb\u00e4uden", "tokens": ["neun", "Ge\u00b7b\u00e4u\u00b7den"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Sieht man von oben Nord- werts liegen die sch\u00f6nsten Felder,", "tokens": ["Sieht", "man", "von", "o\u00b7ben", "Nord", "werts", "lie\u00b7gen", "die", "sch\u00f6ns\u00b7ten", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADV", "TRUNC", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Wies- und Weiden,", "tokens": ["Wies", "und", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "So weit das Auge tragen kann, bis an den Elb-Deich, mit", "tokens": ["So", "weit", "das", "Au\u00b7ge", "tra\u00b7gen", "kann", ",", "bis", "an", "den", "Elb\u00b7Deich", ",", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$,", "KOUS", "APPR", "ART", "NE", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.38": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Der sich bis an die D\u00f6se zieht, als im Amphitheater liegen.", "tokens": ["Der", "sich", "bis", "an", "die", "D\u00f6\u00b7se", "zieht", ",", "als", "im", "Am\u00b7phit\u00b7he\u00b7a\u00b7ter", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.40": {"text": "Zur rechten dreht sich eine Wind-M\u00fchl\u2019, und linker Hand", "tokens": ["Zur", "rech\u00b7ten", "dreht", "sich", "ei\u00b7ne", "Win\u00b7dM\u00fchl'", ",", "und", "lin\u00b7ker", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.41": {"text": "erblicket man", "tokens": ["er\u00b7bli\u00b7cket", "man"], "token_info": ["word", "word"], "pos": ["VVFIN", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.42": {"text": "Der Kugel-Bake ferne Spitze. Hier grenzet nun der Elb-", "tokens": ["Der", "Ku\u00b7gel\u00b7Ba\u00b7ke", "fer\u00b7ne", "Spit\u00b7ze", ".", "Hier", "gren\u00b7zet", "nun", "der", "Elb"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "ADV", "VVFIN", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.43": {"text": "Strohm an,", "tokens": ["Strohm", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.44": {"text": "Von einer solchen Breit\u2019 und Weite, da\u00df man kein Ende", "tokens": ["Von", "ei\u00b7ner", "sol\u00b7chen", "Breit'", "und", "Wei\u00b7te", ",", "da\u00df", "man", "kein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "KON", "NN", "$,", "KOUS", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "sehen kann,", "tokens": ["se\u00b7hen", "kann", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.46": {"text": "Und er sich in der Luft verliert. Da wir auf seinem blauen", "tokens": ["Und", "er", "sich", "in", "der", "Luft", "ver\u00b7liert", ".", "Da", "wir", "auf", "sei\u00b7nem", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.47": {"text": "R\u00fccken", "tokens": ["R\u00fc\u00b7cken"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Inzwischen weiss- und rohte Seegel, mit Lust, in grosser", "tokens": ["I\u00b7nzwi\u00b7schen", "weiss", "und", "roh\u00b7te", "See\u00b7gel", ",", "mit", "Lust", ",", "in", "gros\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "TRUNC", "KON", "ADJA", "NN", "$,", "APPR", "NN", "$,", "APPR", "ADJA"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.49": {"text": "Meng' erblicken,", "tokens": ["Meng'", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.50": {"text": "Die in der Landschaft sch\u00f6nen Grenzen der Fl\u00e4che, die so", "tokens": ["Die", "in", "der", "Land\u00b7schaft", "sch\u00f6\u00b7nen", "Gren\u00b7zen", "der", "Fl\u00e4\u00b7che", ",", "die", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.51": {"text": "glatt, so eben,", "tokens": ["glatt", ",", "so", "e\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "Indem sie hin und wieder schwimmen, der Landschaft fast ein", "tokens": ["In\u00b7dem", "sie", "hin", "und", "wie\u00b7der", "schwim\u00b7men", ",", "der", "Land\u00b7schaft", "fast", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADV", "VVINF", "$,", "ART", "NN", "ADV", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.53": {"text": "Leben geben.", "tokens": ["Le\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}