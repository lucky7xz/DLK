{"textgrid.poem.24660": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von Glockenschall, von Weihrauchduft umflossen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Glockenschall, von Weihrauchduft umflossen,", "tokens": ["Von", "Glo\u00b7cken\u00b7schall", ",", "von", "Weih\u00b7rauch\u00b7duft", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchwogt die Stra\u00dfen festliches Gepr\u00e4nge", "tokens": ["Durch\u00b7wogt", "die", "Stra\u00b7\u00dfen", "fest\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Und lockt ringsum ein froh bewegt Gedr\u00e4nge", "tokens": ["Und", "lockt", "ring\u00b7sum", "ein", "froh", "be\u00b7wegt", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An alle Fenster, \u2013 deines bleibt geschlossen.", "tokens": ["An", "al\u00b7le", "Fens\u00b7ter", ",", "\u2013", "dei\u00b7nes", "bleibt", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "$(", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hab auch ich der Tr\u00e4ume bunte Menge,", "tokens": ["So", "hab", "auch", "ich", "der", "Tr\u00e4u\u00b7me", "bun\u00b7te", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Seele Inhalt, vor dir ausgegossen:", "tokens": ["Der", "See\u00b7le", "In\u00b7halt", ",", "vor", "dir", "aus\u00b7ge\u00b7gos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du merktests kaum, da schwieg ich scheu-verdrossen,", "tokens": ["Du", "merk\u00b7tests", "kaum", ",", "da", "schwieg", "ich", "scheu\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leis verweht der Wind die leisen Kl\u00e4nge.", "tokens": ["Und", "leis", "ver\u00b7weht", "der", "Wind", "die", "lei\u00b7sen", "Kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nimm dich in acht: ein Tag ist schnell entschwunden,", "tokens": ["Nimm", "dich", "in", "acht", ":", "ein", "Tag", "ist", "schnell", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "CARD", "$.", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und leer und \u00f6de liegt die Stra\u00dfe wieder;", "tokens": ["Und", "leer", "und", "\u00f6\u00b7de", "liegt", "die", "Stra\u