{"textgrid.poem.42194": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Altes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein B\u00e4cker,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "B\u00e4\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der prunkte mit einem Wanst,", "tokens": ["Der", "prunk\u00b7te", "mit", "ei\u00b7nem", "Wanst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie du ihn k\u00fchner und kecker", "tokens": ["Wie", "du", "ihn", "k\u00fch\u00b7ner", "und", "ke\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir schwerlich tr\u00e4umen kannst.", "tokens": ["Dir", "schwer\u00b7lich", "tr\u00e4u\u00b7men", "kannst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er hat zum Weibe genommen", "tokens": ["Er", "hat", "zum", "Wei\u00b7be", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein w\u00fcrdiges Gegenst\u00fcck;", "tokens": ["Ein", "w\u00fcr\u00b7di\u00b7ges", "Ge\u00b7gen\u00b7st\u00fcck", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie konnten zusammen nicht kommen,", "tokens": ["Sie", "konn\u00b7ten", "zu\u00b7sam\u00b7men", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie waren viel zu dick.", "tokens": ["Sie", "wa\u00b7ren", "viel", "zu", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war einmal ein B\u00e4cker,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "B\u00e4\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der prunkte mit einem Wanst,", "tokens": ["Der", "prunk\u00b7te", "mit", "ei\u00b7nem", "Wanst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie du ihn k\u00fchner und kecker", "tokens": ["Wie", "du", "ihn", "k\u00fch\u00b7ner", "und", "ke\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dir schwerlich tr\u00e4umen kannst.", "tokens": ["Dir", "schwer\u00b7lich", "tr\u00e4u\u00b7men", "kannst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er hat zum Weibe genommen", "tokens": ["Er", "hat", "zum", "Wei\u00b7be", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "wo