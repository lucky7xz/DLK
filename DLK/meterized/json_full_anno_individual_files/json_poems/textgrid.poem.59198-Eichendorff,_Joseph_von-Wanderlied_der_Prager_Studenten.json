s": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerrissen sind die Schuh,", "tokens": ["Zer\u00b7ris\u00b7sen", "sind", "die", "Schuh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da blasen wir geschwinde", "tokens": ["Da", "bla\u00b7sen", "wir", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und singen noch dazu:", "tokens": ["Und", "sin\u00b7gen", "noch", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Beatus ille homo", "tokens": ["Bea\u00b7tus", "il\u00b7le", "ho\u00b7mo"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Qui sedet in sua domo", "tokens": ["Qui", "se\u00b7det", "in", "sua", "do\u00b7mo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.11": {"text": "Et sedet post fornacem", "tokens": ["Et", "se\u00b7det", "post", "for\u00b7na\u00b7cem"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.12": {"text": "Et habet bonam pacem!", "tokens": ["Et", "ha\u00b7bet", "bo\u00b7nam", "pa\u00b7cem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}}}}