{"textgrid.poem.42624": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lerchenkrieg", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblerchen sind wir, freie Lerchen,", "tokens": ["\u00bb", "ler\u00b7chen", "sind", "wir", ",", "frei\u00b7e", "Ler\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiegen uns im Sonnenschein,", "tokens": ["Wie\u00b7gen", "uns", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steigen auf aus gr\u00fcnen Saaten,", "tokens": ["Stei\u00b7gen", "auf", "aus", "gr\u00fc\u00b7nen", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tauchen in den Himmel ein.\u00ab", "tokens": ["Tau\u00b7chen", "in", "den", "Him\u00b7mel", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tausend Lerchen schwebten singend", "tokens": ["Tau\u00b7send", "Ler\u00b7chen", "schweb\u00b7ten", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob dem weiten, ebnen Ries,", "tokens": ["Ob", "dem", "wei\u00b7ten", ",", "eb\u00b7nen", "Ries", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ihr heller Ruf die Menschen", "tokens": ["Da\u00df", "ihr", "hel\u00b7ler", "Ruf", "die", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Hause bleiben lie\u00df.", "tokens": ["Nicht", "im", "Hau\u00b7se", "blei\u00b7ben", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus der Burg vom Wallersteine", "tokens": ["Aus", "der", "Burg", "vom", "Wal\u00b7ler\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritt der Graf mit seinem Sohn,", "tokens": ["Ritt", "der", "Graf", "mit", "sei\u00b7nem", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will f\u00fcr ihn die goldnen Sporen", "tokens": ["Will", "f\u00fcr", "ihn", "die", "gold\u00b7nen", "Spo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holen an des Kaisers Thron.", "tokens": ["Ho\u00b7len", "an", "des", "Kai\u00b7sers", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freut sich bei dem Lerchenwirbel", "tokens": ["Freut", "sich", "bei", "dem", "Ler\u00b7chen\u00b7wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schon der reichen Vogelbrut,", "tokens": ["Schon", "der", "rei\u00b7chen", "Vo\u00b7gel\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dem Junker ihm zur Seite", "tokens": ["Doch", "dem", "Jun\u00b7ker", "ihm", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpft das Herz von Rittermut.", "tokens": ["H\u00fcpft", "das", "Herz", "von", "Rit\u00b7ter\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus der Stadt mit grauen T\u00fcrmen,", "tokens": ["Aus", "der", "Stadt", "mit", "grau\u00b7en", "T\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Reichsstadt finsterm Tor", "tokens": ["Aus", "der", "Reichs\u00b7stadt", "fins\u00b7term", "Tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den goldnen Sonntagsmorgen", "tokens": ["In", "den", "gold\u00b7nen", "Sonn\u00b7tags\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt Alt und Jung hervor.", "tokens": ["Wan\u00b7delt", "Alt", "und", "Jung", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und der junge Rottenmeister", "tokens": ["Und", "der", "jun\u00b7ge", "Rot\u00b7ten\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchrt zum Garten seine Braut,", "tokens": ["F\u00fchrt", "zum", "Gar\u00b7ten", "sei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccket ihr das erste Veilchen", "tokens": ["Pfl\u00fc\u00b7cket", "ihr", "das", "ers\u00b7te", "Veil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei der Lerchen Jubellaut.", "tokens": ["Bei", "der", "Ler\u00b7chen", "Ju\u00b7bel\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Diese lieben Lenzestage,", "tokens": ["Die\u00b7se", "lie\u00b7ben", "Len\u00b7zes\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, sie waren schnell verbl\u00fcht,", "tokens": ["Ach", ",", "sie", "wa\u00b7ren", "schnell", "ver\u00b7bl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die sch\u00f6nen Sommermonde", "tokens": ["Und", "die", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7mon\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waren auch so bald vergl\u00fcht.", "tokens": ["Wa\u00b7ren", "auch", "so", "bald", "ver\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bblerchen sind wir, freie Lerchen.", "tokens": ["\u00bb", "ler\u00b7chen", "sind", "wir", ",", "frei\u00b7e", "Ler\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht mehr lieblich ist es hier,", "tokens": ["Nicht", "mehr", "lieb\u00b7lich", "ist", "es", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singen ist uns hier verleidet,", "tokens": ["Sin\u00b7gen", "ist", "uns", "hier", "ver\u00b7lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandern, wandern wollen wir.\u00ab", "tokens": ["Wan\u00b7dern", ",", "wan\u00b7dern", "wol\u00b7len", "wir", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "VMFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Abendlich im Herbstesnebel", "tokens": ["A\u00b7bend\u00b7lich", "im", "Herbs\u00b7tes\u00b7ne\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ziehn die B\u00fcrger aus dem Tor,", "tokens": ["Ziehn", "die", "B\u00fcr\u00b7ger", "aus", "dem", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Breiten, richten still die Garne,", "tokens": ["Brei\u00b7ten", ",", "rich\u00b7ten", "still", "die", "Gar\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschen mit gespanntem Ohr.", "tokens": ["Lau\u00b7schen", "mit", "ge\u00b7spann\u00b7tem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Horch, es rauscht! die Lerchen kommen,", "tokens": ["Horch", ",", "es", "rauscht", "!", "die", "Ler\u00b7chen", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch, es rauscht! ein m\u00e4cht'ger Flug!", "tokens": ["Horch", ",", "es", "rauscht", "!", "ein", "m\u00e4cht'\u00b7ger", "Flug", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waffenklirrend in die Garne", "tokens": ["Waf\u00b7fenk\u00b7lir\u00b7rend", "in", "die", "Gar\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprengt und stampft ein reis'ger Zug.", "tokens": ["Sprengt", "und", "stampft", "ein", "reis'\u00b7ger", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ruft der alte Graf vom Rosse:", "tokens": ["Ruft", "der", "al\u00b7te", "Graf", "vom", "Ros\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhilf, Maria, reine Magd!", "tokens": ["\u00bb", "hilf", ",", "Ma\u00b7ria", ",", "rei\u00b7ne", "Magd", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hilf den B\u00fcrgerfrevel strafen,", "tokens": ["Hilf", "den", "B\u00fcr\u00b7ger\u00b7fre\u00b7vel", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns st\u00f6rt die Vogeljagd!\u00ab", "tokens": ["Der", "uns", "st\u00f6rt", "die", "Vo\u00b7gel\u00b7jagd", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Ruft der junge Rottenmeister:", "tokens": ["Ruft", "der", "jun\u00b7ge", "Rot\u00b7ten\u00b7meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbschwert vom Leder! Spie\u00df herbei!", "tokens": ["\u00bb", "schwert", "vom", "Le\u00b7der", "!", "Spie\u00df", "her\u00b7bei", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPRART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerchen darf ein jeder fangen,", "tokens": ["Ler\u00b7chen", "darf", "ein", "je\u00b7der", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kleine V\u00f6gel, die sind frei.\u00ab", "tokens": ["Klei\u00b7ne", "V\u00f6\u00b7gel", ",", "die", "sind", "frei", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Als der graue Morgen d\u00e4mmert,", "tokens": ["Als", "der", "grau\u00b7e", "Mor\u00b7gen", "d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt der Junker tot im Feld;", "tokens": ["Liegt", "der", "Jun\u00b7ker", "tot", "im", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber ihm, aufs Schwert sich st\u00fctzend,", "tokens": ["\u00dc\u00b7ber", "ihm", ",", "aufs", "Schwert", "sich", "st\u00fct\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grimmig, stumm, der greise Held.", "tokens": ["Grim\u00b7mig", ",", "stumm", ",", "der", "grei\u00b7se", "Held", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Zum erschlagnen Rottenmeister", "tokens": ["Zum", "er\u00b7schlag\u00b7nen", "Rot\u00b7ten\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt sich dort sein junges Weib,", "tokens": ["Beugt", "sich", "dort", "sein", "jun\u00b7ges", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den aufgel\u00f6sten Locken", "tokens": ["Mit", "den", "auf\u00b7ge\u00b7l\u00f6s\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckt sie seinen blut'gen Leib.", "tokens": ["Deckt", "sie", "sei\u00b7nen", "blut'\u00b7gen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und noch einmal, eh sie ziehen,", "tokens": ["Und", "noch", "ein\u00b7mal", ",", "eh", "sie", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen tausend Lerchen an,", "tokens": ["Stei\u00b7gen", "tau\u0