"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ziehn die B\u00fcrger aus dem Tor,", "tokens": ["Ziehn", "die", "B\u00fcr\u00b7ger", "aus", "dem", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Breiten, richten still die Garne,", "tokens": ["Brei\u00b7ten", ",", "rich\u00b7ten", "still", "die", "Gar\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauschen mit gespanntem Ohr.", "tokens": ["Lau\u00b7schen", "mit", "ge\u00b7spann\u00b7tem", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Horch, es rauscht! die Lerchen kommen,", "tokens": ["Horch", ",", "es", "rauscht", "!", "die", "Ler\u00b7chen", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch, es rauscht! ein m\u00e4cht'ger Flug!", "tokens": ["Horch", ",", "es", "rauscht", "!", "ein", "m\u00e4cht'\u00b7ger", "Flug", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waffenklirrend in die Garne", "tokens": ["Waf\u00b7fenk\u00b7lir\u00b7rend", "in", "die", "Gar\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprengt und stampft ein reis'ger Zug.", "tokens": ["Sprengt", "und", "stampft", "ein", "reis'\u00b7ger", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.27": {"line.1": {"text": "Ruft der alte Graf vom Rosse:", "tokens": ["Ruft", "der", "al\u00b7te", "Graf", "vom", "Ros\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbhilf, Maria, reine Magd!", "tokens": ["\u00bb", "hilf", ",", "Ma\u00b7ria", ",", "rei\u00b7ne", "Magd", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hilf den B\u00fcrgerfrevel strafen,", "tokens": ["Hilf", "den", "B\u00fcr\u00b7ger\u00b7fre\u00b7vel", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns st\u00f6rt die Vogeljagd!\u00ab", "tokens": ["Der", "uns", "st\u00f6rt", "die", "Vo\u00b7gel\u00b7jagd", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Ruft der junge Rottenmeister:", "tokens": ["Ruft", "der", "jun\u00b7ge", "Rot\u00b7ten\u00b7meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbschwert vom Leder! Spie\u00df herbei!", "tokens": ["\u00bb", "schwert", "vom", "Le\u00b7der", "!", "Spie\u00df", "her\u00b7bei", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPRART", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lerchen darf ein jeder fangen,", "tokens": ["Ler\u00b7chen", "darf", "ein", "je\u00b7der", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kleine V\u00f6gel, die sind frei.\u00ab", "tokens": ["Klei\u00b7ne", "V\u00f6\u00b7gel", ",", "die", "sind", "frei", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "Als der graue Morgen d\u00e4mmert,", "tokens": ["Als", "der", "grau\u00b7e", "Mor\u00b7gen", "d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt der Junker tot im Feld;", "tokens": ["Liegt", "der", "Jun\u00b7ker", "tot", "im", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber ihm, aufs Schwert sich st\u00fctzend,", "tokens": ["\u00dc\u00b7ber", "ihm", ",", "aufs", "Schwert", "sich", "st\u00fct\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grimmig, stumm, der greise Held.", "tokens": ["Grim\u00b7mig", ",", "stumm", ",", "der", "grei\u00b7se", "Held", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Zum erschlagnen Rottenmeister", "tokens": ["Zum", "er\u00b7schlag\u00b7nen", "Rot\u00b7ten\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt sich dort sein junges Weib,", "tokens": ["Beugt", "sich", "dort", "sein", "jun\u00b7ges", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den aufgel\u00f6sten Locken", "tokens": ["Mit", "den", "auf\u00b7ge\u00b7l\u00f6s\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckt sie seinen blut'gen Leib.", "tokens": ["Deckt", "sie", "sei\u00b7nen", "blut'\u00b7gen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.31": {"line.1": {"text": "Und noch einmal, eh sie ziehen,", "tokens": ["Und", "noch", "ein\u00b7mal", ",", "eh", "sie", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen tausend Lerchen an,", "tokens": ["Stei\u00b7gen", "tau\u00b7send", "Ler\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flattern in der Morgensonne,", "tokens": ["Flat\u00b7tern", "in", "der", "Mor\u00b7gen\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmettern, wie sie nie getan:", "tokens": ["Schmet\u00b7tern", ",", "wie", "sie", "nie", "ge\u00b7tan", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.32": {"line.1": {"text": "\u00bblerchen sind wir, freie Lerchen,", "tokens": ["\u00bb", "ler\u00b7chen", "sind", "wir", ",", "frei\u00b7e", "Ler\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliegen \u00fcber Land und Flut;", "tokens": ["Flie\u00b7gen", "\u00fc\u00b7ber", "Land", "und", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die uns fangen, w\u00fcrgen wollten,", "tokens": ["Die", "uns", "fan\u00b7gen", ",", "w\u00fcr\u00b7gen", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegen hier in ihrem Blut.\u00ab", "tokens": ["Lie\u00b7gen", "hier", "in", "ih\u00b7rem", "Blut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}