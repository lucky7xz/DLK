{"dta.poem.3070": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beim Lichtanz\u00fcnden sprich: Willkommen sei die Nacht,", "tokens": ["Beim", "Licht\u00b7an\u00b7z\u00fcn\u00b7den", "sprich", ":", "Will\u00b7kom\u00b7men", "sei", "die", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesegnet der das Licht im Finstern hat gebracht!", "tokens": ["Ge\u00b7seg\u00b7net", "der", "das", "Licht", "im", "Fins\u00b7tern", "hat", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott ist das Licht des Tags, der ohn' ihn kann nicht leuchten,", "tokens": ["Gott", "ist", "das", "Licht", "des", "Tags", ",", "der", "ohn'", "ihn", "kann", "nicht", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit ihm wird die Nacht uns auch nicht finster deuchten.", "tokens": ["Und", "mit", "ihm", "wird", "die", "Nacht", "uns", "auch", "nicht", "fins\u00b7ter", "deuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos