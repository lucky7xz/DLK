{"dta.poem.7524": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich dich kehren mag, du kehrst dich selber zu", "tokens": ["Wie", "ich", "dich", "keh\u00b7ren", "mag", ",", "du", "kehrst", "dich", "sel\u00b7ber", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Licht, o Bl\u00fctenzweig, mich selbst besch\u00e4mest du.", "tokens": ["Dem", "Licht", ",", "o", "Bl\u00fc\u00b7ten\u00b7zweig", ",", "mich", "selbst", "be\u00b7sch\u00e4\u00b7mest", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$,", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und jeder Spro\u00df, verkehrt im Boden eingesenkt,", "tokens": ["Und", "je\u00b7der", "Spro\u00df", ",", "ver\u00b7kehrt", "im", "Bo\u00b7den", "ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat bald das Unterste nach Oben umgelenkt.", "tokens": ["Hat", "bald", "das", "Un\u00b7ters\u00b7te", "nach", "O\u00b7ben", "um\u00b7ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von innerm Drang gedr\u00e4ngt, von \u00e4u\u00dferm Zug gezogen,", "tokens": ["Von", "in\u00b7nerm", "Drang", "ge\u00b7dr\u00e4ngt", ",", "von", "\u00e4u\u00b7\u00dferm", "Zug", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt ihr dem Licht getreu, und bis zum Tod gewogen.", "tokens": ["Bleibt", "ihr", "dem", "Licht", "ge\u00b7treu", ",", "und", "bis", "zum", "Tod", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,", "KON", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So haltet ihr das Licht, ihr dunkeln Trieb', in Ehren,", "tokens": ["So", "hal\u00b7tet", "ihr", "das", "Licht", ",", "ihr", "dun\u00b7keln", "Trieb'", ",", "in", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur der lichte Geist kann ab zur Nacht sich kehren.", "tokens": ["Und", "nur", "der", "lich\u00b7te", "Geist", "kann", "ab", "zur", "Nacht", "sich", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "APPR", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch kann auch er, inde\u00df ihr bleibt an Wurzeln hangen,", "tokens": ["Doch", "kann", "auch", "er", ",", "in\u00b7de\u00df", "ihr", "bleibt", "an", "Wur\u00b7zeln", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Lichte zugewandt, zum Lichte selbst gelangen.", "tokens": ["Dem", "Lich\u00b7te", "zu\u00b7ge\u00b7wandt", ",", "zum", "Lich\u00b7te", "selbst", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}