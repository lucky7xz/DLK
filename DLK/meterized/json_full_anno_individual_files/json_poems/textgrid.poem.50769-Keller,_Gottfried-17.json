{"textgrid.poem.50769": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein lustiger Mediziner", "tokens": ["Ein", "lus\u00b7ti\u00b7ger", "Me\u00b7di\u00b7zi\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War dazumal mein Freund;", "tokens": ["War", "da\u00b7zu\u00b7mal", "mein", "Freund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sa\u00dfen bei vollem Glase", "tokens": ["Wir", "sa\u00b7\u00dfen", "bei", "vol\u00b7lem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um Mitternacht vereint.", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sprach ihm von meiner Liebe,", "tokens": ["Ich", "sprach", "ihm", "von", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Indessen er zecht' und sang,", "tokens": ["In\u00b7des\u00b7sen", "er", "zecht'", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und meine Worte verhallten", "tokens": ["Und", "mei\u00b7ne", "Wor\u00b7te", "ver\u00b7hall\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im wilden Gl\u00e4serklang.", "tokens": ["Im", "wil\u00b7den", "Gl\u00e4\u00b7ser\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch sprach ich immer und st\u00e4rker", "tokens": ["Doch", "sprach", "ich", "im\u00b7mer", "und", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mit h\u00f6herer Liebesglut;", "tokens": ["Mit", "h\u00f6\u00b7he\u00b7rer", "Lie\u00b7bes\u00b7glut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich wollte damit d\u00e4mmen", "tokens": ["Ich", "woll\u00b7te", "da\u00b7mit", "d\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein bange wallendes Blut.", "tokens": ["Mein", "ban\u00b7ge", "wal\u00b7len\u00b7des", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Da wurde er ungeduldig", "tokens": ["Da", "wur\u00b7de", "er", "un\u00b7ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sagte mit barschem Ton:", "tokens": ["Und", "sag\u00b7te", "mit", "bar\u00b7schem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbich kenne deine Geliebte", "tokens": ["\u00bb", "ich", "ken\u00b7ne", "dei\u00b7ne", "Ge\u00b7lieb\u00b7te"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und rate dir ab davon!", "tokens": ["Und", "ra\u00b7te", "dir", "ab", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich rate dir ab, sonst bist du", "tokens": ["Ich", "ra\u00b7te", "dir", "ab", ",", "sonst", "bist", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Witwer im n\u00e4chsten Mai,", "tokens": ["Ein", "Wit\u00b7wer", "im", "n\u00e4chs\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn dann liegt sie im Sarge,", "tokens": ["Denn", "dann", "liegt", "sie", "im", "Sar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "'ne Leiche frank und frei.", "tokens": ["'ne", "Lei\u00b7che", "frank", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Rosen sind eitel Hektik", "tokens": ["Die", "Ro\u00b7sen", "sind", "ei\u00b7tel", "Hek\u00b7tik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN"], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Auf ihrem schmalen Gesicht;", "tokens": ["Auf", "ih\u00b7rem", "schma\u00b7len", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich h\u00f6rte sie heute husten,", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", "heu\u00b7te", "hus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und das gef\u00e4llt mir nicht!", "tokens": ["Und", "das", "ge\u00b7f\u00e4llt", "mir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wohl ist sie ein feines Wesen,", "tokens": ["Wohl", "ist", "sie", "ein", "fei\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch eben nur allzufein!", "tokens": ["Doch", "e\u00b7ben", "nur", "all\u00b7zu\u00b7fein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df fahren den sterblichen Engel,", "tokens": ["La\u00df", "fah\u00b7ren", "den", "sterb\u00b7li\u00b7chen", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sonst trifft dich Kummer und Pein!\u00ab", "tokens": ["Sonst", "trifft", "dich", "Kum\u00b7mer", "und", "Pein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Die rohen Worte schnitten", "tokens": ["Die", "ro\u00b7hen", "Wor\u00b7te", "schnit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir tief in die Seele ein,", "tokens": ["Mir", "tief", "in", "die", "See\u00b7le", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und darum weil leicht was Wahres", "tokens": ["Und", "da\u00b7rum", "weil", "leicht", "was", "Wah\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "KOUS", "ADJD", "PWS", "ADJA"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "An ihnen konnte sein.", "tokens": ["An", "ih\u00b7nen", "konn\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Jedoch mein armes Liebchen", "tokens": ["Je\u00b7doch", "mein", "ar\u00b7mes", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gewann einen Zauber mehr; \u2013", "tokens": ["Ge\u00b7wann", "ei\u00b7nen", "Zau\u00b7ber", "mehr", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, nein, sie kann nicht sterben,", "tokens": ["Nein", ",", "nein", ",", "sie", "kann", "nicht", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir lieben uns allzusehr!", "tokens": ["Wir", "lie\u00b7ben", "uns", "all\u00b7zu\u00b7sehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Am Morgen ward ich ruhig,", "tokens": ["Am", "Mor\u00b7gen", "ward", "ich", "ru\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als die Sonne ins Zimmer fiel;", "tokens": ["Als", "die", "Son\u00b7ne", "ins", "Zim\u00b7mer", "fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ich sah durchs Fenster fr\u00f6hlich", "tokens": ["Ich", "sah", "durchs", "Fens\u00b7ter", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jagenden Wolken Spiel.", "tokens": ["Der", "ja\u00b7gen\u00b7den", "Wol\u00b7ken", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Ich rief: \u00bbEr sprach's im Rausche,", "tokens": ["Ich", "rief", ":", "\u00bb", "Er", "sprach's", "im", "Rau\u00b7sche", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ich war gestern ein Tor!", "tokens": ["Und", "ich", "war", "ge\u00b7stern", "ein", "Tor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es lebe das rosige Leben", "tokens": ["Es", "le\u00b7be", "das", "ro\u00b7si\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und meine Liebe zuvor!\u00ab", "tokens": ["Und", "mei\u00b7ne", "Lie\u00b7be", "zu\u00b7vor", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Ein lustiger Mediziner", "tokens": ["Ein", "lus\u00b7ti\u00b7ger", "Me\u00b7di\u00b7zi\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "War dazumal mein Freund;", "tokens": ["War", "da\u00b7zu\u00b7mal", "mein", "Freund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir sa\u00dfen bei vollem Glase", "tokens": ["Wir", "sa\u00b7\u00dfen", "bei", "vol\u00b7lem", "Gla\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measu