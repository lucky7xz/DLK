{"dta.poem.21057": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Des Grabes  \n Furchtbarkeit und Lieblichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Furchthar ist das Grab!               ", "tokens": ["Furch\u00b7thar", "ist", "das", "Grab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Kalte Winde sausen,", "tokens": ["Kal\u00b7te", "Win\u00b7de", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dumpfe Schauer grausen,", "tokens": ["Dump\u00b7fe", "Schau\u00b7er", "grau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gram und Grauen hausen", "tokens": ["Gram", "und", "Grau\u00b7en", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Um das stumme Grab.", "tokens": ["Um", "das", "stum\u00b7me", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Furchtbar ist das Grab.", "tokens": ["Furcht\u00b7bar", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Lieblich ist das Grab.", "tokens": ["Lieb\u00b7lich", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Linde Stille flistert,", "tokens": ["Lin\u00b7de", "Stil\u00b7le", "flis\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00fchler Schatten d\u00fcstert,", "tokens": ["K\u00fch\u00b7ler", "Schat\u00b7ten", "d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tiefer Friede s\u00e4uselt", "tokens": ["Tie\u00b7fer", "Frie\u00b7de", "s\u00e4u\u00b7selt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Um das stille Grab.", "tokens": ["Um", "das", "stil\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Lieblich ist das Grab.", "tokens": ["Lieb\u00b7lich", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Graunvoll ist das Grab.", "tokens": ["Graun\u00b7voll", "ist", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "\u00c4ngstlich ist des Grabes Enge,", "tokens": ["\u00c4ngst\u00b7lich", "ist", "des", "Gra\u00b7bes", "En\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Breite, seine L\u00e4nge,", "tokens": ["Sei\u00b7ne", "Brei\u00b7te", ",