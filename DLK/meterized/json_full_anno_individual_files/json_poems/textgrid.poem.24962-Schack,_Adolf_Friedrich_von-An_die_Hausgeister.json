{"textgrid.poem.24962": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Hausgeister", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder in dem alten Gleise", "tokens": ["Wie\u00b7der", "in", "dem", "al\u00b7ten", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt das Rad der Tage nun;", "tokens": ["Rollt", "das", "Rad", "der", "Ta\u00b7ge", "nun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des Lebens irrer Reise", "tokens": ["Von", "des", "Le\u00b7bens", "ir\u00b7rer", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehr' ich, um in euerm Kreise", "tokens": ["Kehr'", "ich", ",", "um", "in", "eu\u00b7erm", "Krei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUI", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Froh und gl\u00fccklich auszuruhn.", "tokens": ["Froh", "und", "gl\u00fcck\u00b7lich", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meines Vaterhauses Laren,", "tokens": ["Mei\u00b7nes", "Va\u00b7ter\u00b7hau\u00b7ses", "La\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich vor Irrsal zu bewahren,", "tokens": ["Mich", "vor", "Irr\u00b7sal", "zu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fctet mich bei Tag und Nacht,", "tokens": ["H\u00fc\u00b7tet", "mich", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie ihr, euern Reigen schlingend,", "tokens": ["Wie", "ihr", ",", "eu\u00b7ern", "Rei\u00b7gen", "schlin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "S\u00fc\u00dfe Lieder leise singend,", "tokens": ["S\u00fc\u00b7\u00dfe", "Lie\u00b7der", "lei\u00b7se", "sin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Meine Wiege schon bewacht!", "tokens": ["Mei\u00b7ne", "Wie\u00b7ge", "schon", "be\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun aufs neu mit Glockenklange", "tokens": ["Nun", "aufs", "neu", "mit", "Glo\u00b7cken\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weckt mich, wenn der Morgen graut!", "tokens": ["Weckt", "mich", ",", "wenn", "der", "Mor\u00b7gen", "graut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei der Schwalbe Fr\u00fchgesange,", "tokens": ["Bei", "der", "Schwal\u00b7be", "Fr\u00fch\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich an dem Bogengange", "tokens": ["Die", "sich", "an", "dem", "Bo\u00b7gen\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heimatlich ihr Nest gebaut,", "tokens": ["Hei\u00b7mat\u00b7lich", "ihr", "Nest", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sei ich in der D\u00e4mmerfrische", "tokens": ["Sei", "ich", "in", "der", "D\u00e4m\u00b7mer\u00b7fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "An dem trauten Arbeitstische", "tokens": ["An", "dem", "trau\u00b7ten", "Ar\u00b7beits\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Von der B\u00fccherwelt umringt,", "tokens": ["Von", "der", "B\u00fc\u00b7cher\u00b7welt", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00e4hrend ihr mit lust'gem Klettern", "tokens": ["W\u00e4h\u00b7rend", "ihr", "mit", "lust'\u00b7gem", "Klet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aus den Schr\u00e4nken, von den Brettern", "tokens": ["Aus", "den", "Schr\u00e4n\u00b7ken", ",", "von", "den", "Bret\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mir die lieben B\u00e4nde bringt!", "tokens": ["Mir", "die", "lie\u00b7ben", "B\u00e4n\u00b7de", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern dann lausch' ich euren Spielen,", "tokens": ["Gern", "dann", "lausch'", "ich", "eu\u00b7ren", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn \u2013 als ob vom Wind bewegt", "tokens": ["Wenn", "\u2013", "als", "ob", "vom", "Wind", "be\u00b7wegt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "KOKOM", "KOUS", "APPRART", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leichte Bl\u00e4tter niederfielen \u2013", "tokens": ["Leich\u00b7te", "Bl\u00e4t\u00b7ter", "nie\u00b7der\u00b7fie\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Treppen, auf den Dielen", "tokens": ["Auf", "den", "Trep\u00b7pen", ",", "auf", "den", "Die\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Trippelnd sich eu'r Fu\u00dftritt regt;", "tokens": ["Trip\u00b7pelnd", "sich", "eu'r", "Fu\u00df\u00b7tritt", "regt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn ihr klingelt an den Schellen,", "tokens": ["Wenn", "ihr", "klin\u00b7gelt", "an", "den", "Schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lachend, wie das lust'ge Gellen", "tokens": ["La\u00b7chend", ",", "wie", "das", "lust'\u00b7ge", "Gel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dem Flur ein Echo weckt,", "tokens": ["Auf", "dem", "Flur", "ein", "E\u00b7cho", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder, mit den H\u00e4ndchen klappend,", "tokens": ["O\u00b7der", ",", "mit", "den", "H\u00e4nd\u00b7chen", "klap\u00b7pend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Durch das Morgend\u00e4mmer tappend,", "tokens": ["Durch", "das", "Mor\u00b7gen\u00b7d\u00e4m\u00b7mer", "tap\u00b7pend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Euch mit unsern Gnomen neckt.", "tokens": ["Euch", "mit", "un\u00b7sern", "Gno\u00b7men", "neckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder nachts mit den Geschwistern", "tokens": ["O\u00b7der", "nachts", "mit", "den", "Ge\u00b7schwis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Freunden am Kamin", "tokens": ["Und", "den", "Freun\u00b7den", "am", "Ka\u00b7min"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r' ich in der Flamme Knistern", "tokens": ["H\u00f6r'", "ich", "in", "der", "Flam\u00b7me", "Knis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Stimmen leise fl\u00fcstern;", "tokens": ["Eu\u00b7re", "Stim\u00b7men", "lei\u00b7se", "fl\u00fcs\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aennchen sitzt auf meinen Knien", "tokens": ["A\u00b7enn\u00b7chen", "sitzt", "auf", "mei\u00b7nen", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und erz\u00e4hlt uns sch\u00f6ne M\u00e4rchen,", "tokens": ["Und", "er\u00b7z\u00e4hlt", "uns", "sch\u00f6\u00b7ne", "M\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sei es vom verliebten Kl\u00e4rchen,", "tokens": ["Sei", "es", "vom", "ver\u00b7lieb\u00b7ten", "Kl\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie es sich im Wald verlief,", "tokens": ["Wie", "es", "sich", "im", "Wald", "ver\u00b7lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Oder von der Dornenhecke,", "tokens": ["O\u00b7der", "von", "der", "Dor\u00b7nen\u00b7he\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo im sicheren Versteckte", "tokens": ["Wo", "im", "si\u00b7che\u00b7ren", "Ver\u00b7steck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "R\u00f6schen hundert Jahre schlief.", "tokens": ["R\u00f6\u00b7schen", "hun\u00b7dert", "Jah\u00b7re", "schlief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Spielend mit den blonden Locken,", "tokens": ["Spie\u00b7lend", "mit", "den", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fcss' ich das geliebte Kind;", "tokens": ["K\u00fcss'", "ich", "das", "ge\u00b7lieb\u00b7te", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bertha sitzt indes am Rocken,", "tokens": ["Bert\u00b7ha", "sitzt", "in\u00b7des", "am", "Ro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Spinnrad ohne Stocken", "tokens": ["Und", "das", "Spinn\u00b7rad", "oh\u00b7ne", "Sto\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnurrt im Kreise pfeilgeschwind;", "tokens": ["Schnurrt", "im", "Krei\u00b7se", "pfeil\u00b7ge\u00b7schwind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von des Herbstes Bl\u00e4ttertreiben", "tokens": ["Von", "des", "Herbs\u00b7tes", "Bl\u00e4t\u00b7ter\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Klirren oft die Fensterscheiben,", "tokens": ["Klir\u00b7ren", "oft", "die", "Fens\u00b7ter\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drau\u00dfen rauscht der Eichenbaum,", "tokens": ["Drau\u00b7\u00dfen", "rauscht", "der", "Ei\u00b7chen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und, zu meinen F\u00fc\u00dfen liegend,", "tokens": ["Und", ",", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lie\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bellt, sich fester an mich schmiegend,", "tokens": ["Bellt", ",", "sich", "fes\u00b7ter", "an", "mich", "schmie\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "ADJD", "APPR", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Oft das Windspiel auf im Traum.", "tokens": ["Oft", "das", "Wind\u00b7spiel", "auf", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber von den Glockent\u00fcrmen", "tokens": ["A\u00b7ber", "von", "den", "Glo\u00b7cken\u00b7t\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnt zum Schlaf der zw\u00f6lfte Schlag;", "tokens": ["Mahnt", "zum", "Schlaf", "der", "zw\u00f6lf\u00b7te", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch; ihr Laren, uns von St\u00fcrmen", "tokens": ["Euch", ";", "ihr", "La\u00b7ren", ",", "uns", "von", "St\u00fcr\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$.", "PPOSAT", "NN", "$,", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor Flammennot zu schirmen,", "tokens": ["Und", "vor", "Flam\u00b7men\u00b7not", "zu", "schir\u00b7men", ","], "token_info": ["word", "word", "word", "word", "wor