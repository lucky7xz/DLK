{"dta.poem.21469": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O gold\u2019nes Licht, dein blendendes Strahlenkleid", "tokens": ["O", "gold'\u00b7nes", "Licht", ",", "dein", "blen\u00b7den\u00b7des", "Strah\u00b7len\u00b7kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Umh\u00fclle mir von Neuem den m\u00fcden Geist:", "tokens": ["Um\u00b7h\u00fcl\u00b7le", "mir", "von", "Neu\u00b7em", "den", "m\u00fc\u00b7den", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vergessen such\u2019 ich, Adleraufschwung", "tokens": ["Ver\u00b7ges\u00b7sen", "such'", "ich", ",", "Ad\u00b7ler\u00b7auf\u00b7schwung"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber die Tiefe des d\u00fcst\u2019ren Lebens.", "tokens": ["Ue\u00b7ber", "die", "Tie\u00b7fe", "des", "d\u00fcst'\u00b7ren", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nur du allein giebst Frieden und Wonnelust;", "tokens": ["Nur", "du", "al\u00b7lein", "giebst", "Frie\u00b7den", "und", "Won\u00b7ne\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O wende nicht dein g\u00f6ttliches Vateraug\u2019", "tokens": ["O", "wen\u00b7de", "nicht", "dein", "g\u00f6tt\u00b7li\u00b7ches", "Va\u00b7ter\u00b7aug'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von deinem Kind, das sich hinaussehnt", "tokens": ["Von", "dei\u00b7nem", "Kind", ",", "das", "sich", "hin\u00b7aus\u00b7sehnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber die Enge des d\u00fcst\u2019ren Lebens.", "tokens": ["Ue\u00b7ber", "die", "En\u00b7ge", "des", "d\u00fcst'\u00b7ren", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "O lass in deiner allhinleuchtenden Nacht", "tokens": ["O", "lass", "in", "dei\u00b7ner", "all\u00b7hin\u00b7leuch\u00b7ten\u00b7den", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Alswie ein Kind am blumigen Alpenhang", "tokens": ["Als\u00b7wie", "ein", "Kind", "am", "blu\u00b7mi\u00b7gen", "Al\u00b7pen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mich selig ruh\u2019n, empor mich tr\u00e4umen", "tokens": ["Mich", "se\u00b7lig", "ruh'n", ",", "em\u00b7por", "mich", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVINF", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber die Schwere des d\u00fcst\u2019ren Lebens!", "tokens": ["Ue\u00b7ber", "die", "Schwe\u00b7re", "des", "d\u00fcst'\u00b7ren", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}