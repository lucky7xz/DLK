{"textgrid.poem.40865": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Reigerbaize", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Lenz die Erde wieder im ersten Ku\u00df umschlo\u00df,", "tokens": ["Als", "Lenz", "die", "Er\u00b7de", "wie\u00b7der", "im", "ers\u00b7ten", "Ku\u00df", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ritt aus Br\u00fcgge's Thoren ein bunter J\u00e4gertro\u00df,", "tokens": ["Da", "ritt", "aus", "Br\u00fcg\u00b7ge's", "Tho\u00b7ren", "ein", "bun\u00b7ter", "J\u00e4\u00b7ger\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Viel schmucke Falkoniere sah man zu Rosse ziehn,", "tokens": ["Viel", "schmu\u00b7cke", "Fal\u00b7ko\u00b7nie\u00b7re", "sah", "man", "zu", "Ros\u00b7se", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und an des Gatten Seite die sch\u00f6ne Herzogin.", "tokens": ["Und", "an", "des", "Gat\u00b7ten", "Sei\u00b7te", "die", "sch\u00f6\u00b7ne", "Her\u00b7zo\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Arm sa\u00df ihr ein Falke. Ob seinem wei\u00dfen Gewand", "tokens": ["Am", "Arm", "sa\u00df", "ihr", "ein", "Fal\u00b7ke", ".", "Ob", "sei\u00b7nem", "wei\u00b7\u00dfen", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ward er bei Hofe scherzweis der Dominikaner genannt,", "tokens": ["Ward", "er", "bei", "Ho\u00b7fe", "scher\u00b7zweis", "der", "Do\u00b7mi\u00b7ni\u00b7ka\u00b7ner", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein schwarzes K\u00e4ppchen bedeckt' ihn, er trug ein silbern Collar,", "tokens": ["Ein", "schwar\u00b7zes", "K\u00e4pp\u00b7chen", "be\u00b7deckt'", "ihn", ",", "er", "trug", "ein", "sil\u00b7bern", "Col\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Darauf das W\u00f6rtlein: ", "tokens": ["Da\u00b7rauf", "das", "W\u00f6rt\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Weit dehnt sich ein Haide, da gr\u00fcnt kein schatt'ger Baum", "tokens": ["Weit", "dehnt", "sich", "ein", "Hai\u00b7de", ",", "da", "gr\u00fcnt", "kein", "schatt'\u00b7ger", "Baum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NE", "$,", "ADV", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nur Dorngestr\u00fcppe wuchert zerstreut im \u00f6den Raum,", "tokens": ["Nur", "Dorn\u00b7ge\u00b7str\u00fcp\u00b7pe", "wu\u00b7chert", "zer\u00b7streut", "im", "\u00f6\u00b7den", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Zur Linken lag ein Weiher, des Reigervolkes Bad,", "tokens": ["Zur", "Lin\u00b7ken", "lag", "ein", "Wei\u00b7her", ",", "des", "Rei\u00b7ger\u00b7vol\u00b7kes", "Bad", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da w\u00e4scht es sein Gefieder, sich selber zum Verrath.", "tokens": ["Da", "w\u00e4scht", "es", "sein", "Ge\u00b7fie\u00b7der", ",", "sich", "sel\u00b7ber", "zum", "Ver\u00b7rath", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Jetzt rauscht es in den Wellen, es kreischt aus dem Schilf hervor", "tokens": ["Jetzt", "rauscht", "es", "in", "den", "Wel\u00b7len", ",", "es", "kreischt", "aus", "dem", "Schilf", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und rechts und linkshin fliegen verscheuchte Reiger empor,", "tokens": ["Und", "rechts", "und", "links\u00b7hin", "flie\u00b7gen", "ver\u00b7scheuch\u00b7te", "Rei\u00b7ger", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VVINF", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vom Arm der J\u00e4ger steigen die muth'gen Falken hinan,", "tokens": ["Vom", "Arm", "der", "J\u00e4\u00b7ger", "stei\u00b7gen", "die", "muth'\u00b7gen", "Fal\u00b7ken", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gleich W\u00fcnschen der Menschenseele, so schweben sie himmelan", "tokens": ["Gleich", "W\u00fcn\u00b7schen", "der", "Men\u00b7schen\u00b7see\u00b7le", ",", "so", "schwe\u00b7ben", "sie", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Und jedes J\u00e4gers Auge will mit den Falken ziehn,", "tokens": ["Und", "je\u00b7des", "J\u00e4\u00b7gers", "Au\u00b7ge", "will", "mit", "den", "Fal\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie die in L\u00fcften, sto\u00dfend, zur Rechten und Linken fliehn,", "tokens": ["Wie", "die", "in", "L\u00fcf\u00b7ten", ",", "sto\u00b7\u00dfend", ",", "zur", "Rech\u00b7ten", "und", "Lin\u00b7ken", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "NN", "$,", "ADJD", "$,", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So sprengen flink da unten die Reiter kreuz und quer,", "tokens": ["So", "spren\u00b7gen", "flink", "da", "un\u00b7ten", "die", "Rei\u00b7ter", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ADV", "ART", "ADJA", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es dr\u00f6hnt die bebende Haide, Staub wirbelt dr\u00fcber her.", "tokens": ["Es", "dr\u00f6hnt", "die", "be\u00b7ben\u00b7de", "Hai\u00b7de", ",", "Staub", "wir\u00b7belt", "dr\u00fc\u00b7ber", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch sieh, mit flatternder M\u00e4hne l\u00e4uft dort ein lediges Ro\u00df, \u2013", "tokens": ["Doch", "sieh", ",", "mit", "flat\u00b7tern\u00b7der", "M\u00e4h\u00b7ne", "l\u00e4uft", "dort", "ein", "le\u00b7di\u00b7ges", "Ro\u00df", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wie's schnaubt, wie scheu es blicket! nun sprengt's durch den wirren Tro\u00df,", "tokens": ["Wie's", "schnaubt", ",", "wie", "scheu", "es", "bli\u00b7cket", "!", "nun", "sprengt's", "durch", "den", "wir\u00b7ren", "Tro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Halt an, erfa\u00dft den Z\u00fcgel! wo sank der Reiter hin?", "tokens": ["Halt", "an", ",", "er\u00b7fa\u00dft", "den", "Z\u00fc\u00b7gel", "!", "wo", "sank", "der", "Rei\u00b7ter", "hin", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$.", "PWAV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O Gott, dort liegt im Blute die edle Herzogin.", "tokens": ["O", "Gott", ",", "dort", "liegt", "im", "Blu\u00b7te", "die", "ed\u00b7le", "Her\u00b7zo\u00b7gin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Es lehnt ihr bleiches Antlitz sanft in des Gatten Schoo\u00df,", "tokens": ["Es", "lehnt", "ihr", "blei\u00b7ches", "Ant\u00b7litz", "sanft", "in", "des", "Gat\u00b7ten", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So bla\u00df wie Abendwolken, wenn Sp\u00e4troth l\u00e4ngst zerflo\u00df;", "tokens": ["So", "bla\u00df", "wie", "Ab\u00b7end\u00b7wol\u00b7ken", ",", "wenn", "Sp\u00e4t\u00b7roth", "l\u00e4ngst", "zer\u00b7flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,", "KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ach wie in rother Str\u00f6mung der Lebensquell verspr\u00fcht,", "tokens": ["Ach", "wie", "in", "ro\u00b7ther", "Str\u00f6\u00b7mung", "der", "Le\u00b7bens\u00b7quell", "ver\u00b7spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie reich die blut'ge Rose ihr aus dem Herzen bl\u00fcht!", "tokens": ["Wie", "reich", "die", "blut'\u00b7ge", "Ro\u00b7se", "ihr", "aus", "dem", "Her\u00b7zen", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein Kinderpaar an der Leiche,", "tokens": ["Ein", "Kin\u00b7der\u00b7paar", "an", "der", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beugt z\u00e4rtlich \u00fcber die bleiche, entseelte Mutter sich;", "tokens": ["Beugt", "z\u00e4rt\u00b7lich", "\u00fc\u00b7ber", "die", "blei\u00b7che", ",", "ent\u00b7seel\u00b7te", "Mut\u00b7ter", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PRF", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So neigen zwei Rosenknospen, ", "tokens": ["So", "nei\u00b7gen", "zwei", "Ro\u00b7sen\u00b7knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich \u00fcber die Mutterrose, die sturmentbl\u00e4ttert verbl\u00fcht.", "tokens": ["Sich", "\u00fc\u00b7ber", "die", "Mut\u00b7ter\u00b7ro\u00b7se", ",", "die", "stur\u00b7ment\u00b7bl\u00e4t\u00b7tert", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Mit traurig gesenktem K\u00f6pfchen, im blutget\u00fcnchten Gras,", "tokens": ["Mit", "trau\u00b7rig", "ge\u00b7senk\u00b7tem", "K\u00f6pf\u00b7chen", ",", "im", "blut\u00b7ge\u00b7t\u00fcnch\u00b7ten", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Als Tr\u00f6ster ihr zur Seite der Dominikaner sa\u00df;", "tokens": ["Als", "Tr\u00f6s\u00b7ter", "ihr", "zur", "Sei\u00b7te", "der", "Do\u00b7mi\u00b7ni\u00b7ka\u00b7ner", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Wollt ihr sein Spr\u00fcchlein wissen? sie selbst hat ihn's gelehrt,", "tokens": ["Wollt", "ihr", "sein", "Spr\u00fcch\u00b7lein", "wis\u00b7sen", "?", "sie", "selbst", "hat", "ihn's", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.10": {"line.1": {"text": "Als Lenz die Erde wieder im ersten Ku\u00df umschlo\u00df,", "tokens": ["Als", "Lenz", "die", "Er\u00b7de", "wie\u00b7der", "im", "ers\u00b7ten", "Ku\u00df", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ritt aus Br\u00fcgge's Thoren ein bunter J\u00e4gertro\u00df,", "tokens": ["Da", "ritt", "aus", "Br\u00fcg\u00b7ge's", "Tho\u00b7ren", "ein", "bun\u00b7ter", "J\u00e4\u00b7ger\u00b7tro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Viel schmucke Falkoniere sah man zu Rosse ziehn,", "tokens": ["Viel", "schmu\u00b7cke", "Fal\u00b7ko\u00b7nie\u00b7re", "sah", "man", "zu", "Ros\u00b7se", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und an des Gatten Seite die sch\u00f6ne Herzogin.", "tokens": ["Und", "an", "des", "Gat\u00b7ten", "Sei\u00b7te", "die", "sch\u00f6\u00b7ne", "Her\u00b7zo\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Am Arm sa\u00df