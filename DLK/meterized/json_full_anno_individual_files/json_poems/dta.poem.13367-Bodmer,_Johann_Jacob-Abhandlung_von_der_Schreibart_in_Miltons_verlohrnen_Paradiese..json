{"dta.poem.13367": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abhandlung  \n von der Schreibart in Miltons  \n verlohrnen Paradiese.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast getrost durch sie, und k\u00fchn durch eigne Kraft,", "tokens": ["Du", "hast", "ge\u00b7trost", "durch", "sie", ",", "und", "k\u00fchn", "durch", "eig\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPER", "$,", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon oft den G\u00f6zendienst des Wahnes abgeschafft.", "tokens": ["Schon", "oft", "den", "G\u00f6\u00b7zen\u00b7dienst", "des", "Wah\u00b7nes", "ab\u00b7ge\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}