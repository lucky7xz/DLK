+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Billigt seine Lebensart;", "tokens": ["Bil\u00b7ligt", "sei\u00b7ne", "Le\u00b7ben\u00b7sart", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn er ist, wie ich gelesen,", "tokens": ["Denn", "er", "ist", ",", "wie", "ich", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Vater eines Kinds gewesen,", "tokens": ["Va\u00b7ter", "ei\u00b7nes", "Kinds", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Eh' er Kirchenvater ward.", "tokens": ["Eh'", "er", "Kir\u00b7chen\u00b7va\u00b7ter", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}