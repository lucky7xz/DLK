{"dta.poem.20517": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Basimenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr waffen gute nacht! Es mu\u00df gestorben seyn.", "tokens": ["Ihr", "waf\u00b7fen", "gu\u00b7te", "nacht", "!", "Es", "mu\u00df", "ge\u00b7stor\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein leben hat ein ende/", "tokens": ["Mein", "le\u00b7ben", "hat", "ein", "en\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo ich mich hinwende/", "tokens": ["Und", "wo", "ich", "mich", "hin\u00b7wen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Brennt nichts als schwefel herber pein.", "tokens": ["Brennt", "nichts", "als", "schwe\u00b7fel", "her\u00b7ber", "pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn trotz und hoffart/ die den ersten mensch betrogen/", "tokens": ["Denn", "trotz", "und", "hof\u00b7fart", "/", "die", "den", "ers\u00b7ten", "mensch", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "VVFIN", "$(", "ART", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat meiner hoffnung auch ihr paradis entzogen.", "tokens": ["Hat", "mei\u00b7ner", "hoff\u00b7nung", "auch", "ihr", "pa\u00b7ra\u00b7dis", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es mu\u00df gestorben seyn. Mein hertze nur gedult/", "tokens": ["Es", "mu\u00df", "ge\u00b7stor\u00b7ben", "seyn", ".", "Mein", "hert\u00b7ze", "nur", "ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$.", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fluche nicht der liebe;", "tokens": ["Und", "flu\u00b7che", "nicht", "der", "lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn da\u00df ich mich betr\u00fcbe/", "tokens": ["Denn", "da\u00df", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist meine mehr als ihre schuld.", "tokens": ["Ist", "mei\u00b7ne", "mehr", "als", "ih\u00b7re", "schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PIAT", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hat mir korn gestreut/ ich disteln auffgelesen;", "tokens": ["Sie", "hat", "mir", "korn", "ge\u00b7streut", "/", "ich", "dis\u00b7teln", "auff\u00b7ge\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVPP", "$(", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie ist mein sonnenschein/ ich mein Comet gewesen.", "tokens": ["Sie", "ist", "mein", "son\u00b7nen\u00b7schein", "/", "ich", "mein", "Co\u00b7met", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPER", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}, "stanza.3": {"line.1": {"text": "Mein hertze nur gedult! Denn mit dem tode kan", "tokens": ["Mein", "hert\u00b7ze", "nur", "ge\u00b7dult", "!", "Denn", "mit", "dem", "to\u00b7de", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$.", "KON", "APPR", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch deine s\u00fcnde sterben.", "tokens": ["Auch", "dei\u00b7ne", "s\u00fcn\u00b7de", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den meisten/ die verderben/", "tokens": ["Den", "meis\u00b7ten", "/", "die", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Klebt noch in gr\u00e4bern unflat an:", "tokens": ["Klebt", "noch", "in", "gr\u00e4\u00b7bern", "un\u00b7flat", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein urthel aber ist nunmehr entzwey gerissen/", "tokens": ["Dein", "ur\u00b7thel", "a\u00b7ber", "ist", "nun\u00b7mehr", "ent\u00b7zwey", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ADV", "PTKVZ", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil Basimene nicht darff mein verbrechen wissen.", "tokens": ["Weil", "Ba\u00b7si\u00b7me\u00b7ne", "nicht", "darff", "mein", "ver\u00b7bre\u00b7chen", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "VMFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du aber harter stein/ der dieses zimmer deckt/", "tokens": ["Du", "a\u00b7ber", "har\u00b7ter", "stein", "/", "der", "die\u00b7ses", "zim\u00b7mer", "deckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$(", "ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir bring ich meine klagen/", "tokens": ["Dir", "bring", "ich", "mei\u00b7ne", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir will ich \u00e4rmster sagen/", "tokens": ["Dir", "will", "ich", "\u00e4rms\u00b7ter", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df mich ihr plitz hat angesteckt/", "tokens": ["Da\u00df", "mich", "ihr", "plitz", "hat", "an\u00b7ge\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mein verschwiegnes hertz zwar schlechten ruhm erwirbet/", "tokens": ["Und", "mein", "ver\u00b7schwieg\u00b7nes", "hertz", "zwar", "schlech\u00b7ten", "ruhm", "er\u00b7wir\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch aber auch f\u00fcr nichts als Basimenen stirbet.", "tokens": ["Doch", "a\u00b7ber", "auch", "f\u00fcr", "nichts", "als", "Ba\u00b7si\u00b7me\u00b7nen", "stir\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}