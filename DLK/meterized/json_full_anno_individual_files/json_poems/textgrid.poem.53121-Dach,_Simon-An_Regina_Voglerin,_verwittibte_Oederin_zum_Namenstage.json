{"textgrid.poem.53121": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "An Regina Voglerin, verwittibte Oederin zum Namenstage", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt euch ja, Frau Muhm, des Trauerns nun entschlagen,", "tokens": ["Ihr", "habt", "euch", "ja", ",", "Frau", "Muhm", ",", "des", "Trau\u00b7erns", "nun", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "NN", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00dft ruhen, was schon tod den langen Ruhtag h\u00e4lt,", "tokens": ["La\u00dft", "ru\u00b7hen", ",", "was", "schon", "tod", "den", "lan\u00b7gen", "Ruh\u00b7tag", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "PRELS", "ADV", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd habet eure Seel in Gottes Hand gestellt.", "tokens": ["Vnd", "ha\u00b7bet", "eu\u00b7re", "Seel", "in", "Got\u00b7tes", "Hand", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wohlan! so m\u00fcsst ihr nun auch nach Ergetzung fragen.", "tokens": ["Wo\u00b7hlan", "!", "so", "m\u00fcsst", "ihr", "nun", "auch", "nach", "Er\u00b7get\u00b7zung", "fra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und seht, das Morgenlicht wird euern Namen tragen.", "tokens": ["Und", "seht", ",", "das", "Mor\u00b7gen\u00b7licht", "wird", "eu\u00b7ern", "Na\u00b7men", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entrei\u00dft euch recht der Pein, die Sinn und Leben f\u00e4llt,", "tokens": ["Ent\u00b7rei\u00dft", "euch", "recht", "der", "Pein", ",", "die", "Sinn", "und", "Le\u00b7ben", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Treibt alles Ungemach in eine fremde Welt.", "tokens": ["Treibt", "al\u00b7les", "Un\u00b7ge\u00b7mach", "in", "ei\u00b7ne", "frem\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir Freunde kommen, euch ein Lust Mahl anzusagen.", "tokens": ["Wir", "Freun\u00b7de", "kom\u00b7men", ",", "euch", "ein", "Lust", "Mahl", "an\u00b7zu\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,", "PPER", "ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nehmt von uns allen hin f\u00fcr eure werthe Hand", "tokens": ["Nehmt", "von", "uns", "al\u00b7len", "hin", "f\u00fcr", "eu\u00b7re", "wert\u00b7he", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "PIAT", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zeugn\u00fc\u00df unsrer Gunst dies schlecht gewundne Band,", "tokens": ["Ein", "Zeug\u00b7n\u00fc\u00df", "uns\u00b7rer", "Gunst", "dies", "schlecht", "ge\u00b7wund\u00b7ne", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PDS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch mehr, ein treues Hertz, sucht bald euch lo\u00df zu machen.", "tokens": ["Doch", "mehr", ",", "ein", "treu\u00b7es", "Hertz", ",", "sucht", "bald", "euch", "lo\u00df", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seht einst den tr\u00fcben Herbst, der jetzt auch SonnenSchein", "tokens": ["Seht", "einst", "den", "tr\u00fc\u00b7ben", "Herbst", ",", "der", "jetzt", "auch", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach vielem Regen giebt. Der Mensch kan einst der Pein,", "tokens": ["Nach", "vie\u00b7lem", "Re\u00b7gen", "giebt", ".", "Der", "Mensch", "kan", "einst", "der", "Pein",