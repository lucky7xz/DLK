{"textgrid.poem.65648": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Bund mit Gott", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein K\u00f6nig und kein Kaiser auf dieser Erde Rund", "tokens": ["Kein", "K\u00f6\u00b7nig", "und", "kein", "Kai\u00b7ser", "auf", "die\u00b7ser", "Er\u00b7de", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Will uns die Rechte reichen, zu schlie\u00dfen einen Bund.", "tokens": ["Will", "uns", "die", "Rech\u00b7te", "rei\u00b7chen", ",", "zu", "schlie\u00b7\u00dfen", "ei\u00b7nen", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie haben ihre Heere gesandt bis an den Pruth,", "tokens": ["Sie", "ha\u00b7ben", "ih\u00b7re", "Hee\u00b7re", "ge\u00b7sandt", "bis", "an", "den", "Pru\u00b7th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Es segeln ihre Flotten durch unsre Meeresfluth,", "tokens": ["Es", "se\u00b7geln", "ih\u00b7re", "Flot\u00b7ten", "durch", "uns\u00b7re", "Mee\u00b7res\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie sehn die Wogen gl\u00fchen von unsres Blutes Roth,", "tokens": ["Sie", "sehn", "die", "Wo\u00b7gen", "gl\u00fc\u00b7hen", "von", "uns\u00b7res", "Blu\u00b7tes", "Roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sie schauen unsre Thaten und h\u00f6ren unsre Noth;", "tokens": ["Sie", "schau\u00b7en", "uns\u00b7re", "Tha\u00b7ten", "und", "h\u00f6\u00b7ren", "uns\u00b7re", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Doch tauber, als die Woge, die ihre Schiffe tr\u00e4gt,", "tokens": ["Doch", "tau\u00b7ber", ",", "als", "die", "Wo\u00b7ge", ",", "die", "ih\u00b7re", "Schif\u00b7fe", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Doch h\u00e4rter, als die Klippe, die Kiel und Mast zerschl\u00e4gt,", "tokens": ["Doch", "h\u00e4r\u00b7ter", ",", "als", "die", "Klip\u00b7pe", ",", "die", "Kiel", "und", "Mast", "zer\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Sind sie vorbeigesegelt, als ", "tokens": ["Sind", "sie", "vor\u00b7bei\u00b7ge\u00b7se\u00b7gelt", ",", "als"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Des Meeres Ungeheuer aufschreckt' im tiefsten Sand,", "tokens": ["Des", "Mee\u00b7res", "Un\u00b7ge\u00b7heu\u00b7er", "auf\u00b7schreckt'", "im", "tiefs\u00b7ten", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wo sie der Ruhe pflogen nach ihrem Paschenschmaus", "tokens": ["Wo", "sie", "der", "Ru\u00b7he", "pflo\u00b7gen", "nach", "ih\u00b7rem", "Pa\u00b7schen\u00b7schmaus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Von s\u00fc\u00dfem S\u00e4uglingsfleische. Sie stierten wild heraus", "tokens": ["Von", "s\u00fc\u00b7\u00dfem", "S\u00e4ug\u00b7lings\u00b7flei\u00b7sche", ".", "Sie", "stier\u00b7ten", "wild", "he\u00b7raus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Aus feuerhellen Wogen, und um sie hin und her", "tokens": ["Aus", "feu\u00b7er\u00b7hel\u00b7len", "Wo\u00b7gen", ",", "und", "um", "sie", "hin", "und", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Da schwammen frische Leichen und reizten sie nicht mehr.", "tokens": ["Da", "schwam\u00b7men", "fri\u00b7sche", "Lei\u00b7chen", "und", "reiz\u00b7ten", "sie", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sie sind vorbeigesegelt. Der Herr hat es gesehn.", "tokens": ["Sie", "sind", "vor\u00b7bei\u00b7ge\u00b7se\u00b7gelt", ".", "Der", "Herr", "hat", "es", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+---+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da sandt' er Feuerstr\u00f6me herab aus seinen H\u00f6hn \u2013", "tokens": ["Da", "sandt'", "er", "Feu\u00b7er\u00b7str\u00f6\u00b7me", "her\u00b7ab", "aus", "sei\u00b7nen", "H\u00f6hn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Wohin zielt seine Rechte? Wen meint der Flammenstrahl?", "tokens": ["Wo\u00b7hin", "zielt", "sei\u00b7ne", "Rech\u00b7te", "?", "Wen", "meint", "der", "Flam\u00b7men\u00b7strahl", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Des W\u00fcrgers stolze Flotte fliegt auf in Blitz und Knall,", "tokens": ["Des", "W\u00fcr\u00b7gers", "stol\u00b7ze", "Flot\u00b7te", "fliegt", "auf", "in", "Blitz", "und", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da\u00df donnernd wiederhallen die Berge rund umher,", "tokens": ["Da\u00df", "don\u00b7nernd", "wie\u00b7der\u00b7hal\u00b7len", "die", "Ber\u00b7ge", "rund", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und aus den tiefsten H\u00f6hlen aufbraust das weite Meer.", "tokens": ["Und", "aus", "den", "tiefs\u00b7ten", "H\u00f6h\u00b7len", "auf\u00b7braust", "das", "wei\u00b7te", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Seht, und den W\u00fcrger schleudert ein h\u00f6llenrother Brand", "tokens": ["Seht", ",", "und", "den", "W\u00fcr\u00b7ger", "schleu\u00b7dert", "ein", "h\u00f6l\u00b7len\u00b7ro\u00b7ther", "Brand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Von seinem weichen Polster hin\u00fcber an den Strand,", "tokens": ["Von", "sei\u00b7nem", "wei\u00b7chen", "Pols\u00b7ter", "hin\u00b7\u00fc\u00b7ber", "an", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Wo nicht so viel des Bodens von Blut geblieben rein,", "tokens": ["Wo", "nicht", "so", "viel", "des", "Bo\u00b7dens", "von", "Blut", "ge\u00b7blie\u00b7ben", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ADV", "ART", "NN", "APPR", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Um ihm im letzten R\u00f6cheln ein trocknes Bett zu sein.", "tokens": ["Um", "ihm", "im", "letz\u00b7ten", "R\u00f6\u00b7cheln", "ein", "trock\u00b7nes", "Bett", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "So segelt denn vor\u00fcber und danket Gott dem Herrn,", "tokens": ["So", "se\u00b7gelt", "denn", "vor\u00b7\u00fc\u00b7ber", "und", "dan\u00b7ket", "Gott", "dem", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und was ihr habt gesehen, das meldet nah und fern,", "tokens": ["Und", "was", "ihr", "habt", "ge\u00b7se\u00b7hen", ",", "das", "mel\u00b7det", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "VVPP", "$,", "PDS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Und machet euren Herrschern die Wunderbotschaft kund:", "tokens": ["Und", "ma\u00b7chet", "eu\u00b7ren", "Herr\u00b7schern", "die", "Wun\u00b7der\u00b7bot\u00b7schaft", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Gott hat mit Hellas S\u00f6hnen geschlossen einen Bund,", "tokens": ["Gott", "hat", "mit", "Hel\u00b7las", "S\u00f6h\u00b7nen", "ge\u00b7schlos\u00b7sen", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "NN", "VVPP", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Den heil'gen Bund der Liebe auf Leben und auf Tod,", "tokens": ["Den", "heil'\u00b7gen", "Bund", "der", "Lie\u00b7be", "auf", "Le\u00b7ben", "und", "auf", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Dem H\u00f6ll' und Welt vergebens mit Gold und Eisen droht.", "tokens": ["Dem", "H\u00f6ll'", "und", "Welt", "ver\u00b7ge\u00b7bens", "mit", "Gold", "und", "Ei\u00b7sen", "droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Der heil'ge Bund wird halten, ob alle untergehn,", "tokens": ["Der", "heil'\u00b7ge", "Bund", "wird", "hal\u00b7ten", ",", "ob", "al\u00b7le", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,", "KOUS", "PIS", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Wird mit uns triumphirend einst aus dem Grab erstehn.", "tokens": ["Wird", "mit", "uns", "tri\u00b7um\u00b7phi\u00b7rend", "einst", "aus", "dem", "Grab", "