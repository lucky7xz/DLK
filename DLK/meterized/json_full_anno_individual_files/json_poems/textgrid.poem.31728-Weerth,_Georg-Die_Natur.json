{"textgrid.poem.31728": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur, mit deinen strahlenden Kolossen,", "tokens": ["Na\u00b7tur", ",", "mit", "dei\u00b7nen", "strah\u00b7len\u00b7den", "Ko\u00b7los\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die du die Ewigkeit zur Dauer nahmst:", "tokens": ["Die", "du", "die", "E\u00b7wig\u00b7keit", "zur", "Dau\u00b7er", "nahmst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur zur Vollendung bist du erst ersprossen,", "tokens": ["Nur", "zur", "Vol\u00b7len\u00b7dung", "bist", "du", "erst", "ers\u00b7pros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Seit du im Menschen zum Bewu\u00dftsein kamst,", "tokens": ["Seit", "du", "im", "Men\u00b7schen", "zum", "Be\u00b7wu\u00df\u00b7tsein", "kamst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Menschen nur, des st\u00fcrmende Gedanken", "tokens": ["Im", "Men\u00b7schen", "nur", ",", "des", "st\u00fcr\u00b7men\u00b7de", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Der Freiheit wunderbarstes Gut geraubt,", "tokens": ["Der", "Frei\u00b7heit", "wun\u00b7der\u00b7bars\u00b7tes", "Gut", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der auf den Tr\u00fcmmern jetzt von Trug und Schranken,", "tokens": ["Der", "auf", "den", "Tr\u00fcm\u00b7mern", "jetzt", "von", "Trug", "und", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein eigner Gott, an dich, an sich nur glaubt.", "tokens": ["Sein", "eig\u00b7ner", "Gott", ",", "an", "dich", ",", "an", "sich", "nur", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "PPER", "$,", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohl mag sein Auge keck den Himmel fragen,", "tokens": ["Wohl", "mag", "sein", "Au\u00b7ge", "keck", "den", "Him\u00b7mel", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Sonn an Sonne wirbelnd sich bewegt:", "tokens": ["Wenn", "Sonn", "an", "Son\u00b7ne", "wir\u00b7belnd", "sich", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVPP", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbihr andern Welten, habt ihr je getragen", "tokens": ["\u00bb", "ihr", "an\u00b7dern", "Wel\u00b7ten", ",", "habt", "ihr", "je", "ge\u00b7tra\u00b7gen"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein solches Kleinod, wie die Erde tr\u00e4gt?", "tokens": ["Ein", "sol\u00b7ches", "Klei\u00b7nod", ",", "wie", "die", "Er\u00b7de", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Trugt Menschen ihr, die trotz der grausen Zweifel,", "tokens": ["Trugt", "Men\u00b7schen", "ihr", ",", "die", "trotz", "der", "grau\u00b7sen", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wild zersplittert ihre beste Kraft,", "tokens": ["Die", "wild", "zer\u00b7split\u00b7tert", "ih\u00b7re", "bes\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch stets zum Kampfe mit dem alten Teufel,", "tokens": ["Doch", "stets", "zum", "Kamp\u00b7fe", "mit", "dem", "al\u00b7ten", "Teu\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Wahne, k\u00fchn zusammen sich gerafft?", "tokens": ["Dem", "Wah\u00b7ne", ",", "k\u00fchn", "zu\u00b7sam\u00b7men", "sich", "ge\u00b7rafft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und die gesiegt!\u00ab \u2013 Wohlan, Sieg und Triumphe", "tokens": ["Und", "die", "ge\u00b7siegt", "!", "\u00ab", "\u2013", "Wo\u00b7hlan", ",", "Sieg", "und", "Tri\u00b7um\u00b7phe"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "VVPP", "$.", "$(", "$(", "ADV", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "La\u00dft schmettern eurer Krieger vollsten Chor!", "tokens": ["La\u00dft", "schmet\u00b7tern", "eu\u00b7rer", "Krie\u00b7ger", "volls\u00b7ten", "Chor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es trug der Mensch aus tausendj\u00e4hr'gem Sumpfe", "tokens": ["Es", "trug", "der", "Mensch", "aus", "tau\u00b7send\u00b7j\u00e4hr'\u00b7gem", "Sump\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit jubelnd an das Licht empor.", "tokens": ["Die", "Frei\u00b7heit", "ju\u00b7belnd", "an", "das", "Licht", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was fr\u00fche V\u00f6lker ahnend vorempfunden,", "tokens": ["Was", "fr\u00fc\u00b7he", "V\u00f6l\u00b7ker", "ah\u00b7nend", "vor\u00b7emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er freut sich dessen in bacchant'scher Lust;", "tokens": ["Er", "freut", "sich", "des\u00b7sen", "in", "bac\u00b7chant'\u00b7scher", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er hat den gr\u00f6\u00dften Riesen \u00fcberwunden,", "tokens": ["Er", "hat", "den", "gr\u00f6\u00df\u00b7ten", "Rie\u00b7sen", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vertilgt den Zweifel seiner eignen Brust!", "tokens": ["Ver\u00b7tilgt", "den", "Zwei\u00b7fel", "sei\u00b7ner", "eig\u00b7nen", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der einst dem Feuer seine Kniee beugte,", "tokens": ["Der", "einst", "dem", "Feu\u00b7er", "sei\u00b7ne", "Kni\u00b7ee", "beug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hekatomben opfernd niederschlug,", "tokens": ["Der", "He\u00b7ka\u00b7tom\u00b7ben", "op\u00b7fernd", "nie\u00b7der\u00b7schlug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der einen Gott auf Sinai erzeugte \u2013", "tokens": ["Der", "ei\u00b7nen", "Gott", "auf", "Si\u00b7nai", "er\u00b7zeug\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Triumph! \u2013 der hat jetzt an sich selbst genug!", "tokens": ["Tri\u00b7umph", "!", "\u2013", "der", "hat", "jetzt", "an", "sich", "selbst", "ge\u00b7nug", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "VAFIN", "ADV", "APPR", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie der Kranich liebt die Wolkenbahnen,", "tokens": ["Und", "wie", "der", "Kra\u00b7nich", "liebt", "die", "Wol\u00b7ken\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie der L\u00f6we liebt der W\u00fcste Spur:", "tokens": ["Und", "wie", "der", "L\u00f6\u00b7we", "liebt", "der", "W\u00fcs\u00b7te", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So liebt der Mensch die Fluren seiner Ahnen", "tokens": ["So", "liebt", "der", "Mensch", "die", "Flu\u00b7ren", "sei\u00b7ner", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und weilt entz\u00fcckt auf seiner Erde nur.", "tokens": ["Und", "weilt", "ent\u00b7z\u00fcckt", "auf", "sei\u00b7ner", "Er\u00b7de", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ob Millionen wandeln auch im Dunkeln \u2013", "tokens": ["Ob", "Mil\u00b7lion\u00b7en", "wan\u00b7deln", "auch", "im", "Dun\u00b7keln", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Das Jahr entrollt! \u2013 Es leuchtet sonder Wahl", "tokens": ["Das", "Jahr", "ent\u00b7rollt", "!", "\u2013", "Es", "leuch\u00b7tet", "son\u00b7der", "Wahl"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "$(", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Stern der neuen Zeit, hell wird er funkeln", "tokens": ["Der", "Stern", "der", "neu\u00b7en", "Zeit", ",", "hell", "wird", "er", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die Priester dieser Tage fordern Knechte", "tokens": ["Die", "Pries\u00b7ter", "die\u00b7ser", "Ta\u00b7ge", "for\u00b7dern", "Knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Sklaven nicht \u2013 sie fordern laut und frei,", "tokens": ["Und", "Skla\u00b7ven", "nicht", "\u2013", "sie", "for\u00b7dern", "laut", "und", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "$(", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df jeder, treu dem angestammten Rechte,", "tokens": ["Da\u00df", "je\u00b7der", ",", "treu", "dem", "an\u00b7ge\u00b7stamm\u00b7ten", "Rech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinfort ein Mensch mit freien Menschen sei.", "tokens": ["Hin\u00b7fort", "ein", "Mensch", "mit", "frei\u00b7en", "Men\u00b7schen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Natur, mit deinen strahlenden Kolossen,", "tokens": ["Na\u00b7tur", ",", "mit", "dei\u00b7nen", "strah\u00b7len\u00b7den", "Ko\u00b7los\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die du die Ewigkeit zur Dauer nahmst:", "tokens": ["Die", "du", "die", "E\u00b7wig\u00b7keit", "zur", "Dau\u00b7er", "nahmst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur zur Vollendung bist du erst ersprossen,", "tokens": ["Nur", "zur", "Vol\u00b7len\u00b7dung", "bist", "du", "erst", "ers\u00b7pros\