{"textgrid.poem.25893": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gerne liege ich im Grase horchend,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne liege ich im Grase horchend,", "tokens": ["Ger\u00b7ne", "lie\u00b7ge", "ich", "im", "Gra\u00b7se", "hor\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn die Winde hohen B\u00e4umen Ausdruck geben,", "tokens": ["Wenn", "die", "Win\u00b7de", "ho\u00b7hen", "B\u00e4u\u00b7men", "Aus\u00b7druck", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Da\u00df die Zweige Menschengesten zeigen,", "tokens": ["Da\u00df", "die", "Zwei\u00b7ge", "Men\u00b7schen\u00b7ges\u00b7ten", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Bl\u00e4tter seelenvoll wie Menschenh\u00e4nde leben.", "tokens": ["Und", "die", "Bl\u00e4t\u00b7ter", "see\u00b7len\u00b7voll", "wie", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Niemand wei\u00df es, wo die Winde wohnen,", "tokens": ["Nie\u00b7mand", "wei\u00df", "es", ",", "wo", "die", "Win\u00b7de", "woh\u00b7nen", ","], "token_i