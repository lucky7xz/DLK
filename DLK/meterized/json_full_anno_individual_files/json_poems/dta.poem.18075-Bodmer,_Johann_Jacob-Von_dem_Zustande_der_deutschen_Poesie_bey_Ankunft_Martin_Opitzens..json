{"dta.poem.18075": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Zustande der deutschen  \n Poesie bey Ankunft Martin Opitzens.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Morgens eh s'Tages Schein anbricht", "tokens": ["Mor\u00b7gens", "eh", "s'\u00b7Ta\u00b7ges", "Schein", "an\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit Purpurfarbem hellem Licht,", "tokens": ["Mit", "Pur\u00b7purf\u00b7ar\u00b7bem", "hel\u00b7lem", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gl\u00e4ntzt herf\u00fcr deins Mundes R\u00f6th,", "tokens": ["So", "gl\u00e4ntzt", "her\u00b7f\u00fcr", "deins", "Mun\u00b7des", "R\u00f6th", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie vor der Sonn die Morgenr\u00f6th", "tokens": ["Wie", "vor", "der", "Sonn", "die", "Mor\u00b7gen\u00b7r\u00f6th"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit rosinfarben Wangen", "tokens": ["Mit", "ro\u00b7sin\u00b7far\u00b7ben", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "H\u00fcbsch, lustig, klar, aufgangen.", "tokens": ["H\u00fcbsch", ",", "lus\u00b7tig", ",", "klar", ",", "auf\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein sch\u00f6n karfuncklet h\u00fcbsch Gestirn", "tokens": ["Ein", "sch\u00f6n", "kar\u00b7funck\u00b7let", "h\u00fcbsch", "Ge\u00b7stirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein\u2019 Aeuglein sind an deiner Stirn;", "tokens": ["Dein'", "A\u00b7e\u00b7u\u00b7glein", "sind", "an", "dei\u00b7ner", "Stirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein g\u00fcldnes Haupt ist wohl formirt,", "tokens": ["Dein", "g\u00fcld\u00b7nes", "Haupt", "ist", "wohl", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie s\u2019Himmels Runde sch\u00f6n geziert.", "tokens": ["Wie", "s'\u00b7Him\u00b7mels", "Run\u00b7de", "sch\u00f6n", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Von deiner Aeuglein glitzen", "tokens": ["Von", "dei\u00b7ner", "A\u00b7e\u00b7u\u00b7glein", "glit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Feurflammen rausher spritzen.", "tokens": ["Feur\u00b7flam\u00b7men", "raus\u00b7her", "sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Recht wie die Straln und schnelle Pfeil", "tokens": ["Recht", "wie", "die", "Straln", "und", "schnel\u00b7le", "Pfeil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieselben schiessen her in Eil,", "tokens": ["Die\u00b7sel\u00b7ben", "schies\u00b7sen", "her", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Han mir versehrt mein junges Hertz,", "tokens": ["Han", "mir", "ver\u00b7sehrt", "mein", "jun\u00b7ges", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch lindert sich der br\u00fcnstig Schmertz\u2019,", "tokens": ["Doch", "lin\u00b7dert", "sich", "der", "br\u00fcns\u00b7tig", "Schmertz'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann ich dein Antlitz schaue,", "tokens": ["Wann", "ich", "dein", "Ant\u00b7litz", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Holdseligste Jungfraue.", "tokens": ["Hold\u00b7se\u00b7ligs\u00b7te", "Jung\u00b7frau\u00b7e", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gr\u00fcnest und bl\u00fchest aller Ding,", "tokens": ["Gr\u00fc\u00b7nest", "und", "bl\u00fc\u00b7hest", "al\u00b7ler", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gleichwie ein Lorbaum im Fr\u00fchling,", "tokens": ["Gleich\u00b7wie", "ein", "Lor\u00b7baum", "im", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie eine Tanne w\u00e4ch\u00dft gerad", "tokens": ["Wie", "ei\u00b7ne", "Tan\u00b7ne", "w\u00e4ch\u00dft", "ge\u00b7rad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein werther Leib in gleicher Wad:", "tokens": ["Dein", "wert\u00b7her", "Leib", "in", "glei\u00b7cher", "Wad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein\u2019 Arme beyd sind eben", "tokens": ["Dein'", "Ar\u00b7me", "beyd", "sind", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie zwo neuer Weinreben.", "tokens": ["Wie", "zwo", "neu\u00b7er", "Wein\u00b7re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ADJA", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach da\u00df, was anr\u00fchrn deine H\u00e4nd,", "tokens": ["Ach", "da\u00df", ",", "was", "an\u00b7r\u00fchrn", "dei\u00b7ne", "H\u00e4nd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In rothe R\u00f6slein sey gewendt,", "tokens": ["In", "ro\u00b7the", "R\u00f6s\u00b7lein", "sey", "ge\u00b7wendt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weisse Lilglein wachsen fein,", "tokens": ["Und", "weis\u00b7se", "Lil\u00b7glein", "wach\u00b7sen", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo deine F\u00fc\u00df hingangen seyn:", "tokens": ["Wo", "dei\u00b7ne", "F\u00fc\u00df", "hin\u00b7gan\u00b7gen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von deins Munds Athem s\u00fcsse", "tokens": ["Von", "deins", "Munds", "A\u00b7them", "s\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Braun Violbl\u00fct entsprisse.", "tokens": ["Braun", "Vi\u00b7ol\u00b7bl\u00fct", "ent\u00b7spris\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Ruhm, Prei\u00df, Ehr und Lob ich dir gib,", "tokens": ["Ruhm", ",", "Prei\u00df", ",", "Ehr", "und", "Lob", "ich", "dir", "gib", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PPER", "PPER", "VVIMP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "F\u00fcr andern alln, hertzliebstes Lieb,", "tokens": ["F\u00fcr", "an\u00b7dern", "alln", ",", "hertz\u00b7liebs\u00b7tes", "Lieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Tugend leucht an allem End,", "tokens": ["Dein", "Tu\u00b7gend", "leucht", "an", "al\u00b7lem", "End", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie der Vollmond am Firmament;", "tokens": ["Wie", "der", "Voll\u00b7mond", "am", "Fir\u00b7ma\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bist aller Jungfraun Zierde,", "tokens": ["Bist", "al\u00b7ler", "Jung\u00b7fraun", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Meins Hertzens innre Girde.", "tokens": ["Meins", "Hert\u00b7zens", "inn\u00b7re", "Gir\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Edler ich sch\u00e4tz dein Gunst und Hold,", "tokens": ["Ed\u00b7ler", "ich", "sch\u00e4tz", "dein", "Gunst", "und", "Hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dann Silber und das beste Gold;", "tokens": ["Dann", "Sil\u00b7ber", "und", "das", "bes\u00b7te", "Gold", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Freundlichkeit und sch\u00f6n Geberd", "tokens": ["Dein", "Freund\u00b7lich\u00b7keit", "und", "sch\u00f6n", "Ge\u00b7berd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mehr dann alle Perlen werth:", "tokens": ["Ist", "mehr", "dann", "al\u00b7le", "Per\u00b7len", "werth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es gilt dein Zucht viel reine", "tokens": ["Es", "gilt", "dein", "Zucht", "viel", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mehr dann all edle Steine.", "tokens": ["Mehr", "dann", "all", "ed\u00b7le", "Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Treuhertzig seyn ohn arge List,", "tokens": ["Treu\u00b7hert\u00b7zig", "seyn", "ohn", "ar\u00b7ge", "List", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der rechten Lieb Wahrzeichen ist:", "tokens": ["Der", "rech\u00b7ten", "Lieb", "Wahr\u00b7zei\u00b7chen", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann gleicher Ma\u00df nicht liebest mich,", "tokens": ["Wann", "glei\u00b7cher", "Ma\u00df", "nicht", "lie\u00b7best", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PTKNEG", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df zu, da\u00df ich nur liebe dich.", "tokens": ["La\u00df", "zu", ",", "da\u00df", "ich", "nur", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Hertz geb dir zu eigen,", "tokens": ["Mein", "Hertz", "geb", "dir", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Hertz zu mir thu neigen.", "tokens": ["Dein", "Hertz", "zu", "mir", "thu", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "All Augenblick dein eindenck bin", "tokens": ["All", "Au\u00b7gen\u00b7blick", "dein", "ein\u00b7denck", "bin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meim Gem\u00fcth und in meim Sinn.", "tokens": ["In", "meim", "Ge\u00b7m\u00fcth", "und", "in", "meim", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tags bist mir ein helle Sonn,", "tokens": ["Des", "Tags", "bist", "mir", "ein", "hel\u00b7le", "Sonn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Nachts ein klarscheinender Mon:", "tokens": ["Des", "Nachts", "ein", "klar\u00b7schei\u00b7nen\u00b7der", "Mon", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Thust (liebstes M.) mir geben", "tokens": ["Thust", "(", "liebs\u00b7tes", "M.", ")", "mir", "ge\u00b7ben"], "token_info": ["word", "punct", "word", "abbreviation", "punct", "word", "word"], "pos": ["NN", "$(", "ADJA", "NE", "$(", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Von deinem Glantz mein Leben.", "tokens": ["Von", "dei\u00b7nem", "Glantz", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}