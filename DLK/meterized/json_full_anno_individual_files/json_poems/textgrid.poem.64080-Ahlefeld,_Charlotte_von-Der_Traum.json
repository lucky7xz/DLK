{"textgrid.poem.64080": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein seltner Traum hielt magisch mich umfangen", "tokens": ["Ein", "selt\u00b7ner", "Traum", "hielt", "ma\u00b7gisch", "mich", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zauberte mir Wunderbilder vor.", "tokens": ["Und", "zau\u00b7ber\u00b7te", "mir", "Wun\u00b7der\u00b7bil\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Haines Wipfel rauschten, und es drangen", "tokens": ["Des", "Hai\u00b7nes", "Wip\u00b7fel", "rauschten", ",", "und", "es", "dran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sterne golden durch der Wolken Flor.", "tokens": ["Die", "Ster\u00b7ne", "gol\u00b7den", "durch", "der", "Wol\u00b7ken", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Meer war still, und in den weiten Fluthen", "tokens": ["Das", "Meer", "war", "still", ",", "und", "in", "den", "wei\u00b7ten", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verlohren sich der Abendr\u00f6the Gluthen.", "tokens": ["Ver\u00b7loh\u00b7ren", "sich", "der", "A\u00b7bend\u00b7r\u00f6\u00b7the", "Glu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wandelte allein am \u00f6den Strande,", "tokens": ["Ich", "wan\u00b7del\u00b7te", "al\u00b7lein", "am", "\u00f6\u00b7den", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und tief im Busen regte sich mein Schmerz.", "tokens": ["Und", "tief", "im", "Bu\u00b7sen", "reg\u00b7te", "sich", "mein", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich w\u00fcnschte mich zur\u00fcck in ferne Lande \u2013", "tokens": ["Ich", "w\u00fcnschte", "mich", "zu\u00b7r\u00fcck", "in", "fer\u00b7ne", "Lan\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Heimwehs Qualen f\u00fcllten bang mein Herz.", "tokens": ["Des", "Heim\u00b7wehs", "Qua\u00b7len", "f\u00fcll\u00b7ten", "bang", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den vollen Mond begr\u00fcssten meine Thr\u00e4nen,", "tokens": ["Den", "vol\u00b7len", "Mond", "be\u00b7gr\u00fcs\u00b7sten", "mei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn mich ergriff ein allgewaltig Sehnen.", "tokens": ["Denn", "mich", "er\u00b7griff", "ein", "all\u00b7ge\u00b7wal\u00b7tig", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da hob sich aus des Meeres dunkler Bl\u00e4ue", "tokens": ["Da", "hob", "sich", "aus", "des", "Mee\u00b7res", "dunk\u00b7ler", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein leichter Nebel neben mir empor,", "tokens": ["Ein", "leich\u00b7ter", "Ne\u00b7bel", "ne\u00b7ben", "mir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Und es erklang wie Geisterton der Weihe", "tokens": ["Und", "es", "er\u00b7klang", "wie", "Geis\u00b7ter\u00b7ton", "der", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Melodisch eine Sprache meinem Ohr,", "tokens": ["Me\u00b7lo\u00b7disch", "ei\u00b7ne", "Spra\u00b7che", "mei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie nimmer noch mein trunkner Sinn vernommen;", "tokens": ["Wie", "nim\u00b7mer", "noch", "mein", "trunk\u00b7ner", "Sinn", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie schien aus h\u00f6hern R\u00e4umen herzukommen.", "tokens": ["Sie", "schien", "aus", "h\u00f6\u00b7hern", "R\u00e4u\u00b7men", "her\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbwas seufzest Du mit bangen Klaget\u00f6nen", "tokens": ["\u00bb", "was", "seuf\u00b7zest", "Du", "mit", "ban\u00b7gen", "Kla\u00b7ge\u00b7t\u00f6\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um das verlassne, ferne Vaterland?", "tokens": ["Um", "das", "ver\u00b7lass\u00b7ne", ",", "fer\u00b7ne", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Deinem Loos Dich friedlich zu vers\u00f6hnen,", "tokens": ["Mit", "Dei\u00b7nem", "Loos", "Dich", "fried\u00b7lich", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat mich das Schicksal tr\u00f6stend Dir gesandt;", "tokens": ["Hat", "mich", "das", "Schick\u00b7sal", "tr\u00f6s\u00b7tend", "Dir", "ge\u00b7sandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So blicke denn mit kindlichem Vertrauen", "tokens": ["So", "bli\u00b7cke", "denn", "mit", "kind\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu jenen Sternenh\u00f6hen, die wir schauen.", "tokens": ["Zu", "je\u00b7nen", "Ster\u00b7nen\u00b7h\u00f6\u00b7hen", ",", "die", "wir", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort ist die Heimath, die, vom Wahn verblendet,", "tokens": ["Dort", "ist", "die", "Hei\u00b7math", ",", "die", ",", "vom", "Wahn", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sterbliche sich schon auf Erden tr\u00e4umt.", "tokens": ["Der", "Sterb\u00b7li\u00b7che", "sich", "schon", "auf", "Er\u00b7den", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erst wenn sein dumpfes Pflanzenleben endet,", "tokens": ["Erst", "wenn", "sein", "dum\u00b7pfes", "Pflan\u00b7zen\u00b7le\u00b7ben", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus der Ahndung ihm Erf\u00fcllung keimt \u2013", "tokens": ["Und", "aus", "der", "Ahn\u00b7dung", "ihm", "Er\u00b7f\u00fcl\u00b7lung", "keimt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erst dann gew\u00e4hrt der weisen Vorsicht Hand", "tokens": ["Erst", "dann", "ge\u00b7w\u00e4hrt", "der", "wei\u00b7sen", "Vor\u00b7sicht", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In jenen Sph\u00e4ren ihm ein \u00e4chtes Vaterland.\u00ab", "tokens": ["In", "je\u00b7nen", "Sph\u00e4\u00b7ren", "ihm", "ein", "\u00e4ch\u00b7tes", "Va\u00b7ter\u00b7land", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie Silberlaut' aus Harfensaiten quellen,", "tokens": ["Wie", "Sil\u00b7ber\u00b7laut'", "aus", "Har\u00b7fen\u00b7sai\u00b7ten", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So drang die Stimme tief mir in die Brust,", "tokens": ["So", "drang", "die", "Stim\u00b7me", "tief", "mir", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hob mich auf des Wohllauts goldnen Wellen", "tokens": ["Und", "hob", "mich", "auf", "des", "Wohl\u00b7lauts", "gold\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Bangigkeit zu neuer Lebenslust,", "tokens": ["Aus", "Ban\u00b7gig\u00b7keit", "zu", "neu\u00b7er", "Le\u00b7bens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und frischen Muth \u2013 des Daseyns sch\u00f6nste Bl\u00fcthe,", "tokens": ["Und", "fri\u00b7schen", "Muth", "\u2013", "des", "Da\u00b7seyns", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fchlt ich seitdem im ahnenden Gem\u00fcthe.", "tokens": ["F\u00fchlt", "ich", "seit\u00b7dem", "im", "ah\u00b7nen\u00b7den", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein seltner Traum hielt magisch mich umfangen", "tokens": ["Ein", "selt\u00b7ner", "Traum", "hielt", "ma\u00b7gisch", "mich", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}