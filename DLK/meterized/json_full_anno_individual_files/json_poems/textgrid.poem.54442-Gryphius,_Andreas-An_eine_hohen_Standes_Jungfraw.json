{"textgrid.poem.54442": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "An eine hohen Standes Jungfraw", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein wolgestalter Leib ist billich zuerheben/", "tokens": ["Ein", "wol\u00b7ge\u00b7stal\u00b7ter", "Leib", "ist", "bil\u00b7lich", "zu\u00b7er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch billicher/ wenn Er von Edlem Blut herr\u00fchrt/", "tokens": ["Noch", "bil\u00b7li\u00b7cher", "/", "wenn", "Er", "von", "Ed\u00b7lem", "Blut", "her\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd ein geschickte Seel in selbten einlosirt/", "tokens": ["Vnd", "ein", "ge\u00b7schick\u00b7te", "Seel", "in", "selb\u00b7ten", "ein\u00b7lo\u00b7sirt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Welch einig sich bem\u00fcht der Wei\u00dfheit nach zu streben/", "tokens": ["Welch", "ei\u00b7nig", "sich", "be\u00b7m\u00fcht", "der", "Wei\u00df\u00b7heit", "nach", "zu", "stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "PRF", "VVFIN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "lin