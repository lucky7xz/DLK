{"dta.poem.13752": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "nl:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Morgenleuchten kam geflogen,", "tokens": ["Mor\u00b7gen\u00b7leuch\u00b7ten", "kam", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Vater ward so bleich,", "tokens": ["Und", "der", "Va\u00b7ter", "ward", "so", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Adler hoch dar\u00fcber zogen", "tokens": ["Ad\u00b7ler", "hoch", "da\u00b7r\u00fc\u00b7ber", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "PAV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch das klare Himmelreich,", "tokens": ["Durch", "das", "kla\u00b7re", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der V\u00e4ter stiller Orden", "tokens": ["Und", "der", "V\u00e4\u00b7ter", "stil\u00b7ler", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sank zur Ruh in Ewigkeit,", "tokens": ["Sank", "zur", "Ruh", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steine, wie es lichte worden,", "tokens": ["Stei\u00b7ne", ",", "wie", "es", "lich\u00b7te", "wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Standen da im Hof' zerstreut.", "tokens": ["Stan\u00b7den", "da", "im", "Hof'", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}