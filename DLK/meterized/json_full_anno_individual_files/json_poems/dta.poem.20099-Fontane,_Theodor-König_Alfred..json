{"dta.poem.20099": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Alfred.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Der D\u00e4ne haust mit Mord und Brand", "tokens": ["Der", "D\u00e4\u00b7ne", "haust", "mit", "Mord", "und", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Wessex und Northumberland:", "tokens": ["In", "Wes\u00b7sex", "und", "Nor\u00b7thum\u00b7ber\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Held Alfred irrt im eignen Reich", "tokens": ["Held", "Al\u00b7fred", "irrt", "im", "eig\u00b7nen", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Umher, dem fl\u00fcchtgen Hirsche gleich.", "tokens": ["Um\u00b7her", ",", "dem", "fl\u00fccht\u00b7gen", "Hir\u00b7sche", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei Wolf und Elen tief im Wald,", "tokens": ["Bei", "Wolf", "und", "E\u00b7len", "tief", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da nimmt er seinen Aufenthalt,", "tokens": ["Da", "nimmt", "er", "sei\u00b7nen", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sammelt er, im Schutz der Nacht,", "tokens": ["Da", "sam\u00b7melt", "er", ",", "im", "Schutz", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Heer zu neuer Schlacht.", "tokens": ["Ein", "neu\u00b7es", "Heer", "zu", "neu\u00b7er", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als die Seinen kampfbereit,", "tokens": ["Und", "als", "die", "Sei\u00b7nen", "kampf\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PPOSS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da legt er an ein Harfnerkleid:", "tokens": ["Da", "legt", "er", "an", "ein", "Harf\u00b7ner\u00b7kleid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In\u2019s D\u00e4nenlager will er gehn", "tokens": ["In's", "D\u00e4\u00b7nen\u00b7la\u00b7ger", "will", "er", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Feindes Schw\u00e4chen auszusp\u00e4hn.", "tokens": ["Des", "Fein\u00b7des", "Schw\u00e4\u00b7chen", "aus\u00b7zu\u00b7sp\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schon tritt er k\u00fchn, die Harf\u2019 im Arm,", "tokens": ["Schon", "tritt", "er", "k\u00fchn", ",", "die", "Ha\u00b7rf'", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor K\u00f6nig Guthrums Zecherschwarm,", "tokens": ["Vor", "K\u00f6\u00b7nig", "Gut\u00b7hrums", "Ze\u00b7cher\u00b7schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald in der Becher Kling und Klang", "tokens": ["Bald", "in", "der", "Be\u00b7cher", "Kling", "und", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt K\u00f6nig Alfreds Schlachtgesang.", "tokens": ["T\u00f6nt", "K\u00f6\u00b7nig", "Al\u00b7freds", "Schlacht\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er singt von jenem Zechermahl,", "tokens": ["Er", "singt", "von", "je\u00b7nem", "Ze\u00b7cher\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo statt der Becher Stahl an Stahl", "tokens": ["Wo", "statt", "der", "Be\u00b7cher", "Stahl", "an", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In L\u00fcften klirrt, und Schild an Schild, \u2014", "tokens": ["In", "L\u00fcf\u00b7ten", "klirrt", ",", "und", "Schild", "an", "Schild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "KON", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Blut statt Wein in Str\u00f6men quillt.", "tokens": ["Wo", "Blut", "statt", "Wein", "in", "Str\u00f6\u00b7men", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er singt von jenem Zechermahl,", "tokens": ["Er", "singt", "von", "je\u00b7nem", "Ze\u00b7cher\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo \u201e", "tokens": ["Wo", "\u201e"], "token_info": ["word", "punct"], "pos": ["PWAV", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Kredenzt, und jeder der da trinkt", "tokens": ["Kre\u00b7denzt", ",", "und", "je\u00b7der", "der", "da", "trinkt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PIS", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alle Zeit zu Boden sinkt.", "tokens": ["F\u00fcr", "al\u00b7le", "Zeit", "zu", "Bo\u00b7den", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von seiner Ahnen Kraft und Krieg,", "tokens": ["Von", "sei\u00b7ner", "Ah\u00b7nen", "Kraft", "und", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hengist und dem Stamford-Sieg,", "tokens": ["Von", "Hen\u00b7gist", "und", "dem", "Stam\u00b7ford\u00b7Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Eglesford, wo Horsa fiel, \u2014", "tokens": ["Von", "Eg\u00b7les\u00b7ford", ",", "wo", "Hor\u00b7sa", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "NE", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Singt er ein Lied zum Saitenspiel.", "tokens": ["Singt", "er", "ein", "Lied", "zum", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der D\u00e4nenk\u00f6nig aber lacht", "tokens": ["Der", "D\u00e4\u00b7nen\u00b7k\u00f6\u00b7nig", "a\u00b7ber", "lacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl ob der Sachsen Muth und Macht,", "tokens": ["Wohl", "ob", "der", "Sach\u00b7sen", "Muth", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er lacht, und h\u00f6rt nicht wie das Lied", "tokens": ["Er", "lacht", ",", "und", "h\u00f6rt", "nicht", "wie", "das", "Lied"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Raben schon die Luft durchzieht.", "tokens": ["Der", "Ra\u00b7ben", "schon", "die", "Luft", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Er zecht und jubelt noch im Zelt,", "tokens": ["Er", "zecht", "und", "ju\u00b7belt", "noch", "im", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als schon der siegessichre Held", "tokens": ["Als", "schon", "der", "sie\u00b7ges\u00b7sich\u00b7re", "Held"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Schild und Speer in\u2019s Lager dringt,", "tokens": ["Mit", "Schild", "und", "Speer", "in's", "La\u00b7ger", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und neue Schlachtges\u00e4nge singt.", "tokens": ["Und", "neu\u00b7e", "Schlacht\u00b7ge\u00b7s\u00e4n\u00b7ge", "singt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und wilder jetzt in Feindesreihn", "tokens": ["Und", "wil\u00b7der", "jetzt", "in", "Fein\u00b7des\u00b7reihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greift er, als in die Harf\u2019 hinein,", "tokens": ["Greift", "er", ",", "als", "in", "die", "Ha\u00b7rf'", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und spielt, da\u00df Sait\u2019 um Saite springt,", "tokens": ["Und", "spielt", ",", "da\u00df", "Sait'", "um", "Sai\u00b7te", "springt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Schrei um Schrei gen Himmel dringt.", "tokens": ["Und", "Schrei", "um", "Schrei", "gen", "Him\u00b7mel", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Des Liedes lacht der D&#228;ne nicht,", "tokens": ["Des", "Lie\u00b7des", "lacht", "der", "D", "&#228;", "ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$(", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das klingenscharf zum Herzen spricht,", "tokens": ["Das", "klin\u00b7gen\u00b7scharf", "zum", "Her\u00b7zen", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gen J\u00fctland jagt es \u00fcber\u2019s Meer", "tokens": ["Gen", "J\u00fct\u00b7land", "jagt", "es", "\u00fc\u00b7ber's", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn, ohne Rast und \u2014 ohne Heer.", "tokens": ["Ihn", ",", "oh\u00b7ne", "Rast", "und", "oh\u00b7ne", "Heer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "NN", "KON", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In Wessex und Northumberland", "tokens": ["In", "Wes\u00b7sex", "und", "Nor\u00b7thum\u00b7ber\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Herrscht wieder K\u00f6nig Alfreds Hand,", "tokens": ["Herrscht", "wie\u00b7der", "K\u00f6\u00b7nig", "Al\u00b7freds", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und heimw\u00e4rts lenkt des D\u00e4nen Kiel,", "tokens": ["Und", "heim\u00b7w\u00e4rts", "lenkt", "des", "D\u00e4\u00b7nen", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denkt er an Alfreds Saitenspiel.", "tokens": ["Denkt", "er", "an", "Al\u00b7freds", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}