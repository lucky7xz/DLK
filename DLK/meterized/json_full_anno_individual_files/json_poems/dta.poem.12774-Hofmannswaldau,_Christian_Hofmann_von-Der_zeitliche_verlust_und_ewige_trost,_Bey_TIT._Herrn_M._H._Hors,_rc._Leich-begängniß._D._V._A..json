{"dta.poem.12774": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der zeitliche verlust und ewige trost,  \n  Bey  \n  TIT.  Herrn M. H. Hors, rc.  \n Leich-beg\u00e4ngni\u00df.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die krauckheiten, die uns plagen,", "tokens": ["Die", "krauck\u00b7hei\u00b7ten", ",", "die", "uns", "pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den leib zu grabe tragen,", "tokens": ["Und", "den", "leib", "zu", "gra\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind der ersten s\u00fcnde sold:", "tokens": ["Sind", "der", "ers\u00b7ten", "s\u00fcn\u00b7de", "sold", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor gesundheit, st\u00e4rck und leben,", "tokens": ["Vor", "ge\u00b7sund\u00b7heit", ",", "st\u00e4rck", "und", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wurde n\u00e4scherey gegeben,", "tokens": ["Wur\u00b7de", "n\u00e4\u00b7sche\u00b7rey", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Asche kriegten wir vor gold.", "tokens": ["A\u00b7sche", "krieg\u00b7ten", "wir", "vor", "gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Demnach haben GOtt die krancken,", "tokens": ["Dem\u00b7nach", "ha\u00b7ben", "Gott", "die", "kran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor die medicin zu dancken,", "tokens": ["Vor", "die", "me\u00b7di\u00b7cin", "zu", "dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die n\u00e4chst ihm ihr helffer ist;", "tokens": ["Die", "n\u00e4chst", "ihm", "ihr", "helf\u00b7fer", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Alle kr\u00e4uter und metallen", "tokens": ["Al\u00b7le", "kr\u00e4u\u00b7ter", "und", "me\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00fcssen ihr zu fu\u00dfe fallen,", "tokens": ["M\u00fcs\u00b7sen", "ihr", "zu", "fu\u00b7\u00dfe", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und verschaffen ruh und frist.", "tokens": ["Und", "ver\u00b7schaf\u00b7fen", "ruh", "und", "frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber da\u00df sie selber sterben,", "tokens": ["A\u00b7ber", "da\u00df", "sie", "sel\u00b7ber", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die das leben uns erwerben,", "tokens": ["Die", "das", "le\u00b7ben", "uns", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheinet hart und fast zu viel.", "tokens": ["Schei\u00b7net", "hart", "und", "fast", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADV", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der succurs, der vor uns krieget,", "tokens": ["Der", "suc\u00b7curs", ",", "der", "vor", "uns", "krie\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wird zu zeitlich selbst besieget;", "tokens": ["Wird", "zu", "zeit\u00b7lich", "selbst", "be\u00b7sie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sterben ist doch aller ziel.", "tokens": ["Ster\u00b7ben", "ist", "doch", "al\u00b7ler", "ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O da\u00df wir bey unsern linden", "tokens": ["O", "da\u00df", "wir", "bey", "un\u00b7sern", "lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses itzt an dir empfinden,", "tokens": ["Die\u00b7ses", "itzt", "an", "dir", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O du gro\u00dfer Podalir!", "tokens": ["O", "du", "gro\u00b7\u00dfer", "Po\u00b7da\u00b7lir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine sonderbare gaben,", "tokens": ["Dei\u00b7ne", "son\u00b7der\u00b7ba\u00b7re", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die wir sonst genossen haben,", "tokens": ["Die", "wir", "sonst", "ge\u00b7nos\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlen uns nun f\u00fcr und f\u00fcr.", "tokens": ["Feh\u00b7len", "uns", "nun", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine kunst, die du ge\u00fcbet,", "tokens": ["Dei\u00b7ne", "kunst", ",", "die", "du", "ge\u00b7\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die gantze stadt geliebet,", "tokens": ["Und", "die", "gant\u00b7ze", "stadt", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lag auf einem guten grund.", "tokens": ["Lag", "auf", "ei\u00b7nem", "gu\u00b7ten", "grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Michael, der \u00e4rtzte crone,", "tokens": ["Mic\u00b7hael", ",", "der", "\u00e4rtz\u00b7te", "cro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nahm dich auf zu seinem sohne,", "tokens": ["Nahm", "dich", "auf", "zu", "sei\u00b7nem", "soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du besa\u00dft sein gantzes pfund.", "tokens": ["Du", "be\u00b7sa\u00dft", "sein", "gant\u00b7zes", "pfund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Holland mit den klugen Britten", "tokens": ["Hol\u00b7land", "mit", "den", "klu\u00b7gen", "Brit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und des Frantzmanns nette sitten,", "tokens": ["Und", "des", "Frantz\u00b7manns", "net\u00b7te", "sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du kl\u00fcglich angesehn.", "tokens": ["Hast", "du", "kl\u00fcg\u00b7lich", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was die weisen Welschen weisen,", "tokens": ["Was", "die", "wei\u00b7sen", "Wel\u00b7schen", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Must\u2019 in deinen langen reisen", "tokens": ["Must'", "in", "dei\u00b7nen", "lan\u00b7gen", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir durch aug und h\u00e4nde gehn.", "tokens": ["Dir", "durch", "aug", "und", "h\u00e4n\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bre\u00dflau wolte deine gaben", "tokens": ["Bre\u00df\u00b7lau", "wol\u00b7te", "dei\u00b7ne", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPOSAT", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald darauf gantz eigen haben,", "tokens": ["Bald", "da\u00b7rauf", "gantz", "ei\u00b7gen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und versprach dir seine gunst.", "tokens": ["Und", "ver\u00b7sprach", "dir", "sei\u00b7ne", "gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber Leipzig hat vor allen", "tokens": ["A\u00b7ber", "Leip\u00b7zig", "hat", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir von anfang wohl gefallen,", "tokens": ["Dir", "von", "an\u00b7fang", "wohl", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als die mutter deiner kunst.", "tokens": ["Als", "die", "mut\u00b7ter", "dei\u00b7ner", "kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hier bist du bisher gewachsen,", "tokens": ["Hier", "bist", "du", "bis\u00b7her", "ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und die H\u00e4upter unsrer Sachsen", "tokens": ["Und", "die", "H\u00e4up\u00b7ter", "uns\u00b7rer", "Sach\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haben selber dich erh\u00f6ht.", "tokens": ["Ha\u00b7ben", "sel\u00b7ber", "dich", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine k\u00fcnste, ihre proben,", "tokens": ["Dei\u00b7ne", "k\u00fcns\u00b7te", ",", "ih\u00b7re", "pro\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Musten ihre helden loben,", "tokens": ["Mus\u00b7ten", "ih\u00b7re", "hel\u00b7den", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Saal und Elbe geht.", "tokens": ["Wo", "die", "Saal", "und", "El\u00b7be", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Unsre hohe Pindus-spitzen", "tokens": ["Uns\u00b7re", "ho\u00b7he", "Pin\u00b7dus\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00dfen dich auf ihnen sitzen,", "tokens": ["Lie\u00b7\u00dfen", "dich", "auf", "ih\u00b7nen", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem purpur Rector-hut.", "tokens": ["In", "dem", "pur\u00b7pur", "Rec\u00b7tor\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die catheder hat dein lehren", "tokens": ["Die", "cat\u00b7he\u00b7der", "hat", "dein", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Unsre jugend lassen h\u00f6ren,", "tokens": ["Uns\u00b7re", "ju\u00b7gend", "las\u00b7sen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die schon gute proben thut.", "tokens": ["Die", "schon", "gu\u00b7te", "pro\u00b7ben", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O verlust! den wir nun sehen.", "tokens": ["O", "ver\u00b7lust", "!", "den", "wir", "nun", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ART", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da er v\u00f6llig ist geschehen;", "tokens": ["Da", "er", "v\u00f6l\u00b7lig", "ist", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eher denckt man kaum daran.", "tokens": ["E\u00b7her", "denckt", "man", "kaum", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Finsterni\u00df folgt nach dem lichte,", "tokens": ["Fins\u00b7ter\u00b7ni\u00df", "folgt", "nach", "dem", "lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das leben wird zu nichte,", "tokens": ["Und", "das", "le\u00b7ben", "wird", "zu", "nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "VAFIN", "PTKA", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der artzt ist weggethan.", "tokens": ["Wenn", "der", "artzt", "ist", "weg\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "O gewinn! den ihm hingegen", "tokens": ["O", "ge\u00b7winn", "!", "den", "ihm", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "PPER", "ADV"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Gott im himmel will zulegen", "tokens": ["Gott", "im", "him\u00b7mel", "will", "zu\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf sein wahres Christenthum,", "tokens": ["Auf", "sein", "wah\u00b7res", "Chris\u00b7ten\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort hat er in JEsu wunden,", "tokens": ["Dort", "hat", "er", "in", "Je\u00b7su", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Seine panacee gefunden,", "tokens": ["Sei\u00b7ne", "pa\u00b7na\u00b7cee", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier hat er der Christen ruhm.", "tokens": ["Hier", "hat", "er", "der", "Chris\u00b7ten", "ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "O ein trost vor die betr\u00fcbten!", "tokens": ["O", "ein", "trost", "vor", "die", "be\u00b7tr\u00fcb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sie wissen den geliebten", "tokens": ["Denn", "sie", "wis\u00b7sen", "den", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jtzt bey seinem zweck und ziel:", "tokens": ["Jtzt", "bey", "sei\u00b7nem", "zweck", "und", "ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die hoffnung bleibt uns allen,", "tokens": ["Und", "die", "hoff\u00b7nung", "bleibt", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die \u00e4rtzte selbst hinfallen,", "tokens": ["Wenn", "die", "\u00e4rtz\u00b7te", "selbst", "hin\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df der H\u00f6chst\u2019 uns heilen will.", "tokens": ["Da\u00df", "der", "H\u00f6chst'", "uns", "hei\u00b7len", "will", "."], "tok