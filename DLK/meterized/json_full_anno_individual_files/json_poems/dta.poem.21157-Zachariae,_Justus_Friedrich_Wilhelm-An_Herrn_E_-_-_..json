{"dta.poem.21157": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn E - - .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O E &#8212;, h\u00fclle dich nicht in Melancholey!", "tokens": ["O", "E", "&#8212;", ",", "h\u00fcl\u00b7le", "dich", "nicht", "in", "Me\u00b7lan\u00b7cho\u00b7ley", "!"], "token_info": ["word", "word", "XML_entity", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Verla\u00df die Grotte, die du bewohnst,", "tokens": ["Ver\u00b7la\u00df", "die", "Grot\u00b7te", ",", "die", "du", "be\u00b7wohnst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sitze nicht immer allein beym klagenden Young,", "tokens": ["Und", "sit\u00b7ze", "nicht", "im\u00b7mer", "al\u00b7lein", "beym", "kla\u00b7gen\u00b7den", "Y\u00b7o\u00b7ung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "In schwarze Nachtgedanken verw\u00f6lkt.", "tokens": ["In", "schwar\u00b7ze", "Nacht\u00b7ge\u00b7dan\u00b7ken", "ver\u00b7w\u00f6lkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Schon ziehn die St\u00fcrme daher vom brausenden", "tokens": ["Schon", "ziehn", "die", "St\u00fcr\u00b7me", "da\u00b7her", "vom", "brau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "APPRART", "ADJA"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Blocksberg dampfet schon Wetter herab.", "tokens": ["Der", "Blocks\u00b7berg", "damp\u00b7fet", "schon", "Wet\u00b7ter", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So wie der Preu\u00dfen Armee vom Berge sich w\u00e4lzt,", "tokens": ["So", "wie", "der", "Preu\u00b7\u00dfen", "Ar\u00b7mee", "vom", "Ber\u00b7ge", "sich", "w\u00e4lzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So ziehn die Wolken feindlich vom Harz.", "tokens": ["So", "ziehn", "die", "Wol\u00b7ken", "feind\u00b7lich", "vom", "Harz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Denk an die dunkele Zeit, in Stollberg verweint,", "tokens": ["Denk", "an", "die", "dun\u00b7ke\u00b7le", "Zeit", ",", "in", "Stoll\u00b7berg", "ver\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da du des Unmuths Vaterland sahst.", "tokens": ["Da", "du", "des", "Un\u00b7muths", "Va\u00b7ter\u00b7land", "sahst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Orkane wurden da jung, und reis\u2019ten mit dir;", "tokens": ["Or\u00b7ka\u00b7ne", "wur\u00b7den", "da", "jung", ",", "und", "reis'\u00b7ten", "mit", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "$,", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "---+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jetzt naht sich diese schreckliche Zeit.", "tokens": ["Jetzt", "naht", "sich", "die\u00b7se", "schreck\u00b7li\u00b7che", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Komm, Freund, und heitre sie auf! Schon war-", "tokens": ["Komm", ",", "Freund", ",", "und", "heit\u00b7re", "sie", "auf", "!", "Schon", "wa\u00b7r"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "TRUNC"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ein wohlth\u00e4tiger Ofen auf dich!", "tokens": ["Und", "ein", "wohlt\u00b7h\u00e4\u00b7ti\u00b7ger", "O\u00b7fen", "auf", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem Tobacksgotte brennt schon ein flammendes Licht,", "tokens": ["Dem", "To\u00b7backs\u00b7got\u00b7te", "brennt", "schon", "ein", "flam\u00b7men\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das r\u00e4chend schlechte Verse verzehrt.", "tokens": ["Das", "r\u00e4\u00b7chend", "schlech\u00b7te", "Ver\u00b7se", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun, E \u2014, ist es ein Jahr, da\u00df wir dich hier", "tokens": ["Nun", ",", "E", ",", "ist", "es", "ein", "Jahr", ",", "da\u00df", "wir", "dich", "hier"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$(", "$,", "VAFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich weihe diesen Abend mit Wein.", "tokens": ["Ich", "wei\u00b7he", "die\u00b7sen", "A\u00b7bend", "mit", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie herrlich blinkt er im Glas! Komm, sto\u00df mit mir", "tokens": ["Wie", "herr\u00b7lich", "blinkt", "er", "im", "Glas", "!", "Komm", ",", "sto\u00df", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$.", "VVFIN", "$,", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seline, Cleon, und Doris, und Du!", "tokens": ["Se\u00b7li\u00b7ne", ",", "Cle\u00b7on", ",", "und", "Do\u00b7ris", ",", "und", "Du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "KON", "NE", "$,", "KON", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}