{"dta.poem.5229": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der weinende Amor,  \n bey Betrachtung einer Bilds\u00e4ule zu Char-  \n lottenburg im Garten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Ihm einen Kelch voll Mutterzorn zu schmecken:", "tokens": ["Ihm", "ei\u00b7nen", "Kelch", "voll", "Mut\u00b7ter\u00b7zorn", "zu", "schme\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie nahm ihm Pfeile. Was empfand", "tokens": ["Sie", "nahm", "ihm", "Pfei\u00b7le", ".", "Was", "emp\u00b7fand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Stolz? Wie schaamvoll steht er sich das Auge decken,", "tokens": ["Sein", "Stolz", "?", "Wie", "schaam\u00b7voll", "steht", "er", "sich", "das", "Au\u00b7ge", "de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAV", "ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit seiner linken Hand?", "tokens": ["Mit", "sei\u00b7ner", "lin\u00b7ken", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}