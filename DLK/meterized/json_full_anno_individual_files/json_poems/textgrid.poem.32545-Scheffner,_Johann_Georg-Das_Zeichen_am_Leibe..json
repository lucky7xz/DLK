{"textgrid.poem.32545": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Zeichen am Leibe.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Finette gieng aus ihrem zw\u00f6lften Jahr,", "tokens": ["Fi\u00b7net\u00b7te", "gieng", "aus", "ih\u00b7rem", "zw\u00f6lf\u00b7ten", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ohn da\u00df sie ihren Leib, noch jene Regung kannte", "tokens": ["Ohn", "da\u00df", "sie", "ih\u00b7ren", "Leib", ",", "noch", "je\u00b7ne", "Re\u00b7gung", "kann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "$,", "ADV", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von der die Frau Aebti\u00dfin brannte,", "tokens": ["Von", "der", "die", "Frau", "A\u00b7eb\u00b7ti\u00b7\u00dfin", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die sonsten doch ihr Mentor war.", "tokens": ["Die", "sons\u00b7ten", "doch", "ihr", "Men\u00b7tor", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Einst wusch das M\u00e4dchen sich die Meeresenge", "tokens": ["Einst", "wusch", "das", "M\u00e4d\u00b7chen", "sich", "die", "Mee\u00b7re\u00b7sen\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Die zwischen fleischern S\u00e4ulen liegt,", "tokens": ["Die", "zwi\u00b7schen", "flei\u00b7schern", "S\u00e4u\u00b7len", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die mit schmerzlichem Gedr\u00e4nge", "tokens": ["Durch", "die", "mit", "schmerz\u00b7li\u00b7chem", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Der Mensch ins Reich des Lebens kriecht.", "tokens": ["Der", "Mensch", "ins", "Reich", "des", "Le\u00b7bens", "kriecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbach, schrie sie, ach! da sie die Spro\u00dfen", "tokens": ["\u00bb", "ach", ",", "schrie", "sie", ",", "ach", "!", "da", "sie", "die", "Spro\u00b7\u00dfen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "$,", "ITJ", "$.", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der k\u00fcnftgen schwarzen Locken sah'", "tokens": ["Der", "k\u00fcnft\u00b7gen", "schwar\u00b7zen", "Lo\u00b7cken", "sah'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.12": {"text": "Mit jedem Tage wuchs die Zierde des Gestades,", "tokens": ["Mit", "je\u00b7dem", "Ta\u00b7ge", "wuchs", "die", "Zier\u00b7de", "des", "Ge\u00b7sta\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und krau\u00dfte sich das Schilf ums rothe Meer:", "tokens": ["Und", "krau\u00df\u00b7te", "sich", "das", "Schilf", "ums", "ro\u00b7the", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So bl\u00fchet um die Quelle eines warmen Bades,", "tokens": ["So", "bl\u00fc\u00b7het", "um", "die", "Quel\u00b7le", "ei\u00b7nes", "war\u00b7men", "Ba\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein zart Gestr\u00e4uch, und streuet Schatten um sich her.", "tokens": ["Ein", "zart", "Ge\u00b7str\u00e4uch", ",", "und", "streu\u00b7et", "Schat\u00b7ten", "um", "sich", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "KON", "VVFIN", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Aus Gram verga\u00df sie Spiel und Essen,", "tokens": ["Aus", "Gram", "ver\u00b7ga\u00df", "sie", "Spiel", "und", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Oft blieb sie ganze N\u00e4chte wach,", "tokens": ["Oft", "blieb", "sie", "gan\u00b7ze", "N\u00e4ch\u00b7te", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und konnte nicht das Wunderding verge\u00dfen,", "tokens": ["Und", "konn\u00b7te", "nicht", "das", "Wun\u00b7der\u00b7ding", "ver\u00b7ge\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Das sichtbarlich aus ihrem Leibe brach.", "tokens": ["Das", "sicht\u00b7bar\u00b7lich", "aus", "ih\u00b7rem", "Lei\u00b7be", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die bleiche Wange wies wie sehr sie sich betr\u00fcbte,", "tokens": ["Die", "blei\u00b7che", "Wan\u00b7ge", "wies", "wie", "sehr", "sie", "sich", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Nonnen Trost blieb ohne Frucht,", "tokens": ["Der", "Non\u00b7nen", "Trost", "blieb", "oh\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Auch die Aebti\u00dfin die sie z\u00e4rtlich liebte", "tokens": ["Auch", "die", "A\u00b7eb\u00b7ti\u00b7\u00dfin", "die", "sie", "z\u00e4rt\u00b7lich", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Fleht, predigt, schilt, versucht", "tokens": ["Fleht", ",", "pre\u00b7digt", ",", "schilt", ",", "ver\u00b7sucht"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Durch Schmeicheln ihr den Grund des Kummers abzufragen \u2013", "tokens": ["Durch", "Schmei\u00b7cheln", "ihr", "den", "Grund", "des", "Kum\u00b7mers", "ab\u00b7zu\u00b7fra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u00bbach, gn\u00e4d'ge Frau, so hub sie endlich an,", "tokens": ["\u00bb", "ach", ",", "gn\u00e4d'\u00b7ge", "Frau", ",", "so", "hub", "sie", "end\u00b7lich", "an", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und seufzt und weint, \u00bbwas hilfts mein Leid zu klagen", "tokens": ["Und", "seufzt", "und", "weint", ",", "\u00bb", "was", "hilfts", "mein", "Leid", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "$(", "PWS", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Da doch kein Mensch mir helfen kann,", "tokens": ["Da", "doch", "kein", "Mensch", "mir", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Mich hat des Himmels Zorn geschlagen,", "tokens": ["Mich", "hat", "des", "Him\u00b7mels", "Zorn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ein Zeichen hat sein Grimm an meinem Leib gethan,", "tokens": ["Ein", "Zei\u00b7chen", "hat", "sein", "Grimm", "an", "mei\u00b7nem", "Leib", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das werd ich wohl bis in die Grube tragen,", "tokens": ["Das", "werd", "ich", "wohl", "bis", "in", "die", "Gru\u00b7be", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Da sehn Sie sie selbst das Thier nur an \u2013 \u00ab", "tokens": ["Da", "sehn", "Sie", "sie", "selbst", "das", "Thier", "nur", "an", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADV", "APPR", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Hier hob das sch\u00f6ne Kind den Vorhang seiner Kleider,", "tokens": ["Hier", "hob", "das", "sch\u00f6\u00b7ne", "Kind", "den", "Vor\u00b7hang", "sei\u00b7ner", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und wie\u00df der gn\u00e4d'gen Frau das kleine Wundertier,", "tokens": ["Und", "wie\u00df", "der", "gn\u00e4d'\u00b7gen", "Frau", "das", "klei\u00b7ne", "Wun\u00b7der\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "(ein M\u00f6nch der still im Oratorio sa\u00df", "tokens": ["(", "ein", "M\u00f6nch", "der", "still", "im", "O\u00b7ra\u00b7to\u00b7rio", "sa\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ART", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Half dieses Thierchen mit besehen,", "tokens": ["Half", "die\u00b7ses", "Thier\u00b7chen", "mit", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und glaubt' indem er just vom heilgen Paulus la\u00df", "tokens": ["Und", "glaubt'", "in\u00b7dem", "er", "just", "vom", "heil\u00b7gen", "Pau\u00b7lus", "la\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADV", "APPRART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er s\u00e4he auch den Himmel offen stehen)", "tokens": ["Er", "s\u00e4\u00b7he", "auch", "den", "Him\u00b7mel", "of\u00b7fen", "ste\u00b7hen", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Madam sahs l\u00e4chelnd an, und dacht wohlth\u00e4tig: leider", "tokens": ["Ma\u00b7dam", "sahs", "l\u00e4\u00b7chelnd", "an", ",", "und", "dacht", "wohlt\u00b7h\u00e4\u00b7tig", ":", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "$.", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ist unser Prior jetzt nicht hier,", "tokens": ["Ist", "un\u00b7ser", "Pri\u00b7or", "jetzt", "nicht", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Der w\u00fcrd' geschwind' mit diesem K\u00e4zchen spielen,", "tokens": ["Der", "w\u00fcrd'", "ge\u00b7schwind'", "mit", "die\u00b7sem", "K\u00e4z\u00b7chen", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Der strich ihm gern das zarte Haar,", "tokens": ["Der", "strich", "ihm", "gern", "das", "zar\u00b7te", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Und w\u00fcrd' ihm gleich ins kleine M\u00e4ulchen f\u00fchlen \u2013", "tokens": ["Und", "w\u00fcrd'", "ihm", "gleich", "ins", "klei\u00b7ne", "M\u00e4ul\u00b7chen", "f\u00fch\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Ich weis wie t\u00e4ndelnd er bey mir einst war.", "tokens": ["Ich", "weis", "wie", "t\u00e4n\u00b7delnd", "er", "bey", "mir", "einst", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KOKOM", "ADJD", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Jetzt sprach sie: \u00bbLiebes Kind la\u00df dich das nicht erschrecken,", "tokens": ["Jetzt", "sprach", "sie", ":", "\u00bb", "Lie\u00b7bes", "Kind", "la\u00df", "dich", "das", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADJA", "NN", "VVIMP", "PPER", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Jedwede Nonn', auch ich, hat so ein K\u00e4zchen, da,", "tokens": ["Jed\u00b7we\u00b7de", "Nonn'", ",", "auch", "ich", ",", "hat", "so", "ein", "K\u00e4z\u00b7chen", ",", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PPER", "$,", "VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "$,"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.46": {"text": "Sieh', ich erlaub es dir das mein'ge aufzudecken \u2013", "tokens": ["Sieh'", ",", "ich", "er\u00b7laub", "es", "dir", "das", "mein'\u00b7ge", "auf\u00b7zu\u00b7de\u00b7cken", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PPER", "PDS", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Finette deckt sie auf \u2013 sie sah \u2013", "tokens": ["Fi\u00b7net\u00b7te", "deckt", "sie", "auf", "\u2013", "sie", "sah", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Und schrie: Ha! welche Katz! die hat ja M\u00e4hnen \u2013", "tokens": ["Und", "schrie", ":", "Ha", "!", "wel\u00b7che", "Katz", "!", "die", "hat", "ja", "M\u00e4h\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ITJ", "$.", "PWAT", "NN", "$.", "PDS", "VAFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Und als sie ihr den Balg mit zarten H\u00e4nden strich'", "tokens": ["Und", "als", "sie", "ihr", "den", "Balg", "mit", "zar\u00b7ten", "H\u00e4n\u00b7den", "strich'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Fieng sie woll\u00fcstig an zu g\u00e4hnen. \u2013", "tokens": ["Fi\u00b7eng", "sie", "wol\u00b7l\u00fcs\u00b7tig", "an", "zu", "g\u00e4h\u00b7nen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$.", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.51": {"text": "\u00bbach welch ein Maul! \u2013 Ach Gott erbarme sich!\u00ab", "tokens": ["\u00bb", "ach", "welch", "ein", "Maul", "!", "\u2013", "Ach", "Gott", "er\u00b7bar\u00b7me", "sich", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", 