{"dta.poem.20493": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach da\u00df ich euch nicht meiden m\u00fcste/", "tokens": ["Ach", "da\u00df", "ich", "euch", "nicht", "mei\u00b7den", "m\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sch\u00e4tze dieser dritten welt/", "tokens": ["Ihr", "sch\u00e4t\u00b7ze", "die\u00b7ser", "drit\u00b7ten", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr schnee-geb\u00fcrgten engel-br\u00fcste/", "tokens": ["Ihr", "schnee\u00b7ge\u00b7b\u00fcrg\u00b7ten", "en\u00b7gel\u00b7br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von lufft und seuffzern auffgeschwellt;", "tokens": ["Von", "lufft", "und", "seuff\u00b7zern", "auff\u00b7ge\u00b7schwellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit eurer rundten lieblichkeit", "tokens": ["Mit", "eu\u00b7rer", "rund\u00b7ten", "lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag nichts durchaus verglichen werden/", "tokens": ["Mag", "nichts", "durc\u00b7haus", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weil ihr des himmels und der erden/", "tokens": ["Weil", "ihr", "des", "him\u00b7mels", "und", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des grossen rundtes bilder seyd.", "tokens": ["Des", "gros\u00b7sen", "rund\u00b7tes", "bil\u00b7der", "seyd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr die ihr beyde h\u00e4nde f\u00fcllet/", "tokens": ["Ihr", "die", "ihr", "bey\u00b7de", "h\u00e4n\u00b7de", "f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSAT", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr seyd hier nicht wie anderw\u00e4rts", "tokens": ["Ihr", "seyd", "hier", "nicht", "wie", "an\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In tausend t\u00fcchern eingeh\u00fcllet/", "tokens": ["In", "tau\u00b7send", "t\u00fc\u00b7chern", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und qv\u00e4lt das aug/ und klemmt das hertz:", "tokens": ["Und", "qv\u00e4lt", "das", "aug", "/", "und", "klemmt", "das", "hertz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr zeiget blo\u00df und decket frey/", "tokens": ["Ihr", "zei\u00b7get", "blo\u00df", "und", "de\u00b7cket", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch lindes auff- und nieder-wallen/", "tokens": ["Durch", "lin\u00b7des", "auf\u00b7f", "und", "nie\u00b7der\u00b7wal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "TRUNC", "KON", "ADJA", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da\u00df in euch weissen marmor-ballen", "tokens": ["Da\u00df", "in", "euch", "weis\u00b7sen", "mar\u00b7mor\u00b7bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Blut/ feuer/ geist und leben sey.", "tokens": ["Blut", "/", "feu\u00b7er", "/", "geist", "und", "le\u00b7ben", "sey", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "VVPP", "KON", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auff euren h\u00fcgeln sch\u00f6ne br\u00fcste", "tokens": ["Auff", "eu\u00b7ren", "h\u00fc\u00b7geln", "sch\u00f6\u00b7ne", "br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat eine werthe mildigkeit", "tokens": ["Hat", "ei\u00b7ne", "wert\u00b7he", "mil\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den s\u00fcssen saamen aller l\u00fcste", "tokens": ["Den", "s\u00fcs\u00b7sen", "saa\u00b7men", "al\u00b7ler", "l\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu vollem wachsthum ausgestreut.", "tokens": ["Zu", "vol\u00b7lem", "wach\u00b7sthum", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier ist die s\u00fcsse frucht der welt/", "tokens": ["Hier", "ist", "die", "s\u00fcs\u00b7se", "frucht", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nach dem paradiese schmecket/", "tokens": ["Die", "nach", "dem", "pa\u00b7ra\u00b7die\u00b7se", "schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Darein der starcke leim verstecket/", "tokens": ["Da\u00b7rein", "der", "star\u00b7cke", "leim", "ver\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der alle welt zusammen h\u00e4lt.", "tokens": ["Der", "al\u00b7le", "welt", "zu\u00b7sam\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach m\u00f6chten nur die w\u00fcrffel fallen/", "tokens": ["Ach", "m\u00f6ch\u00b7ten", "nur", "die", "w\u00fcrf\u00b7fel", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich nicht d\u00f6rffte weiter gehn/", "tokens": ["Da\u00df", "ich", "nicht", "d\u00f6rff\u00b7te", "wei\u00b7ter", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00f6nte stets euch zucker-ballen", "tokens": ["Und", "k\u00f6n\u00b7te", "stets", "euch", "zu\u00b7cke\u00b7rbal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In eurem milch-meer schwimmen sehn/", "tokens": ["In", "eu\u00b7rem", "milch\u00b7meer", "schwim\u00b7men", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wolte gern durch manchen ku\u00df/", "tokens": ["Ich", "wol\u00b7te", "gern", "durch", "man\u00b7chen", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auff euch ihr sch\u00f6nsten Liebs-alt\u00e4ren/", "tokens": ["Auff", "euch", "ihr", "sch\u00f6ns\u00b7ten", "Liebs\u00b7al\u00b7t\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die h\u00f6chste billigkeit gew\u00e4hren/", "tokens": ["Die", "h\u00f6chs\u00b7te", "bil\u00b7lig\u00b7keit", "ge\u00b7w\u00e4h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die man an euch verwundern mu\u00df.", "tokens": ["Die", "man", "an", "euch", "ver\u00b7wun\u00b7dern", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch nein der himmel wills nicht leiden/", "tokens": ["Doch", "nein", "der", "him\u00b7mel", "wills", "nicht", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein schicksal reist mich vor euch hin;", "tokens": ["Mein", "schick\u00b7sal", "reist", "mich", "vor", "euch", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lebt wohl/ ich mu\u00df euch ewig meiden/", "tokens": ["Lebt", "wohl", "/", "ich", "mu\u00df", "euch", "e\u00b7wig", "mei\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wiewohl ich euer sclave bin.", "tokens": ["Wie\u00b7wohl", "ich", "eu\u00b7er", "scla\u00b7ve", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was denn der mund nicht leiden kan/", "tokens": ["Was", "denn", "der", "mund", "nicht", "lei\u00b7den", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das nehmt ihr sch\u00f6nsten engel-br\u00fcste/", "tokens": ["Das", "nehmt", "ihr", "sch\u00f6ns\u00b7ten", "en\u00b7gel\u00b7br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr gegenw\u00fcrffe meiner l\u00fcste/", "tokens": ["Ihr", "ge\u00b7gen\u00b7w\u00fcrf\u00b7fe", "mei\u00b7ner", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von liebenden gedancken an.", "tokens": ["Von", "lie\u00b7ben\u00b7den", "ge\u00b7dan\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}