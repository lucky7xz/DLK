{"dta.poem.10995": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Allwissenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es k\u00f6mmt, von einem jeden Menschen', das Hirn, als wie\nein Schauplatz, mir,", "tokens": ["Es", "k\u00f6mmt", ",", "von", "ei\u00b7nem", "je\u00b7den", "Men\u00b7schen'", ",", "das", "Hirn", ",", "als", "wie", "ein", "Schau\u00b7platz", ",", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "PIAT", "NN", "$,", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Denn die f\u00fcnf Sinnen stets ver\u00e4ndern; als Spieler, die", "tokens": ["Denn", "die", "f\u00fcnf", "Sin\u00b7nen", "stets", "ver\u00b7\u00e4n\u00b7dern", ";", "als", "Spie\u00b7ler", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ART", "CARD", "NN", "ADV", "VVINF", "$.", "KOUS", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was m\u00fcssen nicht, an Lust- und Trauer, darin, o Sch\u00f6pfer!", "tokens": ["Was", "m\u00fcs\u00b7sen", "nicht", ",", "an", "Lust", "und", "Trau\u00b7er", ",", "da\u00b7rin", ",", "o", "Sch\u00f6p\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "$,", "APPR", "TRUNC", "KON", "NN", "$,", "PAV", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vor deinen alles sehnden Augen, f\u00fcr Schauspiel aufgef\u00fchret", "tokens": ["Vor", "dei\u00b7nen", "al\u00b7les", "sehn\u00b7den", "Au\u00b7gen", ",", "f\u00fcr", "Schau\u00b7spiel", "auf\u00b7ge\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "PIAT", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}}}}}