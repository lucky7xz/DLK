{"textgrid.poem.57416": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahl", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Europa herrschet. Immer geschmeichelter", "tokens": ["Eu\u00b7ro\u00b7pa", "herr\u00b7schet", ".", "Im\u00b7mer", "ge\u00b7schmei\u00b7chel\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "ADV", "ADJD"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gebietest du der Herrscherin, Sinnlichkeit!", "tokens": ["Ge\u00b7bie\u00b7test", "du", "der", "Herr\u00b7sche\u00b7rin", ",", "Sinn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Blumenkette, die du anlegst,", "tokens": ["Die", "Blu\u00b7men\u00b7ket\u00b7te", ",", "die", "du", "an\u00b7legst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Klirret nicht, aber umringelt fester,", "tokens": ["Klir\u00b7ret", "nicht", ",", "a\u00b7ber", "um\u00b7rin\u00b7gelt", "fes\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Als jene, die den bleichen Gefangenen", "tokens": ["Als", "je\u00b7ne", ",", "die", "den", "blei\u00b7chen", "Ge\u00b7fan\u00b7ge\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Turme lastet. Zauberin Sinnlichkeit,", "tokens": ["Im", "Tur\u00b7me", "las\u00b7tet", ".", "Zau\u00b7be\u00b7rin", "Sinn\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$.", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du t\u00f6test alles, was erinnert,", "tokens": ["Du", "t\u00f6\u00b7test", "al\u00b7les", ",", "was", "e\u00b7rin\u00b7nert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie nicht Leib nur, da\u00df eine Seele", "tokens": ["Da\u00df", "sie", "nicht", "Leib", "nur", ",", "da\u00df", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie auch doch haben! Von der Erhabenen,", "tokens": ["Sie", "auch", "doch", "ha\u00b7ben", "!", "Von", "der", "Er\u00b7ha\u00b7be\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VAINF", "$.", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von ihrer Gr\u00f6\u00dfe red ich nicht, sage nur:", "tokens": ["Von", "ih\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "red", "ich", "nicht", ",", "sa\u00b7ge", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du schl\u00e4ferst ein, da\u00df sie in sich nichts", "tokens": ["Du", "schl\u00e4\u00b7ferst", "ein", ",", "da\u00df", "sie", "in", "sich", "nichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "PRF", "PIS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Au\u00dfer der schlagenden Ader f\u00fchlen.", "tokens": ["Au\u00b7\u00dfer", "der", "schla\u00b7gen\u00b7den", "A\u00b7der", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Das soll nun endlich enden! Der edle Krieg", "tokens": ["Das", "soll", "nun", "end\u00b7lich", "en\u00b7den", "!", "Der", "ed\u00b7le", "Krieg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ADV", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der gro\u00dfen, liebensw\u00fcrdigen Gallier", "tokens": ["Der", "gro\u00b7\u00dfen", ",", "lie\u00b7bens\u00b7w\u00fcr\u00b7di\u00b7gen", "Gal\u00b7lier"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Raubt bis zum letzten Scherf. Euch sinket", "tokens": ["Raubt", "bis", "zum", "letz\u00b7ten", "Scherf", ".", "Euch", "sin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welkend vom Arme die Blumenkette.", "tokens": ["Wel\u00b7kend", "vom", "Ar\u00b7me", "die", "Blu\u00b7men\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die Donnerstimme schallt euch der eisernen", "tokens": ["Die", "Don\u00b7ner\u00b7stim\u00b7me", "schallt", "euch", "der", "ei\u00b7ser\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Notwendigkeit! Ihr strauchelt des Lebens Weg", "tokens": ["Not\u00b7wen\u00b7dig\u00b7keit", "!", "Ihr", "strau\u00b7chelt", "des", "Le\u00b7bens", "Weg"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verarmt: wie w\u00e4r es m\u00f6glich, da\u00df ihr", "tokens": ["Ver\u00b7armt", ":", "wie", "w\u00e4r", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "ihr"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$.", "PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Nun in der Zauberin Scho\u00df noch ruhtet?", "tokens": ["Nun", "in", "der", "Zau\u00b7be\u00b7rin", "Scho\u00df", "noch", "ruh\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch wenn ein Funken Seele vielleicht in euch", "tokens": ["Doch", "wenn", "ein", "Fun\u00b7ken", "See\u00b7le", "viel\u00b7leicht", "in", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aufglimmet, wenn ihr z\u00fcrnt, da\u00df ihr Knechte seid ...", "tokens": ["Auf\u00b7glim\u00b7met", ",", "wenn", "ihr", "z\u00fcrnt", ",", "da\u00df", "ihr", "Knech\u00b7te", "seid", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was frommts? Ihr habt zum Flintenstein die", "tokens": ["Was", "frommts", "?", "Ihr", "habt", "zum", "Flin\u00b7ten\u00b7stein", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Pfennige nicht, noch zu einer Kugel!", "tokens": ["Pfen\u00b7ni\u00b7ge", "nicht", ",", "noch", "zu", "ei\u00b7ner", "Ku\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr saht es welken, h\u00f6rtet die eiserne", "tokens": ["Ihr", "saht", "es", "wel\u00b7ken", ",", "h\u00f6r\u00b7tet", "die", "ei\u00b7ser\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Notwendigkeit. Was wollet ihr tun? Wohlan,", "tokens": ["Not\u00b7wen\u00b7dig\u00b7keit", ".", "Was", "wol\u00b7let", "ihr", "tun", "?", "Wo\u00b7hlan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$.", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur Wahl: Verzweifelt! oder macht euch", "tokens": ["Zur", "Wahl", ":", "Ver\u00b7zwei\u00b7felt", "!", "o\u00b7der", "macht", "euch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "VVPP", "$.", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gl\u00fccklicher, als es der Zauber konnte.", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", ",", "als", "es", "der", "Zau\u00b7ber", "konn\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VMFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wer, was die Sch\u00f6pfung, und was er selbst sei, forscht;", "tokens": ["Wer", ",", "was", "die", "Sch\u00f6p\u00b7fung", ",", "und", "was", "er", "selbst", "sei", ",", "forscht", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "$,", "PRELS", "ART", "NN", "$,", "KON", "PWS", "PPER", "ADV", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Anbetend forscht, was Gott sei, den heitert, st\u00e4rkt", "tokens": ["An\u00b7be\u00b7tend", "forscht", ",", "was", "Gott", "sei", ",", "den", "hei\u00b7tert", ",", "st\u00e4rkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "NN", "VAFIN", "$,", "ART", "VVFIN", "$,", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Genu\u00df des Geistes: wen nach diesen", "tokens": ["Ge\u00b7nu\u00df", "des", "Geis\u00b7tes", ":", "wen", "nach", "die\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PWS", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Quellen nie d\u00fcrstete, der erlieget.", "tokens": ["Quel\u00b7len", "nie", "d\u00fcrs\u00b7te\u00b7te", ",", "der", "er\u00b7lie\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Der K\u00fcnste Blumen k\u00f6nnen zur Heiterkeit", "tokens": ["Der", "K\u00fcns\u00b7te", "Blu\u00b7men", "k\u00f6n\u00b7nen", "zur", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch wieder wecken; f\u00fchrt euch des Kenners Blick.", "tokens": ["Auch", "wie\u00b7der", "we\u00b7cken", ";", "f\u00fchrt", "euch", "des", "Ken\u00b7ners", "Blick", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$.", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Farbe tr\u00fcget oft; der Blumen", "tokens": ["Die", "Far\u00b7be", "tr\u00fc\u00b7get", "oft", ";", "der", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seelen sind labende Wohlger\u00fcche.", "tokens": ["See\u00b7len", "sind", "la\u00b7ben\u00b7de", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Europa herrschet. Immer geschmeichelter", "tokens": ["E