{"dta.poem.9646": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wer k\u00f6nig von Engelland sey/ Wilhelm  \n oder Jacob?  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjlhelm ficht und Jacob ziehet/", "tokens": ["Wjlhelm", "ficht", "und", "Ja\u00b7cob", "zie\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "NE", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fragst du/ wer denn k\u00f6nig bleibt?", "tokens": ["Fragst", "du", "/", "wer", "denn", "k\u00f6\u00b7nig", "bleibt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWS", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der/ der aus dem reiche fliehet/", "tokens": ["Der", "/", "der", "aus", "dem", "rei\u00b7che", "flie\u00b7het", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder der den andern treibt?", "tokens": ["O\u00b7der", "der", "den", "an\u00b7dern", "treibt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jacob spricht/ ich mu\u00df mich sch\u00e4men/", "tokens": ["Ja\u00b7cob", "spricht", "/", "ich", "mu\u00df", "mich", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wilhelm kan nur K\u00f6nig seyn.", "tokens": ["Wil\u00b7helm", "kan", "nur", "K\u00f6\u00b7nig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Solt\u2019 er Engelland nicht nehmen?", "tokens": ["Solt'", "er", "En\u00b7gel\u00b7land", "nicht", "neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00e4um\u2019 ichs ihm doch selber ein.", "tokens": ["R\u00e4um'", "ichs", "ihm", "doch", "sel\u00b7ber", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}