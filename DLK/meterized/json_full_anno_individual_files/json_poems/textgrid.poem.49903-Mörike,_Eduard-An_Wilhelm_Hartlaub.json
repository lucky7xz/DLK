{"textgrid.poem.49903": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Wilhelm Hartlaub", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchs Fenster schien der helle Mond herein;", "tokens": ["Durchs", "Fens\u00b7ter", "schien", "der", "hel\u00b7le", "Mond", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sa\u00dfest am Klavier im D\u00e4mmerschein,", "tokens": ["Du", "sa\u00b7\u00dfest", "am", "Kla\u00b7vier", "im", "D\u00e4m\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Versankst im Traumgew\u00fchl der Melodien,", "tokens": ["Ver\u00b7sankst", "im", "Traum\u00b7ge\u00b7w\u00fchl", "der", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich folgte dir an schwarzen Gr\u00fcnden hin,", "tokens": ["Ich", "folg\u00b7te", "dir", "an", "schwar\u00b7zen", "Gr\u00fcn\u00b7den", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo der Gesang versteckter Quellen klang,", "tokens": ["Wo", "der", "Ge\u00b7sang", "ver\u00b7steck\u00b7ter", "Quel\u00b7len", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.6": {"text": "Gleich Kinderstimmen, die der Wind verschlang.", "tokens": ["Gleich", "Kin\u00b7der\u00b7stim\u00b7men", ",", "die", "der", "Wind", "ver\u00b7schlang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch pl\u00f6tzlich war dein Spiel wie umgewandt,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "war", "dein", "Spiel", "wie", "um\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur blauer Himmel schien noch ausgespannt,", "tokens": ["Nur", "blau\u00b7er", "Him\u00b7mel", "schien", "noch", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder Ton ein lang gehaltnes Schweigen.", "tokens": ["Ein", "je\u00b7der", "Ton", "ein", "lang", "ge\u00b7halt\u00b7nes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da fing das Firmament sich an zu neigen,", "tokens": ["Da", "fing", "das", "Fir\u00b7ma\u00b7ment", "sich", "an", "zu", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APZR", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und j\u00e4h daran herab der Sterne selig Heer", "tokens": ["Und", "j\u00e4h", "da\u00b7ran", "her\u00b7ab", "der", "Ster\u00b7ne", "se\u00b7lig", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PAV", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Glitt rieselnd in ein goldig Nebelmeer,", "tokens": ["Glitt", "rie\u00b7selnd", "in", "ein", "gol\u00b7dig", "Ne\u00b7bel\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis Tropf um Tropfen hell darin zerging,", "tokens": ["Bis", "Tropf", "um", "Trop\u00b7fen", "hell", "da\u00b7rin", "zer\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die alte Nacht den \u00f6den Raum umfing.", "tokens": ["Die", "al\u00b7te", "Nacht", "den", "\u00f6\u00b7den", "Raum", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als du neu ein fr\u00f6hlich Leben wecktest,", "tokens": ["Und", "als", "du", "neu", "ein", "fr\u00f6h\u00b7lich", "Le\u00b7ben", "weck\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Finsternis mit jungem Lichte schrecktest,", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "mit", "jun\u00b7gem", "Lich\u00b7te", "schreck\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War ich schon weit hinweg mit Sinn und Ohr,", "tokens": ["War", "ich", "schon", "weit", "hin\u00b7weg", "mit", "Sinn", "und", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zuletzt warst du es selbst, in den ich mich verlor;", "tokens": ["Zu\u00b7letzt", "warst", "du", "es", "selbst", ",", "in", "den", "ich", "mich", "ver\u00b7lor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Herz durchz\u00fcckt' mit eins ein Freudenstrahl:", "tokens": ["Mein", "Herz", "durch\u00b7z\u00fcckt'", "mit", "eins", "ein", "Freu\u00b7den\u00b7strahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein ganzer Wert erschien mir auf einmal.", "tokens": ["Dein", "gan\u00b7zer", "Wert", "er\u00b7schien", "mir", "auf", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wunderbar empfand ich es, so neu,", "tokens": ["So", "wun\u00b7der\u00b7bar", "emp\u00b7fand", "ich", "es", ",", "so", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df noch bestehe Freundeslieb und Treu!", "tokens": ["Da\u00df", "noch", "be\u00b7ste\u00b7he", "Freun\u00b7des\u00b7lieb", "und", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df uns so sichrer Gegenwart Genu\u00df", "tokens": ["Da\u00df", "uns", "so", "sich\u00b7rer", "Ge\u00b7gen\u00b7wart", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Zusammenh\u00e4lt in Lebens\u00fcberflu\u00df!", "tokens": ["Zu\u00b7sam\u00b7men\u00b7h\u00e4lt", "in", "Le\u00b7bens\u00b7\u00fc\u00b7berf\u00b7lu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sah dein hingesenktes Angesicht", "tokens": ["Ich", "sah", "dein", "hin\u00b7ge\u00b7senk\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Schatten halb und halb im klaren Licht;", "tokens": ["Im", "Schat\u00b7ten", "halb", "und", "halb", "im", "kla\u00b7ren", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du ahntest nicht, wie mir der Busen schwoll,", "tokens": ["Du", "ahn\u00b7test", "nicht", ",", "wie", "mir", "der", "Bu\u00b7sen", "schwoll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie mir das Auge brennend \u00fcberquoll.", "tokens": ["Wie", "mir", "das", "Au\u00b7ge", "bren\u00b7nend", "\u00fc\u00b7berq\u00b7uoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du endigtest; ich schwieg \u2013 Ach warum ist doch eben", "tokens": ["Du", "en\u00b7dig\u00b7test", ";", "ich", "schwieg", "\u2013", "Ach", "wa\u00b7rum", "ist", "doch", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$(", "ITJ", "PWAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem h\u00f6chsten Gl\u00fcck kein Laut des Danks gegeben?", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Gl\u00fcck", "kein", "Laut", "des", "Danks", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da tritt dein T\u00f6chterchen mit Licht herein,", "tokens": ["Da", "tritt", "dein", "T\u00f6ch\u00b7ter\u00b7chen", "mit", "Licht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein l\u00e4ndlich Mahl versammelt gro\u00df und klein,", "tokens": ["Ein", "l\u00e4nd\u00b7lich", "Mahl", "ver\u00b7sam\u00b7melt", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom nahen Kirchturm schallt das Nachtgel\u00e4ut,", "tokens": ["Vom", "na\u00b7hen", "Kirch\u00b7turm", "schallt", "das", "Nacht\u00b7ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verklingend so des Tages Lieblichkeit.", "tokens": ["Ver\u00b7klin\u00b7gend", "so", "des", "Ta\u00b7ges", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Durchs Fenster schien der helle Mond herein;", "tokens": ["Durchs", "Fens\u00b7ter", "schien", "der", "hel\u00b7le", "Mond", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sa\u00dfest am Klavier im D\u00e4mmerschein,", "tokens": ["Du", "sa\u00b7\u00dfest", "am", "Kla\u00b7vier", "im", "D\u00e4m\u00b7mer\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Versankst im Traumgew\u00fchl der Melodien,", "tokens": ["Ver\u00b7sankst", "im", "Traum\u00b7ge\u00b7w\u00fchl", "der", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART