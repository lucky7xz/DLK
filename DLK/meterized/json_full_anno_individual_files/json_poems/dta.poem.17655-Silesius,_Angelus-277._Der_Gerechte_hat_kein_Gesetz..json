{"dta.poem.17655": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "277.  Der Gerechte hat kein Gesetz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr b\u00f6s\u2019 ist das Gesetz: w\u00e4r kein Gebot geschreiben/", "tokens": ["F\u00fcr", "b\u00f6s'", "ist", "das", "Ge\u00b7setz", ":", "w\u00e4r", "kein", "Ge\u00b7bot", "ge\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VAFIN", "ART", "NN", "$.", "VAFIN", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Frommen w\u00fcrden doch Gott und den N\u00e4chsten", "tokens": ["Die", "From\u00b7men", "w\u00fcr\u00b7den", "doch", "Gott", "und", "den", "N\u00e4chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "KON", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "lieben.", "tokens": ["lie\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}