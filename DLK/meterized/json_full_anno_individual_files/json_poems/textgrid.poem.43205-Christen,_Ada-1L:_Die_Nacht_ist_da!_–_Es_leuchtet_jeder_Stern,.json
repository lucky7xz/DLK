{"textgrid.poem.43205": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht ist da! \u2013 Es leuchtet jeder Stern,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist da! \u2013 Es leuchtet jeder Stern,", "tokens": ["Die", "Nacht", "ist", "da", "!", "\u2013", "Es", "leuch\u00b7tet", "je\u00b7der", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "$(", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Mondlicht zittert sacht auf jeder Welle,", "tokens": ["Das", "Mond\u00b7licht", "zit\u00b7tert", "sacht", "auf", "je\u00b7der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der feuchte Sand ergl\u00e4nzet silberhelle,", "tokens": ["Der", "feuch\u00b7te", "Sand", "er\u00b7gl\u00e4n\u00b7zet", "sil\u00b7ber\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Meeres Brausen t\u00f6net dumpf und fern.", "tokens": ["Des", "Mee\u00b7res", "Brau\u00b7sen", "t\u00f6\u00b7net", "dumpf", "und", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es wogt heran und spr\u00fchet Perlen aus,", "tokens": ["Es", "wogt", "he\u00b7ran", "und", "spr\u00fc\u00b7het", "Per\u00b7l