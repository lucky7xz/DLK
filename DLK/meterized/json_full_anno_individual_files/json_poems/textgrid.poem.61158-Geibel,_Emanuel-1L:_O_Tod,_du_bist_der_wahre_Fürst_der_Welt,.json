{"textgrid.poem.61158": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Tod, du bist der wahre F\u00fcrst der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tod, du bist der wahre F\u00fcrst der Welt,", "tokens": ["O", "Tod", ",", "du", "bist", "der", "wah\u00b7re", "F\u00fcrst", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Priester bist du, der mit reinen H\u00e4nden", "tokens": ["Der", "Pries\u00b7ter", "bist", "du", ",", "der", "mit", "rei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Kranz der bleichen Stirn vermag zu spenden", "tokens": ["Den", "Kranz", "der", "blei\u00b7chen", "Stirn", "ver\u00b7mag", "zu", "spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und heil'ge Namen schreibt ans Sternenzelt.", "tokens": ["Und", "heil'\u00b7ge", "Na\u00b7men", "schreibt", "ans", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Linnentuch, zu deinem Dienst bestellt,", "tokens": ["Das", "Lin\u00b7nen\u00b7tuch", ",", "zu", "dei\u00b7ne