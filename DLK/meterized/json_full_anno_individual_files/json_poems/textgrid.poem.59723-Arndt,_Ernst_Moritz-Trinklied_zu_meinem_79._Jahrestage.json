 "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, zweimal hoch und dreimal hoch", "tokens": ["Ja", ",", "zwei\u00b7mal", "hoch", "und", "drei\u00b7mal", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Liebe, K\u00f6nigin der Erde,", "tokens": ["Dir", ",", "Lie\u00b7be", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich in s\u00fc\u00dfer Lust erzog,", "tokens": ["Die", "mich", "in", "s\u00fc\u00b7\u00dfer", "Lust", "er\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Mensch ich ward in Lichtgeb\u00e4rde!", "tokens": ["Da\u00df", "Mensch", "ich", "ward", "in", "Licht\u00b7ge\u00b7b\u00e4r\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schenkt ein, weil noch die Sonne scheint!", "tokens": ["Schenkt", "ein", ",", "weil", "noch", "die", "Son\u00b7ne", "scheint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Liebe soll mein Letztes klingen!", "tokens": ["Der", "Lie\u00b7be", "soll", "mein", "Letz\u00b7tes", "klin\u00b7gen", "!"]