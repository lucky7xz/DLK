 gestritten?", "tokens": ["Im", "Freu\u00b7den\u00b7kampf", "mit", "mir", "ge\u00b7strit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Kampfplatz fern, vom Jubeltisch,", "tokens": ["Vom", "Kampf\u00b7platz", "fern", ",", "vom", "Ju\u00b7bel\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, l\u00e4ngst vom Leben abgeglitten.", "tokens": ["Ach", ",", "l\u00e4ngst", "vom", "Le\u00b7ben", "ab\u00b7ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch schenkt mir ein! Heut will im Schwung", "tokens": ["Doch", "schenkt", "mir", "ein", "!", "Heut", "will", "im", "Schwung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ich \u00fcber Tod und Leben schweben;", "tokens": ["Ich", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schenkt voll mir ein! Heut will ich jung", "tokens": ["Schenkt", "voll", "mir", "ein", "!", "Heut", "will", "ich", "jung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+