{"textgrid.poem.59723": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied zu meinem 79. Jahrestage", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schenkt ein und reicht mir den Pokal,", "tokens": ["Schenkt", "ein", "und", "reicht", "mir", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fcllt mit Gold von edlen Weinen!", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "Gold", "von", "ed\u00b7len", "Wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heut soll ein letzter Sonnenstrahl", "tokens": ["Heut", "soll", "ein", "letz\u00b7ter", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Jugendglanz mein Haupt bescheinen!", "tokens": ["Mit", "Ju\u00b7gend\u00b7glanz", "mein", "Haupt", "be\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend Sonnen gingen zu Tal", "tokens": ["Viel", "tau\u00b7send", "Son\u00b7nen", "gin\u00b7gen", "zu", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit tr\u00fcben und mit hellen Scheinen,", "tokens": ["Mit", "tr\u00fc\u00b7ben", "und", "mit", "hel\u00b7len", "Schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch zieh' ich's Fazit aus der Zahl,", "tokens": ["Doch", "zieh'", "ich's", "Fa\u00b7zit", "aus", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wippt hoch das Lachen auf das Weinen.", "tokens": ["Wippt", "hoch", "das", "La\u00b7chen", "auf", "das", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei diesem Fazit f\u00e4llt mir ein:", "tokens": ["Bei", "die\u00b7sem", "Fa\u00b7zit", "f\u00e4llt", "mir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo seid ihr, meine Schwinger, Klinger", "tokens": ["Wo", "seid", "ihr", ",", "mei\u00b7ne", "Schwin\u00b7ger", ",", "Klin\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von gutem Eisen, gutem Wein?", "tokens": ["Von", "gu\u00b7tem", "Ei\u00b7sen", ",", "gu\u00b7tem", "Wein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo seid ihr, Klinger, Schwinger, Singer?", "tokens": ["Wo", "seid", "ihr", ",", "Klin\u00b7ger", ",", "Schwin\u00b7ger", ",", "Sin\u00b7ger", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ihr, die weiland hell und frisch", "tokens": ["Wo", "ihr", ",", "die", "wei\u00b7land", "hell", "und", "frisch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Freudenkampf mit mir gestritten?", "tokens": ["Im", "Freu\u00b7den\u00b7kampf", "mit", "mir", "ge\u00b7strit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vom Kampfplatz fern, vom Jubeltisch,", "tokens": ["Vom", "Kampf\u00b7platz", "fern", ",", "vom", "Ju\u00b7bel\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, l\u00e4ngst vom Leben abgeglitten.", "tokens": ["Ach", ",", "l\u00e4ngst", "vom", "Le\u00b7ben", "ab\u00b7ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch schenkt mir ein! Heut will im Schwung", "tokens": ["Doch", "schenkt", "mir", "ein", "!", "Heut", "will", "im", "Schwung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ich \u00fcber Tod und Leben schweben;", "tokens": ["Ich", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", "schwe\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schenkt voll mir ein! Heut will ich jung", "tokens": ["Schenkt", "voll", "mir", "ein", "!", "Heut", "will", "ich", "jung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck ein Halbjahrhundert leben \u2013", "tokens": ["Zu\u00b7r\u00fcck", "ein", "Halb\u00b7jahr\u00b7hun\u00b7dert", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und fliegen \u00fcber Staub und Grab", "tokens": ["Und", "flie\u00b7gen", "\u00fc\u00b7ber", "Staub", "und", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach oben alle guten Geister,", "tokens": ["Nach", "o\u00b7ben", "al\u00b7le", "gu\u00b7ten", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie winken heut mir Lust herab", "tokens": ["Sie", "win\u00b7ken", "heut", "mir", "Lust", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und rufen: ", "tokens": ["Und", "ru\u00b7fen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Drum schenkt mir ein! Mein vollstes Glas", "tokens": ["Drum", "schenkt", "mir", "ein", "!", "Mein", "volls\u00b7tes", "Glas"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herrscher \u00fcber Tod und Leben,", "tokens": ["Dem", "Herr\u00b7scher", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der mir ein Herz gab ohne Ha\u00df", "tokens": ["Der", "mir", "ein", "Herz", "gab", "oh\u00b7ne", "Ha\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Harm, sei h\u00f6chster Klang gegeben!", "tokens": ["Und", "Harm", ",", "sei", "h\u00f6chs\u00b7ter", "Klang", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein hoher allen, die den Greis", "tokens": ["Ein", "ho\u00b7her", "al\u00b7len", ",", "die", "den", "Greis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In seinen kalten, grauen Tagen,", "tokens": ["In", "sei\u00b7nen", "kal\u00b7ten", ",", "grau\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wof\u00fcr er kaum zu danken wei\u00df,", "tokens": ["Wo\u00b7f\u00fcr", "er", "kaum", "zu", "dan\u00b7ken", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit treuer, junger Liebe tragen!", "tokens": ["Mit", "treu\u00b7er", ",", "jun\u00b7ger", "Lie\u00b7be", "tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, zweimal hoch und dreimal hoch", "tokens": ["Ja", ",", "zwei\u00b7mal", "hoch", "und", "drei\u00b7mal", "hoch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir, Liebe, K\u00f6nigin der Erde,", "tokens": ["Dir", ",", "Lie\u00b7be", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich in s\u00fc\u00dfer Lust erzog,", "tokens": ["Die", "mich", "in", "s\u00fc\u00b7\u00dfer", "Lust", "er\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Mensch ich ward in Lichtgeb\u00e4rde!", "tokens": ["Da\u00df", "Mensch", "ich", "ward", "in", "Licht\u00b7ge\u00b7b\u00e4r\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schenkt ein, weil noch die Sonne scheint!", "tokens": ["Schenkt", "ein", ",", "weil", "noch", "die", "Son\u00b7ne", "scheint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Liebe soll mein Letztes klingen!", "tokens": ["Der", "Lie\u00b7be", "soll", "mein", "Letz\u00b7tes", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und allem, was mich freundlich meint,", "tokens": ["Und", "al\u00b7lem", ",", "was", "mich", "freund\u00b7lich", "meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Will ich damit mein Sch\u00f6nstes bringen!", "tokens": ["Will", "ich", "da\u00b7mit", "mein", "Sch\u00f6ns\u00b7tes", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schenkt ein und reicht mir den Pokal,", "tokens": ["Schenkt", "ein", "und", "reicht", "mir", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00fcllt mit Gold von edlen Weinen!", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "Gold", "von", "ed\u00b7len", "Wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heut soll ein letzter Sonnenstrahl", "tokens": ["Heut", "soll", "ein", "letz\u00b7ter", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Jugendglanz mein Haupt bescheinen!", "tokens": ["Mit", "Ju\u00b7gend\u00b7glanz", "mein", "Haupt", "be\u00b7schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend Sonnen gingen zu Tal", "tokens": ["Viel", "tau\u00b7send", "Son\u00b7nen", "gin\u00b7gen", "zu", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit tr\u00fcben und mit hellen Scheinen,", "tokens": ["Mit", "tr\u00fc\u00b7ben", "und", "mit", "hel\u00b7len", "Schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch zieh' ich's Fazit aus der Zahl,", "tokens": ["Doch", "zieh'", "ich's", "Fa\u00b7zit", "aus", "der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wippt hoch das Lachen auf das Weinen.", "tokens": ["Wippt", "hoch", "das", "La\u00b7chen", "auf", "das"