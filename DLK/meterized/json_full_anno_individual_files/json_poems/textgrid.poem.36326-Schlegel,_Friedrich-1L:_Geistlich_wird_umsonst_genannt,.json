{"textgrid.poem.36326": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geistlich wird umsonst genannt,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geistlich wird umsonst genannt,", "tokens": ["Geist\u00b7lich", "wird", "um\u00b7sonst", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer nicht Geistes Licht erkannt;", "tokens": ["Wer", "nicht", "Geis\u00b7tes", "Licht", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wissen ist des Glaubens Stern,", "tokens": ["Wis\u00b7sen", "ist", "des", "Glau\u00b7bens", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andacht alles Wissens Kern.", "tokens": ["An\u00b7dacht", "al\u00b7les", "Wis\u00b7sens", "Kern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehr' und lerne Wissenschaft,", "tokens": ["Lehr'", "und", "ler\u00b7ne", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlt dir des Gef\u00fchles Kraft", "tokens": ["Fehlt", "dir", "des", "Ge\u00b7f\u00fch\u00b7les", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Herzens frommer Sinn,", "tokens": ["Und", "des", "Her\u00b7zens", "from\u00b7mer", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00e4llt es bald zum Staube hin.", "tokens": ["F\u00e4llt", "es", "bald", "zum", "Stau\u00b7be", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sch\u00f6ner doch wird nichts gesehn,", "tokens": ["Sch\u00f6\u00b7ner", "doch", "wird", "nichts", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Als wenn die beisammen gehn:", "tokens": ["Als", "wenn", "die", "bei\u00b7sam\u00b7men", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hoher Weisheit Sonnenlicht", "tokens": ["Ho\u00b7her", "Weis\u00b7heit", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und der Kirche stille Pflicht.", "tokens": ["Und", "der", "Kir\u00b7che", "stil\u00b7le", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geistlich wird umsonst genannt,", "tokens": ["Geist\u00b7lich", "wird", "um\u00b7sonst", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "VV