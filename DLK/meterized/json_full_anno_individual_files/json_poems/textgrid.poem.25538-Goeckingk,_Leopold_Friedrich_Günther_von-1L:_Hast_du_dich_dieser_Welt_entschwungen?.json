{"textgrid.poem.25538": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast du dich dieser Welt entschwungen?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du dich dieser Welt entschwungen?", "tokens": ["Hast", "du", "dich", "die\u00b7ser", "Welt", "ent\u00b7schwun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lebst, wo Apoll und Amor spricht?", "tokens": ["Lebst", ",", "wo", "A\u00b7poll", "und", "A\u00b7mor", "spricht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "KON", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Du hast mein Lob, nicht meinen Dank erzwungen,", "tokens": ["Du", "hast", "mein", "Lob", ",", "nicht", "mei\u00b7nen", "Dank", "er\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn heucheln \u2013 ach! das kann ich nicht!", "tokens": ["Denn", "heu\u00b7cheln", "\u2013", "ach", "!", "das", "kann", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "ITJ", "$.", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und weil ich das nicht kann, (verzeihe", "tokens": ["Und", "weil", "ich", "das", "nicht", "kann", ",", "(", "ver\u00b7zei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "PDS", "PTKNEG", "VMFIN", "$,", "$(", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Liebe diesen dreisten Blick", "tokens": ["Der", "Lie\u00b7be", "die\u00b7sen", "dreis\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf solch ein Lied! denn sang es nicht die Treue?)", "tokens": ["Auf", "solch", "ein", "Lied", "!", "denn", "sang", "es", "nicht", "die", "Treu\u00b7e", "?", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$.", "KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So nehm' ich selbst mein Lob zur\u00fcck!", "tokens": ["So", "nehm'", "ich", "selbst", "mein", "Lob", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Eh' deine Stimme, durch die R\u00f6hren", "tokens": ["Eh'", "dei\u00b7ne", "Stim\u00b7me", ",", "durch", "die", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Ohrs, sich in mein Herz verlor,", "tokens": ["Des", "Ohrs", ",", "sich", "in", "mein", "Herz", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da sang ich gern von Amorn und Cytheren", "tokens": ["Da", "sang", "ich", "gern", "von", "A\u00b7morn", "und", "Cy\u00b7the\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Mir am Klaviere Lieder vor.", "tokens": ["Mir", "am", "Kla\u00b7vie\u00b7re", "Lie\u00b7der", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Oft tr\u00e4nkten sie mich mit Vergn\u00fcgen,", "tokens": ["Oft", "tr\u00e4nk\u00b7ten", "sie", "mich", "mit", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Doch mit Empfindung selten nur;", "tokens": ["Doch", "mit", "Emp\u00b7fin\u00b7dung", "sel\u00b7ten", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Denn welches Herz l\u00e4\u00dft sich so leicht betr\u00fcgen?", "tokens": ["Denn", "wel\u00b7ches", "Herz", "l\u00e4\u00dft", "sich", "so", "leicht", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "PRF", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du, du bist Wahrheit, o Natur!", "tokens": ["Du", ",", "du", "bist", "Wahr\u00b7heit", ",", "o", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VAFIN", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Du Sch\u00f6nste! hast mich angezogen!", "tokens": ["Du", "Sch\u00f6ns\u00b7te", "!", "hast", "mich", "an\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Nur aus der Plunderkammer nicht,", "tokens": ["Nur", "aus", "der", "Plun\u00b7der\u00b7kam\u00b7mer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wo \u00fcber alle K\u00f6cher, Pfeil' und Bogen,", "tokens": ["Wo", "\u00fc\u00b7ber", "al\u00b7le", "K\u00f6\u00b7cher", ",", "Pfeil'", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Oft selbst der Witz ein Bein zerbricht.", "tokens": ["Oft", "selbst", "der", "Witz", "ein", "Bein", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Du tr\u00e4nktest mich nicht aus den B\u00e4chen", "tokens": ["Du", "tr\u00e4nk\u00b7test", "mich", "nicht", "aus", "den", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.22": {"text": "S\u00fc\u00dflallender Empfindsamkeit,", "tokens": ["S\u00fc\u00df\u00b7lal\u00b7len\u00b7der", "Emp\u00b7find\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Du lehrtest nur mich biedre Worte sprechen,", "tokens": ["Du", "lehr\u00b7test", "nur", "mich", "bied\u00b7re", "Wor\u00b7te", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wie sie das Herz dem Munde beut!", "tokens": ["Wie", "sie", "das", "Herz", "dem", "Mun\u00b7de", "beut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und lockt' ich nicht in diesem Kleide,", "tokens": ["Und", "lockt'", "ich", "nicht", "in", "die\u00b7sem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Mit dieser Sprach' ihn in das Netz?", "tokens": ["Mit", "die\u00b7ser", "Sprach'", "ihn", "in", "das", "Netz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wozu denn nun des Witzes Prunkgeschmeide,", "tokens": ["Wo\u00b7zu", "denn", "nun", "des", "Wit\u00b7zes", "Prunk\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Wozu der T\u00e4ndeley Geschw\u00e4tz?", "tokens": ["Wo\u00b7zu", "der", "T\u00e4n\u00b7de\u00b7ley", "Ge\u00b7schw\u00e4tz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Einbildung den Durst zu stillen,", "tokens": ["Der", "Ein\u00b7bil\u00b7dung", "den", "Durst", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Das kannst du zwar, Mythologie!", "tokens": ["Das", "kannst", "du", "zwar", ",", "My\u00b7tho\u00b7lo\u00b7gie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Auch konntest du des R\u00f6mers Busen f\u00fcllen,", "tokens": ["Auch", "konn\u00b7test", "du", "des", "R\u00f6\u00b7mers", "Bu\u00b7sen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ihm warst du mehr als Phantasie.", "tokens": ["Ihm", "warst", "du", "mehr", "als", "Phan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ich trinke gern aus deinem Becher,", "tokens": ["Ich", "trin\u00b7ke", "gern", "aus", "dei\u00b7nem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Das Herz nur ist f\u00fcr ihn ein Sieb voll L\u00f6cher,", "tokens": ["Das", "Herz", "nur", "ist", "f\u00fcr", "ihn", "ein", "Sieb", "voll", "L\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Wenn schon der Quell der Liebe quillt.", "tokens": ["Wenn", "schon", "der", "Quell", "der", "Lie\u00b7be", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Sonst neigt' auf Amors Wunderdinge", "tokens": ["Sonst", "neigt'", "auf", "A\u00b7mors", "Wun\u00b7der\u00b7din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Die R\u00f6merin ihr Herz und Ohr;", "tokens": ["Die", "R\u00f6\u00b7me\u00b7rin", "ihr", "Herz", "und", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ich aber bin ein deutsches M\u00e4dchen! singe", "tokens": ["Ich", "a\u00b7ber", "bin", "ein", "deut\u00b7sches", "M\u00e4d\u00b7chen", "!", "sin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Du deutscher Mann! mir Wahrheit vor!", "tokens": ["Du", "deut\u00b7scher", "Mann", "!", "mir", "Wahr\u00b7heit", "vor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "