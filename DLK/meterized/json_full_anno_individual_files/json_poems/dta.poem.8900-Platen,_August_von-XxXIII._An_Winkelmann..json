{"dta.poem.8900": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.  \n  An Winkelmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "So sey der Dank daf\u00fcr an dich gewendet:", "tokens": ["So", "sey", "der", "Dank", "da\u00b7f\u00fcr", "an", "dich", "ge\u00b7wen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl fand dein Geist, was nie beginnt noch endet,", "tokens": ["Wohl", "fand", "dein", "Geist", ",", "was", "nie", "be\u00b7ginnt", "noch", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch fand er's nicht im Predigtbuch der Frommen.", "tokens": ["Doch", "fand", "er's", "nicht", "im", "Pre\u00b7digt\u00b7buch", "der", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir ist das Licht des G\u00f6ttlichen entglommen", "tokens": ["Dir", "ist", "das", "Licht", "des", "G\u00f6tt\u00b7li\u00b7chen", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Werk der Heiden, die es reich gespendet;", "tokens": ["Im", "Werk", "der", "Hei\u00b7den", ",", "die", "es", "reich", "ge\u00b7spen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn himmlisch ist, was immer ist vollendet,", "tokens": ["Denn", "himm\u00b7lisch", "ist", ",", "was", "im\u00b7mer", "ist", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Christus selbst gebietet: Seyd vollkommen!", "tokens": ["Und", "Chris\u00b7tus", "selbst", "ge\u00b7bie\u00b7tet", ":", "Seyd", "voll\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "VVPP", "$.", "VAIMP", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zwar m\u00f6chten gern gewisse schwarze R\u00f6cke", "tokens": ["Zwar", "m\u00f6ch\u00b7ten", "gern", "ge\u00b7wis\u00b7se", "schwar\u00b7ze", "R\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Geist verwickeln, der sich will befreyen,", "tokens": ["Den", "Geist", "ver\u00b7wi\u00b7ckeln", ",", "der", "sich", "will", "be\u00b7fre\u00b7yen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo nicht, uns stellen in die Zahl der B\u00f6cke.", "tokens": ["Wo", "nicht", ",", "uns", "stel\u00b7len", "in", "die", "Zahl", "der", "B\u00f6\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch la\u00dft nur ab, die Heiden zu beschreyen!", "tokens": ["Doch", "la\u00dft", "nur", "ab", ",", "die", "Hei\u00b7den", "zu", "be\u00b7schre\u00b7yen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer Seelen hauchen kann in Marmorbl\u00f6cke,", "tokens": ["Wer", "See\u00b7len", "hau\u00b7chen", "kann", "in", "Mar\u00b7mor\u00b7bl\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ist erhaben \u00fcber Litaneyen.", "tokens": ["Der", "ist", "er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "Li\u00b7ta\u00b7ne\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}