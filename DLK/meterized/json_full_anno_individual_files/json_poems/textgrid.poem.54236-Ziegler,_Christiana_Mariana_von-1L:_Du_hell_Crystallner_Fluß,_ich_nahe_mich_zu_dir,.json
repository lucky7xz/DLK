{"textgrid.poem.54236": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hell Crystallner Flu\u00df, ich nahe mich zu dir,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hell Crystallner Flu\u00df, ich nahe mich zu dir,", "tokens": ["Du", "hell", "Crys\u00b7tall\u00b7ner", "Flu\u00df", ",", "ich", "na\u00b7he", "mich", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bring in Einsamkeit dir meine Klagen f\u00fcr,", "tokens": ["Und", "bring", "in", "Ein\u00b7sam\u00b7keit", "dir", "mei\u00b7ne", "Kla\u00b7gen", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPER", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn was man nicht darff Menschen klagen,", "tokens": ["Denn", "was", "man", "nicht", "darff", "Men\u00b7schen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "PTKNEG", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das kan man doch wohl stummen Zeugen sagen.", "tokens": ["Das", "kan", "man", "doch", "wohl", "stum\u00b7men", "Zeu\u00b7gen", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ja schwieg ich auch bey dem Ger\u00e4usche deiner Wellen,", "tokens": ["Ja", "schwieg", "ich", "auch", "bey", "dem", "Ge\u00b7r\u00e4u\u00b7sche", "dei\u00b7ner", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das meiner Thr\u00e4nen-Fluth sich scheinet zu gesellen,", "tokens": ["Das", "mei\u00b7ner", "Thr\u00e4\u00b7nen\u00b7Fluth", "sich", "schei\u00b7net", "zu", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PRF", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wirst du dennoch mein Verzweiflungs-volles Wesen", "tokens": ["So", "wirst", "du", "den\u00b7noch", "mein", "Ver\u00b7zweif\u00b7lungs\u00b7vol\u00b7les", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gar leicht aus Aug und Minen lesen.", "tokens": ["Gar", "leicht", "aus", "Aug", "und", "Mi\u00b7nen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Furcht, Unruh und Verdru\u00df heist stets mein Zeitvertreib,", "tokens": ["Furcht", ",", "Un\u00b7ruh", "und", "Ver\u00b7dru\u00df", "heist", "stets", "mein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Seele lieget kranck,", "tokens": ["Die", "See\u00b7le", "lie\u00b7get", "kranck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Thr\u00e4nen sind mein Tranck,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "sind", "mein", "Tranck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Seuffzer meine Speise.", "tokens": ["Die", "Seuff\u00b7zer", "mei\u00b7ne", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Kummer-volle Brust, der abgeh\u00e4rmte Leib,", "tokens": ["Die", "Kum\u00b7mer\u00b7vol\u00b7le", "Brust", ",", "der", "ab\u00b7ge\u00b7h\u00e4rm\u00b7te", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schickt sich allm\u00e4hlig schon zu seiner letzten Reise.", "tokens": ["Schickt", "sich", "all\u00b7m\u00e4h\u00b7lig", "schon", "zu", "sei\u00b7ner", "letz\u00b7ten", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ihr Sterne! H\u00f6rt und seht ihr nicht?", "tokens": ["Ihr", "Ster\u00b7ne", "!", "H\u00f6rt", "und", "seht", "ihr", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Was die von euch Verbannte spricht.", "tokens": ["Was", "die", "von", "euch", "Ver\u00b7bann\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr schweigt bey meiner Quaal und Schmertzen,", "tokens": ["Ihr", "schweigt", "bey", "mei\u00b7ner", "Qua\u00b7al", "und", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Geht euch mein Elend nicht zu Hertzen?", "tokens": ["Geht", "euch", "mein", "E\u00b7lend", "nicht", "zu", "Hert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ich glaub, ihr seyd von Stahl und Stein,", "tokens": ["Ich", "glaub", ",", "ihr", "seyd", "von", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Weil ihr bey meiner Noth wolt unempfindlich seyn.", "tokens": ["Weil", "ihr", "bey", "mei\u00b7ner", "Noth", "wolt", "un\u00b7emp\u00b7find\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum will ich mich nur in mir selbst verzehren,", "tokens": ["Drum", "will", "ich", "mich", "nur", "in", "mir", "selbst", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich mag und will nichts mehr,", "tokens": ["Ich", "mag", "und", "will", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und wenn das Gl\u00fccke gleich schon auf dem Wege w\u00e4r,", "tokens": ["Und", "wenn", "das", "Gl\u00fc\u00b7cke", "gleich", "schon", "auf", "dem", "We\u00b7ge", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von H\u00fclff und Rettung h\u00f6ren.", "tokens": ["Von", "H\u00fclff", "und", "Ret\u00b7tung", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ich opfre meiner Jugend Lauf", "tokens": ["Ich", "opf\u00b7re", "mei\u00b7ner", "Ju\u00b7gend", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Mit Flei\u00df dem Harm und Kummer auf,", "tokens": ["Mit", "Flei\u00df", "dem", "Harm", "und", "Kum\u00b7mer", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dem will ich lieber mich zum Raub und Beute geben,", "tokens": ["Dem", "will", "ich", "lie\u00b7ber", "mich", "zum", "Raub", "und", "Beu\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PPER", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als l\u00e4nger unvergn\u00fcget leben.", "tokens": ["Als", "l\u00e4n\u00b7ger", "un\u00b7ver\u00b7gn\u00fc\u00b7get", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Verachtet mich nur nicht, ihr liebsten Quellen,", "tokens": ["Ver\u00b7ach\u00b7tet", "mich", "nur", "nicht", ",", "ihr", "liebs\u00b7ten", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die Welt mag, wie sie will, ihr Urtheil dr\u00fcber f\u00e4llen.", "tokens": ["Die", "Welt", "mag", ",", "wie", "sie", "will", ",", "ihr", "Ur\u00b7theil", "dr\u00fc\u00b7ber", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jedoch, was pr\u00e4g ich euch das Stilleschweigen ein,", "tokens": ["Je\u00b7doch", ",", "was", "pr\u00e4g", "ich", "euch", "das", "Stil\u00b7le\u00b7schwei\u00b7gen", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ihr k\u00f6nt ja nicht Verr\u00e4ther seyn.", "tokens": ["Ihr", "k\u00f6nt", "ja", "nicht", "Ver\u00b7r\u00e4\u00b7ther", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wenn gleich die Welt, so mich vermist,", "tokens": ["Wenn", "gleich", "die", "Welt", ",", "so", "mich", "ver\u00b7mist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Euch auszufragen l\u00fcstern ist.", "tokens": ["Euch", "aus\u00b7zu\u00b7fra\u00b7gen", "l\u00fcs\u00b7tern", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Ich hab euch meine Noth zwar in der Angst erzehlet,", "tokens": ["Ich", "hab", "euch", "mei\u00b7ne", "Noth", "zwar", "in", "der", "Angst", "er\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Doch auch mit allen Flei\u00df den Quell davon verh\u00f6hlet.", "tokens": ["Doch", "auch", "mit", "al\u00b7len", "Flei\u00df", "den", "Quell", "da\u00b7von", "ver\u00b7h\u00f6h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ART", "NN", "PAV", "VVFIN", "$."], "m