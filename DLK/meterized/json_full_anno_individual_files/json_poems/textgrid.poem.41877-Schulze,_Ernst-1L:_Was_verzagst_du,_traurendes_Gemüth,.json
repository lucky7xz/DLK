s": ["Mut\u00b7hig", "strebt", "der", "ed\u00b7le", "Geist", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu der W\u00fcnsche luftig holdem Reich,", "tokens": ["Zu", "der", "W\u00fcn\u00b7sche", "luf\u00b7tig", "hol\u00b7dem", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Thron, zu dem er sich erhoben,", "tokens": ["Und", "der", "Thron", ",", "zu", "dem", "er", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sinkt mit seinem Hoffen nicht zugleich.", "tokens": ["Sinkt", "mit", "sei\u00b7nem", "Hof\u00b7fen", "nicht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ward der Kampf vergebens auch begonnen,", "tokens": ["Ward", "der", "Kampf", "ver\u00b7ge\u00b7bens", "auch", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00fcrd'ger macht die M\u00fche dich dem Ziel;", "tokens": ["W\u00fcrd'\u00b7ger", "macht", "die", "M\u00fc\u00b7he", "dich", "dem", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Zage nicht; gewagt ist stets gewonnen!", "tokens": ["Za\u00b7ge", "nicht", ";", "ge\u00b7wagt", "ist", "stets", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "VVPP", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Hoher Sinn liegt oft im kind'schen Spiel.", "tokens": ["Ho\u00b7her", "Sinn", "liegt", "oft", "im", "kin\u00b7d'\u00b7schen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}