{"textgrid.poem.41877": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was verzagst du, traurendes Gem\u00fcth,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was verzagst du, traurendes Gem\u00fcth,", "tokens": ["Was", "ver\u00b7zagst", "du", ",", "trau\u00b7ren\u00b7des", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bildest stets zum Leid dir neue Leiden?", "tokens": ["Bil\u00b7dest", "stets", "zum", "Leid", "dir", "neu\u00b7e", "Lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Armes Herz, da dich die Liebe flieht,", "tokens": ["Ar\u00b7mes", "Herz", ",", "da", "dich", "die", "Lie\u00b7be", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Willst auch du die Liebe z\u00fcrnend meiden?", "tokens": ["Willst", "auch", "du", "die", "Lie\u00b7be", "z\u00fcr\u00b7nend", "mei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Was dich tief und m\u00e4chtig einst erf\u00fcllt,", "tokens": ["Was", "dich", "tief", "und", "m\u00e4ch\u00b7tig", "einst", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Halt' es fest in Freuden und in Schmerzen,", "tokens": ["Halt'", "es", "fest", "in", "Freu\u00b7den", "und", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Jeder Gram, den dir die Zeit verh\u00fcllt,", "tokens": ["Je\u00b7der", "Gram", ",", "den", "dir", "die", "Zeit", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "War ein heil'ger Schmuck in deinem Herzen.", "tokens": ["War", "ein", "heil'\u00b7ger", "Schmuck", "in", "dei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Kann der Blinde wohl von Farb' und Glanz,", "tokens": ["Kann", "der", "Blin\u00b7de", "wohl", "von", "Fa\u00b7rb'", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Kann vom Klang der Taubgeborne tr\u00e4umen?", "tokens": ["Kann", "vom", "Klang", "der", "Taub\u00b7ge\u00b7bor\u00b7ne", "tr\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Was du ahnst, das t\u00e4uscht dich nimmer ganz;", "tokens": ["Was", "du", "ahnst", ",", "das", "t\u00e4uscht", "dich", "nim\u00b7mer", "ganz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Wort gehalten wird in jenen R\u00e4umen.", "tokens": ["Wort", "ge\u00b7hal\u00b7ten", "wird", "in", "je\u00b7nen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Doch auch hier soll nie der Geist verzagen,", "tokens": ["Doch", "auch", "hier", "soll", "nie", "der", "Geist", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll getrost in jedem Kampfe stehn;", "tokens": ["Soll", "ge\u00b7trost", "in", "je\u00b7dem", "Kamp\u00b7fe", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herrlich ist's, ein gro\u00dfes Leid zu tragen,", "tokens": ["Herr\u00b7lich", "ist's", ",", "ein", "gro\u00b7\u00dfes", "Leid", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00f6ttlich ist's, in Liebe zu vergehn.", "tokens": ["G\u00f6tt\u00b7lich", "ist's", ",", "in", "Lie\u00b7be", "zu", "ver\u00b7gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kalt und todt und deutungslos entschwindet", "tokens": ["Kalt", "und", "todt", "und", "deu\u00b7tungs\u00b7los", "ent\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jedes Bild der unbewegten Brust;", "tokens": ["Je\u00b7des", "Bild", "der", "un\u00b7be\u00b7weg\u00b7ten", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Nur der Gott, der in uns wohnt, empfindet", "tokens": ["Nur", "der", "Gott", ",", "der", "in", "uns", "wohnt", ",", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tiefen Schmerz und wunderbare Lust.", "tokens": ["Tie\u00b7fen", "Schmerz", "und", "wun\u00b7der\u00b7ba\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Wird auch nie das Kleinod dir beschieden,", "tokens": ["Wird", "auch", "nie", "das", "Klei\u00b7nod", "dir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Schon die Sehnsucht ist ein heil'ges Ziel,", "tokens": ["Schon", "die", "Sehn\u00b7sucht", "ist", "ein", "heil'\u00b7ges", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und es bl\u00fcht die Palme schon hienieden", "tokens": ["Und", "es", "bl\u00fcht", "die", "Pal\u00b7me", "schon", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Jedem sch\u00f6nen, gl\u00e4ubigen Gef\u00fchl.", "tokens": ["Je\u00b7dem", "sch\u00f6\u00b7nen", ",", "gl\u00e4u\u00b7bi\u00b7gen", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Mag der Herbst das welke Laub zerstreun,", "tokens": ["Mag", "der", "Herbst", "das", "wel\u00b7ke", "Laub", "zer\u00b7streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mag der Sturm die Bl\u00fcthen dir entf\u00fchren,", "tokens": ["Mag", "der", "Sturm", "die", "Bl\u00fc\u00b7then", "dir", "ent\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Was du liebst, das bleibt auf ewig dein;", "tokens": ["Was", "du", "liebst", ",", "das", "bleibt", "auf", "e\u00b7wig", "dein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nimmer kann das Herz sich selbst verlieren.", "tokens": ["Nim\u00b7mer", "kann", "das", "Herz", "sich", "selbst", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Z\u00fcrne nicht, wenn dich die Welt begr\u00e4nzt;", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "wenn", "dich", "die", "Welt", "be\u00b7gr\u00e4nzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Irdisch ist und endlich jede Schranke,", "tokens": ["Ir\u00b7disch", "ist", "und", "end\u00b7lich", "je\u00b7de", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und im hartbedr\u00e4ngten Herzen gl\u00e4nzt", "tokens": ["Und", "im", "hart\u00b7be\u00b7dr\u00e4ng\u00b7ten", "Her\u00b7zen", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Leuchtender ein g\u00f6ttlicher Gedanke.", "tokens": ["Leuch\u00b7ten\u00b7der", "ein", "g\u00f6tt\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "An dem Glauben bricht des Todes Macht,", "tokens": ["An", "dem", "Glau\u00b7ben", "bricht", "des", "To\u00b7des", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Aus dem Grabe wird die Hoffnung keimen;", "tokens": ["Aus", "dem", "Gra\u00b7be", "wird", "die", "Hoff\u00b7nung", "kei\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Nur der Zweifel irrt in ew'ger Nacht.", "tokens": ["Nur", "der", "Zwei\u00b7fel", "irrt", "in", "ew'\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Wage du zu hoffen und zu tr\u00e4umen!", "tokens": ["Wa\u00b7ge", "du", "zu", "hof\u00b7fen", "und", "zu", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn auch um der Hoffnung Zauberwelten", "tokens": ["Wenn", "auch", "um", "der", "Hoff\u00b7nung", "Zau\u00b7ber\u00b7wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Finster oft ein Sturmgew\u00f6lk sich zog,", "tokens": ["Fins\u00b7ter", "oft", "ein", "Sturm\u00b7ge\u00b7w\u00f6lk", "sich", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df es nie die Tr\u00f6sterin entgelten,", "tokens": ["La\u00df", "es", "nie", "die", "Tr\u00f6s\u00b7te\u00b7rin", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df das Schicksal feindlich dich betrog.", "tokens": ["Da\u00df", "das", "Schick\u00b7sal", "feind\u00b7lich", "dich", "be\u00b7trog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Muthig strebt der edle Geist nach oben", "tokens": ["Mut\u00b7hig", "strebt", "der", "ed\u00b7le", "Geist", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu der W\u00fcnsche luftig holdem Reich,", "tokens": ["Zu", "der", "W\u00fcn\u00b7sche", "luf\u00b7tig", "hol\u00b7dem", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und der Thron, zu dem er sich erhoben,", "tokens": ["Und", "der", "Thron", ",", "zu", "dem", "er", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sinkt mit seinem Hoffen nicht zugleich.", "tokens": ["Sinkt", "mit", "sei\u00b7nem", "Hof\u00b7fen", "nicht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ward der Kampf vergebens auch begonnen,", "tokens": ["Ward", "der", "Kampf", "ver\u00b7ge\u00b7bens", "auch", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "W\u00fcrd'ger macht die M\u00fche dich dem Ziel;", "tokens": ["W\u00fcrd'\u00b7ger", "macht", "die", "M\u00fc\u00b7he", "dich", "dem", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Zage nicht; gewagt ist stets gewonnen!", "tokens": ["Za\u00b7ge", "nicht", ";", "ge\u00b7wagt", "ist", "stets", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "VVPP", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Hoher Sinn liegt oft im kind'schen Spiel.", "tokens": ["Ho\u00b7her", "Sinn", "liegt", "oft", "im", "kin\u00b7d'\u00b7schen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Was verzagst du, traurendes Gem\u00fcth,", "tokens": ["Was", "ver\u00b7zagst", "du", ",", "trau\u00b7ren\u00b7des", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bildest stets zum Leid dir neue Leiden?", "tokens": ["Bil\u00b7dest", "