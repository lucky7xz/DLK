{"textgrid.poem.49448": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin das Kaiserreich an seiner letzten Wende]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin das Kaiserreich an seiner letzten Wende,", "tokens": ["Ich", "bin", "das", "Kai\u00b7ser\u00b7reich", "an", "sei\u00b7ner", "letz\u00b7ten", "Wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem vorbeizieht der Barbaren blonde Flut,", "tokens": ["An", "dem", "vor\u00b7bei\u00b7zieht", "der", "Bar\u00b7ba\u00b7ren", "blon\u00b7de", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Akrostychen sinnt, auf denen m\u00fcde ruht", "tokens": ["Das", "Ak\u00b7ros\u00b7ty\u00b7chen", "sinnt", ",", "auf", "de\u00b7nen", "m\u00fc\u00b7de", "ruht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein sp\u00e4tes Sonnenlicht, wie flimmernd Goldgeblende.", "tokens": ["Ein", "sp\u00e4\u00b7tes", "Son\u00b7nen\u00b7licht", ",", "wie", "flim\u00b7mernd", "Gold\u00b7ge\u00b7blen\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die kranke Seele f\u00fchlt mit dumpfem Weh das Ende.", "tokens": ["Die", "kran\u00b7ke", "See\u00b7le", "f\u00fchlt", "mit", "dum\u00b7pfem", "Weh", "das", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort unten, sagt man, str\u00f6mt in schweren K\u00e4mpfen Blut.", "tokens": ["Dort", "un\u00b7ten", ",", "sagt", "man", ",", "str\u00f6mt", "in", "schwe\u00b7ren", "K\u00e4mp\u00b7fen", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "PIS", "$,", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O nicht dabei zu sein! So schwach und lahm der Mut,", "tokens": ["O", "nicht", "da\u00b7bei", "zu", "sein", "!", "So", "schwach", "und", "lahm", "der", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PAV", "PTKZU", "VAINF", "$.", "ADV", "ADJD", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O dass das Leben nicht so bl\u00fctenlos entschw\u00e4nde!", "tokens": ["O", "dass", "das", "Le\u00b7ben", "nicht", "so", "bl\u00fc\u00b7ten\u00b7los", "ent\u00b7schw\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "NN", "PTKNEG", "ADV", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O ernstes Wollen nicht, noch wahre Kraft zum Tod!", "tokens": ["O", "erns\u00b7tes", "Wol\u00b7len", "nicht", ",", "noch", "wah\u00b7re", "Kraft", "zum", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKNEG", "$,", "ADV", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Getrunken alles. Endest du Bathyll dein Lachen?", "tokens": ["Ge\u00b7trun\u00b7ken", "al\u00b7les", ".", "En\u00b7dest", "du", "Ba\u00b7thyll", "dein", "La\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$.", "VVFIN", "PPER", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Getrunken alles und verzehrt \u2013 nichts mehr zu machen.", "tokens": ["Ge\u00b7trun\u00b7ken", "al\u00b7les", "und", "ver\u00b7zehrt", "\u2013", "nichts", "mehr", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KON", "VVPP", "$(", "PIS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein eitel Lied nur, wert, dass es in Feuer loht,", "tokens": ["Ein", "ei\u00b7tel", "Lied", "nur", ",", "wert", ",", "dass", "es", "in", "Feu\u00b7er", "loht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "$,", "ADJD", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein wenig \u00c4rger mit der Sklaven Schar, der tr\u00e4gen,", "tokens": ["Ein", "we\u00b7nig", "\u00c4r\u00b7ger", "mit", "der", "Skla\u00b7ven", "Schar", ",", "der", "tr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT"