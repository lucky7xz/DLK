{"dta.poem.13233": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enoch eines aber ist, das meine Seele r\u00fchrt,", "tokens": ["\u201e", "noch", "ei\u00b7nes", "a\u00b7ber", "ist", ",", "das", "mei\u00b7ne", "See\u00b7le", "r\u00fchrt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "ADV", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eauch vieler Freunde Muth in Furcht und Zweifel f\u00fchrt;", "tokens": ["\u201e", "auch", "vie\u00b7ler", "Freun\u00b7de", "Muth", "in", "Furcht", "und", "Zwei\u00b7fel", "f\u00fchrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "75\u201dVerg\u00f6nnt, Freundinnen! es in K\u00fcrze vorzutragen;", "tokens": ["\"", "Ver\u00b7g\u00f6nnt", ",", "Freun\u00b7din\u00b7nen", "!", "es", "in", "K\u00fcr\u00b7ze", "vor\u00b7zu\u00b7tra\u00b7gen", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "NN", "$.", "PPER", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder Sache Wichtigkeit verdient, darnach zu fragen:", "tokens": ["\u201e", "der", "Sa\u00b7che", "Wich\u00b7tig\u00b7keit", "ver\u00b7dient", ",", "dar\u00b7nach", "zu", "fra\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "p