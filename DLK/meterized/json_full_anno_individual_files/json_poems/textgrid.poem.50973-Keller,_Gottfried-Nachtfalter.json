{"textgrid.poem.50973": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Nachtfalter", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ermattet von des Tages Not und Pein,", "tokens": ["Er\u00b7mat\u00b7tet", "von", "des", "Ta\u00b7ges", "Not", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die nur auf Wiedersehen von mir schied,", "tokens": ["Die", "nur", "auf", "Wie\u00b7der\u00b7se\u00b7hen", "von", "mir", "schied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sa\u00df ich und schrieb bei einer Kerze Schein,", "tokens": ["Sa\u00df", "ich", "und", "schrieb", "bei", "ei\u00b7ner", "Ker\u00b7ze", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schrieb ein wild und gottverleugnend Lied.", "tokens": ["Und", "schrieb", "ein", "wild", "und", "gott\u00b7ver\u00b7leug\u00b7nend", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch drau\u00dfen lag die klare Sommernacht,", "tokens": ["Doch", "drau\u00b7\u00dfen", "lag", "die", "kla\u00b7re", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mild gr\u00fc\u00dft mein armes Licht der Mondenstrahl,", "tokens": ["Mild", "gr\u00fc\u00dft", "mein", "ar\u00b7mes", "Licht", "der", "Mon\u00b7dens\u00b7trahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und aller Sterne volle goldne Pracht", "tokens": ["Und", "al\u00b7ler", "Ster\u00b7ne", "vol\u00b7le", "gold\u00b7ne", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schaut hoch herab auf mich vom blauen Saal.", "tokens": ["Schaut", "hoch", "her\u00b7ab", "auf", "mich", "vom", "blau\u00b7en", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Am offnen Fenster bl\u00fchen dunkle Nelken,", "tokens": ["Am", "off\u00b7nen", "Fens\u00b7ter", "bl\u00fc\u00b7hen", "dunk\u00b7le", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vielleicht die letzte Nacht vor ihrem Welken.", "tokens": ["Viel\u00b7leicht", "die", "letz\u00b7te", "Nacht", "vor", "ih\u00b7rem", "Wel\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie ich schreib an meinem H\u00f6llenpsalter,", "tokens": ["Und", "wie", "ich", "schreib", "an", "mei\u00b7nem", "H\u00f6l\u00b7len\u00b7psal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die s\u00fc\u00dfe Nacht im Zorne von mir weisend,", "tokens": ["Die", "s\u00fc\u00b7\u00dfe", "Nacht", "im", "Zor\u00b7ne", "von", "mir", "wei\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schwebt herein zu mir ein grauer Falter,", "tokens": ["Da", "schwebt", "her\u00b7ein", "zu", "mir", "ein", "grau\u00b7er", "Fal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit blinder Hast der Kerze Docht umkreisend;", "tokens": ["Mit", "blin\u00b7der", "Hast", "der", "Ker\u00b7ze", "Docht", "um\u00b7krei\u00b7send", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wohl wie sein Schicksal flackerte das Licht,", "tokens": ["Wohl", "wie", "sein", "Schick\u00b7sal", "fla\u00b7cker\u00b7te", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann z\u00fcngelt' seine Flamme still empor", "tokens": ["Dann", "z\u00fcn\u00b7gelt'", "sei\u00b7ne", "Flam\u00b7me", "st