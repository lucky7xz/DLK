{"dta.poem.6916": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, wer kan von so viel Bildern,", "tokens": ["Al\u00b7lein", ",", "wer", "kan", "von", "so", "viel", "Bil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit ohne Pinsel schildern?", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "oh\u00b7ne", "Pin\u00b7sel", "schil\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer kan, sonder Stoff und Zeug zu sehn,", "tokens": ["Und", "wer", "kan", ",", "son\u00b7der", "Stoff", "und", "Zeug", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "$,", "KON", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein sichtbarlich Geb\u00e4u erh\u00f6hn.", "tokens": ["Ein", "sicht\u00b7bar\u00b7lich", "Ge\u00b7b\u00e4u", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woher erh\u00e4lt und nimmt ein forschendes Gem\u00fcth,", "tokens": ["Wo\u00b7her", "er\u00b7h\u00e4lt", "und", "nimmt", "ein", "for\u00b7schen\u00b7des", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schildereyen Meng\u2019 und Z\u00fcge, die es sieht?", "tokens": ["Der", "Schil\u00b7de\u00b7re\u00b7yen", "Meng'", "und", "Z\u00fc\u00b7ge", ",", "die", "es", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}