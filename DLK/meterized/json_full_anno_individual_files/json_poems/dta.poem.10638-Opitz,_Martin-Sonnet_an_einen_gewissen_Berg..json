{"dta.poem.10638": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet an einen gewissen Berg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv gr\u00fcner Berg/ der du mit zweyen Spitzen", "tokens": ["Dv", "gr\u00fc\u00b7ner", "Berg", "/", "der", "du", "mit", "zwe\u00b7yen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Parnasso gleichst/ du hoher Fel\u00df/ bey dir", "tokens": ["Par\u00b7nas\u00b7so", "gleichst", "/", "du", "ho\u00b7her", "Fel\u00df", "/", "bey", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$(", "PPER", "ADJA", "NN", "$(", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "W\u00fcnsch ich in Ruh zubleiben f\u00fcr vnd f\u00fcr/", "tokens": ["W\u00fcnsch", "ich", "in", "Ruh", "zu\u00b7blei\u00b7ben", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVINF", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vnd deine Lust gantz einsam zubesitzen/", "tokens": ["Vnd", "dei\u00b7ne", "Lust", "gantz", "ein\u00b7sam", "zu\u00b7be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weil du mir auch vor aller Welt kanst n\u00fctzen/", "tokens": ["Weil", "du", "mir", "auch", "vor", "al\u00b7ler", "Welt", "kanst", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PIAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dann wann ich bin auff deinen Klippen hier", "tokens": ["Dann", "wann", "ich", "bin", "auff", "dei\u00b7nen", "Klip\u00b7pen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Seh\u2019 ich allzeit der jenen orth f\u00fcr mir/", "tokens": ["Seh'", "ich", "all\u00b7zeit", "der", "je\u00b7nen", "orth", "f\u00fcr", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "PDAT", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die f\u00fcr dem Tod alleine mich kan sch\u00fctzen/", "tokens": ["Die", "f\u00fcr", "dem", "Tod", "al\u00b7lei\u00b7ne", "mich", "kan", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mein h\u00f6chste Frewd vnd meines Lebens Leben:", "tokens": ["Mein", "h\u00f6chs\u00b7te", "Frewd", "vnd", "mei\u00b7nes", "Le\u00b7bens", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So wei\u00df ich auch/ da\u00df man sonst nirgendt findt", "tokens": ["So", "wei\u00df", "ich", "auch", "/", "da\u00df", "man", "sonst", "nir\u00b7gendt", "findt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PIS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit solcher Zier ein einig orth vmbgeben/", "tokens": ["Mit", "sol\u00b7cher", "Zier", "ein", "ei\u00b7nig", "orth", "vmb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Natura hat die Lust allher gesetzet/", "tokens": ["Na\u00b7tu\u00b7ra", "hat", "die", "Lust", "all\u00b7her", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df/ die auff dich mit M\u00fch gestiegen sindt/", "tokens": ["Da\u00df", "/", "die", "auff", "dich", "mit", "M\u00fch", "ge\u00b7stie\u00b7gen", "sindt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ART", "APPR", "PRF", "APPR", "NN", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hinwiderumb auch w\u00fcrden recht ergetzet.", "tokens": ["Hin\u00b7wi\u00b7de\u00b7rumb", "auch", "w\u00fcr\u00b7den", "recht", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}