{"dta.poem.18799": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "An vollen B\u00fcschelzweigen,", "tokens": ["An", "vol\u00b7len", "B\u00fc\u00b7schel\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geliebte, sieh\u2019 nur hin!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "sieh'", "nur", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lass dir die Fr\u00fcchte zeigen", "tokens": ["Lass", "dir", "die", "Fr\u00fcch\u00b7te", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umschalet stachlig gr\u00fcn.", "tokens": ["Um\u00b7scha\u00b7let", "stach\u00b7lig", "gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie h\u00e4ngen l\u00e4ngst geballet,", "tokens": ["Sie", "h\u00e4n\u00b7gen", "l\u00e4ngst", "ge\u00b7bal\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Still, unbekannt mit sich,", "tokens": ["Still", ",", "un\u00b7be\u00b7kannt", "mit", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ast der schaukelnd wallet", "tokens": ["Ein", "Ast", "der", "schau\u00b7kelnd", "wal\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wiegt sie geduldiglich.", "tokens": ["Wiegt", "sie", "ge\u00b7dul\u00b7dig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch immer reift von Innen", "tokens": ["Doch", "im\u00b7mer", "reift", "von", "In\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schwillt der braune Kern,", "tokens": ["Und", "schwillt", "der", "brau\u00b7ne", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er m\u00f6chte Luft gewinnen", "tokens": ["Er", "m\u00f6ch\u00b7te", "Luft", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und s\u00e4h die Sonne gern.", "tokens": ["Und", "s\u00e4h", "die", "Son\u00b7ne", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Schale platzt und nieder", "tokens": ["Die", "Scha\u00b7le", "platzt", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht er sich freudig los;", "tokens": ["Macht", "er", "sich", "freu\u00b7dig", "los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So fallen meine Lieder", "tokens": ["So", "fal\u00b7len", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geh\u00e4uft in deinen Schoos.", "tokens": ["Ge\u00b7h\u00e4uft", "in", "dei\u00b7nen", "Schoos", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}