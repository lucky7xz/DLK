{"textgrid.poem.40026": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Indem ich j\u00fcngst im Herbst erblickte,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst im Herbst erblickte,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", "im", "Herbst", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie, auf das neu, des Gartens-Flur", "tokens": ["Wie", ",", "auf", "das", "neu", ",", "des", "Gar\u00b7ten\u00b7sFlur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch bunt Aurikelchen, als wie im Fr\u00fchling, schm\u00fcckte;", "tokens": ["Manch", "bunt", "Au\u00b7ri\u00b7kel\u00b7chen", ",", "als", "wie", "im", "Fr\u00fch\u00b7ling", ",", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ergetzt' ich mich daran. Es war mein Blick nicht nur,", "tokens": ["Er\u00b7getzt'", "ich", "mich", "da\u00b7ran", ".", "Es", "war", "mein", "Blick", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch ihre Wiederkunft, ger\u00fchrt;", "tokens": ["Durch", "ih\u00b7re", "Wie\u00b7der\u00b7kunft", ",", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele selber ward, f\u00fcr Lust, die sie versp\u00fchrt,", "tokens": ["Die", "See\u00b7le", "sel\u00b7ber", "ward", ",", "f\u00fcr", "Lust", ",", "die", "sie", "ver\u00b7sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bewegt, erf\u00fcllt und eingenommen.", "tokens": ["Be\u00b7wegt", ",", "er\u00b7f\u00fcllt", "und", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hie\u00df das Bl\u00fchmchen sanft, in meinem Sinn, willkommen!", "tokens": ["Ich", "hie\u00df", "das", "Bl\u00fch\u00b7mchen", "sanft", ",", "in", "mei\u00b7nem", "Sinn", ",", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und fielen, bey der Farben buntem Schein,", "tokens": ["Und", "fie\u00b7len", ",", "bey", "der", "Far\u00b7ben", "bun\u00b7tem", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mir die Gedancken ein:", "tokens": ["Mir", "die", "Ge\u00b7dan\u00b7cken", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es kommt dein wiederhohlter Flor", "tokens": ["Es", "kommt", "dein", "wie\u00b7der\u00b7hohl\u00b7ter", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir gleichsam vor,", "tokens": ["Mir", "gleich\u00b7sam", "vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als wenn, vor andern Bluhmen allen,", "tokens": ["Als", "wenn", ",", "vor", "an\u00b7dern", "Bluh\u00b7men", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein starcker Trieb in deinen R\u00f6hren,", "tokens": ["Ein", "star\u00b7cker", "Trieb", "in", "dei\u00b7nen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Sch\u00f6pfer der Natur zu ehren,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebte Bluhme, m\u00fcsse wallen.", "tokens": ["Ge\u00b7lieb\u00b7te", "Bluh\u00b7me", ",", "m\u00fcs\u00b7se", "wal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es scheint', ob h\u00f6rt' ich dich, mit bunten Lippen, sagen:", "tokens": ["Es", "scheint'", ",", "ob", "h\u00f6rt'", "ich", "dich", ",", "mit", "bun\u00b7ten", "Lip\u00b7pen", ",", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "PRF", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Noch eh die lange Winter-Nacht", "tokens": ["Noch", "eh", "die", "lan\u00b7ge", "Win\u00b7ter\u00b7Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich zu dem langen Schlaf gebracht,", "tokens": ["Mich", "zu", "dem", "lan\u00b7gen", "Schlaf", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ichs vorher noch einmahl wagen,", "tokens": ["Will", "ichs", "vor\u00b7her", "noch", "ein\u00b7mahl", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, Gott zum Ruhm, noch einmahl Bluhmen tragen.", "tokens": ["Und", ",", "Gott", "zum", "Ruhm", ",", "noch", "ein\u00b7mahl", "Bluh\u00b7men", "tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "APPRART", "NN", "$,", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vielleicht ger\u00e4th mein sp\u00e4tes Bl\u00fchen mehr,", "tokens": ["Viel\u00b7leicht", "ge\u00b7r\u00e4\u00b7th", "mein", "sp\u00e4\u00b7tes", "Bl\u00fc\u00b7hen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Als meine Bl\u00fcth', im Fr\u00fchling, Gott zur Ehr'!", "tokens": ["Als", "mei\u00b7ne", "Bl\u00fcth'", ",", "im", "Fr\u00fch\u00b7ling", ",", "Gott", "zur", "Ehr'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vielleicht wirft mancher Mensch auf mich mehr, als vorhin,", "tokens": ["Viel\u00b7leicht", "wirft", "man\u00b7cher", "Mensch", "auf", "mich", "mehr", ",", "als", "vor\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "PPER", "ADV", "$,", "KOUS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So Blick als Sinn!", "tokens": ["So", "Blick", "als", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vielleicht kann ihn mein fremd- und unverhofftes Bl\u00fchen,", "tokens": ["Viel\u00b7leicht", "kann", "ihn", "mein", "frem\u00b7d", "und", "un\u00b7ver\u00b7hoff\u00b7tes", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Zu einiger Betrachtung, ziehen", "tokens": ["Zu", "ei\u00b7ni\u00b7ger", "Be\u00b7trach\u00b7tung", ",", "zie\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und seinen Geist durchs Ungewohnte lenken,", "tokens": ["Und", "sei\u00b7nen", "Geist", "durchs", "Un\u00b7ge\u00b7wohn\u00b7te", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Auf sein- und meinen Herrn zu dencken!", "tokens": ["Auf", "sein", "und", "mei\u00b7nen", "Herrn", "zu", "den\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, du hast recht, geliebtes Bl\u00fchmchen, ja!", "tokens": ["Ja", ",", "du", "hast", "recht", ",", "ge\u00b7lieb\u00b7tes", "Bl\u00fch\u00b7mchen", ",", "ja", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du hast nicht mich allein,", "tokens": ["Du", "hast", "nicht", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch deinen unverhofften Schein,", "tokens": ["Durch", "dei\u00b7nen", "un\u00b7ver\u00b7hoff\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs neue zur Aufmercksamkeit gebracht;", "tokens": ["Aufs", "neu\u00b7e", "zur", "Auf\u00b7merck\u00b7sam\u00b7keit", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein jeder fast, wie ich mit Freuden sah,", "tokens": ["Ein", "je\u00b7der", "fast", ",", "wie", "ich", "mit", "Freu\u00b7den", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Neuigkeit bewogen,", "tokens": ["Von", "Neu\u00b7ig\u00b7keit", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ward, durch geheimen Zwang, zu dir gezogen,", "tokens": ["Ward", ",", "durch", "ge\u00b7hei\u00b7men", "Zwang", ",", "zu", "dir", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nahm der sch\u00f6nen Farben Pracht", "tokens": ["Und", "nahm", "der", "sch\u00f6\u00b7nen", "Far\u00b7ben", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In dir, mehr als vorhin in acht.", "tokens": ["In", "dir", ",", "mehr", "als", "vor\u00b7hin", "in", "acht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PIAT", "KOKOM", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! m\u00f6chte doch dein l\u00f6blich Bl\u00fch'n und Gr\u00fcnen,", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "doch", "dein", "l\u00f6b\u00b7lich", "Bl\u00fch'n", "und", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "ADV", "PPOSAT", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deiner fr\u00fch- und sp\u00e4ten Zier,", "tokens": ["Mit", "dei\u00b7ner", "fr\u00fch", "und", "sp\u00e4\u00b7ten", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geliebtes Bl\u00fchmchen, denn auch mir", "tokens": ["Ge\u00b7lieb\u00b7tes", "Bl\u00fch\u00b7mchen", ",", "denn", "auch", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einer Folge dienen!", "tokens": ["Zu", "ei\u00b7ner", "Fol\u00b7ge", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, m\u00f6chte mich dein Beyspiel lehren,", "tokens": ["Ach", ",", "m\u00f6ch\u00b7te", "mich", "dein", "Bey\u00b7spiel", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sch\u00f6pfer der Natur, so fr\u00fch als sp\u00e4t, zu ehren!", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", ",", "so", "fr\u00fch", "als", "sp\u00e4t", ",", "zu", "eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Indem ich j\u00fcngst im Herbst erblickte,", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", "im", "Herbst", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie, auf das neu, des Gartens-Flur", "tokens": ["Wie", ",", "auf", "das", "neu", ",", "des", "Gar\u00b7ten\u00b7sFlur"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch bunt Aurikelchen, als wie im Fr\u00fchling, schm\u00fcckte;", "tokens": ["Manch", "bunt", "Au\u00b7ri\u00b7kel\u00b7chen", ",", "als", "wie", "im", "Fr\u00fch\u00b7ling", ",", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ergetzt' ich mich daran. Es war mein Blick nicht nur,", "tokens": ["Er\u00b7getzt'", "ich", "mich", "da\u00b7ran", ".", "Es", "war", "mein", "Blick", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PAV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch ihre Wiederkunft, ger\u00fchrt;", "tokens": ["Durch", "ih\u00b7re", "Wie\u00b7der\u00b7kunft", ",", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele selber ward, f\u00fcr Lust, die sie versp\u00fchrt,", "tokens": ["Die", "See\u00b7le", "sel\u00b7ber", "ward", ",", "f\u00fcr", "Lust", ",", "die", "sie", "ver\u00b7sp\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bewegt, erf\u00fcllt und eingenommen.", "tokens": ["Be\u00b7wegt", ",", "er\u00b7f\u00fcllt", "und", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich hie\u00df das Bl\u00fchmchen sanft, in meinem Sinn, willkommen!", "tokens": ["Ich", "hie\u00df", "das", "Bl\u00fch\u00b7mchen", "sanft", ",", "in", "mei\u00b7nem", "Sinn", ",", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und fielen, bey der Farben buntem Schein,", "tokens": ["Und", "fie\u00b7len", ",", "bey", "der", "Far\u00b7ben", "bun\u00b7tem", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mir die Gedancken ein:", "tokens": ["Mir", "die", "Ge\u00b7dan\u00b7cken", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es kommt dein wiederhohlter Flor", "tokens": ["Es", "kommt", "dein", "wie\u00b7der\u00b7hohl\u00b7ter", "Flor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir gleichsam vor,", "tokens": ["Mir", "gleich\u00b7sam", "vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als wenn, vor andern Bluhmen allen,", "tokens": ["Als", "wenn", ",", "vor", "an\u00b7dern", "Bluh\u00b7men", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein starcker Trieb in deinen R\u00f6hren,", "tokens": ["Ein", "star\u00b7cker", "Trieb", "in", "dei\u00b7nen", "R\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Sch\u00f6pfer der Natur zu ehren,", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geliebte Bluhme, m\u00fcsse wallen.", "tokens": ["Ge\u00b7lieb\u00b7te", "Bluh\u00b7me", ",", "m\u00fcs\u