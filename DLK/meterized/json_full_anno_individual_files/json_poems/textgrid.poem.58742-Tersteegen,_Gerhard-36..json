{"textgrid.poem.58742": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er heilet alle deine Gebrechen. Psalm 103, 3", "tokens": ["Er", "hei\u00b7let", "al\u00b7le", "dei\u00b7ne", "Ge\u00b7bre\u00b7chen", ".", "Psalm", "10\u00b73", ",", "3"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Krank und des Elends voll, ich komme doch; ach sei", "tokens": ["Krank", "und", "des", "E\u00b7lends", "voll", ",", "ich", "kom\u00b7me", "doch", ";", "ach", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "ART", "NN", "ADJD", "$,", "PPER", "VVFIN", "ADV", "$.", "XY", "XY"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mein Heil und meine Arzenei,", "tokens": ["Mein", "Heil", "und", "mei\u00b7ne", "Ar\u00b7ze\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Feind aus meinem Herzen treibe,", "tokens": ["Den", "Feind", "aus", "mei\u00b7nem", "Her\u00b7zen", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Besitze du dies Herz und ewig drinnen bleibe!", "tokens": ["Be\u00b7sit\u00b7ze", "du", "dies", "Herz", "und", "e\u00b7wig", "drin\u00b7nen", "blei\u00b7be", "!"], "toke