{"dta.poem.138": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die zornige  Almire.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df denn ein heisser Zorn eur Antlitz \u00fcberziehen?", "tokens": ["Mu\u00df", "denn", "ein", "heis\u00b7ser", "Zorn", "eur", "Ant\u00b7litz", "\u00fc\u00b7ber\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man der Sonnen-Licht nach ", "tokens": ["Wenn", "man", "der", "Son\u00b7nen\u00b7Licht", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "weist", "tokens": ["weist"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sie mu\u00df/ ", "tokens": ["Sie", "mu\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Eur schwartzes Augen-Paar zweyfache Strahlen scheust.", "tokens": ["Eur", "schwart\u00b7zes", "Au\u00b7gen\u00b7Paar", "zwey\u00b7fa\u00b7che", "Strah\u00b7len", "scheust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vor diesen Augen mu\u00df ihr g\u00fcldnes Licht erbleichen/", "tokens": ["Vor", "die\u00b7sen", "Au\u00b7gen", "mu\u00df", "ihr", "g\u00fcld\u00b7nes", "Licht", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwey Sonnen k\u00f6nnen mehr/ als eine/ kr\u00e4fftig seyn/", "tokens": ["Zwey", "Son\u00b7nen", "k\u00f6n\u00b7nen", "mehr", "/", "als", "ei\u00b7ne", "/", "kr\u00e4ff\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "ADV", "$(", "KOUS", "ART", "$(", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie will gantz gern vor euch die hohen Seegel streichen/", "tokens": ["Sie", "will", "gantz", "gern", "vor", "euch", "die", "ho\u00b7hen", "See\u00b7gel", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie ziehet ihren Glantz bey euren Flammen ein.", "tokens": ["Sie", "zie\u00b7het", "ih\u00b7ren", "Glantz", "bey", "eu\u00b7ren", "Flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dort hie\u00df ein ", "tokens": ["Dort", "hie\u00df", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.11": {"text": "Der Himmel sah es an/ und z\u00fcrnte nicht darob/", "tokens": ["Der", "Him\u00b7mel", "sah", "es", "an", "/", "und", "z\u00fcrn\u00b7te", "nicht", "da\u00b7rob", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$(", "KON", "VVFIN", "PTKNEG", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich hei\u00df sie nur hinweg nach ", "tokens": ["Ich", "hei\u00df", "sie", "nur", "hin\u00b7weg", "nach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Doch z\u00fcrnt ihr \u00fcber mich; ihr eyfert auf eur Lob.", "tokens": ["Doch", "z\u00fcrnt", "ihr", "\u00fc\u00b7ber", "mich", ";", "ihr", "ey\u00b7fert", "auf", "eur", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein/ es bleibet doch der Prei\u00df den sch\u00f6nen Augen/", "tokens": ["Al\u00b7lein", "/", "es", "blei\u00b7bet", "doch", "der", "Prei\u00df", "den", "sch\u00f6\u00b7nen", "Au\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Sonne heisset das/ was ich gesaget/ recht/", "tokens": ["Die", "Son\u00b7ne", "heis\u00b7set", "das", "/", "was", "ich", "ge\u00b7sa\u00b7get", "/", "recht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "$(", "PWS", "PPER", "VVPP", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie spricht: Mein Blitzen kan bey ihrem Strahl nicht taugen/", "tokens": ["Sie", "spricht", ":", "Mein", "Blit\u00b7zen", "kan", "bey", "ih\u00b7rem", "Strahl", "nicht", "tau\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bey zweenen Sonnen scheint mein Glantz nur allzuschlecht.", "tokens": ["Bey", "zwee\u00b7nen", "Son\u00b7nen", "scheint", "mein", "Glantz", "nur", "all\u00b7zu\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was n\u00fctzet nun eur Zorn annehmliche ", "tokens": ["Was", "n\u00fct\u00b7zet", "nun", "eur", "Zorn", "an\u00b7nehm\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NN", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Was hab ich denn geredt/ das straffens-w\u00fcrdig ist?", "tokens": ["Was", "hab", "ich", "denn", "ge\u00b7redt", "/", "das", "straf\u00b7fen\u00b7sw\u00fcr\u00b7dig", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$(", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der edle Tugend-Trieb/ den ich in euch versp\u00fchre", "tokens": ["Der", "ed\u00b7le", "Tu\u00b7gen\u00b7dTrieb", "/", "den", "ich", "in", "euch", "ver\u00b7sp\u00fch\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Macht euch ohn meine Schuld und ohne Fug entr\u00fcst.", "tokens": ["Macht", "euch", "ohn", "mei\u00b7ne", "Schuld", "und", "oh\u00b7ne", "Fug", "ent\u00b7r\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "Er zieret euren Geist/ er machet euch vollkommen/", "tokens": ["Er", "zie\u00b7ret", "eu\u00b7ren", "Geist", "/", "er", "ma\u00b7chet", "euch", "voll\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er wil des Himmels-Pracht gar nicht gemindert sehn/", "tokens": ["Er", "wil", "des", "Him\u00b7mels\u00b7Pracht", "gar", "nicht", "ge\u00b7min\u00b7dert", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er spricht: Der Sonnen wird ihr Schein so nicht benommen", "tokens": ["Er", "spricht", ":", "Der", "Son\u00b7nen", "wird", "ihr", "Schein", "so", "nicht", "be\u00b7nom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das Auge mu\u00df vor ihr/ nicht sie/ zu Gnaden gehn.", "tokens": ["Das", "Au\u00b7ge", "mu\u00df", "vor", "ihr", "/", "nicht", "sie", "/", "zu", "Gna\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "$(", "PTKNEG", "PPER", "$(", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Allein/ dis machet nicht der Augen - Pracht geringer/", "tokens": ["Al\u00b7lein", "/", "dis", "ma\u00b7chet", "nicht", "der", "Au\u00b7gen", "Pracht", "ge\u00b7rin\u00b7ger", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PDS", "VVFIN", "PTKNEG", "ART", "NN", "$(", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "Die Demuht beugt das Recht der holden Augen nicht:", "tokens": ["Die", "De\u00b7muht", "beugt", "das", "Recht", "der", "hol\u00b7den", "Au\u00b7gen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie bleiben voller Feur und Flammen-reiche Dinger/", "tokens": ["Sie", "blei\u00b7ben", "vol\u00b7ler", "Feur", "und", "Flam\u00b7men\u00b7rei\u00b7che", "Din\u00b7ger", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie sehr hier auch der Trieb der Tugend widerspricht.", "tokens": ["Wie", "sehr", "hier", "auch", "der", "Trieb", "der", "Tu\u00b7gend", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Kan man bey Sonnen-Schein der Kertzen Brand nicht sehen/", "tokens": ["Kan", "man", "bey", "Son\u00b7nen\u00b7Schein", "der", "Kert\u00b7zen", "Brand", "nicht", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "ART", "NN", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Macht dieses Licht der Welt die schlechten Flammen blind?", "tokens": ["Macht", "die\u00b7ses", "Licht", "der", "Welt", "die", "schlech\u00b7ten", "Flam\u00b7men", "blind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So kan die Sonn\u2019 auch nicht den Augen widerstehen/", "tokens": ["So", "kan", "die", "Sonn'", "auch", "nicht", "den", "Au\u00b7gen", "wi\u00b7der\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der sch\u00f6ne doppel Glantz den gr\u00f6\u00dften Beyfall findt.", "tokens": ["Der", "sch\u00f6\u00b7ne", "dop\u00b7pel", "Glantz", "den", "gr\u00f6\u00df\u00b7ten", "Bey\u00b7fall", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Sonn gesteht es selbst/ da\u00df ich die Warheit rede/", "tokens": ["Die", "Sonn", "ge\u00b7steht", "es", "selbst", "/", "da\u00df", "ich", "die", "War\u00b7heit", "re\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie m\u00fcht sich euren Pracht noch ferner zu erh\u00f6hn/", "tokens": ["Sie", "m\u00fcht", "sich", "eu\u00b7ren", "Pracht", "noch", "fer\u00b7ner", "zu", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Eur Z\u00fcrnen schreckt mich nicht! stellt euch nicht allzubl\u00f6de/", "tokens": ["Eur", "Z\u00fcr\u00b7nen", "schreckt", "mich", "nicht", "!", "stellt", "euch", "nicht", "all\u00b7zu\u00b7bl\u00f6\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "PTKNEG", "ADJD", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Eur Weigern machet euch noch tausend mahl so sch\u00f6n.", "tokens": ["Eur", "Wei\u00b7gern", "ma\u00b7chet", "euch", "noch", "tau\u00b7send", "mahl", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "CARD", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bescheidenheit und Zucht die holde Sch\u00f6nheit k\u00fcsset/", "tokens": ["Be\u00b7schei\u00b7den\u00b7heit", "und", "Zucht", "die", "hol\u00b7de", "Sch\u00f6n\u00b7heit", "k\u00fcs\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein Hochmuht wird an euch/ ", "tokens": ["Kein", "Hoch\u00b7muht", "wird", "an", "euch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.40": {"text": "Und dieses macht/ da\u00df ihr den Lob-Spruch gerne misset/", "tokens": ["Und", "die\u00b7ses", "macht", "/", "da\u00df", "ihr", "den", "Lob\u00b7Spruch", "ger\u00b7ne", "mis\u00b7set", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der euch vor aller Welt mit allen Recht geb\u00fchrt.", "tokens": ["Der", "euch", "vor", "al\u00b7ler", "Welt", "mit", "al\u00b7len", "Recht", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Erweget nun bey euch/ vortreffliche ", "tokens": ["Er\u00b7we\u00b7get", "nun", "bey", "euch", "/", "vor\u00b7treff\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$(", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.43": {"text": "Ob eur Erz\u00fcrnen recht und zu besch\u00f6nen sey?", "tokens": ["Ob", "eur", "Er\u00b7z\u00fcr\u00b7nen", "recht", "und", "zu", "be\u00b7sch\u00f6\u00b7nen", "sey", "?"], "token_info": ["word", "word", "word", "wor