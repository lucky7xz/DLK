{"textgrid.poem.24092": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Zwischen Abend und Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da zieht der Flu\u00df und tr\u00e4gt das Abendgold,", "tokens": ["Da", "zieht", "der", "Flu\u00df", "und", "tr\u00e4gt", "das", "A\u00b7bend\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da stehn die Wolken wie ein Goldgebirg:", "tokens": ["Da", "stehn", "die", "Wol\u00b7ken", "wie", "ein", "Gold\u00b7ge\u00b7birg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Abend giebt uns seine goldne Hand.", "tokens": ["Der", "A\u00b7bend", "giebt", "uns", "sei\u00b7ne", "gold\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl, wohl! Der Schlaf und was sch\u00f6n tr\u00e4umen macht", "tokens": ["Wohl", ",", "wohl", "!", "Der", "Schlaf", "und", "was", "sch\u00f6n", "tr\u00e4u\u00b7men", "macht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "ART", "NN", "KON", "PWS", "ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zieht leis die Fl\u00f6re \u00fcber unsre Welt,", "tokens": ["Zieht", "leis", "die", "Fl\u00f6\u00b7re", "\u00fc\u00b7ber", "uns\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Viele sind jetzt gl\u00fccklich, denn sie ruhn.", "tokens": ["Und", "Vie\u00b7le", "sind", "jetzt", "gl\u00fcck\u00b7lich", ",", "denn", "sie", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "ADJD", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An manchem Bette sitzt die Liebe jetzt", "tokens": ["An", "man\u00b7chem", "Bet\u00b7te", "sitzt", "die", "Lie\u00b7be", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Wiegenliedern, und manch m\u00fcdes Haupt", "tokens": ["Mit", "Wie\u00b7gen\u00b7lie\u00b7dern", ",", "und", "manch", "m\u00fc\u00b7des", "Haupt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hat seinen Schoo\u00df nun, ", "tokens": ["Hat", "sei\u00b7nen", "Schoo\u00df", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da liegt wohl eine weiche, wei\u00dfe Hand", "tokens": ["Da", "liegt", "wohl", "ei\u00b7ne", "wei\u00b7che", ",", "wei\u00b7\u00dfe", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Recht leicht und z\u00e4rtlich, h\u00fctend, auf der Stirn", "tokens": ["Recht", "leicht", "und", "z\u00e4rt\u00b7lich", ",", "h\u00fc\u00b7tend", ",", "auf", "der", "Stirn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,", "ADJD", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Des Schlafenden, und seiden streicht das Haar", "tokens": ["Des", "Schla\u00b7fen\u00b7den", ",", "und", "sei\u00b7den", "streicht", "das", "Haar"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVINF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der guten Hegerin die Wangen ihm,", "tokens": ["Der", "gu\u00b7ten", "He\u00b7ge\u00b7rin", "die", "Wan\u00b7gen", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der seine Ruhe in der Liebe hat.", "tokens": ["Der", "sei\u00b7ne", "Ru\u00b7he", "in", "der", "Lie\u00b7be", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie's in den Nestern still wird! Ab und an", "tokens": ["Wie's", "in", "den", "Nes\u00b7tern", "still", "wird", "!", "Ab", "und", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "VAFIN", "$.", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zirpt so ein Meislein, das wohl tr\u00e4umen mag.", "tokens": ["Zirpt", "so", "ein", "Meis\u00b7lein", ",", "das", "wohl", "tr\u00e4u\u00b7men", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Zeit der Nachtigallen ist vorbei.", "tokens": ["Die", "Zeit", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "ist", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Einst hab ich halbe N\u00e4chte lang gelauscht,", "tokens": ["Einst", "hab", "ich", "hal\u00b7be", "N\u00e4ch\u00b7te", "lang", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie unsrer W\u00e4lder vollste Kehle sang,", "tokens": ["Wie", "uns\u00b7rer", "W\u00e4l\u00b7der", "volls\u00b7te", "Keh\u00b7le", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mich ergriff der kleinen Kreatur", "tokens": ["Und", "mich", "er\u00b7griff", "der", "klei\u00b7nen", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufschluchzend Lied im Allerinnersten.", "tokens": ["Auf\u00b7schluch\u00b7zend", "Lied", "im", "Al\u00b7le\u00b7rin\u00b7ners\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich wu\u00dfte wohl: auch mir ist seelenvoll", "tokens": ["Ich", "wu\u00df\u00b7te", "wohl", ":", "auch", "mir", "ist", "see\u00b7len\u00b7voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ADV", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So hei\u00dfe Liebe schluchzend zugewandt,", "tokens": ["So", "hei\u00b7\u00dfe", "Lie\u00b7be", "schluch\u00b7zend", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dankbar trat ich leise an ein Bett", "tokens": ["Und", "dank\u00b7bar", "trat", "ich", "lei\u00b7se", "an", "ein", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und suchte eine kleine warme Hand", "tokens": ["Und", "such\u00b7te", "ei\u00b7ne", "klei\u00b7ne", "war\u00b7me", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und k\u00fc\u00dfte sie. \u2013 Da hatte ich mein Gl\u00fcck.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "sie", ".", "\u2013", "Da", "hat\u00b7te", "ich", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "$(", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das ist vorbei. Es kam ein b\u00f6ser Wind", "tokens": ["Das", "ist", "vor\u00b7bei", ".", "Es", "kam", "ein", "b\u00f6\u00b7ser", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ri\u00df mir meine Rose weg vom Stock", "tokens": ["Und", "ri\u00df", "mir", "mei\u00b7ne", "Ro\u00b7se", "weg", "vom", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und trug sie in die Stadt. Da liegt sie nun", "tokens": ["Und", "trug", "sie", "in", "die", "Stadt", ".", "Da", "liegt", "sie", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entbl\u00e4ttert in der Gosse. S\u00e4h ich sie,", "tokens": ["Ent\u00b7bl\u00e4t\u00b7tert", "in", "der", "Gos\u00b7se", ".", "S\u00e4h", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich k\u00f6nnte sie nicht mehr erkennen. \u2013 Oh,", "tokens": ["Ich", "k\u00f6nn\u00b7te", "sie", "nicht", "mehr", "er\u00b7ken\u00b7nen", ".", "\u2013", "Oh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$(", "ITJ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So herben Schmerz hast du mir angethan,", "tokens": ["So", "her\u00b7ben", "Schmerz", "hast", "du", "mir", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Einstmals Geliebte, da\u00df mein Herz steinhart", "tokens": ["Einst\u00b7mals", "Ge\u00b7lieb\u00b7te", ",", "da\u00df", "mein", "Herz", "stein\u00b7hart"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Von Narben wurde. Eher h\u00f6be ich", "tokens": ["Von", "Nar\u00b7ben", "wur\u00b7de", ".", "E\u00b7her", "h\u00f6\u00b7be", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "$.", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Kot der Gasse, als die Rose auf.", "tokens": ["Den", "Kot", "der", "Gas\u00b7se", ",", "als", "die", "Ro\u00b7se", "auf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Flu\u00df verlor sein Gold. Er geht in Grau.", "tokens": ["Der", "Flu\u00df", "ver\u00b7lor", "sein", "Gold", ".", "Er", "geht", "in", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Grau stehn die Wolken wie ein Bleigebirg.", "tokens": ["Grau", "stehn", "die", "Wol\u00b7ken", "wie", "ein", "Blei\u00b7ge\u00b7birg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"tex