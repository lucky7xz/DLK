{"dta.poem.13834": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und die Stadt vom Felsen droben", "tokens": ["Und", "die", "Stadt", "vom", "Fel\u00b7sen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiegelt sich im Sonnenschein,", "tokens": ["Spie\u00b7gelt", "sich", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Jungfr\u00e4ulein erhoben \u2014", "tokens": ["Wie", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder will ihr Br\u00e4ut'gam sein.", "tokens": ["Je\u00b7der", "will", "ihr", "Br\u00e4ut'\u00b7gam", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}