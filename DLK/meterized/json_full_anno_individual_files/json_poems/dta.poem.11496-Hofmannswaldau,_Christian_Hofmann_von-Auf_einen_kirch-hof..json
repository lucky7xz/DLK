{"dta.poem.11496": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen kirch-hof.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die hier begraben sind, die haben auch gelebet,", "tokens": ["Die", "hier", "be\u00b7gra\u00b7ben", "sind", ",", "die", "ha\u00b7ben", "auch", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$,", "PRELS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der du dieses liest, must eben diese bahn.", "tokens": ["Und", "der", "du", "die\u00b7ses", "liest", ",", "must", "e\u00b7ben", "die\u00b7se", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PDS", "VVFIN", "$,", "VMFIN", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die stund ist nicht mehr weit, in der man dich begr\u00e4bet;", "tokens": ["Die", "stund", "ist", "nicht", "mehr", "weit", ",", "in", "der", "man", "dich", "be\u00b7gr\u00e4\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "APPR", "PRELS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indessen denckst du wohl am wenigsten daran.", "tokens": ["In\u00b7des\u00b7sen", "denckst", "du", "wohl", "am", "we\u00b7nigs\u00b7ten", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "PIS", "PAV", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}}}}