{"dta.poem.9659": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf zwey ungleiche br\u00fcder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein sohn zu Genua ist dick und ungeschickt;", "tokens": ["Dein", "sohn", "zu", "Ge\u00b7nua", "ist", "dick", "und", "un\u00b7ge\u00b7schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der zu Siena zart/ so mann und weib erquickt.", "tokens": ["Der", "zu", "Sie\u00b7na", "zart", "/", "so", "mann", "und", "weib", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "ADJD", "$(", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nim diesem hembd und rock/ gieb jenem gla\u00df und wein/", "tokens": ["Nim", "die\u00b7sem", "hembd", "und", "rock", "/", "gieb", "je\u00b7nem", "gla\u00df", "und", "wein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "KON", "NE", "$(", "VVIMP", "PDAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan Cupido hier und dort ein Bachus seyn.", "tokens": ["So", "kan", "Cu\u00b7pi\u00b7do", "hier", "und", "dort", "ein", "Ba\u00b7chus", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ADV", "KON", "ADV", "ART", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}