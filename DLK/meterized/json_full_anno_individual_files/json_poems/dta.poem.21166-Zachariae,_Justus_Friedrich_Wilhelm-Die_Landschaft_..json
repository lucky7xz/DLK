{"dta.poem.21166": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Landschaft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Feld, dein aufgekl\u00e4rter Himmel,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Feld", ",", "dein", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sanft und rein um stille Fluren flie\u00dft,", "tokens": ["Der", "sanft", "und", "rein", "um", "stil\u00b7le", "Flu\u00b7ren", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Empfange mich vom L\u00e4rm und vom Get\u00fcmmel", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", "vom", "L\u00e4rm", "und", "vom", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der weiten Stadt, wo Unmuth mich umschlie\u00dft.", "tokens": ["Der", "wei\u00b7ten", "Stadt", ",", "wo", "Un\u00b7muth", "mich", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie fr\u00f6hlich steigt aus silberfarbnen Wellen", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "steigt", "aus", "sil\u00b7ber\u00b7farb\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Morgenroth zum feuchten Horizont!", "tokens": ["Das", "Mor\u00b7gen\u00b7roth", "zum", "feuch\u00b7ten", "Ho\u00b7ri\u00b7zont", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der graue Wald, den Lust und Tag erhellen,", "tokens": ["Der", "grau\u00b7e", "Wald", ",", "den", "Lust", "und", "Tag", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeigt in der H\u00f6h die Wipfel schon umsonnt.", "tokens": ["Zeigt", "in", "der", "H\u00f6h", "die", "Wip\u00b7fel", "schon", "um\u00b7sonnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Lerche fliegt in musikalschen Schaaren", "tokens": ["Die", "Ler\u00b7che", "fliegt", "in", "mu\u00b7si\u00b7kal\u00b7schen", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit s\u00fc\u00dfer Stimm auf sichren Haiden fort;", "tokens": ["Mit", "s\u00fc\u00b7\u00dfer", "Stimm", "auf", "sich\u00b7ren", "Hai\u00b7den", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fcrchtet nicht des falschen Garns Gefahren,", "tokens": ["Und", "f\u00fcrch\u00b7tet", "nicht", "des", "fal\u00b7schen", "Garns", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fcrchtet nicht des Feuerrohres Mord.", "tokens": ["Und", "f\u00fcrch\u00b7tet", "nicht", "des", "Feu\u00b7er\u00b7roh\u00b7res", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Voll Anmuth lockt das bl\u00fchende Gestade,", "tokens": ["Voll", "An\u00b7muth", "lockt", "das", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ocker flie\u00dft mit sanftern Wellen fort;", "tokens": ["Die", "O\u00b7cker", "flie\u00dft", "mit", "sanf\u00b7tern", "Wel\u00b7len", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Ufer tanzt die lachende Najade,", "tokens": ["Am", "U\u00b7fer", "tanzt", "die", "la\u00b7chen\u00b7de", "Na\u00b7ja\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tanz und West ihr fliegend Haar verwirrt.", "tokens": ["Der", "Tanz", "und", "West", "ihr", "flie\u00b7gend", "Haar", "ver\u00b7wirrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der wilde Busch, von Bl\u00fcten \u00fcberschneyet,", "tokens": ["Der", "wil\u00b7de", "Busch", ",", "von", "Bl\u00fc\u00b7ten", "\u00fc\u00b7bersc\u00b7hne\u00b7yet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Besieht sich stolz in spiegelklarer Fluth;", "tokens": ["Be\u00b7sieht", "sich", "stolz", "in", "spie\u00b7gel\u00b7kla\u00b7rer", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie flie\u00dft dahin, von keinem Sturm entweihet,", "tokens": ["Sie", "flie\u00dft", "da\u00b7hin", ",", "von", "kei\u00b7nem", "Sturm", "ent\u00b7wei\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So rein und still, wie Silber in der Gluth.", "tokens": ["So", "rein", "und", "still", ",", "wie", "Sil\u00b7ber", "in", "der", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es h\u00e4ngt indes an Klippen voller Weide", "tokens": ["Es", "h\u00e4ngt", "in\u00b7des", "an", "Klip\u00b7pen", "vol\u00b7ler", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der b\u00e4rtge Bock, der die Gestr\u00e4uche nagt;", "tokens": ["Der", "b\u00e4rt\u00b7ge", "Bock", ",", "der", "die", "Ge\u00b7str\u00e4u\u00b7che", "nagt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da unbesorgt der Hirte Lieb und Freude", "tokens": ["Da", "un\u00b7be\u00b7sorgt", "der", "Hir\u00b7te", "Lieb", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf heiserm Rohr den \u00f6den Felsen sagt.", "tokens": ["Auf", "hei\u00b7serm", "Rohr", "den", "\u00f6\u00b7den", "Fel\u00b7sen", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O Einsamkeit, d\u00fcrft ich mich dir ergeben!", "tokens": ["O", "Ein\u00b7sam\u00b7keit", ",", "d\u00fcrft", "ich", "mich", "dir", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PPER", "PPER", "VVPP", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier herrschest du im ungest\u00f6rten Hain.", "tokens": ["Hier", "herr\u00b7schest", "du", "im", "un\u00b7ge\u00b7st\u00f6r\u00b7ten", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum mu\u00df ich im L\u00e4rm der St\u00e4dte leben?", "tokens": ["Wa\u00b7rum", "mu\u00df", "ich", "im", "L\u00e4rm", "der", "St\u00e4d\u00b7te", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier k\u00f6nnt ich froh, wie dieser Hirte, seyn!", "tokens": ["Hier", "k\u00f6nnt", "ich", "froh", ",", "wie", "die\u00b7ser", "Hir\u00b7te", ",", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PDAT", "NN", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}