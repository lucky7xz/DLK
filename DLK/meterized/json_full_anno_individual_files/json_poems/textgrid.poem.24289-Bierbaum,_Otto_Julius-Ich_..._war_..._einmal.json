{"textgrid.poem.24289": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ich ... war ... einmal", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft wei\u00df ich ganz genau: Ich ... war ... einmal;", "tokens": ["Oft", "wei\u00df", "ich", "ganz", "ge\u00b7nau", ":", "Ich", "...", "war", "...", "ein\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "PPER", "$(", "VAFIN", "$(", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe schon einmal all dies gesehn;", "tokens": ["Ich", "ha\u00b7be", "schon", "ein\u00b7mal", "all", "dies", "ge\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "PDS", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Baum vor meinem Fenster rauschte mir", "tokens": ["Der", "Baum", "vor", "mei\u00b7nem", "Fens\u00b7ter", "rauschte", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz so wie jetzt vor tausend Jahren schon;", "tokens": ["Ganz", "so", "wie", "jetzt", "vor", "tau\u00b7send", "Jah\u00b7ren", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "All dieser Schmerz, all diese Lust ist nur", "tokens": ["All", "die\u00b7ser", "Schmerz", ",", "all", "die\u00b7se", "Lust", "ist", "nur"], "token_