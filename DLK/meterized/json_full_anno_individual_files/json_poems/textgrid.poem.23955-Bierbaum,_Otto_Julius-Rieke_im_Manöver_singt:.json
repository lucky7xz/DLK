{"textgrid.poem.23955": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Rieke im Man\u00f6ver singt:", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ulanen, das ist leichte Waar,", "tokens": ["U\u00b7la\u00b7nen", ",", "das", "ist", "leich\u00b7te", "Waar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut hier und morgen dort,", "tokens": ["Heut", "hier", "und", "mor\u00b7gen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum haben sie sich satt gek\u00fc\u00dft,", "tokens": ["Kaum", "ha\u00b7ben", "sie", "sich", "satt", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da reiten sie schon fort.", "tokens": ["Da", "rei\u00b7ten", "sie", "schon", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ade mein Schatz, trara, ade,", "tokens": ["A\u00b7de", "mein", "Schatz", ",", "tra\u00b7ra", ",", "a\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "$,"], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Und wenn ich dich nicht wiederseh,", "tokens": ["Und", "wenn", "ich", "dich", "nicht", "wie\u00b7der\u00b7seh", ","], "token_i