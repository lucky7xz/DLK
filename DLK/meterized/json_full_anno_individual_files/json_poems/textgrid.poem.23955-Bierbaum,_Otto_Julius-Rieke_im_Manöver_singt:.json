gar,", "tokens": ["Dra\u00b7go\u00b7ner", "und", "Hu\u00b7sa\u00b7ren", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sind gleich wie der Wind,", "tokens": ["Die", "sind", "gleich", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schnell reiten haben sie gelernt", "tokens": ["Schnell", "rei\u00b7ten", "ha\u00b7ben", "sie", "ge\u00b7lernt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcssen auch geschwind.", "tokens": ["Und", "k\u00fcs\u00b7sen", "auch", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": ":/: Ade mein Schatz ... :/:", "tokens": [":/", ":", "A\u00b7de", "mein", "Schatz", "...", ":/", ":"], "token_info": ["emoticon", "punct", "word", "word", "word", "punct", "emoticon", "punct"], "pos": ["$(", "$.", "NN", "PPOSAT", "NN", "$(", "$(", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Die Infantristen machens grad", "tokens": ["Die", "In\u00b7fant\u00b7ris\u00b7ten", "ma\u00b7chens", "grad"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wie die Reiterei;", "tokens": ["So", "wie", "die", "Rei\u00b7te\u00b7rei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vorm Zapfenstreich noch schnell ein Ku\u0