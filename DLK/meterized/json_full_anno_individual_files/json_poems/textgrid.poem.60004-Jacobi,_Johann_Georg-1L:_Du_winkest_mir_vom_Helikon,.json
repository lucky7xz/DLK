etra"}, "line.17": {"text": "Wo ihren Liebling Pavillon,", "tokens": ["Wo", "ih\u00b7ren", "Lieb\u00b7ling", "Pa\u00b7vil\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Scherze Hand in Hand umringen,", "tokens": ["Die", "Scher\u00b7ze", "Hand", "in", "Hand", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und bey der Huldg\u00f6ttin Bouillon", "tokens": ["Und", "bey", "der", "Huld\u00b7g\u00f6t\u00b7tin", "Bou\u00b7il\u00b7lon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "La Fare noch und Chaulieu singen?", "tokens": ["La", "Fa\u00b7re", "noch", "und", "Chau\u00b7li\u00b7eu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "O heil'ger, schauervoller Hain,", "tokens": ["O", "heil'\u00b7ger", ",", "schau\u00b7er\u00b7vol\u00b7ler", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Verehrungswerthe, gro\u00dfe Namen!", "tokens": ["Ver\u00b7eh\u00b7rungs\u00b7wert\u00b7he", ",", "gro\u00b7\u00dfe", "Na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ich, Freund, ich soll ihr Sch\u00fcler seyn?", "tokens": ["Ich", ",", "Freund", ",", "ich", "soll", "ihr", "Sch\u00fc\u00b7ler", "seyn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Umsonst w\u00fcnsch' ich, sie nachzuahmen,", "tokens": ["Um\u00b7sonst", "w\u00fcn\u00b7sch'", "ich", ",", "sie", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Sie, die von Vorurtheilen frey,", "tokens": ["Sie", ",", "die", "von", "Vor\u00b7urt\u00b7hei\u00b7len", "frey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der einzigen Natur getreu,", "tokens": ["Der", "ein\u00b7zi\u00b7gen", "Na\u00b7tur", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Zu Lust und Liedern sich verbanden,", "tokens": ["Zu", "Lust", "und", "Lie\u00b7dern", "sich", "ver\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Im Epikur den Weisen fanden,", "tokens": ["Im", "E\u00b7pi\u00b7kur", "den", "Wei\u00b7sen", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und, geitzig auf die schnelle Zeit,", "tokens": ["Und", ",", "geit\u00b7zig", "auf", "die", "schnel\u00b7le", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Und lachend, ohne Bitterkeit,", "tokens": ["Und", "la\u00b7chend", ",", "oh\u00b7ne", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sich an dem Schwarm der Thoren r\u00e4chten;", "tokens": ["Sich", "an", "dem", "Schwarm", "der", "Tho\u00b7ren", "r\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die, durch Gesch\u00e4fte nie gest\u00f6rt,", "tokens": ["Die", ",", "durch", "Ge\u00b7sch\u00e4f\u00b7te", "nie", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "In ihrer frohen Mu\u00dfe blieben,", "tokens": ["In", "ih\u00b7rer", "fro\u00b7hen", "Mu\u00b7\u00dfe", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Mehr liebensw\u00fcrdig, als gelehrt,", "tokens": ["Mehr", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ",", "als", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "F\u00fcr M\u00e4dchen nur und Freunde schrieben;", "tokens": ["F\u00fcr", "M\u00e4d\u00b7chen", "nur", "und", "Freun\u00b7de", "schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und, wenn sie gleich nicht Wochen lang", "tokens": ["Und", ",", "wenn", "sie", "gleich", "nicht", "Wo\u00b7chen", "lang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Bey dem, was ihre Muse sang,", "tokens": ["Bey", "dem", ",", "was", "ih\u00b7re", "Mu\u00b7se", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Von k\u00fcnft'gem Ruhme voll, verweilten,", "tokens": ["Von", "k\u00fcnft'\u00b7gem", "Ruh\u00b7me", "voll", ",", "ver\u00b7weil\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und jedes Liedchen m\u00fchsam feilten,", "tokens": ["Und", "je\u00b7des", "Lied\u00b7chen", "m\u00fch\u00b7sam", "feil\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Doch in der Dichter erstem Rang,", "tokens": ["Doch", "in", "der", "Dich\u00b7ter", "ers\u00b7tem", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Bey schimmernder Pokale Klang,", "tokens": ["Bey", "schim\u00b7mern\u00b7der", "Po\u00b7ka\u00b7le", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Ewigkeit entgegen eilten.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ent\u00b7ge\u00b7gen", "eil\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Freund, ihrer Lieder Harmonie", "tokens": ["Freund", ",", "ih\u00b7rer", "Lie\u00b7der", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Soll immer meinen Geist entz\u00fccken,", "tokens": ["Soll", "im\u00b7mer", "mei\u00b7nen", "Geist", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In tr\u00fcben Tagen mich begl\u00fccken,", "tokens": ["In", "tr\u00fc\u00b7ben", "Ta\u00b7gen", "mich", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Weisheit lehren sollen sie.", "tokens": ["Mich", "Weis\u00b7heit", "leh\u00b7ren", "sol\u00b7len", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Gresset, statt der Lorberbl\u00e4tter,", "tokens": ["Wenn", "Gres\u00b7set", ",", "statt", "der", "Lor\u00b7ber\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Rosen seine Schl\u00e4fe ziert", "tokens": ["Mit", "Ro\u00b7sen", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "ziert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Wagen kleiner Liebesg\u00f6tter", "tokens": ["Im", "Wa\u00b7gen", "klei\u00b7ner", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Tugend uns entgegen f\u00fchrt", "tokens": ["Die", "Tu\u00b7gend", "uns", "ent\u00b7ge\u00b7gen", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und fern von weiten Marmorg\u00e4ngen,", "tokens": ["Und", "fern", "von", "wei\u00b7ten", "Mar\u00b7mor\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo Schmeichler sich mit Thoren dr\u00e4ngen,", "tokens": ["Wo", "Schmeich\u00b7ler", "sich", "mit", "Tho\u00b7ren", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den Ton der Hoheit", "tokens": ["Den", "Ton", "der", "Ho\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und schwere goldne Ketten ha\u00dft:", "tokens": ["Und", "schwe\u00b7re", "gold\u00b7ne", "Ket\u00b7ten", "ha\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann folg' ich unter seine Linden", "tokens": ["Dann", "fol\u00b7g'", "ich", "un\u00b7ter", "sei\u00b7ne", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dem S\u00e4nger, dort das Gl\u00fcck zu finden,", "tokens": ["Dem", "S\u00e4n\u00b7ger", ",", "dort", "das", "Gl\u00fcck", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Das auf dem sichern Rasen thront,", "tokens": ["Das", "auf", "dem", "si\u00b7chern", "Ra\u00b7sen", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Selbst herrscht, und keinem F\u00fcrsten frohnt.", "tokens": ["Selbst", "herrscht", ",", "und", "kei\u00b7nem", "F\u00fcrs\u00b7ten", "frohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Allein, o Freund! ihm nachzusingen,", "tokens": ["Al\u00b7lein", ",", "o", "Freund", "!", "ihm", "nach\u00b7zu\u00b7sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$.", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in das Heiligthum zu dringen,", "tokens": ["Tief", "in", "das", "Hei\u00b7lig\u00b7thum", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Priester mit geweihter Hand", "tokens": ["Wo", "Pries\u00b7ter", "mit", "ge\u00b7weih\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Grazien ihr Opfer bringen,", "tokens": ["Den", "Gra\u00b7zi\u00b7en", "ihr", "Op\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die\u00df hat kein Gott mir zuerkannt!", "tokens": ["Die\u00df", "hat", "kein", "Gott", "mir", "zu\u00b7er\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will, von dir allein genannt,", "tokens": ["Ich", "will", ",", "von", "dir", "al\u00b7lein", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Thal des Helikons mich freuen,", "tokens": ["Im", "Thal", "des", "He\u00b7li\u00b7kons", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und da geheimen Weihrauch streuen,", "tokens": ["Und", "da", "ge\u00b7hei\u00b7men", "Weih\u00b7rauch", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und da der Freundschaft Gl\u00fcck erh\u00f6hn.", "tokens": ["Und", "da", "der", "Freund\u00b7schaft", "Gl\u00fcck", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O! sch\u00f6ner ist kein Gl\u00fcck auf Erden,", "tokens": ["O", "!", "sch\u00f6\u00b7ner", "ist", "kein", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "VAFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Als das, von Gleim geliebt zu werden:", "tokens": ["Als", "das", ",", "von", "Gleim", "ge\u00b7liebt", "zu", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "APPR", "NE", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Nachruhm selbst ist nicht so sch\u00f6n!", "tokens": ["Der", "Nach\u00b7ruhm", "selbst", "ist", "nicht", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}