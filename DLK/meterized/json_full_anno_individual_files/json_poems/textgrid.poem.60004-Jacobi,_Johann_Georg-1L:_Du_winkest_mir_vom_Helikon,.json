{"textgrid.poem.60004": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du winkest mir vom Helikon,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du winkest mir vom Helikon,", "tokens": ["Du", "win\u00b7kest", "mir", "vom", "He\u00b7li\u00b7kon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von jenen schattenreichen H\u00f6hen,", "tokens": ["Von", "je\u00b7nen", "schat\u00b7ten\u00b7rei\u00b7chen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die deinen Freund Anakreon,", "tokens": ["Die", "dei\u00b7nen", "Freund", "A\u00b7nak\u00b7re\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dich im ew'gen Lorber sehn?", "tokens": ["Und", "dich", "im", "ew'\u00b7gen", "Lor\u00b7ber", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du willst, ich soll auf Wegen gehen,", "tokens": ["Du", "willst", ",", "ich", "soll", "auf", "We\u00b7gen", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch deutschen S\u00e4ngern nicht bekannt,", "tokens": ["Noch", "deut\u00b7schen", "S\u00e4n\u00b7gern", "nicht", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ungesucht Chapelle fand?", "tokens": ["Die", "un\u00b7ge\u00b7sucht", "Cha\u00b7pel\u00b7le", "fand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In seinen Hain soll ich mich wagen?", "tokens": ["In", "sei\u00b7nen", "Hain", "soll", "ich", "mich", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo Liebesg\u00f6tter schalkhaft ihn", "tokens": ["Wo", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "schalk\u00b7haft", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Umh\u00fcpfen, sich einander jagen,", "tokens": ["Um\u00b7h\u00fcp\u00b7fen", ",", "sich", "ein\u00b7an\u00b7der", "ja\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ihm mit possierlichem Bem\u00fchn", "tokens": ["Ihm", "mit", "pos\u00b7sier\u00b7li\u00b7chem", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADJA", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erfrischend Eis zum Weine tragen,", "tokens": ["Er\u00b7fri\u00b7schend", "Eis", "zum", "Wei\u00b7ne", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und selbst von seinem Weine gl\u00fchn;", "tokens": ["Und", "selbst", "von", "sei\u00b7nem", "Wei\u00b7ne", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wo Lauben, welche nie verbl\u00fchn,", "tokens": ["Wo", "Lau\u00b7ben", ",", "wel\u00b7che", "nie", "ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein ihm getreues Chor empfingen;", "tokens": ["Ein", "ihm", "ge\u00b7treu\u00b7es", "Chor", "emp\u00b7fin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Wo den vertrauten Bachaumont,", "tokens": ["Wo", "den", "ver\u00b7trau\u00b7ten", "Bach\u00b7au\u00b7mont", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wo ihren Liebling Pavillon,", "tokens": ["Wo", "ih\u00b7ren", "Lieb\u00b7ling", "Pa\u00b7vil\u00b7lon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Scherze Hand in Hand umringen,", "tokens": ["Die", "Scher\u00b7ze", "Hand", "in", "Hand", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und bey der Huldg\u00f6ttin Bouillon", "tokens": ["Und", "bey", "der", "Huld\u00b7g\u00f6t\u00b7tin", "Bou\u00b7il\u00b7lon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "La Fare noch und Chaulieu singen?", "tokens": ["La", "Fa\u00b7re", "noch", "und", "Chau\u00b7li\u00b7eu", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "KON", "NE", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "O heil'ger, schauervoller Hain,", "tokens": ["O", "heil'\u00b7ger", ",", "schau\u00b7er\u00b7vol\u00b7ler", "Hain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Verehrungswerthe, gro\u00dfe Namen!", "tokens": ["Ver\u00b7eh\u00b7rungs\u00b7wert\u00b7he", ",", "gro\u00b7\u00dfe", "Na\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ich, Freund, ich soll ihr Sch\u00fcler seyn?", "tokens": ["Ich", ",", "Freund", ",", "ich", "soll", "ihr", "Sch\u00fc\u00b7ler", "seyn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Umsonst w\u00fcnsch' ich, sie nachzuahmen,", "tokens": ["Um\u00b7sonst", "w\u00fcn\u00b7sch'", "ich", ",", "sie", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Sie, die von Vorurtheilen frey,", "tokens": ["Sie", ",", "die", "von", "Vor\u00b7urt\u00b7hei\u00b7len", "frey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der einzigen Natur getreu,", "tokens": ["Der", "ein\u00b7zi\u00b7gen", "Na\u00b7tur", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Zu Lust und Liedern sich verbanden,", "tokens": ["Zu", "Lust", "und", "Lie\u00b7dern", "sich", "ver\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Im Epikur den Weisen fanden,", "tokens": ["Im", "E\u00b7pi\u00b7kur", "den", "Wei\u00b7sen", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und, geitzig auf die schnelle Zeit,", "tokens": ["Und", ",", "geit\u00b7zig", "auf", "die", "schnel\u00b7le", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Und lachend, ohne Bitterkeit,", "tokens": ["Und", "la\u00b7chend", ",", "oh\u00b7ne", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sich an dem Schwarm der Thoren r\u00e4chten;", "tokens": ["Sich", "an", "dem", "Schwarm", "der", "Tho\u00b7ren", "r\u00e4ch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die, durch Gesch\u00e4fte nie gest\u00f6rt,", "tokens": ["Die", ",", "durch", "Ge\u00b7sch\u00e4f\u00b7te", "nie", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "In ihrer frohen Mu\u00dfe blieben,", "tokens": ["In", "ih\u00b7rer", "fro\u00b7hen", "Mu\u00b7\u00dfe", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Mehr liebensw\u00fcrdig, als gelehrt,", "tokens": ["Mehr", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", ",", "als", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "F\u00fcr M\u00e4dchen nur und Freunde schrieben;", "tokens": ["F\u00fcr", "M\u00e4d\u00b7chen", "nur", "und", "Freun\u00b7de", "schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und, wenn sie gleich nicht Wochen lang", "tokens": ["Und", ",", "wenn", "sie", "gleich", "nicht", "Wo\u00b7chen", "lang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Bey dem, was ihre Muse sang,", "tokens": ["Bey", "dem", ",", "was", "ih\u00b7re", "Mu\u00b7se", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Von k\u00fcnft'gem Ruhme voll, verweilten,", "tokens": ["Von", "k\u00fcnft'\u00b7gem", "Ruh\u00b7me", "voll", ",", "ver\u00b7weil\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und jedes Liedchen m\u00fchsam feilten,", "tokens": ["Und", "je\u00b7des", "Lied\u00b7chen", "m\u00fch\u00b7sam", "feil\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Doch in der Dichter erstem Rang,", "tokens": ["Doch", "in", "der", "Dich\u00b7ter", "ers\u00b7tem", "Rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Bey schimmernder Pokale Klang,", "tokens": ["Bey", "schim\u00b7mern\u00b7der", "Po\u00b7ka\u00b7le", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Ewigkeit entgegen eilten.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ent\u00b7ge\u00b7gen", "eil\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund, ihrer Lieder Harmonie", "tokens": ["Freund", ",", "ih\u00b7rer", "Lie\u00b7der", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Soll immer meinen Geist entz\u00fccken,", "tokens": ["Soll", "im\u00b7mer", "mei\u00b7nen", "Geist", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In tr\u00fcben Tagen mich begl\u00fccken,", "tokens": ["In", "tr\u00fc\u00b7ben", "Ta\u00b7gen", "mich", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Weisheit lehren sollen sie.", "tokens": ["Mich", "Weis\u00b7heit", "leh\u00b7ren", "sol\u00b7len", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Gresset, statt der Lorberbl\u00e4tter,", "tokens": ["Wenn", "Gres\u00b7set", ",", "statt", "der", "Lor\u00b7ber\u00b7bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUI", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Rosen seine Schl\u00e4fe ziert", "tokens": ["Mit", "Ro\u00b7sen", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "ziert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Wagen kleiner Liebesg\u00f6tter", "tokens": ["Im", "Wa\u00b7gen", "klei\u00b7ner", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Tugend uns entgegen f\u00fchrt", "tokens": ["Die", "Tu\u00b7gend", "uns", "ent\u00b7ge\u00b7gen", "f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und fern von weiten Marmorg\u00e4ngen,", "tokens": ["Und", "fern", "von", "wei\u00b7ten", "Mar\u00b7mor\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo Schmeichler sich mit Thoren dr\u00e4ngen,", "tokens": ["Wo", "Schmeich\u00b7ler", "sich", "mit", "Tho\u00b7ren", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den Ton der Hoheit", "tokens": ["Den", "Ton", "der", "Ho\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und schwere goldne Ketten ha\u00dft:", "tokens": ["Und", "schwe\u00b7re", "gold\u00b7ne", "Ket\u00b7ten", "ha\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann folg' ich unter seine Linden", "tokens": ["Dann", "fol\u00b7g'", "ich", "un\u00b7ter", "sei\u00b7ne", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dem S\u00e4nger, dort das Gl\u00fcck zu finden,", "tokens": ["Dem", "S\u00e4n\u00b7ger", 