{"dta.poem.896": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "DiE BAUERN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Vorm Fenster t\u00f6nendes Gr\u00fcn und Rot.", "tokens": ["Vorm", "Fens\u00b7ter", "t\u00f6\u00b7nen\u00b7des", "Gr\u00fcn", "und", "Rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im schwarzverr\u00e4ucherten, niederen Saal", "tokens": ["Im", "schwarz\u00b7ver\u00b7r\u00e4u\u00b7cher\u00b7ten", ",", "nie\u00b7de\u00b7ren", "Saal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sitzen die Knechte und M\u00e4gde beim Mahl;", "tokens": ["Sit\u00b7zen", "die", "Knech\u00b7te", "und", "M\u00e4g\u00b7de", "beim", "Mahl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und sie schenken den Wein und sie brechen das Brot.", "tokens": ["Und", "sie", "schen\u00b7ken", "den", "Wein", "und", "sie", "bre\u00b7chen", "das", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}