{"dta.poem.15878": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur \u00f6fter solltet ihr, statt euch so fremd zu bleiben,", "tokens": ["Nur", "\u00f6f\u00b7ter", "soll\u00b7tet", "ihr", ",", "statt", "euch", "so", "fremd", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "$,", "KOUI", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einander sehn, wonicht, doch an einander schreiben,", "tokens": ["Ein\u00b7an\u00b7der", "sehn", ",", "wo\u00b7nicht", ",", "doch", "an", "ein\u00b7an\u00b7der", "schrei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADJD", "$,", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Schreibenden! das w\u00fcrd' im Federkrieg euch hindern,", "tokens": ["Ihr", "Schrei\u00b7ben\u00b7den", "!", "das", "w\u00fcrd'", "im", "Fe\u00b7der\u00b7krieg", "euch", "hin\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PDS", "VAFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eurer feindlichen Bek\u00e4mpfung Sch\u00e4rfe lindern;", "tokens": ["Und", "eu\u00b7rer", "feind\u00b7li\u00b7chen", "Be\u00b7k\u00e4mp\u00b7fung", "Sch\u00e4r\u00b7fe", "lin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu Statten k\u00e4m' es euch, und euren Lesekindern.", "tokens": ["Zu", "Stat\u00b7ten", "k\u00e4m'", "es", "euch", ",", "und", "eu\u00b7ren", "Le\u00b7se\u00b7kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie mancher hat ins Aug den andern nur gestochen,", "tokens": ["Wie", "man\u00b7cher", "hat", "ins", "Aug", "den", "an\u00b7dern", "nur", "ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "APPRART", "NN", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er ihn nie gesehn, weil er ihn nie gesprochen!", "tokens": ["Weil", "er", "ihn", "nie", "ge\u00b7sehn", ",", "weil", "er", "ihn", "nie", "ge\u00b7spro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Lanzenbrechen h\u00e4tt' ein Wort, ein Blick gebrochen.", "tokens": ["Das", "Lan\u00b7zen\u00b7bre\u00b7chen", "h\u00e4tt'", "ein", "Wort", ",", "ein", "Blick", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Viel besser denk' ich selbst von manchem, den ich sah,", "tokens": ["Viel", "bes\u00b7ser", "denk'", "ich", "selbst", "von", "man\u00b7chem", ",", "den", "ich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnschte mehrern nur w\u00e4r' ich gekommen nah.", "tokens": ["Und", "w\u00fcnschte", "meh\u00b7rern", "nur", "w\u00e4r'", "ich", "ge\u00b7kom\u00b7men", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Das gilt euch Bessern, die ihr stehet so allein;", "tokens": ["Das", "gilt", "euch", "Bes\u00b7sern", ",", "die", "ihr", "ste\u00b7het", "so", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schlechtern sind schon lang' im innigsten Verein,", "tokens": ["Die", "Schlech\u00b7tern", "sind", "schon", "lang'", "im", "in\u00b7nigs\u00b7ten", "Ver\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Und haben nur dadurch sich \u00fcber euch gehoben,", "tokens": ["Und", "ha\u00b7ben", "nur", "da\u00b7durch", "sich", "\u00fc\u00b7ber", "euch", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PAV", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil ihr einander schm\u00e4ht, und sie einander loben.", "tokens": ["Weil", "ihr", "ein\u00b7an\u00b7der", "schm\u00e4ht", ",", "und", "sie", "ein\u00b7an\u00b7der", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "KON", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Loben lasset nur, das stellet ihnen frei,", "tokens": ["Das", "Lo\u00b7ben", "las\u00b7set", "nur", ",", "das", "stel\u00b7let", "ih\u00b7nen", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch steht nicht ihrem Bund durch eure Zwietracht bei!", "tokens": ["Doch", "steht", "nicht", "ih\u00b7rem", "Bund", "durch", "eu\u00b7re", "Zwiet\u00b7racht", "bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}