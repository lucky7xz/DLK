{"dta.poem.16082": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "136.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was, Dichter, suchst du? Ruhm? \u201eWen reizt die Seifenblase?\u201c", "tokens": ["Was", ",", "Dich\u00b7ter", ",", "suchst", "du", "?", "Ruhm", "?", "\u201e", "Wen", "reizt", "die", "Sei\u00b7fen\u00b7bla\u00b7se", "?", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "PPER", "$.", "NN", "$.", "$(", "PWS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Reichth\u00fcmer? \u201eH\u00e4tt' ich auch Lust am gef\u00e4rbten Glase?\u201c", "tokens": ["Reicht\u00b7h\u00fc\u00b7mer", "?", "\u201e", "H\u00e4tt'", "ich", "auch", "Lust", "am", "ge\u00b7f\u00e4rb\u00b7ten", "Gla\u00b7se", "?", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "$(", "VVFIN", "PPER", "ADV", "NN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Mitwirkung in der Zeit? \u201eIch bin nicht deren Sohn.\u201c", "tokens": ["Mit\u00b7wir\u00b7kung", "in", "der", "Zeit", "?", "\u201e", "Ich", "bin", "nicht", "de\u00b7ren", "Sohn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "PRELAT", "NN", "$.", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der Geister Bildung? \u201eSie sind \u00fcberbildet schon.\u201c", "tokens": ["Der", "Geis\u00b7ter", "Bil\u00b7dung", "?", "\u201e", "Sie", "sind", "\u00fc\u00b7ber\u00b7bil\u00b7det", "schon", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "PPER", "VAFIN", "VVPP", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was also suchest du? dir selber zu gen\u00fcgen?", "tokens": ["Was", "al\u00b7so", "su\u00b7chest", "du", "?", "dir", "sel\u00b7ber", "zu", "ge\u00b7n\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$.", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201emich mit dem Schein, als thu' ich etwas, zu betr\u00fcgen.\u201c", "tokens": ["\u201e", "mich", "mit", "dem", "Schein", ",", "als", "thu'", "ich", "et\u00b7was", ",", "zu", "be\u00b7tr\u00fc\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}