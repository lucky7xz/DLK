{"dta.poem.19063": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.  Auf seiner Tante, der Fr\u00e4ulein Hen-  \n riette zu Hennersdorf, Geburts-Fest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vater der Barmhertzigkeit,", "tokens": ["Der", "Va\u00b7ter", "der", "Barm\u00b7hert\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der gantz alleine gut,", "tokens": ["Der", "gantz", "al\u00b7lei\u00b7ne", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer neue Wunder thut,", "tokens": ["Und", "im\u00b7mer", "neu\u00b7e", "Wun\u00b7der", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sey hoch gebenedeyt!", "tokens": ["Sey", "hoch", "ge\u00b7be\u00b7ne\u00b7deyt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Sohn der Rechten Lob und Prei\u00df,", "tokens": ["Dem", "Sohn", "der", "Rech\u00b7ten", "Lob", "und", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der keinem Guts versagt,", "tokens": ["Der", "kei\u00b7nem", "Guts", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sich auch dieser seiner Magd", "tokens": ["Der", "sich", "auch", "die\u00b7ser", "sei\u00b7ner", "Magd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu offenbahren wei\u00df!", "tokens": ["Zu", "of\u00b7fen\u00b7bah\u00b7ren", "wei\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem Geist der Herrlichkeit des HErrn", "tokens": ["Dem", "Geist", "der", "Herr\u00b7lich\u00b7keit", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey danck und Ruhm gebracht!", "tokens": ["Sey", "danck", "und", "Ruhm", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er zeiget seine Kraft und Macht", "tokens": ["Er", "zei\u00b7get", "sei\u00b7ne", "Kraft", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Seelen allzu gern.", "tokens": ["Den", "See\u00b7len", "all\u00b7zu", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Jhr Diener unsers K\u00f6niges,", "tokens": ["Ihr", "Die\u00b7ner", "un\u00b7sers", "K\u00f6\u00b7ni\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Habt Danck vor alle Hut,", "tokens": ["Habt", "Danck", "vor", "al\u00b7le", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr bey seiner Freundin thut:", "tokens": ["Die", "ihr", "bey", "sei\u00b7ner", "Freun\u00b7din", "thut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ihr verdienet es.", "tokens": ["Denn", "ihr", "ver\u00b7die\u00b7net", "es", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie w\u00fcnsch ich diesen lieben Tag", "tokens": ["Wie", "w\u00fcnsch", "ich", "die\u00b7sen", "lie\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch oft mit zu begehn,", "tokens": ["Noch", "oft", "mit", "zu", "be\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und allerley mit anzusehn,", "tokens": ["Und", "al\u00b7ler\u00b7ley", "mit", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darum man dancken mag.", "tokens": ["Da\u00b7rum", "man", "dan\u00b7cken", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PAV", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Herr, dem kein Sohn der Menschen je", "tokens": ["Herr", ",", "dem", "kein", "Sohn", "der", "Men\u00b7schen", "je"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "ART", "NN", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Was Guts umsonst gethan,", "tokens": ["Was", "Guts", "um\u00b7sonst", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der reichlich wieder geben kan:", "tokens": ["Der", "reich\u00b7lich", "wie\u00b7der", "ge\u00b7ben", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Erh\u00f6re mich auch hie!", "tokens": ["Er\u00b7h\u00f6\u00b7re", "mich", "auch", "hie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich habe vieles, was ich wei\u00df,", "tokens": ["Ich", "ha\u00b7be", "vie\u00b7les", ",", "was", "ich", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dieser Seel erlernt;", "tokens": ["Von", "die\u00b7ser", "Seel", "er\u00b7lernt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat mich von der Welt entfernt,", "tokens": ["Sie", "hat", "mich", "von", "der", "Welt", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bracht aufs rechte Gleis.", "tokens": ["Und", "bracht", "aufs", "rech\u00b7te", "Gleis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Womit erstatt ich meine Pflicht?", "tokens": ["Wo\u00b7mit", "er\u00b7statt", "ich", "mei\u00b7ne", "Pflicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sag es jederman.", "tokens": ["Ich", "sag", "es", "je\u00b7der\u00b7man", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch damit ists nicht abgethan,", "tokens": ["Doch", "da\u00b7mit", "ists", "nicht", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nein! ich vergelt es nicht.", "tokens": ["Nein", "!", "ich", "ver\u00b7gelt", "es", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum Licht und Geist der Ewigkeit,", "tokens": ["Drum", "Licht", "und", "Geist", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ruf ich dich herbey,", "tokens": ["So", "ruf", "ich", "dich", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df deine Lieb ihr Segen sey,", "tokens": ["Da\u00df", "dei\u00b7ne", "Lieb", "ihr", "Se\u00b7gen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber sie sich breit!", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "sich", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da\u00df unsre k\u00e4mpfende Gewalt,", "tokens": ["Da\u00df", "uns\u00b7re", "k\u00e4mp\u00b7fen\u00b7de", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "In einer Nachbarschaft,", "tokens": ["In", "ei\u00b7ner", "Nach\u00b7bar\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch immer mit vereinter Kraft", "tokens": ["Auch", "im\u00b7mer", "mit", "ver\u00b7ein\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemeinen Sieg erhalt!", "tokens": ["Ge\u00b7mei\u00b7nen", "Sieg", "er\u00b7halt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da\u00df unsre Waffen immerfort", "tokens": ["Da\u00df", "uns\u00b7re", "Waf\u00b7fen", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verkn\u00fcpft beysammen stehn,", "tokens": ["Ver\u00b7kn\u00fcpft", "bey\u00b7sam\u00b7men", "stehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Feind getrost entgegen gehn,", "tokens": ["Dem", "Feind", "ge\u00b7trost", "ent\u00b7ge\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dringen bi\u00df zum Port.", "tokens": ["Und", "drin\u00b7gen", "bi\u00df", "zum", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "APPRART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So werde von des Vaters Hand,", "tokens": ["So", "wer\u00b7de", "von", "des", "Va\u00b7ters", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nebst ihrer eignen Cron,", "tokens": ["Nebst", "ih\u00b7rer", "eig\u00b7nen", "Cron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch etwas mit von meinem Lohn", "tokens": ["Auch", "et\u00b7was", "mit", "von", "mei\u00b7nem", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr gn\u00e4dig zugewandt!", "tokens": ["Ihr", "gn\u00e4\u00b7dig", "zu\u00b7ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}