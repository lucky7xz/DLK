{"dta.poem.10886": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jagd-Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In den beb\u00fcschten feuchten Gr\u00fcnden,", "tokens": ["In", "den", "be\u00b7b\u00fcschten", "feuch\u00b7ten", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Springt jetzt manch schneller Hirsch, sammt seinen Hinden,", "tokens": ["Springt", "jetzt", "manch", "schnel\u00b7ler", "Hirsch", ",", "sammt", "sei\u00b7nen", "Hin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit leichtem Fu\u00df, und aufgerecktem Ohr,", "tokens": ["Mit", "leich\u00b7tem", "Fu\u00df", ",", "und", "auf\u00b7ge\u00b7reck\u00b7tem", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus dem belaubten Wald, hervor.", "tokens": ["Aus", "dem", "be\u00b7laub\u00b7ten", "Wald", ",", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sieh da! dort rennt in dem bebl\u00fcmten Grase,", "tokens": ["Sieh", "da", "!", "dort", "rennt", "in", "dem", "be\u00b7bl\u00fcm\u00b7ten", "Gra\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein fl\u00fcchtiger, geschwinder Haase;", "tokens": ["Ein", "fl\u00fcch\u00b7ti\u00b7ger", ",", "ge\u00b7schwin\u00b7der", "Haa\u00b7se", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Seht! wie er pl\u00f6tzlich stutzt, sich setzt;", "tokens": ["Seht", "!", "wie", "er", "pl\u00f6tz\u00b7lich", "stutzt", ",", "sich", "setzt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein M\u00e4nnchen macht,", "tokens": ["Ein", "M\u00e4nn\u00b7chen", "macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und da es niemand sieht, an des Gefildes Pracht,", "tokens": ["Und", "da", "es", "nie\u00b7mand", "sieht", ",", "an", "des", "Ge\u00b7fil\u00b7des", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit tausend Spr\u00fcngen, sich ergetzet.", "tokens": ["Mit", "tau\u00b7send", "Spr\u00fcn\u00b7gen", ",", "sich", "er\u00b7get\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}