{"dta.poem.956": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ein erwachen war zu andrem werden               ", "tokens": ["Wie", "ein", "er\u00b7wa\u00b7chen", "war", "zu", "an\u00b7drem", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "VAFIN", "APPR", "PIS", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wir vergangenheit in uns geb&#228;ndigt               ", "tokens": ["Als", "wir", "ver\u00b7gan\u00b7gen\u00b7heit", "in", "uns", "geb", "&#228;", "ndigt"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und als das leben l&#228;chelnd uns geh&#228;ndigt               ", "tokens": ["Und", "als", "das", "le\u00b7ben", "l", "&#228;", "chelnd", "uns", "geh", "&#228;", "ndigt"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "KOUS", "PDS", "VVFIN", "NE", "$(", "VVPP", "PPER", "VVFIN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was lang uns einzig ziel erschien auf erden             ", "tokens": ["Was", "lang", "uns", "ein\u00b7zig", "ziel", "er\u00b7schien", "auf", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPER", "ADJD", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf einmal alle stunden so nur galten:               ", "tokens": ["Auf", "ein\u00b7mal", "al\u00b7le", "stun\u00b7den", "so", "nur", "gal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein m&#252;hevolles werben um die hohe               ", "tokens": ["Ein", "m", "&#252;", "he\u00b7vol\u00b7les", "wer\u00b7ben", "um", "die", "ho\u00b7he"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die uns vereinte die in ihrer lohe               ", "tokens": ["Die", "uns", "ver\u00b7ein\u00b7te", "die", "in", "ih\u00b7rer", "lo\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gestalten um uns tilgte und gewalten.             ", "tokens": ["Ge\u00b7stal\u00b7ten", "um", "uns", "tilg\u00b7te", "und", "ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}