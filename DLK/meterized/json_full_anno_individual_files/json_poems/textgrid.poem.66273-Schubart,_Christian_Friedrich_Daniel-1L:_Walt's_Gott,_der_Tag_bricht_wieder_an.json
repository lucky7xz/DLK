], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meinen Glauben h\u00f6hnt;", "tokens": ["Und", "mei\u00b7nen", "Glau\u00b7ben", "h\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Wenn mich es martert, da\u00df die Welt", "tokens": ["Wenn", "mich", "es", "mar\u00b7tert", ",", "da\u00df", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schimpflich mich verwarf,", "tokens": ["So", "schimpf\u00b7lich", "mich", "ver\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mir eine Thr\u00e4n' entf\u00e4llt,", "tokens": ["Und", "wenn", "mir", "ei\u00b7ne", "Thr\u00e4n'", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich nicht reden darf \u2013", "tokens": ["Weil", "ich", "nicht", "re\u00b7den", "darf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Nicht reden darf mit einem Freund,", "tokens": ["Nicht", "re\u00b7den", "darf", "mit", "ei\u00b7nem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht scherzen mit dem Kind,", "tokens": ["Nicht", "scher\u00b7zen", "mit", "dem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll schweigen, wie ein Menschenfeind,", "tokens": ["Soll", "schwei\u00b7gen", ",", "wie", "ein", "Men\u00b7schen\u00b7feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Br\u00fcder um mich sind.", "tokens": ["Wenn", "Br\u00fc\u00b7der", "um", "mich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "Wenn meine Zelle stumm und todt", "tokens": ["Wenn", "mei\u00b7ne", "Zel\u00b7le", "stumm", "und", "todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Brust und Geist verengt,", "tokens": ["Mir", "Brust", "und", "Geist", "ver\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn wie Blut das Morgenroth", "tokens": ["Und", "wenn", "wie", "Blut", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinen W\u00e4nden h\u00e4ngt;", "tokens": ["An", "mei\u00b7nen", "W\u00e4n\u00b7den", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, 