{"textgrid.poem.66273": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Walt's Gott, der Tag bricht wieder an", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Walt's Gott, der Tag bricht wieder an", "tokens": ["Walt's", "Gott", ",", "der", "Tag", "bricht", "wie\u00b7der", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weckt mich aus der Ruh';", "tokens": ["Und", "weckt", "mich", "aus", "der", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlauf, betritt die Dornenbahn!", "tokens": ["Wohl\u00b7auf", ",", "be\u00b7tritt", "die", "Dor\u00b7nen\u00b7bahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, meine Seele, du!", "tokens": ["Du", ",", "mei\u00b7ne", "See\u00b7le", ",", "du", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da neben meinem Bette steht", "tokens": ["Da", "ne\u00b7ben", "mei\u00b7nem", "Bet\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kreuz, ich nehm' es auf,", "tokens": ["Mein", "Kreuz", ",", "ich", "nehm'", "es", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schick' ein weinendes Gebet", "tokens": ["Und", "schick'", "ein", "wei\u00b7nen\u00b7des", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum lieben Gott hinauf.", "tokens": ["Zum", "lie\u00b7ben", "Gott", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er wird mir's tragen helfen, ach!", "tokens": ["Er", "wird", "mir's", "tra\u00b7gen", "hel\u00b7fen", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVINF", "VVINF", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df es, Gott ist gut;", "tokens": ["Ich", "wei\u00df", "es", ",", "Gott", "ist", "gut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unm\u00e4chtig bin ich, krank und schwach,", "tokens": ["Un\u00b7m\u00e4ch\u00b7tig", "bin", "ich", ",", "krank", "und", "schwach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er aber gibt mir Muth;", "tokens": ["Er", "a\u00b7ber", "gibt", "mir", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df mich die Hoffnung nicht verl\u00e4\u00dft,", "tokens": ["Da\u00df", "mich", "die", "Hoff\u00b7nung", "nicht", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geduld nicht von mir weicht,", "tokens": ["Ge\u00b7duld", "nicht", "von", "mir", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Langeweile, wie die Pest,", "tokens": ["Wenn", "Lan\u00b7ge\u00b7wei\u00b7le", ",", "wie", "die", "Pest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Finstern mich beschleicht.", "tokens": ["Im", "Fins\u00b7tern", "mich", "be\u00b7schleicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wenn Schwermuth meine Seele dr\u00fcckt,", "tokens": ["Wenn", "Schwer\u00b7muth", "mei\u00b7ne", "See\u00b7le", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn jede Nerve dr\u00f6hnt,", "tokens": ["Wenn", "je\u00b7de", "Ner\u00b7ve", "dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn Satan sp\u00f6ttisch auf mich blickt,", "tokens": ["Wenn", "Sa\u00b7tan", "sp\u00f6t\u00b7tisch", "auf", "mich", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meinen Glauben h\u00f6hnt;", "tokens": ["Und", "mei\u00b7nen", "Glau\u00b7ben", "h\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wenn mich es martert, da\u00df die Welt", "tokens": ["Wenn", "mich", "es", "mar\u00b7tert", ",", "da\u00df", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schimpflich mich verwarf,", "tokens": ["So", "schimpf\u00b7lich", "mich", "ver\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mir eine Thr\u00e4n' entf\u00e4llt,", "tokens": ["Und", "wenn", "mir", "ei\u00b7ne", "Thr\u00e4n'", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich nicht reden darf \u2013", "tokens": ["Weil", "ich", "nicht", "re\u00b7den", "darf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nicht reden darf mit einem Freund,", "tokens": ["Nicht", "re\u00b7den", "darf", "mit", "ei\u00b7nem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht scherzen mit dem Kind,", "tokens": ["Nicht", "scher\u00b7zen", "mit", "dem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Soll schweigen, wie ein Menschenfeind,", "tokens": ["Soll", "schwei\u00b7gen", ",", "wie", "ein", "Men\u00b7schen\u00b7feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Br\u00fcder um mich sind.", "tokens": ["Wenn", "Br\u00fc\u00b7der", "um", "mich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wenn meine Zelle stumm und todt", "tokens": ["Wenn", "mei\u00b7ne", "Zel\u00b7le", "stumm", "und", "todt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Brust und Geist verengt,", "tokens": ["Mir", "Brust", "und", "Geist", "ver\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn wie Blut das Morgenroth", "tokens": ["Und", "wenn", "wie", "Blut", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "KOKOM", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinen W\u00e4nden h\u00e4ngt;", "tokens": ["An", "mei\u00b7nen", "W\u00e4n\u00b7den", "h\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wenn f\u00fcrchterlich das Kerkerschlo\u00df", "tokens": ["Wenn", "f\u00fcrch\u00b7ter\u00b7lich", "das", "Ker\u00b7ker\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klirrt in mein Morgenlied,", "tokens": ["Klirrt", "in", "mein", "Mor\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn mein Aug' im Felsenscho\u00df", "tokens": ["Und", "wenn", "mein", "Aug'", "im", "Fel\u00b7sen\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Elend um sich sieht:", "tokens": ["Nur", "E\u00b7lend", "um", "sich", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So wei\u00df ich, Gott im Himmel gibt", "tokens": ["So", "wei\u00df", "ich", ",", "Gott", "im", "Him\u00b7mel", "gibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Armen wieder Muth,", "tokens": ["Mir", "Ar\u00b7men", "wie\u00b7der", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn er, der die Verlassnen liebt,", "tokens": ["Denn", "er", ",", "der", "die", "Ver\u00b7lass\u00b7nen", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mir Verlassnem gut.", "tokens": ["Ist", "mir", "Ver\u00b7lass\u00b7nem", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und so im Namen Jesu tret'", "tokens": ["Und", "so", "im", "Na\u00b7men", "Je\u00b7su", "tret'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich auf die Dornenbahn,", "tokens": ["Ich", "auf", "die", "Dor\u00b7nen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und glaub' und hoff', und les' und bet',", "tokens": ["Und", "glaub'", "und", "hoff'", ",", "und", "les'", "und", "bet'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVIMP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sing', so gut ich kann.", "tokens": ["Und", "sing'", ",", "so", "gut", "ich", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Bald kommt ein Tag, der mich befreit", "tokens": ["Bald", "kommt", "ein", "Tag", ",", "der", "mich", "be\u00b7freit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus meinem Angstgedr\u00e4ng,", "tokens": ["Aus", "mei\u00b7nem", "Angst\u00b7ge\u00b7dr\u00e4ng", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur Freiheit macht die Seele weit,", "tokens": ["Nur", "Frei\u00b7heit", "macht", "die", "See\u00b7le", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Knechtschaft macht sie eng.", "tokens": ["Und", "Knecht\u00b7schaft", "macht", "sie", "eng", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Dann preis' ich dich im weiten Raum,", "tokens": ["Dann", "preis'", "ich", "dich", "im", "wei\u00b7ten", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, Helfer in der Noth,", "tokens": ["Dich", ",", "Hel\u00b7fer", "in", "der", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und halte ohne Zwang und Zaum", "tokens": ["Und", "hal\u00b7te", "oh\u00b7ne", "Zwang", "und", "Zaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein g\u00f6ttliches Gebot.", "tokens": ["Dein", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Walt's Gott, der Tag bricht wieder an", "tokens": ["Walt's", "Gott", ",", "der", "Tag", "bricht", "wie\u00b7der", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weckt mich aus der Ruh';", "tokens": ["Und", "weckt", "mich", "aus", "der", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlauf, betritt die Dornenbahn!", "tokens": ["Wohl\u00b7auf", ",", "be\u00b7tritt", "die", "Dor\u00b7nen\u00b7bahn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, meine Seele, du!", "tokens": ["Du", ",", "mei\u00b7ne", "See\u00b7le", ",", "du", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da neben meinem Bette steht", "tokens": ["Da", "ne\u00b7ben", "mei\u00b7nem", "Bet\u00b7te", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kreuz, ich nehm' es auf,", "tokens": ["Mein", "Kreuz", ",", "ich", "nehm'", "es", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schick' ein weinendes Gebet", "tokens": ["Und", "schick'", "ein", "wei\u00b7nen\u00b7des", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zum lieben Gott hinauf.", "tokens": ["Zum", "lie\u00b7ben", "Gott", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er wird mir's tragen helfen, ach!", "tokens": ["Er", "wird", "mir's", "tra\u00b7gen", "hel\u00b7fen", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVINF", "VVINF", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df es, Gott ist gut;", "tokens": ["Ich", "wei\u00df", "es", ",", "Gott", "ist", "gut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unm\u00e4chtig bin ich, krank und schwach,", "tokens": ["Un\u00b7m\u00e4ch\u00b7tig", "bin", "ich", ",", "krank", "und", "schwach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er aber gibt mir Muth;", "tokens": ["Er", "a\u00b7ber", "gibt", "mir", "Muth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Da\u00df mich die Hoffnung nicht verl\u00e4\u00dft,", "tokens": ["Da\u00df", "mich", "die", "Hoff\