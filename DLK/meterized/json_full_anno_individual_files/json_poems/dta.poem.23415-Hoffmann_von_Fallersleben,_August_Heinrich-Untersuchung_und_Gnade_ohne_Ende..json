{"dta.poem.23415": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Untersuchung und Gnade ohne Ende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Demagogenf\u00e4ngerei", "tokens": ["Die", "De\u00b7ma\u00b7go\u00b7gen\u00b7f\u00e4n\u00b7ge\u00b7rei"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei wieder allgemein!", "tokens": ["Sei", "wie\u00b7der", "all\u00b7ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man denkt und spricht doch gar zu frei:", "tokens": ["Man", "denkt", "und", "spricht", "doch", "gar", "zu", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das soll und darf nicht sein!", "tokens": ["Das", "soll", "und", "darf", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00dft dem Gesetze freien Lauf!", "tokens": ["La\u00dft", "dem", "Ge\u00b7set\u00b7ze", "frei\u00b7en", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr habt genug verziehn.", "tokens": ["Ihr", "habt", "ge\u00b7nug", "ver\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Macht eure Kerker wieder auf", "tokens": ["Macht", "eu\u00b7re", "Ker\u00b7ker", "wie\u00b7der", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr k\u00fcnft'ge Amnestien!", "tokens": ["F\u00fcr", "k\u00fcnft'\u00b7ge", "Am\u00b7nes\u00b7ti\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist die h\u00f6chste Poesie,", "tokens": ["Es", "ist", "die", "h\u00f6chs\u00b7te", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist ein wahres Fest,", "tokens": ["Es", "ist", "ein", "wah\u00b7res", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sich der Gnadenborn doch nie", "tokens": ["Wenn", "sich", "der", "Gna\u00b7den\u00b7born", "doch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nie ersch\u00f6pfen l\u00e4\u00dft.", "tokens": ["Und", "nie", "er\u00b7sch\u00f6p\u00b7fen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}