{"dta.poem.19046": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxVII.  An eines begabten Lehrers Nah-  \n mens-Tage.   Am 30. Novembr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du hochgebenedeyte Liebe,", "tokens": ["Du", "hoch\u00b7ge\u00b7be\u00b7ne\u00b7dey\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sagt in Bethel: H\u00f6re her.", "tokens": ["Man", "sagt", "in", "Be\u00b7thel", ":", "H\u00f6\u00b7re", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O da\u00df der angeflammten Triebe", "tokens": ["O", "da\u00df", "der", "an\u00b7ge\u00b7flamm\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur eine einge Flamme w\u00e4r.", "tokens": ["Nur", "ei\u00b7ne", "ein\u00b7ge", "Flam\u00b7me", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast uns alle angez\u00fcndet,", "tokens": ["Du", "hast", "uns", "al\u00b7le", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf dich sind wir allein gegr\u00fcndet,", "tokens": ["Auf", "dich", "sind", "wir", "al\u00b7lein", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Prediger, und wer ihn h\u00f6rt,", "tokens": ["Der", "Pre\u00b7di\u00b7ger", ",", "und", "wer", "ihn", "h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer als ein reiner Funck entglommen,", "tokens": ["Wer", "als", "ein", "rei\u00b7ner", "Funck", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Hat einen Hauch von dir bekommen,", "tokens": ["Hat", "ei\u00b7nen", "Hauch", "von", "dir", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der wieder in dein Feuer f\u00e4hrt.", "tokens": ["Der", "wie\u00b7der", "in", "dein", "Feu\u00b7er", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was wollen wir so dunckel sprechen?", "tokens": ["Was", "wol\u00b7len", "wir", "so", "dun\u00b7ckel", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wallen in der Dunckelheit.", "tokens": ["Wir", "wal\u00b7len", "in", "der", "Dun\u00b7ckel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wilst du mit deinem Licht durchbrechen,", "tokens": ["Wilst", "du", "mit", "dei\u00b7nem", "Licht", "durch\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schenckst du uns deine Heiterkeit,", "tokens": ["Schenckst", "du", "uns", "dei\u00b7ne", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So k\u00f6nnen wir es offenbaren,", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "es", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was wir im Inneren bewahren,", "tokens": ["Was", "wir", "im", "In\u00b7ne\u00b7ren", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und was so schwer zu deuten ist.", "tokens": ["Und", "was", "so", "schwer", "zu", "deu\u00b7ten", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gieb doch an diesem Freuden-Tage,", "tokens": ["Gieb", "doch", "an", "die\u00b7sem", "Freu\u00b7den\u00b7Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df jeder deutlich sing und sage,", "tokens": ["Da\u00df", "je\u00b7der", "deut\u00b7lich", "sing", "und", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Was du vor eine Liebe bist.", "tokens": ["Was", "du", "vor", "ei\u00b7ne", "Lie\u00b7be", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist ein ewiger Regente,", "tokens": ["Du", "bist", "ein", "e\u00b7wi\u00b7ger", "Re\u00b7gen\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Allein du herrschest in der Zeit,", "tokens": ["Al\u00b7lein", "du", "herr\u00b7schest", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als deine Flamm in Ruhe brennte", "tokens": ["Als", "dei\u00b7ne", "Flamm", "in", "Ru\u00b7he", "brenn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In jener tiefen Ewigkeit,", "tokens": ["In", "je\u00b7ner", "tie\u00b7fen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da wurdest du doch mit Verlangen", "tokens": ["Da", "wur\u00b7dest", "du", "doch", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach einer Creatur gefangen,", "tokens": ["Nach", "ei\u00b7ner", "Crea\u00b7tur", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und diese Creatur sind wir,", "tokens": ["Und", "die\u00b7se", "Crea\u00b7tur", "sind", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wir und noch viele Millionen,", "tokens": ["Wir", "und", "noch", "vie\u00b7le", "Mil\u00b7lion\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ADV", "PIAT", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Die nah und in der ferne wohnen,", "tokens": ["Die", "nah", "und", "in", "der", "fer\u00b7ne", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir alle schreiben uns von dir.", "tokens": ["Wir", "al\u00b7le", "schrei\u00b7ben", "uns", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir wissens wol, da\u00df alle Lande", "tokens": ["Wir", "wis\u00b7sens", "wol", ",", "da\u00df", "al\u00b7le", "Lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit deiner Treu belehnet sind,", "tokens": ["Mit", "dei\u00b7ner", "Treu", "be\u00b7leh\u00b7net", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df man in einem jeden Stande", "tokens": ["Da\u00df", "man", "in", "ei\u00b7nem", "je\u00b7den", "Stan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gewisse GOttes Menschen findt,", "tokens": ["Ge\u00b7wis\u00b7se", "Got\u00b7tes", "Men\u00b7schen", "findt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df in Lausitz mehr Gem\u00fcther,", "tokens": ["Und", "da\u00df", "in", "Lau\u00b7sitz", "mehr", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wir, geniessen deiner G\u00fcter,", "tokens": ["Als", "wir", ",", "ge\u00b7nies\u00b7sen", "dei\u00b7ner", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sind kein sonderlich Geschlecht.", "tokens": ["Wir", "sind", "kein", "son\u00b7der\u00b7lich", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn aber deine Vater-Augen", "tokens": ["Wenn", "a\u00b7ber", "dei\u00b7ne", "Va\u00b7ter\u00b7Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Aufs Niedrige zu sehen taugen,", "tokens": ["Aufs", "Nied\u00b7ri\u00b7ge", "zu", "se\u00b7hen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So haben wir ein eignes Recht.", "tokens": ["So", "ha\u00b7ben", "wir", "ein", "eig\u00b7nes", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es ist dir Vater unentfallen,", "tokens": ["Es", "ist", "dir", "Va\u00b7ter", "un\u00b7ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was man von deinem Bethel h\u00e4lt,", "tokens": ["Was", "man", "von", "dei\u00b7nem", "Be\u00b7thel", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In was vor grosser Schmach wir wallen,", "tokens": ["In", "was", "vor", "gros\u00b7ser", "Schmach", "wir", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie man uns zur\u00fccke stellt.", "tokens": ["Und", "wie", "man", "uns", "zu\u00b7r\u00fc\u00b7cke", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ort ist an sich selbst nicht wichtig,", "tokens": ["Der", "Ort", "ist", "an", "sich", "selbst", "nicht", "wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir suchen dich, damit ists richtig.", "tokens": ["Wir", "su\u00b7chen", "dich", ",", "da\u00b7mit", "ists", "rich\u00b7tig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PAV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wir sind der Welt ihr Gauckel-Spiel.", "tokens": ["Wir", "sind", "der", "Welt", "ihr", "Gau\u00b7ckel\u00b7Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der HErr und seine Unterthanen,", "tokens": ["Der", "Herr", "und", "sei\u00b7ne", "Un\u00b7ter\u00b7tha\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So viel dir ihre Hertze bahnen,", "tokens": ["So", "viel", "dir", "ih\u00b7re", "Hert\u00b7ze", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sind aller L\u00e4sterungen Ziel.", "tokens": ["Sind", "al\u00b7ler", "L\u00e4s\u00b7te\u00b7run\u00b7gen", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hasts ja aber ausgesprochen:", "tokens": ["Du", "hasts", "ja", "a\u00b7ber", "aus\u00b7ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr, die ihr leidet, seyd begl\u00fcckt.", "tokens": ["Ihr", ",", "die", "ihr", "lei\u00b7det", ",", "seyd", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihr mir nach ans Creutz gekrochen,", "tokens": ["Die", "ihr", "mir", "nach", "ans", "Creutz", "ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPR", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr werdet mit hinauf ger\u00fcckt.", "tokens": ["Ihr", "wer\u00b7det", "mit", "hin\u00b7auf", "ge\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ich verbleibe, bleibt mein J\u00fcnger,", "tokens": ["Wo", "ich", "ver\u00b7blei\u00b7be", ",", "bleibt", "mein", "J\u00fcn\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein schlechtes Thun ist nichts geringer,", "tokens": ["Sein", "schlech\u00b7tes", "Thun", "ist", "nichts", "ge\u00b7rin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als was ich in der Welt gethan.", "tokens": ["Als", "was", "ich", "in", "der", "Welt", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Freund! haben wir dich aufgenommen,", "tokens": ["Freund", "!", "ha\u00b7ben", "wir", "dich", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wissen nirgends durchzukommen,", "tokens": ["Und", "wis\u00b7sen", "nir\u00b7gends", "durch\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So nimm du uns auch wieder an.", "tokens": ["So", "nimm", "du", "uns", "auch", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wir leiden ohne alles Murren,", "tokens": ["Wir", "lei\u00b7den", "oh\u00b7ne", "al\u00b7les", "Mur\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir geben gar der Welt die Macht", "tokens": ["Wir", "ge\u00b7ben", "gar", "der", "Welt", "die", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns zu verl\u00e4umden, anzuschnurren,", "tokens": ["Uns", "zu", "ver\u00b7l\u00e4um\u00b7den", ",", "an\u00b7zu\u00b7schnur\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir werden gern um dich verlacht.", "tokens": ["Wir", "wer\u00b7den", "gern", "um", "dich", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man mag uns lose Br\u00fccken bauen,", "tokens": ["Man", "mag", "uns", "lo\u00b7se", "Br\u00fc\u00b7cken", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns soll vor alle dem nicht grauen,", "tokens": ["Uns", "soll", "vor", "al\u00b7le", "dem", "nicht", "grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur nicht um einen b\u00f6sen Schein,", "tokens": ["Nur", "nicht", "um", "ei\u00b7nen", "b\u00f6\u00b7sen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dieweil wir uns ins Amt gedrungen,", "tokens": ["Die\u00b7weil", "wir", "uns", "ins", "Amt", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dieweil wir \u00fcbel umgesprungen.", "tokens": ["Die\u00b7weil", "wir", "\u00fc\u00b7bel", "um\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nein! darum, weil wir Christen seyn.", "tokens": ["Nein", "!", "da\u00b7rum", ",", "weil", "wir", "Chris\u00b7ten", "seyn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PAV", "$,", "KOUS", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Weil aber du der Kinder Lallen,", "tokens": ["Weil", "a\u00b7ber", "du", "der", "Kin\u00b7der", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du, treuer Vater, nie verschm\u00e4ht:", "tokens": ["Du", ",", "treu\u00b7er", "Va\u00b7ter", ",", "nie", "ver\u00b7schm\u00e4ht", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df dir itzo auch gefallen,", "tokens": ["So", "la\u00df", "dir", "it\u00b7zo", "auch", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn die Gemeine zu dir fleht:", "tokens": ["Wenn", "die", "Ge\u00b7mei\u00b7ne", "zu", "dir", "fleht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wollest uns zusammen halten,", "tokens": ["Du", "wol\u00b7lest", "uns", "zu\u00b7sam\u00b7men", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcber unsrer Liebe walten,", "tokens": ["Und", "\u00fc\u00b7ber", "uns\u00b7rer", "Lie\u00b7be", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als \u00fcber deinen Augen-Stern.", "tokens": ["Als", "\u00fc\u00b7ber", "dei\u00b7nen", "Au\u00b7gen\u00b7S\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Wir werden hin und her geschmissen;", "tokens": ["Wir", "wer\u00b7den", "hin", "und", "her", "ge\u00b7schmis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es sey drum nur nichts abgerissen", "tokens": ["Es", "sey", "drum", "nur", "nichts", "ab\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "PIS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von unserm Bunde f\u00fcr den HErrn.", "tokens": ["Von", "un\u00b7serm", "Bun\u00b7de", "f\u00fcr", "den", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und d\u00fcrffen wir noch eines bitten,", "tokens": ["Und", "d\u00fcrf\u00b7fen", "wir", "noch", "ei\u00b7nes", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wollst du unsers Rothens Geist", "tokens": ["So", "wollst", "du", "un\u00b7sers", "Ro\u00b7thens", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinem Frieden \u00fcbersch\u00fctten,", "tokens": ["Mit", "dei\u00b7nem", "Frie\u00b7den", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich bereits in ihm erweist.", "tokens": ["Der", "sich", "be\u00b7reits", "in", "ihm", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es bleiben er, und die ihn lieben,", "tokens": ["Es", "blei\u00b7ben", "er", ",", "und", "die", "ihn", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir zum Ged\u00e4chtni\u00df angeschrieben,", "tokens": ["Dir", "zum", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "an\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als solche, die dein Hertze hegt.", "tokens": ["Als", "sol\u00b7che", ",", "die", "dein", "Hert\u00b7ze", "hegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Man seh in allem, was er handelt,", "tokens": ["Man", "seh", "in", "al\u00b7lem", ",", "was", "er", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df JEsus selber in ihm wandelt,", "tokens": ["Da\u00df", "Je\u00b7sus", "sel\u00b7ber", "in", "ihm", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und alle seine Glieder tr\u00e4gt.", "tokens": ["Und", "al\u00b7le", "sei\u00b7ne", "Glie\u00b7der", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}