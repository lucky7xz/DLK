, ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Horn irrt, er blitzt rosenrot,", "tokens": ["Das", "Horn", "irrt", ",", "er", "blitzt", "ro\u00b7sen\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da ist das Hirschlein wohl verloren,", "tokens": ["Da", "ist", "das", "Hirsc\u00b7hlein", "wohl", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stellt selber sich zum lust'gen Tod.", "tokens": ["Stellt", "sel\u00b7ber", "sich", "zum", "lust'\u00b7gen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Vor allen aber die Verliebten,", "tokens": ["Vor", "al\u00b7len", "a\u00b7ber", "die", "Ver\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die lad ich ein zur J\u00e4gerlust,", "tokens": ["Die", "lad", "ich", "ein", "zur", "J\u00e4\u00b7ger\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur nicht die weinerlich Betr\u00fcbten;", "tokens": ["Nur", "nicht", "die", "wei\u00b7ner\u00b7lich", "Be\u00b7tr\u00fcb\u00b7ten", ";"], "token_in