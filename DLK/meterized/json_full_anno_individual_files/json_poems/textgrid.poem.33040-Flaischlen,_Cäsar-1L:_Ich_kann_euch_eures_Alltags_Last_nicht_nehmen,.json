{"textgrid.poem.33040": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kann euch eures Alltags Last nicht nehmen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann euch eures Alltags Last nicht nehmen,", "tokens": ["Ich", "kann", "euch", "eu\u00b7res", "All\u00b7tags", "Last", "nicht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie mir die meine niemand nehmen kann", "tokens": ["wie", "mir", "die", "mei\u00b7ne", "nie\u00b7mand", "neh\u00b7men", "kann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "PPOSAT", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und auch nicht nehmen soll ...", "tokens": ["und", "auch", "nicht", "neh\u00b7men", "soll", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "ein jeder finde selber sich zurecht,", "tokens": ["ein", "je\u00b7der", "fin\u00b7de", "sel\u00b7ber", "sich", "zu\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ein jeder trage selbst, womit er sich bel\u00e4dt,", "tokens": ["ein", "je\u00b7der", "tra\u00b7ge", "selbst", ",", "wo\u00b7mit", "er", "sich", "be\u00b7l\u00e4dt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und k\u00e4mpfe selber sich durch Weh und Wohl!", "tokens": ["und", "k\u00e4mp\u00b7fe", "sel\u00b7ber", "sich", "durch", "Weh", "und", "Wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "APPR", "NN", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was ich vermag, es ist nicht mehr vielleicht,", "tokens": ["Was", "ich", "ver\u00b7mag", ",", "es", "ist", "nicht", "mehr", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "als euch in stiller Feierabendstunde", "tokens": ["als", "euch", "in", "stil\u00b7ler", "Fei\u00b7er\u00b7a\u00b7bends\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zu zeigen:", "tokens": ["zu", "zei\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "wie es mir gleich tausend andern ging:", "tokens": ["wie", "es", "mir", "gleich", "tau\u00b7send", "an\u00b7dern", "ging", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "CARD", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "wie's mich geduckt,", "tokens": ["wie's", "mich", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "und wie ich gezuckt", "tokens": ["und", "wie", "ich", "ge\u00b7zuckt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "und wie ich jede Zuversicht verlor ...", "tokens": ["und", "wie", "ich", "je\u00b7de", "Zu\u00b7ver\u00b7sicht", "ver\u00b7lor", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "und wie ich pl\u00f6tzlich dann trotzig wurde:", "tokens": ["und", "wie", "ich", "pl\u00f6tz\u00b7lich", "dann", "trot\u00b7zig", "wur\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "was andre zwingen, das zwingst du auch!", "tokens": ["was", "and\u00b7re", "zwin\u00b7gen", ",", "das", "zwingst", "du", "auch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "$,", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "es gibt kein Schicksal! Verlust und Gewinn", "tokens": ["es", "gibt", "kein", "Schick\u00b7sal", "!", "Ver\u00b7lust", "und", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "ist nur, was ich selber will und bin!", "tokens": ["ist", "nur", ",", "was", "ich", "sel\u00b7ber", "will", "und", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PWS", "PPER", "ADV", "VMFIN", "KON", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und wie ich die Arme dann frei mir rang,", "tokens": ["Und", "wie", "ich", "die", "Ar\u00b7me", "dann", "frei", "mir", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "und wie ich den Kopf wieder hoch bekam,", "tokens": ["und", "wie", "ich", "den", "Kopf", "wie\u00b7der", "hoch", "be\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "und wie ich zu mir selber fand,", "tokens": ["und", "wie", "ich", "zu", "mir", "sel\u00b7ber", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wie sich langsam immer klarer,", "tokens": ["und", "wie", "sich", "lang\u00b7sam", "im\u00b7mer", "kla\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "immer freier, voller und wahrer", "tokens": ["im\u00b7mer", "frei\u00b7er", ",", "vol\u00b7ler", "und", "wah\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJA", "KON", "ADJA"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "aus der versch\u00fctteten Tiefe hob:", "tokens": ["aus", "der", "ver\u00b7sch\u00fct\u00b7te\u00b7ten", "Tie\u00b7fe", "hob