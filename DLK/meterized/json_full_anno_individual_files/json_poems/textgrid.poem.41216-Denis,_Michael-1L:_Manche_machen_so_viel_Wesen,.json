{"textgrid.poem.41216": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manche machen so viel Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manche machen so viel Wesen,", "tokens": ["Man\u00b7che", "ma\u00b7chen", "so", "viel", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie Poesien lesen.", "tokens": ["Wenn", "sie", "Poe\u00b7si\u00b7en", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ich begreife nicht warum!", "tokens": ["Ich", "be\u00b7grei\u00b7fe", "nicht", "wa\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sind aller Dichter Werke?", "tokens": ["Was", "sind", "al\u00b7ler", "Dich\u00b7ter", "Wer\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich, ich setze meine St\u00e4rke", "tokens": ["Ich", ",", "ich", "set\u00b7ze", "mei\u00b7ne", "St\u00e4r\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ein Chronographicum.", "tokens": ["In", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mag doch Stax beim H\u00fcbner fluchen,", "tokens": ["Mag", "doch", "Stax", "beim", "H\u00fcb\u00b7ner", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sylben z\u00e4hlen, Reime suchen;", "tokens": ["Syl\u00b7ben", "z\u00e4h\u00b7len", ",", "Rei\u00b7me", "su\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was entgehet mir darum?", "tokens": ["Was", "ent\u00b7ge\u00b7het", "mir", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich mu\u00df seines Flei\u00dfes lachen;", "tokens": ["Ich", "mu\u00df", "sei\u00b7nes", "Flei\u00b7\u00dfes", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er kann Epop\u00f6en machen!", "tokens": ["Er", "kann", "E\u00b7pop\u00b7\u00f6en", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich? ein Chronographicum.", "tokens": ["Ich", "?", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ein Namenstag erscheinet,", "tokens": ["Wenn", "ein", "Na\u00b7mens\u00b7tag", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Um ein Grab die Freundschaft weinet,", "tokens": ["Um", "ein", "Grab", "die", "Freund\u00b7schaft", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O da bleib' ich niemal stumm!", "tokens": ["O", "da", "bleib'", "ich", "nie\u00b7mal", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein F\u00fcrstenkind geboren,", "tokens": ["Wird", "ein", "F\u00fcrs\u00b7ten\u00b7kind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat der Feind die Schlacht verloren,", "tokens": ["Hat", "der", "Feind", "die", "Schlacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flugs ein Chronographicum.", "tokens": ["Flugs", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zeigt sich mir ein neu Geb\u00e4ude,", "tokens": ["Zeigt", "sich", "mir", "ein", "neu", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprech' ich oft mit Herzeleide:", "tokens": ["Sprech'", "ich", "oft", "mit", "Her\u00b7ze\u00b7lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns're Zeiten werden dumm!", "tokens": ["Un\u00b7s'\u00b7re", "Zei\u00b7ten", "wer\u00b7den", "dumm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ueber's Thor ist nichts geschrieben!", "tokens": ["Ue\u00b7ber's", "Thor", "ist", "nichts", "ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ein Frontispice belieben", "tokens": ["Kann", "ein", "Fron\u00b7tis\u00b7pi\u00b7ce", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ohne Chronographicum?", "tokens": ["Oh\u00b7ne", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sind Alt\u00e4re, sind Troph\u00e4en,", "tokens": ["Sind", "Al\u00b7t\u00e4\u00b7re", ",", "sind", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ist ein Trau'rger\u00fcst zu sehen,", "tokens": ["Ist", "ein", "Trau'r\u00b7ge\u00b7r\u00fcst", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O da guck' ich um und um.", "tokens": ["O", "da", "guck'", "ich", "um", "und", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "KOUI", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "And're mag die Kunst entz\u00fccken,", "tokens": ["An\u00b7d'\u00b7re", "mag", "die", "Kunst", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich such' unter Meisterst\u00fccken", "tokens": ["Ich", "such'", "un\u00b7ter", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nur ein Chronographicum.", "tokens": ["Nur", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6r' ich einen Namen nennen,", "tokens": ["H\u00f6r'", "ich", "ei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Gleich beginn' ich auf zu brennen.", "tokens": ["Gleich", "be\u00b7ginn'", "ich", "auf", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcstern denk' ich ihn herum.", "tokens": ["L\u00fcs\u00b7tern", "denk'", "ich", "ihn", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein M \u2013 ein D \u2013 Gewonnen!", "tokens": ["Nur", "ein", "M", "\u2013", "ein", "D", "\u2013", "Ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NE", "$(", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Namen ist ersonnen", "tokens": ["Die\u00b7ser", "Na\u00b7men", "ist", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr ein Chronographicum.", "tokens": ["F\u00fcr", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so soll im Letternhaschen", "tokens": ["Und", "so", "soll", "im", "Let\u00b7tern\u00b7ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Einst der Tod mich \u00fcberraschen.", "tokens": ["Einst", "der", "Tod", "mich", "\u00fc\u00b7berr\u00b7a\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die\u00df sey mein letzter Ruhm,", "tokens": ["Doch", "die\u00df", "sey", "mein", "letz\u00b7ter", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er mich nicht eh' bezwinge,", "tokens": ["Da\u00df", "er", "mich", "nicht", "eh'", "be\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ich noch mein Sterbjahr bringe", "tokens": ["Bis", "ich", "noch", "mein", "Sterb\u00b7jahr", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ein Chronographicum.", "tokens": ["In", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Gr\u00e4ber, die mit Spie\u00df und Stangen,", "tokens": ["Gr\u00e4\u00b7ber", ",", "die", "mit", "Spie\u00df", "und", "Stan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wappen, Helmen, Fahnen prangen,", "tokens": ["Wap\u00b7pen", ",", "Hel\u00b7men", ",", "Fah\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind der Helden Eigenthum.", "tokens": ["Sind", "der", "Hel\u00b7den", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dichtergr\u00e4ber m\u00f6gen gl\u00e4nzen", "tokens": ["Dich\u00b7ter\u00b7gr\u00e4\u00b7ber", "m\u00f6\u00b7gen", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit ersung'nen Lorbeergr\u00e4nzen!", "tokens": ["Mit", "er\u00b7sung'\u00b7nen", "Lor\u00b7beer\u00b7gr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir ein Chronographicum!", "tokens": ["Mir", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Manche machen so viel Wesen,", "tokens": ["Man\u00b7che", "ma\u00b7chen", "so", "viel", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie Poesien lesen.", "tokens": ["Wenn", "sie", "Poe\u00b7si\u00b7en", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ich begreife nicht warum!", "tokens": ["Ich", "be\u00b7grei\u00b7fe", "nicht", "wa\u00b7rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sind aller Dichter Werke?", "tokens": ["Was", "sind", "al\u00b7ler", "Dich\u00b7ter", "Wer\u00b7ke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich, ich setze meine St\u00e4rke", "tokens": ["Ich", ",", "ich", "set\u00b7ze", "mei\u00b7ne", "St\u00e4r\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ein Chronographicum.", "tokens": ["In", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mag doch Stax beim H\u00fcbner fluchen,", "tokens": ["Mag", "doch", "Stax", "beim", "H\u00fcb\u00b7ner", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sylben z\u00e4hlen, Reime suchen;", "tokens": ["Syl\u00b7ben", "z\u00e4h\u00b7len", ",", "Rei\u00b7me", "su\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was entgehet mir darum?", "tokens": ["Was", "ent\u00b7ge\u00b7het", "mir", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich mu\u00df seines Flei\u00dfes lachen;", "tokens": ["Ich", "mu\u00df", "sei\u00b7nes", "Flei\u00b7\u00dfes", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Er kann Epop\u00f6en machen!", "tokens": ["Er", "kann", "E\u00b7pop\u00b7\u00f6en", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich? ein Chronographicum.", "tokens": ["Ich", "?", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn ein Namenstag erscheinet,", "tokens": ["Wenn", "ein", "Na\u00b7mens\u00b7tag", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Um ein Grab die Freundschaft weinet,", "tokens": ["Um", "ein", "Grab", "die", "Freund\u00b7schaft", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O da bleib' ich niemal stumm!", "tokens": ["O", "da", "bleib'", "ich", "nie\u00b7mal", "stumm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein F\u00fcrstenkind geboren,", "tokens": ["Wird", "ein", "F\u00fcrs\u00b7ten\u00b7kind", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hat der Feind die Schlacht verloren,", "tokens": ["Hat", "der", "Feind", "die", "Schlacht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flugs ein Chronographicum.", "tokens": ["Flugs", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Zeigt sich mir ein neu Geb\u00e4ude,", "tokens": ["Zeigt", "sich", "mir", "ein", "neu", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprech' ich oft mit Herzeleide:", "tokens": ["Sprech'", "ich", "oft", "mit", "Her\u00b7ze\u00b7lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns're Zeiten werden dumm!", "tokens": ["Un\u00b7s'\u00b7re", "Zei\u00b7ten", "wer\u00b7den", "dumm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ueber's Thor ist nichts geschrieben!", "tokens": ["Ue\u00b7ber's", "Thor", "ist", "nichts", "ge\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann ein Frontispice belieben", "tokens": ["Kann", "ein", "Fron\u00b7tis\u00b7pi\u00b7ce", "be\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ohne Chronographicum?", "tokens": ["Oh\u00b7ne", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Sind Alt\u00e4re, sind Troph\u00e4en,", "tokens": ["Sind", "Al\u00b7t\u00e4\u00b7re", ",", "sind", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Ist ein Trau'rger\u00fcst zu sehen,", "tokens": ["Ist", "ein", "Trau'r\u00b7ge\u00b7r\u00fcst", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O da guck' ich um und um.", "tokens": ["O", "da", "guck'", "ich", "um", "und", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "KOUI", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "And're mag die Kunst entz\u00fccken,", "tokens": ["An\u00b7d'\u00b7re", "mag", "die", "Kunst", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich such' unter Meisterst\u00fccken", "tokens": ["Ich", "such'", "un\u00b7ter", "Meis\u00b7ter\u00b7st\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Nur ein Chronographicum.", "tokens": ["Nur", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "H\u00f6r' ich einen Namen nennen,", "tokens": ["H\u00f6r'", "ich", "ei\u00b7nen", "Na\u00b7men", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Gleich beginn' ich auf zu brennen.", "tokens": ["Gleich", "be\u00b7ginn'", "ich", "auf", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcstern denk' ich ihn herum.", "tokens": ["L\u00fcs\u00b7tern", "denk'", "ich", "ihn", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein M \u2013 ein D \u2013 Gewonnen!", "tokens": ["Nur", "ein", "M", "\u2013", "ein", "D", "\u2013", "Ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NE", "$(", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser Namen ist ersonnen", "tokens": ["Die\u00b7ser", "Na\u00b7men", "ist", "er\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcr ein Chronographicum.", "tokens": ["F\u00fcr", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und so soll im Letternhaschen", "tokens": ["Und", "so", "soll", "im", "Let\u00b7tern\u00b7ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Einst der Tod mich \u00fcberraschen.", "tokens": ["Einst", "der", "Tod", "mich", "\u00fc\u00b7berr\u00b7a\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die\u00df sey mein letzter Ruhm,", "tokens": ["Doch", "die\u00df", "sey", "mein", "letz\u00b7ter", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er mich nicht eh' bezwinge,", "tokens": ["Da\u00df", "er", "mich", "nicht", "eh'", "be\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bis ich noch mein Sterbjahr bringe", "tokens": ["Bis", "ich", "noch", "mein", "Sterb\u00b7jahr", "brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ein Chronographicum.", "tokens": ["In", "ein", "Chro\u00b7no\u00b7gra\u00b7phi\u00b7cum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Gr\u00e4ber, die mit Spie\u00df und Stangen,", "tokens": ["Gr\u00e4\u00b7ber", ",", "die", "mit", "Spie\u00df", "und", "Stan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wappen, Helmen, Fahnen prangen,", "tokens": ["Wap\u00b7pen", ",", "Hel\u00b7men", ",", "Fah\u00b7nen", "pran\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3":