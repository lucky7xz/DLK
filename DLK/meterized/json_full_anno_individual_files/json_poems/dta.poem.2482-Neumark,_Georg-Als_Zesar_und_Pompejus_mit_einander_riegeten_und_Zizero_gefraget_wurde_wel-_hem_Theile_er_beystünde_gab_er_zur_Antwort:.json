{"dta.poem.2482": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als Zesar und Pompejus mit einander  \n riegeten/ und Zizero gefraget wurde/ wel-  \n hem Theile er beyst\u00fcnde/ gab er zur Antwort:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar ich wei\u00df wol wen ich fliehe/ aber doch nicht wen ich", "tokens": ["Zwar", "ich", "wei\u00df", "wol", "wen", "ich", "flie\u00b7he", "/", "a\u00b7ber", "doch", "nicht", "wen", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$(", "ADV", "ADV", "PTKNEG", "PWS", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "sch\u00fctzen/", "tokens": ["sch\u00fct\u00b7zen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Auch nicht weichem ich sol folge\u0303: Ob der kluge Zesar siegt", "tokens": ["Auch", "nicht", "wei\u00b7chem", "ich", "sol", "fol\u00b7g\u1ebd", ":", "Ob", "der", "klu\u00b7ge", "Ze\u00b7sar", "siegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADJA", "PPER", "VMFIN", "PIS", "$.", "KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und Pompejen nieder schl\u00e4get/ oder ob er unten liegt.", "tokens": ["Und", "Pom\u00b7pe\u00b7jen", "nie\u00b7der", "schl\u00e4\u00b7get", "/", "o\u00b7der", "ob", "er", "un\u00b7ten", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "VVFIN", "$(", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Keines unter diesen beyden wird der R\u00f6mschen Freyheit", "tokens": ["Kei\u00b7nes", "un\u00b7ter", "die\u00b7sen", "bey\u00b7den", "wird", "der", "R\u00f6m\u00b7schen", "Frey\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "PDAT", "PIAT", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "n\u00fctzen.", "tokens": ["n\u00fct\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}