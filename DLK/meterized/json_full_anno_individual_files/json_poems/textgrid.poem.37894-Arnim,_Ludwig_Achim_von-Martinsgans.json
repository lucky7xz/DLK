{"textgrid.poem.37894": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Martinsgans", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Gras wir wollen gehn,", "tokens": ["Nach", "Gras", "wir", "wol\u00b7len", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gel singen sch\u00f6n,", "tokens": ["Die", "V\u00f6\u00b7gel", "sin\u00b7gen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Gutzgauch frey,", "tokens": ["Der", "Gutz\u00b7gauch", "frey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Sein Melodey,", "tokens": ["Sein", "Me\u00b7lo\u00b7dey", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Hallt \u00fcber Berg und Thal,", "tokens": ["Hallt", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die M\u00fchle klappt zumal;", "tokens": ["Die", "M\u00fch\u00b7le", "klappt", "zu\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der M\u00fcller auf der Oberm\u00fchl,", "tokens": ["Der", "M\u00fcl\u00b7ler", "auf", "der", "O\u00b7ber\u00b7m\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der hat der fetten G\u00e4nse viel,", "tokens": ["Der", "hat", "der", "fet\u00b7ten", "G\u00e4n\u00b7se", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Gans hat einen Kragen,", "tokens": ["Die", "Gans", "hat", "ei\u00b7nen", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die wolln wir mit uns tragen.", "tokens": ["Die", "wolln", "wir", "mit", "uns", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der beste Vogel, den ich weis,", "tokens": ["Der", "bes\u00b7te", "Vo\u00b7gel", ",", "den", "ich", "weis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die fette Gans,", "tokens": ["Das", "ist", "die", "fet\u00b7te", "Gans", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat zwei breite F\u00fc\u00dfe,", "tokens": ["Sie", "hat", "zwei", "brei\u00b7te", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu den langen Hals,", "t