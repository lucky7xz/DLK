{"dta.poem.7284": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Erde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6thliche Fl\u00f6ckchen ziehen", "tokens": ["R\u00f6th\u00b7li\u00b7che", "Fl\u00f6ck\u00b7chen", "zie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ueber die Berge fort,", "tokens": ["Ue\u00b7ber", "die", "Ber\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wie Purpurgew\u00e4nder,", "tokens": ["Und", "wie", "Pur\u00b7pur\u00b7ge\u00b7w\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und wie farbige B\u00e4nder", "tokens": ["Und", "wie", "far\u00b7bi\u00b7ge", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Flattert es hier und dort", "tokens": ["Flat\u00b7tert", "es", "hier", "und", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "KON", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "In der steigenden D\u00e4mmrung Hort.", "tokens": ["In", "der", "stei\u00b7gen\u00b7den", "D\u00e4mm\u00b7rung", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Gleich einem K\u00f6nigsgarten,", "tokens": ["Gleich", "ei\u00b7nem", "K\u00f6\u00b7nigs\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den verlassen die F\u00fcrstin hoch \u2014", "tokens": ["Den", "ver\u00b7las\u00b7sen", "die", "F\u00fcrs\u00b7tin", "hoch"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nur in der K\u00fchle ergehen", "tokens": ["Nur", "in", "der", "K\u00fch\u00b7le", "er\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und um die Beete sich drehen", "tokens": ["Und", "um", "die", "Bee\u00b7te", "sich", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fl\u00fcsternd ein Paar Hoffr\u00e4ulein noch.", "tokens": ["Fl\u00fcs\u00b7ternd", "ein", "Paar", "Hof\u00b7fr\u00e4u\u00b7lein", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Da des Himmels Vorhang sinkt,", "tokens": ["Da", "des", "Him\u00b7mels", "Vor\u00b7hang", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oeffnet sich der Erde Brust,", "tokens": ["Oeff\u00b7net", "sich", "der", "Er\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise, leise Kr\u00e4utlein trinkt,", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", "Kr\u00e4ut\u00b7lein", "trinkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und entschlummert unbewu\u00dft;", "tokens": ["Und", "ent\u00b7schlum\u00b7mert", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sein furchtsam W\u00e4chterlein,", "tokens": ["Und", "sein", "furcht\u00b7sam", "W\u00e4ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrmchen mit dem gr\u00fcnen Schein,", "tokens": ["W\u00fcrm\u00b7chen", "mit", "dem", "gr\u00fc\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Z\u00fcndet an dem Gl\u00fchholz sein", "tokens": ["Z\u00fcn\u00b7det", "an", "dem", "Gl\u00fch\u00b7holz", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leuchtchen klein.", "tokens": ["Leucht\u00b7chen", "klein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Der G\u00e4rtner, \u00fcber die Blumen gebeugt,", "tokens": ["Der", "G\u00e4rt\u00b7ner", ",", "\u00fc\u00b7ber", "die", "Blu\u00b7men", "ge\u00b7beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sp\u00fcrt an der Sohle den Thau,", "tokens": ["Sp\u00fcrt", "an", "der", "Soh\u00b7le", "den", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gleich vom n\u00e4chsten Halme er streicht", "tokens": ["Gleich", "vom", "n\u00e4chs\u00b7ten", "Hal\u00b7me", "er", "streicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "L\u00e4chelnd die Tropfen lau;", "tokens": ["L\u00e4\u00b7chelnd", "die", "Trop\u00b7fen", "lau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Geht noch einmal entlang den Wall,", "tokens": ["Geht", "noch", "ein\u00b7mal", "ent\u00b7lang", "den", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pr\u00fcft jede Knospe genau und gut:", "tokens": ["Pr\u00fcft", "je\u00b7de", "Knos\u00b7pe", "ge\u00b7nau", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "\u201eschlaft denn\u201c, spricht er, \u201eihr Kindlein all,", "tokens": ["\u201e", "schlaft", "denn", "\u201c", ",", "spricht", "er", ",", "\u201e", "ihr", "Kin\u00b7dlein", "all", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PPOSAT", "NN", "PIAT", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Schlafet! ich la\u00df euch der Mutter Hut;", "tokens": ["Schla\u00b7fet", "!", "ich", "la\u00df", "euch", "der", "Mut\u00b7ter", "Hut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Liebe Erde! mir sind die Wimpern schwer,", "tokens": ["Lie\u00b7be", "Er\u00b7de", "!", "mir", "sind", "die", "Wim\u00b7pern", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Hab' die letzte Nacht durchwacht,", "tokens": ["Hab'", "die", "letz\u00b7te", "Nacht", "durch\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Breit ", "tokens": ["Breit"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Nimm ", "tokens": ["Nimm"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}}}}}