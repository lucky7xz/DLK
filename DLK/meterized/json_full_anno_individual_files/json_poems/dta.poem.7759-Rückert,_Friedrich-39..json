{"dta.poem.7759": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Reicher sah den Dieb, der an der Hand verholen", "tokens": ["Ein", "Rei\u00b7cher", "sah", "den", "Dieb", ",", "der", "an", "der", "Hand", "ver\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trug einen Edelstein, den jenem er gestohlen.", "tokens": ["Trug", "ei\u00b7nen", "E\u00b7del\u00b7stein", ",", "den", "je\u00b7nem", "er", "ge\u00b7stoh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PDAT", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Abnehmen wollte er den Schatz ihm vor Gericht,", "tokens": ["Ab\u00b7neh\u00b7men", "woll\u00b7te", "er", "den", "Schatz", "ihm", "vor", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sah dem armen Dieb er erst ins Angesicht;", "tokens": ["Da", "sah", "dem", "ar\u00b7men", "Dieb", "er", "erst", "ins", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und sprach mitleidig so, als h\u00e4tt' er ihn gekr\u00e4nkt:", "tokens": ["Und", "sprach", "mit\u00b7lei\u00b7dig", "so", ",", "als", "h\u00e4tt'", "er", "ihn", "ge\u00b7kr\u00e4nkt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wahr? ich habe dir den Edelstein geschenkt! \u2014", "tokens": ["Nicht", "wahr", "?", "ich", "ha\u00b7be", "dir", "den", "E\u00b7del\u00b7stein", "ge\u00b7schenkt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Mensch, wo h\u00e4ttest du dein Leben hergenommen,", "tokens": ["O", "Mensch", ",", "wo", "h\u00e4t\u00b7test", "du", "dein", "Le\u00b7ben", "her\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du es nicht geschenkt h\u00e4ttest von Gott bekommen?", "tokens": ["Wenn", "du", "es", "nicht", "ge\u00b7schenkt", "h\u00e4t\u00b7test", "von", "Gott", "be\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "VAFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}