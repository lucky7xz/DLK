{"textgrid.poem.51992": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo wiltu mit dem Buch, o liebste Musa hin?", "tokens": ["Wo", "wil\u00b7tu", "mit", "dem", "Buch", ",", "o", "liebs\u00b7te", "Mu\u00b7sa", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "APPR", "ART", "NN", "$,", "FM", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sol deine Verse dann der Drucker \u00fcbersetzen?", "tokens": ["Sol", "dei\u00b7ne", "Ver\u00b7se", "dann", "der", "Dru\u00b7cker", "\u00fc\u00b7bers\u00b7et\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit was gedenckestu den Leser zu ergetzen?", "tokens": ["Mit", "was", "ge\u00b7den\u00b7ckes\u00b7tu", "den", "Le\u00b7ser", "zu", "er\u00b7get\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der alle Sylben wird auff neue Wirbel ziehn.", "tokens": ["Der", "al\u00b7le", "Syl\u00b7ben", "wird", "auff", "neu\u00b7e", "Wir\u00b7bel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er mache, was er thut. Die\u00df ist mein Trost und Ziel,", "tokens": ["Er", "ma\u00b7che", ",", "was", "er", "thut", ".", "Die\u00df", "ist", "mein", "Trost", "und", "Ziel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00e4llstu ihm, so hat er ja nicht fug zu klagen:", "tokens": ["Ge\u00b7f\u00e4lls\u00b7tu", "ihm", ",", "so", "hat", "er", "ja", "nicht", "fug", "zu", "kla\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob er viel Bl\u00e4tter m\u00fc\u00df im Lesen \u00fcberschlagen:", "tokens": ["Ob", "er", "viel", "Bl\u00e4t\u00b7ter", "m\u00fc\u00df", "im", "Le\u00b7sen", "\u00fc\u00b7bersc\u00b7hla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo nicht, gehstu gleich auff, vertirbt mir nicht zu viel.", "tokens": ["Wo", "nicht", ",", "gehs\u00b7tu", "gleich", "auff", ",", "ver\u00b7tirbt", "mir", "nicht", "zu", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "PPER", "PTKNEG", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df es ja gestehn, wer itzt nicht sp\u00e4t und fr\u00fch,", "tokens": ["Ich", "mu\u00df", "es", "ja", "ge\u00b7stehn", ",", "wer", "itzt", "nicht", "sp\u00e4t", "und", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,", "PWS", "ADV", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey seiner Lampen sitzt und Helden sucht zu singen,", "tokens": ["Bey", "sei\u00b7ner", "Lam\u00b7pen", "sitzt", "und", "Hel\u00b7den", "sucht", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach diesem fragt man nicht, wie seine Verse klingen,", "tokens": ["Nach", "die\u00b7sem", "fragt", "man", "nicht", ",", "wie", "sei\u00b7ne", "Ver\u00b7se", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch blobt man Seine blo\u00df, und lieset dennoch die.", "tokens": ["Doch", "blobt", "man", "Sei\u00b7ne", "blo\u00df", ",", "und", "lie\u00b7set", "den\u00b7noch", "die", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "ADV", "$,", "KON", "VVFIN", "ADV", "ART", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo wiltu mit dem Buch, o liebste Musa hin?", "tokens": ["Wo", "wil\u00b7tu", "mit", "dem", "Buch", ",", "o", "liebs\u00b7te", "Mu\u00b7sa", "hin", "?"], "token_info": ["wor