{"dta.poem.13746": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch was seh' ich! w\u00fcst, verfallen", "tokens": ["Doch", "was", "seh'", "ich", "!", "w\u00fcst", ",", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zimmer, Hof und Bogen sind,", "tokens": ["Zim\u00b7mer", ",", "Hof", "und", "Bo\u00b7gen", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam meine Tritte hallen,", "tokens": ["Ein\u00b7sam", "mei\u00b7ne", "Trit\u00b7te", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Fenster pfeift der Wind.", "tokens": ["Durch", "die", "Fens\u00b7ter", "pfeift", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Ahnenbilder lagen", "tokens": ["Al\u00b7le", "Ah\u00b7nen\u00b7bil\u00b7der", "la\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glanzlos in den Schutt verw\u00fchlt,", "tokens": ["Glanz\u00b7los", "in", "den", "Schutt", "ver\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Zitter drauf zerschlagen,", "tokens": ["Und", "die", "Zit\u00b7ter", "drauf", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der ich als Kind gespielt.", "tokens": ["Auf", "der", "ich", "als", "Kind", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}