{"dta.poem.100": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er kan ohne  Asterien  nicht leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonnen-Wende folgt der Sonnen Strahlen nach/", "tokens": ["Die", "Son\u00b7nen\u00b7Wen\u00b7de", "folgt", "der", "Son\u00b7nen", "Strah\u00b7len", "nach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Palm-Baum liebet sehr den Crystallinen-Bach/", "tokens": ["Der", "Pal\u00b7mBaum", "lie\u00b7bet", "sehr", "den", "Cry\u00b7stal\u00b7li\u00b7nen\u00b7Bach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Eisen-Stein beliebt der Nord zum Angel-Sterne/", "tokens": ["Dem", "Ei\u00b7sen\u00b7Stein", "be\u00b7liebt", "der", "Nord", "zum", "An\u00b7gel\u00b7S\u00b7ter\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Ulm und Reben sind einander selten ferne", "tokens": ["Der", "Ulm", "und", "Re\u00b7ben", "sind", "ein\u00b7an\u00b7der", "sel\u00b7ten", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "PRF", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Jmgleichen ist mein Geist auch recht besonders froh/", "tokens": ["Jm\u00b7glei\u00b7chen", "ist", "mein", "Geist", "auch", "recht", "be\u00b7son\u00b7ders", "froh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn er sein Seelen-Licht kan in der N\u00e4he schauen/", "tokens": ["Wenn", "er", "sein", "See\u00b7len\u00b7Licht", "kan", "in", "der", "N\u00e4\u00b7he", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ihren Augen-Schein darf als der Sonnen trauen/", "tokens": ["Und", "ih\u00b7ren", "Au\u00b7gen\u00b7Schein", "darf", "als", "der", "Son\u00b7nen", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn ihm der s\u00fcsse Mund die Anmuths Quellen reicht/", "tokens": ["Wenn", "ihm", "der", "s\u00fcs\u00b7se", "Mund", "die", "An\u00b7muths", "Quel\u00b7len", "reicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und die erh\u00f6hte Brust sich als den Nord-Pol zeigt;", "tokens": ["Und", "die", "er\u00b7h\u00f6h\u00b7te", "Brust", "sich", "als", "den", "Nord\u00b7Pol", "zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PRF", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn der ", "tokens": ["Wenn", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Jhm einen Unterhalt und Maure ab-will geben:", "tokens": ["Jhm", "ei\u00b7nen", "Un\u00b7ter\u00b7halt", "und", "Mau\u00b7re", "ab\u00b7will", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn ihn der Glieder-Agt wie Stroh wird an sich ziehn/", "tokens": ["Wenn", "ihn", "der", "Glie\u00b7der\u00b7Agt", "wie", "Stroh", "wird", "an", "sich", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "NN", "VAFIN", "APPR", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und ihr Magnet-Stein nicht sein Eisen denckt zu fliehn.", "tokens": ["Und", "ihr", "Ma\u00b7gnet\u00b7Stein", "nicht", "sein", "Ei\u00b7sen", "denckt", "zu", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Wann aber ihr Gesicht ein schwartzer Zorn verd\u00fcstert/", "tokens": ["Wann", "a\u00b7ber", "ihr", "Ge\u00b7sicht", "ein", "schwart\u00b7zer", "Zorn", "ver\u00b7d\u00fcs\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So sind die Sonnen-Blum\u2019n und er genau verschwistert/", "tokens": ["So", "sind", "die", "Son\u00b7nen\u00b7Blum'n", "und", "er", "ge\u00b7nau", "ver\u00b7schwis\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn ihrer Lippen-Bach ihm auch entzogen wird/", "tokens": ["Wenn", "ih\u00b7rer", "Lip\u00b7pen\u00b7Bach", "ihm", "auch", "ent\u00b7zo\u00b7gen", "wird", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So wird dem Palm und ihm des Lebens-Safft entf\u00fchrt.", "tokens": ["So", "wird", "dem", "Palm", "und", "ihm", "des", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.19": {"text": "Wenn nun der ", "tokens": ["Wenn", "nun", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Alsdenn sein Liebs-Magnet sich mit Betr\u00fcbni\u00df decket.", "tokens": ["Als\u00b7denn", "sein", "Liebs\u00b7Ma\u00b7gnet", "sich", "mit", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn er den zarten Leib nicht kan zum Ulmen sehn/", "tokens": ["Wenn", "er", "den", "zar\u00b7ten", "Leib", "nicht", "kan", "zum", "Ul\u00b7men", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VMFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So mu\u00df er wie der Ranck verlassen untergehn.", "tokens": ["So", "mu\u00df", "er", "wie", "der", "Ranck", "ver\u00b7las\u00b7sen", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "KOKOM", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn ihm die Maure wird der zarten Brust entrissen/", "tokens": ["Wenn", "ihm", "die", "Mau\u00b7re", "wird", "der", "zar\u00b7ten", "Brust", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So mu\u00df das Winter-Gr\u00fcn/ und er die Erde k\u00fcssen;", "tokens": ["So", "mu\u00df", "das", "Win\u00b7ter\u00b7Gr\u00fcn", "/", "und", "er", "die", "Er\u00b7de", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$(", "KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wenn ihn der Glieder-Agt wie Stroh nicht zu sich rei\u00dft/", "tokens": ["Wenn", "ihn", "der", "Glie\u00b7der\u00b7Agt", "wie", "Stroh", "nicht", "zu", "sich", "rei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "NN", "PTKNEG", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ihr Magnet als Feind sein Eisen von sich schmeist ", "tokens": ["Und", "ihr", "Mag\u00b7net", "als", "Feind", "sein", "Ei\u00b7sen", "von", "sich", "schmeist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KOUS", "NN", "PPOSAT", "NN", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Alsdann so mu\u00df ich mich dem Sterben \u00fcbergeben/", "tokens": ["Als\u00b7dann", "so", "mu\u00df", "ich", "mich", "dem", "Ster\u00b7ben", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Weil ohn\u2019 ", "tokens": ["Weil", "ohn'"], "token_info": ["word", "word"], "pos": ["KOUS", "NE"], "meter": "+-", "measure": "trochaic.single"}}}}}