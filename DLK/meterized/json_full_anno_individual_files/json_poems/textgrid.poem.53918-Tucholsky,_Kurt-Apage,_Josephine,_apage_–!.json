{"textgrid.poem.53918": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Apage, Josephine, apage \u2013!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Wien zuckte neulich die Baker mit ihrem Popo,", "tokens": ["In", "Wi\u00b7en", "zuck\u00b7te", "neu\u00b7lich", "die", "Ba\u00b7ker", "mit", "ih\u00b7rem", "Po\u00b7po", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "denn es zieren die Kugeln ihrer Br\u00fcste manch sch\u00f6nes Revue-Tableau.", "tokens": ["denn", "es", "zie\u00b7ren", "die", "Ku\u00b7geln", "ih\u00b7rer", "Br\u00fcs\u00b7te", "manch", "sch\u00f6\u00b7nes", "Re\u00b7vue\u00b7Ta\u00b7bleau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Auch tanzt sie bald auf dem rechten, bald auf dem linken Bein \u2013", "tokens": ["Auch", "tanzt", "sie", "bald", "auf", "dem", "rech\u00b7ten", ",", "bald", "auf", "dem", "lin\u00b7ken", "Bein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und schielen kann sie, da\u00df das Wei\u00dfe nur so ergl\u00e4nzt in ihren \u00c4ugelein.", "tokens": ["und", "schie\u00b7len", "kann", "sie", ",", "da\u00df", "das", "Wei\u00b7\u00dfe", "nur", "so", "er\u00b7gl\u00e4nzt", "in", "ih\u00b7ren", "\u00c4u\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Dies haben die Zentrums-Schwarzen, die jungen und die alten,", "tokens": ["Dies", "ha\u00b7ben", "die", "Zen\u00b7trums\u00b7Schwa\u00b7rzen", ",", "die", "jun\u00b7gen", "und", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+--++-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "leider f\u00fcr eine Anspielung auf ihre Kirche gehalten.", "tokens": ["lei\u00b7der", "f\u00fcr", "ei\u00b7ne", "An\u00b7spie\u00b7lung", "auf", "ih\u00b7re", "Kir\u00b7che", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Auch f\u00fchlten sie sich bedroht in ihrer Sittlichkeit,", "tokens": ["Auch", "f\u00fchl\u00b7ten", "sie", "sich", "be\u00b7droht", "in", "ih\u00b7rer", "Sitt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "und sie lie\u00dfen die Glocken l\u00e4uten, ganz wie in schwerer Zeit.", "tokens": ["und", "sie", "lie\u00b7\u00dfen", "die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ",", "ganz", "wie", "in", "schwe\u00b7rer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Drei S\u00fchnegottesdienste stiegen auf zum \u00f6sterreichischen Himmel,", "tokens": ["Drei", "S\u00fch\u00b7ne\u00b7got\u00b7tes\u00b7diens\u00b7te", "stie\u00b7gen", "auf", "zum", "\u00f6s\u00b7ter\u00b7rei\u00b7chi\u00b7schen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "und die Bu\u00dfglocke gefiel sich in einem moralischen Gebimmel.", "tokens": ["und", "die", "Bu\u00df\u00b7glo\u00b7cke", "ge\u00b7fiel", "sich", "in", "ei\u00b7nem", "mo\u00b7ra\u00b7li\u00b7schen", "Ge\u00b7bim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+--+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn eine T\u00e4nzerin gut gewachsen ist", "tokens": ["Wenn", "ei\u00b7ne", "T\u00e4n\u00b7ze\u00b7rin", "gut", "ge\u00b7wach\u00b7sen", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und einen Venus-K\u00f6rper hat, der nicht aus Sachsen ist;", "tokens": ["und", "ei\u00b7nen", "Ve\u00b7nus\u00b7K\u00f6r\u00b7per", "hat", ",", "der", "nicht", "aus", "Sach\u00b7sen", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "APPR", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "und wenn sie tanzt, da\u00df nur der Rhythmus so knackt,", "tokens": ["und", "wenn", "sie", "tanzt", ",", "da\u00df", "nur", "der", "Rhyth\u00b7mus", "so", "knackt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "und wenn sie ein ganzes Theater bei allen Sinnen packt;", "tokens": ["und", "wenn", "sie", "ein", "gan\u00b7zes", "The\u00b7a\u00b7ter", "bei", "al\u00b7len", "Sin\u00b7nen", "packt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "und wenn das Leben bunt ist hierzulande \u2013:", "tokens": ["und", "wenn", "das", "Le\u00b7ben", "bunt", "ist", "hier\u00b7zu\u00b7lan\u00b7de", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "VAFIN", "ADV", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "an seinem Kreuz sehen mu\u00df, wie sich die Felder blutig r\u00f6ten;", "tokens": ["an", "sei\u00b7nem", "Kreuz", "se\u00b7hen", "mu\u00df", ",", "wie", "sich", "die", "Fel\u00b7der", "blu\u00b7tig", "r\u00f6\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "PWAV", "PRF", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "wenn die Pfaffen Kanonen und Flugzeuge segnen", "tokens": ["wenn", "die", "Pfaf\u00b7fen", "Ka\u00b7no\u00b7nen", "und", "Flug\u00b7zeu\u00b7ge", "seg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "KON", "NN", "VVINF"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "und in den Feldgottesdiensten beten, da\u00df es Blut m\u00f6ge regnen;", "tokens": ["und", "in", "den", "Feld\u00b7got\u00b7tes\u00b7diens\u00b7ten", "be\u00b7ten", ",", "da\u00df", "es", "Blut", "m\u00f6\u00b7ge", "reg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,", "KOUS", "PPER", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "und wenn die Vertreter Gottes auf Erden", "tokens": ["und", "wenn", "die", "Ver\u00b7tre\u00b7ter", "Got\u00b7tes", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Soldaten-H\u00e4mmel treiben, auf da\u00df sie geschlachtet werden;", "tokens": ["Sol\u00b7da\u00b7ten\u00b7H\u00e4m\u00b7mel", "trei\u00b7ben", ",", "auf", "da\u00df", "sie", "ge\u00b7schlach\u00b7tet", "wer\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPR", "KOUS", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "und wenn die Glocken l\u00e4uten: \u00bbMord!\u00ab und die Chor\u00e4le hallen:", "tokens": ["und", "wenn", "die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ":", "\u00bb", "Mord", "!", "\u00ab", "und", "die", "Cho\u00b7r\u00e4\u00b7le", "hal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$.", "$(", "NN", "$.", "$(", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "\u00bbmord! Ihr sollt eure Feinde niederknallen!\u00ab", "tokens": ["\u00bb", "mord", "!", "Ihr", "sollt", "eu\u00b7re", "Fein\u00b7de", "nie\u00b7der\u00b7knal\u00b7len", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn jemand so verr\u00e4t den Gottessohn \u2013:", "tokens": ["Und", "wenn", "je\u00b7mand", "so", "ver\u00b7r\u00e4t", "den", "Got\u00b7tes\u00b7sohn", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "ART", "NN", "$(", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Das ist Religion.", "tokens": ["Das", "ist", "Re\u00b7li\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "In Wien zuckte neulich die Baker mit ihrem Popo,", "tokens": ["In", "Wi\u00b7en", "zuck\u00b7te", "neu\u00b7lich", "die", "Ba\u00b7ker", "mit", "ih\u00b7rem", "Po\u00b7po", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "denn es zieren die Kugeln ihrer Br\u00fcste manch sch