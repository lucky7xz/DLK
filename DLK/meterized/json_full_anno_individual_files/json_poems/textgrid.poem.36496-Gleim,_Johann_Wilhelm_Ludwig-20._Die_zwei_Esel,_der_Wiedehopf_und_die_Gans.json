{"textgrid.poem.36496": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "20. Die zwei Esel, der Wiedehopf und die Gans", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Esel schrien um die Wette;", "tokens": ["Zwei", "E\u00b7sel", "schri\u00b7en", "um", "die", "Wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wette war ein Distelkopf.", "tokens": ["Die", "Wet\u00b7te", "war", "ein", "Dis\u00b7tel\u00b7kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn einer auch gewonnen h\u00e4tte,", "tokens": ["Wenn", "ei\u00b7ner", "auch", "ge\u00b7won\u00b7nen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was w\u00e4r's denn? sprach ein Wiedehopf;", "tokens": ["Was", "w\u00e4r's", "denn", "?", "sprach", "ein", "Wie\u00b7de\u00b7hopf", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn's um die Ehre", "tokens": ["Wenn's", "um", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Sch\u00f6nheit w\u00e4re,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Dann lie\u00df ich eine Wette gelten! \u2013", "tokens": ["Dann", "lie\u00df", "ich", "ei\u00b7ne", "Wet\u00b7te", "gel\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was da der sch\u00f6ne Dummkopf quackelt,", "tokens": ["Was", "da", "der", "sch\u00f6\u00b7ne", "Dumm\u00b7kopf", "qua\u00b7ckelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruft eine Gans von fern, und wackelt", "tokens": ["Ruft", "ei\u00b7ne", "Gans", "von", "fern", ",", "und", "wa\u00b7ckelt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Esel und vor Wiedehopf vorbei.", "tokens": ["Vor", "E\u00b7sel", "und", "vor", "Wie\u00b7de\u00b7hopf", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Esel wiederholten ihr Geschrei,", "tokens": ["Die", "E\u00b7sel", "wie\u00b7der\u00b7hol\u00b7ten", "ihr", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Gans kehrt um und schnattert: O du wettest,", "tokens": ["Die", "Gans", "kehrt", "um", "und", "schnat\u00b7tert", ":", "O", "