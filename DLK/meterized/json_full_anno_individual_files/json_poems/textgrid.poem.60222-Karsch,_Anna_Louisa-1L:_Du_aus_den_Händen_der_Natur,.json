, "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "NE", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die rauhen Winde st\u00fcrmten um sie her.", "tokens": ["Die", "rau\u00b7hen", "Win\u00b7de", "st\u00fcrm\u00b7ten", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wie ist die Reben-Wand von ihrem Schmuck so leer!", "tokens": ["Wie", "ist", "die", "Re\u00b7ben\u00b7Wand", "von", "ih\u00b7rem", "Schmuck", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nichts gr\u00fcnet mehr in dem beliebten Raume,", "tokens": ["Nichts", "gr\u00fc\u00b7net", "mehr", "in", "dem", "be\u00b7lieb\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wo du Lustwandeln giengst, wo Blumen sich geb\u00fcckt,", "tokens": ["Wo", "du", "Lust\u00b7wan\u00b7deln", "giengst", ",", "wo", "Blu\u00b7men", "sich", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,", "PWAV", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Vor deines weissen Kleides Saume,", "tokens": ["Vor", "dei\u00b7nes", "weis\u00b7sen", "Klei\u00b7des", "Sau\u00b7me", ","]