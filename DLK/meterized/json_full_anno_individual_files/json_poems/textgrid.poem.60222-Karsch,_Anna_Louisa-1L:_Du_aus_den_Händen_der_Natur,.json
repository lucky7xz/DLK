{"textgrid.poem.60222": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du aus den H\u00e4nden der Natur,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du aus den H\u00e4nden der Natur,", "tokens": ["Du", "aus", "den", "H\u00e4n\u00b7den", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihrem Ruhm hervorgegangene Sch\u00f6ne!", "tokens": ["Zu", "ih\u00b7rem", "Ruhm", "her\u00b7vor\u00b7ge\u00b7gan\u00b7ge\u00b7ne", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jetzt singet, auf der arm gewordnen Flur,", "tokens": ["Jetzt", "sin\u00b7get", ",", "auf", "der", "arm", "ge\u00b7word\u00b7nen", "Flur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht mehr die Lerche. Jetzt verlernt die Th\u00f6ne", "tokens": ["Nicht", "mehr", "die", "Ler\u00b7che", ".", "Jetzt", "ver\u00b7lernt", "die", "Th\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Selbst deiner Schwester Nachtigall. Sie schweigt", "tokens": ["Selbst", "dei\u00b7ner", "Schwes\u00b7ter", "Nach\u00b7ti\u00b7gall", ".", "Sie", "schweigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In ihrem melancholischen Geh\u00e4use;", "tokens": ["In", "ih\u00b7rem", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Ge\u00b7h\u00e4u\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Tief denkend sitzt sie da \u2013 so sitzet oft der Weise,", "tokens": ["Tief", "den\u00b7kend", "sitzt", "sie", "da", "\u2013", "so", "sit\u00b7zet", "oft", "der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "ADV", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Der Menschenfreund, wenn fremde Noth ihn beugt,", "tokens": ["Der", "Men\u00b7schen\u00b7freund", ",", "wenn", "frem\u00b7de", "Noth", "ihn", "beugt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn dr\u00fcckend Elend kommt mit jung gewordnen Tagen,", "tokens": ["Wenn", "dr\u00fc\u00b7ckend", "E\u00b7lend", "kommt", "mit", "jung", "ge\u00b7word\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn durch das Vaterland die lautgest\u00f6hnten Klagen", "tokens": ["Wenn", "durch", "das", "Va\u00b7ter\u00b7land", "die", "laut\u00b7ge\u00b7st\u00f6hn\u00b7ten", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erschallen allgemein: Dann sitzet traurig er,", "tokens": ["Er\u00b7schal\u00b7len", "all\u00b7ge\u00b7mein", ":", "Dann", "sit\u00b7zet", "trau\u00b7rig", "er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VVFIN", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verstummt von Schmerz, und blickt umher,", "tokens": ["Ver\u00b7stummt", "von", "Schmerz", ",", "und", "blickt", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ob aufgekl\u00e4rtre Tage kommen \u2013", "tokens": ["Ob", "auf\u00b7ge\u00b7kl\u00e4rt\u00b7re", "Ta\u00b7ge", "kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Du holdes M\u00e4dchen, von zwey Frommen,", "tokens": ["Du", "hol\u00b7des", "M\u00e4d\u00b7chen", ",", "von", "zwey", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Im Lande Friedrichs auf die Welt gebracht;", "tokens": ["Im", "Lan\u00b7de", "Fried\u00b7richs", "auf", "die", "Welt", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Unmuthig siehest du den B\u00e4umen ihre Pracht,", "tokens": ["Un\u00b7mut\u00b7hig", "sie\u00b7hest", "du", "den", "B\u00e4u\u00b7men", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Den Blumen ihren Reiz benommen.", "tokens": ["Den", "Blu\u00b7men", "ih\u00b7ren", "Reiz", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Maulbeerbaum \u2013 er stehet bl\u00e4tterlos;", "tokens": ["Der", "Maul\u00b7be\u00b7er\u00b7baum", "\u2013", "er", "ste\u00b7het", "bl\u00e4t\u00b7ter\u00b7los", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Wie liegen unter ihm, die stolz getragne Locken", "tokens": ["Wie", "lie\u00b7gen", "un\u00b7ter", "ihm", ",", "die", "stolz", "ge\u00b7trag\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "$,", "PRELS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zerstreut, auf schwarzer Erde Schoo\u00df,", "tokens": ["Zer\u00b7streut", ",", "auf", "schwar\u00b7zer", "Er\u00b7de", "Schoo\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Den blassen Leichen gleich! O! ihre Sterbeglocken,", "tokens": ["Den", "blas\u00b7sen", "Lei\u00b7chen", "gleich", "!", "O", "!", "ih\u00b7re", "Ster\u00b7be\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "NE", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die rauhen Winde st\u00fcrmten um sie her.", "tokens": ["Die", "rau\u00b7hen", "Win\u00b7de", "st\u00fcrm\u00b7ten", "um", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wie ist die Reben-Wand von ihrem Schmuck so leer!", "tokens": ["Wie", "ist", "die", "Re\u00b7ben\u00b7Wand", "von", "ih\u00b7rem", "Schmuck", "so", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nichts gr\u00fcnet mehr in dem beliebten Raume,", "tokens": ["Nichts", "gr\u00fc\u00b7net", "mehr", "in", "dem", "be\u00b7lieb\u00b7ten", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wo du Lustwandeln giengst, wo Blumen sich geb\u00fcckt,", "tokens": ["Wo", "du", "Lust\u00b7wan\u00b7deln", "giengst", ",", "wo", "Blu\u00b7men", "sich", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,", "PWAV", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Vor deines weissen Kleides Saume,", "tokens": ["Vor", "dei\u00b7nes", "weis\u00b7sen", "Klei\u00b7des", "Sau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wann sie dein Angesicht erblickt.", "tokens": ["Wann", "sie", "dein", "An\u00b7ge\u00b7sicht", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So nimmt die Zeit, einst G\u00fcter der Natur", "tokens": ["So", "nimmt", "die", "Zeit", ",", "einst", "G\u00fc\u00b7ter", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir sch\u00f6nes Kind! Dein Herbst, dein Winter werden kommen", "tokens": ["Dir", "sch\u00f6\u00b7nes", "Kind", "!", "Dein", "Herbst", ",", "dein", "Win\u00b7ter", "wer\u00b7den", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit r\u00e4uberischer Hand. Dann wird, wie von der Flur,", "tokens": ["Mit", "r\u00e4u\u00b7be\u00b7ri\u00b7scher", "Hand", ".", "Dann", "wird", ",", "wie", "von", "der", "Flur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Reiz von dieser Wange weggenommen.", "tokens": ["Der", "Reiz", "von", "die\u00b7ser", "Wan\u00b7ge", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie lassen dir des Herzens Sch\u00f6nheit nur!", "tokens": ["Sie", "las\u00b7sen", "dir", "des", "Her\u00b7zens", "Sch\u00f6n\u00b7heit", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur den Verstand heraufgereift, nur Z\u00fcge", "tokens": ["Nur", "den", "Ver\u00b7stand", "her\u00b7auf\u00b7ge\u00b7reift", ",", "nur", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Seele, die mit Tugend ausgeschm\u00fcckt", "tokens": ["Der", "See\u00b7le", ",", "die", "mit", "Tu\u00b7gend", "aus\u00b7ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nicht von der Zeit, vom Zufall nicht erdr\u00fcckt,", "tokens": ["Nicht", "von", "der", "Zeit", ",", "vom", "Zu\u00b7fall", "nicht", "er\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Bezeuget, da\u00df in ihr der Gottheit Funke liege!", "tokens": ["Be\u00b7zeu\u00b7get", ",", "da\u00df", "in", "ihr", "der", "Got\u00b7theit", "Fun\u00b7ke", "lie\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wann achtzehn Erndten noch vor\u00fcber gehn,", "tokens": ["Wann", "acht\u00b7zehn", "Ernd\u00b7ten", "noch", "vor\u00b7\u00fc\u00b7ber", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und Krankheit nicht in Dir Verw\u00fcstung angerichtet;", "tokens": ["Und", "Krank\u00b7heit", "nicht", "in", "Dir", "Ver\u00b7w\u00fcs\u00b7tung", "an\u00b7ge\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "APPR", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dann ist vielleicht noch dieses Antlitz sch\u00f6n,", "tokens": ["Dann", "ist", "viel\u00b7leicht", "noch", "die\u00b7ses", "Ant\u00b7litz", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Das alle Kunst der Mahlerey zernichtet.", "tokens": ["Das", "al\u00b7le", "Kunst", "der", "Mah\u00b7le\u00b7rey", "zer\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wann aber funfzig Sommer du gelebt;", "tokens": ["Wann", "a\u00b7ber", "funf\u00b7zig", "Som\u00b7mer", "du", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "CARD", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Alsdann haucht alle Reize von den Wangen", "tokens": ["Als\u00b7dann", "haucht", "al\u00b7le", "Rei\u00b7ze", "von", "den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Die starke Zeit, vor der die G\u00e4rten sind vergangen,", "tokens": ["Die", "star\u00b7ke", "Zeit", ",", "vor", "der", "die", "G\u00e4r\u00b7ten", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die pr\u00e4chtig in der Luft geschwebt.", "tokens": ["Die", "pr\u00e4ch\u00b7tig", "in", "der", "Luft", "ge\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dein \u00e4u\u00dfrer Bau, so k\u00fcnstlich er gewebt,", "tokens": ["Dein", "\u00e4u\u00df\u00b7rer", "Bau", ",", "so", "k\u00fcnst\u00b7lich", "er", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So fein die Nerven auch sind \u00fcberzogen worden,", "tokens": ["So", "fein", "die", "Ner\u00b7ven", "auch", "sind", "\u00fc\u00b7berz\u00b7o\u00b7gen", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "VAFIN", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist nichtig, mu\u00df vergehn; wie Bl\u00fcten im April,", "tokens": ["Ist", "nich\u00b7tig", ",", "mu\u00df", "ver\u00b7gehn", ";", "wie", "Bl\u00fc\u00b7ten", "im", "Ap\u00b7ril", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VMFIN", "VVINF", "$.", "PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn n\u00e4chtlich sie ein Frost kommt in der Knospe morden,", "tokens": ["Wenn", "n\u00e4cht\u00b7lich", "sie", "ein", "Frost", "kommt", "in", "der", "Knos\u00b7pe", "mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wenn ins Leben sie die Sonne wecken will,", "tokens": ["Und", "wenn", "ins", "Le\u00b7ben", "sie", "die", "Son\u00b7ne", "we\u00b7cken", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "PPER", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Noch ungestalt und welk an Zweige kleben \u2013", "tokens": ["Noch", "un\u00b7ge\u00b7stalt", "und", "welk", "an", "Zwei\u00b7ge", "kle\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Dir aber sollen noch die Jahre Reizung geben.", "tokens": ["Dir", "a\u00b7ber", "sol\u00b7len", "noch", "die", "Jah\u00b7re", "Rei\u00b7zung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punc