{"dta.poem.19038": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  Eigene Hochzeit-Gedancken.   Am 7. Septemb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Cron und Lohn behertzter Ringer,", "tokens": ["Cron", "und", "Lohn", "be\u00b7hertz\u00b7ter", "Rin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Seeligkeit Herwiederbringer,", "tokens": ["Der", "See\u00b7lig\u00b7keit", "Her\u00b7wie\u00b7der\u00b7brin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "HeRR JESU, HERR der Herrlichkeit,", "tokens": ["HeRR", "Je\u00b7SU", ",", "HeRR", "der", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schau vor deines Thrones Stuffen,", "tokens": ["Schau", "vor", "dei\u00b7nes", "Thro\u00b7nes", "Stuf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwey Seelen, welche zu dir ruffen,", "tokens": ["Zwey", "See\u00b7len", ",", "wel\u00b7che", "zu", "dir", "ruf\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie w\u00e4ren gerne benedeyt.", "tokens": ["Sie", "w\u00e4\u00b7ren", "ger\u00b7ne", "be\u00b7ne\u00b7deyt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du seegnest ja so gern, Geseegneter des HErrn,", "tokens": ["Du", "seeg\u00b7nest", "ja", "so", "gern", ",", "Ge\u00b7seeg\u00b7ne\u00b7ter", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir begehrens,", "tokens": ["Wir", "be\u00b7geh\u00b7rens", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "So komm herein, wir sind ja dein,", "tokens": ["So", "komm", "her\u00b7ein", ",", "wir", "sind", "ja", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und la\u00df uns recht geseegnet seyn!", "tokens": ["Und", "la\u00df", "uns", "recht", "ge\u00b7seeg\u00b7net", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Brunnquell aller Seeligkeiten,", "tokens": ["Brunn\u00b7quell", "al\u00b7ler", "See\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! fahre fort uns zu bereiten", "tokens": ["Ach", "!", "fah\u00b7re", "fort", "uns", "zu", "be\u00b7rei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PTKVZ", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So, wie es dir gef\u00e4llig ist:", "tokens": ["So", ",", "wie", "es", "dir", "ge\u00b7f\u00e4l\u00b7lig", "ist", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir als von Natur verdorben,", "tokens": ["Wir", "als", "von", "Na\u00b7tur", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir sind dem Leben abgestorben,", "tokens": ["Wir", "sind", "dem", "Le\u00b7ben", "ab\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Darinnen du zu finden bist.", "tokens": ["Da\u00b7rin\u00b7nen", "du", "zu", "fin\u00b7den", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So t\u00f6dte doch den Feind, der uns zu st\u00fcrtzen meynt,", "tokens": ["So", "t\u00f6d\u00b7te", "doch", "den", "Feind", ",", "der", "uns", "zu", "st\u00fcrt\u00b7zen", "meynt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vollbringe nun, uns abzuthun,", "tokens": ["Voll\u00b7brin\u00b7ge", "nun", ",", "uns", "ab\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df wir in deinem Tode ruhn.", "tokens": ["Da\u00df", "wir", "in", "dei\u00b7nem", "To\u00b7de", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Seelig sind die Geistlich Armen,", "tokens": ["See\u00b7lig", "sind", "die", "Geist\u00b7lich", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie finden leichtlich dein Erbarmen,", "tokens": ["Sie", "fin\u00b7den", "leicht\u00b7lich", "dein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da im Gegentheil die Reichen,", "tokens": ["Da", "im", "Ge\u00b7gen\u00b7theil", "die", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und die gar satt sind, ferne weichen", "tokens": ["Und", "die", "gar", "satt", "sind", ",", "fer\u00b7ne", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJD", "VAFIN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von deines K\u00f6nigreiches Zier.", "tokens": ["Von", "dei\u00b7nes", "K\u00f6\u00b7nig\u00b7rei\u00b7ches", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach! mach uns Arme reich, doch deiner Armuth gleich.", "tokens": ["Ach", "!", "mach", "uns", "Ar\u00b7me", "reich", ",", "doch", "dei\u00b7ner", "Ar\u00b7muth", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "NN", "ADJD", "$,", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gib uns, JESU,", "tokens": ["Gib", "uns", ",", "Je\u00b7SU", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Den reichen Muth! dem irrdisch Guth", "tokens": ["Den", "rei\u00b7chen", "Muth", "!", "dem", "irr\u00b7disch", "Guth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Recht weh, und Armuth sanffte thut!", "tokens": ["Recht", "weh", ",", "und", "Ar\u00b7muth", "sanff\u00b7te", "thut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "KON", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seelig sind, die leide tragen,", "tokens": ["See\u00b7lig", "sind", ",", "die", "lei\u00b7de", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie sollen Trosts genug erjagen;", "tokens": ["Sie", "sol\u00b7len", "Trosts", "ge\u00b7nug", "er\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Hertzog gieng den Weg voran:", "tokens": ["Ihr", "Hert\u00b7zog", "gieng", "den", "Weg", "vo\u00b7ran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stieg Er auf durch Creutz und Leiden:", "tokens": ["Stieg", "Er", "auf", "durch", "Creutz", "und", "Lei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So will Er uns den Kelch bescheiden,", "tokens": ["So", "will", "Er", "uns", "den", "Kelch", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Jhm hienieden gut gethan,", "tokens": ["Der", "Jhm", "hien\u00b7ie\u00b7den", "gut", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns ist in dieser Zeit kein Feyertag bereit;", "tokens": ["Uns", "ist", "in", "die\u00b7ser", "Zeit", "kein", "Fe\u00b7yer\u00b7tag", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier gilts Weinen:", "tokens": ["Hier", "gilts", "Wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Beym Lammes-Mahl ist keine Quaal;", "tokens": ["Beym", "Lam\u00b7mes\u00b7Mahl", "ist", "kei\u00b7ne", "Qua\u00b7al", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wir aber gehn durchs Jammerthal.", "tokens": ["Wir", "a\u00b7ber", "gehn", "durchs", "Jam\u00b7mer\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seelig sind die sanfften Geister,", "tokens": ["See\u00b7lig", "sind", "die", "sanff\u00b7ten", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie sind auf Erden Herrn und Meister;", "tokens": ["Sie", "sind", "auf", "Er\u00b7den", "Herrn", "und", "Meis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und niemand sieht es ihnen an;", "tokens": ["Und", "nie\u00b7mand", "sieht", "es", "ih\u00b7nen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sie doch durch stillen Wandel", "tokens": ["Da", "sie", "doch", "durch", "stil\u00b7len", "Wan\u00b7del"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In allerley Gesch\u00e4fft und Handel", "tokens": ["In", "al\u00b7ler\u00b7ley", "Ge\u00b7sch\u00e4fft", "und", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Lammes-Wesen dargethan.", "tokens": ["Ihr", "Lam\u00b7mes\u00b7We\u00b7sen", "dar\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist ihr Br\u00e4utigam, das erstgebohrne Lamm,", "tokens": ["Es", "ist", "ihr", "Br\u00e4u\u00b7ti\u00b7gam", ",", "das", "erst\u00b7ge\u00b7bohr\u00b7ne", "Lamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Lamm und L\u00f6we,", "tokens": ["Lamm", "und", "L\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Gar sanfft und weich, doch starck zugleich;", "tokens": ["Gar", "sanfft", "und", "weich", ",", "doch", "starck", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "$,", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So sind auch die aus seinem Reich.", "tokens": ["So", "sind", "auch", "die", "aus", "sei\u00b7nem", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn der Feinde stoltze Rotten", "tokens": ["Wenn", "der", "Fein\u00b7de", "stolt\u00b7ze", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der armen Einfalt JESU spotten,", "tokens": ["Der", "ar\u00b7men", "Ein\u00b7falt", "Je\u00b7SU", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und seiner sanfften L\u00e4mmerlein;", "tokens": ["Und", "sei\u00b7ner", "sanff\u00b7ten", "L\u00e4m\u00b7mer\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unverr\u00fcckt am C\u00f6rper hangen,)", "tokens": ["Und", "un\u00b7ver\u00b7r\u00fcckt", "am", "C\u00f6r\u00b7per", "han\u00b7gen", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So m\u00fcssen sie offt schrecklich seyn.", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "offt", "schreck\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint einem Goliath der kleine David matt,", "tokens": ["Scheint", "ei\u00b7nem", "Go\u00b7li\u00b7ath", "der", "klei\u00b7ne", "Da\u00b7vid", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will er h\u00f6hnen;", "tokens": ["Will", "er", "h\u00f6h\u00b7nen", ";"], "token_info": 