{"dta.poem.23856": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Bereitung zum Tode.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Morgen ist vorbey/ die Kindheit meiner Tage/", "tokens": ["Mein", "Mor\u00b7gen", "ist", "vor\u00b7bey", "/", "die", "Kind\u00b7heit", "mei\u00b7ner", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ich den hingebracht/ das wei\u00df ich selber nicht;", "tokens": ["Wie", "ich", "den", "hin\u00b7ge\u00b7bracht", "/", "das", "wei\u00df", "ich", "sel\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$(", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Mittag ist vorbey/ der ohngefehr die Wage", "tokens": ["Mein", "Mit\u00b7tag", "ist", "vor\u00b7bey", "/", "der", "ohn\u00b7ge\u00b7fehr", "die", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ART", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des kurtzen Lebens hielt. HErr geh nicht ins Gericht!", "tokens": ["Des", "kurt\u00b7zen", "Le\u00b7bens", "hielt", ".", "Herr", "geh", "nicht", "ins", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Ich kenne dein Gesetz/ und kenne meine Schuld!", "tokens": ["Ich", "ken\u00b7ne", "dein", "Ge\u00b7setz", "/", "und", "ken\u00b7ne", "mei\u00b7ne", "Schuld", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Abend kommt heran/ itzt solten Thr\u00e4nen rinnen;", "tokens": ["Mein", "A\u00b7bend", "kommt", "he\u00b7ran", "/", "itzt", "sol\u00b7ten", "Thr\u00e4\u00b7nen", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$(", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch nim\u0303t mein b\u00f6ser Trieb/ mein s\u00fcndliches Beginnen/", "tokens": ["Doch", "nim\u0303t", "mein", "b\u00f6\u00b7ser", "Trieb", "/", "mein", "s\u00fcnd\u00b7li\u00b7ches", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit jedem Alter zu. Ach trage noch Gedult!", "tokens": ["Mit", "je\u00b7dem", "Al\u00b7ter", "zu", ".", "Ach", "tra\u00b7ge", "noch", "Ge\u00b7dult", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "ITJ", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df mich nicht auf die letzt/ in solche Nacht verfallen/", "tokens": ["La\u00df", "mich", "nicht", "auf", "die", "letzt", "/", "in", "sol\u00b7che", "Nacht", "ver\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "ART", "ADV", "$(", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die mich auf ewiglich von deinen Augen st\u00f6\u00dft!", "tokens": ["Die", "mich", "auf", "e\u00b7wig\u00b7lich", "von", "dei\u00b7nen", "Au\u00b7gen", "st\u00f6\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein/ sondern la\u00df dein Hertz f\u00fcr einen S\u00fcnder wallen/", "tokens": ["Nein", "/", "son\u00b7dern", "la\u00df", "dein", "Hertz", "f\u00fcr", "ei\u00b7nen", "S\u00fcn\u00b7der", "wal\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KON", "VVIMP", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den so ein theures Blut/ als wie dein Sohn erl\u00f6\u00dft", "tokens": ["Den", "so", "ein", "theu\u00b7res", "Blut", "/", "als", "wie", "dein", "Sohn", "er\u00b7l\u00f6\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$(", "KOUS", "KOKOM", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mir h\u00e4ngt/ ich wei\u00df es wol/ zu grosse Schwachheit an;", "tokens": ["Mir", "h\u00e4ngt", "/", "ich", "wei\u00df", "es", "wol", "/", "zu", "gros\u00b7se", "Schwach\u00b7heit", "an", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Heut schreib\u2019 ich etwas guts; doch dir ist unverborgen/", "tokens": ["Heut", "schreib'", "ich", "et\u00b7was", "guts", ";", "doch", "dir", "ist", "un\u00b7ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "ADV", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "- - - - ob zwischen heut und morgen/", "tokens": ["ob", "zwi\u00b7schen", "heut", "und", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "$(", "KOUS", "APPR", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Satan meinen Wunsch nicht anders lencken kan.", "tokens": ["Der", "Sa\u00b7tan", "mei\u00b7nen", "Wunsch", "nicht", "an\u00b7ders", "len\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "- - - - - - - - -", "tokens": [], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.18": {"text": "Indessen f\u00fchl ich wol/ da\u00df meine Kr\u00e4ffte schwinden;", "tokens": ["In\u00b7des\u00b7sen", "f\u00fchl", "ich", "wol", "/", "da\u00df", "mei\u00b7ne", "Kr\u00e4ff\u00b7te", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df allbereit ein Tod in Sinn und Glieder w\u00fchlt;", "tokens": ["Da\u00df", "all\u00b7be\u00b7reit", "ein", "Tod", "in", "Sinn", "und", "Glie\u00b7der", "w\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich seh die h\u00f6chste Noht/ mit dir mich zu verbinden/", "tokens": ["Ich", "seh", "die", "h\u00f6chs\u00b7te", "Noht", "/", "mit", "dir", "mich", "zu", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da deine Sanfftmuht noch auf meine Rettung zielt.", "tokens": ["Da", "dei\u00b7ne", "Sanfft\u00b7muht", "noch", "auf", "mei\u00b7ne", "Ret\u00b7tung", "zielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mich schreckt der schwere Fluch/ den deine Rache dreut/", "tokens": ["Mich", "schreckt", "der", "schwe\u00b7re", "Fluch", "/", "den", "dei\u00b7ne", "Ra\u00b7che", "dreut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn sich mein Fleisch emp\u00f6rt und deiner Liebe Stuf-", "tokens": ["Wenn", "sich", "mein", "Fleisch", "em\u00b7p\u00f6rt", "und", "dei\u00b7ner", "Lie\u00b7be", "Stuf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADJD", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So gar ver\u00e4chtlich h\u00e4lt; HErr hast du mich geruffen/", "tokens": ["So", "gar", "ver\u00b7\u00e4cht\u00b7lich", "h\u00e4lt", ";", "Herr", "hast", "du", "mich", "ge\u00b7ruf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$.", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "So rei\u00df auch mit Gewalt mich aus der Eitelkeit.", "tokens": ["So", "rei\u00df", "auch", "mit", "Ge\u00b7walt", "mich", "aus", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}