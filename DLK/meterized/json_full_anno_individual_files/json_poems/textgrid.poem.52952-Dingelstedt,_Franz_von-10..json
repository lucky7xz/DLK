{"textgrid.poem.52952": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stirb, Engel, stirb in meinen Armen pl\u00f6tzlich!", "tokens": ["Stirb", ",", "En\u00b7gel", ",", "stirb", "in", "mei\u00b7nen", "Ar\u00b7men", "pl\u00f6tz\u00b7lich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Ku\u00df la\u00df Deinen roten Mund erkalten,", "tokens": ["Im", "Ku\u00df", "la\u00df", "Dei\u00b7nen", "ro\u00b7ten", "Mund", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Ku\u00df den letzten Seufzer sanft zerflie\u00dfen!", "tokens": ["Im", "Ku\u00df", "den", "letz\u00b7ten", "Seuf\u00b7zer", "sanft", "zer\u00b7flie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann soll mein Herz Dein Bildnis unverletzlich,", "tokens": ["Dann", "soll", "mein", "Herz", "Dein", "Bild\u00b7nis", "un\u00b7ver\u00b7letz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie Sarg und Grab, in seinem Schreine halten", "tokens": ["Wie", "Sarg", "und", "Grab", ",", "in",