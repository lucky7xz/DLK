{"textgrid.poem.46826": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sie feiern Freudenfeste]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie feiern Freudenfeste", "tokens": ["Sie", "fei\u00b7ern", "Freu\u00b7den\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und laden frohe G\u00e4ste,", "tokens": ["Und", "la\u00b7den", "fro\u00b7he", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und haben uns, o Schaden,", "tokens": ["Und", "ha\u00b7ben", "uns", ",", "o", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dazu nicht eingeladen.", "tokens": ["Da\u00b7zu", "nicht", "ein\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich denke gar, sie n\u00e4hmen", "tokens": ["Ich", "den\u00b7ke", "gar", ",", "sie", "n\u00e4h\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns auf nicht, wenn wir k\u00e4men;", "tokens": ["Uns", "auf", "nicht", ",", "wenn", "wir", "k\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic