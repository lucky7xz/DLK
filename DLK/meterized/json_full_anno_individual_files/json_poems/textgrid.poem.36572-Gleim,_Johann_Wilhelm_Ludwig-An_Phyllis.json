{"textgrid.poem.36572": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Phyllis", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich betete die junge Phyllis an,", "tokens": ["Ich", "be\u00b7te\u00b7te", "die", "jun\u00b7ge", "Phyl\u00b7lis", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Sch\u00f6nheit, Witz und Tugend angetrieben;", "tokens": ["Von", "Sch\u00f6n\u00b7heit", ",", "Witz", "und", "Tu\u00b7gend", "an\u00b7ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Sch\u00f6nheit, Witz und Tugend kann", "tokens": ["Und", "Sch\u00f6n\u00b7heit", ",", "Witz", "und", "Tu\u00b7gend", "kann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich itzt nicht reizen, sie zu lieben!", "tokens": ["Mich", "itzt", "nicht", "rei\u00b7zen", ",", "sie", "zu", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie betet itzt den jungen Daphnis an,", "tokens": ["Sie", "be\u00b7tet", "itzt", "den", "jun\u00b7gen", "Daph\u00b7nis", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "AR