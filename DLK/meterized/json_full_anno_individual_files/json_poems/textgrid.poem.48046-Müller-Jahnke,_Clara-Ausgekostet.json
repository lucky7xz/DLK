{"textgrid.poem.48046": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Ausgekostet", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In alle Tiefen bin ich gestiegen,", "tokens": ["In", "al\u00b7le", "Tie\u00b7fen", "bin", "ich", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "erklommen habe ich alle H\u00f6hn:", "tokens": ["er\u00b7klom\u00b7men", "ha\u00b7be", "ich", "al\u00b7le", "H\u00f6hn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "ich sah um die Gipfel die Adler fliegen", "tokens": ["ich", "sah", "um", "die", "Gip\u00b7fel", "die", "Ad\u00b7ler", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und h\u00f6rte der St\u00fcrzenden Angstgest\u00f6hn.", "tokens": ["und", "h\u00f6r\u00b7te", "der", "St\u00fcr\u00b7zen\u00b7den", "Angst\u00b7ge\u00b7st\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Nichts Menschliches ist mir fremd geblieben;", "tokens": ["Nichts", "Menschli\u00b7ches", "ist", "mir", "fremd", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$."],