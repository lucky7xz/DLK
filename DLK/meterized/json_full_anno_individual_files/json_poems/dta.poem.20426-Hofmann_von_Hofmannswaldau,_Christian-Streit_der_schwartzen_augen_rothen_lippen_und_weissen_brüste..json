{"dta.poem.20426": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die seelen pflegen hier zusammenkunfft zu haben/", "tokens": ["Die", "see\u00b7len", "pfle\u00b7gen", "hier", "zu\u00b7sam\u00b7men\u00b7kunfft", "zu", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und speisen sich mit lust durch s\u00fcssen honigseim:", "tokens": ["Und", "spei\u00b7sen", "sich", "mit", "lust", "durch", "s\u00fcs\u00b7sen", "ho\u00b7ni\u00b7gseim", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier pflantzet die natur den reichthum ihrer gaben/", "tokens": ["Hier", "pflant\u00b7zet", "die", "na\u00b7tur", "den", "reicht\u00b7hum", "ih\u00b7rer", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Venus kocht allhier den allerbesten leim.", "tokens": ["Und", "Ve\u00b7nus", "kocht", "all\u00b7hier", "den", "al\u00b7ler\u00b7bes\u00b7ten", "leim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein tropffen recht gebraucht/ leimt geist und geist zusammen;", "tokens": ["Ein", "tropf\u00b7fen", "recht", "ge\u00b7braucht", "/", "leimt", "geist", "und", "geist", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Thut nun der leim zuschlecht des mundes kr\u00e4ffte kund/", "tokens": ["Thut", "nun", "der", "leim", "zu\u00b7schlecht", "des", "mun\u00b7des", "kr\u00e4ff\u00b7te", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und zeiget nicht genung die funcken meiner flammen/", "tokens": ["Und", "zei\u00b7get", "nicht", "ge\u00b7nung", "die", "fun\u00b7cken", "mei\u00b7ner", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So k\u00fcsse man alsbald doch einen sch\u00f6nen mund.", "tokens": ["So", "k\u00fcs\u00b7se", "man", "als\u00b7bald", "doch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}