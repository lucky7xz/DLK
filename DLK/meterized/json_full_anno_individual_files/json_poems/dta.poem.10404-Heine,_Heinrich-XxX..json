{"dta.poem.10404": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die blauen Veilchen der Aeugelein,", "tokens": ["Die", "blau\u00b7en", "Veil\u00b7chen", "der", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die rothen Rosen der W\u00e4ngelein,", "tokens": ["Die", "ro\u00b7then", "Ro\u00b7sen", "der", "W\u00e4n\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die wei\u00dfen Lilien der H\u00e4ndchen klein,", "tokens": ["Die", "wei\u00b7\u00dfen", "Li\u00b7li\u00b7en", "der", "H\u00e4nd\u00b7chen", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die bl\u00fchen und bl\u00fchen noch immerfort,", "tokens": ["Die", "bl\u00fc\u00b7hen", "und", "bl\u00fc\u00b7hen", "noch", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und nur das Herzchen ist verdorrt.", "tokens": ["Und", "nur", "das", "Herz\u00b7chen", "ist", "ver\u00b7dorrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+