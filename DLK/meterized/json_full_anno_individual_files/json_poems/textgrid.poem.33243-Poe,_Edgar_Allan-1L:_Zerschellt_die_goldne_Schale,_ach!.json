{"textgrid.poem.33243": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zerschellt die goldne Schale, ach!", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zerschellt die goldne Schale, ach!", "tokens": ["Zer\u00b7schellt", "die", "gold\u00b7ne", "Scha\u00b7le", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Geist so fern entflogen!", "tokens": ["Der", "Geist", "so", "fern", "ent\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schickt Glockenschall der Seele nach,", "tokens": ["Schickt", "Glo\u00b7cken\u00b7schall", "der", "See\u00b7le", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die fort zum Styx gezogen!", "tokens": ["die", "fort", "zum", "Styx", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Guy de Vere, weinst ", "tokens": ["Und", "Guy", "de", "Ve\u00b7re", ",", "weinst"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NE", "NE", "NN", "$,", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt oder nie sei tr\u00fcbe!", "tokens": ["Jetzt", "o\u00b7der", "nie", "sei", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VAFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da liegt, sieh her, und liebt nie mehr", "tokens": ["Da", "liegt", ",", "sieh", "her", ",", "und", "liebt", "nie", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVIMP", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Lenore, deine Liebe.", "tokens": ["Le\u00b7no\u00b7re", ",", "dei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Komm! la\u00df vollziehn mit frommem Wort", "tokens": ["Komm", "!", "la\u00df", "voll\u00b7ziehn", "mit", "from\u00b7mem", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "VVIMP", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "des Grabes Heiligung \u2013", "tokens": ["des", "Gra\u00b7bes", "Hei\u00b7li\u00b7gung", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nichts K\u00f6niglichres stirbt hinfort", "tokens": ["Nichts", "K\u00f6\u00b7nig\u00b7lich\u00b7res", "stirbt", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "als sie, die starb so jung \u2013", "tokens": ["als", "sie", ",", "die", "starb", "so", "jung", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Man singe, bete immerfort", "tokens": ["Man", "sin\u00b7ge", ",", "be\u00b7te", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "f\u00fcr sie, die starb zu jung.", "tokens": ["f\u00fcr", "sie", ",", "die", "starb", "zu", "jung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "VVFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbwichte! ihr Reichtum war euch lieb,", "tokens": ["\u00bb", "wich\u00b7te", "!", "ihr", "Reich\u00b7tum", "war", "euch", "lieb", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "ihr Stolz war euch verha\u00dft,", "tokens": ["ihr", "Stolz", "war", "euch", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und da die Zarte fiel und blieb,", "tokens": ["Und", "da", "die", "Zar\u00b7te", "fiel", "und", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das Grab ihr segnen la\u00dft!", "tokens": ["das", "Grab", "ihr", "seg\u00b7nen", "la\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Ritual und Requiem,", "tokens": ["Das", "Ri\u00b7tu\u00b7al", "und", "Re\u00b7qui\u00b7em", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Durch euch \u2013 durch euch: den b\u00f6sen Blick?", "tokens": ["Durch", "euch", "\u2013", "durch", "euch", ":", "den", "b\u00f6\u00b7sen", "Blick", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "APPR", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Durch euch: die L\u00e4sterung,", "tokens": ["Durch", "euch", ":", "die", "L\u00e4s\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Die diese Unschuld totgehetzt,", "tokens": ["Die", "die\u00b7se", "Un\u00b7schuld", "tot\u00b7ge\u00b7hetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "die starb \u2013 und starb so jung?\u00ab", "tokens": ["die", "starb", "\u2013", "und", "starb", "so", "jung", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sing wie am Feiertag", "tokens": ["Sing", "wie", "am", "Fei\u00b7er\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "APPRART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lied zu Gott, da\u00df keine Qual", "tokens": ["Ein", "Lied", "zu", "Gott", ",", "da\u00df", "kei\u00b7ne", "Qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Tote f\u00fchlen mag.", "tokens": ["die", "To\u00b7te", "f\u00fch\u00b7len", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lenore schritt voran, und mit", "tokens": ["Le\u00b7no\u00b7re", "schritt", "vo\u00b7ran", ",", "und", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ihr flog die Hoffnung traut", "tokens": ["ihr", "flog", "die", "Hoff\u00b7nung", "traut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "\u2013 Die unbedacht und toll dich macht \u2013", "tokens": ["\u2013", "Die", "un\u00b7be\u00b7dacht", "und", "toll", "dich", "macht", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "auf die erkorene Braut:", "tokens": ["auf", "die", "er\u00b7ko\u00b7re\u00b7ne", "Braut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "So sanft sie war und wunderbar,", "tokens": ["So", "sanft", "sie", "war", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "erlag sie dem Geschick \u2013", "tokens": ["er\u00b7lag", "sie", "dem", "Ge\u00b7schick", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das Leben noch im gelben Haar,", "tokens": ["Das", "Le\u00b7ben", "noch", "im", "gel\u00b7ben", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "doch nicht in ihrem Blick \u2013", "tokens": ["doch", "nicht", "in", "ih\u00b7rem", "Blick", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Noch immerdar im gelben Haar,", "tokens": ["Noch", "im\u00b7mer\u00b7dar", "im", "gel\u00b7ben", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "doch Tod in ihrem Blick.", "tokens": ["doch", "Tod", "in", "ih\u00b7rem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbhinweg! Leicht wacht mein Herz heut nacht:", "tokens": ["\u00bb", "hin\u00b7weg", "!", "Leicht", "wacht", "mein", "Herz", "heut", "nacht", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Schmerzlied will ich klagen,", "tokens": ["Kein", "Schmerz\u00b7lied", "will", "ich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Triumph soll meinen Engel sacht", "tokens": ["Tri\u00b7umph", "soll", "mei\u00b7nen", "En\u00b7gel", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im heiligen Fluge tragen.", "tokens": ["im", "hei\u00b7li\u00b7gen", "Flu\u00b7ge", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Kein Glockenschlag! da\u00df nicht noch zag", "tokens": ["Kein", "Glo\u00b7cken\u00b7schlag", "!", "da\u00df", "nicht", "noch", "zag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$.", "KOUS", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die s\u00fc\u00dfe Seele werde", "tokens": ["die", "s\u00fc\u00b7\u00dfe", "See\u00b7le", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bei solchem Ton, aufgleitend schon", "tokens": ["Bei", "sol\u00b7chem", "Ton", ",", "auf\u00b7glei\u00b7tend", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVPP", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "von der verfluchten Erde:", "tokens": ["von", "der", "ver\u00b7fluch\u00b7ten", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu Freunden hin, von Feinden hier,", "tokens": ["Zu", "Freun\u00b7den", "hin", ",", "von", "Fein\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", 