{"textgrid.poem.50818": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiheitsbaum", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Tannenbaum im Schwarzwald steht,", "tokens": ["Ein", "Tan\u00b7nen\u00b7baum", "im", "Schwarz\u00b7wald", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der w\u00e4chst schon manches Jahr;", "tokens": ["Der", "w\u00e4chst", "schon", "man\u00b7ches", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Wipfel hoch ins Blaue geht,", "tokens": ["Sein", "Wip\u00b7fel", "hoch", "ins", "Blau\u00b7e", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drin fliegt sein gr\u00fcnes Haar.", "tokens": ["Drin", "fliegt", "sein", "gr\u00fc\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Wurzel hat den Erdengrund", "tokens": ["Die", "Wur\u00b7zel", "hat", "den", "Er\u00b7den\u00b7grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar innig angefa\u00dft,", "tokens": ["Gar", "in\u00b7nig", "an\u00b7ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und darum bleibt der Baum gesund,", "tokens": ["Und", "da\u00b7rum", "bleibt", "der", "Baum", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auch der Nordwind rast!", "tokens": ["Wie", "auch", "der", "Nord\u00b7wind", "rast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch alles, was auf Erden ist,", "tokens": ["Doch", "al\u00b7les", ",", "was", "auf", "Er\u00b7den", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df haben seine Zeit:", "tokens": ["Mu\u00df", "ha\u00b7ben", "sei\u00b7ne", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tannenbaum zu seiner Frist", "tokens": ["Der", "Tan\u00b7nen\u00b7baum", "zu", "sei\u00b7ner", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum F\u00e4llen ist bereit!", "tokens": ["Zum", "F\u00e4l\u00b7len", "ist", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann schm\u00fcckt man ihn, dann f\u00fchrt man ihn", "tokens": ["Dann", "schm\u00fcckt", "man", "ihn", ",", "dann", "f\u00fchrt", "man", "ihn"], "token_in