{"textgrid.poem.38335": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "16. Hochzeitmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil ich nun seh die goldnen Wangen", "tokens": ["Weil", "ich", "nun", "seh", "die", "gold\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmelsmorgenr\u00f6the prangen,", "tokens": ["Der", "Him\u00b7mels\u00b7mor\u00b7gen\u00b7r\u00f6\u00b7the", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So will auch ich dem Himmel zu,", "tokens": ["So", "will", "auch", "ich", "dem", "Him\u00b7mel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will der Leibsruh Abschied geben,", "tokens": ["Ich", "will", "der", "Leibs\u00b7ruh", "Ab\u00b7schied", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mich zu meinem Gott erheben,", "tokens": ["Und", "mich", "zu", "mei\u00b7nem", "Gott", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Gott, der meiner Seele Ruh.", "tokens": ["Zu", "Gott", ",", "der", "mei\u