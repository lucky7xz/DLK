{"textgrid.poem.63787": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist entthront die stolze Wellenbraut,", "tokens": ["Nun", "ist", "ent\u00b7thront", "die", "stol\u00b7ze", "Wel\u00b7len\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die einst den trotz'gen Nacken bog dem Meere.", "tokens": ["Die", "einst", "den", "trotz'\u00b7gen", "Na\u00b7cken", "bog", "dem", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wird sie mehr auf goldner Prachtgaleere", "tokens": ["Nicht", "wird", "sie", "mehr", "auf", "gold\u00b7ner", "Pracht\u00b7ga\u00b7lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem ungest\u00fcmen Freier angetraut.", "tokens": ["Dem", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Frei\u00b7er", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch in der Lenznacht, wenn mit Donnerlaut", "tokens": ["Doch", "in", "der", "Lenz\u00b7nacht", ",", "wenn", "mit", "Don\u00b7ner\u00b7laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Springflut steigt, dann ist's, als ob die Hehre", "tokens": ["Die", "Spring\u00b7flut", "steigt", ",", "dann", "ist's", ",", "als", "ob", "die", "Heh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wehrlos dem Element zu eigen w\u00e4re,", "tokens": ["Wehr\u00b7los", "dem", "E\u00b7le\u00b7ment", "zu", "ei\u00b7gen", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Auf das sie tags so k\u00fchl herniederschaut.", "tokens": ["Auf", "das", "sie", "tags", "so", "k\u00fchl", "her\u00b7nie\u00b7der\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hoch \u00fcber die Piazzetta schwillt die Flut", "tokens": ["Hoch", "\u00fc\u00b7ber", "die", "Piaz\u00b7zet\u00b7ta", "schwillt", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und braust herein, ers\u00e4ufend alle Gassen,", "tokens": ["Und", "braust", "her\u00b7ein", ",", "er\u00b7s\u00e4u\u00b7fend", "al\u00b7le", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "U