{"dta.poem.12476": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen \u00fcberwuchern unsre Saaten,", "tokens": ["Die", "Blu\u00b7men", "\u00fc\u00b7berw\u00b7uc\u00b7hern", "uns\u00b7re", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drum fehlet uns ein Held von \u00e4chtem Korne,", "tokens": ["Drum", "feh\u00b7let", "uns", "ein", "Held", "von", "\u00e4ch\u00b7tem", "Kor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der tief getrunken aus der Mannheit Borne", "tokens": ["Der", "tief", "ge\u00b7trun\u00b7ken", "aus", "der", "Mann\u00b7heit", "Bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und helfen kann, wo Tausende nur rathen;", "tokens": ["Und", "hel\u00b7fen", "kann", ",", "wo", "Tau\u00b7sen\u00b7de", "nur", "ra\u00b7then", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$,", "PWAV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Der sich versteht auf hohe, freie Thaten,", "tokens": ["Der", "sich", "ver\u00b7steht", "auf", "ho\u00b7he", ",", "frei\u00b7e", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "De\u00df Auge flammt in hellem Liebeszorne,", "tokens": ["De\u00df", "Au\u00b7ge", "flammt", "in", "hel\u00b7lem", "Lie\u00b7bes\u00b7zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der die Tyrannen peitschet mit dem Dorne", "tokens": ["Der", "die", "Ty\u00b7ran\u00b7nen", "peit\u00b7schet", "mit", "dem", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jeder Rose, so sie uns zertraten.", "tokens": ["Von", "je\u00b7der", "Ro\u00b7se", ",", "so", "sie", "uns", "zer\u00b7tra\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Held, de\u00df Worte leuchten in die Runde,", "tokens": ["Ein", "Held", ",", "de\u00df", "Wor\u00b7te", "leuch\u00b7ten", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der unsers Vaterlands zersprengte Theile", "tokens": ["Der", "un\u00b7sers", "Va\u00b7ter\u00b7lands", "zer\u00b7spreng\u00b7te", "Thei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zusammenzaubern kann zu neuem Bunde;", "tokens": ["Zu\u00b7sam\u00b7men\u00b7zau\u00b7bern", "kann", "zu", "neu\u00b7em", "Bun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Held, der, wo die Noth erheischet Eile,", "tokens": ["Ein", "Held", ",", "der", ",", "wo", "die", "Noth", "er\u00b7hei\u00b7schet", "Ei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Waffen in der Hand tr\u00e4gt, statt im Munde,", "tokens": ["Die", "Waf\u00b7fen", "in", "der", "Hand", "tr\u00e4gt", ",", "statt", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "KOUI", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Schwert greift, statt nach Pinsel oder Feile.", "tokens": ["Zum", "Schwert", "greift", ",", "statt", "nach", "Pin\u00b7sel", "o\u00b7der", "Fei\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUI", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}