{"dta.poem.564": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der unvorsichtige  Mops.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst der gute Mops an Jungfern sich erg\u00f6tzte", "tokens": ["Als", "j\u00fcngst", "der", "gu\u00b7te", "Mops", "an", "Jung\u00b7fern", "sich", "er\u00b7g\u00f6tz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lief er nach Haasen-Art die L\u00e4ng\u2019 und in die Quer", "tokens": ["Lief", "er", "nach", "Haa\u00b7sen\u00b7Art", "die", "L\u00e4ng'", "und", "in", "die", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fast alle Gassen durch/ bis da\u00df von ohngefehr", "tokens": ["Fast", "al\u00b7le", "Gas\u00b7sen", "durch", "/", "bis", "da\u00df", "von", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "APPR", "$(", "APPR", "KOUS", "APPR", "ADJD"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die artge ", "tokens": ["Die", "art\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Hier wird man Possen sehen", "tokens": ["Hier", "wird", "man", "Pos\u00b7sen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Spracher: und spitzte gleich sein breit ", "tokens": ["Spra\u00b7cher", ":", "und", "spitz\u00b7te", "gleich", "sein", "breit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Als er nun n\u00e4her gehen/", "tokens": ["Als", "er", "nun", "n\u00e4\u00b7her", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und den Rubinen Mund der Sch\u00f6nen wolte k\u00fcssen/", "tokens": ["Und", "den", "Ru\u00b7bi\u00b7nen", "Mund", "der", "Sch\u00f6\u00b7nen", "wol\u00b7te", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da fiel der arme Tropff zu ihren zarten F\u00fcssen/", "tokens": ["Da", "fiel", "der", "ar\u00b7me", "Tropff", "zu", "ih\u00b7ren", "zar\u00b7ten", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und weltzte sich im Koht als ein halb todtes Gaul/", "tokens": ["Und", "weltz\u00b7te", "sich", "im", "Koht", "als", "ein", "halb", "tod\u00b7tes", "Gaul", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "KOKOM", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An dieser Ebentheur hat ", "tokens": ["An", "die\u00b7ser", "E\u00b7bent\u00b7heur", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Da\u00df Lieben ohn Bedacht in Schimpff und Schanden f\u00fchrt.", "tokens": ["Da\u00df", "Lie\u00b7ben", "ohn", "Be\u00b7dacht", "in", "Schimpff", "und", "Schan\u00b7den", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}