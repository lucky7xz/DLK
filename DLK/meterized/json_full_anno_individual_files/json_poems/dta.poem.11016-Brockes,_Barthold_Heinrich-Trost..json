{"dta.poem.11016": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um die Begierden zu vermindern,", "tokens": ["Um", "die", "Be\u00b7gier\u00b7den", "zu", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja ebenfalls um Plag und Gram zu lindern;", "tokens": ["Ja", "e\u00b7ben\u00b7falls", "um", "Plag", "und", "Gram", "zu", "lin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erweget dieses, da\u00df die Zeit,", "tokens": ["Er\u00b7we\u00b7get", "die\u00b7ses", ",", "da\u00df", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdru\u00df, Gram, Schmerzen, Plag und Leid,", "tokens": ["Ver\u00b7dru\u00df", ",", "Gram", ",", "Schmer\u00b7zen", ",", "Plag", "und", "Leid", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Best\u00e4ndig mit sich nimmt, durch jeden Augenblick,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "mit", "sich", "nimmt", ",", "durch", "je\u00b7den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie jung ihr auch von Jahren seyd,", "tokens": ["Wie", "jung", "ihr", "auch", "von", "Jah\u00b7ren", "seyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird von euch selbst ein St\u00fcck", "tokens": ["Wird", "von", "euch", "selbst", "ein", "St\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Euch weggerissen und entf\u00fchrt,", "tokens": ["Euch", "weg\u00b7ge\u00b7ris\u00b7sen", "und", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da alles, was man sieht und h\u00f6ret,", "tokens": ["Da", "al\u00b7les", ",", "was", "man", "sieht", "und", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In des Vergangnen Schlund und Abgrund sich verliehrt,", "tokens": ["In", "des", "Ver\u00b7gang\u00b7nen", "Schlund", "und", "Ab\u00b7grund", "sich", "ver\u00b7liehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Woraus es nimmer wiederkehret.", "tokens": ["Wo\u00b7raus", "es", "nim\u00b7mer", "wie\u00b7der\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}