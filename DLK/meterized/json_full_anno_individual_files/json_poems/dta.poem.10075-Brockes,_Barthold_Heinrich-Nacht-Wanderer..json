{"dta.poem.10075": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nacht-Wanderer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich der Menschen Thun betrachte,", "tokens": ["Wenn", "ich", "der", "Men\u00b7schen", "Thun", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf ihren Zweck, den Trieb und ihre Wirckung", "tokens": ["Auf", "ih\u00b7ren", "Zweck", ",", "den", "Trieb", "und", "ih\u00b7re", "Wir\u00b7ckung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kommt ihr gantzes Leben mir", "tokens": ["So", "kommt", "ihr", "gant\u00b7zes", "Le\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht anders, als das Thun monds\u00fcchtger Wandrer", "tokens": ["Nicht", "an\u00b7ders", ",", "als", "das", "Thun", "mond\u00b7s\u00fccht\u00b7ger", "Wand\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dieselben thun verschiedne Sachen,", "tokens": ["Die\u00b7sel\u00b7ben", "thun", "ver\u00b7schied\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der festen Meinung, da\u00df sie wachen:", "tokens": ["Der", "fes\u00b7ten", "Mei\u00b7nung", ",", "da\u00df", "sie", "wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie steigen, klettern, gehen, stehn,", "tokens": ["Sie", "stei\u00b7gen", ",", "klet\u00b7tern", ",", "ge\u00b7hen", ",", "stehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie glauben, da\u00df sie h\u00f6ren, sehn;", "tokens": ["Sie", "glau\u00b7ben", ",", "da\u00df", "sie", "h\u00f6\u00b7ren", ",", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sie doch wircklich taub und blind", "tokens": ["Da", "sie", "doch", "wir\u00b7ck\u00b7lich", "taub", "und", "blind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "KON", "ADJD"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "F\u00fcr alles, und nur blos f\u00fcr eins empfindlich sind.", "tokens": ["F\u00fcr", "al\u00b7les", ",", "und", "nur", "blos", "f\u00fcr", "eins", "emp\u00b7find\u00b7lich", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KON", "ADV", "ADV", "APPR", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So geht es leider auf der Welt:", "tokens": ["So", "geht", "es", "lei\u00b7der", "auf", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine Theil von uns strebt nach der Ehre Wind;", "tokens": ["Der", "ei\u00b7ne", "Theil", "von", "uns", "strebt", "nach", "der", "Eh\u00b7re", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der andre l\u00e4ufft und rennt: was sucht er? nichts als", "tokens": ["Der", "and\u00b7re", "l\u00e4ufft", "und", "rennt", ":", "was", "sucht", "er", "?", "nichts", "als"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$.", "PIS", "KOKOM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der dritte, mit entflammter Brust,", "tokens": ["Der", "drit\u00b7te", ",", "mit", "ent\u00b7flamm\u00b7ter", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sucht blo\u00df bey Wein und Weibern Lust.", "tokens": ["Sucht", "blo\u00df", "bey", "Wein", "und", "Wei\u00b7bern", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein ieder ist so sehr auf seinen Zweck erpicht,", "tokens": ["Ein", "ie\u00b7der", "ist", "so", "sehr", "auf", "sei\u00b7nen", "Zweck", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er nichts anders sieht noch h\u00f6ret,", "tokens": ["Da\u00df", "er", "nichts", "an\u00b7ders", "sieht", "noch", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Empfindet, achtet, noch begehret.", "tokens": ["Emp\u00b7fin\u00b7det", ",", "ach\u00b7tet", ",", "noch", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Einfolglich ist, was ist, f\u00fcr ihn, als w\u00e4r\u2019 es nicht.", "tokens": ["Ein\u00b7folg\u00b7lich", "ist", ",", "was", "ist", ",", "f\u00fcr", "ihn", ",", "als", "w\u00e4r'", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PWS", "VAFIN", "$,", "APPR", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir sehen das, was unser GOTT geschaffen,", "tokens": ["Wir", "se\u00b7hen", "das", ",", "was", "un\u00b7ser", "GoTT", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nicht anders an, als wenn wir schlaffen,", "tokens": ["Nicht", "an\u00b7ders", "an", ",", "als", "wenn", "wir", "schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn minder, als verschiedne wachend sehn", "tokens": ["Denn", "min\u00b7der", ",", "als", "ver\u00b7schied\u00b7ne", "wa\u00b7chend", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "ADJA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Des Sch\u00f6pfers Werck, kann es im Schlaffe kaum", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Werck", ",", "kann", "es", "im", "Schlaf\u00b7fe", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "VMFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}