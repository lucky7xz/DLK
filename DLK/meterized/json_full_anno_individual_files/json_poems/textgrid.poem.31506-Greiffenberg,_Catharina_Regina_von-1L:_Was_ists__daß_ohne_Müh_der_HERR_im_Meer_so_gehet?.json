{"textgrid.poem.31506": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ists / da\u00df ohne M\u00fch der HERR im Meer so gehet?", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ists / da\u00df ohne M\u00fch der HERR im Meer so gehet?", "tokens": ["Was", "ists", "/", "da\u00df", "oh\u00b7ne", "M\u00fch", "der", "HeRR", "im", "Meer", "so", "ge\u00b7het", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "KOUS", "APPR", "NN", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weicht dann das nasse Glas / die schnelle Welle / nicht?", "tokens": ["weicht", "dann", "das", "nas\u00b7se", "Glas", "/", "die", "schnel\u00b7le", "Wel\u00b7le", "/", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$(", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein! sie ist Demant-hart / zu tragen den verpflicht /", "tokens": ["Nein", "!", "sie", "ist", "De\u00b7mant\u00b7hart", "/", "zu", "tra\u00b7gen", "den", "ver\u00b7pflicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "NE", "$(", "PTKZU", "VVINF", "ART", "VVFIN", "$("], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "der samt der Erd sie tr\u00e4gt / so lang die Welt bestehet.", "tokens": ["der", "samt", "der", "Erd", "sie", "tr\u00e4gt", "/", "so", "lang", "die", "Welt", "be\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$(", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie da\u00df kein Wirbelwind herwehend' ihn \u00fcmdrehet!", "tokens": ["Wie", "da\u00df", "kein", "Wir\u00b7bel\u00b7wind", "her\u00b7we\u00b7hend'", "ihn", "\u00fcm\u00b7dre\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er macht den Wind geschwind verschwinden / wann Er spricht;", "tokens": ["Er", "macht", "den", "Wind", "ge\u00b7schwind", "ver\u00b7schwin\u00b7den", "/", "wann", "Er", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ein Wort; ihm alsobald sein rasends blasen bricht.", "tokens": ["ein", "Wort", ";", "ihm", "al\u00b7so\u00b7bald", "sein", "ra\u00b7sends", "bla\u00b7sen", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist doch sein Mund der Grund / draus erstlich Er gewehet!", "tokens": ["Ist", "doch", "sein", "Mund", "der", "Grund", "/", "draus", "erst\u00b7lich", "Er", "ge\u00b7we\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART",