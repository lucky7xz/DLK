{"dta.poem.12821": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den schlafenden Jonas.  \n Madrigal.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach, Jona! la\u00df dich doch erwecken,", "tokens": ["Ach", ",", "Jo\u00b7na", "!", "la\u00df", "dich", "doch", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der abgrund rei\u00dft sich auf,", "tokens": ["Der", "ab\u00b7grund", "rei\u00dft", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und suchet dich mit wellen zu bedecken;", "tokens": ["Und", "su\u00b7chet", "dich", "mit", "wel\u00b7len", "zu", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der schaum der fluten schiest vorbey,", "tokens": ["Der", "schaum", "der", "flu\u00b7ten", "schiest", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und will durch seine raserey,", "tokens": ["Und", "will", "durch", "sei\u00b7ne", "ra\u00b7se\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die see zu deinem grabe machen.", "tokens": ["Die", "see", "zu", "dei\u00b7nem", "gra\u00b7be", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wickle dich aus deinen tr\u00e4umen.", "tokens": ["So", "wick\u00b7le", "dich", "aus", "dei\u00b7nen", "tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn flut und alle wetter krachen,", "tokens": ["Wenn", "flut", "und", "al\u00b7le", "wet\u00b7ter", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ja tolle wellen sch\u00e4umen,", "tokens": ["Ja", "tol\u00b7le", "wel\u00b7len", "sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Mu\u00df ieder auf der wache stehn.", "tokens": ["Mu\u00df", "ie\u00b7der", "auf", "der", "wa\u00b7che", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer wolte sicher schlafen gehn?", "tokens": ["Wer", "wol\u00b7te", "si\u00b7cher", "schla\u00b7fen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}