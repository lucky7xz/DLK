{"textgrid.poem.47902": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Zuschrifft", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bellona/ welche dich begabt mit wehrten Palmen/", "tokens": ["Bel\u00b7lo\u00b7na", "/", "wel\u00b7che", "dich", "be\u00b7gabt", "mit", "wehr\u00b7ten", "Pal\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PRF", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und deinen Siegespracht/ mit Lobgesang und Psalmen/", "tokens": ["und", "dei\u00b7nen", "Sie\u00b7ge\u00b7spracht", "/", "mit", "Lob\u00b7ge\u00b7sang", "und", "Psal\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "verewigt in der Welt/ hat ihr erhabnes Haubt/", "tokens": ["ve\u00b7re\u00b7wigt", "in", "der", "Welt", "/", "hat", "ihr", "er\u00b7hab\u00b7nes", "Haubt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "de\u00df gr\u00fcnen Lorbeerkrantzs/ zu deinem Lob beraubt/", "tokens": ["de\u00df", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7krantzs", "/", "zu", "dei\u00b7nem", "Lob", "be\u00b7raubt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und dich damit bekr\u00f6nt. Wie solches ist geschehen/", "tokens": ["und", "dich", "da\u00b7mit", "be\u00b7kr\u00f6nt", ".", "Wie", "sol\u00b7ches", "ist", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "VVFIN", "$.", "PWAV", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "hat vor verwichner Zeit die Teutsche Welt gesehen:", "tokens": ["hat", "vor", "ver\u00b7wich\u00b7ner", "Zeit", "die", "Teut\u00b7sche", "Welt", "ge\u00b7se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Irene trat her zu/ mit der Oliven-Kron/", "tokens": ["I\u00b7re\u00b7ne", "trat", "her", "zu", "/", "mit", "der", "O\u00b7li\u00b7ven\u00b7Kron", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKZU", "$(", "APPR", "ART", "NN", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "bekr\u00f6ne dich damit/ und halff dir auf den Thron/", "tokens": ["be\u00b7kr\u00f6\u00b7ne", "dich", "da\u00b7mit", "/", "und", "halff", "dir", "auf", "den", "Thron", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$(", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "de\u00df Norden K\u00f6nigreichs. Der h\u00f6chste Preis in Kriegen", "tokens": ["de\u00df", "Nor\u00b7den", "K\u00f6\u00b7nig\u00b7reichs", ".", "Der", "h\u00f6chs\u00b7te", "Preis", "in", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ist/ mit dem Heldenmuht/ im Siege/ sich besiegen/", "tokens": ["ist", "/", "mit", "dem", "Hel\u00b7den\u00b7muht", "/", "im", "Sie\u00b7ge", "/", "sich", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$(", "APPR", "ART", "NN", "$(", "APPRART", "NN", "$(", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und zwingen die Begierd/ die sonsten nimmersatt", "tokens": ["und", "zwin\u00b7gen", "die", "Be\u00b7gierd", "/", "die", "sons\u00b7ten", "nim\u00b7mer\u00b7satt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "sich/ in dem gr\u00f6ssten Gl\u00fcck/ nicht leicht vergn\u00fcget hat.", "tokens": ["sich", "/", "in", "dem", "gr\u00f6ss\u00b7ten", "Gl\u00fcck", "/", "nicht", "leicht", "ver\u00b7gn\u00fc\u00b7get", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$(", "APPR", "ART", "ADJA", "NN", "$(", "PTKNEG", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Irene/ welche du in Teutschland eingef\u00fchret/", "tokens": ["I\u00b7re\u00b7ne", "/", "wel\u00b7che", "du", "in", "Teutschland", "ein\u00b7ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "APPR", "NE", "VVFIN", "$("], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "hat dich/ mit ihrer Kron/ wie jetzt gesagt/ bezieret/", "tokens": ["hat", "dich", "/", "mit", "ih\u00b7rer", "Kron", "/", "wie", "jetzt", "ge\u00b7sagt", "/", "be\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "APPR", "PPOSAT", "NN", "$(", "KOKOM", "ADV", "VVPP", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und r\u00fchmet Deinen Ruhm: die waare Seligkeit/", "tokens": ["und", "r\u00fch\u00b7met", "Dei\u00b7nen", "Ruhm", ":", "die", "waa\u00b7re", "Se\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ist dem/ der Friede stifft/ als Gottes Kind/ bereit/", "tokens": ["ist", "dem", "/", "der", "Frie\u00b7de", "stifft", "/", "als", "Got\u00b7tes", "Kind", "/", "be\u00b7reit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "$(", "ART", "NN", "VVFIN", "$(", "KOUS", "NN", "NN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bellona saget frey/ da\u00df sie zwar \u00fcberwunden/", "tokens": ["Bel\u00b7lo\u00b7na", "sa\u00b7get", "frey", "/", "da\u00df", "sie", "zwar", "\u00fc\u00b7berw\u00b7un\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "doch in beliebter Hand den milden Tod gefunden/", "tokens": ["doch", "in", "be\u00b7lieb\u00b7ter", "Hand", "den", "mil\u00b7den", "Tod", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der nun die letzte Welt von neuem Leben macht/", "tokens": ["der", "nun", "die", "letz\u00b7te", "Welt", "von", "neu\u00b7em", "Le\u00b7ben", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und der die Friedenszeit hat an das Liecht gebracht.", "tokens": ["und", "der", "die", "Frie\u00b7dens\u00b7zeit", "hat", "an", "das", "Liecht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vulcanus hat die Kunst de\u00df Schmittenwercks erfunden/", "tokens": ["Vul\u00b7ca\u00b7nus", "hat", "die", "Kunst", "de\u00df", "Schmit\u00b7ten\u00b7wercks", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Apollo/ wie man heilt die Schmertzenvolle Wunden/", "tokens": ["A\u00b7pol\u00b7lo", "/", "wie", "man", "heilt", "die", "Schmert\u00b7zen\u00b7vol\u00b7le", "Wun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PIS", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der sch\u00f6ne Majen-Sohn der g\u00fcldnen Leyren Band:", "tokens": ["der", "sch\u00f6\u00b7ne", "Ma\u00b7jen\u00b7Sohn", "der", "g\u00fcld\u00b7nen", "Ley\u00b7ren", "Band", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "de\u00df wegen sie gesetzt sind in der G\u00f6tterstand/", "tokens": ["de\u00df", "we\u00b7gen", "sie", "ge\u00b7setzt", "sind", "in", "der", "G\u00f6t\u00b7ter\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVPP", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "nach jener Heyden Weis. Wie soll man den verehren/", "tokens": ["nach", "je\u00b7ner", "Hey\u00b7den", "Weis", ".", "Wie", "soll", "man", "den", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "PWAV", "VMFIN", "PIS", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der uns den werthsten Schatz/ den Frieden/ zu bescheren/", "tokens": ["der", "uns", "den", "werths\u00b7ten", "Schatz", "/", "den", "Frie\u00b7den", "/", "zu", "be\u00b7sche\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "erfreulichst sich bem\u00fcht? Der diesen Krieg gestillt/", "tokens": ["er\u00b7freu\u00b7lichst", "sich", "be\u00b7m\u00fcht", "?", "Der", "die\u00b7sen", "Krieg", "ge\u00b7stillt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "$.", "ART", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ist gleich dem Erdengott/ mit Himmelsgeist erf\u00fcllt.", "tokens": ["ist", "gleich", "dem", "Er\u00b7den\u00b7gott", "/", "mit", "Him\u00b7mels\u00b7geist", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O hocherleuchter F\u00fcrst/ der du den Fried erzeuget/", "tokens": ["O", "ho\u00b7cher\u00b7leuch\u00b7ter", "F\u00fcrst", "/", "der", "du", "den", "Fried", "er\u00b7zeu\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "f\u00fcr dir Irene sich/ als ihrem Vatter/ neiget/", "tokens": ["f\u00fcr", "dir", "I\u00b7re\u00b7ne", "sich", "/", "als", "ih\u00b7rem", "Vat\u00b7ter", "/", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PRF", "$(", "KOUS", "PPOSAT", "NN", "$(", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "der Tochter deines Lobs la\u00df deines Namens-Ehr/", "tokens": ["der", "Toch\u00b7ter", "dei\u00b7nes", "Lobs", "la\u00df", "dei\u00b7nes", "Na\u00b7mens\u00b7Ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVIMP", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "erw\u00fcnschend/ da\u00df dein Reich sich nac