{"textgrid.poem.62209": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[im Traum sah ich die Mutter heut]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Traum sah ich die Mutter heut,", "tokens": ["Im", "Traum", "sah", "ich", "die", "Mut\u00b7ter", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O golden s\u00fc\u00dfer Traum! \u2013", "tokens": ["O", "gol\u00b7den", "s\u00fc\u00b7\u00dfer", "Traum", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sie so sch\u00f6n und wunderbar,", "tokens": ["Ich", "sah", "sie", "so", "sch\u00f6n", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie oft im Leben kaum.", "tokens": ["Wie", "oft", "im", "Le\u00b7ben", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was kommst Du zu verk\u00fcnden mir,", "tokens": ["Was", "kommst", "Du", "zu", "ver\u00b7k\u00fcn\u00b7den", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKZU", "VVINF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebes Engelsbild? \u2013", "tokens": ["Du", "lie\u00b