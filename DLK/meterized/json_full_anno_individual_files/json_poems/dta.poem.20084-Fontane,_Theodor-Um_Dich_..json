{"dta.poem.20084": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Du darfst mi\u00dfmuthig nicht verzagen,", "tokens": ["Du", "darfst", "mi\u00df\u00b7mut\u00b7hig", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Liebe nicht noch im Gesang,", "tokens": ["In", "Lie\u00b7be", "nicht", "noch", "im", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mal ein allzu k\u00fchnes Wagen,", "tokens": ["Wenn", "mal", "ein", "all\u00b7zu", "k\u00fch\u00b7nes", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "PTKA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wurf im Wettspiel Dir mi\u00dflang.", "tokens": ["Ein", "Wurf", "im", "Wett\u00b7spiel", "Dir", "mi\u00df\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wes Fu\u00df w\u00e4r\u2019 niemals fehlgesprungen?", "tokens": ["Wes", "Fu\u00df", "w\u00e4r'", "nie\u00b7mals", "fehl\u00b7ge\u00b7sprun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer lief nicht irr\u2019 auf seinem Lauf?", "tokens": ["Wer", "lief", "nicht", "irr'", "auf", "sei\u00b7nem", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blick hin auf das, was Dir gelungen,", "tokens": ["Blick", "hin", "auf", "das", ",", "was", "Dir", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und richte ", "tokens": ["Und", "rich\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Vor\u00fcber ziehn die tr\u00fcben Wetter,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ziehn", "die", "tr\u00fc\u00b7ben", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es lacht aufs Neu der Sonne Glanz,", "tokens": ["Es", "lacht", "aufs", "Neu", "der", "Son\u00b7ne", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ob verwehn die ", "tokens": ["Und", "ob", "ver\u00b7wehn", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}