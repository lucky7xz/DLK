d", "aus", "dem", "Schwall", "der", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "die wie hingejagte Wolken mir entgleiten,", "tokens": ["die", "wie", "hin\u00b7ge\u00b7jag\u00b7te", "Wol\u00b7ken", "mir", "ent\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Bleibt tief mein Traum", "tokens": ["Bleibt", "tief", "mein", "Traum"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "wie \u00fcber blaue Heimatseen in sich selbst gekehrt.", "tokens": ["wie", "\u00fc\u00b7ber", "blau\u00b7e", "Hei\u00b7mat\u00b7seen", "in", "sich", "selbst", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Um meines Lebens", "tokens": ["Um", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "ungewisse Schalen neigen", "tokens": ["un\u00b7ge\u00b7wis\u00b7se", "Scha\u00b7len", "nei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dr\u00e4ngen sich die Bilder,", "tokens": ["Und", "dr\u00e4n\u00b7gen", "sich", "die", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iamb