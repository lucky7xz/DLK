{"textgrid.poem.25387": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Der Fl\u00fcchtling", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sich mein Leib", "tokens": ["Da", "sich", "mein", "Leib"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "in jener G\u00e4rten Zaubergrund verirrte,", "tokens": ["in", "je\u00b7ner", "G\u00e4r\u00b7ten", "Zau\u00b7ber\u00b7grund", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo blauer Schierling", "tokens": ["Wo", "blau\u00b7er", "Schier\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "zwischen Stauden dunkler Tollkirschbl\u00fcten stand,", "tokens": ["zwi\u00b7schen", "Stau\u00b7den", "dunk\u00b7ler", "Toll\u00b7kirschbl\u00fc\u00b7ten", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-++-+", "measure": "anapaest.init"}, "line.5": {"text": "Was hilft es, da\u00df ein sp\u00e4ter Tagesschein", "tokens": ["Was", "hilft", "es", ",", "da\u00df", "ein", "sp\u00e4\u00b7ter", "Ta\u00b7ges\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "den Kn\u00e4uel bunter Fiebertr\u00e4ume mir entwirrte,", "tokens": ["den", "Kn\u00e4u\u00b7el", "bun\u00b7ter", "Fie\u00b7ber\u00b7tr\u00e4u\u00b7me", "mir", "ent\u00b7wirr\u00b7t