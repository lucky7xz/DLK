{"textgrid.poem.48743": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "30. Auf seinen Namenstag, unter w\u00e4hrender Reise auf der Ocke begangen, 1636 Juni 29.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So komme du denn her, du sch\u00f6nste der Najaden,", "tokens": ["So", "kom\u00b7me", "du", "denn", "her", ",", "du", "sch\u00f6ns\u00b7te", "der", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil meine ", "tokens": ["weil", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "mich itzt nicht binden kan, um da\u00df wir ferne sind,", "tokens": ["mich", "itzt", "nicht", "bin\u00b7den", "kan", ",", "um", "da\u00df", "wir", "fer\u00b7ne", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,", "KOUI", "KOUS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "komm, ", "tokens": ["komm", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "und Hamadryaden, die oftmals mit dir baden,", "tokens": ["und", "Ha\u00b7mad\u00b7ry\u00b7a\u00b7den", ",", "die", "oft\u00b7mals", "mit", "dir", "ba\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "komm, binde mich f\u00fcr sie. Der k\u00fchle Westenwind", "tokens": ["komm", ",", "bin\u00b7de", "mich", "f\u00fcr", "sie", ".", "Der", "k\u00fch\u00b7le", "Wes\u00b7ten\u00b7wind"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bricht Blumen durch den Tal, da manche Nymphe rinnt", "tokens": ["bricht", "Blu\u00b7men", "durch", "den", "Tal", ",", "da", "man\u00b7che", "Nym\u00b7phe", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und schwimmet auf uns zu, mit Farben schwer beladen.", "tokens": ["und", "schwim\u00b7met", "auf", "uns", "zu", ",", "mit", "Far\u00b7ben", "schwer", "be\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lies Rosen, M\u00fcnze, Klee, Borrag und Quendel aus,", "tokens": ["Lies", "Ro\u00b7sen", ",", "M\u00fcn\u00b7ze", ",", "Klee", ",", "Bor\u00b7rag", "und", "Quen\u00b7del", "aus", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mach f\u00fcr mein Haupt und Hand mir einen Kranz und Strau\u00df", "tokens": ["mach", "f\u00fcr", "mein", "Haupt", "und", "Hand", "mir", "ei\u00b7nen", "Kranz", "und", "Strau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "PPER", "ART", "NN", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und hauch' ein L\u00fcftlein drein, das nach der Liebe r\u00fcche.", "tokens": ["und", "hauch'", "ein", "L\u00fcft\u00b7lein", "drein", ",", "das", "nach", "der", "Lie\u00b7be", "r\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr andern gehet aus, f\u00fchrt ein belaubtes Zelt", "tokens": ["Ihr", "an\u00b7dern", "ge\u00b7het", "aus", ",", "f\u00fchrt", "ein", "be\u00b7laub\u00b7tes", "Zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von jungen \u00c4sten auf. So ist es wol bestellt.", "tokens": ["von", "jun\u00b7gen", "\u00c4s\u00b7ten", "auf", ".", "So", "ist", "es", "wol", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So will ich fr\u00f6lich sein, bis Ph\u00f6bus sich verkriche.", "tokens": ["So", "will", "ich", "fr\u00f6\u00b7lich", "sein", ",", "bis", "Ph\u00f6\u00b7bus", "sich", "ver\u00b7kri\u00b7che", "."], "token_info": ["word", "word", "word", "word",