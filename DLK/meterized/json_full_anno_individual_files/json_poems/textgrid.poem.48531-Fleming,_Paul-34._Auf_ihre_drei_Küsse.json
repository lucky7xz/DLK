{"textgrid.poem.48531": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Auf ihre drei K\u00fcsse", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War oder war ich nicht, da mich mein Lieb entz\u00fcckte", "tokens": ["War", "o\u00b7der", "war", "ich", "nicht", ",", "da", "mich", "mein", "Lieb", "ent\u00b7z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und ihren feuchten Mund auf meinen dreimal dr\u00fcckte?", "tokens": ["und", "ih\u00b7ren", "feuch\u00b7ten", "Mund", "auf", "mei\u00b7nen", "drei\u00b7mal", "dr\u00fcck\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "War oder war ich nicht? Kupido, sag' es mir!", "tokens": ["War", "o\u00b7der", "war", "ich", "nicht", "?", "Ku\u00b7pi\u00b7do", ",", "sag'", "es", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VAFIN", "PPER", "PTKNEG", "$.", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, so mache mich recht dankbar gegen ihr", "tokens": ["Wo", "nicht", ",", "so", "ma\u00b7che", "mich", "recht", "dank\u00b7bar", "ge\u00b7gen", "ihr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und schaffe, da\u00df ich sie noch heute zur Gen\u00fcge", "tokens": ["und", "schaf\u00b7fe", ",", "da\u00df", "ich", "sie", "noch", "heu\u00b7te", "zur", "Ge\u00b7n\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "zehnmalzehntausentmal f\u00fcr einmal k\u00fcssen m\u00fcge!", "tokens": ["zehn\u00b7mal\u00b7zehn\u00b7tau\u00b7sent\u00b7mal", "f\u00fcr", "ein\u00b7mal", "k\u00fcs\u00b7sen", "m\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "War oder war ich nicht, da mich mein Lieb entz\u00fcckte", "tokens": ["War", "o\u00b7der", "war", "ich", "nicht", ",", "da", "mich", "mein", "Lieb", "ent\u00b7z\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KON", "VAFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und ihren feuchten Mund auf meinen dreimal dr\u00fcckte?", "tokens": ["und", "ih\u00b7ren", "feuch\u00b7ten", "Mund", "auf", "mei\u00b7nen", "drei\u00b7mal", "dr\u00fcck\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "War oder war ich nicht? Kupido, sag' es mir!", "tokens": ["War", "o\u00b7