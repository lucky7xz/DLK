{"dta.poem.13781": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Banner, der so k\u00fchle wallt!", "tokens": ["Ban\u00b7ner", ",", "der", "so", "k\u00fch\u00b7le", "wallt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Deinen gr\u00fcnen Wogen", "tokens": ["Un\u00b7ter", "Dei\u00b7nen", "gr\u00fc\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hast du treu uns auferzogen.", "tokens": ["Hast", "du", "treu", "uns", "auf\u00b7er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frommer Sagen Aufenthalt!", "tokens": ["From\u00b7mer", "Sa\u00b7gen", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebe wohl,", "tokens": ["Le\u00b7be", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Lebe wohl, du sch\u00f6ner Wald!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "sch\u00f6\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "tr