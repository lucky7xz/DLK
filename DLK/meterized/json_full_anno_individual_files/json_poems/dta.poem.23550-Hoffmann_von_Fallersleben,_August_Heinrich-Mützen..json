{"dta.poem.23550": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fctzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wunderth\u00e4tig ward die M\u00fctze,", "tokens": ["Wun\u00b7der\u00b7th\u00e4\u00b7tig", "ward", "die", "M\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dereinst Francesco ", "tokens": ["Die", "de\u00b7reinst", "Fran\u00b7ce\u00b7sco"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist Wunder doch genug!", "tokens": ["Das", "ist", "Wun\u00b7der", "doch", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die franz\u00f6sische Freiheitsm\u00fctze", "tokens": ["Die", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "Frei\u00b7heits\u00b7m\u00fct\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward zur Kaiserkrone gar \u2014", "tokens": ["Ward", "zur", "Kai\u00b7ser\u00b7kro\u00b7ne", "gar"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O wie gro\u00df, wie wunderbar!", "tokens": ["O", "wie", "gro\u00df", ",", "wie", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und des Preu\u00dfen Landwehrm\u00fctze", "tokens": ["Und", "des", "Preu\u00b7\u00dfen", "Land\u00b7wehr\u00b7m\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward ein deutscher Siegeshut \u2014", "tokens": ["Ward", "ein", "deut\u00b7scher", "Sie\u00b7ges\u00b7hut"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dies Wunder that uns gut.", "tokens": ["Und", "dies", "Wun\u00b7der", "that", "uns", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch bei unsern heut'gen M\u00fctzen", "tokens": ["Doch", "bei", "un\u00b7sern", "heut'\u00b7gen", "M\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist von Wunder keine Spur,", "tokens": ["Ist", "von", "Wun\u00b7der", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es sind \u2014 Schlafm\u00fctzen nur.", "tokens": ["Denn", "es", "sind", "Schlaf\u00b7m\u00fct\u00b7zen", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$(", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}