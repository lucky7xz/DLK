{"textgrid.poem.59919": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df den Muckern ihre Tugend,", "tokens": ["La\u00df", "den", "Mu\u00b7ckern", "ih\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was daran ist, Herr, du wei\u00dft es;", "tokens": ["Was", "da\u00b7ran", "ist", ",", "Herr", ",", "du", "wei\u00dft", "es", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VAFIN", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Nur erhalte mir die Jugend", "tokens": ["Nur", "er\u00b7hal\u00b7te", "mir", "die", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meines Herzens, meines Geistes!", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", ",", "mei\u00b7nes", "Geis\u00b7tes", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo so edle Weine flie\u00dfen,", "tokens": ["Wo", "so", "ed\u00b7le", "Wei\u00b7ne", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df die Quelle doch wohl echt sein;", "tokens": ["Mu\u00df", "die", "Quel\u00b7le", "doch", "wo