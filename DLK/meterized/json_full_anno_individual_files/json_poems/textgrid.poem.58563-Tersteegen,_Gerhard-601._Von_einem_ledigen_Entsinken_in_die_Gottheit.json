{"textgrid.poem.58563": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "601. Von einem ledigen Entsinken in die Gottheit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott hat mich getr\u00f6stet wohl,", "tokens": ["Mein", "Gott", "hat", "mich", "ge\u00b7tr\u00f6s\u00b7tet", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich ganz leer bin, wie ich soll,", "tokens": ["Wenn", "ich", "ganz", "leer", "bin", ",", "wie", "ich", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er mich herstellt und neu will machen;", "tokens": ["Er", "mich", "her\u00b7stellt", "und", "neu", "will", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nnt' ich nur g\u00e4nzlich ledig stehn,", "tokens": ["K\u00f6nnt'", "ich", "nur", "g\u00e4nz\u00b7lich", "le\u00b7dig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Christus immer weitergehn,", "tokens": ["Mit", "Chris\u00b7tus", "im\u00b7mer", "wei\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das w\u00e4r' die reichste Sach' der Sachen!", "tokens": ["Das", "w\u00e4r'", "die", "reichs\u00b7te", "Sach'", "der", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch will er mich noch weiterbringen,", "tokens": ["Doch", "will", "er", "mich", "noch", "wei\u00b7ter\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich soll ins Meer der Gottheit dringen;", "tokens": ["Ich", "soll", "ins", "Meer", "der", "Got\u00b7theit", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da bin ich aller Sorgen ohn',", "tokens": ["Da", "bin", "ich", "al\u00b7ler", "Sor\u00b7gen", "ohn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da st\u00f6rt mich keiner Stimme Ton.", "tokens": ["Da", "st\u00f6rt", "mich", "kei\u00b7ner", "Stim\u00b7me", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ach Gott, wie wohl, wie reich, wie selig", "tokens": ["Ach", "Gott", ",", "wie", "wohl", ",", "wie", "reich", ",", "wie", "se\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "NN", "$,", "PWAV", "ADV", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Schwimmt man im Meer der Gottheit ja!", "tokens": ["Schwimmt", "man", "im", "Meer", "der", "Got\u00b7theit", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Nur Gott ist unsre Sache da,", "tokens": ["Nur", "Gott", "ist", "uns\u00b7re", "Sa\u00b7che", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das Herz ist trostesvoll und fr\u00f6hlich.", "tokens": ["Das", "Herz", "ist", "tros\u00b7tes\u00b7voll", "und", "fr\u00f6h\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Gott mir verspricht dies ew'ge Leben,", "tokens": ["Gott", "mir", "ver\u00b7spricht", "dies", "ew'\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Er wird mir's auch gewi\u00dflich geben;", "tokens": ["Er", "wird", "mir's", "auch", "ge\u00b7wi\u00df\u00b7lich", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nur da\u00df ich, vom Geschaffen frei,", "tokens": ["Nur", "da\u00df", "ich", ",", "vom", "Ge\u00b7schaf\u00b7fen", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "wo