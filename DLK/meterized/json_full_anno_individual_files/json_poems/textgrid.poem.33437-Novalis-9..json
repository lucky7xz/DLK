: "iambic.tri"}, "line.3": {"text": "Da\u00df bald an allen Orten tagt", "tokens": ["Da\u00df", "bald", "an", "al\u00b7len", "Or\u00b7ten", "tagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das neue Himmelreich.", "tokens": ["Das", "neu\u00b7e", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt scheint die Welt dem neuen Sinn", "tokens": ["Jetzt", "scheint", "die", "Welt", "dem", "neu\u00b7en", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erst wie ein Vaterland;", "tokens": ["Erst", "wie", "ein", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein neues Leben nimmt man hin", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "nimmt", "man", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcckt aus seiner Hand.", "tokens": ["Ent\u00b7z\u00fcckt", "aus", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hinunter in das tiefe Meer", "tokens": ["Hin\u00b7un\u00b7ter", "in", "das", "tie\u00b7fe", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versank des Todes Graun,", "tokens": ["Ver\u00b7sank", "des", "To\u00b7des", "Graun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder kann nun leicht und hehr", "tokens": ["Und", "je\u00b7der", "kann", "nun", "leicht", "und", "hehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seine Zukunft schaun.", "tokens": ["In", "sei\u00b7ne", "Zu\u00b7kunft", "schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der dunkle Weg, den er betrat,", "tokens": ["Der", "dunk\u00b7le", "Weg", ",", "den", "er", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht in den Himmel aus,", "tokens": ["Geht", "in", "den", "Him\u00b7mel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer nur h\u00f6rt auf seinen Rath,", "tokens": ["Und", "wer", "nur", "h\u00f6rt", "auf", "sei\u00b7nen", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt auch in Vaters Haus.", "tokens": ["Kommt", "auch", "in", "Va\u00b7ters", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "NN"