, "und", "wird", "nun", "bei", "uns", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VAFIN", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alles uns verl\u00e4\u00dft!", "tokens": ["Wenn", "al\u00b7les", "uns", "ver\u00b7l\u00e4\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und so soll dieser Tag uns seyn", "tokens": ["Und", "so", "soll", "die\u00b7ser", "Tag", "uns", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PDAT", "NN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Weltverj\u00fcngungs-Fest.", "tokens": ["Ein", "Welt\u00b7ver\u00b7j\u00fcn\u00b7gungs\u00b7Fest", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}