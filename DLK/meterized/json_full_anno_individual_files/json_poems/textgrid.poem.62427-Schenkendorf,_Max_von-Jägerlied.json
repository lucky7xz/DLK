{"textgrid.poem.62427": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4gerlied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach gr\u00fcner Farb' mein Herz begehrt,", "tokens": ["Nach", "gr\u00fc\u00b7ner", "Fa\u00b7rb'", "mein", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur s\u00fc\u00dfen Augenweide.", "tokens": ["Zur", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen\u00b7wei\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann wird mir solche Lust gew\u00e4hrt,", "tokens": ["Wann", "wird", "mir", "sol\u00b7che", "Lust", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu gehn im gr\u00fcnen Kleide.", "tokens": ["Zu", "gehn", "im", "gr\u00fc\u00b7nen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie Gotteshand im gr\u00fcnen Mai", "tokens": ["Wie", "Got\u00b7tes\u00b7hand", "im", "gr\u00fc\u00b7nen", "Mai"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Fluren kleidet sch\u00f6n und neu,", "tokens": ["Die", "Flu\u00b7ren", "klei\u00b7det", "sch\u00f6n", "und", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lie\u00df ich mich gerne schauen.", "tokens": ["Lie\u00df", "ich", "mich", "ger\u00b7ne", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So nenne k\u00fchn die Farbenpracht,", "tokens": ["So", "nen\u00b7ne", "k\u00fchn", "die", "Far\u00b7ben\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Die dir das Herz entz\u00fcndet.", "tokens": ["Die", "dir", "das", "Herz", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf gr\u00fcnem Grund hat Gottes Macht", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Grund", "hat", "Got\u00b7tes", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde Bau gegr\u00fcndet.", "tokens": ["Der", "Er\u00b7de", "Bau", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir wollen uns f\u00fcr dich bem\u00fchn.", "tokens": ["Wir", "wol\u00b7len", "uns", "f\u00fcr", "dich", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist's Wiesengr\u00fcn? ist's Waldesgr\u00fcn?", "tokens": ["Ist's", "Wie\u00b7sen\u00b7gr\u00fcn", "?", "ist's", "Wal\u00b7des\u00b7gr\u00fcn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist's Gr\u00fcn von edlen Steinen?", "tokens": ["Ist's", "Gr\u00fcn", "von", "ed\u00b7len", "Stei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist die s\u00fc\u00dfe Fr\u00fchlingslust,", "tokens": ["Es", "ist", "die", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sind der Hoffnung Farben,", "tokens": ["Es", "sind", "der", "Hoff\u00b7nung", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die nimmermehr in Menschenbrust", "tokens": ["Die", "nim\u00b7mer\u00b7mehr", "in", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entschliefen, noch erstarben.", "tokens": ["Ent\u00b7schlie\u00b7fen", ",", "noch", "er\u00b7star\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie brechen vor in gr\u00fcner Glut,", "tokens": ["Sie", "bre\u00b7chen", "vor", "in", "gr\u00fc\u00b7ner", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freiheits-Lust, der Freiheits-Muth,", "tokens": ["Die", "Frei\u00b7heits\u00b7Lust", ",", "der", "Frei\u00b7heits\u00b7Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die haben mich ergriffen.", "tokens": ["Die", "ha\u00b7ben", "mich", "er\u00b7grif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ist ein junger Tannenwald,", "tokens": ["Es", "ist", "ein", "jun\u00b7ger", "Tan\u00b7nen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gr\u00fcner Wald aus Norden,", "tokens": ["Ein", "gr\u00fc\u00b7ner", "Wald", "aus", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So schlank und adlig von Gestalt,", "tokens": ["So", "schlank", "und", "ad\u00b7lig", "von", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ritterlicher Orden.", "tokens": ["Ein", "rit\u00b7ter\u00b7li\u00b7cher", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sturm, der seine Zweige regt,", "tokens": ["Der", "Sturm", ",", "der", "sei\u00b7ne", "Zwei\u00b7ge", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat auch mein tiefstes Herz bewegt,", "tokens": ["Hat", "auch", "mein", "tiefs\u00b7tes", "Herz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der heil'ge Sturmwind Gottes.", "tokens": ["Der", "heil'\u00b7ge", "Sturm\u00b7wind", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist die schmucke J\u00e4gerschaar", "tokens": ["Es", "ist", "die", "schmu\u00b7cke", "J\u00e4\u00b7ger\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der jungen tapfern Preu\u00dfen,", "tokens": ["Der", "jun\u00b7gen", "tap\u00b7fern", "Preu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sollen nun und immerdar", "tokens": ["Die", "sol\u00b7len", "nun", "und", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns rechte J\u00e4ger hei\u00dfen.", "tokens": ["Uns", "rech\u00b7te", "J\u00e4\u00b7ger", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr bestes Wild ist ein Tyrann,", "tokens": ["Ihr", "bes\u00b7tes", "Wild", "ist", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drauf zielen alle Mann f\u00fcr Mann,", "tokens": ["Drauf", "zie\u00b7len", "al\u00b7le", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O st\u00fcnd' ich unter ihnen.", "tokens": ["O", "st\u00fcnd'", "ich", "un\u00b7ter", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und ist es das, du deutsches Blut,", "tokens": ["Und", "ist", "es", "das", ",", "du", "deut\u00b7sches", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was deinen Busen schwellet,", "tokens": ["Was", "dei\u00b7nen", "Bu\u00b7sen", "schwel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sei der Schaar voll Lust und Muth", "tokens": ["So", "sei", "der", "Schaar", "voll", "Lust", "und", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Ehren zugesellet!", "tokens": ["In", "Eh\u00b7ren", "zu\u00b7ge\u00b7sel\u00b7let", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du junger gr\u00fcner Freiheits-Spro\u00df,", "tokens": ["Du", "jun\u00b7ger", "gr\u00fc\u00b7ner", "Frei\u00b7heits\u00b7Spro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimm hin das heilige Gescho\u00df,", "tokens": ["Nimm", "hin", "das", "hei\u00b7li\u00b7ge", "Ge\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und t\u00f6dte den Tyrannen.", "tokens": ["Und", "t\u00f6d\u00b7te", "den", "Ty\u00b7ran\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O gr\u00fcne Lust, o Gottes-Kraft,", "tokens": ["O", "gr\u00fc\u00b7ne", "Lust", ",", "o", "Got\u00b7tes\u00b7Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Sehnen ist gestillet,", "tokens": ["Mein", "Seh\u00b7nen", "ist", "ge\u00b7stil\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Freiheitstrieb und Fr\u00fchlingssaft", "tokens": ["Wo", "Frei\u00b7heit\u00b7strieb", "und", "Fr\u00fch\u00b7lings\u00b7saft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In tausend Adern quillet.", "tokens": ["In", "tau\u00b7send", "A\u00b7dern", "q