{"textgrid.poem.31620": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schneidet / schneidet ab mit Freuden / was der milde Himmel gibt /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneidet / schneidet ab mit Freuden / was der milde Himmel gibt /", "tokens": ["Schnei\u00b7det", "/", "schnei\u00b7det", "ab", "mit", "Freu\u00b7den", "/", "was", "der", "mil\u00b7de", "Him\u00b7mel", "gibt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PTKVZ", "APPR", "NN", "$(", "PWS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "die verguldte Lebens-Kron / fechsnet ietzund in die Scheuren:", "tokens": ["die", "ver\u00b7guld\u00b7te", "Le\u00b7bens\u00b7Kron", "/", "fechs\u00b7net", "ie\u00b7tzund", "in", "die", "Scheu\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gott wird sie / wie auf dem Feld / segnen auch in euren M\u00e4uren.", "tokens": ["Gott", "wird", "sie", "/", "wie", "auf", "dem", "Feld", "/", "seg\u00b7nen", "auch", "in", "eu\u00b7ren", "M\u00e4u\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$(", "KOKOM", "APPR", "ART", "NN", "$(", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "++--+-++-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Dem Allwesenden / durch diese / auch zu uns zukommen