{"dta.poem.1279": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nur eine allein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nt ich der Sonnen Glantz in allen Augen finden/", "tokens": ["K\u00f6nt", "ich", "der", "Son\u00b7nen", "Glantz", "in", "al\u00b7len", "Au\u00b7gen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4r ich auch vergn\u00fcgt mit manchem Sternen-", "tokens": ["So", "w\u00e4r", "ich", "auch", "ver\u00b7gn\u00fcgt", "mit", "man\u00b7chem", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Strahl/", "tokens": ["Strahl", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sie zeigten mir dein Bild als Spiegel allzumahl.", "tokens": ["Sie", "zeig\u00b7ten", "mir", "dein", "Bild", "als", "Spie\u00b7gel", "all\u00b7zu\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "KOUS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil aber Mond und Stern bey heller Sonn erblin-", "tokens": ["Weil", "a\u00b7ber", "Mond", "und", "Stern", "bey", "hel\u00b7ler", "Sonn", "er\u00b7blin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "So will ich auch mein Hertz an die alleine binden.", "tokens": ["So", "will", "ich", "auch", "mein", "Hertz", "an", "die", "al\u00b7lei\u00b7ne", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}