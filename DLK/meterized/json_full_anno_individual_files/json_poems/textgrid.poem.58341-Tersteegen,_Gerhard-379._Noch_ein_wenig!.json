{"textgrid.poem.58341": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "379. Noch ein wenig!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald ist mein letztes St\u00fcndlein da,", "tokens": ["Bald", "ist", "mein", "letz\u00b7tes", "St\u00fcnd\u00b7lein", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worin ich werd' zum Herren gehen,", "tokens": ["Wo\u00b7rin", "ich", "werd'", "zum", "Her\u00b7ren", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bleib' ich ihm auf ewig nah", "tokens": ["Dann", "bleib'", "ich", "ihm", "auf", "e\u00b7wig", "nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und werd' ihn klar im Lichte sehen;", "tokens": ["Und", "werd'", "ihn", "klar", "im", "Lich\u00b7te", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich trag' indes ein wenig noch", "tokens": ["Ich", "trag'", "in\u00b7des", "ein", "we\u00b7nig", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Jesus hier sein Kreuzesjoch.", "tokens": ["Mit", "Je\u00