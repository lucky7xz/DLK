{"dta.poem.9739": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer betagten Jungfrau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht wundre leser dich/ da\u00df sich in so viel jahren", "tokens": ["Nicht", "wund\u00b7re", "le\u00b7ser", "dich", "/", "da\u00df", "sich", "in", "so", "viel", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "PPER", "$(", "KOUS", "PRF", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die jungfrau/ die hier ruht/ nicht hat verlangt zu paaren.", "tokens": ["Die", "jung\u00b7frau", "/", "die", "hier", "ruht", "/", "nicht", "hat", "ver\u00b7langt", "zu", "paa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "ADV", "VVFIN", "$(", "PTKNEG", "VAFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der todt hat sie ihm l\u00e4ngst erwehlt zu einer braut/", "tokens": ["Der", "todt", "hat", "sie", "ihm", "l\u00e4ngst", "er\u00b7wehlt", "zu", "ei\u00b7ner", "braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVFIN", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie so fett wie er/ und mehr als reich an haut.", "tokens": ["Weil", "sie", "so", "fett", "wie", "er", "/", "und", "mehr", "als", "reich", "an", "haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$(", "KON", "PIAT", "KOKOM", "ADJD", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}