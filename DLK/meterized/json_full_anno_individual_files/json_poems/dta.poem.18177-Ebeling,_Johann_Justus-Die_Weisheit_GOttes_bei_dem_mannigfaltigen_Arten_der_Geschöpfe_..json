{"dta.poem.18177": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Weisheit GOttes  \n bei dem mannigfaltigen  \n  Arten der Gesch\u00f6pfe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer diese Welt beschaut, der Kreaturen", "tokens": ["Wer", "die\u00b7se", "Welt", "be\u00b7schaut", ",", "der", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PDAT", "NN", "VVPP", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heer,", "tokens": ["Heer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Darin sich bildend zeigt des grossen", "tokens": ["Da\u00b7rin", "sich", "bil\u00b7dend", "zeigt", "des", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PRF", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00f6pfers Ehr:", "tokens": ["Sch\u00f6p\u00b7fers", "Ehr", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der sieht wie seine Macht zu dem be-", "tokens": ["Der", "sieht", "wie", "sei\u00b7ne", "Macht", "zu", "dem", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "PPOSAT", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "quemen Leben,", "tokens": ["que\u00b7men", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Uns allerlei Gesch\u00f6pf zum Ueberflus gegeben.", "tokens": ["Uns", "al\u00b7ler\u00b7lei", "Ge\u00b7sch\u00f6pf", "zum", "Ue\u00b7ber\u00b7flus", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man \u00fcberrechne nur die ungez\u00e4hlte Zahl", "tokens": ["Man", "\u00fc\u00b7berr\u00b7ech\u00b7ne", "nur", "die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Sternen welche dort an blauen Himmels Saal,", "tokens": ["Der", "Ster\u00b7nen", "wel\u00b7che", "dort", "an", "blau\u00b7en", "Him\u00b7mels", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Schimmer vollen Glanz, in Millionen prangen,", "tokens": ["Mit", "Schim\u00b7mer", "vol\u00b7len", "Glanz", ",", "in", "Mil\u00b7lion\u00b7en", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Und nach dem Augenschein, nah bei einander hangen.", "tokens": ["Und", "nach", "dem", "Au\u00b7gen\u00b7schein", ",", "nah", "bei", "ein\u00b7an\u00b7der", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man denke dabei nach, was in der Tieffen Grund,", "tokens": ["Man", "den\u00b7ke", "da\u00b7bei", "nach", ",", "was", "in", "der", "Tief\u00b7fen", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PAV", "PTKVZ", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Durch ein Vergr\u00f6ssrungs Glas uns noch wird wei-", "tokens": ["Durch", "ein", "Ver\u00b7gr\u00f6ss\u00b7rungs", "Glas", "uns", "noch", "wird", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "ADV", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "ter kund;", "tokens": ["ter", "kund", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Und was uns vor ein Heer, noch in der H\u00f6h ver-", "tokens": ["Und", "was", "uns", "vor", "ein", "Heer", ",", "noch", "in", "der", "H\u00f6h", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "stekket,", "tokens": ["stek\u00b7ket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Die noch kein Ferne Glas den Forschern hat ent-", "tokens": ["Die", "noch", "kein", "Fer\u00b7ne", "Glas", "den", "For\u00b7schern", "hat", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "NN", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "dekket.", "tokens": ["dek\u00b7ket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Man sehe auf die Erd, was vor ein Mannigfalt,", "tokens": ["Man", "se\u00b7he", "auf", "die", "Erd", ",", "was", "vor", "ein", "Man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Entwikkelt sich dem Aug, an Art und an Gestalt,", "tokens": ["Ent\u00b7wik\u00b7kelt", "sich", "dem", "Aug", ",", "an", "Art", "und", "an", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Theils an Lebendigen, theils todten Kreaturen,", "tokens": ["Theils", "an", "Le\u00b7ben\u00b7di\u00b7gen", ",", "theils", "tod\u00b7ten", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Auf Bergen, in dem Thal, in W\u00e4ldern, auf den", "tokens": ["Auf", "Ber\u00b7gen", ",", "in", "dem", "Thal", ",", "in", "W\u00e4l\u00b7dern", ",", "auf", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Fluren.", "tokens": ["Flu\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Wie viele Arten sind im Thiere Reich zu sehn,", "tokens": ["Wie", "vie\u00b7le", "Ar\u00b7ten", "sind", "im", "Thie\u00b7re", "Reich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "APPRART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die theils auf Erden sind, theils fliegen in den", "tokens": ["Die", "theils", "auf", "Er\u00b7den", "sind", ",", "theils", "flie\u00b7gen", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "VAFIN", "$,", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "H\u00f6hn:", "tokens": ["H\u00f6hn", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Wer hat aus aller Welt, die V\u00f6gel \u00fcberz\u00e4hlet,", "tokens": ["Wer", "hat", "aus", "al\u00b7ler", "Welt", ",", "die", "V\u00f6\u00b7gel", "\u00fc\u00b7berz\u00b7\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.29": {"text": "w\u00e4hlet.", "tokens": ["w\u00e4h\u00b7let", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Wie mannigfaltig ist, die unterschiedne Art,", "tokens": ["Wie", "man\u00b7nig\u00b7fal\u00b7tig", "ist", ",", "die", "un\u00b7ter\u00b7schied\u00b7ne", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die unter ein Geschlecht, geordnet und verpaart.", "tokens": ["Die", "un\u00b7ter", "ein", "Ge\u00b7schlecht", ",", "ge\u00b7ord\u00b7net", "und", "ver\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie viele Fische sind an Arten und Gestalten,", "tokens": ["Wie", "vie\u00b7le", "Fi\u00b7sche", "sind", "an", "Ar\u00b7ten", "und", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die sich im Meer und See, in Flus und Teich", "tokens": ["Die", "sich", "im", "Meer", "und", "See", ",", "in", "Flus", "und", "Teich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "aufhalten?", "tokens": ["auf\u00b7hal\u00b7ten", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Wie viele Thiere sind, die in dem gr\u00fcnen Wald,", "tokens": ["Wie", "vie\u00b7le", "Thie\u00b7re", "sind", ",", "die", "in", "dem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Berge und Geb\u00fcsch erw\u00e4hlt zum Aufenthalt,", "tokens": ["Die", "Ber\u00b7ge", "und", "Ge\u00b7b\u00fcsch", "er\u00b7w\u00e4hlt", "zum", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die unterschiedlich doch, an ihren Bildungs-Zei-", "tokens": ["Die", "un\u00b7ter\u00b7schied\u00b7lich", "doch", ",", "an", "ih\u00b7ren", "Bil\u00b7dungs\u00b7Zei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "chen,", "tokens": ["chen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Und wegen der Natur nicht v\u00f6llig zu vergleichen?", "tokens": ["Und", "we\u00b7gen", "der", "Na\u00b7tur", "nicht", "v\u00f6l\u00b7lig", "zu", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das sieht man ebenfals, auch an dem zahmen Vieh,", "tokens": ["Das", "sieht", "man", "e\u00b7ben\u00b7fals", ",", "auch", "an", "dem", "zah\u00b7men", "Vieh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das auf dem Anger geht, und mit der s\u00fcssen M\u00fch,", "tokens": ["Das", "auf", "dem", "An\u00b7ger", "geht", ",", "und", "mit", "der", "s\u00fcs\u00b7sen", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Zu unsrer Nahrung sich, im Klee und Grase n\u00e4hret,", "tokens": ["Zu", "uns\u00b7rer", "Nah\u00b7rung", "sich", ",", "im", "Klee", "und", "Gra\u00b7se", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$,", "APPRART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und dadurch unsre Kost uns woll gekocht, bescheret!", "tokens": ["Und", "da\u00b7durch", "uns\u00b7re", "Kost", "uns", "woll", "ge\u00b7kocht", ",", "be\u00b7sche\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Es giebt uns ", "tokens": ["Es", "giebt", "uns"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.45": {"text": "Die er in Milch und Fleisch, uns dadurch zubereit,", "tokens": ["Die", "er", "in", "Milch", "und", "Fleisch", ",", "uns", "da\u00b7durch", "zu\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$,", "PPER", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Auch von verschiedner Art, da\u00df sie verschieden", "tokens": ["Auch", "von", "ver\u00b7schied\u00b7ner", "Art", ",", "da\u00df", "sie", "ver\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "schmekken", "tokens": ["schmek\u00b7ken"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.48": {"text": "Und nicht durch einerlei den Ekel uns erwekken.", "tokens": ["Und", "nicht", "durch", "ei\u00b7ner\u00b7lei", "den", "E\u00b7kel", "uns", "er\u00b7wek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PIS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Man sehe abermahl die vielen B\u00e4ume an,", "tokens": ["Man", "se\u00b7he", "a\u00b7ber\u00b7mahl", "die", "vie\u00b7len", "B\u00e4u\u00b7me", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die man im Pflanzenreich verschiedlich finden kan,", "tokens": ["Die", "man", "im", "Pflan\u00b7zen\u00b7reich", "ver\u00b7schied\u00b7lich", "fin\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "So unterschiedlich sie, sind auch die sch\u00f6nen Fr\u00fcchte,", "tokens": ["So", "un\u00b7ter\u00b7schied\u00b7lich", "sie", ",", "sind", "auch", "die", "sch\u00f6\u00b7nen", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$,", "VAFIN