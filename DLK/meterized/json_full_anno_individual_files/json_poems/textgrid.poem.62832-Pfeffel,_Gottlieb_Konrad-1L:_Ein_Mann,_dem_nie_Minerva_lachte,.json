ns": ["Da\u00df", "du", "in", "dei\u00b7nem", "gan\u00b7zen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht einen Vers mehr machen willst.", "tokens": ["Nicht", "ei\u00b7nen", "Vers", "mehr", "ma\u00b7chen", "willst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er schw\u00f6rt. Ich h\u00e4tte selbst geschworen,", "tokens": ["Er", "schw\u00f6rt", ".", "Ich", "h\u00e4t\u00b7te", "selbst", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Leyer gleich zerst\u00f6rt.", "tokens": ["Und", "mei\u00b7ne", "Le\u00b7yer", "gleich", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nun wird kein M\u00e4cen gebohren,", "tokens": ["Doch", "nun", "wird", "kein", "M\u00e4\u00b7cen", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der so die Kunst zu schweigen lehrt.", "tokens": ["Der", "so", "die", "Kunst", "zu", "schwei\u00b7gen", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Sylla zwar is