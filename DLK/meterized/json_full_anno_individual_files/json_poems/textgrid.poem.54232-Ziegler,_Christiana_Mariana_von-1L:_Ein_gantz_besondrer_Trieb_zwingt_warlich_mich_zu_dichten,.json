en F\u00fcssen steht;", "tokens": ["Wenn", "ja", "dein", "Pe\u00b7ga\u00b7sus", "auf", "lah\u00b7men", "F\u00fcs\u00b7sen", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wolt ihn, glaub es mir, mit eigner Hand verbinden,", "tokens": ["Ich", "wolt", "ihn", ",", "glaub", "es", "mir", ",", "mit", "eig\u00b7ner", "Hand", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil mir dergleichen Fall recht nah zu Hertzen geht.", "tokens": ["Weil", "mir", "derg\u00b7lei\u00b7chen", "Fall", "recht", "nah", "zu", "Hert\u00b7zen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sag, warum wilst du nicht die s\u00fcssen Thone r\u00fchren,", "tokens": ["Sag", ",", "wa\u00b7rum", "wilst", "du", "nicht", "die", "s\u00fcs\u00b7sen", "Tho\u00b7ne", "r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist denn dein Sayten-Spiel so gantz und gar verstimmt?", "tokens": ["Ist", "denn", "dein", "Say\u00b7ten\u00b7Spiel", "so", "gantz", "und", "gar", "ver\u00b7stimmt", "?"], "token_info": ["word", "w