"punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Herr N-- der artge Mann, den ich nicht anders kannte,", "tokens": ["Herr", "N\u00b7", "der", "art\u00b7ge", "Mann", ",", "den", "ich", "nicht", "an\u00b7ders", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.34": {"text": "Als nur dem Rufe nach, nun aber von Person,", "tokens": ["Als", "nur", "dem", "Ru\u00b7fe", "nach", ",", "nun", "a\u00b7ber", "von", "Per\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.35": {"text": "Nach dessen K\u00e4nntni\u00df ich recht vor Verlangen brannte,", "tokens": ["Nach", "des\u00b7sen", "K\u00e4nnt\u00b7ni\u00df", "ich", "recht", "vor", "Ver\u00b7lan\u00b7gen", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn jeder r\u00fchmte mir ihn als Amphions Sohn,", "tokens": ["Denn", "je\u00b7der", "r\u00fchm\u00b7te", "mir", "ihn", "als", "Am\u00b7phi\u00b7ons", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PPER", "KOUS", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der w\u00fcrde, hofft ich, mir von seiner Kunst was weisen;", "tokens": ["Der", "w\u00fcr\u00b7de", ",", "hofft", "ich", ",", "mir", "von", "sei\u00b7ner", "Kunst", "was", "wei\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "APPR", "PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Allein mein Bitten ward mir leider! nicht gew\u00e4hrt", "tokens": ["Al\u00b7lein", "mein", "Bit\u00b7ten", "ward", "mir", "lei\u00b7der", "!", "nicht", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$.", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er war gleich in Begriff schon wieder wegzureisen,", "tokens": ["Er", "war", "gleich", "in", "Be\u00b7griff", "schon", "wie\u00b7der", "weg\u00b7zu\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch hat er gegen mich sich endlich noch erkl\u00e4hrt,", "tokens": ["Doch", "hat", "er", "ge\u00b7gen", "mich", "sich", "end\u00b7lich", "noch", "er\u00b7kl\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nach seiner Ankunfft mir etwas zu \u00fcberschicken,", "tokens": ["Nach", "sei\u00b7ner", "An\u00b7kunfft", "mir", "et\u00b7was", "zu", "\u