{"textgrid.poem.54232": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gantz besondrer Trieb zwingt warlich mich zu dichten,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gantz besondrer Trieb zwingt warlich mich zu dichten,", "tokens": ["Ein", "gantz", "be\u00b7sond\u00b7rer", "Trieb", "zwingt", "war\u00b7lich", "mich", "zu", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Begierde st\u00f6hrt vor di\u00dfmahl meinen Geist,", "tokens": ["Und", "die", "Be\u00b7gier\u00b7de", "st\u00f6hrt", "vor", "di\u00df\u00b7mahl", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum mu\u00df ich dir so gleich, mein werther Freund, berichten,", "tokens": ["Drum", "mu\u00df", "ich", "dir", "so", "gleich", ",", "mein", "wert\u00b7her", "Freund", ",", "be\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df mein Verlangen gro\u00df nach deinen Wohlstand heist.", "tokens": ["Da\u00df", "mein", "Ver\u00b7lan\u00b7gen", "gro\u00df", "nach", "dei\u00b7nen", "Wohl\u00b7stand", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich dencke Tag vor Tag, was mu\u00df Herr N-- machen?", "tokens": ["Ich", "den\u00b7cke", "Tag", "vor", "Tag", ",", "was", "mu\u00df", "Herr", "N\u00b7", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,", "PWS", "VMFIN", "NN", "TRUNC", "VVINF", "$."], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.6": {"text": "Sein Ph\u00f6bus liegt vielleicht mit ihm zugleich auch kranck,", "tokens": ["Sein", "Ph\u00f6\u00b7bus", "liegt", "viel\u00b7leicht", "mit", "ihm", "zu\u00b7gleich", "auch", "kranck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich h\u00f6re nichts von ihm; bey so gestalten Sachen,", "tokens": ["Ich", "h\u00f6\u00b7re", "nichts", "von", "ihm", ";", "bey", "so", "ge\u00b7stal\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$.", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wei\u00df ich dem Musen-Gott wahrhafftig schlechten Danck.", "tokens": ["Wei\u00df", "ich", "dem", "Mu\u00b7sen\u00b7Gott", "wahr\u00b7haff\u00b7tig", "schlech\u00b7ten", "Danck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O k\u00f6nt ich doch ein Kraut und \u00e4chtes Pflaster finden,", "tokens": ["O", "k\u00f6nt", "ich", "doch", "ein", "Kraut", "und", "\u00e4ch\u00b7tes", "Pflas\u00b7ter", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "ART", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ja dein Pegasus auf lahmen F\u00fcssen steht;", "tokens": ["Wenn", "ja", "dein", "Pe\u00b7ga\u00b7sus", "auf", "lah\u00b7men", "F\u00fcs\u00b7sen", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wolt ihn, glaub es mir, mit eigner Hand verbinden,", "tokens": ["Ich", "wolt", "ihn", ",", "glaub", "es", "mir", ",", "mit", "eig\u00b7ner", "Hand", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil mir dergleichen Fall recht nah zu Hertzen geht.", "tokens": ["Weil", "mir", "derg\u00b7lei\u00b7chen", "Fall", "recht", "nah", "zu", "Hert\u00b7zen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sag, warum wilst du nicht die s\u00fcssen Thone r\u00fchren,", "tokens": ["Sag", ",", "wa\u00b7rum", "wilst", "du", "nicht", "die", "s\u00fcs\u00b7sen", "Tho\u00b7ne", "r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist denn dein Sayten-Spiel so gantz und gar verstimmt?", "tokens": ["Ist", "denn", "dein", "Say\u00b7ten\u00b7Spiel", "so", "gantz", "und", "gar", "ver\u00b7stimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Will deine Muse nicht den Helicon mehr zieren?", "tokens": ["Will", "dei\u00b7ne", "Mu\u00b7se", "nicht", "den", "He\u00b7li\u00b7con", "mehr", "zie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann noch ein F\u00fcnckgen Gunst in deinen Hertzen glimmt,", "tokens": ["Wann", "noch", "ein", "F\u00fcnck\u00b7gen", "Gunst", "in", "dei\u00b7nen", "Hert\u00b7zen", "glimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So hilff Apollens Ruhm noch fernerhin besingen,", "tokens": ["So", "hilff", "A\u00b7pol\u00b7lens", "Ruhm", "noch", "fer\u00b7ner\u00b7hin", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du bist sein liebster Sohn, der ihm zur Seiten sitzt,", "tokens": ["Du", "bist", "sein", "liebs\u00b7ter", "Sohn", ",", "der", "ihm", "zur", "Sei\u00b7ten", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn deiner Lieder Schall, die gantz besonders klingen,", "tokens": ["Denn", "dei\u00b7ner", "Lie\u00b7der", "Schall", ",", "die", "gantz", "be\u00b7son\u00b7ders", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Erweisen, da\u00df er dich vor allen andern sch\u00fctzt.", "tokens": ["Er\u00b7wei\u00b7sen", ",", "da\u00df", "er", "dich", "vor", "al\u00b7len", "an\u00b7dern", "sch\u00fctzt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PRF", "APPR", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O la\u00df mich von der Gunst die Brosamen geniessen,", "tokens": ["O", "la\u00df", "mich", "von", "der", "Gunst", "die", "Bro\u00b7sa\u00b7men", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn euch Calliope der Musen Tafel deckt,", "tokens": ["Wenn", "euch", "Cal\u00b7li\u00b7o\u00b7pe", "der", "Mu\u00b7sen", "Ta\u00b7fel", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Du wirst verhoffentlich nicht schon die Freundschafft schliessen,", "tokens": ["Du", "wirst", "ver\u00b7hof\u00b7fent\u00b7lich", "nicht", "schon", "die", "Freund\u00b7schafft", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Durch deine Zuschrifft wird mein Geist recht aufgeweckt.", "tokens": ["Durch", "dei\u00b7ne", "Zu\u00b7schrifft", "wird", "mein", "Geist", "recht", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er liegt ein halbes Jahr und l\u00e4nger noch im Schlummer,", "tokens": ["Er", "liegt", "ein", "hal\u00b7bes", "Jahr", "und", "l\u00e4n\u00b7ger", "noch", "im", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein Schweigen macht, da\u00df mir Hertz, Geist und Muth entf\u00e4llt;", "tokens": ["Dein", "Schwei\u00b7gen", "macht", ",", "da\u00df", "mir", "Hertz", ",", "Geist", "und", "Muth", "ent\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich mache warlich mir dar\u00fcber grossen Kummer,", "tokens": ["Ich", "ma\u00b7che", "war\u00b7lich", "mir", "da\u00b7r\u00fc\u00b7ber", "gros\u00b7sen", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sinne hin und her, was dich zur\u00fccke h\u00e4lt.", "tokens": ["Und", "sin\u00b7ne", "hin", "und", "her", ",", "was", "dich", "zu\u00b7r\u00fc\u00b7cke", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "PWS", "PPER", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ergreiffe deinen Kiel und la\u00df mich balde wissen,", "tokens": ["Er\u00b7greif\u00b7fe", "dei\u00b7nen", "Kiel", "und", "la\u00df", "mich", "bal\u00b7de", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ob du mein Freund annoch wilst heissen und auch seyn,", "tokens": ["Ob", "du", "mein", "Freund", "an\u00b7noch", "wilst", "heis\u00b7sen", "und", "auch", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "KON", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So will ich Hofnungs-voll hier meine Reime schliessen;", "tokens": ["So", "will", "ich", "Hof\u00b7nungs\u00b7voll", "hier", "mei\u00b7ne", "Rei\u00b7me", "schlies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Doch nein! es f\u00e4llt mir noch was zu berichten ein.", "tokens": ["Doch", "nein", "!", "es", "f\u00e4llt", "mir", "noch", "was", "zu", "be\u00b7rich\u00b7ten", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Herr N-- der artge Mann, den ich nicht anders kannte,", "tokens": ["Herr", "N\u00b7", "der", "art\u00b7ge", "Mann", ",", "den", "ich", "nicht", "an\u00b7ders", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.34": {"text": "Als nur dem Rufe nach, nun aber von Person,", "tokens": ["Als", "nur", "dem", "Ru\u00b7fe", "nach", ",", "nun", "a\u00b7ber", "von", "Per\u00b7son", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$,", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.35": {"text": "Nach dessen K\u00e4nntni\u00df ich recht vor Verlangen brannte,", "tokens": ["Nach", "des\u00b7sen", "K\u00e4nnt\u00b7ni\u00df", "ich", "recht", "vor", "Ver\u00b7lan\u00b7gen", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn jeder r\u00fchmte mir ihn als Amphions Sohn,", "tokens": ["Denn", "je\u00b7der", "r\u00fchm\u00b7te", "mir", "ihn", "als", "Am\u00b7phi\u00b7ons", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PPER", "KOUS", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der w\u00fcrde, hofft ich, mir von seiner Kunst was weisen;", "tokens": ["Der", "w\u00fcr\u00b7de", ",", "hofft", "ich", ",", "mir", "von", "sei\u00b7ner", "Kunst", "was", "wei\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PPER", "APPR", "PPOSAT", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Allein mein Bitten ward mir leider! nicht gew\u00e4hrt", "tokens": ["Al\u00b7lein", "mein", "Bit\u00b7ten", "ward", "mir", "lei\u00b7der", "!", "nicht", "ge\u00b7w\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$.", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er war gleich in Begriff schon wieder wegzureisen,", "tokens": ["Er", "war", "gleich", "in", "Be\u00b7griff", "schon", "wie\u00b7der", "weg\u00b7zu\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch hat er gegen mich sich endlich noch erkl\u00e4hrt,", "tokens": ["Doch", "hat", "er", "ge\u00b7gen", "mich", "sich", "end\u00b7lich", "noch", "er\u00b7kl\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "PRF", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Nach seiner Ankunfft mir etwas zu \u00fcberschicken,", "tokens": ["Nach", "sei\u00b7ner", "An\u00b7kunfft", "mir", "et\u00b7was", "zu", "\u00fc\u00b7bersc\u00b7hi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Da\u00df er aufs Clavicin mit eigner Hand gesetzt,", "tokens": ["Da\u00df", "er", "aufs", "Cla\u00b7vi\u00b7cin", "mit", "eig\u00b7ner", "Hand", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wanns Ouverturen sind und starck gesetzte St\u00fccken", "tokens": ["Wanns", "Ou\u00b7ver\u00b7tu\u00b7ren", "sind", "und", "starck", "ge\u00b7setz\u00b7te", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So wird mein Ohr dadurch schon zum voraus erg\u00f6tzt.", "tokens": ["So", "wird", "mein", "Ohr", "da\u00b7durch", "schon", "zum", "vo\u00b7raus", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PAV", "ADV", "APPRART", "ADV", "VVPP", "$."], "meter": "