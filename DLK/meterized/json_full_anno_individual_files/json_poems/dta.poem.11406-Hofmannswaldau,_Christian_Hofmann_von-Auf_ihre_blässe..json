{"dta.poem.11406": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre bl\u00e4sse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der bl\u00e4sse matter schein,", "tokens": ["Der", "bl\u00e4s\u00b7se", "mat\u00b7ter", "schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So Daphnens antlitz eingenommen,", "tokens": ["So", "Daph\u00b7nens", "ant\u00b7litz", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df meines hertzens asche seyn,", "tokens": ["Mu\u00df", "mei\u00b7nes", "hert\u00b7zens", "asc\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das n\u00e4chst durch ihre gluth den letzten rest bekommen.", "tokens": ["Das", "n\u00e4chst", "durch", "ih\u00b7re", "gluth", "den", "letz\u00b7ten", "rest", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun geht das leich-beg\u00e4ngni\u00df an;", "tokens": ["Nun", "geht", "das", "leich\u00b7be\u00b7g\u00e4ng\u00b7ni\u00df", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jedoch ich bin vergn\u00fcgt, weil ich mich r\u00fchmen kan,", "tokens": ["Je\u00b7doch", "ich", "bin", "ver\u00b7gn\u00fcgt", ",", "weil", "ich", "mich", "r\u00fch\u00b7men", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df mein hertze Daphnens sch\u00f6nheit noch zu seiner bahre habe.", "tokens": ["Da\u00df", "mein", "hert\u00b7ze", "Daph\u00b7nens", "sch\u00f6n\u00b7heit", "noch", "zu", "sei\u00b7ner", "bah\u00b7re", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Die liebe selbst bedient sich ihrer blicke pracht,", "tokens": ["Die", "lie\u00b7be", "selbst", "be\u00b7dient", "sich", "ih\u00b7rer", "bli\u00b7cke", "pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und leuchtet ihm in dieser trauer-nacht", "tokens": ["Und", "leuch\u00b7tet", "ihm", "in", "die\u00b7ser", "trau\u00b7e\u00b7rnacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit tausend lichtern itzt zu grabe.", "tokens": ["Mit", "tau\u00b7send", "lich\u00b7tern", "itzt", "zu", "gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}