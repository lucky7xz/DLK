{"textgrid.poem.50495": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "19. Auf Palinur den Staatsmann", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahr ists, dass ", "tokens": ["Wahr", "ists", ",", "dass"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "VAFIN", "$,", "KOUS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Weil mit der sanfften Fluht sein knarrend Ruder spielet;", "tokens": ["Weil", "mit", "der", "sanff\u00b7ten", "Fluht", "sein", "knar\u00b7rend", "Ru\u00b7der", "spie\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein so bald die See des Eols Nohtzwang f\u00fchlet,", "tokens": ["Al\u00b7lein", "so", "bald", "die", "See", "des", "E\u00b7ols", "Noht\u00b7zwang", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und den erhitzten Schaum bis an die Wolcken spritzt;", "tokens": ["Und", "den", "er\u00b7hitz\u00b7ten", "Schaum", "bis", "an", "die", "Wol\u00b7cken", "spritzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn, wie ", "tokens": ["Wenn", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["KOUS", "$,", "PWAV"], "meter": "-+", "measure"