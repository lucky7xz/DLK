{"dta.poem.8474": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wer dich erblickt,", "tokens": ["Wer", "dich", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und deinen Himmel trinket;", "tokens": ["Und", "dei\u00b7nen", "Him\u00b7mel", "trin\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wem dein Gesicht", "tokens": ["Wem", "dein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Voll Engellicht", "tokens": ["Voll", "En\u00b7gel\u00b7licht"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Den Gruss des Friedens winket.", "tokens": ["Den", "Gruss", "des", "Frie\u00b7dens", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein s\u00fcsser Blick,", "tokens": ["Ein", "s\u00fcs\u00b7ser", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein Wink, ein Nick,", "tokens": ["Ein", "Wink", ",", "ein", "Nick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Gl\u00e4nzt mir wie Fr\u00fchlingssonnen;", "tokens": ["Gl\u00e4nzt", "mir", "wie", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ganzen Tag", "tokens": ["Den", "gan\u00b7zen", "Tag"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sinn' ich ihm nach,", "tokens": ["Sinn'", "ich", "ihm", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und schweb' in Himmelswonnen.", "tokens": ["Und", "schweb'", "in", "Him\u00b7mels\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dein holdes Bild", "tokens": ["Dein", "hol\u00b7des", "Bild"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchrt mich so mild", "tokens": ["F\u00fchrt", "mich", "so", "mild"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "An sanfter Blumenkette;", "tokens": ["An", "sanf\u00b7ter", "Blu\u00b7men\u00b7ket\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In meinem Arm", "tokens": ["In", "mei\u00b7nem", "Arm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Erwacht es warm,", "tokens": ["Er\u00b7wacht", "es", "warm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und geht mit mir zu Bette.", "tokens": ["Und", "geht", "mit", "mir", "zu", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Begl\u00fcckt, begl\u00fcckt,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer dich erblickt,", "tokens": ["Wer", "dich", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und deinen Himmel trinket;", "tokens": ["Und", "dei\u00b7nen", "Him\u00b7mel", "trin\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wen s\u00fcsser Blick", "tokens": ["Wen", "s\u00fcs\u00b7ser", "Blick"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Wink und Nick", "tokens": ["Und", "Wink", "und", "Nick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zum s\u00fcssern Kusse winket.", "tokens": ["Zum", "s\u00fcs\u00b7sern", "Kus\u00b7se", "win\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}