{"textgrid.poem.49158": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Christliche Betrachtung des Todes", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie seelig lebt doch der allhier,", "tokens": ["Wie", "see\u00b7lig", "lebt", "doch", "der", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets sich seinen Tod stellt f\u00fcr", "tokens": ["Der", "stets", "sich", "sei\u00b7nen", "Tod", "stellt", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all' sein Thun mi\u00dft nach dem Ende;", "tokens": ["Und", "all'", "sein", "Thun", "mi\u00dft", "nach", "dem", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird die Lust in uns ged\u00e4mpft,", "tokens": ["So", "wird", "die", "Lust", "in", "uns", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird der hohe Muth bek\u00e4mpft;", "tokens": ["So", "wird", "der", "ho\u00b7he", "Muth", "be\u00b7k\u00e4mpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies zieht vom Gut die reichen H\u00e4nde,", "tokens": ["Dies", "zieht", "vom", "Gut", "die", "rei\u00b7chen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Nichts ist, das uns mehr erh\u00e4lt,", "tokens": ["Und", "Nichts", "ist", ",", "das", "uns", "mehr", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df man immer bei sich f\u00e4llt.", "tokens": ["Als", "da\u00df", "man", "im\u00b7mer", "bei", "sich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie schwer will aber dies uns ein!", "tokens": ["Wie", "schwer", "will", "a\u00b7ber", "dies", "uns", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "PDS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben Adern und Gebein,", "tokens": ["Wir", "ha\u00b7ben", "A\u00b7dern", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nichts mehr lieben als ihr Leben.", "tokens": ["Die", "nichts", "mehr", "lie\u00b7ben", "als", "ihr", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Herr, wir sind hierzu zu schlecht,", "tokens": ["Ach", "Herr", ",", "wir", "sind", "hier\u00b7zu", "zu", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "PAV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lehr' du uns diese Kunst doch recht,", "tokens": ["Lehr'", "du", "uns", "die\u00b7se", "Kunst", "doch", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du nur kannst solche Klugheit geben.", "tokens": ["Du", "nur", "kannst", "sol\u00b7che", "Klug\u00b7heit", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "T\u00f6dtst du uns in uns, liebster Gott,", "tokens": ["T\u00f6dtst", "du", "uns", "in", "uns", ",", "liebs\u00b7ter", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So leben wir dir sonder Tod.", "tokens": ["So", "le\u00b7ben", "wir", "dir", "son\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo l\u00e4uft sonst unser Herz hinaus?", "tokens": ["Wo", "l\u00e4uft", "sonst", "un\u00b7ser", "Herz", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sieht auf dein erhabnes Haus,", "tokens": ["Wer", "sieht", "auf", "dein", "er\u00b7hab\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wei\u00df er sich sicher bei der Erden?", "tokens": ["Wei\u00df", "er", "sich", "si\u00b7cher", "bei", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sehnt sich nach der Ewigkeit,", "tokens": ["Wer", "sehnt", "sich", "nach", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang' ihm wol ist bei der Zeit,", "tokens": ["So", "lang'", "ihm", "wol", "ist", "bei", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo er nicht denkt, was er wird werden?", "tokens": ["Wo", "er", "nicht", "denkt", ",", "was", "er", "wird", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00f6nnt' er nur Herr sein von der Welt,", "tokens": ["K\u00f6nnt'", "er", "nur", "Herr", "sein", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liess' er dir gern dein Sternenzelt.", "tokens": ["Liess'", "er", "dir", "gern", "dein", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Giebst du uns aber wei\u00dflich ein,", "tokens": ["Giebst", "du", "uns", "a\u00b7ber", "wei\u00df\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kl\u00e4glich wir geboren sein", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "wir", "ge\u00b7bo\u00b7ren", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unverhofft zum \u00f6fftern scheiden,", "tokens": ["Und", "un\u00b7ver\u00b7hofft", "zum", "\u00f6ff\u00b7tern", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stellst du uns unsre Werke f\u00fcr,", "tokens": ["Stellst", "du", "uns", "uns\u00b7re", "Wer\u00b7ke", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie die verschrieben sind bei dir", "tokens": ["Wie", "die", "ver\u00b7schrie\u00b7ben", "sind", "bei", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was daf\u00fcr wir sollten leiden,", "tokens": ["Und", "was", "da\u00b7f\u00fcr", "wir", "soll\u00b7ten", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, wie besorget, tr\u00fcb' und scheu", "tokens": ["Ach", ",", "wie", "be\u00b7sor\u00b7get", ",", "tr\u00fcb'", "und", "scheu"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "$,", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird unsrer Seele doch hierbei.", "tokens": ["Wird", "uns\u00b7rer", "See\u00b7le", "doch", "hier\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann nehmen wir uns recht in Acht", "tokens": ["Dann", "neh\u00b7men", "wir", "uns", "recht", "in", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sind stets auf den Schlu\u00df bedacht,", "tokens": ["Und", "sind", "stets", "auf", "den", "Schlu\u00df", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den wir hier endlich m\u00fcssen machen,", "tokens": ["Den", "wir", "hier", "end\u00b7lich", "m\u00fcs\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann kommt uns Alles nichtig f\u00fcr,", "tokens": ["Dann", "kommt", "uns", "Al\u00b7les", "nich\u00b7tig", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sind wir Pilgrims nur allhier", "tokens": ["Dann", "sind", "wir", "Pil\u00b7grims", "nur", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und achten Nichts von unsern Sachen", "tokens": ["Und", "ach\u00b7ten", "Nichts", "von", "un\u00b7sern", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehn in unser k\u00fcnftig Grab", "tokens": ["Und", "ziehn", "in", "un\u00b7ser", "k\u00fcnf\u00b7tig", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit unsern Sinnen t\u00e4glich ab.", "tokens": ["Mit", "un\u00b7sern", "Sin\u00b7nen", "t\u00e4g\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, halt' uns stets also bereit", "tokens": ["Herr", ",", "halt'", "uns", "stets", "al\u00b7so", "be\u00b7reit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und lass' uns fertig sein bei Zeit,", "tokens": ["Und", "lass'", "uns", "fer\u00b7tig", "sein", "bei", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VAINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um, wenn du rufest, zu erscheinen;", "tokens": ["Um", ",", "wenn", "du", "ru\u00b7fest", ",", "zu", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es sei dann langsam oder fr\u00fch,", "tokens": ["Es", "sei", "dann", "lang\u00b7sam", "o\u00b7der", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sei auch, wo es sei und wie,", "tokens": ["Es", "sei", "auch", ",", "wo", "es", "sei", "und", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Tod hat etwas an den Deinen.", "tokens": ["Kein", "Tod", "hat", "et\u00b7was", "an", "den", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer eh' stirbt, eh' er sterben soll,", "tokens": ["Wer", "eh'", "stirbt", ",", "eh'", "er", "ster\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Derselbe stirbt allzeit wol.", "tokens": ["Der\u00b7sel\u00b7be", "stirbt", "all\u00b7zeit", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Der Weg ist zwar sehr hart und streng,", "tokens": ["Der", "Weg", "ist", "zwar", "sehr", "hart", "und", "streng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies machet unsrer S\u00fcnden Meng';", "tokens": ["Dies", "ma\u00b7chet", "uns\u00b7rer", "S\u00fcn\u00b7den", "Meng'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weil dein Sohn f\u00fcr uns gestorben,", "tokens": ["Doch", "weil", "dein", "Sohn", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist der Tod kein Tod nicht mehr;", "tokens": ["So", "ist", "der", "Tod", "kein", "Tod", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Grab ist unsre Himmelsf\u00e4hr',", "tokens": ["Sein", "Grab", "ist", "uns\u00b7re", "Him\u00b7mels\u00b7f\u00e4hr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die uns dort liefert unverdorben.", "tokens": ["Die", "uns", "dort", "lie\u00b7fert", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Tod, o ein'ges Gl\u00fcck der Welt,", "tokens": ["O", "Tod", ",", "o", "ein'\u00b7ges", "Gl\u00fcck", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der lebt recht, der an dir sich h\u00e4lt!", "tokens": ["Der", "lebt", "recht", ",", "der", "an", "dir", "sich", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie seelig lebt doch der allhier,", "tokens": ["Wie", "see\u00b7lig", "lebt", "doch", "der", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets sich seinen Tod stellt f\u00fcr", "tokens": ["Der", "stets", "sich", "sei\u00b7nen", "Tod", "stellt", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "PPOSAT", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all' sein Thun mi\u00dft nach dem Ende;", "tokens": ["Und", "all'", "sein", "Thun", "mi\u00dft", "nach", "dem", "En\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wird die Lust in uns ged\u00e4mpft,", "tokens": ["So", "wird", "die", "Lust", "in", "uns", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird der hohe Muth bek\u00e4mpft;", "tokens": ["So", "wird", "der", "ho\u00b7he", "Muth", "be\u00b7k\u00e4mpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dies zieht vom Gut die reichen H\u00e4nde,", "tokens": ["Dies", "zieht", "vom", "Gut", "die", "rei\u00b7chen", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Nichts ist, das uns mehr erh\u00e4lt,", "tokens": ["Und", "Nichts", "ist", ",", "das", "uns", "mehr", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df man immer bei sich f\u00e4llt.", "tokens": ["Als", "da\u00df", "man", "im\u00b7mer", "bei", "sich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie schwer will aber dies uns ein!", "tokens": ["Wie", "schwer", "will", "a\u00b7ber", "dies", "uns", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "PDS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben Adern und Gebein,", "tokens": ["Wir", "ha\u00b7ben", "A\u00b7dern", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die nichts mehr lieben als ihr Leben.", "tokens": ["Die", "nichts", "mehr", "lie\u00b7ben", "als", "ihr", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Herr, wir sind hierzu zu schlecht,", "tokens": ["Ach", "Herr", ",", "wir", "sind", "hier\u00b7zu", "zu", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VAFIN", "PAV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lehr' du uns diese Kunst doch recht,", "tokens": ["Lehr'", "du", "uns", "die\u00b7se", "Kunst", "doch", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du nur kannst solche Klugheit geben.", "tokens": ["Du", "nur", "kannst", "sol\u00b7che", "Klug\u00b7heit", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "T\u00f6dtst du uns in uns, liebster Gott,", "tokens": ["T\u00f6dtst", "du", "uns", "in", "uns", ",", "liebs\u00b7ter", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So leben wir dir sonder Tod.", "tokens": ["So", "le\u00b7ben", "wir", "dir", "son\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wo l\u00e4uft sonst unser Herz hinaus?", "tokens": ["Wo", "l\u00e4uft", "sonst", "un\u00b7ser", "Herz", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sieht auf dein erhabnes Haus,", "tokens": ["Wer", "sieht", "auf", "dein", "er\u00b7hab\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wei\u00df er sich sicher bei der Erden?", "tokens": ["Wei\u00df", "er", "sich", "si\u00b7cher", "bei", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sehnt sich nach der Ewigkeit,", "tokens": ["Wer", "sehnt", "sich", "nach", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang' ihm wol ist bei der Zeit,", "tokens": ["So", "lang'", "ihm", "wol", "ist", "bei", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo er nicht denkt, was er wird werden?", "tokens": ["Wo", "er", "nicht", "denkt", ",", "was", "er", "wird", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "K\u00f6nnt' er nur Herr sein von der Welt,", "tokens": ["K\u00f6nnt'", "er", "nur", "Herr", "sein", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liess' er dir gern dein Sternenzelt.", "tokens": ["Liess'", "er", "dir", "gern", "dein", "Ster\u00b7nen\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Giebst du uns aber wei\u00dflich ein,", "tokens": ["Giebst", "du", "uns", "a\u00b7ber", "wei\u00df\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie kl\u00e4glich wir geboren sein", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "wir", "ge\u00b7bo\u00b7ren", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unverhofft zum \u00f6fftern scheiden,", "tokens": ["Und", "un\u00b7ver\u00b7hofft", "zum", "\u00f6ff\u00b7tern", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stellst du uns unsre Werke f\u00fcr,", "tokens": ["Stellst", "du", "uns", "uns\u00b7re", "Wer\u00b7ke", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie die verschrieben sind bei dir", "tokens": ["Wie", "die", "ver\u00b7schrie\u00b7ben", "sind", "bei", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "VVPP", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was daf\u00fcr wir sollten leiden,", "tokens": ["Und", "was", "da\u00b7f\u00fcr", "wir", "soll\u00b7ten", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, wie besorget, tr\u00fcb' und scheu", "tokens": ["Ach", ",", "wie", "be\u00b7sor\u00b7get", ",", "tr\u00fcb'", "und", "scheu"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "$,", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird unsrer Seele doch hierbei.", "tokens": ["Wird", "uns\u00b7rer", "See\u00b7le", "doch", "hier\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dann nehmen wir uns recht in Acht", "tokens": ["Dann", "neh\u00b7men", "wir", "uns", "recht", "in", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sind stets auf den Schlu\u00df bedacht,", "tokens": ["Und", "sind", "stets", "auf", "den", "Schlu\u00df", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den wir hier endlich m\u00fcssen machen,", "tokens": ["Den", "wir", "hier", "end\u00b7lich", "m\u00fcs\u00b7sen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann kommt uns Alles nichtig f\u00fcr,", "tokens": ["Dann", "kommt", "uns", "Al\u00b7les", "nich\u00b7tig", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADJD", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann sind wir Pilgrims nur allhier", "tokens": ["Dann", "sind", "wir", "Pil\u00b7grims", "nur", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und achten Nichts von unsern Sachen", "tokens": ["Und", "ach\u00b7ten", "Nichts", "von", "un\u00b7sern", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehn in unser k\u00fcnftig Grab", "tokens": ["Und", "ziehn", "in", "un\u00b7ser", "k\u00fcnf\u00b7tig", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit unsern Sinnen t\u00e4glich ab.", "tokens": ["Mit", "un\u00b7sern", "Sin\u00b7nen", "t\u00e4g\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Herr, halt' uns stets also bereit", "tokens": ["Herr", ",", "halt'", "uns", "stets", "al\u00b7so", "be\u00b7reit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und lass' uns fertig sein bei Zeit,", "tokens": ["Und", "lass'", "uns", "fer\u00b7tig", "sein", "bei", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VAINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um, wenn du rufest, zu erscheinen;", "tokens": ["Um", ",", "wenn", "du", "ru\u00b7fest", ",", "zu", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "$,", "KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es sei dann langsam oder fr\u00fch,", "tokens": ["Es", "sei", "dann", "lang\u00b7sam", "o\u00b7der", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sei auch, wo es sei und wie,", "tokens": ["Es", "sei", "auch", ",", "wo", "es", "sei", "und", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Tod hat etwas an den Deinen.", "tokens": ["Kein", "Tod", "hat", "et\u00b7was", "an", "den", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wer eh' stirbt, eh' er sterben soll,", "tokens": ["Wer", "eh'", "stirbt", ",", "eh'", "er", "ster\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Derselbe stirbt allzeit wol.", "tokens": ["Der\u00b7sel\u00b7be", "stirbt", "all\u00b7zeit", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Der Weg ist zwar sehr hart und streng,", "tokens": ["Der", "Weg", "ist", "zwar", "sehr", "hart", "und", "streng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies machet unsrer S\u00fcnden Meng';", "tokens": ["Dies", "ma\u00b7chet", "uns\u00b7rer", "S\u00fcn\u00b7den", "Meng'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch weil dein Sohn f\u00fcr uns gestorben,", "tokens": ["Doch", "weil", "dein", "Sohn", "f\u00fcr", "uns", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist der Tod kein Tod nicht mehr;", "tokens": ["So", "ist", "der", "Tod", "kein", "Tod", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Grab ist unsre Himmelsf\u00e4hr',", "tokens": ["Sein", "Grab", "ist", "uns\u00b7re", "Him\u00b7mels\u00b7f\u00e4hr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die uns dort liefert unverdorben.", "tokens": ["Die", "uns", "dort", "lie\u00b7fert", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O Tod, o ein'ges Gl\u00fcck der Welt,", "tokens": ["O", "Tod", ",", "o", "ein'\u00b7ges", "Gl\u00fcck", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der lebt recht, der an dir sich h\u00e4lt!", "tokens": ["Der", "lebt", "recht", ",", "der", "an", "dir", "sich", "h\u00e4lt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$."], "mete