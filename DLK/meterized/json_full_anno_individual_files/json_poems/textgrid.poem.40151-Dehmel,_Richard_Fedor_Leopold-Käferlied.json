{"textgrid.poem.40151": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "K\u00e4ferlied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maik\u00e4fer, surr,", "tokens": ["Mai\u00b7k\u00e4\u00b7fer", ",", "surr", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "bleib du sitzen nur!", "tokens": ["bleib", "du", "sit\u00b7zen", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Breite deine F\u00fchler aus,", "tokens": ["Brei\u00b7te", "dei\u00b7ne", "F\u00fch\u00b7ler", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mach zwei kleine F\u00e4cher draus,", "tokens": ["mach", "zwei", "klei\u00b7ne", "F\u00e4\u00b7cher", "draus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "schwing sie hin und her,", "tokens": ["schwing", "sie", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "z\u00e4hle mi wat v\u00f6r!", "tokens": ["z\u00e4h\u00b7le", "mi", "wat", "v\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.7": {"text": "Z\u00e4hle: ich will mit dir z\u00e4hlen,", "tokens": ["Z\u0