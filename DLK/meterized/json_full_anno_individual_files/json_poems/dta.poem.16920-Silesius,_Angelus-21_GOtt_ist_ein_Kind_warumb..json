{"dta.poem.16920": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "21  GOtt ist ein Kind/ warumb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ewge GOttes Sohn wird heut erst Kind genennt/", "tokens": ["Der", "Ew\u00b7ge", "Got\u00b7tes", "Sohn", "wird", "heut", "erst", "Kind", "ge\u00b7nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ADV", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da Er doch tausend Jahr den Vatter schon gekennt:", "tokens": ["Da", "Er", "doch", "tau\u00b7send", "Jahr", "den", "Vat\u00b7ter", "schon", "ge\u00b7kennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "CARD", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warumb? Er war nie Kind. Die Mutter machts allein", "tokens": ["Wa\u00b7rumb", "?", "Er", "war", "nie", "Kind", ".", "Die", "Mut\u00b7ter", "machts", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PPER", "VAFIN", "ADV", "NN", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Er warhafftiglich kan Kind gegr\u00fcsset seyn.", "tokens": ["Da\u00df", "Er", "war\u00b7haff\u00b7tig\u00b7lich", "kan", "Kind", "ge\u00b7gr\u00fcs\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "