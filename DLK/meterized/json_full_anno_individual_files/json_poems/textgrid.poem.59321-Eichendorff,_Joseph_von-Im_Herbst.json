{"textgrid.poem.59321": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Wald wird falb, die Bl\u00e4tter fallen,", "tokens": ["Der", "Wald", "wird", "falb", ",", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie \u00f6d und still der Raum!", "tokens": ["Wie", "\u00f6d", "und", "still", "der", "Raum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00e4chlein nur gehn durch die Buchenhallen", "tokens": ["Die", "B\u00e4ch\u00b7lein", "nur", "gehn", "durch", "die", "Bu\u00b7chen\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lind rauschend wie im Traum,", "tokens": ["Lind", "rau\u00b7schend", "wie", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und Abendglocken schallen", "tokens": ["Und", "A\u00b7bend\u00b7glo\u00b7cken", "schal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fern von des Waldes Saum.", "tokens": ["Fern", "von", "des", "Wal\u00b7des", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Was wollt ihr mich so wild verlocken", "tokens": ["Was", "wollt", "ihr", "mich", "so", "wild", "ver\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Einsamkeit?", "tokens": ["In", "die\u00b7ser", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie in der Heimat klingen diese Glocken", "tokens": ["Wie", "in", "der", "Hei\u00b7mat", "klin\u00b7gen", "die\u00b7se", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus stiller Kinderzeit \u2013", "tokens": ["Aus", "stil\u00b7ler", "Kin\u00b7der\u00b7zeit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wende mich erschrocken", "tokens": ["Ich", "wen\u00b7de", "mich", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, was mich liebt, ist weit!", "tokens": ["Ach", ",", "was", "mich", "liebt", ",", "ist", "weit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So brecht hervor nur, alte Lieder,", "tokens": ["So", "brec