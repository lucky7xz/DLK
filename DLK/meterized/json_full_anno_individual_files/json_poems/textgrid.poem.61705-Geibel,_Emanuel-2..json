{"textgrid.poem.61705": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spielende Flammen hoffnungsloser Liebe,", "tokens": ["Spie\u00b7len\u00b7de", "Flam\u00b7men", "hoff\u00b7nungs\u00b7lo\u00b7ser", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Was lockt ihr mich und z\u00fcngelt ohne Ruh'?", "tokens": ["Was", "lockt", "ihr", "mich", "und", "z\u00fcn\u00b7gelt", "oh\u00b7ne", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bezwungen strebt vom t\u00f6dlich s\u00fc\u00dfen Triebe", "tokens": ["Be\u00b7zwun\u00b7gen", "strebt", "vom", "t\u00f6d\u00b7lich", "s\u00fc\u00b7\u00dfen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Herz euch zu.", "tokens": ["Dies", "Herz", "euch", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wohl kennt es euer tr\u00fcgerisch Gefunkel", "tokens": ["Wohl", "kennt", "es", "eu\u00b7er", "tr\u00fc\u00b7ge\u00b7risch", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und glaubt der schmeichelnden V