{"textgrid.poem.56605": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Son P\u00fcsseln un Garneern un Arfen planten", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Son P\u00fcsseln un Garneern un Arfen planten", "tokens": ["Son", "P\u00fcs\u00b7seln", "un", "Gar\u00b7neern", "un", "Ar\u00b7fen", "plan\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "FM", "FM", "FM", "NN", "VVFIN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Dat gift en rechte Seel- un Arbeitsruh.", "tokens": ["Dat", "gift", "en", "rech\u00b7te", "Seel", "un", "Ar\u00b7beits\u00b7ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "De Platz an Tun hentlank is doch de beste,", "tokens": ["De", "Platz", "an", "Tun", "hent\u00b7lank", "is", "doch", "de", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "FM", "FM", "FM", "KON", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "De hett de frie warme Morgens\u00fcnn.", "tokens": ["De", "hett", "de", "frie", "war\u00b7me", "Mor\u00b7gen\u00b7s\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "En beten rop dar gift dat Hus al Schatten", "tokens": ["En", "be\u00b7ten", "rop", "dar", "gift", "dat", "Hus", "al", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "PTKVZ", "VVFIN", "ART", "NE", "NE", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Un ok de Eschen baben \u0153werhin.", "tokens": ["Un", "ok", "de", "E\u00b7schen", "ba\u00b7ben", "\u0153wer\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wa is de wussen! Dat is rein en K\u0119rl,", "tokens": ["Wa", "is", "de", "wus\u00b7sen", "!", "Dat", "is", "rein", "en", "K\u0119rl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$.", "FM", "FM", "FM", "FM", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "En breden Sleef \u2013 un schier! un wat v\u0153rn H\u00f6chde!", "tokens": ["En", "bre\u00b7den", "Sleef", "\u2013", "un", "schier", "!", "un", "wat", "v\u0153rn", "H\u00f6ch\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "FM", "FM", "$.", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "De Tun ward ok to breet, de mutt mal knippt warrn,", "tokens": ["De", "Tun", "ward", "ok", "to", "breet", ",", "de", "mutt", "mal", "knippt", "warrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "FM", "FM", "FM", "$,", "NE", "ADJD", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "He's ok to hoch v\u0153r Greten mit \u0119r T\u00fcg.", "tokens": ["He's", "ok", "to", "hoch", "v\u0153r", "Gre\u00b7ten", "mit", "\u0119r", "T\u00fcg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wa hett dat Unkrut \u00fcnner D\u0119g un Tier!", "tokens": ["Wa", "hett", "dat", "Un\u00b7krut", "\u00fcn\u00b7ner", "D\u0119g", "un", "Tier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJA", "NN", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Brennetteln? t\u00f6f, dat Deert! ik will di krigen!", "tokens": ["Bren\u00b7net\u00b7teln", "?", "t\u00f6f", ",", "dat", "Deert", "!", "ik", "will", "di", "kri\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "$,", "PRELS", "NE", "$.", "NE", "VMFIN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Un all dat anner T\u00fcs mit lange Wutteln,", "tokens": ["Un", "all", "dat", "an\u00b7ner", "T\u00fcs", "mit", "lan\u00b7ge", "Wut\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Al all de K\u00f6pp herut \u2013 un Quitsch un Qu\u0119ken!", "tokens": ["Al", "all", "de", "K\u00f6pp", "he\u00b7rut", "\u2013", "un", "Quitsch", "un", "Qu\u0119ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "FM", "FM", "FM", "$(", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Son Judenschol! \u2013 wa hett dat Lust to wassen!", "tokens": ["Son", "Ju\u00b7den\u00b7schol", "!", "\u2013", "wa", "hett", "dat", "Lust", "to", "was\u00b7sen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "$(", "XY", "VAFIN", "ART", "NN", "NE", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.16": {"text": "Dats rech en Irrgarn ... kikt se ni herut", "tokens": ["Dats", "rech", "en", "Irr\u00b7garn", "...", "kikt", "se", "ni", "he\u00b7rut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "FM.fr", "FM.fr", "FM.fr", "FM.fr"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit blanke K\u00f6pp, as h\u00f6rn se mit dermank?", "tokens": ["Mit", "blan\u00b7ke", "K\u00f6pp", ",", "as", "h\u00f6rn", "se", "mit", "der\u00b7mank", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wulln ok mal tosehn, ob de S\u00fcnn noch schin?", "tokens": ["Wulln", "ok", "mal", "to\u00b7sehn", ",", "ob", "de", "S\u00fcnn", "noch", "schin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VVINF", "$,", "KOUS", "NE", "NE", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Un H\u00f6hnerswark \u2013 man los! \u2013 en F\u00f6r to Tid!", "tokens": ["Un", "H\u00f6h\u00b7ners\u00b7wark", "\u2013", "man", "los", "!", "\u2013", "en", "F\u00f6r", "to", "Tid", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PIS", "PTKVZ", "$.", "$(", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "En arm Kastanje rein dermank versn\u0119rt!", "tokens": ["En", "arm", "Kas\u00b7tan\u00b7je", "rein", "der\u00b7mank", "ver\u00b7sn\u0119rt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "De sch\u00fcll j\u00fcm doch ni ...", "tokens": ["De", "sch\u00fcll", "j\u00fcm", "doch", "ni", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "\u00bbguden Morgen, Nachbar!\u00ab", "tokens": ["\u00bb", "gu\u00b7den", "Mor\u00b7gen", ",", "Nach\u00b7bar", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "De Ole rich sik langsam inne H\u00f6ch", "tokens": ["De", "O\u00b7le", "rich", "sik", "lang\u00b7sam", "in\u00b7ne", "H\u00f6ch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Un wisch den Sweet vun N\u00e4s un Pockennarben \u2013", "tokens": ["Un", "wisch", "den", "Sweet", "vun", "N\u00e4s", "un", "Po\u00b7cken\u00b7nar\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.25": {"text": "\u00bbgun Morn Herr Paster! dats en V\u0153rjahrsdag!", "tokens": ["\u00bb", "gun", "Morn", "Herr", "Pas\u00b7ter", "!", "dats", "en", "V\u0153r\u00b7jahrs\u00b7dag", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "NN", "NN", "$.", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "So mutt dat w\u0119n, dat is al redi warm!\u00ab", "tokens": ["So", "mutt", "dat", "w\u0119n", ",", "dat", "is", "al", "re\u00b7di", "warm", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Herr Paster l\u0119 de Ellbagn op den Tun", "tokens": ["Herr", "Pas\u00b7ter", "l\u0119", "de", "Ell\u00b7bagn", "op", "den", "Tun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "NE", "NE", "NE", "NE", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Un keek na alle Kanten in den Garn.", "tokens": ["Un", "keek", "na", "al\u00b7le", "Kan\u00b7ten", "in", "den", "Garn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "De Tulken keemn al op un Osterbl\u00f6m,", "tokens": ["De", "Tul\u00b7ken", "ke\u00b7emn", "al", "op", "un", "Os\u00b7ter\u00b7bl\u00f6m", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "De Stickb\u0119rnb\u00fcscher harrn al gr\u00f6ne Bl\u0153d,", "tokens": ["De", "Stick\u00b7b\u0119rn\u00b7b\u00fc\u00b7scher", "harrn", "al", "gr\u00f6\u00b7ne", "Bl\u0153d", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Doch heeln de Im sik an de P\u0119perbl\u00f6m,", "tokens": ["Doch", "heeln", "de", "Im", "sik", "an", "de", "P\u0119\u00b7per\u00b7bl\u00f6m", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "APPRART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Un sachden keem de eerste Bottervagel", "tokens": ["Un", "sach\u00b7den", "keem", "de", "e\u00b7ers\u00b7te", "Bot\u00b7ter\u00b7va\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.33": {"text": "So lisen as en Blatt, un blank as Gold,", "tokens": ["So", "li\u00b7sen", "as", "en", "Blatt", ",", "un", "blank", "as", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "NN", "$,", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Vunt Hus her\u0153wer inne helle S\u00fcnn,", "tokens": ["Vunt", "Hus", "her\u0153wer", "in\u00b7ne", "hel\u00b7le", "S\u00fcnn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.35": {"text": "As keem he vun den Eschenbom hendal.", "tokens": ["As", "keem", "he", "vun", "den", "E\u00b7schen\u00b7bom", "hen\u00b7dal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Doch seet he kum, so keem en anner Fleerlink", "tokens": ["Doch", "seet", "he", "kum", ",", "so", "keem", "en", "an\u00b7ner", "Fleer\u00b7link"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "$,", "ADV", "ADJA", "FM", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "D\u0153rt Hus hend\u0153r in vuller Fahrt un Hitten.", "tokens": ["D\u0153rt", "Hus", "hen\u00b7d\u0153r", "in", "vul\u00b7ler", "Fahrt", "un", "Hit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "De Parlen stunn' em hell um Mund un V\u0153rkopp,", "tokens": ["De", "Par\u00b7len", "stunn'", "em", "hell", "um", "Mund", "un", "V\u0153r\u00b7kopp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Un um de Schullern slunkern g\u0119le Haar.", "tokens": ["Un", "um", "de", "Schul\u00b7lern", "slun\u00b7kern", "g\u0119le", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "De seeg ut blaue Ogen wild herum,", "tokens": ["De", "seeg", "ut", "blau\u00b7e", "O\u00b7gen", "wild", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "De M\u00fctz in Hand, he s\u00f6ch wul na de anner.", "tokens": ["De", "M\u00fctz", "in", "Hand", ",", "he", "s\u00f6ch", "wul", "na", "de", "an\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "\u00bbwa hett \u0118r Heinri dar en Jung so smuck", "tokens": ["\u00bb", "wa", "hett", "\u0118r", "Hein\u00b7ri", "dar", "en", "Jung", "so", "smuck"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "As Melk un Blot,\u00ab so seggt Herr Paster lisen:", "tokens": ["As", "Melk", "un", "Blot", ",", "\u00ab", "so", "seggt", "Herr", "Pas\u00b7ter", "li\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "FM", "NN", "$,", "$(", "ADV", "VVFIN", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "\u00bbde's j\u00fcs so old as min Maria \u0119r.", "tokens": ["\u00bb", "de's", "j\u00fcs", "so", "old", "as", "min", "Ma\u00b7ria", "\u0119r", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "To Pingsten kumt se mal mitsams \u0119r Docter.", "tokens": ["To", "Pings\u00b7ten", "kumt", "se", "mal", "mit\u00b7sams", "\u0119r", "Doc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Dat ward en L