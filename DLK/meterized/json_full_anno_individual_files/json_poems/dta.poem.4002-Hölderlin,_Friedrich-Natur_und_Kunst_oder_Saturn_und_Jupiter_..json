{"dta.poem.4002": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Kunst  \n oder  \n Saturn und Jupiter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du waltest hoch am Tag' und es bl\u00fchet Dein", "tokens": ["Du", "wal\u00b7test", "hoch", "am", "Tag'", "und", "es", "bl\u00fc\u00b7het", "Dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "KON", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gesetz, Du h\u00e4ltst die Wage, Saturnus Sohn!", "tokens": ["Ge\u00b7setz", ",", "Du", "h\u00e4ltst", "die", "Wa\u00b7ge", ",", "Sa\u00b7tur\u00b7nus", "Sohn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und theilst die Loos' und ruhest froh im", "tokens": ["Und", "theilst", "die", "Loos'", "und", "ru\u00b7hest", "froh", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADJD", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruhm der unsterblichen Herrscherk\u00fcnste.", "tokens": ["Ruhm", "der", "uns\u00b7terb\u00b7li\u00b7chen", "Herr\u00b7scher\u00b7k\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Doch in den Abgrund, sagen die S\u00e4nger sich,", "tokens": ["Doch", "in", "den", "Ab\u00b7grund", ",", "sa\u00b7gen", "die", "S\u00e4n\u00b7ger", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Habst Du den heil'gen Vater, den eignen, einst", "tokens": ["Habst", "Du", "den", "heil'\u00b7gen", "Va\u00b7ter", ",", "den", "eig\u00b7nen", ",", "einst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verwiesen und es jammern drunten,", "tokens": ["Ver\u00b7wie\u00b7sen", "und", "es", "jam\u00b7mern", "drun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da, wo die Wilden vor Dir mit Recht sind,", "tokens": ["Da", ",", "wo", "die", "Wil\u00b7den", "vor", "Dir", "mit", "Recht", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schuldlos der Gott der goldenen Zeit schon l\u00e4ngst,", "tokens": ["Schuld\u00b7los", "der", "Gott", "der", "gol\u00b7de\u00b7nen", "Zeit", "schon", "l\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "ADV", "ADV", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Einst m\u00fchelos, und gr\u00f6\u00dfer, wie Du, wenn schon", "tokens": ["Einst", "m\u00fc\u00b7he\u00b7los", ",", "und", "gr\u00f6\u00b7\u00dfer", ",", "wie", "Du", ",", "wenn", "schon"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADJD", "$,", "PWAV", "PPER", "$,", "KOUS", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er kein Gebot aussprach und ihn der", "tokens": ["Er", "kein", "Ge\u00b7bot", "aus\u00b7sprach", "und", "ihn", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "KON", "PPER", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sterblichen Keiner mit Namen nannte.", "tokens": ["Sterb\u00b7li\u00b7chen", "Kei\u00b7ner", "mit", "Na\u00b7men", "nann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Herab denn! oder sch\u00e4me des Danks Dich nicht!", "tokens": ["Her\u00b7ab", "denn", "!", "o\u00b7der", "sch\u00e4\u00b7me", "des", "Danks", "Dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und willst Du bleiben, diene dem Aelteren", "tokens": ["Und", "willst", "Du", "blei\u00b7ben", ",", "die\u00b7ne", "dem", "A\u00b7el\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und g\u00f6nn' es ihm, da\u00df ihn vor Allen,", "tokens": ["Und", "g\u00f6nn'", "es", "ihm", ",", "da\u00df", "ihn", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00f6ttern und Menschen, der S\u00e4nger nenne!", "tokens": ["G\u00f6t\u00b7tern", "und", "Men\u00b7schen", ",", "der", "S\u00e4n\u00b7ger", "nen\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+--+--++--", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Denn, wie aus dem Gew\u00f6lke Dein Blitz, so kommt", "tokens": ["Denn", ",", "wie", "aus", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "Dein", "Blitz", ",", "so", "kommt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ADV", "VVFIN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von ihm, was Dein ist, siehe! so zeugt von ihm,", "tokens": ["Von", "ihm", ",", "was", "Dein", "ist", ",", "sie\u00b7he", "!", "so", "zeugt", "von", "ihm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPOSAT", "VAFIN", "$,", "VVIMP", "$.", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Was Du gebeutst, und aus Saturnus", "tokens": ["Was", "Du", "ge\u00b7beutst", ",", "und", "aus", "Sa\u00b7tur\u00b7nus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frieden ist jegliche Macht erwachsen.", "tokens": ["Frie\u00b7den", "ist", "jeg\u00b7li\u00b7che", "Macht", "er\u00b7wach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Und hab' ich erst am Herzen Lebendiges", "tokens": ["Und", "hab'", "ich", "erst", "am", "Her\u00b7zen", "Le\u00b7ben\u00b7di\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00fchlt und d\u00e4mmert, was Du gestaltetest.", "tokens": ["Ge\u00b7f\u00fchlt", "und", "d\u00e4m\u00b7mert", ",", "was", "Du", "ge\u00b7stal\u00b7te\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und war in ihrer Wiege mir in", "tokens": ["Und", "war", "in", "ih\u00b7rer", "Wie\u00b7ge", "mir", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "PPER", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wonne die wechselnde Zeit entschlummert:", "tokens": ["Won\u00b7ne", "die", "wech\u00b7seln\u00b7de", "Zeit", "ent\u00b7schlum\u00b7mert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Dann kenn' ich Dich, Kronion, dann h\u00f6r' ich Dich,", "tokens": ["Dann", "kenn'", "ich", "Dich", ",", "Kro\u00b7ni\u00b7on", ",", "dann", "h\u00f6r'", "ich", "Dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NE", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den weisen Meister, welcher, wie wir, ein Sohn", "tokens": ["Den", "wei\u00b7sen", "Meis\u00b7ter", ",", "wel\u00b7cher", ",", "wie", "wir", ",", "ein", "Sohn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Zeit, Gesetze giebt und, was die", "tokens": ["Der", "Zeit", ",", "Ge\u00b7set\u00b7ze", "giebt", "und", ",", "was", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "KON", "$,", "PRELS", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heilige D\u00e4mmerung birgt, verk\u00fcndet.", "tokens": ["Hei\u00b7li\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "birgt", ",", "ver\u00b7k\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}