{"dta.poem.449": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie zur Straffe k\u00fcssen muste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.85", "cy:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Florinde soll der Ku\u00df mir eine Straffe seyn?", "tokens": ["Flo\u00b7rin\u00b7de", "soll", "der", "Ku\u00df", "mir", "ei\u00b7ne", "Straf\u00b7fe", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werd\u2019 ich immerfort nach solcher Straffe ringen/", "tokens": ["So", "werd'", "ich", "im\u00b7mer\u00b7fort", "nach", "sol\u00b7cher", "Straf\u00b7fe", "rin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich werd\u2019 sonst nichtes nicht als was man strafft vollbringen/", "tokens": ["Ich", "werd'", "sonst", "nich\u00b7tes", "nicht", "als", "was", "man", "strafft", "voll\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PTKNEG", "KOUS", "PRELS", "PIS", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn diese Straffe geht mir allzulieblich ein.", "tokens": ["Denn", "die\u00b7se", "Straf\u00b7fe", "geht", "mir", "all\u00b7zu\u00b7lieb\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein mein Mund will sich mit einem nicht vergn\u00fcgen/", "tokens": ["Al\u00b7lein", "mein", "Mund", "will", "sich", "mit", "ei\u00b7nem", "nicht", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "PRF", "APPR", "ART", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich nehm die Straffe an/ solt\u2019 ich gleich tausend kriegen.", "tokens": ["Ich", "nehm", "die", "Straf\u00b7fe", "an", "/", "solt'", "ich", "gleich", "tau\u00b7send", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}