{"dta.poem.6966": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein pl\u00f6tzlich Uebel w\u00fcrckt ein Ohnmacht offtermahlen:", "tokens": ["Ein", "pl\u00f6tz\u00b7lich", "Ue\u00b7bel", "w\u00fcrckt", "ein", "Ohn\u00b7macht", "off\u00b7ter\u00b7mah\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann l\u00e4sset es, als w\u00e4ren tausend Strahlen", "tokens": ["Dann", "l\u00e4s\u00b7set", "es", ",", "als", "w\u00e4\u00b7ren", "tau\u00b7send", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von der Vernunfft, die wir von oben haben,", "tokens": ["Von", "der", "Ver\u00b7nunfft", ",", "die", "wir", "von", "o\u00b7ben", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jm Schutt des C\u00f6rpers mit vergraben.", "tokens": ["Jm", "Schutt", "des", "C\u00f6r\u00b7pers", "mit", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch verlieret sie ihr wahres Wesen nicht,", "tokens": ["Je\u00b7doch", "ver\u00b7lie\u00b7ret", "sie", "ihr", "wah\u00b7res", "We\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und gleicht so dann des Himmels Licht,", "tokens": ["Und", "gleicht", "so", "dann", "des", "Him\u00b7mels", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn schwartzer Wolcken Dufft es unsern Augen raubet.", "tokens": ["Wenn", "schwart\u00b7zer", "Wol\u00b7cken", "Dufft", "es", "un\u00b7sern", "Au\u00b7gen", "rau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie, wenn der wilde Nord entsetzlich schnaubet,", "tokens": ["Wie", ",", "wenn", "der", "wil\u00b7de", "Nord", "ent\u00b7setz\u00b7lich", "schnau\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und unter schwartzen Dunst den lichten Tag begr\u00e4bt;", "tokens": ["Und", "un\u00b7ter", "schwart\u00b7zen", "Dunst", "den", "lich\u00b7ten", "Tag", "be\u00b7gr\u00e4bt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn aus des Meeres Schoo\u00df und schlipffrichten Mor\u00e4sten,", "tokens": ["Wenn", "aus", "des", "Mee\u00b7res", "Schoo\u00df", "und", "schlipf\u00b7frich\u00b7ten", "Mo\u00b7r\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ein dicker Nebel sich erhebt,", "tokens": ["Ein", "di\u00b7cker", "Ne\u00b7bel", "sich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und da\u00df der gantze Kreis der Vesten", "tokens": ["Und", "da\u00df", "der", "gant\u00b7ze", "Kreis", "der", "Ves\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Voll dicker Finsternissen schwebt,", "tokens": ["Voll", "di\u00b7cker", "Fins\u00b7ter\u00b7nis\u00b7sen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "Der fast die Schrecken-Grufft der H\u00f6llen", "tokens": ["Der", "fast", "die", "Schre\u00b7cken\u00b7G\u00b7rufft", "der", "H\u00f6l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Scheint vorzustellen,", "tokens": ["Scheint", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVIZU", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und aller Dinge Form vermischt, verwirrt, verschrenckt,", "tokens": ["Und", "al\u00b7ler", "Din\u00b7ge", "Form", "ver\u00b7mischt", ",", "ver\u00b7wirrt", ",", "ver\u00b7schrenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVPP", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und sie in einen Chaos senckt;", "tokens": ["Und", "sie", "in", "ei\u00b7nen", "Chaos", "senckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Spricht jemand wol mit Recht deswegen;", "tokens": ["Spricht", "je\u00b7mand", "wol", "mit", "Recht", "des\u00b7we\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die Sonne selber ist nicht mehr zugegen?", "tokens": ["Die", "Son\u00b7ne", "sel\u00b7ber", "ist", "nicht", "mehr", "zu\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}