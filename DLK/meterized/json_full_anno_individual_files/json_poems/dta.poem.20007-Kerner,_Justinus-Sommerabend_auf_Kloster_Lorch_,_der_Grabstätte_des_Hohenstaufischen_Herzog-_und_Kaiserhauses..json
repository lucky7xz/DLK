{"dta.poem.20007": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sommerabend  \n auf Kloster  Lorch , der Grabst\u00e4tte des Hohenstaufischen Herzog- und  \n Kaiserhauses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach mildem Abendregen", "tokens": ["Nach", "mil\u00b7dem", "A\u00b7bend\u00b7re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die L\u00fcfte k\u00fchlend weh'n;", "tokens": ["Die", "L\u00fcf\u00b7te", "k\u00fch\u00b7lend", "weh'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Landes reicher Seegen", "tokens": ["Des", "Lan\u00b7des", "rei\u00b7cher", "See\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dampft auf zu blauen H\u00f6h'n.", "tokens": ["Dampft", "auf", "zu", "blau\u00b7en", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Duft kommt herangezogen", "tokens": ["Duft", "kommt", "her\u00b7an\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Blumen, Kr\u00e4utern gr\u00fcn,", "tokens": ["Von", "Blu\u00b7men", ",", "Kr\u00e4u\u00b7tern", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die unter goldnen Wogen", "tokens": ["Die", "un\u00b7ter", "gold\u00b7nen", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Aehrenfeld's erbl\u00fch'n.", "tokens": ["Des", "A\u00b7eh\u00b7ren\u00b7feld's", "er\u00b7bl\u00fch'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es rauschen durch die Stille", "tokens": ["Es", "rau\u00b7schen", "durch", "die", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Aehren, voll und schwer.", "tokens": ["Die", "A\u00b7eh\u00b7ren", ",", "voll", "und", "schwer", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Wald in \u00fcpp'ger F\u00fclle", "tokens": ["Der", "Wald", "in", "\u00fcpp'\u00b7ger", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht schwarz ein n\u00e4chtlich Meer.", "tokens": ["Steht", "schwarz", "ein", "n\u00e4cht\u00b7lich", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcber ihn sich breitet", "tokens": ["Und", "\u00fc\u00b7ber", "ihn", "sich", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein stolzer Felsenkranz,", "tokens": ["Ein", "stol\u00b7zer", "Fel\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das ist die Alp, gekleidet", "tokens": ["Das", "ist", "die", "Alp", ",", "ge\u00b7klei\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In blauen Himmelsglanz.", "tokens": ["In", "blau\u00b7en", "Him\u00b7mels\u00b7glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und all' die Berg' und Auen,", "tokens": ["Und", "all'", "die", "Ber\u00b7g'", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bebaut mit flei\u00df'ger Hand,", "tokens": ["Be\u00b7baut", "mit", "flei\u00df'\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die\u00df Land so sch\u00f6n zu schauen,", "tokens": ["Die\u00df", "Land", "so", "sch\u00f6n", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist deutsches Vaterland!", "tokens": ["Ist", "deut\u00b7sches", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gek\u00fc\u00dft von Himmelsbl\u00e4ue,", "tokens": ["Ge\u00b7k\u00fc\u00dft", "von", "Him\u00b7mels\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht es, des Himmels Braut.", "tokens": ["Steht", "es", ",", "des", "Him\u00b7mels", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00fctzt, Br\u00fcder, sie mit Treue!", "tokens": ["Sch\u00fctzt", ",", "Br\u00fc\u00b7der", ",", "sie", "mit", "Treu\u00b7e", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gott hat sie euch vertraut!", "tokens": ["Gott", "hat", "sie", "euch", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schlaft s\u00fc\u00df, die ihr den Degen", "tokens": ["Schlaft", "s\u00fc\u00df", ",", "die", "ihr", "den", "De\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcr diese Braut gef\u00fchrt,", "tokens": ["F\u00fcr", "die\u00b7se", "Braut", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die auf des Sieges Wegen", "tokens": ["Die", "auf", "des", "Sie\u00b7ges", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "J\u00fcngst sel'ger Tod ber\u00fchrt!", "tokens": ["J\u00fcngst", "sel'\u00b7ger", "Tod", "be\u00b7r\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch hier aus alten Zeiten", "tokens": ["Auch", "hier", "aus", "al\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schl\u00e4ft manches Heldenbild,", "tokens": ["Schl\u00e4ft", "man\u00b7ches", "Hel\u00b7den\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das einst in blut'gen Streiten", "tokens": ["Das", "einst", "in", "blut'\u00b7gen", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "War deutschem Land ein Schild.", "tokens": ["War", "deut\u00b7schem", "Land", "ein", "Schild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch ragt der Fels vor allen,", "tokens": ["Noch", "ragt", "der", "Fels", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drauf einst der Helden Haus,", "tokens": ["Drauf", "einst", "der", "Hel\u00b7den", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist auch ihr Leib zerfallen,", "tokens": ["Ist", "auch", "ihr", "Leib", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Treu' h\u00e4lt ewig aus.", "tokens": ["Die", "Treu'", "h\u00e4lt", "e\u00b7wig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum stieg in Kampfes Tagen", "tokens": ["Drum", "stieg", "in", "Kamp\u00b7fes", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier aus der Gr\u00fcfte Nacht", "tokens": ["Hier", "aus", "der", "Gr\u00fcf\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Manch' alter Held, zu tragen", "tokens": ["Man\u00b7ch'", "al\u00b7ter", "Held", ",", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das Siegspanier der Schlacht.", "tokens": ["Das", "Siegs\u00b7pa\u00b7nier", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit solchen treu verbunden,", "tokens": ["Mit", "sol\u00b7chen", "treu", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da k\u00e4mpften M\u00e4nner gut,", "tokens": ["Da", "k\u00e4mpf\u00b7ten", "M\u00e4n\u00b7ner", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sprang aus sel'gen Wunden", "tokens": ["Da", "sprang", "aus", "sel'\u00b7gen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Heilquell, deutsches Blut.", "tokens": ["Ein", "Heil\u00b7quell", ",", "deut\u00b7sches", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft deutschen Muth nicht sinken,", "tokens": ["La\u00dft", "deut\u00b7schen", "Muth", "nicht", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So lang' noch Alpen steh'n,", "tokens": ["So", "lang'", "noch", "Al\u00b7pen", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Euch Heldengeister winken", "tokens": ["Euch", "Hel\u00b7den\u00b7geis\u00b7ter", "win\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von ihren blauen H\u00f6h'n!", "tokens": ["Von", "ih\u00b7ren", "blau\u00b7en", "H\u00f6h'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "H\u00e4ngt fest, wie Waldes-Eichen,", "tokens": ["H\u00e4ngt", "fest", ",", "wie", "Wal\u00b7des\u00b7Ei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am heil'gen deutschen Land!", "tokens": ["Am", "heil'\u00b7gen", "deut\u00b7schen", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl ritterlich euch reichen", "tokens": ["Wohl", "rit\u00b7ter\u00b7lich", "euch", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Schutz und Trutz die Hand!", "tokens": ["Zu", "Schutz", "und", "Trutz", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Braut in Himmelssch\u00f6ne", "tokens": ["Die", "Braut", "in", "Him\u00b7mels\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die\u00df Land so seegenreich,", "tokens": ["Die\u00df", "Land", "so", "see\u00b7gen\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Will starke, treue S\u00f6hne,", "tokens": ["Will", "star\u00b7ke", ",", "treu\u00b7e", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den ew'gen Alpen gleich.", "tokens": ["Den", "ew'\u00b7gen", "Al\u00b7pen", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}