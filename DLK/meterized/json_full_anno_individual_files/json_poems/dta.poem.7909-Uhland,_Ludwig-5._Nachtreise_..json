{"dta.poem.7909": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5.  Nachtreise .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.71", "en:0.28"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich reit\u2019 in\u2019s finstre Land hinein,", "tokens": ["Ich", "reit'", "in's", "finst\u00b7re", "Land", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Mond, noch Sterne geben Schein,", "tokens": ["Nicht", "Mond", ",", "noch", "Ster\u00b7ne", "ge\u00b7ben", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die kalten Winde tosen.", "tokens": ["Die", "kal\u00b7ten", "Win\u00b7de", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oft hab\u2019 ich diesen Weg gemacht,", "tokens": ["Oft", "hab'", "ich", "die\u00b7sen", "Weg", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann goldner Sonnenschein gelacht,", "tokens": ["Wann", "gold\u00b7ner", "Son\u00b7nen\u00b7schein", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei lauer L\u00fcfte Kosen.", "tokens": ["Bei", "lau\u00b7er", "L\u00fcf\u00b7te", "Ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich reit\u2019 am finstern Garten hin,", "tokens": ["Ich", "reit'", "am", "fins\u00b7tern", "Gar\u00b7ten", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die d\u00fcrren B\u00e4ume sausen drin,", "tokens": ["Die", "d\u00fcr\u00b7ren", "B\u00e4u\u00b7me", "sau\u00b7sen", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die welken Bl\u00e4tter fallen.", "tokens": ["Die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier pflegt\u2019 ich in der Rosenzeit,", "tokens": ["Hier", "pflegt'", "ich", "in", "der", "Ro\u00b7sen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann Alles sich der Liebe weiht,", "tokens": ["Wann", "Al\u00b7les", "sich", "der", "Lie\u00b7be", "weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit meinem Lieb zu wallen.", "tokens": ["Mit", "mei\u00b7nem", "Lieb", "zu", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Erloschen ist der Sonne Stral,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "der", "Son\u00b7ne", "Stral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwelkt die Rosen allzumal,", "tokens": ["Ver\u00b7welkt", "die", "Ro\u00b7sen", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Lieb zu Grab getragen.", "tokens": ["Mein", "Lieb", "zu", "Grab", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich reit\u2019 in\u2019s finstre Land hinein,", "tokens": ["Ich", "reit'", "in's", "finst\u00b7re", "Land", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Wintersturm, ohn\u2019 allen Schein,", "tokens": ["Im", "Win\u00b7ter\u00b7sturm", ",", "ohn'", "al\u00b7len", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Mantel umgeschlagen.", "tokens": ["Den", "Man\u00b7tel", "um\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}