{"textgrid.poem.26027": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mein Herz ist ein ruhiges St\u00fcck Erde geworden", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt sitzt die Sonnenflamm' auf meinem Dach und d\u00f6rrt die Ziegel,", "tokens": ["Jetzt", "sitzt", "die", "Son\u00b7nen\u00b7flamm'", "auf", "mei\u00b7nem", "Dach", "und", "d\u00f6rrt", "die", "Zie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und drinnen geht die Liebe bei mir auf und um,", "tokens": ["Und", "drin\u00b7nen", "geht", "die", "Lie\u00b7be", "bei", "mir", "auf", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nachts sind noch die H\u00e4nde wie die Ziegel hei\u00df,", "tokens": ["Und", "nachts", "sind", "noch", "die", "H\u00e4n\u00b7de", "wie", "die", "Zie\u00b7gel", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "ART", "NN", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die H\u00e4nde, die die Liebe halten wollen.", "tokens": ["Die", "H\u00e4n\u00b7de", ",", "die", "die", "Lie\u00b7be", "hal\u00b7ten", "wol\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos":