{"dta.poem.10641": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Fleuch wo dir hingeliebt/ wohin du nur kanst kommen/", "tokens": ["Fleuch", "wo", "dir", "hin\u00b7ge\u00b7liebt", "/", "wo\u00b7hin", "du", "nur", "kanst", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "VVFIN", "$(", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Fleuch mein Gem\u00fcthe/ fleuch Lufft/ Fewer/ Wasser/ Erdt/", "tokens": ["Fleuch", "mein", "Ge\u00b7m\u00fc\u00b7the", "/", "fleuch", "Lufft", "/", "Fe\u00b7wer", "/", "Was\u00b7ser", "/", "Erdt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "VVIMP", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du magst doch nicht entgehn/ dein vorsatz wirdt verkert/", "tokens": ["Du", "magst", "doch", "nicht", "ent\u00b7gehn", "/", "dein", "vor\u00b7satz", "wirdt", "ver\u00b7kert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dich mein Lieb inn sich schon g\u00e4ntzlich eingenommen.", "tokens": ["Weil", "dich", "mein", "Lieb", "inn", "sich", "schon", "g\u00e4nt