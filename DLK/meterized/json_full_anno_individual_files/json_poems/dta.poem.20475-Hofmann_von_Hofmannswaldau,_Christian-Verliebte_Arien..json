{"dta.poem.20475": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Asterie/ ich bin gebunden/", "tokens": ["As\u00b7te\u00b7rie", "/", "ich", "bin", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zwar durch nichts als einen ku\u00df/", "tokens": ["Und", "zwar", "durch", "nichts", "als", "ei\u00b7nen", "ku\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein ku\u00df der macht mir seelen-wunden/", "tokens": ["Dein", "ku\u00df", "der", "macht", "mir", "see\u00b7len\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "PPER", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So/ da\u00df ich t\u00f6dtlich krancken mu\u00df.", "tokens": ["So", "/", "da\u00df", "ich", "t\u00f6dt\u00b7lich", "kran\u00b7cken", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich werd entz\u00fccket", "tokens": ["Ich", "werd", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Durch einen mund/", "tokens": ["Durch", "ei\u00b7nen", "mund", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und auch ber\u00fccket/", "tokens": ["Und", "auch", "be\u00b7r\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Da\u00df von der stund", "tokens": ["Da\u00df", "von", "der", "stund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich nicht gesund.", "tokens": ["Ich", "nicht", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Entdecke mir doch/ was das k\u00fcssen", "tokens": ["Ent\u00b7de\u00b7cke", "mir", "doch", "/", "was", "das", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "PWS", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen rechten ursprung sey;", "tokens": ["In", "sei\u00b7nen", "rech\u00b7ten", "ur\u00b7sprung", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sich die geister in sich schliessen", "tokens": ["Wenn", "sich", "die", "geis\u00b7ter", "in", "sich", "schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch so verliebte zauberey/", "tokens": ["Durch", "so", "ver\u00b7lieb\u00b7te", "zau\u00b7be\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "NE", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und sich verschrencken", "tokens": ["Und", "sich", "ver\u00b7schren\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRF", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In schneller eil;", "tokens": ["In", "schnel\u00b7ler", "eil", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wer kan gedencken/", "tokens": ["Wer", "kan", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Was vor ein theil", "tokens": ["Was", "vor", "ein", "theil"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Der k\u00fcsse pfeil?", "tokens": ["Der", "k\u00fcs\u00b7se", "pfeil", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dein mund der ist mein wollust-keller/", "tokens": ["Dein", "mund", "der", "ist", "mein", "wol\u00b7lust\u00b7kel\u00b7ler", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Worinn sich offt berauscht die seel;", "tokens": ["Wo\u00b7rinn", "sich", "offt", "be\u00b7rauscht", "die", "seel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich achte nicht den muscateller", "tokens": ["Ich", "ach\u00b7te", "nicht", "den", "mus\u00b7ca\u00b7tel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Wann nur in der corallen-h\u00f6l", "tokens": ["Wann", "nur", "in", "der", "coral\u00b7len\u00b7h\u00f6l"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die seele weidet", "tokens": ["Die", "see\u00b7le", "wei\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nebst deinem geist/", "tokens": ["Nebst", "dei\u00b7nem", "geist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der nicht eh\u2019 scheidet", "tokens": ["Der", "nicht", "eh'", "schei\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und von mir reist/", "tokens": ["Und", "von", "mir", "reist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Bi\u00df ich gespeist.", "tokens": ["Bi\u00df", "ich", "ge\u00b7speist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Asterie/ la\u00df deine lippen/", "tokens": ["As\u00b7te\u00b7rie", "/", "la\u00df", "dei\u00b7ne", "lip\u00b7pen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir nur nicht harte felsen seyn;", "tokens": ["Mir", "nur", "nicht", "har\u00b7te", "fel\u00b7sen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schiffer flieh\u2019t sonst hohe klippen/", "tokens": ["Ein", "schif\u00b7fer", "flieh't", "sonst", "ho\u00b7he", "klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dir lauff ich im hafen ein/", "tokens": ["Bey", "dir", "lauff", "ich", "im", "ha\u00b7fen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein ku\u00df der bleibet", "tokens": ["Dein", "ku\u00df", "der", "blei\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mein lust-magnet/", "tokens": ["Mein", "lust\u00b7ma\u00b7gnet", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu dem mich treibet", "tokens": ["Zu", "dem", "mich", "trei\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die treu/ so steht/", "tokens": ["Die", "treu", "/", "so", "steht", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ADV", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und nicht vergeht.", "tokens": ["Und", "nicht", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Hastu von mir was mehr empfunden/", "tokens": ["Has\u00b7tu", "von", "mir", "was", "mehr", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als den verschrenckten lippen-rauch?", "tokens": ["Als", "den", "ver\u00b7schrenck\u00b7ten", "lip\u00b7pen\u00b7rauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat lieb und ku\u00df ein garn gewunden/", "tokens": ["Hat", "lieb", "und", "ku\u00df", "ein", "garn", "ge\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "NN", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6ner als der rosen-strauch;", "tokens": ["So", "sch\u00f6\u00b7ner", "als", "der", "ro\u00b7sen\u00b7strauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So magstu glauben/", "tokens": ["So", "mags\u00b7tu", "glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df ich kein schnee/", "tokens": ["Da\u00df", "ich", "kein", "schnee", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und da\u00df mit schrauben", "tokens": ["Und", "da\u00df", "mit", "schrau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Im hertzen steh", "tokens": ["Im", "hert\u00b7zen", "steh"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Asterie.", "tokens": ["As\u00b7te\u00b7rie", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}