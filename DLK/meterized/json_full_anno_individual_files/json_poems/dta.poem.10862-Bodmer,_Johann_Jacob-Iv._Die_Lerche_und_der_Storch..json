{"dta.poem.10862": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Iv.  \n  Die Lerche und der Storch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem die Lerch der Berge Spitze", "tokens": ["Nach\u00b7dem", "die", "Lerch", "der", "Ber\u00b7ge", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrer holden Stimm erf\u00fcllt,", "tokens": ["Mit", "ih\u00b7rer", "hol\u00b7den", "Stimm", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ersieht sie sich zu ihrem Sitze", "tokens": ["Er\u00b7sieht", "sie", "sich", "zu", "ih\u00b7rem", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Feld, wo Thau auf Blumen quillt.", "tokens": ["Ein", "Feld", ",", "wo", "Thau", "auf", "Blu\u00b7men", "quillt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sencket sich der Erden zu,", "tokens": ["Sie", "sen\u00b7cket", "sich", "der", "Er\u00b7den", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch einsmahls st\u00f6ret ihre Ruh", "tokens": ["Doch", "eins\u00b7mahls", "st\u00f6\u00b7ret", "ih\u00b7re", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Storch, der dort spatzieren gieng,", "tokens": ["Ein", "Storch", ",", "der", "dort", "spat\u00b7zie\u00b7ren", "gieng", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Froschen oder Bienen fieng.", "tokens": ["Und", "Fro\u00b7schen", "o\u00b7der", "Bie\u00b7nen", "fi\u00b7eng", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Bist du es, sprach der Storch zur Lerche.", "tokens": ["Bist", "du", "es", ",", "sprach", "der", "Storch", "zur", "Ler\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Hertz gefa\u00dft! du weist die St\u00f6rche", "tokens": ["Ein", "Hertz", "ge\u00b7fa\u00dft", "!", "du", "weist", "die", "St\u00f6r\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Verschlingen kein gefiedert Thier,", "tokens": ["Ver\u00b7schlin\u00b7gen", "kein", "ge\u00b7fie\u00b7dert", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Drum traue mir, und bleibe hier.", "tokens": ["Drum", "trau\u00b7e", "mir", ",", "und", "blei\u00b7be", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr dich sind St\u00f6rche keine Fremden;", "tokens": ["F\u00fcr", "dich", "sind", "St\u00f6r\u00b7che", "kei\u00b7ne", "Frem\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dich soll denn meine Gegenwart", "tokens": ["Dich", "soll", "denn", "mei\u00b7ne", "Ge\u00b7gen\u00b7wart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "O liebste Lerche, nicht befremden.", "tokens": ["O", "liebs\u00b7te", "Ler\u00b7che", ",", "nicht", "be\u00b7frem\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Was Federn hat, ist einer Art.", "tokens": ["Was", "Fe\u00b7dern", "hat", ",", "ist", "ei\u00b7ner", "Art", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So recht, erwiederte die Lerche", "tokens": ["So", "recht", ",", "er\u00b7wie\u00b7der\u00b7te", "die", "Ler\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Mein Leib ist viel zu klein f\u00fcr St\u00f6rche,", "tokens": ["Mein", "Leib", "ist", "viel", "zu", "klein", "f\u00fcr", "St\u00f6r\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Doch sind die Froschen fast so klein,", "tokens": ["Doch", "sind", "die", "Fro\u00b7schen", "fast", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und schlingst du sie so schnell hinein.", "tokens": ["Und", "schlingst", "du", "sie", "so", "schnell", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was mehr ist, frissest du ja Bienen,", "tokens": ["Was", "mehr", "ist", ",", "fris\u00b7sest", "du", "ja", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die Meisen sonst zur Speise dienen.", "tokens": ["Die", "Mei\u00b7sen", "sonst", "zur", "Spei\u00b7se", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Nein, nein, dein blutgef\u00e4rbter Schnabel", "tokens": ["Nein", ",", "nein", ",", "dein", "blut\u00b7ge\u00b7f\u00e4rb\u00b7ter", "Schna\u00b7bel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Lehrt mich es seye keine Fabel,", "tokens": ["Lehrt", "mich", "es", "se\u00b7ye", "kei\u00b7ne", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df du der Schlangen Meister seyst.", "tokens": ["Da\u00df", "du", "der", "Schlan\u00b7gen", "Meis\u00b7ter", "seyst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ey ", "tokens": ["Ey"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Und fliegt auf einen fernen H\u00fcgel.", "tokens": ["Und", "fliegt", "auf", "ei\u00b7nen", "fer\u00b7nen", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}