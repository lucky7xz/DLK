{"textgrid.poem.33804": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Brandung", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Festball haben heut die Wogen", "tokens": ["Fest\u00b7ball", "ha\u00b7ben", "heut", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die schwarzen Klippen her,", "tokens": ["Um", "die", "schwar\u00b7zen", "Klip\u00b7pen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4umend und in weiten Bogen,", "tokens": ["Sch\u00e4u\u00b7mend", "und", "in", "wei\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuz und quer", "tokens": ["Kreuz", "und", "quer"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und in Schleppgewanden schwer", "tokens": ["Und", "in", "Schlepp\u00b7ge\u00b7wan\u00b7den", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommen sie zum Strand gezogen.", "tokens": ["Kom\u00b7men", "sie", "zum", "Strand", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Uralt tolle Wasserschw\u00e4nke", "tokens": ["Ur\u00b7alt", "tol\u00b7le", "Was\u00b7ser\u00b7schw\u00e4n\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spr\u00fchn sie zischend sich ins Ohr,", "tokens": ["Spr\u00fchn", "sie", "zi\u00b7schend", "sich", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spritzen um die Felsenb\u00e4nke", "tokens": ["Sprit\u00b7zen", "um", "die", "Fel\u00b7sen\u00b7b\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch empor,", "tokens": ["Hoch", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dunkle dr\u00e4ngen dunklere vor,", "tokens": ["Dunk\u00b7le", "dr\u00e4n\u00b7gen", "dunk\u00b7le\u00b7re", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wild wie Rosse zu der Tr\u00e4nke.", "tokens": ["Wild", "wie", "Ros\u00b7se", "zu", "der", "Tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus den Wirbeln ragt im Schwalle", "tokens": ["Aus", "den", "Wir\u00b7beln", "ragt", "im", "Schwal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ein steingewordnes Schiff,", "tokens": ["Hier", "ein", "stein\u00b7ge\u00b7word\u00b7nes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort ein Untier \u2013 Rumpf und Kralle", "tokens": ["Dort", "ein", "Un\u00b7tier", "\u2013", "Rumpf", "und", "Kral\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward zum Riff.", "tokens": ["Ward", "zum", "Riff", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Horch, da t\u00f6nt ein geller Pfiff,", "tokens": ["Horch", ",", "da", "t\u00f6nt", "ein", "gel\u00b7ler", "Pfiff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nacht umf\u00e4ngt die Felsenhalle.", "tokens": ["Nacht", "um\u00b7f\u00e4ngt", "die", "Fel\u00b7sen\u00b7hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nacht \u2013 und immer schaumbest\u00e4ubter", "tokens": ["Nacht", "\u2013", "und", "im\u00b7mer", "schaum\u00b7be\u00b7st\u00e4ub\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KON", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogt's heran, und Schaum bedeckt", "tokens": ["Wogt's", "he\u00b7ran", ",", "und", "Schaum", "be\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KON", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All der Unget\u00fcme H\u00e4upter;", "tokens": ["All", "der", "Un\u00b7ge\u00b7t\u00fc\u00b7me", "H\u00e4up\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auferweckt", "tokens": ["Au\u00b7fer\u00b7weckt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Scheinen sie, wie wenn sich reckt", "tokens": ["Schei\u00b7nen", "sie", ",", "wie", "wenn", "sich", "reckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PRF", "VVFIN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ein von langem Schlaf Bet\u00e4ubter.", "tokens": ["Ein", "von", "lan\u00b7gem", "Schlaf", "Be\u00b7t\u00e4ub\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ha, jetzt gibt es Schl\u00e4gereien!", "tokens": ["Ha", ",", "jetzt", "gibt", "es", "Schl\u00e4\u00b7ge\u00b7rei\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das Wrack im Meeresscho\u00df", "tokens": ["Um", "das", "Wrack", "im", "Mee\u00b7res\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streiten sie zu zwei und dreien;", "tokens": ["Strei\u00b7ten", "sie", "zu", "zwei", "und", "drei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "KON", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klein und gro\u00df", "tokens": ["Klein", "und", "gro\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Hauen aufeinander los", "tokens": ["Hau\u00b7en", "auf\u00b7ein\u00b7an\u00b7der", "los"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Gez\u00e4hn und Grat von Haien.", "tokens": ["Mit", "Ge\u00b7z\u00e4hn", "und", "Grat", "von", "Hai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kiefern rei\u00dfen, Flossen, Schuppen", "tokens": ["Kie\u00b7fern", "rei\u00b7\u00dfen", ",", "Flos\u00b7sen", ",", "Schup\u00b7pen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Ungeheuer aus;", "tokens": ["Sich", "die", "Un\u00b7ge\u00b7heu\u00b7er", "aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An der Steine schwarzen Kuppen,", "tokens": ["An", "der", "Stei\u00b7ne", "schwar\u00b7zen", "Kup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Gebraus,", "tokens": ["Im", "Ge\u00b7braus", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "An den Faden eines Taus", "tokens": ["An", "den", "Fa\u00b7den", "ei\u00b7nes", "Taus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ringen sie in ganzen Gruppen.", "tokens": ["Rin\u00b7gen", "sie", "in", "gan\u00b7zen", "Grup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit versunknen Enterhaken", "tokens": ["Mit", "ver\u00b7sun\u00b7knen", "En\u00b7ter\u00b7ha\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommen sie herauf vom Grund;", "tokens": ["Kom\u00b7men", "sie", "her\u00b7auf", "vom", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie sich am Schopfe packen", "tokens": ["Wie", "sie", "sich", "am", "Schop\u00b7fe", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Schlund", "tokens": ["Und", "am", "Schlund"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Sich verbei\u00dfen und schon wund", "tokens": ["Sich", "ver\u00b7bei\u00b7\u00dfen", "und", "schon", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch die Sch\u00e4del sich zerknacken!", "tokens": ["Noch", "die", "Sch\u00e4\u00b7del", "sich", "zer\u00b7kna\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wem bleibt wohl die Siegeskrone?", "tokens": ["Wem", "bleibt", "wohl", "die", "Sie\u00b7ges\u00b7kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieh! die Andern alle taucht", "tokens": ["Sieh", "!", "die", "An\u00b7dern", "al\u00b7le", "taucht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein gewaltiger Tritone!", "tokens": ["Ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Tri\u00b7to\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er pfaucht,", "tokens": ["Wie", "er", "pfaucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da sein letzter Feind verhaucht,", "tokens": ["Da", "sein", "letz\u00b7ter", "Feind", "ver\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein elender Epigone!", "tokens": ["Ein", "e\u00b7len\u00b7der", "E\u00b7pi\u00b7go\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Um den stolzen Sieger schwellen,", "tokens": ["Um", "den", "stol\u00b7zen", "Sie\u00b7ger", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kosend seinen wei\u00dfen Bart,", "tokens": ["Ko\u00b7send", "sei\u00b7nen", "wei\u00b7\u00dfen", "Bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leichtgesch\u00fcrzte Mondlichtwellen,", "tokens": ["Leicht\u00b7ge\u00b7sch\u00fcrz\u00b7te", "Mond\u00b7licht\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hold und zart,", "tokens": ["Hold", "und", "zart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die zu seiner Siegesfahrt", "tokens": ["Die", "zu", "sei\u00b7ner", "Sie\u00b7ges\u00b7fahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ringsumher die Nacht erhellen.", "tokens": ["Rings\u00b7um\u00b7her", "die", "Nacht", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Festball haben heut die Wogen", "tokens": ["Fest\u00b7ball", "ha\u00b7ben", "heut", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die schwarzen Klippen her,", "tokens": ["Um", "die", "schwar\u00b7zen", "Klip\u00b7pen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4umend und in weiten Bogen,", "tokens": ["Sch\u00e4u\u00b7mend", "und", "in", "wei\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kreuz und quer", "tokens": ["Kreuz", "und", "quer"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und in Schleppgewanden schwer", "tokens": ["Und", "in", "Schlepp\u00b7ge\u00b7wan\u00b7den", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommen sie zum Strand gezogen.", "tokens": ["Kom\u00b7men", "sie", "zum", "Strand", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Uralt tolle Wasserschw\u00e4nke", "tokens": ["Ur\u00b7alt", "tol\u00b7le", "Was\u00b7ser\u00b7schw\u00e4n\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spr\u00fchn sie zischend sich ins Ohr,", "tokens": ["Spr\u00fchn", "sie", "zi\u00b7schend", "sich", "ins", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spritzen um die Felsenb\u00e4nke", "tokens": ["Sprit\u00b7zen", "um", "die", "Fel\u00b7sen\u00b7b\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch empor,", "tokens": ["Hoch", "em\u00b7por", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dunkle dr\u00e4ngen dunklere vor,", "tokens": ["Dunk\u00b7le", "dr\u00e4n\u00b7gen", "dunk\u00b7le\u00b7re", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wild wie Rosse zu der Tr\u00e4nke.", "tokens": ["Wild", "wie", "Ros\u00b7se", "zu", "der", "Tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aus den Wirbeln ragt im Schwalle", "tokens": ["Aus", "den", "Wir\u00b7beln", "ragt", "im", "Schwal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier ein steingewordnes Schiff,", "tokens": ["Hier", "ein", "stein\u00b7ge\u00b7word\u00b7nes", "Schiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort ein Untier \u2013 Rumpf und Kralle", "tokens": ["Dort", "ein", "Un\u00b7tier", "\u2013", "Rumpf", "und", "Kral\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward zum Riff.", "tokens": ["Ward", "zum", "Riff", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Horch, da t\u00f6nt ein geller Pfiff,", "tokens": ["Horch", ",", "da", "t\u00f6nt", "ein", "gel\u00b7ler", "Pfiff", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nacht umf\u00e4ngt die Felsenhalle.", "tokens": ["Nacht", "um\u00b7f\u00e4ngt", "die", "Fel\u00b7sen\u00b7hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nacht \u2013 und immer schaumbest\u00e4ubter", "tokens": ["Nacht", "\u2013", "und", "im\u00b7mer", "schaum\u00b7be\u00b7st\u00e4ub\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KON", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogt's heran, und Schaum bedeckt", "tokens": ["Wogt's", "he\u00b7ran", ",", "und", "Schaum", "be\u00b7deckt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "KON", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All der Unget\u00fcme H\u00e4upter;", "tokens": ["All", "der", "Un\u00b7ge\u00b7t\u00fc\u00b7me", "H\u00e4up\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auferweckt", "tokens": ["Au\u00b7fer\u00b7weckt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Scheinen sie, wie wenn sich reckt", "tokens": ["Schei\u00b7nen", "sie", ",", "wie", "wenn", "sich", "reckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PRF", "VVFIN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Ein von langem Schlaf Bet\u00e4ubter.", "tokens": ["Ein", "von", "lan\u00b7gem", "Schlaf", "Be\u00b7t\u00e4ub\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ha, jetzt gibt es Schl\u00e4gereien!", "tokens": ["Ha", ",", "jetzt", "gibt", "es", "Schl\u00e4\u00b7ge\u00b7rei\u00b7en", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das Wrack im Meeresscho\u00df", "tokens": ["Um", "das", "Wrack", "im", "Mee\u00b7res\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streiten sie zu zwei und dreien;", "tokens": ["Strei\u00b7ten", "sie", "zu", "zwei", "und", "drei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "KON", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klein und gro\u00df", "tokens": ["Klein", "und", "gro\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Hauen aufeinander los", "tokens": ["Hau\u00b7en", "auf\u00b7ein\u00b7an\u00b7der", "los"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Gez\u00e4hn und Grat von Haien.", "tokens": ["Mit", "Ge\u00b7z\u00e4hn", "und", "Grat", "von", "Hai\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Kiefern rei\u00dfen, Flossen, Schuppen", "tokens": ["Kie\u00b7fern", "rei\u00b7\u00dfen", ",", "Flos\u00b7sen", ",", "Schup\u00b7pen"], "token_info