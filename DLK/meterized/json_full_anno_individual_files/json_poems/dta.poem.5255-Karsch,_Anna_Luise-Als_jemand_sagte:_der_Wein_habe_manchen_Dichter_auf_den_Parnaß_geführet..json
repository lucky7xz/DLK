{"dta.poem.5255": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als jemand sagte: der Wein habe  \n manchen Dichter auf den Parna\u00df  \n gef\u00fchret.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht Bachus, nicht das Gl\u00fcck befand", "tokens": ["Nicht", "Ba\u00b7chus", ",", "nicht", "das", "Gl\u00fcck", "be\u00b7fand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "$,", "PTKNEG", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich bey mir, da ich ging zum hohen Musensitze;", "tokens": ["Sich", "bey", "mir", ",", "da", "ich", "ging", "zum", "ho\u00b7hen", "Mu\u00b7sen\u00b7sit\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Freundschaft hielt mich bey der Hand,", "tokens": ["Die", "Freund\u00b7schaft", "hielt", "mich", "bey", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fchrte l\u00e4chelnd mich bis an des Berges Spitze.", "tokens": ["Und", "f\u00fchr\u00b7te", "l\u00e4\u00b7chelnd", "mich", "bis", "an", "des", "Ber\u00b7ges", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}