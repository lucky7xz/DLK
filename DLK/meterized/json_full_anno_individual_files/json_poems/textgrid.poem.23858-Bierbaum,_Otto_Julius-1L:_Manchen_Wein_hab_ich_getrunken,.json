{"textgrid.poem.23858": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manchen Wein hab ich getrunken,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchen Wein hab ich getrunken,", "tokens": ["Man\u00b7chen", "Wein", "hab", "ich", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchem sch\u00f6nen Kinde bin", "tokens": ["Man\u00b7chem", "sch\u00f6\u00b7nen", "Kin\u00b7de", "bin"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verliebt ans Herz gesunken;", "tokens": ["Ich", "ver\u00b7liebt", "ans", "Herz", "ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt geht alles n\u00fcchtern hin,", "tokens": ["Jetzt", "geht", "al\u00b7les", "n\u00fcch\u00b7tern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Abgezirkelt, abgemessen,", "tokens": ["Ab\u00b7ge\u00b7zir\u00b7kelt", ",", "ab\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das ist des Liedes Sinn:", "tokens": ["Und", "das", "ist", "des", "Lie\u00b7des", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN",