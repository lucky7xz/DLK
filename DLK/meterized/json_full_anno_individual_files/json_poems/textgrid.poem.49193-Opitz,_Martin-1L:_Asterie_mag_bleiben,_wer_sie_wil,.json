lt,", "tokens": ["Der", "Uep\u00b7pig\u00b7keit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Darauff zuvor erlegen", "tokens": ["Dar\u00b7auff", "zu\u00b7vor", "er\u00b7le\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Manch freyer k\u00fchner Heldt.", "tokens": ["Manch", "frey\u00b7er", "k\u00fch\u00b7ner", "Heldt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Sch\u00f6nheit zwar veracht' ich g\u00e4ntzlich nicht,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "zwar", "ver\u00b7acht'", "ich", "g\u00e4ntz\u00b7lich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie von oben k\u00f6mpt,", "tokens": ["Weil", "sie", "von", "o\u00b7ben", "k\u00f6mpt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sag' ich nur, da\u00df sie gar leichte bricht", "tokens": ["Das", "sag'", "ich", "nur", ",", "da\u00df", "sie", "gar", "leich\u00b7te", "bricht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und bald ein Ende nimpt;", "tokens": ["Und", "bald", "ein", "En\u00b7de", "nimpt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der rote Mund, die Wangen,", "tokens": ["Der", "ro\u00b7te", "Mund", ",", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der sch\u00f6nen Augen Glantz,", "tokens": ["Der", "sch\u00f6\u00b7nen", "Au\u00b7gen", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja alle Pracht und Prangen", "tokens": ["Ja", "al\u00b7le", "Pracht", "und", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist wie ein Rosenkrantz.", "tokens": ["Ist", "wie", "ein", "Ro\u00b7sen\u00b7krantz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wer Tugend liebt, der stirbet nimmermehr,", "tokens": ["Wer", "Tu\u00b7gend", "liebt", ",", "der", "stir\u00b7bet", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er dringt durch alle Noth,", "tokens": ["Er", "dringt", "durch", "al\u00b7le", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch alle Welt erklingt sein Lob und Ehr,", "tokens": ["Durch", "al\u00b7le", "Welt", "er\u00b7klingt", "sein", "Lob", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er bleibt und lebet todt:", "tokens": ["Er", "bleibt", "und", "le\u00b7bet", "todt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drumb wil ich nichts mehr schreiben", "tokens": ["Drumb", "wil", "ich", "nichts", "mehr", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von zeitlicher Begiehr,", "tokens": ["Von", "zeit\u00b7li\u00b7cher", "Be\u00b7giehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wird mein Lob bekleiben", "tokens": ["So", "wird", "mein", "Lob", "be\u00b7klei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und gr\u00fcnen f\u00fcr und f\u00fcr.", "tokens": ["Und", "gr\u00fc\u00b7nen", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Weg, Venus, weg, du Pest der jungen Zeit,", "tokens": ["Weg", ",", "Ve\u00b7nus", ",", "weg", ",", "du", "Pest", "der", "jun\u00b7gen", "Zeit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PTKVZ", "$,", "PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich selbst vergesse mein;", "tokens": ["Ich", "selbst", "ver\u00b7ges\u00b7se", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wil jetzt gehn den Lauff der Ewigkeit", "tokens": ["Ich", "wil", "jetzt", "gehn", "den", "Lauff", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und auff der s\u00fcssen Pein", "tokens": ["Und", "auff", "der", "s\u00fcs\u00b7sen", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verwirten Bahn nicht wallen,", "tokens": ["Ver\u00b7wir\u00b7ten", "Bahn", "nicht", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Tugend ist mein Ziel;", "tokens": ["Die", "Tu\u00b7gend", "ist", "mein", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Asterie sampt allen", "tokens": ["As\u00b7te\u00b7rie", "sampt", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mag bleiben, wer sie wil.", "tokens": ["Mag", "blei\u00b7ben", ",", "wer", "sie", "wil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}