{"textgrid.poem.67231": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: R\u00f6thliche, goldbes\u00e4umte Wolken h\u00fcllen", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6thliche, goldbes\u00e4umte Wolken h\u00fcllen", "tokens": ["R\u00f6th\u00b7li\u00b7che", ",", "gold\u00b7be\u00b7s\u00e4um\u00b7te", "Wol\u00b7ken", "h\u00fcl\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ihre Strahlen nicht mehr! Sie kommt, die Sonne!", "tokens": ["Ih\u00b7re", "Strah\u00b7len", "nicht", "mehr", "!", "Sie", "kommt", ",", "die", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Blickt allg\u00fctig l\u00e4chelnde Freud' und junges", "tokens": ["Blickt", "all\u00b7g\u00fc\u00b7tig", "l\u00e4\u00b7cheln\u00b7de", "Freud'", "und", "jun\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "KON", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Leben hernieder!", "tokens": ["Le\u00b7ben", "her\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schimmernder bl\u00fch'n die thaubenetzten Fluren;", "tokens": ["Schim\u00b7mern\u00b7der", "bl\u00fch'n", "die", "thau\u00b7be\u00b7netz\u00b7ten", "Flu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Jedes zitternde Bl\u00fcmchen athmet Freude,", "tokens": ["Je\u00b7des", "zit\u00b7tern\u00b7de", "Bl\u00fcm\u00b7chen", "ath\u00b7met", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Strahlt in Regenbogen die Sonnenblicke", "tokens": ["Strahlt", "in", "Re\u00b7gen\u00b7bo\u00b7gen", "die", "Son\u00b7nen\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lieblicher um sich.", "tokens": ["Lieb\u00b7li\u00b7cher", "um", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Himmlischer aber l\u00e4chelt mir das Auge,", "tokens": ["Himm\u00b7li\u00b7scher", "a\u00b7ber", "l\u00e4\u00b7chelt", "mir", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ach! das Grazienauge meines M\u00e4dchens!", "tokens": ["Ach", "!", "das", "Gra\u00b7zi\u00b7en\u00b7au\u00b7ge", "mei\u00b7nes", "M\u00e4d\u00b7chens", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Blicket mild in's Herz mir noch ungef\u00fchlte,", "tokens": ["Bli\u00b7cket", "mild", "in's", "Herz", "mir", "noch", "un\u00b7ge\u00b7f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PPER", "ADV", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Selige Freuden!", "tokens": ["Se\u00b7li\u00b7ge", "Freu\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wallendes Leben bebt durch jede Nerve,", "tokens": ["Wal\u00b7len\u00b7des", "Le\u00b7ben", "bebt", "durch", "je\u00b7de", "Ner\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Klopft in jeglichem Pulse; frohe Schauer", "tokens": ["Klopft", "in", "jeg\u00b7li\u00b7chem", "Pul\u00b7se", ";", "fro\u00b7he", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "$.", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Str\u00f6men in die trunkene Seele namenloses", "tokens": ["Str\u00f6\u00b7men", "in", "die", "trun\u00b7ke\u00b7ne", "See\u00b7le", "na\u00b7men\u00b7lo\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Entz\u00fccken!", "tokens": ["Ent\u00b7z\u00fc\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Aber ach! Wehmuth blickt mir oft ihr blaues", "tokens": ["A\u00b7ber", "ach", "!", "Weh\u00b7muth", "blickt", "mir", "oft", "ihr", "blau\u00b7es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auge! Wehmuth und Tr\u00fcbsinn! Dann entquellen", "tokens": ["Au\u00b7ge", "!", "Weh\u00b7muth", "und", "Tr\u00fcb\u00b7sinn", "!", "Dann", "ent\u00b7quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "KON", "NN", "$.", "ADV", "VVINF"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sehnsuchtsseufzer, thaut mir der Liebe Z\u00e4hre", "tokens": ["Sehn\u00b7suchts\u00b7seuf\u00b7zer", ",", "thaut", "mir", "der", "Lie\u00b7be", "Z\u00e4h\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Ueber die Wange!", "tokens": ["Ue\u00b7ber", "die", "Wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Duftige Nebel locket so die Sonne", "tokens": ["Duf\u00b7ti\u00b7ge", "Ne\u00b7bel", "lo\u00b7cket", "so", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Aus dem Blumengefild' am Sommerabend;", "tokens": ["Aus", "dem", "Blu\u00b7men\u00b7ge\u00b7fild'", "am", "Som\u00b7mer\u00b7a\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Tr\u00fcbe steigt der wolkige Schleier, tr\u00e4ufelt", "tokens": ["Tr\u00fc\u00b7be", "steigt", "der", "wol\u00b7ki\u00b7ge", "Schlei\u00b7er", ",", "tr\u00e4u\u00b7felt"], "token_info": ["word", "word", "word", "word", "word", "punct", "