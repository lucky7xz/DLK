{"dta.poem.20899": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Und diesen grossen Klo\u00df mit leichter M\u00fch zu tragen/", "tokens": ["Und", "die\u00b7sen", "gros\u00b7sen", "Klo\u00df", "mit", "leich\u00b7ter", "M\u00fch", "zu", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bilden sie ihn ein/ es m\u00fcst ein Atlas seyn/", "tokens": ["So", "bil\u00b7den", "sie", "ihn", "ein", "/", "es", "m\u00fcst", "ein", "At\u00b7las", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "$(", "PPER", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sich zu solcher Last mit K\u00fcnheit d\u00fcrffte wagen/", "tokens": ["Der", "sich", "zu", "sol\u00b7cher", "Last", "mit", "K\u00fcn\u00b7heit", "d\u00fcrff\u00b7te", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dessen Schuldern nichts zu niedrig noch zu klein.", "tokens": ["Und", "des\u00b7sen", "Schul\u00b7dern", "nichts", "zu", "nied\u00b7rig", "noch", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "PIS", "PTKA", "ADJD", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}