{"textgrid.poem.33155": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1953.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine, aber doch dabey ganz zu deinen sachen: halte sie von feinden frey, sie und ihre nachen.", "tokens": ["Klei\u00b7ne", ",", "a\u00b7ber", "doch", "da\u00b7bey", "ganz", "zu", "dei\u00b7nen", "sa\u00b7chen", ":", "hal\u00b7te", "sie", "von", "fein\u00b7den", "frey", ",", "sie", "und", "ih\u00b7re", "na\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "ADV", "PAV", "ADV", "APPR", "PPOSAT", "VVINF", "$.", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "+-+----+-+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wiege sie im seiten-schrein durch die wilden wellen, bis sie werden dr\u00fcben seyn, bey ihren gesellen.", "tokens": ["Wie\u00b7ge", "sie", "im", "sei\u00b7ten\u00b7schrein", "durch", "die", "wil\u00b7den", "wel\u00b7len", ",", "bis", "sie", "wer\u00b7den", "dr\u00fc\u00b7ben", "seyn", ",", "bey", "ih\u00b7ren", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "PPOSAT", "APPR", "ART", "ADJA", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "ADV", "VAINF", "$,", "APPR", "PPOSAT", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Blut und wunden m\u00fcssen sie immerdar geleiten: deiner engel ihre m\u00fch bleib an ihrer seiten.", "tokens": ["Blut", "und", "wun\u00b7den", "m\u00fcs\u00b7sen", "sie", "im\u00b7mer\u00b7dar", "ge\u00b7lei\u00b7ten", ":", "dei\u00b7ner", "en\u00b7gel", "ih\u00b7re", "m\u00fch", "bleib", "an", "ih\u00b7rer", "sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "VMFIN", "PPER", "ADV", "VVINF", "$.", "PPOSAT", "NN", "PPOSAT", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Es sey der Gemeine gl\u00fck ihr licht auf den wegen, und daheim der wunden-blik ihr einiger segen.", "tokens": ["Es", "sey", "der", "Ge\u00b7mei\u00b7ne", "gl\u00fck", "ihr", "licht", "auf", "den", "we\u00b7gen", ",", "und", "da\u00b7heim", "der", "wun\u00b7den\u00b7blik", "ihr", "ei\u00b7ni\u00b7ger", "se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "APPR", "$,", "KON", "ADV", "ART", "NN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+--+-+-+--+-+-+-+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn du sie hineingebracht, und wirs werden h\u00f6ren, danken wir der wunden-macht unter allen ch\u00f6ren;", "tokens": ["Wenn", "du", "sie", "hin\u00b7ein\u00b7ge\u00b7bracht", ",", "und", "wirs", "wer\u00b7den", "h\u00f6\u00b7ren", ",", "dan\u00b7ken", "wir", "der", "wun\u00b7den\u00b7macht", "un\u00b7ter", "al\u00b7len", "ch\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "KON", "PIS", "VAFIN", "