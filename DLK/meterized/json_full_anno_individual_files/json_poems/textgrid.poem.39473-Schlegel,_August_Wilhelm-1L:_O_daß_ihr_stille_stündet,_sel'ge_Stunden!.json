{"textgrid.poem.39473": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O da\u00df ihr stille st\u00fcndet, sel'ge Stunden!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ihr stille st\u00fcndet, sel'ge Stunden!", "tokens": ["O", "da\u00df", "ihr", "stil\u00b7le", "st\u00fcn\u00b7det", ",", "sel'\u00b7ge", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil ihr verdient zu weilen, m\u00fc\u00dft ihr eilen,", "tokens": ["Weil", "ihr", "ver\u00b7dient", "zu", "wei\u00b7len", ",", "m\u00fc\u00dft", "ihr", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was euch vervielfacht, scheint euch zu zertheilen:", "tokens": ["Was", "euch", "ver\u00b7viel\u00b7facht", ",", "scheint", "euch", "zu", "zer\u00b7thei\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Endlos Entz\u00fccken macht euch zu Sekunden.", "tokens": ["End\u00b7los", "Ent\u00b7z\u00fc\u00b7cken", "macht", "euch", "zu", "Se\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "\u00bbwas klagst du? Wie gefunden, so verschwunden.", "tokens": ["\u00bb", "was", "klagst", "du", "?", "Wie", "ge\u00b7fun\u00b7den", ",", "so", "ver\u00b7schwun\u00b7den", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "PWAV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befiedert trugen wir mit Amors Pfeilen", "tokens": ["Be\u00b7fie\u00b7dert", "tru\u00b7gen", "wir", "mit", "A\u00b7mors", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir Lust herbei, und s\u00fc\u00dfe Gunst, zu heilen", "tokens": ["Dir", "Lust", "her\u00b7bei", ",", "und", "s\u00fc\u00b7\u00dfe", "Gunst", ",", "zu", "hei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "PTKVZ", "$,", "KON", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunden, die dein Herz kaum \u00fcberwunden.\u00ab", "tokens": ["Die", "Wun\u00b7den", ",", "die", "dein", "Herz", "kaum", "\u00fc\u00b7berw\u00b7un\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So seid denn, Stunden, meiner Wonne Musen!", "tokens": ["So", "seid", "denn", ",", "Stun\u00b7den", ",", "mei\u00b7ner", "Won\u00b7ne", "Mu\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "m