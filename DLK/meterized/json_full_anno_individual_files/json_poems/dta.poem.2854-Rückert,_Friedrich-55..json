{"dta.poem.2854": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du freuest dich, mein Sohn, da\u00df du in diesem Orden,", "tokens": ["Du", "freu\u00b7est", "dich", ",", "mein", "Sohn", ",", "da\u00df", "du", "in", "die\u00b7sem", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dem du stehst, nunmehr der erste bist geworden.", "tokens": ["In", "dem", "du", "stehst", ",", "nun\u00b7mehr", "der", "ers\u00b7te", "bist", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "ADV", "ART", "ADJA", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Ehrgeiz lob' ich zwar, doch sein Bereich ist klein,", "tokens": ["Den", "Ehr\u00b7geiz", "lob'", "ich", "zwar", ",", "doch", "sein", "Be\u00b7reich", "ist", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn hier der erste nicht noch letzte sollst du seyn.", "tokens": ["Denn", "hier", "der", "ers\u00b7te", "nicht", "noch", "letz\u00b7te", "sollst", "du", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "PTKNEG", "ADV", "ADJA", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu h\u00f6herm Orden soll dein Ehrgeiz dich befiedern,", "tokens": ["Zu", "h\u00f6\u00b7herm", "Or\u00b7den", "soll", "dein", "Ehr\u00b7geiz", "dich", "be\u00b7fie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des letzter h\u00f6her steht als du der erst' im niedern.", "tokens": ["Des", "letz\u00b7ter", "h\u00f6\u00b7her", "steht", "als", "du", "der", "er\u00b7st'", "im", "nie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "KOUS", "PPER", "ART", "VVFIN", "APPRART", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}