{"textgrid.poem.49507": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Abt ** Entschlu\u00df bey Erbauung seines Klosters", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil alles nach Verdienst klein oder gro\u00df mu\u00df seyn,", "tokens": ["Weil", "al\u00b7les", "nach", "Ver\u00b7dienst", "klein", "o\u00b7der", "gro\u00df", "mu\u00df", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADJD", "KON", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sey mein E\u00dfsaal gro\u00df, und meine Kirche klein.", "tokens": ["So", "sey", "mein", "E\u00df\u00b7saal", "gro\u00df", ",", "und", "mei\u00b7ne", "Kir\u00b7che", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und macht, die Uhr zu reguliren,", "tokens": ["Und", "macht", ",", "die", "Uhr", "zu", "re\u00b7gu\u00b7li\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und aufzuziehn, und einzuschmieren,", "tokens": ["Und", "auf\u00b7zu\u00b7ziehn", ",", "und", "ein\u00b7zu\u00b7schmie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIZU", "$,", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Br\u00fcdern M\u00fch:", "tokens": ["Den", "Br\