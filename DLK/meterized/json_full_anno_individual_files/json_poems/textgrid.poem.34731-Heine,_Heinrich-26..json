{"textgrid.poem.34731": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Nelken duftig atmen!", "tokens": ["Wie", "die", "Nel\u00b7ken", "duf\u00b7tig", "at\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Sterne, ein Gewimmel", "tokens": ["Wie", "die", "Ster\u00b7ne", ",", "ein", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldner Bienen, \u00e4ngstlich schimmern", "tokens": ["Gold\u00b7ner", "Bie\u00b7nen", ",", "\u00e4ngst\u00b7lich", "schim\u00b7mern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem veilchenblauen Himmel!", "tokens": ["An", "dem", "veil\u00b7chen\u00b7blau\u00b7en", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus dem Dunkel der Kastanien", "tokens": ["Aus", "dem", "Dun\u00b7kel", "der", "Kas\u00b7ta\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Gl\u00e4nzt das Landhaus, wei\u00df und l\u00fcstern,", "tokens": ["Gl\u00e4nzt", "das", "Land\u00b7haus", ",", "wei\u00df", "und", "l\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punc