{"textgrid.poem.28310": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das,", "tokens": ["Das", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Was", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Gott heist,", "tokens": ["Gott", "heist", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wers leist,", "tokens": ["Wers", "leist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der besteht,", "tokens": ["Der", "be\u00b7steht", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wanns gleich geht", "tokens": ["Wanns", "gleich", "geht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Arg \u00fcberau\u00df,", "tokens": ["Arg", "\u00fc\u00b7be\u00b7rau\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Tumm, krumm und krau\u00df.", "tokens": ["Tumm", ",", "krumm", "und", "krau\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Er lacht nur dazu,", "tokens": ["Er", "lacht", "nur", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Was immermehr thu", "tokens": ["Was", "im\u00b7mer\u00b7mehr", "thu"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.11": {"text": "Der Teuffel mit blasen,", "tokens": ["Der", "Teuf\u00b7fel", "mit", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Die Welt mit viel rasen,", "tokens": ["Die", "Welt", "mit", "viel", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Der Tod mit Z\u00e4hne-wetzen,", "tokens": ["Der", "Tod", "mit", "Z\u00e4h\u00b7ne\u00b7wet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Das Fleisch mit dem entsetzen.", "tokens": ["Das", "Fleisch", "mit", "dem", "ent\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Er l\u00e4st ihm di\u00df genugsam seyn,", "tokens": ["Er", "l\u00e4st", "ihm", "di\u00df", "ge\u00b7nug\u00b7sam", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ist seine Krafft gleich schwach und klein,", "tokens": ["Ist", "sei\u00b7ne", "Krafft", "gleich", "schwach", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "O, dem er dient, dem er vertrauet,", "tokens": ["O", ",", "dem", "er", "dient", ",", "dem", "er", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dem hat f\u00fcr keinem noch gegrauet;", "tokens": ["Dem", "hat", "f\u00fcr", "kei\u00b7nem", "noch", "ge\u00b7grau\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was ist ihm als zu wincken mehr zu thun?", "tokens": ["Was", "ist", "ihm", "als", "zu", "win\u00b7cken", "mehr", "zu", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOKOM", "PTKZU", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So f\u00e4llt dahin in einem schnellen nun", "tokens": ["So", "f\u00e4llt", "da\u00b7hin", "in", "ei\u00b7nem", "schnel\u00b7len", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PAV", "APPR", "ART", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Das, was da ist, wie das, das vormals ware.", "tokens": ["Das", ",", "was", "da", "ist", ",", "wie", "das", ",", "das", "vor\u00b7mals", "wa\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VAFIN", "$,", "PWAV", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der steh auff Gott, der stehn wil f\u00fcr Gefahre!", "tokens": ["Der", "steh", "auff", "Gott", ",", "der", "stehn", "wil", "f\u00fcr", "Ge\u00b7fah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "PRELS", "VVINF", "VMFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Er steht viel fester noch, als feste Cedern stehn,", "tokens": ["Er", "steht", "viel", "fes\u00b7ter", "noch", ",", "als", "fes\u00b7te", "Ce\u00b7dern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Regen, Thau, Reiff, Schnee, Frost, Hitze wird angehn;", "tokens": ["Die", "Re\u00b7gen", ",", "Thau", ",", "Reiff", ",", "Schnee", ",", "Frost", ",", "Hit\u00b7ze", "wird", "an\u00b7gehn", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er steht viel fester noch, als auff den Bergen Schl\u00f6sser,", "tokens": ["Er", "steht", "viel", "fes\u00b7ter", "noch", ",", "als", "auff", "den", "Ber\u00b7gen", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Als Felsen im Geh\u00f6ltz, als Klippen im Gew\u00e4sser.", "tokens": ["Als", "Fel\u00b7sen", "im", "Ge\u00b7h\u00f6ltz", ",", "als", "Klip\u00b7pen", "im", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "$,", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer aber seinen Sinn auff Eitelkeiten stellet,", "tokens": ["Wer", "a\u00b7ber", "sei\u00b7nen", "Sinn", "auff", "Ei\u00b7tel\u00b7kei\u00b7ten", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von Gotte sich entzeucht, von seinem Worte f\u00e4llet,", "tokens": ["Von", "Got\u00b7te", "sich", "ent\u00b7zeucht", ",", "von", "sei\u00b7nem", "Wor\u00b7te", "f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der gibt sich auff das Ei\u00df, der nimmt ihm einen Grund,", "tokens": ["Der", "gibt", "sich", "auff", "das", "Ei\u00df", ",", "der", "nimmt", "ihm", "ei\u00b7nen", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der schlipffrig ist, der hoch, der schwanckend ist und rund.", "tokens": ["Der", "schlipf\u00b7frig", "ist", ",", "der", "hoch", ",", "der", "schwan\u00b7ckend", "ist", "und", "rund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das, was er hat, bleibt vielmals nicht bi\u00df morgen,", "tokens": ["Das", ",", "was", "er", "hat", ",", "bleibt", "viel\u00b7mals", "nicht", "bi\u00df", "mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Wird leer von Hab und reich an Angst und Sorgen.", "tokens": ["Wird", "leer", "von", "Hab", "und", "reich", "an", "Angst", "und", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Da\u00df, dem er dient, das wei\u00df ihm selbst nicht Rath;", "tokens": ["Da\u00df", ",", "dem", "er", "dient", ",", "das", "wei\u00df", "ihm", "selbst", "nicht", "Rath", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Sein eigne Witz hat keine Krafft noch That.", "tokens": ["Sein", "eig\u00b7ne", "Witz", "hat", "kei\u00b7ne", "Krafft", "noch", "That", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Das, was er darff, wo soll ers suchen?", "tokens": ["Das", ",", "was", "er", "darff", ",", "wo", "soll", "ers", "su\u00b7chen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VMFIN", "$,", "PWAV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wann man ihn dr\u00e4ngt, was soll sein pochen?", "tokens": ["Wann", "man", "ihn", "dr\u00e4ngt", ",", "was", "soll", "sein", "po\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$,", "PWS", "VMFIN", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wem klagt er Hohn? Wem klagt er Noth?", "tokens": ["Wem", "klagt", "er", "Hohn", "?", "Wem", "klagt", "er", "Noth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$.", "PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Wer zehlt die Threnen? R\u00e4cht den Spot?", "tokens": ["Wer", "zehlt", "die", "Thre\u00b7nen", "?", "R\u00e4cht", "den", "Spot", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und deckt ihn mit dem Schaten?", "tokens": ["Und", "deckt", "ihn", "mit", "dem", "Scha\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Wann er nun soll entrathen", "tokens": ["Wann", "er", "nun", "soll", "ent\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.41": {"text": "De\u00df Lebens im sterben,", "tokens": ["De\u00df", "Le\u00b7bens", "im", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Was hat er zu erben?", "tokens": ["Was", "hat", "er", "zu", "er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.43": {"text": "Das ewige Weh,", "tokens": ["Das", "e\u00b7wi\u00b7ge", "Weh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.44": {"text": "Da nimmer vergeh", "tokens": ["Da", "nim\u00b7mer", "ver\u00b7geh"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Der Wurm und Schmertz", "tokens": ["Der", "Wurm", "und", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "An Leib und Hertz,", "tokens": ["An", "Leib", "und", "Hertz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.47": {"text": "Da sein Gut", "tokens": ["Da", "sein", "Gut"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.48": {"text": "In der Glut", "tokens": ["In", "der", "Glut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+", "measure": "anapaest.init"}, "line.49": {"text": "Wird blo\u00df", "tokens": ["Wird", "blo\u00df"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.50": {"text": "H\u00fclff\u2013lo\u00df", "tokens": ["H\u00fclff", "\u2013", "lo\u00df"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$(", "PTKVZ"], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "Pein", "tokens": ["Pein"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.52": {"text": "Seyn.", "tokens": ["Seyn", "."], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Das,", "tokens": ["Das", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Was", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Gott heist,", "tokens": ["Gott", "heist", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Wers leist,", "tokens": ["Wers", "leist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der besteht,", "tokens": ["Der", "be\u00b7steht", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wanns gleich geht", "tokens": ["Wanns", "gleich", "geht"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Arg \u00fcberau\u00df,", "tokens": ["Arg", "\u00fc\u00b7be\u00b7rau\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Tumm, krumm und krau\u00df.", "tokens": ["Tumm", ",", "krumm", "und", "krau\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJ