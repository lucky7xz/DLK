{"textgrid.poem.65041": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mag h\u00f6hnend auch die Welt dar\u00fcber richten,", "tokens": ["Mag", "h\u00f6h\u00b7nend", "auch", "die", "Welt", "da\u00b7r\u00fc\u00b7ber", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein tiefstes W\u00fcnschen will ich nicht verhehlen:", "tokens": ["Mein", "tiefs\u00b7tes", "W\u00fcn\u00b7schen", "will", "ich", "nicht", "ver\u00b7heh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4tt' ich vom Schicksal eine Gunst zu w\u00e4hlen,", "tokens": ["H\u00e4tt'", "ich", "vom", "Schick\u00b7sal", "ei\u00b7ne", "Gunst", "zu", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich w\u00e4hlte mir den Ruhm, den sonnenlichten!", "tokens": ["Ich", "w\u00e4hl\u00b7te", "mir", "den", "Ruhm", ",", "den", "son\u00b7nen\u00b7lich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O selig Loos, schon hier in Staubesschichten", "tokens": ["O", "se\u00b7lig", "Loos", ",", "schon", "hier", "in", "Stau\u00b7bes\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NE", "$,", "ADV", "ADV", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem Glanz der Ewigkeit sich zu verm\u00e4hlen,", "tokens": ["Dem", "Glanz", "der", "E\u00b7wig\u00b7keit", "sich", "zu", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu jenen Ueberwindern mitzuz\u00e4hlen,", "tokens": ["Zu", "je\u00b7nen", "Ue\u00b7ber\u00b7win\u00b7dern", "mit\u00b7zu\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, g\u00f6tterstark, des Todes Bann vernichten!", "tokens": ["Die", ",", "g\u00f6t\u00b7ter\u00b7stark", ",", "des", "To\u00b7des", "Bann", "ver\u00b7nich\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu wissen, da\u00df die tiefe Schmerzensklage,", "tokens": ["Zu", "wis\u00b7sen", ",", "da\u00df", "die", "tie\u00b7fe", "Schmer\u00b7zens\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freudenhymnen, welche uns enthallen,", "tokens": ["Die", "Freu\u00b7den\u00b7hym\u00b7nen", ",", "wel\u00b7che", "uns", "ent\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein k\u00f6stlich Erbtheil f\u00fcr die sp\u00e4tsten Tage!", "tokens": ["Ein", "k\u00f6st\u00b7lich", "E\u00b7rbtheil", "f\u00fcr", "die", "sp\u00e4ts\u00b7ten", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df unser Name wird auf Erden wallen,", "tokens": ["Da\u00df", "un\u00b7ser", "Na\u00b7me", "wird", "auf", "Er\u00b7den", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn auch schon l\u00e4ngst im stillen Sarkophage", "tokens": ["Wenn", "auch", "schon", "l\u00e4ngst", "im", "stil\u00b7len", "Sar\u00b7ko\u00b7pha\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des gl\u00fch'nden Herzens Aschenrest zerfallen!", "tokens": ["Des", "gl\u00fch'n\u00b7den", "Her\u00b7zens", "A\u00b7schen\u00b7rest", "zer\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mag h\u00f6hnend auch die Welt dar\u00fcber richten,", "tokens": ["Mag", "h\u00f6h\u00b7nend", "auch", "die", "Welt", "da\u00b7r\u00fc\u00b7ber", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN", "PAV", "VVINF", "$,"], 