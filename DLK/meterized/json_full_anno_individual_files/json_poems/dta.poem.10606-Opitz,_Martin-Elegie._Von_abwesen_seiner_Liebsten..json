{"dta.poem.10606": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.  \n  Von abwesen seiner Liebsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Werdt ich die Zeit wol sehn/ da\u00df doch der Tag anbreche/", "tokens": ["Werdt", "ich", "die", "Zeit", "wol", "sehn", "/", "da\u00df", "doch", "der", "Tag", "an\u00b7bre\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darinnen ich mein Lieb noch endtlich schawen soll?", "tokens": ["Da\u00b7rin\u00b7nen", "ich", "mein", "Lieb", "noch", "endt\u00b7lich", "scha\u00b7wen", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Stunden laufft doch fort/ fliegt weg/ als Wasserb\u00e4che/", "tokens": ["Ihr", "Stun\u00b7den", "laufft", "doch", "fort", "/", "fliegt", "weg", "/", "als", "Was\u00b7ser\u00b7b\u00e4\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKVZ", "$(", "VVFIN", "PTKVZ", "$(", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil jhr so langsam seyt/ bin ich gantz trawren voll/", "tokens": ["Weil", "jhr", "so", "lang\u00b7sam", "seyt", "/", "bin", "ich", "gantz", "traw\u00b7ren", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "PPER", "ADV", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wach auff ", "tokens": ["Wach", "auff"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Vnd sprich/ er solle fort/ es sey schon zimlich spat/", "tokens": ["Vnd", "sprich", "/", "er", "sol\u00b7le", "fort", "/", "es", "sey", "schon", "zim\u00b7lich", "spat", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "PPER", "VMFIN", "PTKVZ", "$(", "PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df er betrogen werd/ vnd nahe sich der Erde/", "tokens": ["Da\u00df", "er", "be\u00b7tro\u00b7gen", "werd", "/", "vnd", "na\u00b7he", "sich", "der", "Er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$(", "KON", "APPR", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach Thetis la\u00df jhn gehn den langen Sommergradt.", "tokens": ["Ach", "The\u00b7tis", "la\u00df", "jhn", "gehn", "den", "lan\u00b7gen", "Som\u00b7mer\u00b7gradt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVIMP", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Monde/ warumb denn thustu dich so verweilen?", "tokens": ["Du", "Mon\u00b7de", "/", "wa\u00b7rumb", "denn", "thu\u00b7stu", "dich", "so", "ver\u00b7wei\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "La\u00df doch den ", "tokens": ["La\u00df", "doch", "den"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Sieh\u2019 ob du wohl vermagst die Sonne zuereilen/", "tokens": ["Sieh'", "ob", "du", "wohl", "ver\u00b7magst", "die", "Son\u00b7ne", "zu\u00b7e\u00b7rei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd einzukommen noch in jhr verg\u00fcltes Hau\u00df.", "tokens": ["Vnd", "ein\u00b7zu\u00b7kom\u00b7men", "noch", "in", "jhr", "ver\u00b7g\u00fcl\u00b7tes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich mu\u00df noch manche Stundt in Sorg vnd Kummer schweben/", "tokens": ["Ich", "mu\u00df", "noch", "man\u00b7che", "Stundt", "in", "Sorg", "vnd", "Kum\u00b7mer", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich mu\u00df in Trawrigkeit verbringen lange Zeit/", "tokens": ["Ich", "mu\u00df", "in", "Traw\u00b7rig\u00b7keit", "ver\u00b7brin\u00b7gen", "lan\u00b7ge", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVPP", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Eh da\u00df der Tag anbricht/ darinnen mich mein Leben", "tokens": ["Eh", "da\u00df", "der", "Tag", "an\u00b7bricht", "/", "da\u00b7rin\u00b7nen", "mich", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "VVFIN", "$(", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Anscheine mit dem Liecht der hohen Freundligkeit.", "tokens": ["An\u00b7schei\u00b7ne", "mit", "dem", "Liecht", "der", "ho\u00b7hen", "Freund\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach warumb hab ich doch in mein Gem\u00fcth empfangen", "tokens": ["Ach", "wa\u00b7rumb", "hab", "ich", "doch", "in", "mein", "Ge\u00b7m\u00fcth", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ihr vnerh\u00f6rte Zier/ vnd Tugendt gantz vnd gar?", "tokens": ["Ihr", "vner\u00b7h\u00f6r\u00b7te", "Zier", "/", "vnd", "Tu\u00b7gendt", "gantz", "vnd", "gar", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "NN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Mein Hertze seufftzt nach jhr/ vnd brennet mit verlangen/", "tokens": ["Mein", "Hert\u00b7ze", "seufftzt", "nach", "jhr", "/", "vnd", "bren\u00b7net", "mit", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "$(", "KON", "VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vnd macht mir einen Tag ein gantzes langes Jahr.", "tokens": ["Vnd", "macht", "mir", "ei\u00b7nen", "Tag", "ein", "gant\u00b7zes", "lan\u00b7ges", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als mich das schn\u00f6de Gl\u00fcck von jhrer Hand gerissen/", "tokens": ["Als", "mich", "das", "schn\u00f6\u00b7de", "Gl\u00fcck", "von", "jhrer", "Hand", "ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Hat es zugleiche mich gerissen auch von mir/", "tokens": ["Hat", "es", "zu\u00b7glei\u00b7che", "mich", "ge\u00b7ris\u00b7sen", "auch", "von", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich mu\u00df mein brennend Hertz mit Thr\u00e4nen st\u00e4ts begiessen/", "tokens": ["Ich", "mu\u00df", "mein", "bren\u00b7nend", "Hertz", "mit", "Thr\u00e4\u00b7nen", "st\u00e4ts", "be\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJD", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich bin nicht bey mir selbst/ wann ich nicht bin bey jhr.", "tokens": ["Ich", "bin", "nicht", "bey", "mir", "selbst", "/", "wann", "ich", "nicht", "bin", "bey", "jhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "ADV", "$(", "PWAV", "PPER", "PTKNEG", "VAFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach solt ich sehen nur jhr G\u00f6ttliches Gesichte/", "tokens": ["Ach", "solt", "ich", "se\u00b7hen", "nur", "jhr", "G\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie seelig wehren mir Gedancken/ Muth vnd Sinn/", "tokens": ["Wie", "see\u00b7lig", "weh\u00b7ren", "mir", "Ge\u00b7dan\u00b7cken", "/", "Muth", "vnd", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein eintzig Augenblick von jhrem hellen Liechte", "tokens": ["Ein", "eint\u00b7zig", "Au\u00b7gen\u00b7blick", "von", "jhrem", "hel\u00b7len", "Liech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Ist gar genug/ da\u00df er nem\u2019 all mein Trawren hin.", "tokens": ["Ist", "gar", "ge\u00b7nug", "/", "da\u00df", "er", "nem'", "all", "mein", "Traw\u00b7ren", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$(", "KOUS", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach keme doch die Zeit der hochgew\u00fcnschten frewden/", "tokens": ["Ach", "ke\u00b7me", "doch", "die", "Zeit", "der", "hoch\u00b7ge\u00b7w\u00fcnschten", "frew\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Da\u00df ich erblickte nur den wunderklaren schein/", "tokens": ["Da\u00df", "ich", "er\u00b7blick\u00b7te", "nur", "den", "wun\u00b7der\u00b7kla\u00b7ren", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wann aber ich von jhr mich werde m\u00fcssen scheiden/", "tokens": ["Wann", "a\u00b7ber", "ich", "von", "jhr", "mich", "wer\u00b7de", "m\u00fcs\u00b7sen", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "APPR", "PPER", "PPER", "VAFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Begehr ich weiter nicht bey Leben mehr zusein.", "tokens": ["Be\u00b7gehr", "ich", "wei\u00b7ter", "nicht", "bey", "Le\u00b7ben", "mehr", "zu\u00b7sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}