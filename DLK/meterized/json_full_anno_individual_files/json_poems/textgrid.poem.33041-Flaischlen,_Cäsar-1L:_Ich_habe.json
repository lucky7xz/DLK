ine.7": {"text": "ganz fern aus Kinderzeiten her", "tokens": ["ganz", "fern", "aus", "Kin\u00b7der\u00b7zei\u00b7ten", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "klingt seinen Klang es in mein Leben,", "tokens": ["klingt", "sei\u00b7nen", "Klang", "es", "in", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "voll heimlichen Gl\u00fccks, bald fern, bald nah,", "tokens": ["voll", "heim\u00b7li\u00b7chen", "Gl\u00fccks", ",", "bald", "fern", ",", "bald", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "pl\u00f6tzlich verstummt und pl\u00f6tzlich wieder da ...", "tokens": ["pl\u00f6tz\u00b7lich", "ver\u00b7stummt", "und", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "da", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ADJD", "ADV", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ein Lachen, wei\u00dft du, wie's im Walde lacht,", "tokens": ["Ein", "La\u00b7chen", ",", "wei\u00dft", "du", ",", "wie's", "im", "Wal\u00b7de", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wenn in Hochsommermittern\u00e4chten,", "tokens": ["wenn", "in", "Hoch\u00b7som\u00b7mer\u00b7mit\u00b7ter