{"dta.poem.2720": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Hn. A. O. den 3. Octobr.  \n 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Oehm wird auff die Mess'/ ihr Leipziger/ nicht kom-\nmen/", "tokens": ["Herr", "O\u00b7ehm", "wird", "auff", "die", "Mess'", "/", "ihr", "Leip\u00b7zi\u00b7ger", "/", "nicht", "kom", "men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "PTKNEG", "TRUNC", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Umbsonst erwartet ihn Gew\u00f6lbe/ Tisch und Haus.", "tokens": ["Um\u00b7bsonst", "er\u00b7war\u00b7tet", "ihn", "Ge\u00b7w\u00f6l\u00b7be", "/", "Tisch", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und habt ihr noch zur Zeit die Ursach nicht vernommen?", "tokens": ["Und", "habt", "ihr", "noch", "zur", "Zeit", "die", "Ur\u00b7sach", "nicht", "ver\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er zahlt itzt der Natur den letzten Wechsel aus.", "tokens": ["Er", "zahlt", "itzt", "der", "Na\u00b7tur", "den", "letz\u00b7ten", "Wech\u00b7sel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er macht itzt einen Schlu\u00df auff mehr als Millionen/", "tokens": ["Er", "macht", "itzt", "ei\u00b7nen", "Schlu\u00df", "auff", "mehr", "als", "Mil\u00b7lion\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Verkehrt sein fahrend Haab in ein best\u00e4ndig Gut.", "tokens": ["Ver\u00b7kehrt", "sein", "fah\u00b7rend", "Ha\u00b7ab", "in", "ein", "be\u00b7st\u00e4n\u00b7dig", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "ADJD", "NN", "APPR", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Gewinnt/ o Wunderwerck! an Pfennigen Duplonen:", "tokens": ["Ge\u00b7winnt", "/", "o", "Wun\u00b7der\u00b7werck", "!", "an", "Pfen\u00b7ni\u00b7gen", "Du\u00b7plo\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "FM", "NN", "$.", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ists m\u00f6glich da\u00df ein Mann so grosse Dinge thut?", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "ein", "Mann", "so", "gros\u00b7se", "Din\u00b7ge", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wohl dem der also die Conte hat gestellet/", "tokens": ["Und", "wohl", "dem", "der", "al\u00b7so", "die", "Con\u00b7te", "hat", "ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ART", "ADV", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Wie richtig wird er nicht die letzte Stunde stehn!", "tokens": ["Wie", "rich\u00b7tig", "wird", "er", "nicht", "die", "letz\u00b7te", "Stun\u00b7de", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich wei\u00df/ ihr Leipziger/ da\u00df ihr das Urtheil f\u00e4llet:", "tokens": ["Ich", "wei\u00df", "/", "ihr", "Leip\u00b7zi\u00b7ger", "/", "da\u00df", "ihr", "das", "Ur\u00b7theil", "f\u00e4l\u00b7let", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Herr Oehm hat ausgezahlt/ er mag nun weiter gehn.", "tokens": ["Herr", "O\u00b7ehm", "hat", "aus\u00b7ge\u00b7zahlt", "/", "er", "mag", "nun", "wei\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "$(", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "Und freylich geht er weg vom Jahrmarckt aller S\u00fcnden/", "tokens": ["Und", "frey\u00b7lich", "geht", "er", "weg", "vom", "Jahr\u00b7marckt", "al\u00b7ler", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von der grundb\u00f6sen Welt/ der Messe voller Trug/", "tokens": ["Von", "der", "grund\u00b7b\u00f6\u00b7sen", "Welt", "/", "der", "Mes\u00b7se", "vol\u00b7ler", "Trug", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo ein neu Ungeheur von Lastern stets zufinden/", "tokens": ["Wo", "ein", "neu", "Un\u00b7ge\u00b7heur", "von", "Las\u00b7tern", "stets", "zu\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "APPR", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo Wei\u00dfheit n\u00e4rrisch ist/ und Narrheit wei\u00df und klug:", "tokens": ["Wo", "Wei\u00df\u00b7heit", "n\u00e4r\u00b7risch", "ist", "/", "und", "Nar\u00b7rheit", "wei\u00df", "und", "klug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VAFIN", "$(", "KON", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wo wir den Kindern gleich mit Tocken-Spiele handeln/", "tokens": ["Wo", "wir", "den", "Kin\u00b7dern", "gleich", "mit", "To\u00b7cken\u00b7Spie\u00b7le", "han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo wir bey Mangel reich/ und arm bey Reichthum seyn.", "tokens": ["Wo", "wir", "bey", "Man\u00b7gel", "reich", "/", "und", "arm", "bey", "Reicht\u00b7hum", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "$(", "KON", "ADJD", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gleich Fechtern/ die geblendt nur in dem finstern wandeln/", "tokens": ["Gleich", "Fech\u00b7tern", "/", "die", "ge\u00b7blendt", "nur", "in", "dem", "fins\u00b7tern", "wan\u00b7deln", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$(", "ART", "VVFIN", "ADV", "APPR", "ART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wechseln Kie\u00df und Gla\u00df vor Perl und Demant ein.", "tokens": ["Und", "wech\u00b7seln", "Kie\u00df", "und", "Gla\u00df", "vor", "Perl", "und", "De\u00b7mant", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wo keine Flacke nicht steckt unserm Geld-Durst Gr\u00e4ntzen/", "tokens": ["Wo", "kei\u00b7ne", "Fla\u00b7cke", "nicht", "steckt", "un\u00b7serm", "Geld\u00b7Durst", "Gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo uns die Element nicht sattsam geben Zoll;", "tokens": ["Wo", "uns", "die", "E\u00b7le\u00b7ment", "nicht", "satt\u00b7sam", "ge\u00b7ben", "Zoll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo unsre D\u00fcrfftigkeit kein Zuflu\u00df kan erg\u00e4ntzen", "tokens": ["Wo", "uns\u00b7re", "D\u00fcr\u00b7ff\u00b7tig\u00b7keit", "kein", "Zu\u00b7flu\u00df", "kan", "er\u00b7g\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.24": {"text": "Und jeder Platz und Raum ist von Banditen voll.", "tokens": ["Und", "je\u00b7der", "Platz", "und", "Raum", "ist", "von", "Ban\u00b7di\u00b7ten", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo Worte sonder Werck/ und Wercke sonder Liebe/", "tokens": ["Wo", "Wor\u00b7te", "son\u00b7der", "Werck", "/", "und", "Wer\u00b7cke", "son\u00b7der", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$(", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wo ein verstelltes Hertz das Muster dieser Zeit.", "tokens": ["Wo", "ein", "ver\u00b7stell\u00b7tes", "Hertz", "das", "Mus\u00b7ter", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wo Blicke Stricke sind/ und Minen solche Diebe/", "tokens": ["Wo", "Bli\u00b7cke", "Stri\u00b7cke", "sind", "/", "und", "Mi\u00b7nen", "sol\u00b7che", "Die\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VAFIN", "$(", "KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die heimlich brechen ein durch Schertz und Freundligkeit.", "tokens": ["Die", "heim\u00b7lich", "bre\u00b7chen", "ein", "durch", "Schertz", "und", "Freund\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wo in dem Eigen-Nutz das Hertze wird entbrennen/", "tokens": ["Wo", "in", "dem", "Ei\u00b7gen\u00b7Nutz", "das", "Hert\u00b7ze", "wird", "ent\u00b7bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PDS", "VVFIN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wenns am Nechsten k\u00f6mmt/ ist Lieb und Wohlthat kalt.", "tokens": ["Und", "wenns", "am", "Nechs\u00b7ten", "k\u00f6mmt", "/", "ist", "Lieb", "und", "Wohlt\u00b7hat", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "VVFIN", "$(", "VAFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo wir die Balcken leicht bey uns/ nicht Splitter kennen/", "tokens": ["Wo", "wir", "die", "Bal\u00b7cken", "leicht", "bey", "uns", "/", "nicht", "Split\u00b7ter", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "APPR", "PPER", "$(", "PTKNEG", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und \u00e4ndern nach dem Gl\u00fcck auch Sinnen und Gestalt;", "tokens": ["Und", "\u00e4n\u00b7dern", "nach", "dem", "Gl\u00fcck", "auch", "Sin\u00b7nen", "und", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wo wir an eignen Ruhm/ an GOtt sehr wenig dencken", "tokens": ["Wo", "wir", "an", "eig\u00b7nen", "Ruhm", "/", "an", "Gott", "sehr", "we\u00b7nig", "den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$(", "APPR", "NN", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und dessen Ehr und Furcht nur l\u00f6blicher Gebrauch.", "tokens": ["Und", "des\u00b7sen", "Ehr", "und", "Furcht", "nur", "l\u00f6b\u00b7li\u00b7cher", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "KON", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wo wir dem Teuffel Gold und fetten Storax schencken/", "tokens": ["Wo", "wir", "dem", "Teuf\u00b7fel", "Gold", "und", "fet\u00b7ten", "Sto\u00b7rax", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hingegen unserm GOtt der S\u00fcnden Dampff und Rauch;", "tokens": ["Hin\u00b7ge\u00b7gen", "un\u00b7serm", "Gott", "der", "S\u00fcn\u00b7den", "Dampff", "und", "Rauch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Wo jeder fehlen wil nur andern zugefallen/", "tokens": ["Wo", "je\u00b7der", "feh\u00b7len", "wil", "nur", "an\u00b7dern", "zu\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "VMFIN", "ADV", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wo Treu und Eydschwur nur ein Mantel nach der Welt;", "tokens": ["Wo", "Treu", "und", "Eyd\u00b7schwur", "nur", "ein", "Man\u00b7tel", "nach", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NE", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wo Honig in dem Mund und in dem Hertzen Gallen/", "tokens": ["Wo", "Ho\u00b7nig", "in", "dem", "Mund", "und", "in", "dem", "Hert\u00b7zen", "Gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wo man sich zu dem Theil/ das obgesieget/ h\u00e4lt.", "tokens": ["Wo", "man", "sich", "zu", "dem", "Theil", "/", "das", "ob\u00b7ge\u00b7sie\u00b7get", "/", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "APPR", "ART", "NN", "$(", "PDS", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "al