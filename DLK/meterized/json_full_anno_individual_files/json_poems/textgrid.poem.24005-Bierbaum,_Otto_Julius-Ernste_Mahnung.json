{"textgrid.poem.24005": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ernste Mahnung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine lachenden Augen ruhen auf mir", "tokens": ["Dei\u00b7ne", "la\u00b7chen\u00b7den", "Au\u00b7gen", "ru\u00b7hen", "auf", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sonnenscheinwarm und tr\u00f6sten mein Herz;", "tokens": ["Son\u00b7nen\u00b7schein\u00b7warm", "und", "tr\u00f6s\u00b7ten", "mein", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dein kleines Gr\u00fcbchen der rechten Wange", "tokens": ["Dein", "klei\u00b7nes", "Gr\u00fcb\u00b7chen", "der", "rech\u00b7ten", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Macht lustig mein Herz, denk ich blos seiner;", "tokens": ["Macht", "lus\u00b7tig", "mein", "Herz", ",", "denk", "ich", "blos", "sei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Dein rascher Schritt belebt mein Auge", "tokens": ["Dein", "ra\u00b7scher", "Schritt", "be\u00b7lebt", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und spendet Fl\u00fcgel meinen Gedanken;", "tokens": ["Und", "spen\u00b7det", "Fl\u00fc\u00b7gel", "mei\u00b7nen", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dein Schelmenkinn d\u00fcnkt mich so witzig", "tokens": ["Dein", "Schel\u00b7men\u00b7kinn", "d\u00fcnkt", "mich", "so", "wit\u00b7zig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie zehn franz\u00f6sische Kom\u00f6dien", "tokens": ["Wie", "zehn", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "Ko\u00b7m\u00f6\u00b7di\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und drei\u00dfigtausend urgermanische;", "tokens": ["Und", "drei\u00b7\u00dfig\u00b7tau\u00b7send", "ur\u00b7ger\u00b7ma\u00b7ni\u00b7sche", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Deiner Lippen geschwungener Liebesbogen", "tokens": ["Dei\u00b7ner", "Lip\u00b7pen", "ge\u00b7schwun\u00b7ge\u00b7ner", "Lie\u00b7bes\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Jagt Ku\u00dfwild auf in meinem Herzen", "tokens": ["Jagt", "Ku\u00df\u00b7wild", "auf", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "(ich denke du f