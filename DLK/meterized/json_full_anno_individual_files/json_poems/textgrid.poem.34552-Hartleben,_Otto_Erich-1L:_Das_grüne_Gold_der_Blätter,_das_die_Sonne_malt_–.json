{"textgrid.poem.34552": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das gr\u00fcne Gold der Bl\u00e4tter, das die Sonne malt \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das gr\u00fcne Gold der Bl\u00e4tter, das die Sonne malt \u2013", "tokens": ["Das", "gr\u00fc\u00b7ne", "Gold", "der", "Bl\u00e4t\u00b7ter", ",", "das", "die", "Son\u00b7ne", "malt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich seh es noch, wie's dir vom weissen Kleide blitzt,", "tokens": ["ich", "seh", "es", "noch", ",", "wie's", "dir", "vom", "weis\u00b7sen", "Klei\u00b7de", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und f\u00fchle deine H\u00e4nde noch auf meinem Haar ...", "tokens": ["und", "f\u00fch\u00b7le", "dei\u00b7ne", "H\u00e4n\u00b7de", "noch", "auf", "mei\u00b7nem", "Haar", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die wilden Blumen dufteten rings so stark und s\u00fcss.", "tokens": ["Die", "wil\u00b7den", "Blu\u00b7men", "duf\u00b7te\u00b7ten", "rings", "so", "stark", "und", "s\u00fcss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Was sprachst du doch? \u2013 Ich h\u00f6re deine Stimme nicht,", "tokens": ["Was", "sprachst", "du", "doch", "?", "\u2013", "Ich", "h\u00f6\u00b7re", "dei\u00b7ne", "Stim\u00b7me", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vergebens sinn ich ihrem fernen Klange nach.", "tokens": ["ver\u00b7ge\u00b7bens", "sinn", "ich", "ih\u00b7rem", "fer\u00b7nen", "Klan\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin allein \u2013 in meine offnen H\u00e4nde f\u00e4llt", "tokens": ["Ich", "bin", "al\u00b7lein", "\u2013", "in", "mei\u00b7ne", "off\u00b7nen", "H\u00e4n\u00b7de", "f\u00e4llt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$(", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das gr\u00fcne Gold der Bl\u00e4tter, das die Sonne malt.", "tokens": ["das", "gr\u00fc\u00b7ne", "Gold", "der", "Bl\u00e4t\u00b7ter", ",", "das", "die", "Son\u00b7ne", "malt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text":