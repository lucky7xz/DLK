{"textgrid.poem.60698": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Toter ging in Trauer hin", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Toter ging in Trauer hin", "tokens": ["Ein", "To\u00b7ter", "ging", "in", "Trau\u00b7er", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Ort der letzten Ruhest\u00e4tten,", "tokens": ["Zum", "Ort", "der", "letz\u00b7ten", "Ru\u00b7he\u00b7st\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ohne Leid begleitet ihn", "tokens": ["Und", "oh\u00b7ne", "Leid", "be\u00b7glei\u00b7tet", "ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Pfarrer, um ihn einzubetten.", "tokens": ["Ein", "Pfar\u00b7rer", ",", "um", "ihn", "ein\u00b7zu\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Abgeschiedne fuhr im Wagen.", "tokens": ["Der", "Ab\u00b7ge\u00b7schied\u00b7ne", "fuhr", "im", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er war nach altem Brauch verwahrt", "tokens": ["Er", "war", "nach", "al\u00b7tem", "Brauch", "ver\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und trug ein h\u00f6lzern Kleid der Art,", "tokens": ["Und", "trug", "ein", "h\u00f6l\u00b7zern", "Kleid", "der", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Tote allezeit es tragen,", "tokens": ["Wie", "To\u00b7te", "al\u00b7le\u00b7zeit", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ohne es jemals abzulegen.", "tokens": ["Oh\u00b7ne", "es", "je\u00b7mals", "ab\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "VVIZU", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Der Pfarrer schritt dem Mann zur Seite,", "tokens": ["Der", "Pfar\u00b7rer", "schritt", "dem", "Mann", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dem er Gebet und Spr\u00fcche weihte,", "tokens": ["Dem", "er", "Ge\u00b7bet", "und", "Spr\u00fc\u00b7che", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Und dachte an den blanken Segen,", "tokens": ["Und", "dach\u00b7te", "an", "den", "blan\u00b7ken", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "V