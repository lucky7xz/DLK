 ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Vom besten Tropfen ringsumher;", "tokens": ["Vom", "bes\u00b7ten", "Trop\u00b7fen", "rings\u00b7um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sie w\u00fcrden, was ihm gleichfalls sehr", "tokens": ["Sie", "w\u00fcr\u00b7den", ",", "was", "ihm", "gleich\u00b7falls", "sehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Am Herzen lag, f\u00fcr seine nette", "tokens": ["Am", "Her\u00b7zen", "lag", ",", "f\u00fcr", "sei\u00b7ne", "net\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Gef\u00e4llige Nichte etwas bringen,", "tokens": ["Ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Nich\u00b7te", "et\u00b7was", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Auch f\u00fcr die K\u00f6chin, die Babette,", "tokens": ["Auch", "f\u00fcr", "die", "K\u00f6\u00b7chin", ",", "die", "Ba\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Ein h\u00fcbsches Kleid vor allen Dingen.", "tokens": ["Ein", "h\u00fcb\u00b7sches", "Kleid", "vor", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word",