{"textgrid.poem.48247": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Luren-Konzert", "genre": "verse", "period": "N.A.", "pub_year": 1895, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Kopenhagen, gro\u00df und gesperrt,", "tokens": ["In", "Ko\u00b7pen\u00b7ha\u00b7gen", ",", "gro\u00df", "und", "ge\u00b7sperrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Am Saal-Eingange stand: ", "tokens": ["Am", "Saa\u00b7lEin\u00b7gan\u00b7ge", "stand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und an meinen Gastfreund jener Tage", "tokens": ["Und", "an", "mei\u00b7nen", "Gast\u00b7freund", "je\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Richte voll Neugier ich die Frage:", "tokens": ["Rich\u00b7te", "voll", "Neu\u00b7gier", "ich", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbsage, was meint das? Bis Fausts Lemuren", "tokens": ["\u00bb", "sa\u00b7ge", ",", "was", "meint", "das", "?", "Bis", "Fausts", "Le\u00b7mu\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "PDS", "$.", "APPR", "NN", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Reicht es gerade. Doch was sind Luren?\u00ab", "tokens": ["Reicht", "es", "ge\u00b7ra\u00b7de", ".", "Doch", "was", "sind", "Lu\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "KON", "PWS", "VAFIN", "NN", "$.", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "\u00bbluren, in Tagen der Goten und Geten,", "tokens": ["\u00bb", "lu\u00b7ren", ",", "in", "Ta\u00b7gen", "der", "Go\u00b7ten", "und", "Ge\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "APPR", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Hie\u00dfen unsre Nordlands-Trompeten,", "tokens": ["Hie\u00b7\u00dfen", "uns\u00b7re", "Nord\u00b7lands\u00b7Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "H\u00f6rner waren's, von sieben Fu\u00df L\u00e4nge,", "tokens": ["H\u00f6r\u00b7ner", "wa\u00b7ren's", ",", "von", "sie\u00b7ben", "Fu\u00df", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "APPR", "CARD", "NN", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schlachtruf waren ihre Kl\u00e4nge,", "tokens": ["Schlach\u00b7truf", "wa\u00b7ren", "ih\u00b7re", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Luren, lange vor Gorm dem Alten,", "tokens": ["Die", "Lu\u00b7ren", ",", "lan\u00b7ge", "vor", "Gorm", "dem", "Al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00dcbers Moor und \u00fcber die Heide schallten ...", "tokens": ["\u00dc\u00b7bers", "Moor", "und", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "schall\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wo der Steindamm sich hinzieht, stieben die Funken,", "tokens": ["Wo", "der", "Stein\u00b7damm", "sich", "hin\u00b7zieht", ",", "stie\u00b7ben", "die", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "In den Sumpf ist Ro\u00df und Tro\u00df versunken,", "tokens": ["In", "den", "Sumpf", "ist", "Ro\u00df", "und", "Tro\u00df", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und versunken unter die Binsen und Gr\u00e4ser", "tokens": ["Und", "ver\u00b7sun\u00b7ken", "un\u00b7ter", "die", "Bin\u00b7sen", "und", "Gr\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Waren zuletzt auch die Lurenbl\u00e4ser.", "tokens": ["Wa\u00b7ren", "zu\u00b7letzt", "auch", "die", "Lu\u00b7ren\u00b7bl\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Da lagen sie. Bis zu zweitausend Jahren", "tokens": ["Da", "la\u00b7gen", "sie", ".", "Bis", "zu", "zweit\u00b7au\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "APPR", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind Nebel und Wind dr\u00fcber hingefahren,", "tokens": ["Sind", "Ne\u00b7bel", "und", "Wind", "dr\u00fc\u00b7ber", "hin\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PAV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Eines Tages aber grub man, und Schwert und Knauf", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "a\u00b7ber", "grub", "man", ",", "und", "Schwert", "und", "Knauf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PIS", "$,", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und die Luren auch stiegen wieder herauf,", "tokens": ["Und", "die", "Lu\u00b7ren", "auch", "stie\u00b7gen", "wie\u00b7der", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Herauf aus dem Moorgrund unterm Rasen,", "tokens": ["Her\u00b7auf", "aus", "dem", "Mo\u00b7or\u00b7grund", "un\u00b7term", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und auf diesen Luren wird heute geblasen.\u00ab", "tokens": ["Und", "auf", "die\u00b7sen", "Lu\u00b7ren", "wird", "heu\u00b7te", "ge\u00b7bla\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Ein tret' ich. Im Saal, an Estrad' und Wand,", "tokens": ["Ein", "tret'", "ich", ".", "Im", "Saal", ",", "an", "E\u00b7strad'", "und", "Wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "$.", "APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sitzen sch\u00f6ne Frauen, die F\u00e4cher in Hand;", "tokens": ["Sit\u00b7zen", "sch\u00f6\u00b7ne", "Frau\u00b7en", ",", "die", "F\u00e4\u00b7cher", "in", "Hand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Luftig die Kleider, kokett die H\u00fcte,", "tokens": ["Luf\u00b7tig", "die", "Klei\u00b7der", ",", "ko\u00b7kett", "die", "H\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Vorn an der Brust eine Heidekrautbl\u00fcte,", "tokens": ["Vorn", "an", "der", "Brust", "ei\u00b7ne", "Hei\u00b7de\u00b7kraut\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "So sitzen sie da; Lorgnon und Gl\u00e4ser", "tokens": ["So", "sit\u00b7zen", "sie", "da", ";", "Lorg\u00b7non", "und", "Gl\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "NE", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Richten sich auf die Lurenbl\u00e4ser.", "tokens": ["Rich\u00b7ten", "sich", "auf", "die", "Lu\u00b7ren\u00b7bl\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Das sind ihrer drei. Blond-nordisch ihr Haar,", "tokens": ["Das", "sind", "ih\u00b7rer", "drei", ".", "Blon\u00b7dnor\u00b7disch", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "CARD", "$.", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Keiner \u00fcber drei\u00dfig Jahr,", "tokens": ["Kei\u00b7ner", "\u00fc\u00b7ber", "drei\u00b7\u00dfig", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Br\u00fcstung jetzt sind sie herangetreten,", "tokens": ["An", "die", "Br\u00fcs\u00b7tung", "jetzt", "sind", "sie", "her\u00b7an\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Hoch heben sie langsam ihre Trompeten,", "tokens": ["Hoch", "he\u00b7ben", "sie", "lang\u00b7sam", "ih\u00b7re", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und die Luren, so lang' in Tod gebunden,", "tokens": ["Und", "die", "Lu\u00b7ren", ",", "so", "lang'", "in", "Tod", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Haben aufs neue Leben gefunden.", "tokens": ["Ha\u00b7ben", "aufs", "neu\u00b7e", "Le\u00b7ben", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Es fallen die Schwerter, es klappen die Schilde,", "tokens": ["Es", "fal\u00b7len", "die", "Schwer\u00b7ter", ",", "es", "klap\u00b7pen", "die", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Walk\u00fcren jagen, es jagt Brunhilde,", "tokens": ["Wal\u00b7k\u00fc\u00b7ren", "ja\u00b7gen", ",", "es", "jagt", "Brun\u00b7hil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von der Toten hochaufget\u00fcrmtem Wall", "tokens": ["Von", "der", "To\u00b7ten", "hoch\u00b7auf\u00b7ge\u00b7t\u00fcrm\u00b7tem", "Wall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-++-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Aufw\u00e4rts geht es nach Walhall.", "tokens": ["Auf\u00b7w\u00e4rts", "geht", "es", "nach", "Wal\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Und nun verklingt es; die K\u00f6pfe geneigt,", "tokens": ["Und", "nun", "ver\u00b7klingt", "es", ";", "die", "K\u00f6p\u00b7fe", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lauscht noch alles, als alles schon schweigt.", "tokens": ["Lauscht", "noch", "al\u00b7les", ",", "als", "al\u00b7les", "schon", "schweigt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Drau\u00dfen am Eingang, gro\u00df und gesperrt,", "tokens": ["Drau\u00b7\u00dfen", "am", "Ein\u00b7gang", ",", "gro\u00df", "und", "ge\u00b7sperrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Las ich noch einmal: ", "tokens": ["Las", "ich", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "In Kopenhagen, gro\u00df und gesperrt,", "tokens": ["In", "Ko\u00b7pen\u00b7ha\u00b7gen", ",", "gro\u00df", "und", "ge\u00b7sperrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Am Saal-Eingange stand: ", "tokens": ["Am", "Saa\u00b7lEin\u00b7gan\u00b7ge", "stand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und an meinen Gastfreund jener Tage", "tokens": ["Und", "an", "mei\u00b7nen", "Gast\u00b7freund", "je\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Richte voll Neugier ich die Frage:", "tokens": ["Rich\u00b7te", "voll", "Neu\u00b7gier", "ich", "die", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "\u00bbsage, was meint das? Bis Fausts Lemuren", "tokens": ["\u00bb", "sa\u00b7ge", ",", "was", "meint", "das", "?", "Bis", "Fausts", "Le\u00b7mu\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "PWS", "VVFIN", "PDS", "$.", "APPR", "NN", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Reicht es gerade. Doch was sind Luren?\u00ab", "tokens": ["Reicht", "es", "ge\u00b7ra\u00b7de", ".", "Doch", "was", "sind", "Lu\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "KON", "PWS", "VAFIN", "NN", "$.", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "\u00bbluren, in Tagen der Goten und Geten,", "tokens": ["\u00bb", "lu\u00b7ren", ",", "in", "Ta\u00b7gen", "der", "Go\u00b7ten", "und", "Ge\u00b7ten", ","], "token_info": ["punc