{"textgrid.poem.62272": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[jeder Glaube ist der rechte]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder Glaube ist der rechte,", "tokens": ["Je\u00b7der", "Glau\u00b7be", "ist", "der", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Liebe, sie, die echte,", "tokens": ["Wenn", "die", "Lie\u00b7be", ",", "sie", ",", "die", "ech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen, die am Himmel thront,", "tokens": ["Je\u00b7nen", ",", "die", "am", "Him\u00b7mel", "thront", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Herz' des Menschen wohnt.", "tokens": ["In", "dem", "Her\u00b7z'", "des", "Men\u00b7schen", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Liebe allumfassend", "tokens": ["Je\u00b7ne", "Lie\u00b7be", "al\u00b7lum\u00b7fas\u00b7send"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mensch und Tier, und niemand hassend,", "tokens": ["Mensch", "und", "Tier", ",", "und", "nie\u00b7mand", "has\u00b7send", ","], "token_