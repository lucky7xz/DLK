{"textgrid.poem.35500": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Offenbarung", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Nacht, zur Nacht an den Wassern ich ging \u2013", "tokens": ["Zur", "Nacht", ",", "zur", "Nacht", "an", "den", "Was\u00b7sern", "ich", "ging", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Nacht lag schwarz, zerlastend, schw\u00fcl ...", "tokens": ["Die", "Nacht", "lag", "schwarz", ",", "zer\u00b7las\u00b7tend", ",", "schw\u00fcl", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVPP", "$,", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meiner Seele Angstgef\u00fchl", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le", "Angst\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit zuckenden Fingern die Engnis umfing ...", "tokens": ["Mit", "zu\u00b7cken\u00b7den", "Fin\u00b7gern", "die", "Eng\u00b7nis", "um\u00b7fing", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Von den Wassern herauf erklang es, erscholl,", "tokens": ["Von", "den", "Was\u00b7sern", "her\u00b7auf", "er\u00b7klang", "es", ",", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "$,", "ADJD", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Als orgelte drunten ein Sturmchoral,", "tokens": ["Als", "or\u00b7gel\u00b7te", "drun\u00b7ten", "ein", "Sturmc\u00b7ho\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und doch war die Welt des Schweigens so voll \u2013", "tokens": ["Und", "doch", "war", "die", "Welt", "des", "Schwei\u00b7gens", "so", "voll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur in mir schrie die Qual ...", "tokens": ["Nur", "in", "mir", "schrie", "die", "Qual", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Nacht zerdr\u00fcckte mich und zerschmolz", "tokens": ["Die", "Nacht", "zer\u00b7dr\u00fcck\u00b7te", "mich", "und", "zer\u00b7schmolz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mit br\u00fcnstigem Atem, was einst empor", "tokens": ["Mit", "br\u00fcns\u00b7ti\u00b7gem", "A\u00b7tem", ",", "was", "einst", "em\u00b7por"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "PTKVZ"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "In m\u00e4rzigen Tr\u00e4umen sich reckte so stolz \u2013", "tokens": ["In", "m\u00e4r\u00b7zi\u00b7gen", "Tr\u00e4u\u00b7men", "sich", "reck\u00b7te", "so", "stolz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Draus aber kroch Angst und Furcht hervor ...", "tokens": ["Draus", "a\u00b7ber", "kroch", "Angst", "und", "Furcht", "her\u00b7vor", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nur Furcht vor dem hellen, dem harten Licht,", "tokens": ["Nur", "Furcht", "vor", "dem", "hel\u00b7len", ",", "dem", "har\u00b7ten", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das alles in zwingende N\u00e4he schiebt,", "tokens": ["Das", "al\u00b7les", "in", "zwin\u00b7gen\u00b7de", "N\u00e4\u00b7he", "schiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dran meiner Seele harmonisch Gedicht", "tokens": ["Dran", "mei\u00b7ner", "See\u00b7le", "har\u00b7mo\u00b7nisch", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "In tausend Fetzen und Splitter zerstiebt ...", "tokens": ["In", "tau\u00b7send", "Fet\u00b7zen", "und", "Split\u00b7ter", "zer\u00b7stiebt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Der Wind strich feucht und die Flut lief sacht \u2013", "tokens": ["Der", "Wind", "strich", "feucht", "und", "die", "Flut", "lief", "sacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich deckte der Nacht blauschwarzer Schild \u2013 \u2013", "tokens": ["Mich", "deck\u00b7te", "der", "Nacht", "blau\u00b7schwar\u00b7zer", "Schild", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da hat es sich mir in Gnaden enth\u00fcllt", "tokens": ["Da", "hat", "es", "sich", "mir", "in", "Gna\u00b7den", "ent\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und satte Genesung mir eingebracht ...", "tokens": ["Und", "sat\u00b7te", "Ge\u00b7ne\u00b7sung", "mir", "ein\u00b7ge\u00b7bracht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl tr\u00f6stet die Nacht und z\u00e4rtlich gibt", "tokens": ["Wohl", "tr\u00f6s\u00b7tet", "die", "Nacht", "und", "z\u00e4rt\u00b7lich", "gibt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie der Einsamkeit Brust dem Verirrten hin \u2013", "tokens": ["Sie", "der", "Ein\u00b7sam\u00b7keit", "Brust", "dem", "Ver\u00b7irr\u00b7ten", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie hat die Verlassenen immer geliebt", "tokens": ["Sie", "hat", "die", "Ver\u00b7las\u00b7se\u00b7nen", "im\u00b7mer", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und den wundenzerfolterten Duldersinn.", "tokens": ["Und", "den", "wun\u00b7den\u00b7zer\u00b7fol\u00b7ter\u00b7ten", "Dul\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Sie d\u00e4mpft das Weh und blendet den Blick", "tokens": ["Sie", "d\u00e4mpft", "das", "Weh", "und", "blen\u00b7det", "den", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vor des Tages zerkr\u00fcmelter Vielheitswelt \u2013", "tokens": ["Vor", "des", "Ta\u00b7ges", "zer\u00b7kr\u00fc\u00b7mel\u00b7ter", "Viel\u00b7heits\u00b7welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch, wenn sich der Himmel im Osten erhellt,", "tokens": ["Doch", ",", "wenn", "sich", "der", "Him\u00b7mel", "im", "Os\u00b7ten", "er\u00b7hellt", ","], "token_info": ["word", "punct", "wo