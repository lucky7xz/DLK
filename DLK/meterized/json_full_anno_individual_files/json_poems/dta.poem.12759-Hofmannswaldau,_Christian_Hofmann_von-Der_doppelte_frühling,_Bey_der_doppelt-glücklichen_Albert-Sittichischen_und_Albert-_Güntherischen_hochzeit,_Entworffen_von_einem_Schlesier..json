{"dta.poem.12759": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der doppelte fr\u00fchling,  \n Bey der doppelt-gl\u00fccklichen  \n  Albert-Sittichischen und Albert-  \n G\u00fcntherischen hochzeit,  \n Entworffen von einem Schlesier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die erd ist doch des himmels seine braut;", "tokens": ["Die", "erd", "ist", "doch", "des", "him\u00b7mels", "sei\u00b7ne", "braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schm\u00fcckt er itzt mit blumen statt der seide;", "tokens": ["Die", "schm\u00fcckt", "er", "itzt", "mit", "blu\u00b7men", "statt", "der", "sei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Ost und West sonst theures in sich schaut,", "tokens": ["Was", "Ost", "und", "West", "sonst", "theu\u00b7res", "in", "sich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ADV", "ADJA", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das steht besch\u00e4mt vor diesem wunder-kleide.", "tokens": ["Das", "steht", "be\u00b7sch\u00e4mt", "vor", "die\u00b7sem", "wun\u00b7der\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "P\u00f6snien entf\u00e4rben den rubin,", "tokens": ["P\u00f6s\u00b7ni\u00b7en", "ent\u00b7f\u00e4r\u00b7ben", "den", "ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der perlen schnee weicht gerne den nareissen,", "tokens": ["Der", "per\u00b7len", "schnee", "weicht", "ger\u00b7ne", "den", "na\u00b7reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der hiacint sticht jeden t\u00fcrckis hin,", "tokens": ["Der", "hia\u00b7cint", "sticht", "je\u00b7den", "t\u00fcr\u00b7ckis", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der ametist hat feilgen nachgehn m\u00fcssen.", "tokens": ["Der", "a\u00b7me\u00b7tist", "hat", "feil\u00b7gen", "nach\u00b7gehn", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das schlechte gra\u00df ist mit smaragd beleget,", "tokens": ["Das", "schlech\u00b7te", "gra\u00df", "ist", "mit", "sma\u00b7ragd", "be\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Kein purpur f\u00fchrt des amaranthen licht,", "tokens": ["Kein", "pur\u00b7pur", "f\u00fchrt", "des", "a\u00b7ma\u00b7ran\u00b7then", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Was k\u00fcnstliches Cathaja stickt und tr\u00e4get,", "tokens": ["Was", "k\u00fcnst\u00b7li\u00b7ches", "Cat\u00b7ha\u00b7ja", "stickt", "und", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Erreicht die kunst der tulipanen nicht.", "tokens": ["Er\u00b7reicht", "die", "kunst", "der", "tu\u00b7li\u00b7pa\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zum zeugnisse, wie er sich m\u00fcsse sehnen,", "tokens": ["Zum", "zeug\u00b7nis\u00b7se", ",", "wie", "er", "sich", "m\u00fcs\u00b7se", "seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn seine braut in diesem schmucke lacht;", "tokens": ["Wenn", "sei\u00b7ne", "braut", "in", "die\u00b7sem", "schmu\u00b7cke", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "So ist gewi\u00df, da\u00df er die liebes-thr\u00e4nen,", "tokens": ["So", "ist", "ge\u00b7wi\u00df", ",", "da\u00df", "er", "die", "lie\u00b7bes\u00b7thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Den feuchten thau zu diamanten macht.", "tokens": ["Den", "feuch\u00b7ten", "thau", "zu", "di\u00b7a\u00b7man\u00b7ten", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O sch\u00f6ne zeit! die lauter balsam haucht,", "tokens": ["O", "sch\u00f6\u00b7ne", "zeit", "!", "die", "lau\u00b7ter", "bal\u00b7sam", "haucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die ambra streut durch die gelinde l\u00fcffte,", "tokens": ["Die", "am\u00b7bra", "streut", "durch", "die", "ge\u00b7lin\u00b7de", "l\u00fcff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "In der jesmin von allen bl\u00e4ttern raucht,", "tokens": ["In", "der", "jes\u00b7min", "von", "al\u00b7len", "bl\u00e4t\u00b7tern", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und bisam dringt durch rauhe felsen kl\u00fcffte,", "tokens": ["Und", "bi\u00b7sam", "dringt", "durch", "rau\u00b7he", "fel\u00b7sen", "kl\u00fcff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wo nectar aus gemeinen steinen schwitzt,", "tokens": ["Wo", "nec\u00b7tar", "aus", "ge\u00b7mei\u00b7nen", "stei\u00b7nen", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und iedes feld tr\u00e4gt tausend panaceen,", "tokens": ["Und", "ie\u00b7des", "feld", "tr\u00e4gt", "tau\u00b7send", "pa\u00b7na\u00b7ce\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wo Chloris selbst vergn\u00fcgt in blumen sitzt;", "tokens": ["Wo", "Chlo\u00b7ris", "selbst", "ver\u00b7gn\u00fcgt", "in", "blu\u00b7men", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gleichwie ihr fu\u00df nur will auf rosen gehen.", "tokens": ["Gleich\u00b7wie", "ihr", "fu\u00df", "nur", "will", "auf", "ro\u00b7sen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "ADV", "VMFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Da\u00df deine gunst nicht will best\u00e4ndig w\u00e4hren,", "tokens": ["Da\u00df", "dei\u00b7ne", "gunst", "nicht", "will", "be\u00b7st\u00e4n\u00b7dig", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Da\u00df nicht das jahr soll zweymal fr\u00fchling seyn,", "tokens": ["Da\u00df", "nicht", "das", "jahr", "soll", "zwey\u00b7mal", "fr\u00fch\u00b7ling", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ja, da\u00df die welt nicht fr\u00fcchte kan geb\u00e4hren,", "tokens": ["Ja", ",", "da\u00df", "die", "welt", "nicht", "fr\u00fcch\u00b7te", "kan", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die blumen statt der schaalen schliessen ein;", "tokens": ["Die", "blu\u00b7men", "statt", "der", "schaa\u00b7len", "schlies\u00b7sen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Nein, nein, es ist di\u00df, was der himmel liebet,", "tokens": ["Nein", ",", "nein", ",", "es", "ist", "di\u00df", ",", "was", "der", "him\u00b7mel", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VAFIN", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Was iedes jahr ihm wie aufs neu antraut,", "tokens": ["Was", "ie\u00b7des", "jahr", "ihm", "wie", "aufs", "neu", "an\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "PPER", "KOKOM", "APPRART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Da er inde\u00df bekante k\u00fc\u00dfe giebet,", "tokens": ["Da", "er", "in\u00b7de\u00df", "be\u00b7kan\u00b7te", "k\u00fc\u00b7\u00dfe", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Nur eine welt, ein fr\u00fchling, eine braut.", "tokens": ["Nur", "ei\u00b7ne", "welt", ",", "ein", "fr\u00fch\u00b7ling", ",", "ei\u00b7ne", "braut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJD", "$,", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Heut aber weist ein ungemeines band,", "tokens": ["Heut", "a\u00b7ber", "weist", "ein", "un\u00b7ge\u00b7mei\u00b7nes", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Das zweiffels ohn im himmel ist gebunden,", "tokens": ["Das", "zweif\u00b7fels", "ohn", "im", "him\u00b7mel", "ist", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Doch auf der welt auch als des gl\u00fcckes pfand,", "tokens": ["Doch", "auf", "der", "welt", "auch", "als", "des", "gl\u00fc\u00b7ckes", "pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Den letzten zug und beyfall hat gefunden.", "tokens": ["Den", "letz\u00b7ten", "zug", "und", "bey\u00b7fall", "hat", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Heut weist ein schlu\u00df, der ewig feste h\u00e4lt,", "tokens": ["Heut", "weist", "ein", "schlu\u00df", ",", "der", "e\u00b7wig", "fes\u00b7te", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Dasjenige, was selbst der lauff der zeiten,", "tokens": ["Das\u00b7je\u00b7ni\u00b7ge", ",", "was", "selbst", "der", "lauff", "der", "zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Nicht in den umgang seines wechsels stellt,", "tokens": ["Nicht", "in", "den", "um\u00b7gang", "sei\u00b7nes", "wech\u00b7sels", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Jm fr\u00fchlinge zwey fr\u00fchling an zwey br\u00e4uten.", "tokens": ["Jm", "fr\u00fch\u00b7lin\u00b7ge", "zwey", "fr\u00fch\u00b7ling", "an", "zwey", "br\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "CARD", "VVFIN", "APPR", "CARD", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.41": {"text": "Die jugend ist der fr\u00fchling unser jahre;", "tokens": ["Die", "ju\u00b7gend", "ist", "der", "fr\u00fch\u00b7ling", "un\u00b7ser", "jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "So wie der lentz des jahres jugend hei\u00dft;", "tokens": ["So", "wie", "der", "lentz", "des", "jah\u00b7res", "ju\u00b7gend", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Der jugend feld tr\u00e4gt angenehme waare,", "tokens": ["Der", "ju\u00b7gend", "feld", "tr\u00e4gt", "an\u00b7ge\u00b7neh\u00b7me", "waa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Denn die man vor des fr\u00fchlings kinder preist,", "tokens": ["Denn", "die", "man", "vor", "des", "fr\u00fch\u00b7lings", "kin\u00b7der", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Das blum-werck, so die frische jugend treibet,", "tokens": ["Das", "blum\u00b7\u00b7werck", ",", "so", "die", "fri\u00b7sche", "ju\u00b7gend", "trei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Wird sch\u00f6ner, als des fr\u00fchlings glantz und lust:", "tokens": ["Wird", "sch\u00f6\u00b7ner", ",", "als", "des", "fr\u00fch\u00b7lings", "glantz", "und", "lust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VA