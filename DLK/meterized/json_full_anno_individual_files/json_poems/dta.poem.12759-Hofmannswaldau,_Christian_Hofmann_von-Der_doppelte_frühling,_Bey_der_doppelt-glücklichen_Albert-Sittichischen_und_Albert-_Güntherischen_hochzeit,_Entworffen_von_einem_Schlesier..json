bic.penta"}, "line.89": {"text": "Der fr\u00fchling selbst, indem ihr hochzeit gebet,", "tokens": ["Der", "fr\u00fch\u00b7ling", "selbst", ",", "in\u00b7dem", "ihr", "hoch\u00b7zeit", "ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.90": {"text": "Verspricht euch nichts, als das vergn\u00fcgung macht:", "tokens": ["Ver\u00b7spricht", "euch", "nichts", ",", "als", "das", "ver\u00b7gn\u00fc\u00b7gung", "macht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.91": {"text": "Wie ihr itzund in lauter freuden lebet,", "tokens": ["Wie", "ihr", "it\u00b7zund", "in", "lau\u00b7ter", "freu\u00b7den", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.92": {"text": "So soll euch seyn eur leben zugebracht.", "tokens": ["So", "soll", "euch", "seyn", "eur", "le\u00b7ben", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.93": {"text": "Euch wird der zeit ihr wechsel nicht beschweren,", "tokens": ["Euch", "wird", "der", "zeit", "ihr", "wech\u00b7sel", "nicht", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.94": {"text": "Nur dieses mu\u00df zu mehrer lust geschehn,", "tokens": ["Nur", "die\u00b7ses", "mu\u00df", "zu", "meh\u00b7rer", "lust", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.95": {"text": "Eh noch der lentz wird einmal wiederkehren,", "tokens": ["Eh", "noch", "der", "lentz", "wird", "ein\u00b7mal", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.96": {"text": "Wird euch der herbst schon zweyfach fruchtbar sehn.", "tokens": ["Wird", "euch", "der", "herbst", "schon", "zwey\u00b7fach", "frucht\u00b7bar", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}