unct"], "pos": ["ADV", "VVPP", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.79": {"text": "So giebt es nun als vater auch zwey br\u00e4ute", "tokens": ["So", "giebt", "es", "nun", "als", "va\u00b7ter", "auch", "zwey", "br\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "NN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.80": {"text": "Denjenigen, wo es vor lehrer war.", "tokens": ["Den\u00b7je\u00b7ni\u00b7gen", ",", "wo", "es", "vor", "leh\u00b7rer", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.81": {"text": "Geht, Sch\u00f6nes paar! zum andern paare hin;", "tokens": ["Geht", ",", "Sch\u00f6\u00b7nes", "paar", "!", "zum", "an\u00b7dern", "paa\u00b7re", "hin", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "APPRART", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.82": {"text": "Und paaret euch zusammen in die wette;", "tokens": ["Und", "paa\u00b7ret", "euch", "zu\u00b7sam\u00b7men", "in", "die", "wet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.83": {"text": "Der sternen heer, das kurtz noch munter schien,", "tokens": ["Der", "ster\u00b7nen", "heer", ",", "das", "kurtz", "noch", "mun\u00b7ter", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADV", "ADJD", "VVFIN", "$,"], "met