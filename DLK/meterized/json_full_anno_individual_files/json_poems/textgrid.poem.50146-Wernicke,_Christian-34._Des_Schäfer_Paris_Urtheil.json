{"textgrid.poem.50146": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "34. Des Sch\u00e4fer Paris Urtheil", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als sich vor einem ", "tokens": ["Als", "sich", "vor", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Entbl\u00f6ssten einst ", "tokens": ["Ent\u00b7bl\u00f6ss\u00b7ten", "einst"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und er auff ihr verwirrt Geschrey", "tokens": ["Und", "er", "auff", "ihr", "ver\u00b7wirrt", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPER", "ADJD", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Solt' urtheiln, ", "tokens": ["Solt'", "ur\u00b7theiln", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Auch dass man in ihm Muht erweckte,", "tokens": ["Auch", "dass", "man", "in", "ihm", "Muht", "er\u00b7weck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "APPR", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ein' ihm ein ", "tokens": ["Die", "Ein'", "ihm", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Da stand er als ein Dudentopff,", "tokens": ["Da", "stand", "er", "als", "ein", "Du\u00b7den\u00b7topff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kratzte l\u00e4chelnd seinen Kopff;", "tokens": ["Und", "kratz\u00b7te", "l\u00e4\u00b7chelnd", "sei\u00b7nen", "Kopff", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zwar sah' er als sie vor ihm stunden,", "tokens": ["Zwar", "sah'", "er", "als", "sie", "vor", "ihm", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "PPER", "APPR", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und nichtes hatten umgebunden,", "tokens": ["Und", "nich\u00b7tes", "hat\u00b7ten", "um\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Doch d\u00f6rffte, dieser zu bedienen,", "tokens": ["Doch", "d\u00f6rff\u00b7te", ",", "die\u00b7ser", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der T\u00f6lpel sich nicht gleich erk\u00fchnen.", "tokens": ["Der", "T\u00f6l\u00b7pel", "sich", "nicht", "gleich", "er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Zuletzt grieff' er, wie er sie fand,", "tokens": ["Zu\u00b7letzt", "grief\u00b7f'", "er", ",", "wie", "er", "sie", "fand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Die Eine bey ", "tokens": ["Die", "Ei\u00b7ne", "bey"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und sagte zu den zweyen andern:", "tokens": ["Und", "sag\u00b7te", "zu", "den", "zwe\u00b7yen", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ihr k\u00f6nnt nur frey von hinnen wandern,", "tokens": ["Ihr", "k\u00f6nnt", "nur", "frey", "von", "hin\u00b7nen", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Denn ich bin, ", "tokens": ["Denn", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Unm\u00f6glich nicht ", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Und wo ihr von ", "tokens": ["Und", "wo", "ihr", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Viel minder ", "tokens": ["Viel", "min\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Als sich vor ei