d", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In namenlosen Wehen;", "tokens": ["In", "na\u00b7men\u00b7lo\u00b7sen", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Licht, o Herr, ist ihr entr\u00fcckt,", "tokens": ["Dein", "Licht", ",", "o", "Herr", ",", "ist", "ihr", "ent\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Licht scheint auszugehen.", "tokens": ["Ihr", "Licht", "scheint", "aus\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wollst wieder sie erl\u00f6sen", "tokens": ["Wollst", "wie\u00b7der", "sie", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von der Gewalt des B\u00f6sen!", "tokens": ["Von", "der", "Ge\u00b7walt", "des", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dich rufen Leid und Klageton,", "tokens": ["Dich", "ru\u00b7fen", "Leid", "und", "Kla\u00b7ge\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir winkt ein Meer von Thr\u00e4nen,", "tokens": ["Dir", "winkt", "ein", "Meer", "von", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und leise Seufzer, kaum entflohn", "tokens": ["Und", "lei\u00b7se", "Seuf\u00b7zer", ",", "kaum", "ent\u00b7flohn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bescheidnem bangem Sehnen,", "tokens": ["Be\u00b7scheid\u00b7nem", "ban\u00b7gem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum Retten, zum Befreien,", "tokens": ["Zum", "Ret\u00b7ten", ",", "zum", "Be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Alte zu erneuen.", "tokens": ["Das", "Al\u00b7te", "zu", "er\u00b7neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Menschensohn voll Lieb' und Macht,", "tokens": ["O", "Men\u00b7schen\u00b7sohn", "voll", "Lieb'", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O h\u00f6chstes ew'ges Leben,", "tokens": ["O", "h\u00f6chs\u00b7tes", "ew'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast oft s