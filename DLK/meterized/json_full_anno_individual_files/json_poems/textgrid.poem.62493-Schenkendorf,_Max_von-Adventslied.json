{"textgrid.poem.62493": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Adventslied", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm nieder aus der Jungfrau Schoo\u00df,", "tokens": ["Komm", "nie\u00b7der", "aus", "der", "Jung\u00b7frau", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Kind, aus Himmelsauen!", "tokens": ["O", "Kind", ",", "aus", "Him\u00b7mel\u00b7sau\u00b7en", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sehnt sich alles, klein und gro\u00df,", "tokens": ["Es", "sehnt", "sich", "al\u00b7les", ",", "klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Antlitz dir zu schauen;", "tokens": ["Ins", "Ant\u00b7litz", "dir", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es schmachtet deinem Segen", "tokens": ["Es", "schmach\u00b7tet", "dei\u00b7nem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Erde, Herr, entgegen.", "tokens": ["Die", "Er\u00b7de", ",", "Herr", ",", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punc