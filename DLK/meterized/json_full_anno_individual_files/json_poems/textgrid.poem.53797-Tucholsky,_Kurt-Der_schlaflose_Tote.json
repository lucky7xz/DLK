{"textgrid.poem.53797": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der schlaflose Tote", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da, wo das Grab war, schlief er nicht,", "tokens": ["Da", ",", "wo", "das", "Grab", "war", ",", "schlief", "er", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er konnte da nicht schlafen;", "tokens": ["er", "konn\u00b7te", "da", "nicht", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "denn rechts war eine Zuckerfabrik", "tokens": ["denn", "rechts", "war", "ei\u00b7ne", "Zu\u00b7cker\u00b7fab\u00b7rik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und links ein Treidelhafen.", "tokens": ["und", "links", "ein", "Trei\u00b7del\u00b7ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So spukte er denn \u00fcberall umher.", "tokens": ["So", "spuk\u00b7te", "er", "denn", "\u00fc\u00b7be\u00b7rall", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er spukte an der Kasernenwand,", "tokens": ["Er", "spuk\u00b7te", "an", "der", "Ka\u00b7ser\u00b7nen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "da stand ein Fenster offen \u2013", "tokens": ["da", "stand", "ein", "Fens\u00b7ter", "of\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "doch weil es nach Leder und Leutnants roch,", "tokens": ["doch", "weil", "es", "nach", "Le\u00b7der", "und", "Leut\u00b7nants", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so ist er davongeloffen.", "tokens": ["so", "ist", "er", "da\u00b7von\u00b7ge\u00b7lof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und das von Rechts wegen.", "tokens": ["Und", "das", "von", "Rechts", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "APPR", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Er spukte im Kirchenchor. Da begann", "tokens": ["Er", "spuk\u00b7te", "im", "Kir\u00b7chen\u00b7chor", ".", "Da", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "der Pfaff auf den Knien zu krauchen:", "tokens": ["der", "Pfaff", "auf", "den", "Kni\u00b7en", "zu", "krau\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbbist du vielleicht Christus, du wei\u00dfer Mann?", "tokens": ["\u00bb", "bist", "du", "viel\u00b7leicht", "Chris\u00b7tus", ",", "du", "wei\u00b7\u00dfer", "Mann", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Dann k\u00f6nnen wir dich nicht gebrauchen.", "tokens": ["Dann", "k\u00f6n\u00b7nen", "wir", "dich", "nicht", "ge\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was haben wir aus dir gemacht \u2013!\u00ab", "tokens": ["Was", "ha\u00b7ben", "wir", "aus", "dir", "ge\u00b7macht", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er spukte auf einer Redaktion.", "tokens": ["Er", "spuk\u00b7te", "auf", "ei\u00b7ner", "Re\u00b7dak\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da erhob sich der Hauptschriftleiter", "tokens": ["Da", "er\u00b7hob", "sich", "der", "Haupt\u00b7schrift\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "und sagte: \u00bbIch verstehe Sie schon \u2013", "tokens": ["und", "sag\u00b7te", ":", "\u00bb", "Ich", "ver\u00b7ste\u00b7he", "Sie", "schon", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "aber gehen Sie ein Haus weiter!", "tokens": ["a\u00b7ber", "ge\u00b7hen", "Sie", "ein", "Haus", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Unser erstklassiges Publikum besteht leider", "tokens": ["Un\u00b7ser", "erst\u00b7klas\u00b7si\u00b7ges", "Pub\u00b7li\u00b7kum", "be\u00b7steht", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "aus lauter Idioten!\u00ab", "tokens": ["aus", "lau\u00b7ter", "I\u00b7dio\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Er lie\u00df die Gew\u00e4nder im Reichstag wehn,", "tokens": ["Er", "lie\u00df", "die", "Ge\u00b7w\u00e4n\u00b7der", "im", "Reichs\u00b7tag", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "da hatte ers gut getroffen:", "tokens": ["da", "hat\u00b7te", "ers", "gut", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einen Geist, einen Geist, den kannten sie nicht,", "tokens": ["Ei\u00b7nen", "Geist", ",", "ei\u00b7nen", "Geist", ",", "den", "kann\u00b7ten", "sie", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "das Maul stand ihnen offen.", "tokens": ["das", "Maul", "stand", "ih\u00b7nen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Haben Sie \u00fcbrigens die feinpointierte Rede", "tokens": ["Ha\u00b7ben", "Sie", "\u00fcb\u00b7ri\u00b7gens", "die", "fein\u00b7po\u00b7in\u00b7tier\u00b7te", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "des Abgeordneten Breitscheid gelesen \u2013?", "tokens": ["des", "Ab\u00b7ge\u00b7ord\u00b7ne\u00b7ten", "Breit\u00b7scheid", "ge\u00b7le\u00b7sen", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Er spukte im Landgericht III zu Berlin.", "tokens": ["Er", "spuk\u00b7te", "im", "Land\u00b7ge\u00b7richt", "I\u00b7iI", "zu", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "CARD", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was bot sich da seinem Blicke:", "tokens": ["Was", "bot", "sich", "da", "sei\u00b7nem", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die deutsche Themis \u2013 welch ein Schreck:", "tokens": ["Die", "deut\u00b7sche", "The\u00b7mis", "\u2013", "welch", "ein", "Schreck", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine alte gespenstische Zicke.", "tokens": ["Ei\u00b7ne", "al\u00b7te", "ge\u00b7spens\u00b7ti\u00b7sche", "Zi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In den Zellen winselten Zehntausende.", "tokens": ["In", "den", "Zel\u00b7len", "win\u00b7sel\u00b7ten", "Zehn\u00b7tau\u00b7sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+-+--++--", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Er spukte in der Wilhelmstra\u00dfe:", "tokens": ["Er", "spuk\u00b7te", "in", "der", "Wil\u00b7helms\u00b7tra\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rayonchefs und B\u00fcrokraten,", "tokens": ["Ra\u00b7yon\u00b7chefs", "und", "B\u00fc\u00b7ro\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sowie die Kommis der Diplomatie", "tokens": ["so\u00b7wie", "die", "Kom\u00b7mis", "der", "Dip\u00b7lo\u00b7ma\u00b7tie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und ehemalige Soldaten.", "tokens": ["und", "e\u00b7hem\u00b7a\u00b7li\u00b7ge", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Daf\u00fcr bluten nachher sechzig Millionen.", "tokens": ["Da\u00b7f\u00fcr", "blu\u00b7ten", "nach\u00b7her", "sech\u00b7zig", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Dies alles sowie Addalin", "tokens": ["Dies", "al\u00b7les", "so\u00b7wie", "Ad\u00b7da\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wirkten nichts gegen sein Getue.", "tokens": ["wirk\u00b7ten", "nichts", "ge\u00b7gen", "sein", "Ge\u00b7tue", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein schlafloser Toter weht durch die Welt \u2013", "tokens": ["Ein", "schlaf\u00b7lo\u00b7ser", "To\u00b7ter", "weht", "durch", "die", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott gebe ihm ewige Ruhe!", "tokens": ["Gott", "ge\u00b7be", "ihm", "e\u00b7wi\u00b7ge", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Da, wo das Grab war, schlief er nicht,", "tokens": ["Da", ",", "wo", "das", "Grab", "war", ",", "schlief", "er", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er konnte da nicht schlafen;", "tokens": ["er", "konn\u00b7te", "da", "nicht", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "denn rechts war eine Zuckerfabrik", "tokens": ["denn", "rechts", "war", "ei\u00b7ne", "Zu\u00b7cker\u00b7fab\u00b7rik"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "und links ein Treidelhafen.", "tokens": ["und", "links", "ein", "Trei\u00b7del\u00b7ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So spukte er denn \u00fcberall umher.", "tokens": ["So", "spuk\u00b7te", "er", "denn", "\u00fc\u00b7be\u00b7rall", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Er spukte an der Kasernenwand,", "tokens": ["Er", "spuk\u00b7te", "an", "der", "Ka\u00b7ser\u00b7nen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "da stand ein Fenster offen \u2013", "tokens": ["da", "stand", "ein", "Fens\u00b7ter", "of\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "doch weil es nach Leder und Leutnants roch,", "tokens": ["doch", "weil", "es", "nach", "Le\u00b7der", "und", "Leut\u00b7nants", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "so ist er davongeloffen.", "tokens": ["so", "ist", "er", "da\u00b7von\u00b7ge\u00b7lof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und das von Rechts wegen.", "tokens": ["Und", "das", "von", "Rechts", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "APPR", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Er spukte im Kirchenchor. Da begann", "tokens": ["Er", "spuk\u00b7te", "im", "Kir\u00b7chen\u00b7chor", ".", "Da", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "der Pfaff auf den Knien zu krauchen:", "tokens": ["der", "Pfaff", "auf", "den", "Kni\u00b7en", "zu", "krau\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbbist du vielleicht Christus, du wei\u00dfer Mann?", "tokens": ["\u00bb", "bist", "du", "viel\u00b7leicht", "Chris\u00b7tus", ",", "du", "wei\u00b7\u00dfer", "Mann", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Dann k\u00f6nnen wir dich nicht gebrauchen.", "tokens": ["Dann", "k\u00f6n\u00b7nen", "wir", "dich", "nicht", "ge\u00b7brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was haben wir aus dir gemacht \u2013!\u00ab", "tokens": ["Was", "ha\u00b7ben", "wir", "aus", "dir", "ge\u00b7macht", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Er spukte auf einer Redaktion.", "tokens": ["Er", "spuk\u00b7te", "auf", "ei\u00b7ner", "Re\u00b7dak\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da erhob sich der Hauptschriftleiter", "tokens": ["Da", "er\u00b7hob", "sich", "der", "Haupt\u00b7schrift\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "und sagte: \u00bbIch verstehe Sie schon \u2013", "tokens": ["und", "sag\u00b7te", ":", "\u00bb", "Ich", "ver\u00b7ste\u00b7he", "Sie", "schon", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "aber gehen Sie ein Haus weiter!", "tokens": ["a\u00b7ber", "ge\u00b7hen", "Sie", "ein", "Haus", "wei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Unser erstklassiges Publikum besteht leider", "tokens": ["Un\u00b7ser", "erst\u00b7klas\u00b7si\u00b7ges", "Pub\u00b7li\u00b7kum", "be\u00b7steht", "lei\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "aus lauter Idioten!\u00ab", "tokens": ["aus", "lau\u00b7ter", "I\u00b7dio\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Er lie\u00df die Gew\u00e4nder im Reichstag wehn,", "tokens": ["Er", "lie\u00df", "die", "Ge\u00b7w\u00e4n\u00b7der", "im", "Reichs\u00b7tag", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "da hatte ers gut getroffen:", "tokens": ["da", "hat\u00b7te", "ers", "gut", "ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einen Geist, einen Geist, den kannten sie nicht,", "tokens": ["Ei\u00b7nen", "Geist", ",", "ei\u00b7nen", "Geist", ",", "den", "kann\u00b7ten", "sie", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "das Maul stand ihnen offen.", "tokens": ["das", "Maul", "stand", "ih\u00b7nen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Haben Sie \u00fcbrigens die feinpointierte Rede", "tokens": ["Ha\u00b7ben", "Sie", "\u00fcb\u00b7ri\u00b7gens", "die", "fein\u00b7po\u00b7in\u00b7tier\u00b7te", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "des Abgeordneten Breitscheid gelesen \u2013?", "tokens": ["des", "Ab\u00b7ge\u00b7ord\u00b7ne\u00b7ten", "Breit\u00b7scheid", "ge\u00b7le\u00b7sen", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "$."], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Er spukte im Landgericht III zu Berlin.", "tokens": ["Er", "spuk\u00b7te", "im", "Land\u00b7ge\u00b7richt", "I\u00b7iI", "zu", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "CARD", "APPR", "NE", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was bot sich da seinem Blicke:", "tokens": ["Was", "bot", "sich", "da", "sei\u00b7nem", "Bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die deutsche Themis \u2013 welch ein Schreck:", "tokens": ["Die", "deut\u00b7sche", "The\u00b7mis", "\u2013", "welch", "ein", "Schreck", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eine alte gespenstische Zicke.", "tokens": ["Ei\u00b7ne", "al\u00b7te", "ge\u00b7spens\u00b7ti\u00b7sche", "Zi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "In den Zellen winselten Zehntausende.", "tokens": ["In", "den", "Zel\u00b7len", "win\u00b7sel\u00b7ten", "Zehn\u00b7tau\u00b7sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+-+--++--", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Er spukte in der Wilhelmstra\u00dfe:", "tokens": ["Er", "spuk\u00b7te", "in", "der", "Wil\u00b7helms\u00b7tra\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rayonchefs und B\u00fcrokraten,", "tokens": ["Ra\u00b7yon\u00b7chefs", "und", "B\u00fc\u00b7ro\u00b7kra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sowie die Kommis der Diplomatie", "tokens": ["so\u00b7wie", "die", "Kom\u00b7mis", "der", "Dip\u00b7lo\u00b7ma\u00b7tie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und ehemalige Soldaten.", "tokens": ["und", "e\u00b7hem\u00b7a\u00b7li\u00b7ge", "Sol\u00b7da\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Daf\u00fcr bluten nachher sechzig Millionen.", "tokens": ["Da\u00b7f\u00fcr", "blu\u00b7ten", "nach\u00b7her", "sech\u00b7zig", "Mil\u00b7lion\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "CARD", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Dies alles sowie Addalin", "tokens": ["Dies", "al\u00b7les", "so\u00b7wie", "Ad\u00b7da\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wirkten nichts gegen sein Getue.", "tokens": ["wirk\u00b7ten", "nichts", "ge\u00b7gen", "sein", "Ge\u00b7tue", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ein schlafloser Toter weht durch die Welt \u2013", "tokens": ["Ein", "schlaf\u00b7lo\u00b7ser", "To\u00b7ter", "weht", "durch", "die", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott gebe ihm ewige Ruhe!", "tokens": ["Gott", "ge\u00b7be", "ihm", "e\u00b7wi\u00b7ge", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Amen.", "tokens": ["A\u00b7men", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}