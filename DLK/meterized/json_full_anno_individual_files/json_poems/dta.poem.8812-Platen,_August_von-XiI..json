{"dta.poem.8812": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Keiner m\u00f6ge d'rum verschweigen was im Busen voll-", "tokens": ["Kei\u00b7ner", "m\u00f6\u00b7ge", "d'\u00b7rum", "ver\u00b7schwei\u00b7gen", "was", "im", "Bu\u00b7sen", "voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "VVPP", "PRELS", "APPRART", "NN", "TRUNC"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Meine Habe sind Gedanken, Worte sind es, T\u00f6ne sind's,", "tokens": ["Mei\u00b7ne", "Ha\u00b7be", "sind", "Ge\u00b7dan\u00b7ken", ",", "Wor\u00b7te", "sind", "es", ",", "T\u00f6\u00b7ne", "sin\u00b7d's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "NN", "VAFIN", "PPER", "$,", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn sie dir gefallen, horche! Wenn sie dich erm\u00fcden,", "tokens": ["Wenn", "sie", "dir", "ge\u00b7fal\u00b7len", ",", "hor\u00b7che", "!", "Wenn", "sie", "dich", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "VVFIN", "$.", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Einen wei\u00df ich, m\u00f6gt ihr Alle mich verdammen, wei\u00df", "tokens": ["Ei\u00b7nen", "wei\u00df", "ich", ",", "m\u00f6gt", "ihr", "Al\u00b7le", "mich", "ver\u00b7dam\u00b7men", ",", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PIAT", "PRF", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Wen ich tausendmal verlezte, wer mir tausendmal ver-", "tokens": ["Wen", "ich", "tau\u00b7send\u00b7mal", "ver\u00b7lez\u00b7te", ",", "wer", "mir", "tau\u00b7send\u00b7mal", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Sieh mich hier im Staub und setze deine Ferse mir auf's", "tokens": ["Sieh", "mich", "hier", "im", "Staub", "und", "set\u00b7ze", "dei\u00b7ne", "Fer\u00b7se", "mir", "auf's"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "PPER", "APPRART"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.7": {"text": "Mich, den lezten von den lezten deiner lezten Sclaven", "tokens": ["Mich", ",", "den", "lez\u00b7ten", "von", "den", "lez\u00b7ten", "dei\u00b7ner", "lez\u00b7ten", "Scla\u00b7ven"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "APPR", "ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Denn was soll der Stolz? Wie ", "tokens": ["Denn", "was", "soll", "der", "Stolz", "?", "Wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWS", "VMFIN", "ART", "NN", "$.", "PWAV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Doch es kommt der Tag, an dem es wiederfordert, der", "tokens": ["Doch", "es", "kommt", "der", "Tag", ",", "an", "dem", "es", "wie\u00b7der\u00b7for\u00b7dert", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PRELS"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}}}}