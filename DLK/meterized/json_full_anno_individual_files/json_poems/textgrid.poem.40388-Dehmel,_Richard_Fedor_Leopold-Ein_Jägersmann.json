{"textgrid.poem.40388": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein J\u00e4gersmann", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein J\u00e4gersmann werden,", "tokens": ["Ich", "kann", "ein", "J\u00e4\u00b7gers\u00b7mann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ich hab eine sichre Hand;", "tokens": ["ich", "hab", "ei\u00b7ne", "sich\u00b7re", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ich werde von der Schie\u00dfbudenfrau", "tokens": ["ich", "wer\u00b7de", "von", "der", "Schie\u00df\u00b7bu\u00b7den\u00b7frau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "immer \u00bbklein Tell\u00ab genannt.", "tokens": ["im\u00b7mer", "\u00bb", "klein", "Tell", "\u00ab", "ge\u00b7nannt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "NN", "$(", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ich hab auch kaltes Blut.", "tokens": ["Ich", "hab", "auch", "kal\u00b7tes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich zucke nicht mit der Wimper,", "tokens": ["Ich", "zu\u00b7cke", "nicht", "mit", "der", "Wim\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "dr\u00fcck ich die Knallb\u00fcchse ab.", "tokens": ["dr\u00fcck", "ich", "die", "Knall\u00b7b\u00fcch\u00b7se", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Mir soll kein Wilddieb ins Handwerk pfuschen;", "tokens": ["Mir", "soll", "kein", "Wild\u00b7dieb", "ins", "Hand\u00b7werk", "pfu\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ich bringe ihn auf den Trab,", "tokens": ["ich", "brin\u00b7ge", "ihn", "auf", "den", "Trab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und w\u00e4r er schlau wie ein Teckel.", "tokens": ["und", "w\u00e4r", "er", "schlau", "wie", "ein", "Te\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich w\u00fcrde wohl selber wildern,", "tokens": ["Ich", "w\u00fcr\u00b7de", "wohl", "sel\u00b7ber", "wil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "h\u00e4tt ich kein eigen Revier.", "tokens": ["h\u00e4tt", "ich", "kein", "ei\u00b7gen", "Re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "So aber, Kerl: Mann gegen Mann,", "tokens": ["So", "a\u00b7ber", ",", "Kerl", ":", "Mann", "ge\u00b7gen", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "ich sch\u00fctze den Forst vor dir,", "tokens": ["ich", "sch\u00fct\u00b7ze", "den", "Forst", "vor", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "das ist meine Pflicht, Halunke!", "tokens": ["das", "ist", "mei\u00b7ne", "Pflicht", ",", "Ha\u00b7lun\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gewehr her! oder \u2013 gib Feuer!", "tokens": ["Ge\u00b7wehr", "her", "!", "o\u00b7der", "\u2013", "gib", "Feu\u00b7er", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "KON", "$(", "VVIMP", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auge in Auge! La\u00df sehn:", "tokens": ["Au\u00b7ge", "in", "Au\u00b7ge", "!", "La\u00df", "sehn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "VVIMP", "VVINF", "$."], "meter": "+--+---", "measure": "dactylic.di.plus"}, "line.3": {"text": "piff paff, wen's trifft, dem wird noch", "tokens": ["piff", "paff", ",", "wen's", "trifft", ",", "dem", "wird", "noch"], "token_info": ["word", "word", "punct", "word