{"textgrid.poem.49255": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "3. An die Hirschbergischen B\u00e4der", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Nymfen, die ihr hier umb dieses gr\u00fcne Thal,", "tokens": ["Ihr", "Nym\u00b7fen", ",", "die", "ihr", "hier", "umb", "die\u00b7ses", "gr\u00fc\u00b7ne", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit dem Schneegebirg' unnd Alpen steht umbgeben,", "tokens": ["Das", "mit", "dem", "Schnee\u00b7ge\u00b7bir\u00b7g'", "unnd", "Al\u00b7pen", "steht", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In stiller Einsamkeit verbringet euer Leben,", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "ver\u00b7brin\u00b7get", "eu\u00b7er", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr weissen Najades, und du, du sch\u00f6nes Qual,", "tokens": ["Ihr", "weis\u00b7sen", "Na\u00b7ja\u00b7des", ",", "und", "du", ",", "du", "sch\u00f6\u00b7nes", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch dessen werthe Krafft die Menschen sonder Zahl", "tokens": ["Durch", "des\u00b7sen", "wert\u00b7he", "Krafft", "die", "Men\u00b7schen", "son\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00dfher genesen seynd, wollt ihr nach Ehre streben", "tokens": ["Bi\u00df\u00b7her", "ge\u00b7ne\u00b7sen", "seynd", ",", "wollt", "ihr", "nach", "Eh\u00b7re", "stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die nimmer sterblich ist, und euch noch h\u00f6her heben,", "tokens": ["Die", "nim\u00b7mer", "sterb\u00b7lich", "ist", ",", "und", "euch", "noch", "h\u00f6\u00b7her", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "KON", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nemt di\u00df edle Pfand, das nun auch dieses mal", "tokens": ["So", "nemt", "di\u00df", "ed\u00b7le", "Pfand", ",", "das", "nun", "auch", "die\u00b7ses", "mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sich zu euch hier gemacht gesund und frisch zu werden.", "tokens": ["Sich", "zu", "euch", "hier", "ge\u00b7macht", "ge\u00b7sund", "und", "frisch", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADV", "VVPP", "ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewahret und versorgt die Hoffnung unsrer Erden,", "tokens": ["Be\u00b7wah\u00b7ret", "und", "ver\u00b7sorgt", "die", "Hoff\u00b7nung", "uns\u00b7rer", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df Landes besten Trost, so bringet ihr den Danck", "tokens": ["Di\u00df", "Lan\u00b7des", "bes\u00b7ten", "Trost", ",", "so", "brin\u00b7get", "ihr", "den", "Danck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Treu und Wachsamkeit, da\u00df ihr durch euer Pflegen", "tokens": ["Der", "Treu", "und", "Wach\u00b7sam\u00b7keit", ",", "da\u00df", "ihr", "durch", "eu\u00b7er", "Pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Habt den erquickt, an dem uns s\u00e4mptlich ist gelegen.", "tokens": ["Habt", "den", "er\u00b7quickt", ",", "an", "dem", "uns", "s\u00e4mpt\u00b7lich", "ist", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "VVPP", "$,", "APPR", "PRELS", "PPER", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist er nicht mehr wolauff, so sind wir alle kranck.", "tokens": ["Ist", "er", "nicht", "mehr", "wo\u00b7lauff", ",", "so", "sind", "wir", "al\u00b7le", "kranck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr Nymfen, die ihr hier umb dieses gr\u00fcne Thal,", "tokens": ["Ihr", "Nym\u00b7fen", ",", "die", "ihr", "hier", "umb", "die\u00b7ses", "gr\u00fc\u00b7ne", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mit dem Schneegebirg' unnd Alpen steht umbgeben,", "tokens": ["Das", "mit", "dem", "Schnee\u00b7ge\u00b7bir\u00b7g'", "unnd", "Al\u00b7pen", "steht", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In stiller Einsamkeit verbringet euer Leben,", "tokens": ["In", "stil\u00b7ler", "Ein\u00b7sam\u00b7keit", "ver\u00b7brin\u00b7get", "eu\u00b7er", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr weissen Najades, und du, du sch\u00f6nes Qual,", "tokens": ["Ihr", "weis\u00b7sen", "Na\u00b7ja\u00b7des", ",", "und", "du", ",", "du", "sch\u00f6\u00b7nes", "Qual", ","], "to