{"textgrid.poem.35675": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "38. Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freies Sinns Aufhellung gesp\u00e4ht und Wahrheit,", "tokens": ["Frei\u00b7es", "Sinns", "Auf\u00b7hel\u00b7lung", "ge\u00b7sp\u00e4ht", "und", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVPP", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Sonder Scheu, ob Papst und Tyrann durch Machtspruch", "tokens": ["Son\u00b7der", "Scheu", ",", "ob", "Papst", "und", "Ty\u00b7rann", "durch", "Machts\u00b7pruch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Geistesflug einzw\u00e4ng'; und ge\u00fcbt mit reiner", "tokens": ["Geis\u00b7tes\u00b7flug", "ein\u00b7zw\u00e4ng'", ";", "und", "ge\u00b7\u00fcbt", "mit", "rei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "KON", "VVPP", "APPR", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Seele, was recht ist!", "tokens": ["See\u00b7le", ",", "was", "recht", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Das allein schafft heiteren Blick zur Gottheit:", "tokens": ["Das", "al\u00b7lein", "schafft", "hei\u00b7te\u00b7ren", "Blick", "zur", "Got\u00b7theit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Das allein Gleichmut, wenn im Strom des Lebens", "tokens": ["Das", "al\u00b7lein", "Gleich\u00b7mut", ",", "wenn", "im", "Strom", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sanft der Kahn fortwallt, wenn geb\u00e4umt von Sturmwind", "tokens": ["Sanft", "der", "Kahn", "fort\u00b7wallt", ",", "wenn", "ge\u00b7b\u00e4umt", "von", "Sturm\u00b7wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "KOUS", "VVPP", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Toset die Brandung;", "tokens": ["To\u00b7set", "die", "Bran\u00b7dung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Das allein auch gl\u00e4ttet am tr\u00fcben Ausflu\u00df", "tokens": ["Das", "al\u00b7lein", "auch", "gl\u00e4t\u00b7tet", "am", "tr\u00fc\u00b7ben", "Aus\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Durch den Meerschwall Bahn zu dem stillen Eiland,", "tokens": ["Durch", "den", "Meer\u00b7schwall", "Bahn", "zu", "dem", "stil\u00b7len", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wo uns Freund', Urv\u00e4ter und Weis' aus allem", "tokens": ["Wo", "uns", "Freund'", ",", "Ur\u00b7v\u00e4\u00b7ter", "und", "Weis'", "aus", "al\u00b7lem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "KON", "NN", "APPR", "PIS"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Volke begr\u00fc\u00dfen.", "tokens": ["Vol\u00b7ke", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Keine Ruh', Einschl\u00e4ferung nur mit Angsttraum,", "tokens": ["Kei\u00b7ne", "Ruh'", ",", "Ein\u00b7schl\u00e4\u00b7fe\u00b7rung", "nur", "mit", "Angst\u00b7traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Schafft dir M\u00f6nchsabla\u00df um Verdienst des andern,", "tokens": ["Schafft", "dir", "M\u00f6nch\u00b7sab\u00b7la\u00df", "um", "Ver\u00b7dienst", "des", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Augendrehn, R\u00e4uchwerk und Kastein, und Bannspruch", "tokens": ["Au\u00b7gen\u00b7drehn", ",", "R\u00e4uch\u00b7werk", "und", "Kas\u00b7tein", ",", "und", "Banns\u00b7pruch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "KON", "NN"], "meter": "+--++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Pl\u00e4rrendes Anflehns.", "tokens": ["Pl\u00e4r\u00b7ren\u00b7des", "An\u00b7flehns", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Du zum Licht zwangloser Vernunft von Luther", "tokens": ["Du", "zum", "Licht", "zwang\u00b7lo\u00b7ser", "Ver\u00b7nunft", "von", "Lu\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Miterk\u00e4mpft, du Forscher der Offenbarung,", "tokens": ["Mi\u00b7ter\u00b7k\u00e4mpft", ",", "du", "For\u00b7scher", "der", "Of\u00b7fen\u00b7ba\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Du im Anhauch griechischer Luft gehobner", "tokens": ["Du", "im", "An\u00b7hauch", "grie\u00b7chi\u00b7scher", "Luft", "ge\u00b7hob\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "ADJA", "NN", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Adler der Freiheit!", "tokens": ["Ad\u00b7ler", "der", "Frei\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Du verkennst Erbtugend und Schwung zum \u00c4ther?", "tokens": ["Du", "ver\u00b7kennst", "Erb\u00b7tu\u00b7gend", "und", "Schwung", "zum", "\u00c4\u00b7ther", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KON", "NN", "APPRART", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und, o Schmach! dem\u00fctigest dich in grauser", "tokens": ["Und", ",", "o", "Schmach", "!", "de\u00b7m\u00fc\u00b7ti\u00b7gest", "dich", "in", "grau\u00b7ser"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "$.", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hildebrand' unmenschlichen Fron, dich dumpfem", "tokens": ["Hil\u00b7de\u00b7brand'", "un\u00b7menschli\u00b7chen", "Fron", ",", "dich", "dum\u00b7pfem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "ADJA"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Glauben verpflichtend,", "tokens": ["Glau\u00b7ben", "ver\u00b7pflich\u00b7tend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Pfaffenknecht? Ab schw\u00f6rest du Licht und Wahrheit?", "tokens": ["Pfaf\u00b7fen\u00b7knecht", "?", "Ab", "schw\u00f6\u00b7rest", "du", "Licht", "und", "Wahr\u00b7heit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am Altarschmaus dann des gebacknen Gottes", "tokens": ["Am", "Al\u00b7tar\u00b7schmaus", "dann", "des", "ge\u00b7back\u00b7nen", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schnaubst du dem, was Menschen vom Tier erhebet,", "tokens": ["Schnaubst", "du", "dem", ",", "was", "Men\u00b7schen", "vom", "Tier", "er\u00b7he\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "$,", "PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ha\u00df und Verfolgung?", "tokens": ["Ha\u00df", "und", "Ver\u00b7fol\u00b7gung", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "H\u00f6r', o Stolberg; Worte von Gott verk\u00fcnd' ich", "tokens": ["H\u00f6r'", ",", "o", "Stol\u00b7berg", ";", "Wor\u00b7te", "von", "Gott", "ver\u00b7k\u00fcnd'", "ich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$.", "NN", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Alter Freund. Mi\u00dftraue der Priestersatzung,", "tokens": ["Al\u00b7ter", "Freund", ".", "Mi\u00df\u00b7trau\u00b7e", "der", "Pries\u00b7ter\u00b7sat\u00b7zung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wenn den Abgott auch der Sirene Zauber-", "tokens": ["Wenn", "den", "Ab\u00b7gott", "auch", "der", "Si\u00b7re\u00b7ne", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stimme besch\u00f6nigt!", "tokens": ["Stim\u00b7me", "be\u00b7sch\u00f6\u00b7nigt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Schau', wie dort aufstarrender Pfaffen Chortanz", "tokens": ["Schau'", ",", "wie", "dort", "auf\u00b7star\u00b7ren\u00b7der", "Pfaf\u00b7fen", "Chor\u00b7tanz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ADJA", "NN", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Um des Abgotts Opferaltar einherhinkt:", "tokens": ["Um", "des", "Ab\u00b7gotts", "Op\u00b7fer\u00b7al\u00b7tar", "ein\u00b7her\u00b7hinkt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u00bbgott, allein uns Gott! o gesegn' allein uns,", "tokens": ["\u00bb", "gott", ",", "al\u00b7lein", "uns", "Gott", "!", "o", "ge\u00b7segn'", "al\u00b7lein", "uns", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "PPER", "NN", "$.", "FM", "FM", "ADV", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Fluche den andern!", "tokens": ["Flu\u00b7che", "den", "an\u00b7dern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Unser Schrein, ach! unsre Gel\u00fcbd' erh\u00f6r' uns,", "tokens": ["Un\u00b7ser", "Schrein", ",", "ach", "!", "uns\u00b7re", "Ge\u00b7l\u00fcbd'", "er\u00b7h\u00f6r'", "uns", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Unsres Leibs Blutstr\u00f6me! das Blut Verkl\u00e4rter,", "tokens": ["Uns\u00b7res", "Leibs", "Blut\u00b7str\u00f6\u00b7me", "!", "das", "Blut", "Ver\u00b7kl\u00e4r\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Die f\u00fcr uns abb\u00fc\u00dften!\u00ab Umsonst! denn ohrlos", "tokens": ["Die", "f\u00fcr", "uns", "ab\u00b7b\u00fc\u00df\u0