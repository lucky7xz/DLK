{"textgrid.poem.38585": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Tugend. Dem Grabe Elisas geweiht", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, Vollendete! du hast den Kranz errungen", "tokens": ["Heil", "dir", ",", "Voll\u00b7en\u00b7de\u00b7te", "!", "du", "hast", "den", "Kranz", "er\u00b7run\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Den dir die Tugend wand; durch tr\u00fcbe D\u00e4mmerungen", "tokens": ["Den", "dir", "die", "Tu\u00b7gend", "wand", ";", "durch", "tr\u00fc\u00b7be", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drangst du mit Himmelskraft empor zum ewgen Licht,", "tokens": ["Drangst", "du", "mit", "Him\u00b7mels\u00b7kraft", "em\u00b7por", "zum", "ew\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich schreckte selbst die Nacht am Scheidewege nicht;", "tokens": ["Dich", "schreck\u00b7te", "selbst", "die", "Nacht", "am", "Schei\u00b7de\u00b7we\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Schimmer jenes Heils, das dort am Wonneziel", "tokens": ["Ein", "Schim\u00b7mer", "je\u00b7nes", "Heils", ",", "das", "dort", "am", "Won\u00b7ne\u00b7ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der guten Seelen stralt, erhob dich zum Gef\u00fchl", "tokens": ["Der", "gu\u00b7ten", "See\u00b7len", "stralt", ",", "er\u00b7hob", "dich", "zum", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Unverg\u00e4nglichkeit.", "tokens": ["Der", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und dies Gef\u00fchl vor dem das w\u00fcthende Get\u00fcmmel", "tokens": ["Und", "dies", "Ge\u00b7f\u00fchl", "vor", "dem", "das", "w\u00fct\u00b7hen\u00b7de", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erdenst\u00fcrme schweigt, das einen ganzen Himmel", "tokens": ["Der", "Er\u00b7den\u00b7st\u00fcr\u00b7me", "schweigt", ",", "das", "ei\u00b7nen", "gan\u00b7zen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stillheitrer, sanfter Ruh' in edle Seelen gie\u00dft,", "tokens": ["Still\u00b7hei\u00b7trer", ",", "sanf\u00b7ter", "Ruh'", "in", "ed\u00b7le", "See\u00b7len", "gie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist der erhabne Lohn der aus der Tugend flie\u00dft!", "tokens": ["Ist", "der", "er\u00b7hab\u00b7ne", "Lohn", "der", "aus", "der", "Tu\u00b7gend", "flie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo diese Gottheit wohnt, bl\u00fcht Engelseligkeit,", "tokens": ["Wo", "die\u00b7se", "Got\u00b7theit", "wohnt", ",", "bl\u00fcht", "En\u00b7gel\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wallt spiegelrein und still der Strom der Lebenszeit", "tokens": ["Wallt", "spie\u00b7gel\u00b7rein", "und", "still", "der", "Strom", "der", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch Paradiesesau'n!", "tokens": ["Durch", "Pa\u00b7ra\u00b7die\u00b7se\u00b7sau'n", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es mag umschw\u00e4rzt von Nacht und grausen Ungewittern,", "tokens": ["Es", "mag", "um\u00b7schw\u00e4rzt", "von", "Nacht", "und", "grau\u00b7sen", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Donnersturm umras't, des Erdballs Axe zittern,", "tokens": ["Vom", "Don\u00b7ner\u00b7sturm", "um\u00b7ras't", ",", "des", "Erd\u00b7balls", "A\u00b7xe", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Elemente Kampf Tod und Vernichtung dr\u00e4u'n,", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7te", "Kampf", "Tod", "und", "Ver\u00b7nich\u00b7tung", "dr\u00e4u'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und stolzer Flotten Macht wie d\u00fcrres Laub verstreun:", "tokens": ["Und", "stol\u00b7zer", "Flot\u00b7ten", "Macht", "wie", "d\u00fcr\u00b7res", "Laub", "ver\u00b7streun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo diese Gottheit wohnt, erheitert sich die Luft,", "tokens": ["Wo", "die\u00b7se", "Got\u00b7theit", "wohnt", ",", "er\u00b7hei\u00b7tert", "sich", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Fluren sind Gesang, und K\u00fchlung weht und Duft", "tokens": ["Die", "Flu\u00b7ren", "sind", "Ge\u00b7sang", ",", "und", "K\u00fch\u00b7lung", "weht", "und", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "KON", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus stiller Haine Gr\u00fcn!", "tokens": ["Aus", "stil\u00b7ler", "Hai\u00b7ne", "Gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es mag, am j\u00e4hen Rand verla\u00dfner, wilder K\u00fcsten", "tokens": ["Es", "mag", ",", "am", "j\u00e4\u00b7hen", "Rand", "ver\u00b7la\u00df\u00b7ner", ",", "wil\u00b7der", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "APPRART", "ADJA", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf rauher Felsenbahn, in menschenleeren W\u00fcsten", "tokens": ["Auf", "rau\u00b7her", "Fel\u00b7sen\u00b7bahn", ",", "in", "men\u00b7schen\u00b7lee\u00b7ren", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der m\u00fcde Wandrer gehn; schon brach sein Pilgerstab,", "tokens": ["Der", "m\u00fc\u00b7de", "Wand\u00b7rer", "gehn", ";", "schon", "brach", "sein", "Pil\u00b7ger\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schon d\u00fcnkt die Sch\u00f6pfung ihm ein immeroffnes Grab:", "tokens": ["Schon", "d\u00fcnkt", "die", "Sch\u00f6p\u00b7fung", "ihm", "ein", "im\u00b7me\u00b7roff\u00b7nes", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wo diese Gottheit wohnt, versch\u00f6nt sich jeder Pfad,", "tokens": ["Wo", "die\u00b7se", "Got\u00b7theit", "wohnt", ",", "ver\u00b7sch\u00f6nt", "sich", "je\u00b7der", "Pfad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo ihres Lieblings Tritt voll Zuversicht sich naht,", "tokens": ["Wo", "ih\u00b7res", "Lieb\u00b7lings", "Tritt", "voll", "Zu\u00b7ver\u00b7sicht", "sich", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "ADJD", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zum Schattengang der Ruh'!", "tokens": ["Zum", "Schat\u00b7ten\u00b7gang", "der", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es mag des Todes Arm, im Vollgenu\u00df der Freuden", "tokens": ["Es", "mag", "des", "To\u00b7des", "Arm", ",", "im", "Voll\u00b7ge\u00b7nu\u00df", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhabner Sympathie, den Freund vom Freunde scheiden,", "tokens": ["Er\u00b7hab\u00b7ner", "Sym\u00b7pa\u00b7thie", ",", "den", "Freund", "vom", "Freun\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sanft und fest und treu, am Abgrund der Gefahr,", "tokens": ["Der", "sanft", "und", "fest", "und", "treu", ",", "am", "Ab\u00b7grund", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie auf der Bahn des Gl\u00fccks, ihm Alles, Alles war:", "tokens": ["Wie", "auf", "der", "Bahn", "des", "Gl\u00fccks", ",", "ihm", "Al\u00b7les", ",", "Al\u00b7les", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,", "PPER", "PIS", "$,", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo diese Gottheit wohnt, Verla\u00dfner, da erhellt", "tokens": ["Wo", "die\u00b7se", "Got\u00b7theit", "wohnt", ",", "Ver\u00b7la\u00df\u00b7ner", ",", "da", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Zukunft Mitternacht ein Stern der bessern Welt", "tokens": ["Der", "Zu\u00b7kunft", "Mit\u00b7ter\u00b7nacht", "ein", "Stern", "der", "bes\u00b7sern", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit sanfter Hofnung Glanz!", "tokens": ["Mit", "sanf\u00b7ter", "Hof\u00b7nung", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es mag, wenn ringsumher die Rosen sich entf\u00e4rben,", "tokens": ["Es", "mag", ",", "wenn", "rings\u00b7um\u00b7her", "die", "Ro\u00b7sen", "sich", "ent\u00b7f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des J\u00fcnglings Scherze fliehn, des Mannes Freuden sterben,", "tokens": ["Des", "J\u00fcng\u00b7lings", "Scher\u00b7ze", "fliehn", ",", "des", "Man\u00b7nes", "Freu\u00b7den", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der lezte s\u00fcsse Ton der Liebe selbst verwehn", "tokens": ["Der", "lez\u00b7te", "s\u00fcs\u00b7se", "Ton", "der", "Lie\u00b7be", "selbst", "ver\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und jedes goldne Bild der T\u00e4uschung untergehn:", "tokens": ["Und", "je\u00b7des", "gold\u00b7ne", "Bild", "der", "T\u00e4u\u00b7schung", "un\u00b7ter\u00b7gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo diese Gottheit wohnt, reicht die Erinnerung", "tokens": ["Wo", "die\u00b7se", "Got\u00b7theit", "wohnt", ",", "reicht", "die", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-++--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dem Allverge\u00dfnen noch den lezten Labetrunk", "tokens": ["Dem", "All\u00b7ver\u00b7ge\u00df\u00b7nen", "noch", "den", "lez\u00b7ten", "La\u00b7be\u00b7trunk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn schon sein Auge bricht!", "tokens": ["Wenn", "schon", "sein", "Au\u00b7ge", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Kein Stundenschlag ert\u00f6nt, kein Tropfen Zeit entfluthet,", "tokens": ["Kein", "Stun\u00b7den\u00b7schlag", "er\u00b7t\u00f6nt", ",", "kein", "Trop\u00b7fen", "Zeit", "ent\u00b7flut\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht ein edles Herz um edle Herzen blutet,", "tokens": ["Wo", "nicht", "ein", "ed\u00b7les", "Herz", "um", "ed\u00b7le", "Her\u00b7zen", "blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"