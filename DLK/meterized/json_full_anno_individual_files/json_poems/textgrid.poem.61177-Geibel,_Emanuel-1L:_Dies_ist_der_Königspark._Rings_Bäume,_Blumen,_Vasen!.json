ollen Kranz ber\u00fchrt,", "tokens": ["Mit", "sei\u00b7ner", "Rech\u00b7ten", "fast", "den", "vol\u00b7len", "Kranz", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er, der das scheue Kind, noch rot von s\u00fc\u00dfem Schrecken,", "tokens": ["Er", ",", "der", "das", "scheu\u00b7e", "Kind", ",", "noch", "rot", "von", "s\u00fc\u00b7\u00dfem", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die deutsche Poesie, aus welschen Taxushecken", "tokens": ["Die", "deut\u00b7sche", "Poe\u00b7sie", ",", "aus", "wel\u00b7schen", "Ta\u00b7xus\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Zum freien Dichterwalde f\u00fchrt.", "tokens": ["Zum", "frei\u00b7en", "Dich\u00b7ter\u00b7wal\u00b7de", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Dies ist der K\u00f6nigspark. Rings B\u00e4ume, Blumen, Vasen!", "tokens": ["Dies", "ist", "der", "K\u00f6\u00b7nigs\u00b7park", ".", "Rings", "B\u00e4u\u00b7me", ",", "Blu\u00b7men", ",", "Va\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "NN", "NN", "$,", "NN", "$,", 