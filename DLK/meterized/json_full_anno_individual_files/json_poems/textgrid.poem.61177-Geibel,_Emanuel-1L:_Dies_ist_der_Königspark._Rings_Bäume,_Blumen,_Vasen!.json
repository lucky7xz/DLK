{"textgrid.poem.61177": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dies ist der K\u00f6nigspark. Rings B\u00e4ume, Blumen, Vasen!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies ist der K\u00f6nigspark. Rings B\u00e4ume, Blumen, Vasen!", "tokens": ["Dies", "ist", "der", "K\u00f6\u00b7nigs\u00b7park", ".", "Rings", "B\u00e4u\u00b7me", ",", "Blu\u00b7men", ",", "Va\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, wie ins Muschelhorn die Steintritonen blasen!", "tokens": ["Sieh", ",", "wie", "ins", "Mu\u00b7schel\u00b7horn", "die", "Stein\u00b7tri\u00b7to\u00b7nen", "bla\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Nymphe spiegelt klar sich in des Beckens Scho\u00df:", "tokens": ["Die", "Nym\u00b7phe", "spie\u00b7gelt", "klar", "sich", "in", "des", "Be\u00b7ckens", "Scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sieh hier der Flora Bild in hoher Rosen Mitten,", "tokens": ["Sieh", "hier", "der", "Flo\u00b7ra", "Bild", "in", "ho\u00b7her", "Ro\u00b7sen", "Mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Laubeng\u00e4nge sieh, so regelrecht geschnitten,", "tokens": ["Die", "Lau\u00b7ben\u00b7g\u00e4n\u00b7ge", "sieh", ",", "so", "re\u00b7gel\u00b7recht", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als w\u00e4ren's Verse Boileaus!", "tokens": ["Als", "w\u00e4\u00b7ren's", "Ver\u00b7se", "Boi\u00b7le\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Vorbei am luft'gen Haus voll fremder V\u00f6gelstimmen", "tokens": ["Vor\u00b7bei", "am", "luft'\u00b7gen", "Haus", "voll", "frem\u00b7der", "V\u00f6\u00b7gel\u00b7stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df uns den Hang empor zu den Terrassen klimmen,", "tokens": ["La\u00df", "uns", "den", "Hang", "em\u00b7por", "zu", "den", "Ter\u00b7ras\u00b7sen", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die der Orange Wuchs umkr\u00e4nzt mit falbem Gr\u00fcn!", "tokens": ["Die", "der", "O\u00b7ran\u00b7ge", "Wuchs", "um\u00b7kr\u00e4nzt", "mit", "fal\u00b7bem", "Gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dort oben ragt, wo frisch sich Tann' und Buche mischen,", "tokens": ["Dort", "o\u00b7ben", "ragt", ",", "wo", "frisch", "sich", "Tann'", "und", "Bu\u00b7che", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "PWAV", "ADJD", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das schmucklos heitre Schlo\u00df mit breiten Fensternischen,", "tokens": ["Das", "schmuck\u00b7los", "heit\u00b7re", "Schlo\u00df", "mit", "brei\u00b7ten", "Fens\u00b7ter\u00b7ni\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darin des Abends Feuer gl\u00fchn.", "tokens": ["Da\u00b7rin", "des", "A\u00b7bends", "Feu\u00b7er", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dort lehnt ein Mann im Stuhl: sein Haupt ist vorgesunken,", "tokens": ["Dort", "lehnt", "ein", "Mann", "im", "Stuhl", ":", "sein", "Haupt", "ist", "vor\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein blaues Auge sinnt, und oft in hellen Funken", "tokens": ["Sein", "blau\u00b7es", "Au\u00b7ge", "sinnt", ",", "und", "oft", "in", "hel\u00b7len", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entz\u00fcndet sich's; so spr\u00fcht aus dunkler Luft ein Blitz.", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "sich's", ";", "so", "spr\u00fcht", "aus", "dunk\u00b7ler", "Luft", "ein", "Blitz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein dreigespitzter Hut bedeckt der Schl\u00e4fe Weichen,", "tokens": ["Ein", "drei\u00b7ge\u00b7spitz\u00b7ter", "Hut", "be\u00b7deckt", "der", "Schl\u00e4\u00b7fe", "Wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Kr\u00fcckstock irrt im Sand und schreibt verworrne Zeichen -", "tokens": ["Sein", "Kr\u00fcck\u00b7stock", "irrt", "im", "Sand", "und", "schreibt", "ver\u00b7worr\u00b7ne", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht irrst du, das ist K\u00f6nig Fritz.", "tokens": ["Nicht", "irrst", "du", ",", "das", "ist", "K\u00f6\u00b7nig", "Fritz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "$,", "PDS", "VAFIN", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sitzt und sinnt und schreibt. Kannst du sein Br\u00fcten deuten?", "tokens": ["Er", "sitzt", "und", "sinnt", "und", "schreibt", ".", "Kannst", "du", "sein", "Br\u00fc\u00b7ten", "deu\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denkt er an Kunersdorf, an Ro\u00dfbach oder Leuthen,", "tokens": ["Denkt", "er", "an", "Ku\u00b7ners\u00b7dorf", ",", "an", "Ro\u00df\u00b7bach", "o\u00b7der", "Leu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Hochkirchs Nacht, durchgl\u00fcht von Flammen hundertfach,", "tokens": ["An", "Hoch\u00b7kirchs", "Nacht", ",", "durch\u00b7gl\u00fcht", "von", "Flam\u00b7men", "hun\u00b7dert\u00b7fach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie dort im roten Qualm gegrollt die Feldkanonen,", "tokens": ["Wie", "dort", "im", "ro\u00b7ten", "Qualm", "ge\u00b7grollt", "die", "Feld\u00b7ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indes die Reiterei mit rasselnden Schwadronen", "tokens": ["In\u00b7des", "die", "Rei\u00b7te\u00b7rei", "mit", "ras\u00b7seln\u00b7den", "Schwad\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Grenadiere Viereck brach?", "tokens": ["Der", "Gre\u00b7na\u00b7die\u00b7re", "Vier\u00b7eck", "brach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schwebt ein Gesetz ihm vor, mit dem er weis' und milde", "tokens": ["Schwebt", "ein", "Ge\u00b7setz", "ihm", "vor", ",", "mit", "dem", "er", "weis'", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein schlachterstarktes Volk zu sch\u00f6ner Menschheit bilde,", "tokens": ["Sein", "schlach\u00b7ter\u00b7stark\u00b7tes", "Volk", "zu", "sch\u00f6\u00b7ner", "Menschheit", "bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Friedensgru\u00df, wo j\u00fcngst die Kriegespauke scholl?", "tokens": ["Ein", "Frie\u00b7dens\u00b7gru\u00df", ",", "wo", "j\u00fcngst", "die", "Krie\u00b7ge\u00b7spau\u00b7ke", "scholl", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ersinnt er einen Reim, der seinen Sieg verkl\u00e4re,", "tokens": ["Er\u00b7sinnt", "er", "ei\u00b7nen", "Reim", ",", "der", "sei\u00b7nen", "Sieg", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oder ein Epigramm, mit dem bei Tisch Voltaire,", "tokens": ["O\u00b7der", "ein", "E\u00b7pig\u00b7ramm", ",", "mit", "dem", "bei", "Tisch", "Vol\u00b7tai\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "APPR", "NN", "NE", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der Schalk, gez\u00fcchtigt werden soll?", "tokens": ["Der", "Schalk", ",", "ge\u00b7z\u00fcch\u00b7tigt", "wer\u00b7den", "soll", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Vielleicht auch treten ihm die Bilder nah, die alten,", "tokens": ["Viel\u00b7leicht", "auch", "tre\u00b7ten", "ihm", "die", "Bil\u00b7der", "nah", ",", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da er im Mondenlicht in seines Schlafrocks Falten", "tokens": ["Da", "er", "im", "Mon\u00b7den\u00b7licht", "in", "sei\u00b7nes", "Schla\u00b7frocks", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sanfte Fl\u00f6t' ergriff, des Vaters \u00c4rgernis?", "tokens": ["Die", "sanf\u00b7te", "Fl\u00f6t'", "er\u00b7griff", ",", "des", "Va\u00b7ters", "\u00c4r\u00b7ger\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des treuen Freundes Geist will er heraufbeschw\u00f6ren,", "tokens": ["Des", "treu\u00b7en", "Freun\u00b7des", "Geist", "will", "er", "her\u00b7auf\u00b7be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem - ach, um ihn! - das Blei aus sieben Feuerr\u00f6hren", "tokens": ["Dem", "ach", ",", "um", "ihn", "!", "das", "Blei", "aus", "sie\u00b7ben", "Feu\u00b7er\u00b7r\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ITJ", "$,", "KOUI", "PPER", "$.", "$(", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00fchne J\u00fcnglingsbrust zerri\u00df?", "tokens": ["Die", "k\u00fch\u00b7ne", "J\u00fcng\u00b7lings\u00b7brust", "zer\u00b7ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Tr\u00e4umt in die Zukunft er? Zeigt ihm den immer vollern,", "tokens": ["Tr\u00e4umt", "in", "die", "Zu\u00b7kunft", "er", "?", "Zeigt", "ihm", "den", "im\u00b7mer", "vol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$.", "VVFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Den immer k\u00fchnern Flug des Aars von Hohenzollern,", "tokens": ["Den", "im\u00b7mer", "k\u00fch\u00b7nern", "Flug", "des", "Aars", "von", "Ho\u00b7hen\u00b7zol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schon den Doppelaar geb\u00e4ndigt, ein Gesicht?", "tokens": ["Der", "schon", "den", "Dop\u00b7pe\u00b7laar", "ge\u00b7b\u00e4n\u00b7digt", ",", "ein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedenkt er, wie dereinst ganz Deutschland hoffend lausche", "tokens": ["Ge\u00b7denkt", "er", ",", "wie", "de\u00b7reinst", "ganz", "Deutschland", "hof\u00b7fend", "lau\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "NE", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und bangend, wenn daher sein schwarzer Fittich rausche? -", "tokens": ["Und", "ban\u00b7gend", ",", "wenn", "da\u00b7her", "sein", "schwar\u00b7zer", "Fit\u00b7tich", "rau\u00b7sche", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O nein, das alles ist es nicht.", "tokens": ["O", "nein", ",", "das", "al\u00b7les", "ist", "es", "nicht", "."], "token_info"