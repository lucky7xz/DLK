"word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Hochkirchs Nacht, durchgl\u00fcht von Flammen hundertfach,", "tokens": ["An", "Hoch\u00b7kirchs", "Nacht", ",", "durch\u00b7gl\u00fcht", "von", "Flam\u00b7men", "hun\u00b7dert\u00b7fach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie dort im roten Qualm gegrollt die Feldkanonen,", "tokens": ["Wie", "dort", "im", "ro\u00b7ten", "Qualm", "ge\u00b7grollt", "die", "Feld\u00b7ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indes die Reiterei mit rasselnden Schwadronen", "tokens": ["In\u00b7des", "die", "Rei\u00b7te\u00b7rei", "mit", "ras\u00b7seln\u00b7den", "Schwad\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Grenadiere Viereck brach?", "tokens": ["Der", "Gre\u00b7na\u00b7die\u00b7re", "Vier\u00b7eck", "brach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Schwebt ein Gesetz ihm vor, mit dem er weis' und milde", "tokens": ["Schwebt", "ein", "Ge\u00b7setz", "ihm", "vor", ",", "mit", "dem", "er", "weis'", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein schlachterstarktes Volk zu sch\u00f6ner Menschheit bilde,", "tokens": ["Sein", "schlach\u00b7ter\u00b7stark\u00b7tes", "Volk", "zu", "sch\u00f6\u00b7ner", "Menschheit", "bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Friedensgru\u00df, wo j\u00fcngst die Kriegespauke scholl?", "tokens": ["Ein", "Frie\u00b7dens\u00b7gru\u00df", ",", "wo", "j\u00fcngst", "die", "Krie\u00b7ge\u00b7spau\u00b7ke", "scholl", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ersinnt er einen Reim, der seinen Sieg verkl\u00e4re,", "tokens": ["Er\u00b7sinnt", "er", "ei\u00b7nen", "Reim", ",", "der", "sei\u00b7nen", "Sieg", "ver\u00b7kl\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Oder ein Epigramm, mit dem bei Tisch Voltaire,", "tokens": ["O\u00b7der", "ein", "E\u00b7pig\u00b7ramm", ",", "mit", "dem", "bei", "Tisch", "Vol\u00b7tai\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "APPR", "NN", "NE", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Der Schalk, gez\u00fcchtigt werden soll?", "tokens": ["Der", "Schalk", ",", "ge\u00b7z\u00fcch\u00b7tigt", "wer\u00b7den", "soll", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Vielleicht auch treten ihm die Bilder nah, die alten,", "tokens": ["Viel\u00b7leicht", "auch", "tre\u00b7ten", "ihm", "die", "Bil\u00b7der", "nah", ",", "die", "al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da er im Mondenlicht in seines Schlafrocks Falten", "tokens": ["Da", "er", "im", "Mon\u00b7den\u00b7licht", "in", "sei\u00b7nes", "Schla\u00b7frocks", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sanfte Fl\u00f6t' ergriff, des Vaters \u00c4rgernis?", "tokens": ["Die", "sanf\u00b7te", "Fl\u00f6t'", "er\u00b7griff", ",", "des", "Va\u00b7ters", "\u00c4r\u00b7ger\u00b7nis", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des treuen Freundes Geist will er heraufbeschw\u00f6ren,", "tokens": ["Des", "treu\u00b7en", "Freun\u00b7des", "Geist", "will", "er", "her\u00b7auf\u00b7be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem - ach, um ihn! - das Blei aus sieben Feuerr\u00f6hren", "tokens": ["Dem", "ach", ",", "um", "ihn", "!", "das", "Blei", "aus", "sie\u00b7ben", "Feu\u00b7er\u00b7r\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ITJ", "$,", "KOUI", "PPER", "$.", "$(", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00fchne J\u00fcnglingsbrust zerri\u00df?", "tokens": ["Die", "k\u00fch\u00b7ne", "J\u00fcng\u00b7lings\u00b7brust", "zer\u00b7ri\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Tr\u00e4umt in die Zukunft er? Zeigt ihm den immer vollern,", "tokens": ["Tr\u00e4umt", "in", "die", "Zu\u00b7kunft", "er", "?", "Zeigt", "ihm", "den", "im\u00b7mer", "vol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$.", "VVFIN", "PPER", "ART", "ADV", "VVINF", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Den immer k\u00fchnern Flug des Aars von Hohenzollern,", "tokens": ["Den", "im\u00b7mer", "k\u00fch\u00b7nern", "Flug", "des", "Aars", "von", "Ho\u00b7hen\u00b7zol\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der schon den Doppelaar geb\u00e4ndigt, ein Gesicht?", "tokens": ["Der", "schon", "den", "Dop\u00b7pe\u00b7laar", "ge\u00b7b\u00e4n\u00b7digt", ",", "ein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gedenkt er, wie dereinst ganz Deutschland hoffend lausche", "tokens": ["Ge\u00b7denkt", "er", ",", "wie", "de\u00b7reinst", "ganz", "Deutschland", "hof\u00b7fend", "lau\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "ADV", "NE", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und bangend, wenn daher sein schwarzer Fittich rausche? -", "tokens": ["Und", "ban\u00b7gend", ",", "wenn", "da\u00b7her", "sein", "schwar\u00b7zer", "Fit\u00b7tich", "rau\u00b7sche", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O nein, das alles ist es nicht.", "tokens": ["O", "nein", ",", "das", "al\u00b7les", "ist", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PRELS", "PIS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Er murrt: \u00bbO Schmerz, als Held gesandt sein einem Volke,", "tokens": ["Er", "murrt", ":", "\u00bb", "O", "Schmerz", ",", "als", "Held", "ge\u00b7sandt", "sein", "ei\u00b7nem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "NN", "$,", "KOUS", "NN", "VVPP", "VAINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2"