{"textgrid.poem.31479": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Aller heiligste Menschgeburt meines Erl\u00f6sers", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Heilig hohes Werk! wie solt' ich dich recht ehren?", "tokens": ["O", "Hei\u00b7lig", "ho\u00b7hes", "Werk", "!", "wie", "solt'", "ich", "dich", "recht", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$.", "PWAV", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "soll ich / vor tieffer Furcht und Ehrerbietigkeit /", "tokens": ["soll", "ich", "/", "vor", "tief\u00b7fer", "Furcht", "und", "Ehr\u00b7er\u00b7bie\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit stiller Wunderung erzeigen meine Freud?", "tokens": ["mit", "stil\u00b7ler", "Wun\u00b7de\u00b7rung", "er\u00b7zei\u00b7gen", "mei\u00b7ne", "Freud", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach! oder darff mein' Hand dein Lob mit Ruhm vermehren?", "tokens": ["Ach", "!", "o\u00b7der", "darff", "mein'", "Hand", "dein", "Lob", "mit", "Ruhm", "ver\u00b7meh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wann dieses: Ewigs Wort / so wollst mir Wort bescheren /", "tokens": ["wann", "die\u00b7ses", ":", "E\u00b7wigs", "Wort", "/", "so", "wollst", "mir", "Wort", "be\u00b7sche\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "$.", "ADJA", "NN", "$(", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dein Wunder-Menschgeburt zu r\u00fchmen in der Zeit!", "tokens": ["dein", "Wun\u00b7der\u00b7Menschge\u00b7burt", "zu", "r\u00fch\u00b7men", "in", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "mein Herz ist schon / ach mach auch Sinn und Hand / bereit!", "tokens": ["mein", "Herz", "ist", "schon", "/", "ach", "mach", "auch", "Sinn", "und", "Hand", "/", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "du wollst mir / was gereicht zu deiner Ehr / gewehren?", "tokens": ["du", "wollst", "mir", "/", "was", "ge\u00b7reicht", "zu", "dei\u00b7ner", "Ehr", "/", "ge\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "PWS", "VVPP", "APPR", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text":