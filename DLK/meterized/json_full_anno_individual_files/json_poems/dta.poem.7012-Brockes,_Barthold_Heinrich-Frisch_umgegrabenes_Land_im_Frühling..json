{"dta.poem.7012": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Frisch umgegrabenes Land  \n im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich also stand und dacht\u2019,", "tokens": ["In\u00b7dem", "ich", "al\u00b7so", "stand", "und", "dacht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erblickt ich da\u00df des Kleides dunckle Pracht,", "tokens": ["Er\u00b7blickt", "ich", "da\u00df", "des", "Klei\u00b7des", "dunck\u00b7le", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch mehr geschm\u00fcckt und sch\u00f6ner noch gezieret,", "tokens": ["Noch", "mehr", "ge\u00b7schm\u00fcckt", "und", "sch\u00f6\u00b7ner", "noch", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit sch\u00f6nem Ranckenwerck brodieret", "tokens": ["Mit", "sch\u00f6\u00b7nem", "Ran\u00b7cken\u00b7werck", "bro\u00b7die\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und recht durchwircket war.", "tokens": ["Und", "recht", "durch\u00b7wir\u00b7cket", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein G\u00e4rtner hatte hier und dar", "tokens": ["Mein", "G\u00e4rt\u00b7ner", "hat\u00b7te", "hier", "und", "dar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den braunen sammtenen Talar", "tokens": ["Den", "brau\u00b7nen", "samm\u00b7te\u00b7nen", "Ta\u00b7lar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit nettem Laubwerck sch\u00f6n gestickt,", "tokens": ["Mit", "net\u00b7tem", "Laub\u00b7werck", "sch\u00f6n", "ge\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ihn mit zarten Laub geschm\u00fcckt,", "tokens": ["Und", "ihn", "mit", "zar\u00b7ten", "Laub", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da er von jungen Erbs- und Bohnen", "tokens": ["Da", "er", "von", "jun\u00b7gen", "Erbs", "und", "Boh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Pflantzen, welche er im Winter aufgebracht,", "tokens": ["Die", "Pflant\u00b7zen", ",", "wel\u00b7che", "er", "im", "Win\u00b7ter", "auf\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bald hier bald dar verpflantzt. Der gr\u00fcnen Farbe Pracht", "tokens": ["Bald", "hier", "bald", "dar", "ver\u00b7pflantzt", ".", "Der", "gr\u00fc\u00b7nen", "Far\u00b7be", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "PTKVZ", "VVPP", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zumahl,", "tokens": ["Zu\u00b7mahl", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Wenn durch das zarte Laub der helle Sonnen Strahl,", "tokens": ["Wenn", "durch", "das", "zar\u00b7te", "Laub", "der", "hel\u00b7le", "Son\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit holdem Schimmer drang, war so vortrefflich sch\u00f6n,", "tokens": ["Mit", "hol\u00b7dem", "Schim\u00b7mer", "drang", ",", "war", "so", "vor\u00b7treff\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So klar so lieblich anzusehn,", "tokens": ["So", "klar", "so", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Absonderlich auf dem so duncklen Grunde;", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "auf", "dem", "so", "dunck\u00b7len", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.18": {"text": "Da\u00df ich, dadurch ger\u00fchrt, mit Hertz und Munde,", "tokens": ["Da\u00df", "ich", ",", "da\u00b7durch", "ge\u00b7r\u00fchrt", ",", "mit", "Hertz", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PAV", "VVPP", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Dem grossen Sch\u00f6pfer aller Dinge,", "tokens": ["Dem", "gros\u00b7sen", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Ein fr\u00f6lich Fr\u00fchlings-Lied gleich an zu singen finge:", "tokens": ["Ein", "fr\u00f6\u00b7lich", "Fr\u00fch\u00b7lings\u00b7Lied", "gleich", "an", "zu", "sin\u00b7gen", "fin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}