{"dta.poem.19710": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Zugv\u00f6gel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wie so sch\u00f6n, wie h\u00fcbsch und fein               ", "tokens": ["Ach", "wie", "so", "sch\u00f6n", ",", "wie", "h\u00fcbsch", "und", "fein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "$,", "PWAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind deine Tritt Maria rein", "tokens": ["Sind", "dei\u00b7ne", "Tritt", "Ma\u00b7ria", "rein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deinem Sch\u00fchlein leis dahin,", "tokens": ["In", "dei\u00b7nem", "Sch\u00fch\u00b7lein", "leis", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Jungfrau, was hast du im Sinn?", "tokens": ["Ach", "Jung\u00b7frau", ",", "was", "hast", "du", "im", "Sinn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "PWS", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Du weist, was unterm Herzen tragst,", "tokens": ["Du", "weist", ",", "was", "un\u00b7term", "Her\u00b7zen", "tragst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich wundert, wie du eilen magst?", "tokens": ["Mich", "wun\u00b7dert", ",", "wie", "du", "ei\u00b7len", "magst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eh\u00f6r mich nun an, du frommes Weib,", "tokens": ["\u201e", "h\u00f6r", "mich", "nun", "an", ",", "du", "from\u00b7mes", "Weib", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u201eich trag in meinem reinen Leib,", "tokens": ["\u201e", "ich", "trag", "in", "mei\u00b7nem", "rei\u00b7nen", "Leib", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u201eich trag in mir das ew'ge Wort,", "tokens": ["\u201e", "ich", "trag", "in", "mir", "das", "ew'\u00b7ge", "Wort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u201ebeschwert mich nicht, ja hilft mich fort;", "tokens": ["\u201e", "be\u00b7schwert", "mich", "nicht", ",", "ja", "hilft", "mich", "fort", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u201egleich wie die Federn dem V\u00f6gelein", "tokens": ["\u201e", "gleich", "wie", "die", "Fe\u00b7dern", "dem", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u201enicht hinderlich, nein h\u00fclflich seyn,", "tokens": ["\u201e", "nicht", "hin\u00b7der\u00b7lich", ",", "nein", "h\u00fcl\u00b7flich", "seyn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADJD", "$,", "PTKANT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u201edie Ruder keinem Schiff zur Last,", "tokens": ["\u201e", "die", "Ru\u00b7der", "kei\u00b7nem", "Schiff", "zur", "Last", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "\u201enein treibens, da\u00df es ohne Rast", "tokens": ["\u201e", "nein", "trei\u00b7bens", ",", "da\u00df", "es", "oh\u00b7ne", "Rast"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "\u201ehinschwanket, schwebet ganz allein", "tokens": ["\u201e", "hin\u00b7schwan\u00b7ket", ",", "schwe\u00b7bet", "ganz", "al\u00b7lein"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u201eund bringt den Sohn des Herren heim.\u201c", "tokens": ["\u201e", "und", "bringt", "den", "Sohn", "des", "Her\u00b7ren", "heim", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}