{"textgrid.poem.38601": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mitgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Irrgang dieses Lebens", "tokens": ["Im", "Irr\u00b7gang", "die\u00b7ses", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ists oft so bang' und schw\u00fcl!", "tokens": ["Ists", "oft", "so", "bang'", "und", "schw\u00fcl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher fleht vergebens", "tokens": ["Und", "man\u00b7cher", "fleht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um Trost und Mitgef\u00fchl.", "tokens": ["Um", "Trost", "und", "Mit\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du hast umsonst so sehnlich", "tokens": ["Du", "hast", "um\u00b7sonst", "so", "sehn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Himmel nicht gefleht;", "tokens": ["Zum", "Him\u00b7mel", "nicht", "ge\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du fandst, dem deinen \u00e4hnlich,", "tokens": ["Du", "fandst", "