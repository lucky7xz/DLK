{"dta.poem.9217": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Eine neue Jungfer-Noth.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wje ist das zarte jungfer-fleisch", "tokens": ["Wie", "ist", "das", "zar\u00b7te", "jung\u00b7fer\u00b7fleisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den m\u00e4dgen so beschwerlich/", "tokens": ["Den", "m\u00e4d\u00b7gen", "so", "be\u00b7schwer\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann/ sind sie fromm und keusch/", "tokens": ["Dann", "/", "sind", "sie", "fromm", "und", "keusch", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So ist es gleichwohl sehr gef\u00e4hrlich/", "tokens": ["So", "ist", "es", "gleich\u00b7wohl", "sehr", "ge\u00b7f\u00e4hr\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil ein falscher wahn", "tokens": ["Die\u00b7weil", "ein", "fal\u00b7scher", "wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die sch\u00f6nste tugend schimpffen kan.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "tu\u00b7gend", "schimpf\u00b7fen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Die leuthe sehen auf den schein/", "tokens": ["Die", "leu\u00b7the", "se\u00b7hen", "auf", "den", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lassen solche s", "tokens": ["Und", "las\u00b7sen", "sol\u00b7che", "s"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tugend richter seyn/", "tokens": ["Der", "tu\u00b7gend", "rich\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die weder fromm noch b\u00f6se machen/", "tokens": ["Die", "we\u00b7der", "fromm", "noch", "b\u00f6\u00b7se", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "ADJD", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dencken unverwandt/", "tokens": ["Und", "den\u00b7cken", "un\u00b7ver\u00b7wandt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein m\u00fcckgen sey ein elefant.", "tokens": ["Ein", "m\u00fcck\u00b7gen", "sey", "ein", "e\u00b7le\u00b7fant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJD", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "3. Man lasse doch ein m\u00e4dgen gehn/", "tokens": ["Man", "las\u00b7se", "doch", "ein", "m\u00e4d\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bey den junggesellen/", "tokens": ["Und", "bey", "den", "jung\u00b7ge\u00b7sel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein viertelst\u00fcndgen stehn/", "tokens": ["Ein", "vier\u00b7tel\u00b7st\u00fcnd\u00b7gen", "stehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie werden sich die leute stellen/", "tokens": ["Wie", "wer\u00b7den", "sich", "die", "leu\u00b7te", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als m\u00fcste sie allein", "tokens": ["Als", "m\u00fcs\u00b7te", "sie", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die aller \u00e4rgfte hure seyn.", "tokens": ["Die", "al\u00b7ler", "\u00e4rgf\u00b7te", "hu\u00b7re", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ein m\u00e4dgen kan nicht/ wie sie will", "tokens": ["Ein", "m\u00e4d\u00b7gen", "kan", "nicht", "/", "wie", "sie", "will"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VMFIN", "PTKNEG", "$(", "PWAV", "PPER", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch in der kirche beten:", "tokens": ["Auch", "in", "der", "kir\u00b7che", "be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn l\u00e4st sie gar zu viel", "tokens": ["Denn", "l\u00e4st", "sie", "gar", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die augen aus den falten treten/", "tokens": ["Die", "au\u00b7gen", "aus", "den", "fal\u00b7ten", "tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sieht den himmel an;", "tokens": ["Und", "sieht", "den", "him\u00b7mel", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So heists/ sie meynt den courtisan.", "tokens": ["So", "heists", "/", "sie", "meynt", "den", "cour\u00b7ti\u00b7san", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "PPER", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wie manche zeitung wird erdacht;", "tokens": ["Wie", "man\u00b7che", "zei\u00b7tung", "wird", "er\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da hat sich die versprochen/", "tokens": ["Da", "hat", "sich", "die", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die wird ausgelacht/", "tokens": ["Und", "die", "wird", "aus\u00b7ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dieweil ihr schatz die treu gebrochen/", "tokens": ["Die\u00b7weil", "ihr", "schatz", "die", "treu", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wird die sache klar/", "tokens": ["Und", "wird", "die", "sa\u00b7che", "klar", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So ist das zehnde wort nicht war.", "tokens": ["So", "ist", "das", "zehn\u00b7de", "wort", "nicht", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Die tummen junggesellen", "tokens": ["Die", "tum\u00b7men", "jung\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die solten sich darbey", "tokens": ["Die", "sol\u00b7ten", "sich", "dar\u00b7bey"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur ernsten gegenwehre stellen/", "tokens": ["Zur", "erns\u00b7ten", "ge\u00b7gen\u00b7weh\u00b7re", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bleiben sie zur ruh/", "tokens": ["So", "blei\u00b7ben", "sie", "zur", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und helffen noch wohl selbst darzu.", "tokens": ["Und", "helf\u00b7fen", "noch", "wohl", "selbst", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Die schwitzet in das bette/", "tokens": ["Die", "schwit\u00b7zet", "in", "das", "bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PDS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die hat ein b\u00f6ses bein/", "tokens": ["Die", "hat", "ein", "b\u00f6\u00b7ses", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die tr\u00e4gt nur eine falsche kette/", "tokens": ["Die", "tr\u00e4gt", "nur", "ei\u00b7ne", "fal\u00b7sche", "ket\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die schminckt ihr angesicht/", "tokens": ["Die", "schminckt", "ihr", "an\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die folgt der lieben mutter nicht.", "tokens": ["Die", "folgt", "der", "lie\u00b7ben", "mut\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Die nimt sechs thaler zum gewinn/", "tokens": ["Die", "nimt", "sechs", "tha\u00b7ler", "zum", "ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "CARD", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schleppen doch die m\u00e4use", "tokens": ["Der", "schlep\u00b7pen", "doch", "die", "m\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Holtz-\u00e4pffel sonst wohin/", "tokens": ["Holtz\u00b7\u00e4pf\u00b7fel", "sonst", "wo\u00b7hin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PWAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die bleibt bey ihrer alten weise/", "tokens": ["Die", "bleibt", "bey", "ih\u00b7rer", "al\u00b7ten", "wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ist ein halbes schwein/", "tokens": ["Die", "ist", "ein", "hal\u00b7bes", "schwein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die s\u00e4ufft so gerne brantewein.", "tokens": ["Die", "s\u00e4ufft", "so", "ger\u00b7ne", "bran\u00b7te\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "9. Und die hat einen holen zahn/", "tokens": ["Und", "die", "hat", "ei\u00b7nen", "ho\u00b7len", "zahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum stinckts ihr aus dem loche/", "tokens": ["Drum", "stinckts", "ihr", "aus", "dem", "lo\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die schielt den liebsten an", "tokens": ["Die", "schielt", "den", "liebs\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und gucket in die andre woche/", "tokens": ["Und", "gu\u00b7cket", "in", "die", "and\u00b7re", "wo\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ist ohns henckers danck", "tokens": ["Die", "ist", "ohns", "hen\u00b7ckers", "danck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NE", "NE", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wohl gar am lieben dinge kranck.", "tokens": ["Wohl", "gar", "am", "lie\u00b7ben", "din\u00b7ge", "kranck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "10. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Jm hertzen wohl geplaget/", "tokens": ["Jm", "hert\u00b7zen", "wohl", "ge\u00b7pla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df/ wann sie sich besinnt/", "tokens": ["Da\u00df", "/", "wann", "sie", "sich", "be\u00b7sinnt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PPER", "PRF", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus schmertzen und verzweifflung saget/", "tokens": ["Aus", "schmert\u00b7zen", "und", "ver\u00b7zweif\u00b7flung", "sa\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein leben wird mir schwer/", "tokens": ["Mein", "le\u00b7ben", "wird", "mir", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach wer ein junggeselle w\u00e4r.", "tokens": ["Ach", "wer", "ein", "jung\u00b7ge\u00b7sel\u00b7le", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}