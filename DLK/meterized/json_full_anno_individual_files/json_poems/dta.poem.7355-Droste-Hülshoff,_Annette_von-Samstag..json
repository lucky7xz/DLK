{"dta.poem.7355": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Samstag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie funkeln hell die Sterne,", "tokens": ["Wie", "fun\u00b7keln", "hell", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie dunkel scheint der Grund,", "tokens": ["Wie", "dun\u00b7kel", "scheint", "der", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus des Teiches Spiegel", "tokens": ["Und", "aus", "des", "Tei\u00b7ches", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steigt dort der Mond am H\u00fcgel", "tokens": ["Steigt", "dort", "der", "Mond", "am", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Grad um die elfte Stund'.", "tokens": ["Grad", "um", "die", "elf\u00b7te", "Stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hebt vom Predigthefte", "tokens": ["Da", "hebt", "vom", "Pre\u00b7dig\u00b7thef\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der m\u00fcde Pfarrer sich;", "tokens": ["Der", "m\u00fc\u00b7de", "Pfar\u00b7rer", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl war er unverdrossen,", "tokens": ["Wohl", "war", "er", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und endlich ist's geschlossen,", "tokens": ["Und", "end\u00b7lich", "ist's", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit langem Federstrich.", "tokens": ["Mit", "lan\u00b7gem", "Fe\u00b7der\u00b7strich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun \u00f6ffnet er das Fenster,", "tokens": ["Nun", "\u00f6ff\u00b7net", "er", "das", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er trinkt den milden Duft,", "tokens": ["Er", "trinkt", "den", "mil\u00b7den", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spricht: \u201eWer sollt es sagen,", "tokens": ["Und", "spricht", ":", "\u201e", "Wer", "sollt", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch Schnee vor wenig Tagen,", "tokens": ["Noch", "Schnee", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und dies ist Maienluft.\u201c", "tokens": ["Und", "dies", "ist", "Mai\u00b7en\u00b7luft", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die strahlende Rotunde", "tokens": ["Die", "strah\u00b7len\u00b7de", "Ro\u00b7tun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein ernster Blick durchsp\u00e4ht,", "tokens": ["Sein", "erns\u00b7ter", "Blick", "durch\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon will der Himmelswagen", "tokens": ["Schon", "will", "der", "Him\u00b7mels\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Deichsel abw\u00e4rts tragen.", "tokens": ["Die", "Deich\u00b7sel", "ab\u00b7w\u00e4rts", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eja, ja es ist schon sp\u00e4t!\u201c", "tokens": ["\u201e", "ja", ",", "ja", "es", "ist", "schon", "sp\u00e4t", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "PPER", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als dies Wort gesprochen,", "tokens": ["Und", "als", "dies", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es f\u00e4llt dem Pfarrer auf,", "tokens": ["Es", "f\u00e4llt", "dem", "Pfar\u00b7rer", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als m\u00fc\u00df' er eben deuten", "tokens": ["Als", "m\u00fc\u00df'", "er", "e\u00b7ben", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf sich der ganz zerstreuten,", "tokens": ["Auf", "sich", "der", "ganz", "zer\u00b7streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Arglosen Rede Lauf.", "tokens": ["Arg\u00b7lo\u00b7sen", "Re\u00b7de", "Lauf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nie schien er sich so hager,", "tokens": ["Nie", "schien", "er", "sich", "so", "ha\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie f\u00fchlt' er sich so alt,", "tokens": ["Nie", "f\u00fchlt'", "er", "sich", "so", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als seit er heut begraben", "tokens": ["Als", "seit", "er", "heut", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den langen Moriz Raben,", "tokens": ["Den", "lan\u00b7gen", "Mo\u00b7riz", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den F\u00f6rster dort vom Wald.", "tokens": ["Den", "F\u00f6rs\u00b7ter", "dort", "vom", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Am gleichen Tag geboren,", "tokens": ["Am", "glei\u00b7chen", "Tag", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Getauft am gleichen Tag!", "tokens": ["Ge\u00b7tauft", "am", "glei\u00b7chen", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist ein seltsam Wesen,", "tokens": ["Das", "ist", "ein", "selt\u00b7sam", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00e4\u00dft uns deutlich lesen,", "tokens": ["Und", "l\u00e4\u00dft", "uns", "deut\u00b7lich", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was wohl die Zeit vermag!", "tokens": ["Was", "wohl", "die", "Zeit", "ver\u00b7mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Nacht geheimes Funkeln,", "tokens": ["Der", "Nacht", "ge\u00b7hei\u00b7mes", "Fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und da\u00df sich eben mu\u00df,", "tokens": ["Und", "da\u00df", "sich", "e\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ADV", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Mondesstrahlen steigen,", "tokens": ["Wie", "Mon\u00b7des\u00b7strah\u00b7len", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der frische H\u00fcgel zeigen,", "tokens": ["Der", "fri\u00b7sche", "H\u00fc\u00b7gel", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Kreuz an seinem Fu\u00df:", "tokens": ["Das", "Kreuz", "an", "sei\u00b7nem", "Fu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das macht ihn ganz beklommen,", "tokens": ["Das", "macht", "ihn", "ganz", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den sehr betagten Mann,", "tokens": ["Den", "sehr", "be\u00b7tag\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht den Flieder schwanken,", "tokens": ["Er", "sieht", "den", "Flie\u00b7der", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00e4ngs des H\u00fcgels wanken", "tokens": ["Und", "l\u00e4ngs", "des", "H\u00fc\u00b7gels", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Schatten ab und an.", "tokens": ["Die", "Schat\u00b7ten", "ab", "und", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie oft sprach nicht der Todte", "tokens": ["Wie", "oft", "sprach", "nicht", "der", "Tod\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach seiner Weise k\u00fchn:", "tokens": ["Nach", "sei\u00b7ner", "Wei\u00b7se", "k\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eherr Pfarr', wir alten Knaben,", "tokens": ["\u201e", "herr", "Pfarr'", ",", "wir", "al\u00b7ten", "Kna\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir m\u00fcssen sachte traben,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "sach\u00b7te", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kirchhofsblumen bl\u00fchn.\u201c", "tokens": ["Die", "Kirch\u00b7hofs\u00b7blu\u00b7men", "bl\u00fchn", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u201eso m\u00f6gen sie denn bl\u00fchen!\u201c", "tokens": ["\u201e", "so", "m\u00f6\u00b7gen", "sie", "denn", "bl\u00fc\u00b7hen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Spricht sanft der fromme Mann,", "tokens": ["Spricht", "sanft", "der", "from\u00b7me", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat sich aufgerichtet,", "tokens": ["Er", "hat", "sich", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Auge, mild umlichtet,", "tokens": ["Sein", "Au\u00b7ge", ",", "mild", "um\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schaut fest den Aether an.", "tokens": ["Schaut", "fest", "den", "A\u00b7e\u00b7ther", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+----+", "measure": "dactylic.init"}}, "stanza.12": {"line.1": {"text": "\u201ehast Du gesandt ein Zeichen", "tokens": ["\u201e", "hast", "Du", "ge\u00b7sandt", "ein", "Zei\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "ART", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Durch meinen eignen Mund,", "tokens": ["Durch", "mei\u00b7nen", "eig\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und willst mich gn\u00e4dig mahnen", "tokens": ["Und", "willst", "mich", "gn\u00e4\u00b7dig", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An unser Aller Ahnen,", "tokens": ["An", "un\u00b7ser", "Al\u00b7ler", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uralten ew'gen Bund;\u201c", "tokens": ["Ur\u00b7al\u00b7ten", "ew'\u00b7gen", "Bund", ";", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u201enicht l\u00e4ssig sollst Du finden", "tokens": ["\u201e", "nicht", "l\u00e4s\u00b7sig", "sollst", "Du", "fin\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den, der Dein Siegel tr\u00e4gt,", "tokens": ["Den", ",", "der", "Dein", "Sie\u00b7gel", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["N