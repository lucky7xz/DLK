{"textgrid.poem.59375": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wollen mir vertraun die blauen Weiten,", "tokens": ["Was", "wol\u00b7len", "mir", "ver\u00b7traun", "die", "blau\u00b7en", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Landes Glanz, die Wirrung s\u00fc\u00dfer Lieder,", "tokens": ["Des", "Lan\u00b7des", "Glanz", ",", "die", "Wir\u00b7rung", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist so wohl, so bang! Seid ihr es wieder", "tokens": ["Mir", "ist", "so", "wohl", ",", "so", "bang", "!", "Seid", "ihr", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "ADJD", "$.", "VAIMP", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der frommen Kindheit stille Blumenzeiten?", "tokens": ["Der", "from\u00b7men", "Kind\u00b7heit", "stil\u00b7le", "Blu\u00b7men\u00b7zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl wei\u00df ich's \u2013 dieser Farben heimlich Spreiten", "tokens": ["Wohl", "wei\u00df", "ich's", "\u2013", "die\u00b7ser", "Far\u00b7ben", "heim\u00b7lich", "Sprei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$(", "PDAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deckt einer Jungfrau strahlend reine Glieder;", "tokens": ["Deckt", "ei\u00b7ner", "Jung\u00b7frau", "strah\u00b7lend", "rei\u00b7ne", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es wogt der gro\u00dfe Schleier auf und nieder,", "tokens": ["Es", "wogt", "der", "gro\u00b7\u00dfe", "Schlei\u00b7er", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie schlummert drunten fort seit Ewigkeiten.", "tokens": ["Sie", "schlum\u00b7mert", "drun\u00b7ten", "fort", "seit", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir ist in solchen linden, blauen Tagen,", "tokens": ["Mir", "ist", "in", "sol\u00b7chen", "lin\u00b7den", ",", "blau\u00b7en", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als m\u00fc\u00dften alle Farben auferstehen,", "tokens": ["Als", "m\u00fc\u00df\u00b7ten", "al\u00b7le", "Far\u00b7ben", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus blauer Fern sie endlich zu mir gehen.", "tokens": ["Aus", "blau\u00b7er", "Fern", "sie", "end\u00b7lich", "zu", "mir", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wart ich still, schau in den Fr\u00fchling milde,", "tokens": ["So", "wart", "ich", "still", ",", "schau", "in", "den", "Fr\u00fch\u00b7ling", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ganze Herz weint nach dem s\u00fc\u00dfen Bilde,", "tokens": ["Das", "gan\u00b7ze", "Herz", "weint", "nach", "dem", "s\u00fc\u00b7\u00dfen", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor Freud, vor Schmerz? \u2013 ich wei\u00df es nicht zu sagen.", "tokens": ["Vor", "Freud", ",", "vor", "Schmerz", "?", "\u2013", "ich", "wei\u00df", "es", "nicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$.", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was wollen mir vertraun die blauen Weiten,", "tokens": ["Was", "wol\u00b7len", "mir", "ver\