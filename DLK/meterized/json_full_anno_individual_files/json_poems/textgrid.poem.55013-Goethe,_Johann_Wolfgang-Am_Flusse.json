{"textgrid.poem.55013": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Am Flusse", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verflie\u00dfet, vielgeliebte Lieder,", "tokens": ["Ver\u00b7flie\u00b7\u00dfet", ",", "viel\u00b7ge\u00b7lieb\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Meere der Vergessenheit!", "tokens": ["Zum", "Mee\u00b7re", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Knabe sing entz\u00fcckt euch wieder,", "tokens": ["Kein", "Kna\u00b7be", "sing", "ent\u00b7z\u00fcckt", "euch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein M\u00e4dchen in der Bl\u00fctenzeit.", "tokens": ["Kein", "M\u00e4d\u00b7chen", "in", "der", "Bl\u00fc\u00b7ten\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr sanget nur von meiner Lieben;", "tokens": ["Ihr", "san\u00b7get", "nur", "von", "mei\u00b7ner", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun spricht sie meiner Treue Hohn.", "tokens": ["Nun", "spricht", "sie", "mei\u00