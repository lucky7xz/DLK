{"dta.poem.19622": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die spanischen Br\u00fcder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eda find' ich dich! In Wintergraus", "tokens": ["\u201e", "da", "find'", "ich", "dich", "!", "In", "Win\u00b7ter\u00b7graus"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00e4lt dich ein deutsches Donaunest,", "tokens": ["H\u00e4lt", "dich", "ein", "deut\u00b7sches", "Do\u00b7nau\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein schneebelastet Giebelhaus,", "tokens": ["Ein", "schnee\u00b7be\u00b7las\u00b7tet", "Gie\u00b7bel\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kind einer hei\u00dfen Sonne, fest.", "tokens": ["Kind", "ei\u00b7ner", "hei\u00b7\u00dfen", "Son\u00b7ne", ",", "fest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was treibst du hier? Mit toller Brunst", "tokens": ["Was", "treibst", "du", "hier", "?", "Mit", "tol\u00b7ler", "Brunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bohrst du dich in Folianten ein?", "tokens": ["Bohrst", "du", "dich", "in", "Fo\u00b7li\u00b7an\u00b7ten", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vom Teufel kommt die schwarze Kunst!", "tokens": ["Vom", "Teu\u00b7fel", "kommt", "die", "schwar\u00b7ze", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Griechisch? Die Kirche spricht Latein!", "tokens": ["Grie\u00b7chisch", "?", "Die", "Kir\u00b7che", "spricht", "La\u00b7tein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Dar\u00fcber sitzest, Nacht um Nacht,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "sit\u00b7zest", ",", "Nacht", "um", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du auf? Noch qualmt der Lampe Docht!", "tokens": ["Du", "auf", "?", "Noch", "qualmt", "der", "Lam\u00b7pe", "Docht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch siehst du bleich und \u00fcberwacht,", "tokens": ["Auch", "siehst", "du", "bleich", "und", "\u00fc\u00b7ber\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sonst so weidlich ritt und focht!", "tokens": ["Der", "sonst", "so", "weid\u00b7lich", "ritt", "und", "focht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du darbst? Du meidest jede Lust?", "tokens": ["Du", "darbst", "?", "Du", "mei\u00b7dest", "je\u00b7de", "Lust", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von allem Denken mach dich frei!", "tokens": ["Von", "al\u00b7lem", "Den\u00b7ken", "mach", "dich", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verbrenn' an einer warmen Brust,", "tokens": ["Ver\u00b7brenn'", "an", "ei\u00b7ner", "war\u00b7men", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ertr\u00e4nk' in Wein die Ketzerei!", "tokens": ["Er\u00b7tr\u00e4nk'", "in", "Wein", "die", "Ket\u00b7ze\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ergreife Schwert und Eisenhut!", "tokens": ["Er\u00b7grei\u00b7fe", "Schwert", "und", "Ei\u00b7sen\u00b7hut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Spanier ward die Welt zum Raub!", "tokens": ["Dem", "Spa\u00b7nier", "ward", "die", "Welt", "zum", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Flandern! Eh dein Edelblut", "tokens": ["Nach", "Flan\u00b7dern", "!", "Eh", "dein", "E\u00b7del\u00b7blut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Versiegt in ekelm B\u00fccherstaub!", "tokens": ["Ver\u00b7siegt", "in", "e\u00b7kelm", "B\u00fc\u00b7cher\u00b7staub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Bruder Juan, komm mit mir,", "tokens": ["Mein", "Bru\u00b7der", "Juan", ",", "komm", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Beflecke nicht der Diaz Ruhm!", "tokens": ["Be\u00b7fle\u00b7cke", "nicht", "der", "Diaz", "Ruhm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ers\u00e4uf' im Quadalquivir", "tokens": ["Er\u00b7s\u00e4uf'", "im", "Quad\u00b7al\u00b7qui\u00b7vir"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das gottverdammte Lutherthum!", "tokens": ["Das", "gott\u00b7ver\u00b7damm\u00b7te", "Lu\u00b7ther\u00b7thum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In Wittenberg hast du \u2014 absurd! \u2014", "tokens": ["In", "Wit\u00b7ten\u00b7berg", "hast", "du", "ab\u00b7surd", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "$(", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf einer Schule Bank gehockt!", "tokens": ["Auf", "ei\u00b7ner", "Schu\u00b7le", "Bank", "ge\u00b7hockt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei diesem Dolch an meinem Gurt,", "tokens": ["Bei", "die\u00b7sem", "Dolch", "an", "mei\u00b7nem", "Gurt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich morde den der dich verlockt!", "tokens": ["Ich", "mor\u00b7de", "den", "der", "dich", "ver\u00b7lockt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Vater ist ein alter Christ", "tokens": ["Der", "Va\u00b7ter", "ist", "ein", "al\u00b7ter", "Christ"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00e4he lieber dich im Grab!", "tokens": ["Und", "s\u00e4\u00b7he", "lie\u00b7ber", "dich", "im", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Mutter, welche gl\u00e4ubig ist \u2014", "tokens": ["Die", "Mut\u00b7ter", ",", "wel\u00b7che", "gl\u00e4u\u00b7big", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mutter dr\u00fcckst das Herz du ab!", "tokens": ["Der", "Mut\u00b7ter", "dr\u00fcckst", "das", "Herz", "du", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nie hat ein Diaz falsch geglaubt!", "tokens": ["Nie", "hat", "ein", "Diaz", "falsch", "ge\u00b7glaubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nicht wahr? ", "tokens": ["Nicht", "wahr", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Geliebter Bruder, theures Haupt!", "tokens": ["Ge\u00b7lieb\u00b7ter", "Bru\u00b7der", ",", "theu\u00b7res", "Haupt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich eilte deinen Schritten nach!", "tokens": ["Ich", "eil\u00b7te", "dei\u00b7nen", "Schrit\u00b7ten", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Juan, ich rei\u00dfe dich heraus", "tokens": ["Juan", ",", "ich", "rei\u00b7\u00dfe", "dich", "he\u00b7raus"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dieser meiner Arme Kraft!", "tokens": ["Mit", "die\u00b7ser", "mei\u00b7ner", "Ar\u00b7me", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rosse stampfen vor dem Haus,", "tokens": ["Die", "Ros\u00b7se", "stamp\u00b7fen", "vor", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gef\u00fchrt von meiner Dienerschaft.", "tokens": ["Ge\u00b7f\u00fchrt", "von", "mei\u00b7ner", "Die\u00b7ner\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du schweigst? Bekenn mir ob's geschah!", "tokens": ["Du", "schweigst", "?", "Be\u00b7kenn", "mir", "ob's", "ge\u00b7schah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juan, es mu\u00df geschieden sein!\u201c", "tokens": ["Juan", ",", "es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Eng h\u00e4lt den Bruder er umfa\u00dft,", "tokens": ["Eng", "h\u00e4lt", "den", "Bru\u00b7der", "er", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bang st\u00f6hnend senkt er Blick in Blick,", "tokens": ["Bang", "st\u00f6h\u00b7nend", "senkt", "er", "Blick", "in", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00fcsst, k\u00fcsst ihn noch einmal in Hast \u2014", "tokens": ["K\u00fcsst", ",", "k\u00fcsst", "ihn", "noch", "ein\u00b7mal", "in", "Hast"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NE", "$("], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und st\u00f6\u00dft den Dolch ihm durchs Genick.", "tokens": ["Und", "st\u00f6\u00dft", "den", "Dolch", "ihm", "durchs", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Er h\u00e4lt den Bruder lang im Arm,", "tokens": ["Er", "h\u00e4lt", "den", "Bru\u00b7der", "lang", "im", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit unersch\u00f6pften Thr\u00e4nen netzt", "tokens": ["Mit", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Thr\u00e4\u00b7nen", "netzt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und badet er den Todten warm:", "tokens": ["Und", "ba\u00b7det", "er", "den", "Tod\u00b7ten", "warm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201enoch starbest als ein Christ du jetzt!\u201c", "tokens": ["\u201e", "noch", "star\u00b7best", "als", "ein", "Christ", "du", "jetzt", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "KOKOM", "ART", "NN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}