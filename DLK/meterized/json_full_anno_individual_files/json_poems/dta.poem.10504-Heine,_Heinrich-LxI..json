{"dta.poem.10504": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt', meine Schmerzen erg\u00f6ssen", "tokens": ["Ich", "wollt'", ",", "mei\u00b7ne", "Schmer\u00b7zen", "er\u00b7g\u00f6s\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sich all' in ein einziges Wort,", "tokens": ["Sich", "all'", "in", "ein", "ein\u00b7zi\u00b7ges", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das g\u00e4b' ich den lustigen Winden,", "tokens": ["Das", "g\u00e4b'", "ich", "den", "lus\u00b7ti\u00b7gen", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die tr\u00fcgen es lustig fort.", "tokens": ["Die", "tr\u00fc\u00b7gen", "es", "lus\u00b7tig", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie tragen zu dir, Geliebte,", "tokens": ["Sie", "tra\u00b7gen", "zu", "dir", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das schmerzerf\u00fcllte Wort;", "tokens": ["Das", "schmer\u00b7zer\u00b7f\u00fcll\u00b7te", "Wort", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du h\u00f6rst es zu jeder Stunde,", "tokens": ["Du", "h\u00f6rst", "es", "zu", "je\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du h\u00f6rst es an jedem Ort.", "tokens": ["Du", "h\u00f6rst", "es", "an", "je\u00b7dem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und hast du zum n\u00e4chtlichen Schlummer", "tokens": ["Und", "hast", "du", "zum", "n\u00e4cht\u00b7li\u00b7chen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Geschlossen die Augen kaum,", "tokens": ["Ge\u00b7schlos\u00b7sen", "die", "Au\u00b7gen", "kaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So wird dich mein Auge verfolgen", "tokens": ["So", "wird", "dich", "mein", "Au\u00b7ge", "ver\u00b7fol\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis in den tiefsten Traum.", "tokens": ["Bis", "in", "den", "tiefs\u00b7ten", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}