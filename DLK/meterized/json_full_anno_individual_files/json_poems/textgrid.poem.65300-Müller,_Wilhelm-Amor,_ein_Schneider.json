{"textgrid.poem.65300": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Schneider", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor ist ein Schneider worden,", "tokens": ["A\u00b7mor", "ist", "ein", "Schnei\u00b7der", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4ht die ersten runden Mieder", "tokens": ["N\u00e4ht", "die", "ers\u00b7ten", "run\u00b7den", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die jungen Erdent\u00f6chter,", "tokens": ["F\u00fcr", "die", "jun\u00b7gen", "Er\u00b7den\u00b7t\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4ht hinein viel kleine Seufzer,", "tokens": ["N\u00e4ht", "hin\u00b7ein", "viel", "klei\u00b7ne", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viele leise, bl\u00f6de W\u00fcnsche,", "tokens": ["Vie\u00b7le", "lei\u00b7se", ",", "bl\u00f6\u00b7de", "W\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bange Neugier, scheue L\u00fcstchen,", "tokens": ["Ban\u00b7ge", "Neu\u00b7gier", ",", "scheu\u00b7e", "L\u00fcst\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und viel s\u00fc\u00dfes Namenloses.", "tokens": ["Und", "viel", "s\u00fc\u00b7\u00dfes", "Na\u00b7men\u00b7lo\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Manche Nadel bleibt zerbrochen", "tokens": ["Man\u00b7che", "Na\u00b7del", "bleibt", "zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwischen Zeug und Futter sitzen,", "tokens": ["Zwi\u00b7schen", "Zeug", "und", "Fut\u00b7ter", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Die nachher den Busen stachelt", "tokens": ["Die", "nach\u00b7her", "den", "Bu\u00b7sen", "sta\u00b7chelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und das Herz lebendig kitzelt.", "tokens": ["Und", "das", "Herz", "le\u00b7ben\u00b7dig", "kit\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auch manch Tr\u00f6pfchen seines Blutes", "tokens": ["Auch", "manch", "Tr\u00f6pf\u00b7chen", "sei\u00b7nes", "Blu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "L\u00e4\u00dft der Gott aus Nadelwunden", "tokens": ["L\u00e4\u00dft", "der", "Gott", "aus", "Na\u00b7del\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "In das weiche Linnen fallen.", "tokens": ["In", "das", "wei\u00b7che", "Lin\u00b7nen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "H\u00fctet euch vor solcher Waare!", "tokens": ["H\u00fc\u00b7tet", "euch", "vor", "sol\u00b7cher", "Waa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Denn die rothen Tropfen brennen,", "tokens": ["Denn", "die", "ro\u00b7then", "Trop\u00b7fen", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Unaufhaltsam, unerl\u00f6schlich,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "un\u00b7er\u00b7l\u00f6\u00b7schlich", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sich durch Adern, Fleisch und Nerven", "tokens": ["Sich", "durch", "A\u00b7dern", ",", "Fleisch", "und", "Ner\u00b7ven"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Bis in's tiefste Herzensgr\u00fcbchen.", "tokens": ["Bis", "in's", "tiefs\u00b7te", "Her\u00b7zens\u00b7gr\u00fcb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter":