{"dta.poem.6238": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir m\u00f6gen scharffen Stahl, wir m\u00f6gen Feur empfinden,", "tokens": ["Wir", "m\u00f6\u00b7gen", "scharf\u00b7fen", "Stahl", ",", "wir", "m\u00f6\u00b7gen", "Feur", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "$,", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird die W\u00fcrckung uns, darauf zu sehn, verbinden,", "tokens": ["So", "wird", "die", "W\u00fcr\u00b7ckung", "uns", ",", "da\u00b7rauf", "zu", "sehn", ",", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "$,", "PAV", "PTKZU", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf welche Weis und Art dies brennt und jenes sticht.", "tokens": ["Auf", "wel\u00b7che", "Weis", "und", "Art", "dies", "brennt", "und", "je\u00b7nes", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PDS", "VVFIN", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir sprechen hier von unsrer Seele nicht.", "tokens": ["Wir", "spre\u00b7chen", "hier", "von", "uns\u00b7rer", "See\u00b7le", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wil davon zu andrer Zeit gedencken;", "tokens": ["Ich", "wil", "da\u00b7von", "zu", "an\u00b7drer", "Zeit", "ge\u00b7den\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PTKZU", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu Anfangs m\u00fcssen wir uns zu den C\u00f6rpern lencken.", "tokens": ["Zu", "An\u00b7fangs", "m\u00fcs\u00b7sen", "wir", "uns", "zu", "den", "C\u00f6r\u00b7pern", "len\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}