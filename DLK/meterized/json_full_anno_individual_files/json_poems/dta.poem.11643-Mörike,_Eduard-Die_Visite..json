{"dta.poem.11643": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Visite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.73", "da:0.14", "no:0.12"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Philister kommen angezogen:", "tokens": ["Phi\u00b7lis\u00b7ter", "kom\u00b7men", "an\u00b7ge\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sucht im Garten mich und Haus;", "tokens": ["Man", "sucht", "im", "Gar\u00b7ten", "mich", "und", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch war der Vogel ausgeflogen", "tokens": ["Doch", "war", "der", "Vo\u00b7gel", "aus\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum vielgeliebten Wald hinaus.", "tokens": ["Zum", "viel\u00b7ge\u00b7lieb\u00b7ten", "Wald", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, mich auch da zu st\u00f6ren;", "tokens": ["Sie", "kom\u00b7men", ",", "mich", "auch", "da", "zu", "st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schon hei\u00dft es: Horcht! die Nachtigall!", "tokens": ["Schon", "hei\u00dft", "es", ":", "Horcht", "!", "die", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u2014 Gleich lass' ich mich als Gukuk h\u00f6ren,", "tokens": ["Gleich", "lass'", "ich", "mich", "als", "Gu\u00b7kuk", "h\u00f6\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PRF", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Bin nirgends und bin \u00fcberall.", "tokens": ["Bin", "nir\u00b7gends", "und", "bin", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So f\u00fchrt' ich sie, nur wie im Traume,", "tokens": ["So", "f\u00fchrt'", "ich", "sie", ",", "nur", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Puck im ganzen Wald herum;", "tokens": ["Als", "Puck", "im", "gan\u00b7zen", "Wald", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich pfiff und sang von jedem Baume,", "tokens": ["Ich", "pfiff", "und", "sang", "von", "je\u00b7dem", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sahn sich fast die H\u00e4lse krumm.", "tokens": ["Sie", "sahn", "sich", "fast", "die", "H\u00e4l\u00b7se", "krumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schalten sie: Verfluchte Possen!", "tokens": ["Nun", "schal\u00b7ten", "sie", ":", "Ver\u00b7fluch\u00b7te", "Pos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sonderling, der Grobian!", "tokens": ["Der", "Son\u00b7der\u00b7ling", ",", "der", "Gro\u00b7bi\u00b7an", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da komm' ich grunzend angeschossen,", "tokens": ["Da", "komm'", "ich", "grun\u00b7zend", "an\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Eber, mit gefletschtem Zahn.", "tokens": ["Ein", "E\u00b7ber", ",", "mit", "ge\u00b7fletschtem", "Zahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Mit Schrei'n, als wenn der Boden brennte,", "tokens": ["Mit", "Schrei'n", ",", "als", "wenn", "der", "Bo\u00b7den", "brenn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerstob ein Theil im wilden Lauf,", "tokens": ["Zer\u00b7stob", "ein", "Theil", "im", "wil\u00b7den", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Andern kletterten behende", "tokens": ["Die", "An\u00b7dern", "klet\u00b7ter\u00b7ten", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den n\u00e4chsten besten Baum hinauf;", "tokens": ["Den", "n\u00e4chs\u00b7ten", "bes\u00b7ten", "Baum", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie krochen weislich bis zum Wipfel,", "tokens": ["Sie", "kro\u00b7chen", "weis\u00b7lich", "bis", "zum", "Wip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sahen nicht einmal zur\u00fcck,", "tokens": ["Und", "sa\u00b7hen", "nicht", "ein\u00b7mal", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ich als Eichhorn sa\u00df im Gipfel:", "tokens": ["Doch", "ich", "als", "Eich\u00b7horn", "sa\u00df", "im", "Gip\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich gr\u00fc\u00dfe sie und w\u00fcnsche Gl\u00fcck.", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "sie", "und", "w\u00fcn\u00b7sche", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eei, welch ein allerliebstes Sp\u00e4\u00dfchen!", "tokens": ["\u201e", "ei", ",", "welch", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "Sp\u00e4\u00df\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gott gr\u00fc\u00df' euch, sch\u00f6ne Fraun und Herrn!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", ",", "sch\u00f6\u00b7ne", "Fraun", "und", "Herrn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kommen, hoff' ich, auf ein T\u00e4\u00dfchen", "tokens": ["Sie", "kom\u00b7men", ",", "hoff'", "ich", ",", "auf", "ein", "T\u00e4\u00df\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eichel-Kaffee? Von Herzen gern!\u201c", "tokens": ["Ei\u00b7chel\u00b7Kaf\u00b7fee", "?", "Von", "Her\u00b7zen", "gern", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "APPR", "NN", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "\u2014 Allein sie fanden's nicht gem\u00fcthlich", "tokens": ["Al\u00b7lein", "sie", "fan\u00b7den's", "nicht", "ge\u00b7m\u00fcth\u00b7lich"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PTKNEG", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In dieser ungewohnten H\u00f6h'.", "tokens": ["In", "die\u00b7ser", "un\u00b7ge\u00b7wohn\u00b7ten", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So schieden wir f\u00fcr heute g\u00fctlich;", "tokens": ["So", "schie\u00b7den", "wir", "f\u00fcr", "heu\u00b7te", "g\u00fct\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch wehe meiner Renomm", "tokens": ["Doch", "we\u00b7he", "mei\u00b7ner", "Re\u00b7nomm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}