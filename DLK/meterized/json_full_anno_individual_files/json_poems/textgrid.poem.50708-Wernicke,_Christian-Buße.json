{"textgrid.poem.50708": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fand sein zartes Weib ein Ehmann in Gefahr", "tokens": ["Es", "fand", "sein", "zar\u00b7tes", "Weib", "ein", "Eh\u00b7mann", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wollte, weil es so zu Rom gebr\u00e4uchlich war,", "tokens": ["Und", "woll\u00b7te", ",", "weil", "es", "so", "zu", "Rom", "ge\u00b7br\u00e4uch\u00b7lich", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "NE", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus gro\u00dfer Liebe sich bequemen,", "tokens": ["Aus", "gro\u00b7\u00dfer", "Lie\u00b7be", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ruthenstreich' ihr abzunehmen,", "tokens": ["Die", "Ru\u00b7then\u00b7streich'", "ihr", "ab\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die in der Beicht' ein M\u00f6nch ihr auferlegt.", "tokens": ["Die", "in", "der", "Beicht'", "ein", "M\u00f6nch", "ihr", "auf\u00b7er\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "w