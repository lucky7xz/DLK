{"textgrid.poem.54028": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Das Nachtgespenst", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Tage bin ich ein gewisser Planke", "tokens": ["Am", "Ta\u00b7ge", "bin", "ich", "ein", "ge\u00b7wis\u00b7ser", "Plan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nur: Planke, Erich . . . Breite Stra\u00dfe zehn.", "tokens": ["nur", ":", "Plan\u00b7ke", ",", "E\u00b7rich", ".", ".", ".", "Brei\u00b7te", "Stra\u00b7\u00dfe", "zehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "$,", "NE", "$.", "$.", "$.", "ADJA", "NN", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man merkt mir gar nichts an. Und kein Gedanke", "tokens": ["Man", "merkt", "mir", "gar", "nichts", "an", ".", "Und", "kein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$.", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "l\u00e4\u00dft auf der Stra\u00dfe jemand nach mir sehn.", "tokens": ["l\u00e4\u00dft", "auf", "der", "Stra\u00b7\u00dfe", "je\u00b7mand", "nach", "mir", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch abends, wenn die Bogenlampen zischen,", "tokens": ["Doch", "a\u00b7bends", ",", "wenn", "die", "Bo\u00b7gen\u00b7lam\u00b7pen", "zi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dann geh ich los und spiele still Versteck.", "tokens": ["dann", "geh", "ich", "los", "und", "spie\u00b7le", "still", "Ver\u00b7steck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Rein ins Parterre . . . ich kann so rasch entwischen . . .", "tokens": ["Rein", "ins", "Par\u00b7ter\u00b7re", ".", ".", ".", "ich", "kann", "so", "rasch", "ent\u00b7wi\u00b7schen", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "$.", "$.", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "den Frauen zieh ich gern die Decke weg \u2013", "tokens": ["den", "Frau\u00b7en", "zieh", "ich", "gern", "die", "De\u00b7cke", "weg", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erschrecken Sie nicht!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Erschrecken Sie doch!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "doch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Erschrecken Sie nicht!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Erschrecken Sie doch!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "doch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ich bin schon lange weg, wenn du um Hilfe rennst:", "tokens": ["Ich", "bin", "schon", "lan\u00b7ge", "weg", ",", "wenn", "du", "um", "Hil\u00b7fe", "rennst", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich bin das Nachtgespenst!", "tokens": ["Ich", "bin", "das", "Nacht\u00b7ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ich bin das Nachtgespenst!", "tokens": ["Ich", "bin", "das", "Nacht\u00b7ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "\u00bbwer ist da? Mach mal Licht! Mama \u2013!\u00ab", "tokens": ["\u00bb", "wer", "ist", "da", "?", "Mach", "mal", "Licht", "!", "Ma\u00b7ma", "\u2013", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "$.", "VVFIN", "ADV", "NN", "$.", "NN", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich stehle nichts. Ich tappe durch die R\u00e4ume.", "tokens": ["Ich", "steh\u00b7le", "nichts", ".", "Ich", "tap\u00b7pe", "durch", "die", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Na ja, ein Taschenbuch . . . was man so findt . . .", "tokens": ["Na", "ja", ",", "ein", "Ta\u00b7schen\u00b7buch", ".", ".", ".", "was", "man", "so", "findt", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "ADV", "$,", "ART", "NN", "$.", "$.", "$.", "PWS", "PIS", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das sind so alte, tiefe Kindertr\u00e4ume:", "tokens": ["Das", "sind", "so", "al\u00b7te", ",", "tie\u00b7fe", "Kin\u00b7der\u00b7tr\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie wohl die fremden Menschen nackend sind?", "tokens": ["wie", "wohl", "die", "frem\u00b7den", "Men\u00b7schen", "na\u00b7ckend", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie liegen warm gekuschelt wie die Katzen . . .", "tokens": ["Sie", "lie\u00b7gen", "warm", "ge\u00b7ku\u00b7schelt", "wie", "die", "Kat\u00b7zen", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVPP", "KOKOM", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die eine Hand am Bein . . . die Decke kraus . . .", "tokens": ["die", "ei\u00b7ne", "Hand", "am", "Bein", ".", ".", ".", "die", "De\u00b7cke", "kraus", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "$.", "$.", "$.", "ART", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich h\u00f6r sie noch vor Angst am Schalter kratzen \u2013", "tokens": ["Ich", "h\u00f6r", "sie", "noch", "vor", "Angst", "am", "Schal\u00b7ter", "krat\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ein Sprung \u2013 tiefatmend steh ich vor dem Haus \u2013", "tokens": ["ein", "Sprung", "\u2013", "tie\u00b7fat\u00b7mend", "steh", "ich", "vor", "dem", "Haus", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erschrecken Sie nicht!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Erschrecken Sie doch!", "tokens": ["Er\u00b7schre\u00b7cken", "Sie", "doch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Ich bin schon lange weg, wenn du im Halbschlaf pennst . . .", "tokens": ["Ich", "bin", "schon", "lan\u00b7ge", "weg", ",", "wenn", "du", "im", "Halb\u00b7schlaf", "pennst", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich bin das Nachtgespenst!", "tokens": ["Ich", "bin", "das", "Nacht\u00b7ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ich bin das Nachtgespenst!", "tokens": ["Ich", "bin", "das", "Nacht\u00b7ge\u00b7spenst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "\u00bbwilli! Einbrecher! Williiii . . .!\u00ab", "tokens": ["\u00bb", "wil\u00b7li", "!", "Ein\u00b7bre\u00b7cher", "!", "Wil\u00b7li\u00b7i\u00b7ii", ".", ".", ".", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "FM", "$.", "NN", "$.", "NE", "$.", "$.", "$.", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Mir ist das Sport, Romantik ist so selten.", "tokens": ["Mir", "ist", "das", "Sport", ",", "Ro\u00b7man\u00b7tik", "ist", "so", "sel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Mittag drauf steh ich in der ", "tokens": ["Am", "Mit\u00b7tag", "drauf", "steh", "ich", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PAV", "VVFIN", "PPER", "APPR", "ART"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Ich bin ein Expedient. Man will doch auch was gelten.", "tokens": ["Ich", "bin", "ein", "Ex\u00b7pe\u00b7di\u00b7ent", ".", "Man", "will", "doch", "auch", "was", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PIS", "VMFIN", "ADV", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den ganzen Tag riech ich das fremde Bett . . .", "tokens": ["Den", "gan\u00b7zen", "Tag", "riech", "ich", "das", "frem\u00b7de", "Bett", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Angst rieselt auf mich zu. Ich bin ein Kaiser,", "tokens": ["Angst", "rie\u00b7selt", "auf", "mich", "zu", ".", "Ich", "bin", "ein", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ein F\u00fcrst der Nacht! Ich schmecke ihren Schrei . . .", "tokens": ["ein", "F\u00fcrst", "der", "Nacht", "!", "Ich", "schme\u00b7cke", "ih\u00b7ren", "Schrei", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und nachher br\u00fcllen sie vor Furcht sich heiser.", "tokens": ["Und", "nach\u00b7her", "br\u00fcl\u00b7len", "sie", "vor", "F