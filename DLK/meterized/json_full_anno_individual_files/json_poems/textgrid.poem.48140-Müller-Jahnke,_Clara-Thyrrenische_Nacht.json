{"textgrid.poem.48140": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Thyrrenische Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcsse mich, du! Der Himmel bl\u00fcht", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "du", "!", "Der", "Him\u00b7mel", "bl\u00fcht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "$.", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "wie lauter Granaten und Rosen.", "tokens": ["wie", "lau\u00b7ter", "Gra\u00b7na\u00b7ten", "und", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Flugfeuer von Lippe zu Lippe spr\u00fcht,", "tokens": ["Flug\u00b7feu\u00b7er", "von", "Lip\u00b7pe", "zu", "Lip\u00b7pe", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "NE", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und der Berg der ewigen Gluten gl\u00fcht \u2013", "tokens": ["und", "der", "Berg", "der", "e\u00b7wi\u00b7gen", "Glu\u00b7ten", "gl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die Tiefen kochen und tosen.", "tokens": ["die", "Tie\u00b7fen", "ko\u00b7chen", "und", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.r