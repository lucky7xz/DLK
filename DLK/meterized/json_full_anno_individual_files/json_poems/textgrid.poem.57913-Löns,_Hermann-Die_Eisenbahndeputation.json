{"textgrid.poem.57913": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Eisenbahndeputation", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Berlin, nach Berlin, fahr' nicht nach Berlin,", "tokens": ["Nach", "Ber\u00b7lin", ",", "nach", "Ber\u00b7lin", ",", "fahr'", "nicht", "nach", "Ber\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,", "VVFIN", "PTKNEG", "APPR", "NE", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mein Sohn, ich rate dir gut,", "tokens": ["Mein", "Sohn", ",", "ich", "ra\u00b7te", "dir", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wenn du mit einem Minister dort sprichst,", "tokens": ["Und", "wenn", "du", "mit", "ei\u00b7nem", "Mi\u00b7nis\u00b7ter", "dort", "sprichst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mein Sohon, so sei auf der Hut.", "tokens": ["Mein", "So\u00b7hon", ",", "so", "sei", "auf", "der", "Hut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ein Minister, der ist schon auf alles geeicht,", "tokens": ["Ein", "Mi\u00b7nis\u00b7ter", ",", "der", "ist", "schon", "auf", "al\u00b7les", "ge\u00b7eicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "APPR", "PIS", "VVPP", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und redet die Kreuz und die Quer,", "tokens": ["Und", "re\u00b7det", "die", "Kreuz", "und", "die", "Quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du denkst dann, es ist was, und wenn du's besiehst,", "tokens": ["Du", "denkst", "dann", ",", "es", "ist", "was", ",", "und", "wenn", "du's", "be\u00b7siehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PIS", "$,", "KON", "KOUS", "PIS", "VVFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Dann bist du so klug wie vorher.", "tokens": ["Dann", "bist", "du", "so", "klug", "wie", "vor\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Von der Eisenbahn gehst zur Finanz du dann hin,", "tokens": ["Von", "der", "Ei\u00b7sen\u00b7bahn", "gehst", "zur", "Fi\u00b7nanz", "du", "dann", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPRART", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sehr freundlich empf\u00e4ngt man dich dort,", "tokens": ["Sehr", "freund\u00b7lich", "emp\u00b7f\u00e4ngt", "man", "dich", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "PRF", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Man sagt dir daselbst, was du lange schon wei\u00dft,", "tokens": ["Man", "sagt", "dir", "da\u00b7selbst", ",", "was", "du", "lan\u00b7ge", "schon", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und freudig verl\u00e4\u00dft du den Ort.", "tokens": ["Und", "freu\u00b7dig", "ver\u00b7l\u00e4\u00dft", "du", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Aber bist du dann drau\u00dfen, dann wei\u00dft du nicht hin", "tokens": ["A\u00b7ber", "bist", "du", "dann", "drau\u00b7\u00dfen", ",", "dann", "wei\u00dft", "du", "nicht", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKVZ"], "meter": "+-+--+-+++-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Noch her, nicht ein und noch aus,", "tokens": ["Noch", "her", ",", "nicht", "ein", "und", "noch", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PTKNEG", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann bist du so klug und so dumm wie vorher", "tokens": ["Dann", "bist", "du", "so", "klug", "und", "so", "dumm", "wie", "vor\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wor