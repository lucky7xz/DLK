{"dta.poem.13484": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Lied, mit Thr\u00e4nen halb geschrieben,", "tokens": ["Lied", ",", "mit", "Thr\u00e4\u00b7nen", "halb", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dorthin \u00fcber Berg und Kluft,", "tokens": ["Dor\u00b7thin", "\u00fc\u00b7ber", "Berg", "und", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wo die Liebste mein geblieben,", "tokens": ["Wo", "die", "Liebs\u00b7te", "mein", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwing' dich durch die blaue Luft!", "tokens": ["Schwing'", "dich", "durch", "die", "blau\u00b7e", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist sie roth und lustig, sage:", "tokens": ["Ist", "sie", "roth", "und", "lus\u00b7tig", ",", "sa\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich sey krank von Herzensgrund;", "tokens": ["Ich", "sey", "krank", "von", "Her\u00b7zens\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weint sie Nachts, sinnt still bei Tage,", "tokens": ["Weint", "sie", "Nachts", ",", "sinnt", "still", "bei", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja dann sag: ich sey gesund!", "tokens": ["Ja", "dann", "sag", ":", "ich", "sey", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist vorbei ihr treues Lieben,", "tokens": ["Ist", "vor\u00b7bei", "ihr", "treu\u00b7es", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun, so end' auch Lust und Noth,", "tokens": ["Nun", ",", "so", "end'", "auch", "Lust", "und", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu allen, die mich lieben,", "tokens": ["Und", "zu", "al\u00b7len", ",", "die", "mich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliege, sage: ich sey todt!", "tokens": ["Flie\u00b7ge", ",", "sa\u00b7ge", ":", "ich", "sey", "todt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}