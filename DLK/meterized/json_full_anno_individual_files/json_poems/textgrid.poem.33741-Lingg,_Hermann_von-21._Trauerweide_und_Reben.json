{"textgrid.poem.33741": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "21. Trauerweide und Reben", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Heimat hatte mich beschenkt mit Reben,", "tokens": ["Die", "Hei\u00b7mat", "hat\u00b7te", "mich", "be\u00b7schenkt", "mit", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die pflanzt' ich ein an meine Gartenmauer", "tokens": ["Die", "pflanzt'", "ich", "ein", "an", "mei\u00b7ne", "Gar\u00b7ten\u00b7mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bat den Himmel, ihnen Schutz vor Schauer", "tokens": ["Und", "bat", "den", "Him\u00b7mel", ",", "ih\u00b7nen", "Schutz", "vor", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PPER", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ihrer Bl\u00fcte Sonnenschein zu geben.", "tokens": ["Und", "ih\u00b7rer", "Bl\u00fc\u00b7te", "Son\u00b7nen\u00b7schein", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da stie\u00df ich mit der Schaufel hart daneben", "tokens": ["Da", "stie\u00df", "ich", "mit", "der", "Schau\u00b7fel", "hart", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "PAV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Wurzeln eines Baums von trotz'ger Dauer.", "tokens": ["Auf", "Wur\u00b7zeln", "ei\u00b7nes", "Baums", "von", "trotz'\u00b7ger", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Tr\u00e4nenweide, du bist's, Bild der Trauer?", "tokens": ["O", "Tr\u00e4\u00b7nen\u00b7wei\u00b7de", ",", "du", "bist's", ",", "Bild", "der", "Trau\u00b7er", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NE", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll ich dich dulden hier, den Tod beim Leben?", "tokens": ["Soll", "ich", "dich", "dul\u00b7den", "hier", ",", "den", "Tod", "beim", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Umwinde nur, ich mu\u00df es dir gestatten,", "tokens": ["Um\u00b7win\u00b7de", "nur", ",", "ich", "mu\u00df", "es", "dir", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wurzeln, denen Lust entsprie\u00dft, mit deinen,", "tokens": ["Die", "Wur\u00b7zeln", ",", "de\u00b7nen", "Lust", "ent\u00b7sprie\u00dft", ",", "mit", "dei\u00b7nen", ","], "token_info": ["word", "word", "punct