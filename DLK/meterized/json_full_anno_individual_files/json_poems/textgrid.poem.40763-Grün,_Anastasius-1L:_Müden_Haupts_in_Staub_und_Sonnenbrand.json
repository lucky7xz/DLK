{"textgrid.poem.40763": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00fcden Haupts in Staub und Sonnenbrand", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcden Haupts in Staub und Sonnenbrand", "tokens": ["M\u00fc\u00b7den", "Haupts", "in", "Staub", "und", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schleicht des Wegs der Bruder Terminant,", "tokens": ["Schleicht", "des", "Wegs", "der", "Bru\u00b7der", "Ter\u00b7mi\u00b7nant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hlt zur Mittagsrast den Meilenstein,", "tokens": ["W\u00e4hlt", "zur", "Mit\u00b7tags\u00b7rast", "den", "Mei\u00b7len\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wischt vom Schwei\u00df die blanke Glatze rein.", "tokens": ["Wischt", "vom", "Schwei\u00df", "die", "blan\u00b7ke", "Glat\u00b7ze", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bettelfahrt ist auch der Demut schwer,", "tokens": ["Bet\u00b7tel\u00b7fahrt", "ist", "auch", "der", "De\u00b7mut", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schwerste Last ein Bettelsack, der leer!", "tokens": ["Schwers\u00b7te", "Last", "ein", "Bet\u00b7tel\u00b7sack", ",", "der", "leer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tr\u00e4umend blickt der M\u00f6nch zum Zwillichschlauch.", "tokens": ["Tr\u00e4u\u00b7mend", "blickt", "der", "M\u00f6nch", "zum", "Zwil\u00b7lich\u00b7schlauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbalter, was verschlang nicht schon dein Bauch!", "tokens": ["\u00bb", "al\u00b7ter", ",", "was", "ver\u00b7schlang", "nicht", "schon", "dein", "Bauch", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "PWS", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Zogst um K\u00f6rnlein Weizens einst nur aus", "tokens": ["Zogst", "um", "K\u00f6rn\u00b7lein", "Wei\u00b7zens", "einst", "nur", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN", "ADV", "ADV", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr des Herren Leib im Gotteshaus;", "tokens": ["F\u00fcr", "des", "Her\u00b7ren", "Leib", "im", "Got\u00b7tes\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch es fiel davon so reichlich ab,", "tokens": ["Doch", "es", "fiel", "da\u00b7von", "so", "reich\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df auch unserm Leib es Rundung gab.", "tokens": ["Da\u00df", "auch", "un\u00b7serm", "Leib", "es", "Run\u00b7dung", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Batest einst nur um ein K\u00e4nnlein Wein,", "tokens": ["Ba\u00b7test", "einst", "nur", "um", "ein", "K\u00e4nn\u00b7lein", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Opfernd ihn zum Blut des Herrn zu weihn;", "tokens": ["Op\u00b7fernd", "ihn", "zum", "Blut", "des", "Herrn", "zu", "weihn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Krug und Fa\u00df auch f\u00fcllt das s\u00fc\u00dfe Blut,", "tokens": ["Krug", "und", "Fa\u00df", "auch", "f\u00fcllt", "das", "s\u00fc\u00b7\u00dfe", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00e4rbt das Antlitz sch\u00f6n in Rosengluth.", "tokens": ["F\u00e4rbt", "das", "Ant\u00b7litz", "sch\u00f6n", "in", "Ro\u00b7sen\u00b7gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "F\u00fcr das Altartuch ein B\u00fcschlein Flachs,", "tokens": ["F\u00fcr", "das", "Al\u00b7tar\u00b7tuch", "ein", "B\u00fc\u00b7schlein", "Flachs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr die Kerzen nur ein St\u00fcmpfchen Wachs!", "tokens": ["F\u00fcr", "die", "Ker\u00b7zen", "nur", "ein", "St\u00fcmpf\u00b7chen", "Wachs", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lein doch kleidet nicht die Mensa blos,", "tokens": ["Lein", "doch", "klei\u00b7det", "nicht", "die", "Men\u00b7sa", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den Waben auch viel Honig flo\u00df.", "tokens": ["Aus", "den", "Wa\u00b7ben", "auch", "viel", "Ho\u00b7nig", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Suchtest f\u00fcr die h\u00e4r'ne Kutte nur", "tokens": ["Such\u00b7test", "f\u00fcr", "die", "h\u00e4r'\u00b7ne", "Kut\u00b7te", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gr\u00f6bsten Abfall bei der Wollenschur;", "tokens": ["Gr\u00f6bs\u00b7ten", "Ab\u00b7fall", "bei", "der", "Wol\u00b7len\u00b7schur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch sie ma\u00dfen uns so reiches Ma\u00df,", "tokens": ["Doch", "sie", "ma\u00b7\u00dfen", "uns", "so", "rei\u00b7ches", "Ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df gar warm sich's in der Wolle sa\u00df.", "tokens": ["Da\u00df", "gar", "warm", "sich's", "in", "der", "Wol\u00b7le", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "F\u00fcr Sandalen nur den Lederstreif!", "tokens": ["F\u00fcr", "San\u00b7da\u00b7len", "nur", "den", "Le\u00b7der\u00b7streif", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Doch der dehnte sich zum weiten Reif,", "tokens": ["Doch", "der", "dehn\u00b7te", "sich", "zum", "wei\u00b7ten", "Reif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wie einst Dido's Riem, der rings das Land,", "tokens": ["Wie", "einst", "Di\u00b7do's", "Riem", ",", "der", "rings", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "NE", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Forst und Acker, Teich und Trift umspannt.", "tokens": ["Forst", "und", "A\u00b7cker", ",", "Teich", "und", "Trift", "um\u00b7spannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Leerten einst die Br\u00fcder dich zum Grund,", "tokens": ["Leer\u00b7ten", "einst", "die", "Br\u00fc\u00b7der", "dich", "zum", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfes Wunder, zauberhafter Fund:", "tokens": ["S\u00fc\u00b7\u00dfes", "Wun\u00b7der", ",", "zau\u00b7ber\u00b7haf\u00b7ter", "Fund", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Seid' und Sammt, Geschmeid und Prachtgewand,", "tokens": ["Seid'", "und", "Sammt", ",", "Ge\u00b7schmeid", "und", "Pracht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stab und Ring f\u00fcr die Pr\u00e4latenhand!", "tokens": ["Stab", "und", "Ring", "f\u00fcr", "die", "Pr\u00e4\u00b7la\u00b7ten\u00b7hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Gold und Silber, Schmelz und Edelstein,", "tokens": ["Gold", "und", "Sil\u00b7ber", ",", "Schmelz", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Burgen, G\u00fclten, M\u00fcnster und Abtei'n,", "tokens": ["Bur\u00b7gen", ",", "G\u00fcl\u00b7ten", ",", "M\u00fcns\u00b7ter", "und", "Ab\u00b7tei'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Himmelsgnaden, Erdenherrlichkeit", "tokens": ["Him\u00b7mels\u00b7gna\u00b7den", ",", "Er\u00b7den\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00fctten sie aus deinen Falten weit! \u2013", "tokens": ["Sch\u00fct\u00b7ten", "sie", "aus", "dei\u00b7nen", "Fal\u00b7ten", "weit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Um des M\u00f6nches Haupt, wie Sonnenlicht", "tokens": ["Um", "des", "M\u00f6n\u00b7ches", "Haupt", ",", "wie", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leuchtend, flie\u00dft das holde Traumgesicht,", "tokens": ["Leuch\u00b7tend", ",", "flie\u00dft", "das", "hol\u00b7de", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "R\u00fcstig nach dem Stabe greift die Hand,", "tokens": ["R\u00fcs\u00b7tig", "nach", "dem", "Sta\u00b7be", "greift", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hoch erhobnen Haupts blickt er ins Land.", "tokens": ["Hoch", "er\u00b7hob\u00b7nen", "Haupts", "blickt", "er", "ins", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.11": {"line.1": {"text": "Um die Schultern seinen Sack gelegt,", "tokens": ["Um", "die", "Schul\u00b7tern", "sei\u00b7nen", "Sack", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ha, wie stolz er jetzt den leeren tr\u00e4gt!", "tokens": ["Ha", ",", "wie", "stolz", "er", "jetzt", "den", "lee\u00b7ren", "tr\u00e4gt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schlo\u00df er ja den ganzen Erdball ein,", "tokens": ["Schlo\u00df", "er", "ja", "den", "gan\u00b7zen", "Erd\u00b7ball", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Sternenhimmel obendrein!", "tokens": ["Und", "den", "Ster\u00b7nen\u00b7him\u00b7mel", "o\u00b7ben\u00b7drein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Milder Traumgott, die geschenkt du hast,", "tokens": ["Mil\u00b7der", "Traum\u00b7gott", ",", "die", "ge\u00b7schenkt", "du", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hilf sie tragen auch, die schwerste Last,", "tokens": ["Hilf", "sie", "tra\u00b7gen", "auch", ",", "die", "schwers\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die B\u00fcrde leicht und sanft ihm sei", "tokens": ["Da\u00df", "die", "B\u00fcr\u00b7de", "leicht", "und", "sanft", "ihm", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "VVFIN", "PPER", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie einst jenen, deren Tag vorbei.\u00ab", "tokens": ["Wie", "einst", "je\u00b7nen", ",", "de\u00b7ren", "Tag", "vor\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PDS", "$,", "PRELAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "M\u00fcden Haupts in Staub und Sonnenbrand", "tokens": ["M\u00fc\u00b7den", "Haupts", "in", "Staub", "und", "Son\u00b7nen\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schleicht des Wegs der Bruder Terminant,", "tokens": ["Schleicht", "des", "Wegs", "der", "Bru\u00b7der", "Ter\u00b7mi\u0