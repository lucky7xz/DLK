+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sch\u00fctten sie aus deinen Falten weit! \u2013", "tokens": ["Sch\u00fct\u00b7ten", "sie", "aus", "dei\u00b7nen", "Fal\u00b7ten", "weit", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.22": {"line.1": {"text": "Um des M\u00f6nches Haupt, wie Sonnenlicht", "tokens": ["Um", "des", "M\u00f6n\u00b7ches", "Haupt", ",", "wie", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Leuchtend, flie\u00dft das holde Traumgesicht,", "tokens": ["Leuch\u00b7tend", ",", "flie\u00dft", "das", "hol\u00b7de", "Traum\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "R\u00fcstig nach dem Stabe greift die Hand,", "tokens": ["R\u00fcs\u00b7tig", "nach", "dem", "Sta\u00b7be", "greift", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hoch erhobnen Haupts blickt er ins Land.", "tokens": ["Hoch", "er\u00b7hob\u00b7nen", "Haupts", "blickt", "er", "ins", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.23": {"line.1": {"text": "Um die Schultern seinen Sack gelegt,", "tokens": ["Um", "die", "Schul\u00b7tern", "sei\u00b7nen", "Sack", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ha, wie stolz er jetzt den leeren tr\u00e4gt!", "tokens": ["Ha", ",", "wie", "stolz", "er", "jetzt", "den", "lee\u00b7ren", "tr\u00e4gt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schlo\u00df er ja den ganzen Erdball ein,", "tokens": ["Schlo\u00df", "er", "ja", "den", "gan\u00b7zen", "Erd\u00b7ball", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und den Sternenhimmel obendrein!", "tokens": ["Und", "den", "Ster\u00b7nen\u00b7him\u00b7mel", "o\u00b7ben\u00b7drein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Milder Traumgott, die geschenkt du hast,", "tokens": ["Mil\u00b7der", "Traum\u00b7gott", ",", "die", "ge\u00b7schenkt", "du", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hilf sie tragen auch, die schwerste Last,", "tokens": ["Hilf", "sie", "tra\u00b7gen", "auch", ",", "die", "schwers\u00b7te", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df die B\u00fcrde leicht und sanft ihm sei", "tokens": ["Da\u00df", "die", "B\u00fcr\u00b7de", "leicht", "und", "sanft", "ihm", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "VVFIN", "PPER", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie einst jenen, deren Tag vorbei.\u00ab", "tokens": ["Wie", "einst", "je\u00b7nen", ",", "de\u00b7ren", "Tag", "vor\u00b7bei", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "PDS", "$,", "PRELAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}