{"dta.poem.11999": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Je H\u00f6heres du aus vom H\u00f6chsten sagen magst,", "tokens": ["Je", "H\u00f6\u00b7he\u00b7res", "du", "aus", "vom", "H\u00f6chs\u00b7ten", "sa\u00b7gen", "magst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "APPR", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je tiefer f\u00fchlst du da\u00df du nichts im Grunde sagst.", "tokens": ["Je", "tie\u00b7fer", "f\u00fchlst", "du", "da\u00df", "du", "nichts", "im", "Grun\u00b7de", "sagst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "KOUS", "PPER", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Magst du's mit reichstem Schmuck der Fantasie umkleiden,", "tokens": ["Magst", "du's", "mit", "reichs\u00b7tem", "Schmuck", "der", "Fan\u00b7ta\u00b7sie", "um\u00b7klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit feinster Sondrung auch vom Irdischen ausscheiden;", "tokens": ["Mit", "feins\u00b7ter", "Sond\u00b7rung", "auch", "vom", "Ir\u00b7di\u00b7schen", "aus\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort machst du Geistiges zu leiblicher Erscheinung,", "tokens": ["Dort", "machst", "du", "Geis\u00b7ti\u00b7ges", "zu", "leib\u00b7li\u00b7cher", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier das vollste Ja zur leeresten Verneinung.", "tokens": ["Und", "hier", "das", "volls\u00b7te", "Ja", "zur", "lee\u00b7res\u00b7ten", "Ver\u00b7nei\u00b7nung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was anders also kannst du thun als dich bequemen,", "tokens": ["Was", "an\u00b7ders", "al\u00b7so", "kannst", "du", "thun", "als", "dich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "KOKOM", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jetzt dies zu setzen und es dann zur\u00fcckzunehmen?", "tokens": ["Jetzt", "dies", "zu", "set\u00b7zen", "und", "es", "dann", "zu\u00b7r\u00fcck\u00b7zu\u00b7neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PTKZU", "VVINF", "KON", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was alles du von ihm magst sagen, da\u00df es sei,", "tokens": ["Was", "al\u00b7les", "du", "von", "ihm", "magst", "sa\u00b7gen", ",", "da\u00df", "es", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist nicht was du sagst, doch was du f\u00fchlst dabei.", "tokens": ["Es", "ist", "nicht", "was", "du", "sagst", ",", "doch", "was", "du", "f\u00fchlst", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$,", "KON", "PWS", "PPER", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}