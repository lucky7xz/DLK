{"textgrid.poem.41821": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Germanias Standbild auf dem Niederwald", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Jahre sind's \u2013 da stand der Dom vollendet,", "tokens": ["Drei", "Jah\u00b7re", "sin\u00b7d's", "\u2013", "da", "stand", "der", "Dom", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NE", "$(", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Dom zu K\u00f6ln, nun herrlich ausgebaut;", "tokens": ["Der", "Dom", "zu", "K\u00f6ln", ",", "nun", "herr\u00b7lich", "aus\u00b7ge\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Siegesmal, dem deutschen Volk gespendet,", "tokens": ["Ein", "Sie\u00b7ges\u00b7mal", ",", "dem", "deut\u00b7schen", "Volk", "ge\u00b7spen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das endlich einig seiner Kraft vertraut.", "tokens": ["Das", "end\u00b7lich", "ei\u00b7nig", "sei\u00b7ner", "Kraft", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und frohgemut begann vor vierzig Jahren", "tokens": ["Und", "froh\u00b7ge\u00b7mut", "be\u00b7gann", "vor", "vier\u00b7zig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der V\u00e4ter Erbe heilger Kunst zu wahren.", "tokens": ["Der", "V\u00e4\u00b7ter", "Er\u00b7be", "heil\u00b7ger", "Kunst", "zu", "wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wie ich einst in meiner Jugend Tagen", "tokens": ["Und", "wie", "ich", "einst", "in", "mei\u00b7ner", "Ju\u00b7gend", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon sehnsuchtsvoll zum gr\u00fcnen Rheine zog", "tokens": ["Schon", "sehn\u00b7suchts\u00b7voll", "zum", "gr\u00fc\u00b7nen", "Rhei\u00b7ne", "zog"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und um mich Viele wollten schon verzagen,", "tokens": ["Und", "um", "mich", "Vie\u00b7le", "woll\u00b7ten", "schon", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PRF", "PIS", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil scheinbar die Prophetenstimme log", "tokens": ["Weil", "schein\u00b7bar", "die", "Pro\u00b7phe\u00b7ten\u00b7stim\u00b7me", "log"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und falsch gesprochen von des Doms Erneuen \u2013", "tokens": ["Und", "falsch", "ge\u00b7spro\u00b7chen", "von", "des", "Doms", "Er\u00b7neu\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun gilt es der Erf\u00fcllung sich zu freuen!", "tokens": ["Nun", "gilt", "es", "der", "Er\u00b7f\u00fcl\u00b7lung", "sich", "zu", "freu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt ward ein andres Standbild aufgerichtet:", "tokens": ["Jetzt", "ward", "ein", "and\u00b7res", "Stand\u00b7bild", "auf\u00b7ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.3": {"text": "All' was ich einst von ihr getr\u00e4umt, gedichtet,", "tokens": ["All'", "was", "ich", "einst", "von", "ihr", "ge\u00b7tr\u00e4umt", ",", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ward Wirklichkeit in herrlicher Gestalt,", "tokens": ["Ward", "Wirk\u00b7lich\u00b7keit", "in", "herr\u00b7li\u00b7cher", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wallfahrend wie zu einem Heiligtume", "tokens": ["Wall\u00b7fah\u00b7rend", "wie", "zu", "ei\u00b7nem", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Naht alles Volk und weiht sich ihrem Ruhme.", "tokens": ["Naht", "al\u00b7les", "Volk", "und", "weiht", "sich", "ih\u00b7rem", "Ruh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auch ich sah sie in goldnem Sonnenglanze", "tokens": ["Auch", "ich", "sah", "sie", "in", "gold\u00b7nem", "Son\u00b7nen\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und wieder dann in stiller Mondennacht,", "tokens": ["Und", "wie\u00b7der", "dann", "in", "stil\u00b7ler", "Mon\u00b7den\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das edle Frauenhaupt im Eichenkranze,", "tokens": ["Das", "ed\u00b7le", "Frau\u00b7en\u00b7haupt", "im", "Ei\u00b7chen\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erhoben hoch zu ihres Volkes Wacht \u2013", "tokens": ["Er\u00b7ho\u00b7ben", "hoch", "zu", "ih\u00b7res", "Vol\u00b7kes", "Wacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Germania \u2013 einst nur ein Traum der Thoren \u2013", "tokens": ["Ger\u00b7ma\u00b7nia", "\u2013", "einst", "nur", "ein", "Traum", "der", "Tho\u00b7ren", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jetzt zu der Schirmerin des Reichs erkoren.", "tokens": ["Jetzt", "zu", "der", "Schir\u00b7me\u00b7rin", "des", "Reichs", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wehn im Sonnenglanz die deutschen Fahnen", "tokens": ["Und", "wehn", "im", "Son\u00b7nen\u00b7glanz", "die", "deut\u00b7schen", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu ihrer Ehr und donnern Schu\u00df um Schu\u00df \u2013", "tokens": ["Zu", "ih\u00b7rer", "Ehr", "und", "don\u00b7nern", "Schu\u00df", "um", "Schu\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im stillen Mondschein lie\u00df ein selig Ahnen", "tokens": ["Im", "stil\u00b7len", "Mond\u00b7schein", "lie\u00df", "ein", "se\u00b7lig", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich leis vernehmen einen andern Gru\u00df:", "tokens": ["Mich", "leis", "ver\u00b7neh\u00b7men", "ei\u00b7nen", "an\u00b7dern", "Gru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ward ", "tokens": ["Ward"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "So ziemt's uns Allen unsre Kraft erproben.", "tokens": ["So", "ziemt's", "uns", "Al\u00b7len", "uns\u00b7re", "Kraft", "er\u00b7pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So ziemt's uns Allen nach dem Ziel zu ringen,", "tokens": ["So", "ziemt's", "uns", "Al\u00b7len", "nach", "dem", "Ziel", "zu", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das hier erscheint in herrlicher Gestalt.", "tokens": ["Das", "hier", "er\u00b7scheint", "in", "herr\u00b7li\u00b7cher", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was deutsche Frauen streben, mu\u00df gelingen,", "tokens": ["Was", "deut\u00b7sche", "Frau\u00b7en", "stre\u00b7ben", ",", "mu\u00df", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Germania wird selbst uns Hort und Halt.", "tokens": ["Ger\u00b7ma\u00b7nia", "wird", "selbst", "uns", "Hort", "und", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPER", "NN", "KON", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Jetzt ist es leichter Sieg zu prophezeihen,", "tokens": ["Jetzt", "ist", "es", "leich\u00b7ter", "Sieg", "zu", "pro\u00b7phe\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn wir zu ihrer Ehr uns selbst befreien.", "tokens": ["Wenn", "wir", "zu", "ih\u00b7rer", "Ehr", "uns", "selbst", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Str\u00f6mt jetzt am Rheine alles Volk zusammen", "tokens": ["Str\u00f6mt", "jetzt", "am", "Rhei\u00b7ne", "al\u00b7les", "Volk", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NE", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und feiert man bei ihr ein Siegesfest,", "tokens": ["Und", "fei\u00b7ert", "man", "bei", "ihr", "ein", "Sie\u00b7ges\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lodern hoch der Freudenfeuer Flammen", "tokens": ["Und", "lo\u00b7dern", "hoch", "der", "Freu\u00b7den\u00b7feu\u00b7er", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Glauben, da\u00df sie nie ihr Volk verl\u00e4\u00dft,", "tokens": ["Im", "Glau\u00b7ben", ",", "da\u00df", "sie", "nie", "ihr", "Volk", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So d\u00fcrfen wir auch ihrer Huld vertrauen,", "tokens": ["So", "d\u00fcr\u00b7fen", "wir", "auch", "ih\u00b7rer", "Huld", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die h\u00f6chste Frau verl\u00e4\u00dft auch nicht die Frauen.", "tokens": ["Die", "h\u00f6chs\u00b7te", "Frau", "ver\u00b7l\u00e4\u00dft", "auch", "nicht", "die", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie fordern ihren Teil als Priesterinnen", "tokens": ["Sie", "for\u00b7dern", "ih\u00b7ren", "Teil", "als", "Pries\u00b7te\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Dienst des Reiches, das man ihr geweiht;", "tokens": ["Im", "Dienst", "des", "Rei\u00b7ches", ",", "das", "man", "ihr", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In ihrem Dienste wollen sie beginnen", "tokens": ["In", "ih\u00b7rem", "Diens\u00b7te", "wol\u00b7len", "sie", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das neue Werk der freien bessern Zeit,", "tokens": ["Das", "neu\u00b7e", "Werk", "der", "frei\u00b7en", "bes\u00b7sern", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vertrauend knieen sie zu ihren F\u00fc\u00dfen,", "tokens": ["Ver\u00b7trau\u00b7end", "kni\u00b7e\u00b7en", "sie", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Als h\u00f6chste Sch\u00fctzern sie zu begr\u00fc\u00dfen.", "tokens": ["Als", "h\u00f6chs\u00b7te", "Sch\u00fct\u00b7zern", "sie", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Drei Jahre sind's \u2013 da stand der Dom vollendet,", "tokens": ["Drei", "Jah\u00b7re", "sin\u00b7d's", "\u2013", "da", "stand", "der", "Dom", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NE", "$(", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic