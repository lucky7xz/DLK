{"textgrid.poem.41812": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Pa\u00dfwort, das gleich einem Sonnenstrahle", "tokens": ["Das", "Pa\u00df\u00b7wort", ",", "das", "gleich", "ei\u00b7nem", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Pfad zum Ziele zeigt, licht\u00fcbergossen,", "tokens": ["Den", "Pfad", "zum", "Zie\u00b7le", "zeigt", ",", "licht\u00b7\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bauh\u00fctten \u00f6ffnet, die uns sonst verschlossen,", "tokens": ["Bau\u00b7h\u00fct\u00b7ten", "\u00f6ff\u00b7net", ",", "die", "uns", "sonst", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Pa\u00dfwort hei\u00dft: Getreu dem Ideale.", "tokens": ["Dies", "Pa\u00df\u00b7wort", "hei\u00dft", ":", "Ge\u00b7treu", "dem", "I\u00b7dea\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "$.", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Vor dieser Losung sind mit einem male", "tokens": ["Vor", "die\u00b7ser", "Lo\u00b7sung", "sind", "mit", "ei\u00b7nem", "ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verscheucht die Thoren, die es stets verdrossen,", "tokens": ["Ver\u00b7scheucht", "die", "Tho\u00b7ren", ",", "die", "es", "stets", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn keusche Seelen Himmelstrank genossen,", "tokens": ["Wenn", "keu\u00b7sche", "See\u00b7len", "Him\u00b7mel\u00b7strank", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entquollen einem wunderreichen Grale.", "tokens": ["Ent\u00b7quol\u00b7len", "ei\u00b7nem", "wun\u00b7der\u00b7rei\u00b7chen", "Gra\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir aber sind durch dieses Wort verbunden", "tokens": ["Wir", "a\u00b7ber", "sind", "durch", "die\u00b7ses", "Wort", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit heilgem Band, durch keine Macht zu trennen,", "tokens": ["Mit", "heil\u00b7gem", "Band", ",", "durch", "kei\u00b7ne", "Macht", "zu", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Seit wir uns so auf gleichem Pfad gefunden!", "tokens": ["Seit", "wir", "uns", "so", "auf", "glei\u00b7chem", "Pfad", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wollen k\u00fchn vor einer Welt bekennen,", "tokens": ["Und", "wol\u00b7len", "k\u00fchn", "vor", "ei\u00b7ner", "Welt", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wandellos, schl\u00fcg' sie uns drum auch Wunden,", "tokens": ["Da\u00df", "wan\u00b7del\u00b7los", ",", "schl\u00fcg'", "sie", "uns", "drum", "auch", "Wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "VVFIN", "PPER", "PRF", "PAV", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir Streiter uns f\u00fcrs Ideale nennen", "tokens": ["Wir", "Strei\u00b7ter", "uns", "f\u00fcrs", "I\u00b7dea\u00b7le", "nen\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Das Pa\u00dfwort, das gleich einem Sonnenstrahle", "tokens": ["Das", "Pa\u00df\u00b7wort", ",", "das", "gleich", "ei\u00b7nem", "Son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Pfad zum Ziele zeigt, licht\u00fcberg