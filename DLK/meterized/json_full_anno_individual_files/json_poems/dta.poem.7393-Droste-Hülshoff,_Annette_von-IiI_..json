{"dta.poem.7393": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie friedlich in der Erde Schoo\u00df die still geringen Leutchen", "tokens": ["Wie", "fried\u00b7lich", "in", "der", "Er\u00b7de", "Schoo\u00df", "die", "still", "ge\u00b7rin\u00b7gen", "Leut\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "schlafen!", "tokens": ["schla\u00b7fen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Endlich ein Pf\u00fchl nach hartem Stroh, nach saurer Fahrt", "tokens": ["End\u00b7lich", "ein", "Pf\u00fchl", "nach", "har\u00b7tem", "Stroh", ",", "nach", "sau\u00b7rer", "Fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "endlich ein Hafen!", "tokens": ["end\u00b7lich", "ein", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dem Flockenwulste, sichtbar kaum, entheben sich die niedern", "tokens": ["Dem", "Flo\u00b7cken\u00b7wuls\u00b7te", ",", "sicht\u00b7bar", "kaum", ",", "ent\u00b7he\u00b7ben", "sich", "die", "nie\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "ADV", "$,", "VVFIN", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "H\u00fcgel,", "tokens": ["H\u00fc\u00b7gel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Doch Gottes Engel kennt sie wohl, und schirmend breitet er", "tokens": ["Doch", "Got\u00b7tes", "En\u00b7gel", "kennt", "sie", "wohl", ",", "und", "schir\u00b7mend", "brei\u00b7tet", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VVFIN", "PPER", "ADV", "$,", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "die Fl\u00fcgel", "tokens": ["die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Den Kreuzlein zu, die Pflock an Pflock", "tokens": ["Den", "Kreuz\u00b7lein", "zu", ",", "die", "Pflock", "an", "Pflock"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich reihen um den Marmorblock.", "tokens": ["Sich", "rei\u00b7hen", "um", "den", "Mar\u00b7mor\u00b7block", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Sockel kreucht der Drachenwurm, und scheint zum Grund", "tokens": ["Am", "So\u00b7ckel", "kreucht", "der", "Dra\u00b7chen\u00b7wurm", ",", "und", "scheint", "zum", "Grund"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hinabzukrallen,", "tokens": ["hin\u00b7ab\u00b7zu\u00b7kral\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Zum todten Wuchrer unter'm Stein, von eigner Frevelhand", "tokens": ["Zum", "tod\u00b7ten", "Wuch\u00b7rer", "un\u00b7ter'm", "Stein", ",", "von", "eig\u00b7ner", "Fre\u00b7vel\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gefallen,", "tokens": ["ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wohl hat ihm Gold ein ehrlich Grab geworben an der Fried-", "tokens": ["Wohl", "hat", "ihm", "Gold", "ein", "ehr\u00b7lich", "Grab", "ge\u00b7wor\u00b7ben", "an", "der", "Frie\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ART", "ADJD", "NN", "VVPP", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "hofsmauer,", "tokens": ["hofs\u00b7mau\u00b7er", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Doch dr\u00fcber zuckt sein Flammenschwert Sankt Michael in", "tokens": ["Doch", "dr\u00fc\u00b7ber", "zuckt", "sein", "Flam\u00b7men\u00b7schwert", "Sankt", "Mic\u00b7hael", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPOSAT", "NN", "VVFIN", "NE", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zorn und Trauer,", "tokens": ["Zorn", "und", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "So silbergrau, ein Nachtgesicht,", "tokens": ["So", "sil\u00b7ber\u00b7grau", ",", "ein", "Nacht\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Steht das versteinerte Gericht.", "tokens": ["Steht", "das", "ver\u00b7stei\u00b7ner\u00b7te", "Ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom \u00f6den Hause, seinem einst, wo blutge Thr\u00e4nen sind", "tokens": ["Vom", "\u00f6\u00b7den", "Hau\u00b7se", ",", "sei\u00b7nem", "einst", ",", "wo", "blut\u00b7ge", "Thr\u00e4\u00b7nen", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "ADV", "$,", "PWAV", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "geflossen,", "tokens": ["ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hat sich ein seltsam d\u00e4mmernd Licht bis an den Marmel-", "tokens": ["Hat", "sich", "ein", "selt\u00b7sam", "d\u00e4m\u00b7mernd", "Licht", "bis", "an", "den", "Mar\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJD", "VVPP", "NN", "APPR", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "stein ergossen,", "tokens": ["stein", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es ist als ob das Monument bei der Ber\u00fchrung zitternd", "tokens": ["Es", "ist", "als", "ob", "das", "Mo\u00b7nu\u00b7ment", "bei", "der", "Be\u00b7r\u00fch\u00b7rung", "zit\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "schwanke,", "tokens": ["schwan\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Im Schnee w\u00fchlend eine Hand dem Schuldner sich entgegen", "tokens": ["Im", "Schnee", "w\u00fch\u00b7lend", "ei\u00b7ne", "Hand", "dem", "Schuld\u00b7ner", "sich", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "ART", "NN", "PRF", "PTKVZ"], "meter": "-++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "ranke;", "tokens": ["ran\u00b7ke", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Er k\u00f6mmt, er naht, die Pforte dr\u00f6hnt,", "tokens": ["Er", "k\u00f6mmt", ",", "er", "naht", ",", "die", "Pfor\u00b7te", "dr\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er hat sich an den Stein gelehnt;", "tokens": ["Er", "hat", "sich", "an", "den", "Stein", "ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bleich wie der Marmor \u00fcber ihm, und finster wie das Kreuz", "tokens": ["Bleich", "wie", "der", "Mar\u00b7mor", "\u00fc\u00b7ber", "ihm", ",", "und", "fins\u00b7ter", "wie", "das", "Kreuz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "PPER", "$,", "KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "zur Seiten,", "tokens": ["zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Von Stirn und Wimper, Z\u00e4hren gleich, geschmolznen Reifes", "tokens": ["Von", "Stirn", "und", "Wim\u00b7per", ",", "Z\u00e4h\u00b7ren", "gleich", ",", "ge\u00b7schmolz\u00b7nen", "Rei\u00b7fes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "ADV", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tropfen gleiten;", "tokens": ["Trop\u00b7fen", "glei\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Was er in dieser schweren Nacht gelitten oder auch ges\u00fcndet,", "tokens": ["Was", "er", "in", "die\u00b7ser", "schwe\u00b7ren", "Nacht", "ge\u00b7lit\u00b7ten", "o\u00b7der", "auch", "ge\u00b7s\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "ADJA", "NN", "VVPP", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Er hat es Keinem je geklagt und Keinem reuig es verk\u00fcndet;", "tokens": ["Er", "hat", "es", "Kei\u00b7nem", "je", "ge\u00b7klagt", "und", "Kei\u00b7nem", "reu\u00b7ig", "es", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "KON", "PIS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.7": {"text": "In's Dunkel starrt er, wie man wohl", "tokens": ["In's", "Dun\u00b7kel", "starrt", "er", ",", "wie", "man", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So starrt gedankenlos und hohl.", "tokens": ["So", "starrt", "ge\u00b7dan\u00b7ken\u00b7los", "und", "hohl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihm ist, als f\u00fchl' er noch die Hand die seinen Federzug", "tokens": ["Ihm", "ist", ",", "als", "f\u00fchl'", "er", "noch", "die", "Hand", "die", "sei\u00b7nen", "Fe\u00b7der\u00b7zug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "geleitet,", "tokens": ["ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Als f\u00fchle er den Nadelstich, der seines Blutes Quell be-", "tokens": ["Als", "f\u00fch\u00b7le", "er", "den", "Na\u00b7del\u00b7stich", ",", "der", "sei\u00b7nes", "Blu\u00b7tes", "Quell", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "reitet,", "tokens": ["rei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und leise zitternd tastet er zum Gurte, \u2014 h\u00f6rst du nicht", "tokens": ["Und", "lei\u00b7se", "zit\u00b7ternd", "tas\u00b7tet", "er", "zum", "Gur\u00b7te", ",", "h\u00f6rst", "du", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VVFIN", "PPER", "APPRART", "NN", "$,", "$(", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "ein Knirren,", "tokens": ["ein", "Knir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Viel schrillender als Uhrgetick, viel zarter als der Spange", "tokens": ["Viel", "schril\u00b7len\u00b7der", "als", "Uhr\u00b7ge\u00b7tick", ",", "viel", "zar\u00b7ter", "als", "der", "Span\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Klirren? \u2014", "tokens": ["Klir\u00b7ren", "?"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "O, seine Heimath, still umlaubt!", "tokens": ["O", ",", "sei\u00b7ne", "Hei\u00b7math", ",", "still", "um\u00b7laubt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O, seines Vaters graues Haupt!", "tokens": ["O", ",", "sei\u00b7nes", "Va\u00b7ters", "grau\u00b7es", "Haupt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bewu\u00dftlos an des Engels Knie dr\u00fcckt er die Stirn, klemmt", "tokens": ["Be\u00b7wu\u00dft\u00b7los", "an", "des", "En\u00b7gels", "Knie", "dr\u00fcckt", "er", "die", "Stirn", ",", "klemmt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "er die H\u00e4nde,", "tokens": ["er", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der todten G\u00e4ule Klingeln h\u00f6rt er schleichen durch die", "tokens": ["Der", "tod\u00b7ten", "G\u00e4u\u00b7le", "Klin\u00b7geln", "h\u00f6rt", "er", "schlei\u00b7chen", "durch", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fichtenw\u00e4nde;", "tokens": ["Fich\u00b7ten\u00b7w\u00e4n\u00b7de", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Gen\u00fcber ihm am Horizonte schleifen schwarze Wolkenspalten,", "tokens": ["Ge\u00b7n\u00fc\u00b7ber", "ihm", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "schlei\u00b7fen", "schwar\u00b7ze", "Wol\u00b7ken\u00b7spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.6": {"text": "Wie l\u00e4ssig eine tr\u00e4ge Hand zum Sarge schleift des Bahr-", "tokens": ["Wie", "l\u00e4s\u00b7sig", "ei\u00b7ne", "tr\u00e4\u00b7ge", "Hand", "zum", "Sar\u00b7ge", "schleift", "des", "Bahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "tuchs Falten;", "tokens": ["tuchs", "Fal\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Er streicht das Auge, reckt sich auf,", "tokens": ["Er", "streicht", "das", "Au\u00b7ge", ",", "reckt", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und schaut zum Aetherdom hinauf.", "tokens": ["Und", "schaut", "zum", "A\u00b7e\u00b7ther\u00b7dom", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Noch h\u00e4ngt die Mondesampel klar am goldgestickten Kuppel-", "tokens": ["Noch", "h\u00e4ngt", "die", "Mon\u00b7de\u00b7sam\u00b7pel", "klar", "am", "gold\u00b7ge\u00b7stick\u00b7ten", "Kup\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "ringe,", "tokens": ["rin\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Noch leuchtet von Sankt Thomas Thurm das Kreuz wie", "tokens": ["Noch", "leuch\u00b7tet", "von", "Sankt", "Tho\u00b7mas", "Thurm", "das", "Kreuz", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "VVFIN", "NE", "NN", "ART", "NN", "KOKOM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "eine Doppelklinge,", "tokens": ["ei\u00b7ne", "Dop\u00b7pel\u00b7klin\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Noch ist die Stunde nicht, wo sich der Hahn auf seiner Stange", "tokens": ["Noch", "ist", "die", "Stun\u00b7de", "nicht", ",", "wo", "sich", "der", "Hahn", "auf", "sei\u00b7ner", "Stan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PWAV", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "sch\u00fcttelt,", "tokens": ["sch\u00fct\u00b7telt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "O eilig, eilig, eh die Uhr das letzte Sandkorn hat ger\u00fcttelt!", "tokens": ["O", "ei\u00b7lig", ",", "ei\u00b7lig", ",", "eh", "die", "Uhr", "das", "letz\u00b7te", "Sand\u00b7korn", "hat", "ge\u00b7r\u00fct\u00b7telt", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.8": {"text": "Er wendet sich, da \u2014 horch, ein Klang,", "tokens": ["Er", "wen\u00b7det", "sich", ",", "da", "horch", ",", "ein", "Klang", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "$(", "VVIMP", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und wieder einer, schwer und bang!", "tokens": ["Und", "wie\u00b7der", "ei\u00b7ner", ",", "schwer", "und", "bang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und mit dem zw\u00f6lften Schlage hat der Wolkenmantel sich", "tokens": ["Und", "mit", "dem", "zw\u00f6lf\u00b7ten", "Schla\u00b7ge", "hat", "der", "Wol\u00b7ken\u00b7man\u00b7tel", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gebreitet,", "tokens": ["ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der immer h\u00f6her, riesig hoch, sich um die Himmelskuppel", "tokens": ["Der", "im\u00b7mer", "h\u00f6\u00b7her", ",", "rie\u00b7sig", "hoch", ",", "sich", "um", "die", "Him\u00b7mels\u00b7kup\u00b7pel"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "ADJD", "ADJD", "$,", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "weitet,", "tokens": ["wei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und, horch! \u2014 ein langgedehnter Schrei, des Hahn