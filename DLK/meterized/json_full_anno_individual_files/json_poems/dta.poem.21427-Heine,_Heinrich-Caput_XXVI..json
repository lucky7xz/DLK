{"dta.poem.21427": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denke mit Widerwillen noch", "tokens": ["Ich", "den\u00b7ke", "mit", "Wi\u00b7der\u00b7wil\u00b7len", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An jene schn\u00f6den, verfluchten", "tokens": ["An", "je\u00b7ne", "schn\u00f6\u00b7den", ",", "ver\u00b7fluch\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "ADJA", "$,", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vorspielger\u00fcche, das schien ein Gemisch", "tokens": ["Vor\u00b7spiel\u00b7ge\u00b7r\u00fc\u00b7che", ",", "das", "schien", "ein", "Ge\u00b7misch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PDS", "VVFIN", "ART", "NN"], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Von altem Kohl und Juchten.", "tokens": ["Von", "al\u00b7tem", "Kohl", "und", "Juch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Entsetzlich waren die D\u00fcfte, o Gott!", "tokens": ["Ent\u00b7setz\u00b7lich", "wa\u00b7ren", "die", "D\u00fcf\u00b7te", ",", "o", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sich nachher erhuben;", "tokens": ["Die", "sich", "nach\u00b7her", "er\u00b7hu\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es war als fegte man den Mist", "tokens": ["Es", "war", "als", "feg\u00b7te", "man", "den", "Mist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus sechs und drei\u00dfig Gruben. \u2013 \u2013 \u2013", "tokens": ["Aus", "sechs", "und", "drei\u00b7\u00dfig", "Gru\u00b7ben", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "CARD", "KON", "CARD", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich wei\u00df wohl was Saint-Just gesagt", "tokens": ["Ich", "wei\u00df", "wohl", "was", "Saint\u00b7Just", "ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weiland im Wohlfahrtsausschu\u00df:", "tokens": ["Wei\u00b7land", "im", "Wohl\u00b7fahrts\u00b7aus\u00b7schu\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Man heile die gro\u00dfe Krankheit nicht", "tokens": ["Man", "hei\u00b7le", "die", "gro\u00b7\u00dfe", "Krank\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Rosen\u00f6l und Moschus \u2013", "tokens": ["Mit", "Ro\u00b7sen\u00b7\u00f6l", "und", "Mo\u00b7schus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Doch dieser deutsche Zukunftsduft", "tokens": ["Doch", "die\u00b7ser", "deut\u00b7sche", "Zu\u00b7kunfts\u00b7duft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mocht alles \u00fcberragen", "tokens": ["Mocht", "al\u00b7les", "\u00fc\u00b7berr\u00b7a\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was meine Nase je geahnt \u2013", "tokens": ["Was", "mei\u00b7ne", "Na\u00b7se", "je", "ge\u00b7ahnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich konnt es nicht l\u00e4nger ertragen \u2013 \u2013 \u2013", "tokens": ["Ich", "konnt", "es", "nicht", "l\u00e4n\u00b7ger", "er\u00b7tra\u00b7gen", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$(", "$(", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}