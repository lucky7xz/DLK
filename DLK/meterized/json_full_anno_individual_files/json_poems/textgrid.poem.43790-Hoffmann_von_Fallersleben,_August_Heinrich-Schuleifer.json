{"textgrid.poem.43790": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schuleifer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gerne wir auch in fr\u00f6hlicher Weise", "tokens": ["So", "ger\u00b7ne", "wir", "auch", "in", "fr\u00f6h\u00b7li\u00b7cher", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uns tummeln im Schnee und auf dem Eise", "tokens": ["Uns", "tum\u00b7meln", "im", "Schnee", "und", "auf", "dem", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit Schlittschuhlaufen und Schlittenfahren,", "tokens": ["Mit", "Schlitt\u00b7schuhl\u00b7au\u00b7fen", "und", "Schlit\u00b7ten\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schneeb\u00e4llen und Glandern in lustigen Schaaren,", "tokens": ["Schnee\u00b7b\u00e4l\u00b7len", "und", "Glan\u00b7dern", "in", "lus\u00b7ti\u00b7gen", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So wird doch wahrlich unterdessen", "tokens": ["So", "wird", "doch", "wahr\u00b7lich", "un\u00b7ter\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "AD