{"dta.poem.1428": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT/ der dich lieben Sohn den Eltern hat gegeben/", "tokens": ["GoTT", "/", "der", "dich", "lie\u00b7ben", "Sohn", "den", "El\u00b7tern", "hat", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gab seinen eignen Sohn f\u00fcr unser aller Leben", "tokens": ["Gab", "sei\u00b7nen", "eig\u00b7nen", "Sohn", "f\u00fcr", "un\u00b7ser", "al\u00b7ler", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus holder Lieb und Gunst/ der ist auch dir geschenckt/", "tokens": ["Aus", "hol\u00b7der", "Lieb", "und", "Gunst", "/", "der", "ist", "auch", "dir", "ge\u00b7schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$(", "ART", "VAFIN", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn dein verneutes Hertz stets gl\u00e4ubig an ihn denckt.", "tokens": ["Wenn", "dein", "ver\u00b7neu\u00b7tes", "Hertz", "stets", "gl\u00e4u\u00b7big", "an", "ihn", "denckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "