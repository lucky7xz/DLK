{"textgrid.poem.58568": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "606. Trost der Auferstehung", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, f\u00fcrchtet nicht, ihr meine Glieder,", "tokens": ["Nein", ",", "f\u00fcrch\u00b7tet", "nicht", ",", "ihr", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr kommt zu Gott mit Christus wieder,", "tokens": ["Ihr", "kommt", "zu", "Gott", "mit", "Chris\u00b7tus", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NE", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das glaubet frei!", "tokens": ["Das", "glau\u00b7bet", "frei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Er tr\u00e4gt euch als die Seinen eben,", "tokens": ["Er", "tr\u00e4gt", "euch", "als", "die", "Sei\u00b7nen", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "PPOSS", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ruft euch mit sich vom Tod ins Leben,", "tokens": ["Ruft", "euch", "mit", "sich", "vom", "Tod", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist getreu.", "tokens": ["Er", "ist", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Will Krankheit drohn, gr\u00fc\u00dft sie mit Freuden,", "tokens": ["Will", "Krank\u00b7heit", "drohn", ",", "gr\u00fc\u00dft", "sie", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verachtet Not, Tr\u00fcbsal und Leiden,", "tokens": ["Ver\u00b7ach\u00b7tet", "Not", ",", "Tr\u00fcb\u00b7sal", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Was kommen mag!", "tokens": ["Was", "kom\u00b7men", "mag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Das d\u00fcstre Grab getrost ansehet,", "tokens": ["Das", "d\u00fcst\u00b7re", "Grab", "ge\u00b7trost", "an\u00b7se\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Christus, dem Erstandnen, gehet,", "tokens": ["Mit", "Chris\u00b7tus", ",", "dem", "Er\u00b7stand\u00b7nen", ",", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Wie's geht, ihm nach!", "tokens": ["Wie's", "geht", ",", "ihm", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punc