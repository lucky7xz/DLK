{"textgrid.poem.46569": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df ich trinken soll und essen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich trinken soll und essen,", "tokens": ["Da\u00df", "ich", "trin\u00b7ken", "soll", "und", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Essen, trinken, nach wie vor,", "tokens": ["Es\u00b7sen", ",", "trin\u00b7ken", ",", "nach", "wie", "vor", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "APPR", "KOKOM", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dabei vergessen,", "tokens": ["Und", "da\u00b7bei", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df ich dich verlor!", "tokens": ["Da\u00df", "ich", "dich", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch das St\u00fchlchen, wo du sa\u00dfest", "tokens": ["Doch", "das", "St\u00fchl\u00b7chen", ",", "wo", "du", "sa\u00b7\u00dfest"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zur Linken, stehet leer,", "tokens": ["Mir", "zur", "Lin\u00b7ken", ",", "ste\u00b7het", "leer", ","], "token_info": ["word", "word", "word",