{"textgrid.poem.62227": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Poesie ist ein Gebiet,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Poesie ist ein Gebiet,", "tokens": ["Die", "Poe\u00b7sie", "ist", "ein", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo alle Bl\u00fcten treiben.", "tokens": ["Wo", "al\u00b7le", "Bl\u00fc\u00b7ten", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt soll ich gar ein Burschenlied", "tokens": ["Jetzt", "soll", "ich", "gar", "ein", "Bur\u00b7schen\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr die Studenten schreiben.", "tokens": ["F\u00fcr", "die", "Stu\u00b7den\u00b7ten", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Wohlan, es sei, ich fange an,", "tokens": ["Wo\u00b7hlan", ",", "es", "sei", ",", "ich", "fan\u00b7ge", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schreib', so gut ich schreiben kann.", "tokens": ["Und", "schreib'", ",", "so", "gut", "ich", "schrei\u00b7ben", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich lob' mir die Studentenschaft,", "tokens": ["Ich", "lob'", "mir", "die", "Stu\u00b7den\u00b7ten\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die brav, fidel und bieder,", "tokens": ["Die", "brav", ",", "fi\u00b7del", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit hellem Geist und Mut und Kraft", "tokens": ["Mit", "hel\u00b7lem", "Geist", "und", "Mut", "und", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch h\u00e4lt die deutschen Lieder.", "tokens": ["Hoch", "h\u00e4lt", "die", "deut\u00b7schen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Liedern zieht er in die Welt,", "tokens": ["Mit", "Lie\u00b7dern", "zieht", "er", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein solcher Bursche ist ein Held.", "tokens": ["Ein", "sol\u00b7cher", "Bur\u00b7sche", "ist", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im schmucken, reichgestickten Kleid,", "tokens": ["Im", "schmu\u00b7cken", ",", "reich\u00b7ge\u00b7stick\u00b7ten", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Humpen und mit Degen", "tokens": ["Mit", "Hum\u00b7pen", "und", "mit", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist gern geseh'n er weit und breit,", "tokens": ["Ist", "gern", "ge\u00b7seh'n", "er", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf allen deutschen Wegen.", "tokens": ["Auf", "al\u00b7len", "deut\u00b7schen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein solcher Bursche ist ein Held,", "tokens": ["Ein", "sol\u00b7cher", "Bur\u00b7sche", "ist", "ein", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er zieht als Sieger durch die Welt.", "tokens": ["Er", "zieht", "als", "Sie\u00b7ger", "durch", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und zeigt man ihm ein b\u00f6ses Weib,", "tokens": ["Und", "zeigt", "man", "ihm", "ein", "b\u00f6\u00b7ses", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Braut ihm zu ersetzen,", "tokens": ["Die", "Braut", "ihm", "zu", "er\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weicht tausend Schritte er vom Leib,", "tokens": ["Weicht", "tau\u00b7send", "Schrit\u00b7te", "er", "vom", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er l\u00e4\u00dft sich nichts verhetzen.", "tokens": ["Er", "l\u00e4\u00dft", "sich", "nichts", "ver\u00b7het\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit achtzehn Jahr' hat er gefreit,", "tokens": ["Mit", "acht\u00b7zehn", "Jahr'", "hat", "er", "ge\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und damals war er grundgescheit.", "tokens": ["Und", "da\u00b7mals", "war", "er", "grund\u00b7ge\u00b7scheit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Studenten, unsere Zukunft einst", "tokens": ["Stu\u00b7den\u00b7ten", ",", "un\u00b7se\u00b7re", "Zu\u00b7kunft", "einst"], "token_info": ["word", 