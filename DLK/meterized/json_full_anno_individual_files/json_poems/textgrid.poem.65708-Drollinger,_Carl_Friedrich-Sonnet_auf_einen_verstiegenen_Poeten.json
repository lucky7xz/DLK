{"textgrid.poem.65708": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet auf einen verstiegenen Poeten", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch flammenschwangrer Schall der l\u00e4rmenden Trompeten,", "tokens": ["Welch", "flam\u00b7men\u00b7schwang\u00b7rer", "Schall", "der", "l\u00e4r\u00b7men\u00b7den", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch grasses Angstget\u00f6hn benebelt mein Geh\u00f6r!", "tokens": ["Welch", "gras\u00b7ses", "Angst\u00b7ge\u00b7t\u00f6hn", "be\u00b7ne\u00b7belt", "mein", "Ge\u00b7h\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Trommel schwarzer Klang entbrennt je mehr und mehr,", "tokens": ["Der", "Trom\u00b7mel", "schwar\u00b7zer", "Klang", "ent\u00b7brennt", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und macht den bangen Wall von blasser Furcht err\u00f6hten.", "tokens": ["Und", "macht", "den", "ban\u00b7gen", "Wall", "von", "blas\u00b7ser", "Furcht", "er\u00b7r\u00f6h\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der St\u00fccke Donner br\u00fcllt, gleich d\u00fcstern Blutcometen;", "tokens": ["Der", "St\u00fc\u00b7cke", "Don\u00b7ner", "br\u00fcllt", ",", "gleich", "d\u00fcs\u00b7tern", "Blut\u00b7co\u00b7me\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ha\u00fcser sind entseelt, die Tempel Athem-leer.", "tokens": ["Die", "Ha\u00fc\u00b7ser", "sind", "ent\u00b7seelt", ",", "die", "Tem\u00b7pel", "A\u00b7them\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wimmelt \u00fcberall der Leichen reges Heer,", "tokens": ["Es", "wim\u00b7melt", "\u00fc\u00b7be\u00b7rall", "der", "Lei\u00b7chen", "re\u00b7ges", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und f\u00fchrt ein Klaggeschrey von Jammer, Mord und T\u00f6dten.", "tokens": ["Und", "f\u00fchrt", "ein", "Klag\u00b7ge\u00b7schrey", "von", "Jam\u00b7mer", ",", "Mord", "und", "T\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich schau die arme Stadt, wie sie von Tr\u00e4hnen gl\u00fcht;", "tokens": ["Ich", "schau", "die", "ar\u00b7me", "Stadt", ",", "wie", "sie", "von", "Tr\u00e4h\u00b7nen", "gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir ihr zerst\u00fccktes Volk vor ihrem W\u00fcrger flieht,", "tokens": ["Wir", "ihr", "zer\u00b7st\u00fcck\u00b7tes", "Volk", "vor", "ih\u00b7rem", "W\u00fcr\u00b7ger", "flieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und eine W\u00fcsteney die \u00f6de Gegend dr\u00fccket.", "tokens": ["Und", "ei\u00b7ne", "W\u00fcs\u00b7te\u00b7ney", "die", "\u00f6\u00b7de", "Ge\u00b7gend", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verirrter Dichter, halt mit dem betr\u00fcbten Spiel!", "tokens": ["Ver\u00b7irr\u00b7ter", "Dich\u00b7ter", ",", "halt", "mit", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Spiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ungehe\u00fcre Sturm, der deine Stadt befiel,", "tokens": ["Der", "un\u00b7ge\u00b7he\u00fc\u00b7re", "Sturm", ",", "der", "dei\u00b7ne", "Stadt", "be\u00b7fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat auch dein bl\u00f6des Haupt getroffen und verr\u00fccket.", "tokens": ["Hat", "auch", "dein", "bl\u00f6\u00b7des", "Haupt", "ge\u00b7trof\u00b7fen", "und", "ver\u00b7r\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Welch flammenschwangrer Schall der l\u00e4rmenden Trompeten,", "tokens": ["Welch", "flam\u00b7men\u00b7schwang\u00b7rer", "Schall", "der", "l\u00e4r\u00b7men\u00b7den", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch grasses Angstget\u00f6hn benebelt mein Geh\u00f6r!", "tokens": ["Welch", "gras\u00b7ses", "Angst\u00b7ge\u00b7t\u00f6hn", "be\u00b7ne\u00b7belt", "mein", "Ge\u00b7h\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Trommel schwarzer Klang entbrennt je mehr und mehr,", "tokens": ["Der", "Trom\u00b7mel", "schwar\u00b7zer", "Klang", "ent\u00b7brennt", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und macht den bangen Wall von blasser Furcht err\u00f6hten.", "tokens": ["Und", "macht", "den", "ban\u00b7gen", "Wall", "von", "blas\u00b7ser", "Furcht", "er\u00b7r\u00f6h\u00b7ten", "."], "token_info": ["word", "word", "word", "word"