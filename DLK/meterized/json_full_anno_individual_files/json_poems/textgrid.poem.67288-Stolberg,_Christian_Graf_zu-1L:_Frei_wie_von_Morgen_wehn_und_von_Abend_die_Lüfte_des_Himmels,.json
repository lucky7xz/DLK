ord", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,", "KON", "PPER", "VVINF", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Wie aus Elysiums Hain, Tr\u00e4ume der Seligen ihn.", "tokens": ["Wie", "aus", "E\u00b7ly\u00b7si\u00b7ums", "Hain", ",", "Tr\u00e4u\u00b7me", "der", "Se\u00b7li\u00b7gen", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NE", "$,", "NN", "ART", "NN", "PPER", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ph\u00f6bus stieg aus dem Schoo\u00dfe der Thetis. Erbleichend verbarg sich", "tokens": ["Ph\u00f6\u00b7bus", "stieg", "aus", "dem", "Schoo\u00b7\u00dfe", "der", "The\u00b7tis", ".", "Er\u00b7blei\u00b7chend", "ver\u00b7barg", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "ART", "NE", "$.", "VVPP", "VVFIN", "PRF"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ph\u00f6be. Des Gl\u00fccklichen Aug' \u00f6ffnet sich, staunet umher;", "tokens": ["Ph\u00f6\u00b7be", ".", "Des", "Gl\u00fcck\u00b7li\u00b7chen", "Aug'", "\u00f6ff\u00b7net", "sich", ",", "stau\u00b7net", "um\u00b7her", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "VVFIN", "PRF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sonn' und Morgenschimmer und Meer und Wald und Gestade,", "tokens": ["Sonn'", "und", "Mor\u00b7gen\u00b7schim\u00b7mer", "und", "Meer", "und", "Wald", "und", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Murmelnder Bach im Thal, Alles ist hehr ihm und neu,", "tokens": ["Mur\u00b7meln\u00b7der", "Bach", "im", "Thal", ",", "Al\u00b7les", "ist", "hehr", "ihm", "und", "neu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$,", "PIS", "VAFIN", "ADV", "PPER", "KON", "ADJD", "$,"], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "F\u00fclle des wallenden Herzens beseelet ihm Alles mit Leben,", "tokens": ["F\u00fcl\u00b7le", "des", "wal\u00b7len\u00b7den", "Her\u00b7zens", "be\u00b7see\u00b7let", "ihm", "Al\u00b7les", "mit", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "H\u00fcgel und Ros' und Gew\u00f6lk \u2013 Freundinn ist Nachtigall ihm!", "tokens": ["H\u00fc\u00b7gel", "und", "Ros'", "und", "Ge\u00b7w\u00f6lk", "\u2013", "Freun\u00b7dinn", "ist", "Nach\u00b7ti\u00b7gall", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "KON", "NN", "$(", "NN", "VAFIN", "NN", "PPER", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}}, "stanza.9": {"line.1": {"text": "Also spendet auch Jene die Gaben, es ahnet dem Liebling", "tokens": ["Al\u00b7so", "spen\u00b7det", "auch", "Je\u00b7ne", "die", "Ga\u00b7ben", ",", "es", "ah\u00b7net", "dem", "Lieb\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht, wenn die Muse das Haupt ihm mit dem Lorbeer bekr\u00e4nzt \u2013", "tokens": ["Nicht", ",", "wenn", "die", "Mu\u00b7se", "das", "Haupt", "ihm", "mit", "dem", "Lor\u00b7beer", "be\u00b7kr\u00e4nzt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ward auch mir ein Bl\u00e4ttchen des Daphne-Baumes, so ist's nicht", "tokens": ["Ward", "auch", "mir", "ein", "Bl\u00e4tt\u00b7chen", "des", "Daph\u00b7ne\u00b7Bau\u00b7mes", ",", "so", "ist's", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "PTKNEG"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Garbe des Flei\u00dfes, nur Ihr dank' ich das Wiegengeschenk.", "tokens": ["Gar\u00b7be", "des", "Flei\u00b7\u00dfes", ",", "nur", "Ihr", "dank'", "ich", "das", "Wie\u00b7gen\u00b7ge\u00b7schenk", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Einst umg\u00fcrtete mir mit zarten H\u00e4nden die Sohlen", "tokens": ["Einst", "um\u00b7g\u00fcr\u00b7te\u00b7te", "mir", "mit", "zar\u00b7ten", "H\u00e4n\u00b7den", "die", "Soh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sie, und ich wagt' es, einher schritt ich auf hohem Kothurn.", "tokens": ["Sie", ",", "und", "ich", "wagt'", "es", ",", "ein\u00b7her", "schritt", "ich", "auf", "ho\u00b7hem", "Ko\u00b7thurn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "$,", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Mein Get\u00f6n' und was Sie in s\u00fc\u00dfer Umarmungen Zauber", "tokens": ["Mein", "Ge\u00b7t\u00f6n'", "und", "was", "Sie", "in", "s\u00fc\u00b7\u00dfer", "Um\u00b7ar\u00b7mun\u00b7gen", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PWS", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Fl\u00fcsterte Ihm, der mir ist mehr noch als Bruder und Freund,", "tokens": ["Fl\u00fcs\u00b7ter\u00b7te", "Ihm", ",", "der", "mir", "ist", "mehr", "noch", "als", "Bru\u00b7der", "und", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "VAFIN", "ADV", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+---+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Nimm es zum Angebinde mein S\u00f6hnlein! \u2013Siehe das Alter", "tokens": ["Nimm", "es", "zum", "An\u00b7ge\u00b7bin\u00b7de", "mein", "S\u00f6hn\u00b7lein", "!", "\u2013", "Sie\u00b7he", "das", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$.", "$(", "VVIMP", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ist redselig, und schon eifr' ich dem Nestor ja nach.", "tokens": ["Ist", "red\u00b7se\u00b7lig", ",", "und", "schon", "eifr'", "ich", "dem", "Nes\u00b7tor", "ja", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "War nicht Er, de\u00df Enkel Du bist, mein Lebensgef\u00e4hrte?", "tokens": ["War", "nicht", "Er", ",", "de\u00df", "En\u00b7kel", "Du", "bist", ",", "mein", "Le\u00b7bens\u00b7ge\u00b7f\u00e4hr\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PPER", "$,", "ART", "NN", "PPER", "VAFIN", "$,", "PPOSAT", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ach, mein Bernstorff! Er, von Millionen beweint!", "tokens": ["Ach", ",", "mein", "Bern\u00b7storff", "!", "Er", ",", "von", "Mil\u00b7lion\u00b7en", "be\u00b7weint", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$.", "PPER", "$,", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Bernstorff! Er, dem wie wenigen Auserw\u00e4hlten, in F\u00fclle", "tokens": ["Bern\u00b7storff", "!", "Er", ",", "dem", "wie", "we\u00b7ni\u00b7gen", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ",", "in", "F\u00fcl\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "$,", "PRELS", "KOKOM", "PIAT", "NN", "$,", "APPR", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "So des Sch\u00f6pfers Hauch Weihe des Himmels verlieh! \u2013", "tokens": ["So", "des", "Sch\u00f6p\u00b7fers", "Hauch", "Wei\u00b7he", "des", "Him\u00b7mels", "ver\u00b7lieh", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "NN", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "H\u00f6re nun! Wird's Dir im Herzen so warm, so voll und so dr\u00e4ngend,", "tokens": ["H\u00f6\u00b7re", "nun", "!", "Wird's", "Dir", "im", "Her\u00b7zen", "so", "warm", ",", "so", "voll", "und", "so", "dr\u00e4n\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+--++-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Steigt aus der Tiefe Dir Stern, Stern und Gestirn Dir empor,", "tokens": ["Steigt", "aus", "der", "Tie\u00b7fe", "Dir", "Stern", ",", "Stern", "und", "Ge\u00b7stirn", "Dir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Dann ist nicht ferne die s\u00fc\u00dfe S\u00e4ngerinn! Lausche dem Fittig',", "tokens": ["Dann", "ist", "nicht", "fer\u00b7ne", "die", "s\u00fc\u00b7\u00dfe", "S\u00e4n\u00b7ge\u00b7rinn", "!", "Lau\u00b7sche", "dem", "Fit\u00b7tig'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Oeffne das Pf\u00f6rtchen und f\u00fchr' ein in das K\u00e4mmerchen Sie!", "tokens": ["Oeff\u00b7ne", "das", "Pf\u00f6rt\u00b7chen", "und", "f\u00fchr'", "ein", "in", "das", "K\u00e4m\u00b7mer\u00b7chen", "Sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "APPR", "ART", "NN", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Einsam die Einsame! \u2013 Rauscht dir im Taubenschlag oben Gewimmel,", "tokens": ["Ein\u00b7sam", "die", "Ein\u00b7sa\u00b7me", "!", "\u2013", "Rauscht", "dir", "im", "Tau\u00b7ben\u00b7schlag", "o\u00b7ben", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$.", "$(", "NN", "PPER", "APPRART", "NN", "ADV", "NN", "$,"], "meter": "+--+--+--+-+---+-", "measure": "dactylic.tri.plus"}, "line.14": {"text": "La\u00df in die Wildni\u00df hinaus flattern den luftigen Schwarm!", "tokens": ["La\u00df", "in", "die", "Wild\u00b7ni\u00df", "hin\u00b7aus", "flat\u00b7tern", "den", "luf\u00b7ti\u00b7gen", "Schwarm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "APZR", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Jene stammt aus G\u00f6ttergeschlecht, und vom h\u00f6heren", "tokens": ["Je\u00b7ne", "stammt", "aus", "G\u00f6t\u00b7ter\u00b7ge\u00b7schlecht", ",", "und", "vom", "h\u00f6\u00b7he\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "KON", "APPRART", "ADJA"], "meter": "+-+-+--+--+--", "measure": "trochaic.penta.relaxed"}}}}}