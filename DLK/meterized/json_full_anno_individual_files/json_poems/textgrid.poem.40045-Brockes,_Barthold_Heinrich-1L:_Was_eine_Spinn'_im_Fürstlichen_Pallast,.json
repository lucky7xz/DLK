{"textgrid.poem.40045": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was eine Spinn' im F\u00fcrstlichen Pallast,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was eine Spinn' im F\u00fcrstlichen Pallast,", "tokens": ["Was", "ei\u00b7ne", "Spinn'", "im", "F\u00fcrst\u00b7li\u00b7chen", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Sammt und Marmor schm\u00fcckt, Gold, Purpur und Damast,", "tokens": ["Den", "Sammt", "und", "Mar\u00b7mor", "schm\u00fcckt", ",", "Gold", ",", "Pur\u00b7pur", "und", "Da\u00b7mast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Die alle Pracht f\u00fcr nichtes sch\u00e4tzet,", "tokens": ["Die", "al\u00b7le", "Pracht", "f\u00fcr", "nich\u00b7tes", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihr bestaubtes Netz, und anders nichts, gef\u00e4llt,", "tokens": ["Der", "ihr", "be\u00b7staub\u00b7tes", "Netz", ",", "und", "an\u00b7ders", "nichts", ",", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$,", "KON", "ADV", "PIS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.