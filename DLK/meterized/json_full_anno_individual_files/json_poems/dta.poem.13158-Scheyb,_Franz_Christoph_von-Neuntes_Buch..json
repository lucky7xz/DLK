{"dta.poem.13158": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Thalia war bereit: sie stellte wieder dar,", "tokens": ["Tha\u00b7lia", "war", "be\u00b7reit", ":", "sie", "stell\u00b7te", "wie\u00b7der", "dar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Was von der Sonne Glanz vor ihr erw\u00e4hnet war.", "tokens": ["Was", "von", "der", "Son\u00b7ne", "Glanz", "vor", "ihr", "er\u00b7w\u00e4h\u00b7net", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "235\u201dMan sprach \u201d, erhohlte sie, wie sich die Sterne regen;", "tokens": ["\"", "Man", "sprach", "\"", ",", "er\u00b7hohl\u00b7te", "sie", ",", "wie", "sich", "die", "Ster\u00b7ne", "re\u00b7gen", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "$(", "$,", "VVFIN", "PPER", "$,", "PWAV", "PRF", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewie sie sich Tag und Nacht herum zu drehen pflegen:", "tokens": ["\u201e", "wie", "sie", "sich", "Tag", "und", "Nacht", "he\u00b7rum", "zu", "dre\u00b7hen", "pfle\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PRF", "NN", "KON", "NN", "APZR", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewie dort der Sonne Ball, und hier des Mondes Rund", "tokens": ["\u201e", "wie", "dort", "der", "Son\u00b7ne", "Ball", ",", "und", "hier", "des", "Mon\u00b7des", "Rund"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "NN", "$,", "KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edurch niemahls m\u00fcden Lauf den Wechsel jeder Stund,", "tokens": ["\u201e", "durch", "nie\u00b7mahls", "m\u00fc\u00b7den", "Lauf", "den", "Wech\u00b7sel", "je\u00b7der", "Stund", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eder Jahre, Monate, der Tage, N\u00e4chte weisen:", "tokens": ["\u201e", "der", "Jah\u00b7re", ",", "Mo\u00b7na\u00b7te", ",", "der", "Ta\u00b7ge", ",", "N\u00e4ch\u00b7te", "wei\u00b7sen", ":"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "NN", "$,", "ART", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "240\u201dWie wir der Erde Scho\u00df der Nahrung halber preisen.", "tokens": ["\"", "Wie", "wir", "der", "Er\u00b7de", "Scho\u00df", "der", "Nah\u00b7rung", "hal\u00b7ber", "prei\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "NN", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}