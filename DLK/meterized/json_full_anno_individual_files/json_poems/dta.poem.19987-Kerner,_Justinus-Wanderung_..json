{"dta.poem.19987": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Wanderung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf und froh gewandert", "tokens": ["Wohl\u00b7auf", "und", "froh", "ge\u00b7wan\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In's unbekannte Land!", "tokens": ["In's", "un\u00b7be\u00b7kann\u00b7te", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerrissen, ach! zerrissen", "tokens": ["Zer\u00b7ris\u00b7sen", ",", "ach", "!", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["VVPP", "$,", "ITJ", "$.", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist manches theure Band.", "tokens": ["Ist", "man\u00b7ches", "theu\u00b7re", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr heimatlichen Kreutze", "tokens": ["Ihr", "hei\u00b7mat\u00b7li\u00b7chen", "Kreut\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo ich oft betend lag,", "tokens": ["Wo", "ich", "oft", "be\u00b7tend", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr B\u00e4ume, ach ihr H\u00fcgel", "tokens": ["Ihr", "B\u00e4u\u00b7me", ",", "ach", "ihr", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "XY", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O blickt mir segnend nach!", "tokens": ["O", "blickt", "mir", "seg\u00b7nend", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch schl\u00e4ft die weite Erde,", "tokens": ["Noch", "schl\u00e4ft", "die", "wei\u00b7te", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Vogel weckt den Hain,", "tokens": ["Kein", "Vo\u00b7gel", "weckt", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch bin ich nicht verlassen,", "tokens": ["Doch", "bin", "ich", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch bin ich nicht allein:", "tokens": ["Doch", "bin", "ich", "nicht", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn ach auf meinem Herzen", "tokens": ["Denn", "ach", "auf", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trag ich ihr theures Pfand,", "tokens": ["Trag", "ich", "ihr", "theu\u00b7res", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchl's und Erd' und Himmel", "tokens": ["Ich", "f\u00fchl's", "und", "Erd'", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind innig mir verwandt.", "tokens": ["Sind", "in\u00b7nig", "mir", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}