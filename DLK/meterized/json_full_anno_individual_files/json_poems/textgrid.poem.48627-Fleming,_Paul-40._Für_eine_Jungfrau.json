{"textgrid.poem.48627": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "40. F\u00fcr eine Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mai, der k\u00f6mmt gegangen", "tokens": ["Der", "Mai", ",", "der", "k\u00f6mmt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und hat die sch\u00f6nen Wangen", "tokens": ["und", "hat", "die", "sch\u00f6\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "mit Blumen ausgemalt.", "tokens": ["mit", "Blu\u00b7men", "aus\u00b7ge\u00b7malt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das Leid der langen Fr\u00f6ste", "tokens": ["Das", "Leid", "der", "lan\u00b7gen", "Fr\u00f6s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "wird durch die warmen Weste", "tokens": ["wird", "durch", "die", "war\u00b7men", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "mit Wollust reich bezahlt.", "tokens": ["mit", "Wol\u00b7lust", "reich", "be\u00b7zahlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auch euer Tag, der liebe,", "tokens": ["Auch", "eu\u00b7er