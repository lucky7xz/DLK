{"textgrid.poem.60355": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Salzburger Kirchhof", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6ner Ort, den Toten auserkoren", "tokens": ["O", "sch\u00f6\u00b7ner", "Ort", ",", "den", "To\u00b7ten", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Ruhest\u00e4tte f\u00fcr die m\u00fcden Glieder!", "tokens": ["Zur", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "f\u00fcr", "die", "m\u00fc\u00b7den", "Glie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier singt der Fr\u00fchling Auferstehungslieder,", "tokens": ["Hier", "singt", "der", "Fr\u00fch\u00b7ling", "Auf\u00b7er\u00b7ste\u00b7hungs\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom treuen Sonnenblick zur\u00fcckbeschworen.", "tokens": ["Vom", "treu\u00b7en", "Son\u00b7nen\u00b7blick", "zu\u00b7r\u00fcck\u00b7be\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn alle Schmerzen auch ein Herz durchbohren,", "tokens": ["Wenn", "al\u00b7le", "Schmer\u00b7zen", "auch", "ein", "Herz", "durch\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem man sein Liebstes senkt zur Grube nieder,", "tokens": ["Dem", "man", "sein", "Liebs\u00b7tes", "senkt", "zur", "Gru\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch glaubt es leichter hier: wir sehn uns wieder,", "tokens": ["Doch", "glaubt", "es", "leich\u00b7ter", "hier", ":", "wir", "sehn", "uns", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sind die Toten uns nicht ganz verloren.", "tokens": ["Es", "sind", "die", "To\u00b7ten", "uns", "nicht", "ganz", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der fremde Wandrer, kommend aus der Ferne,", "tokens": ["Der", "frem\u00b7de", "Wand\u00b7rer", ",", "kom\u00b7mend", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem hier kein Gl\u00fcck vermodert, weilt doch gerne", "tokens": ["Dem", "hier", "kein", "Gl\u00fcck", "ver\u00b7mo\u00b7dert", ",", "weilt", "doch", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "PIAT", "NN", "VVPP", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier, wo die Sch\u00f6nheit H\u00fcterin der Toten.", "tokens": ["Hier", ",", "wo", "die", "Sch\u00f6n\u00b7heit", "H\u00fc\u00b7te\u00b7rin", "der", "To\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie schlafen tief und sanft in ihren Armen,", "tokens": ["Sie", "schla\u00b7fen", "tief", "und", "sanft", "in", "ih\u00b7ren", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Worin zu neuem Leben sie erwarmen;", "tokens": ["Wo\u00b7rin", "zu", "neu\u00b7em", "Le\u00b7ben", "sie", "er\u00b7war\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Blumen winkens, ihre stillen Boten.", "tokens": ["Die", "Blu\u00b7men", "win\u00b7kens", ",", "ih\u00b7re", "stil\u00b7len", "Bo\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O sch\u00f6ner Ort, den Toten auserkoren", "tokens": ["O", "sch\u00f6\u00b7ner", "Ort", ",", "den", "To\u00b7ten", "au\u00b7ser\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.pent