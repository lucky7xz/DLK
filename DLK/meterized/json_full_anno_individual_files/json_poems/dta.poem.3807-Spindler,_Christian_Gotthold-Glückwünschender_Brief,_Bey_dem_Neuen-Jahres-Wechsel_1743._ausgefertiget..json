{"dta.poem.3807": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschender Brief,  \n Bey dem Neuen-Jahres-Wechsel 1743. ausgefertiget.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Dir, Wehrter G\u00f6nner, sucht mein Geist", "tokens": ["Dir", ",", "Wehr\u00b7ter", "G\u00f6n\u00b7ner", ",", "sucht", "mein", "Geist"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied mit W\u00fcnschen zu entrichten.", "tokens": ["Ein", "Lied", "mit", "W\u00fcn\u00b7schen", "zu", "ent\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil mich die Zeit darauff verweist,", "tokens": ["Weil", "mich", "die", "Zeit", "dar\u00b7auff", "ver\u00b7weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich nach derselben dichten.", "tokens": ["So", "will", "ich", "nach", "der\u00b7sel\u00b7ben", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verzeyh, wenn Kunst und Nachdruck fehlt,", "tokens": ["Ver\u00b7zeyh", ",", "wenn", "Kunst", "und", "Nach\u00b7druck", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gnug, da\u00df sich Hertz und Wunsch aus Redlig-", "tokens": ["Gnug", ",", "da\u00df", "sich", "Hertz", "und", "Wunsch", "aus", "Red\u00b7lig"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PRF", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "keit verm\u00e4hlt.", "tokens": ["keit", "ver\u00b7m\u00e4hlt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Das alte Jahr ist nun vorbey,", "tokens": ["Das", "al\u00b7te", "Jahr", "ist", "nun", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das neue gl\u00fccklich angefangen;", "tokens": ["Das", "neu\u00b7e", "gl\u00fcck\u00b7lich", "an\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da Freund und Stadt dich wie aufs neu", "tokens": ["Da", "Freund", "und", "Stadt", "dich", "wie", "aufs", "neu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "KOKOM", "APPRART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ihren Schutz und Trost empfangen.", "tokens": ["Als", "ih\u00b7ren", "Schutz", "und", "Trost", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der H\u00f6chste sey gelobt, geehrt,", "tokens": ["Der", "H\u00f6chs\u00b7te", "sey", "ge\u00b7lobt", ",", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Jahre, G\u00fcte, Heil, u. Gl\u00fcck, und Kr\u00e4ffte mehrt.", "tokens": ["Der", "Jah\u00b7re", ",", "G\u00fc\u00b7te", ",", "Heil", ",", "u.", "Gl\u00fcck", ",", "und", "Kr\u00e4ff\u00b7te", "mehrt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "abbreviation", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Gott r\u00fcste dich noch ferner aus,", "tokens": ["Gott", "r\u00fcs\u00b7te", "dich", "noch", "fer\u00b7ner", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Werck mit Ruhm und Lust zu treiben!", "tokens": ["Sein", "Werck", "mit", "Ruhm", "und", "Lust", "zu", "trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er la\u00df dein hohes Priester Hau\u00df", "tokens": ["Er", "la\u00df", "dein", "ho\u00b7hes", "Pries\u00b7ter", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch lange Zeit im Seegen bleiben!", "tokens": ["Noch", "lan\u00b7ge", "Zeit", "im", "See\u00b7gen", "blei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm immer mehr an Krafft und Ruh,", "tokens": ["Nimm", "im\u00b7mer", "mehr", "an", "Krafft", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesundheit, Alter, Wohl, und an Vergn\u00fcgen zu.", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Al\u00b7ter", ",", "Wohl", ",", "und", "an", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "$,", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Hoffnung sieht es als geschehn,", "tokens": ["Die", "Hoff\u00b7nung", "sieht", "es", "als", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst in sp\u00e4te Zeiten gr\u00fcnen.", "tokens": ["Du", "wirst", "in", "sp\u00e4\u00b7te", "Zei\u00b7ten", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Geist verspricht sichs selbst zu sehn,", "tokens": ["Mein", "Geist", "ver\u00b7spricht", "sichs", "selbst", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht sich deiner zu bedienen,", "tokens": ["Und", "w\u00fcnscht", "sich", "dei\u00b7ner", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wei\u00df, was noch der Himmel schickt,", "tokens": ["Wer", "wei\u00df", ",", "was", "noch", "der", "Him\u00b7mel", "schickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ob mir nicht bald dein Rath und treuer Beystand", "tokens": ["Ob", "mir", "nicht", "bald", "dein", "Rath", "und", "treu\u00b7er", "Beys\u00b7tand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "gl\u00fcckt.", "tokens": ["gl\u00fcckt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "So h\u00e4tte denn mein matter Kiel", "tokens": ["So", "h\u00e4t\u00b7te", "denn", "mein", "mat\u00b7ter", "Kiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schuld in etwas abgetragen.", "tokens": ["Die", "Schuld", "in", "et\u00b7was", "ab\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein ungewohntes S\u00e4yten-Spiel", "tokens": ["Mein", "un\u00b7ge\u00b7wohn\u00b7tes", "S\u00e4y\u00b7ten\u00b7Spiel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df billig nach Erlaubni\u00df fragen.", "tokens": ["Mu\u00df", "bil\u00b7lig", "nach", "Er\u00b7laub\u00b7ni\u00df", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df du r\u00e4umst mein Bitten ein,", "tokens": ["Ich", "wei\u00df", "du", "r\u00e4umst", "mein", "Bit\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wirst auch k\u00fcnfftighin mein Freund und G\u00f6n-", "tokens": ["Und", "wirst", "auch", "k\u00fcnff\u00b7tig\u00b7hin", "mein", "Freund", "und", "G\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "ner seyn.", "tokens": ["ner", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}