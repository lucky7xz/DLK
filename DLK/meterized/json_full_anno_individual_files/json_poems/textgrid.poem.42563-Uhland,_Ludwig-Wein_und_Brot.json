{"textgrid.poem.42563": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Wein und Brot", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solche D\u00fcfte sind mein Leben,", "tokens": ["Sol\u00b7che", "D\u00fcf\u00b7te", "sind", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verscheuchen all mein Leid:", "tokens": ["Die", "ver\u00b7scheu\u00b7chen", "all", "mein", "Leid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00fchen auf dem Berg die Reben,", "tokens": ["Bl\u00fc\u00b7hen", "auf", "dem", "Berg", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcht im Tale das Getreid.", "tokens": ["Bl\u00fcht", "im", "Ta\u00b7le", "das", "Ge\u00b7treid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Donnern werden bald die Tennen,", "tokens": ["Don\u00b7nern", "wer\u00b7den", "bald", "die", "Ten\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald die M\u00fchlen rauschend gehn,", "tokens": ["Bald", "die", "M\u00fch\u00b7len", "rau\u00b7schend", "gehn", ","], "token_info": ["word", "word", "