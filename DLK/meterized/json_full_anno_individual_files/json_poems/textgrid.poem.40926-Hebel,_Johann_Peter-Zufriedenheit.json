{"textgrid.poem.40926": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Zufriedenheit", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s schwimmt menge Ma im \u00dcberflu\u00df", "tokens": ["'s", "schwimmt", "men\u00b7ge", "Ma", "im", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "NE", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "het Huus und Hof und Geld", "tokens": ["het", "Huus", "und", "Hof", "und", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und wenig Freud und viel Verdru\u00df", "tokens": ["und", "we\u00b7nig", "Freud", "und", "viel", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und Sorgen in der Welt.", "tokens": ["und", "Sor\u00b7gen", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und het er viel, se gehrt er viel", "tokens": ["Und", "het", "er", "viel", ",", "se", "gehrt", "er", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und neeft und grumset allewil.", "tokens": ["und", "neeft", "und", "grum\u00b7set", "al\u00b7le\u00b7wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"]