{"dta.poem.2281": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bitte um ein l\u00e4ngeres Leben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lieber Tod! du wirst dich irren;", "tokens": ["Lie\u00b7ber", "Tod", "!", "du", "wirst", "dich", "ir\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchst du etwa meinen Nachbar", "tokens": ["Suchst", "du", "et\u00b7wa", "mei\u00b7nen", "Nach\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit dem alten krummen R\u00fckken?", "tokens": ["Mit", "dem", "al\u00b7ten", "krum\u00b7men", "R\u00fck\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geh nur hin! er wohnt zur Rechten;", "tokens": ["Geh", "nur", "hin", "!", "er", "wohnt", "zur", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geh nur hin! du wirst ihn finden,", "tokens": ["Geh", "nur", "hin", "!", "du", "wirst", "ihn", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und er hat dich schon gerufen.", "tokens": ["Und", "er", "hat", "dich", "schon", "ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieber Tod! du wirst dich irren;", "tokens": ["Lie\u00b7ber", "Tod", "!", "du", "wirst", "dich", "ir\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lieber Tod! geh doch nur weiter.", "tokens": ["Lie\u00b7ber", "Tod", "!", "geh", "doch", "nur", "wei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da! hier ist die ganze Tasche!", "tokens": ["Da", "!", "hier", "ist", "die", "gan\u00b7ze", "Ta\u00b7sche", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Alle diese Schwanzdukaten", "tokens": ["Al\u00b7le", "die\u00b7se", "Schwanz\u00b7du\u00b7ka\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PIS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hab ich einst f\u00fcr dich gewechselt.", "tokens": ["Hab", "ich", "einst", "f\u00fcr", "dich", "ge\u00b7wech\u00b7selt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nimm sie hin, und geh nur weiter.", "tokens": ["Nimm", "sie", "hin", ",", "und", "geh", "nur", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00f6rst du nicht den Nachbar rufen?", "tokens": ["H\u00f6rst", "du", "nicht", "den", "Nach\u00b7bar", "ru\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Hol ihn nur, er wird dirs danken.", "tokens": ["Hol", "ihn", "nur", ",", "er", "wird", "dirs", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Tod! du irrst dich; soll ich schweren?", "tokens": ["Tod", "!", "du", "irrst", "dich", ";", "soll", "ich", "schwe\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Nein, wir wollen uns nicht zanken.", "tokens": ["Nein", ",", "wir", "wol\u00b7len", "uns", "nicht", "zan\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Sieh! hier sind noch mehr Dukaten.", "tokens": ["Sieh", "!", "hier", "sind", "noch", "mehr", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Sei so gut, wie unsre Richter.", "tokens": ["Sei", "so", "gut", ",", "wie", "uns\u00b7re", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "La\u00df dich doch nicht l\u00e4nger bitten!", "tokens": ["La\u00df", "dich", "doch", "nicht", "l\u00e4n\u00b7ger", "bit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Nimm das Gold, und la\u00df mich leben.", "tokens": ["Nimm", "das", "Gold", ",", "und", "la\u00df", "mich", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wilst du nicht, so la\u00df es bleiben;", "tokens": ["Wilst", "du", "nicht", ",", "so", "la\u00df", "es", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "La\u00df mich nur noch einmal k\u00fcssen.", "tokens": ["La\u00df", "mich", "nur", "noch", "ein\u00b7mal", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}