{"dta.poem.19990": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner auf der H\u00f6he .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verla\u00df die kalten H\u00f6hen", "tokens": ["Ver\u00b7la\u00df", "die", "kal\u00b7ten", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du armer G\u00e4rtnersmann!", "tokens": ["Du", "ar\u00b7mer", "G\u00e4rt\u00b7ners\u00b7mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Garten steht voll Moose,", "tokens": ["Dein", "Gar\u00b7ten", "steht", "voll", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Hyacinth', nicht Rose", "tokens": ["Nicht", "Hya\u00b7cinth'", ",", "nicht", "Ro\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Man in ihm finden kann.", "tokens": ["Man", "in", "ihm", "fin\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im warmen Thale unten", "tokens": ["Im", "war\u00b7men", "Tha\u00b7le", "un\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich der G\u00e4rten viel,", "tokens": ["Sah", "ich", "der", "G\u00e4r\u00b7ten", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen steh'n in F\u00fclle", "tokens": ["Die", "Blu\u00b7men", "steh'n", "in", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihre bunte H\u00fclle", "tokens": ["Und", "ih\u00b7re", "bun\u00b7te", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gew\u00e4hrt ein lustig Spiel.", "tokens": ["Ge\u00b7w\u00e4hrt", "ein", "lus\u00b7tig", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Garten auf der H\u00f6he", "tokens": ["Im", "Gar\u00b7ten", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist schon die Bl\u00fcthe aus.", "tokens": ["Ist", "schon", "die", "Bl\u00fc\u00b7the", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00f6cht ihrer nimmer warten,", "tokens": ["M\u00f6cht", "ih\u00b7rer", "nim\u00b7mer", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Alter! verla\u00df den Garten,", "tokens": ["Al\u00b7ter", "!", "ver\u00b7la\u00df", "den", "Gar\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dein armbestelltes Haus!", "tokens": ["Dein", "arm\u00b7be\u00b7stell\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der G\u00e4rtner gab nicht Rede", "tokens": ["Der", "G\u00e4rt\u00b7ner", "gab", "nicht", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Wand'rer aus dem Thal,", "tokens": ["Dem", "Wan\u00b7d'\u00b7rer", "aus", "dem", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Blieb still wie tr\u00e4umend stehen", "tokens": ["Blieb", "still", "wie", "tr\u00e4u\u00b7mend", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KOKOM", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis da\u00df voll Gluth die H\u00f6hen", "tokens": ["Bis", "da\u00df", "voll", "Gluth", "die", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im letzten Abendstrahl,", "tokens": ["Im", "letz\u00b7ten", "A\u00b7bend\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bis Nacht in enger Tiefe,", "tokens": ["Bis", "Nacht", "in", "en\u00b7ger", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Erde rings verschwand,", "tokens": ["Die", "Er\u00b7de", "rings", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Goldwolken sich erhoben,", "tokens": ["Gold\u00b7wol\u00b7ken", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seltsame Bilder woben,", "tokens": ["Selt\u00b7sa\u00b7me", "Bil\u00b7der", "wo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ein seelig' Zauberland.", "tokens": ["Ein", "see\u00b7lig'", "Zau\u00b7ber\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort, Fremder! steht mein Garten", "tokens": ["Dort", ",", "Frem\u00b7der", "!", "steht", "mein", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach drauf der G\u00e4rtnersmann,", "tokens": ["Sprach", "drauf", "der", "G\u00e4rt\u00b7ners\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo sind die kalten Moose?", "tokens": ["Wo", "sind", "die", "kal\u00b7ten", "Moo\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieh Hyacinth und Rose", "tokens": ["Sieh", "Hya\u00b7cin\u00b7th", "und", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf himmelblauem Plan!", "tokens": ["Auf", "him\u00b7mel\u00b7blau\u00b7em", "Plan", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und sieh von Gold erbauet", "tokens": ["Und", "sieh", "von", "Gold", "er\u00b7bau\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein herrlich K\u00f6nighaus,", "tokens": ["Ein", "herr\u00b7lich", "K\u00f6\u00b7nig\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sterne dr\u00fcber stehen", "tokens": ["Die", "Ster\u00b7ne", "dr\u00fc\u00b7ber", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gluthroth die Wimpel wehen,", "tokens": ["Gluth\u00b7roth", "die", "Wim\u00b7pel", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drinn geh' ich ein und aus.", "tokens": ["Drinn", "geh'", "ich", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}