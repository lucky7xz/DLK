{"textgrid.poem.43285": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich zog dann fort, und als ich wiederkam,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich zog dann fort, und als ich wiederkam,", "tokens": ["Ich", "zog", "dann", "fort", ",", "und", "als", "ich", "wie\u00b7der\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "War leer das Haus ... Die Alten l\u00e4ngst gestorben,", "tokens": ["War", "leer", "das", "Haus", "...", "Die", "Al\u00b7ten", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das blonde Kind weit in die Welt gegangen ...", "tokens": ["Das", "blon\u00b7de", "Kind", "weit", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich mu\u00dfte lange \u2013 lange \u2013 lange suchen,", "tokens": ["Ich", "mu\u00df\u00b7te", "lan\u00b7ge", "\u2013", "lan\u00b7ge", "\u2013", "lan\u00b7ge", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$(", "ADV", "$(", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis ich Dich fand ...", "tokens": ["Bis", "ich", "Dich", "fand", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bei harten Menschen fand ich wieder Dich,", "tokens": ["Bei", "har\u00b7ten", "Men\u00b7schen", "fand", "ich", "wie\u00b7der", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bei harter Arbeit ... Ohne Wunsch und Klage,", "tokens": ["Bei", "har\u00b7ter", "Ar\u00b7beit", "...", "Oh\u00b7ne", "Wunsch", "und", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So m\u00fcd und einsam, ohne Gl\u00fcck und Jugend ...", "tokens": ["So", "m\u00fcd", "und", "ein\u00b7sam", ",", "oh\u00b7ne", "Gl\u00fcck", "und", "Ju\u00b7gend", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "KOUI", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da kam die Stunde, wo Dich innig liebte", "tokens": ["Da", "kam", "die", "Stun\u00b7de", ",", "wo", "Dich", "in\u00b7nig", "lieb\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mein starkes Herz!", "tokens": ["Mein", "star\u00b7kes", "Herz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wo ich, der Armuth und der Arbeit Sohn,", "tokens": ["Wo", "ich", ",", "der", "Ar\u00b7muth", "und", "der", "Ar\u00b7beit", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Um Dich, Du bleiches Kind des Elends, freite,", "tokens": ["Um", "Dich", ",", "Du", "blei\u00b7ches", "Kind", "des", "E\u00b7lends", ",", "frei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das mich nicht liebte, aber mir vertraute", "tokens": ["Das", "mich", "nicht", "lieb\u00b7te", ",", "a\u00b7ber", "mir", "ver\u00b7trau\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und vor mir stand voll Schreck und scheuem Zagen", "tokens": ["Und", "vor", "mir", "stand", "voll", "Schreck", "und", "scheu\u00b7em", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und weinend schwieg. \u2013", "tokens": ["Und", "wei\u00b7nend", "schwieg", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Doch als Du sp\u00e4ter Deine liebe Hand", "tokens": ["Doch", "als", "Du", "sp\u00e4\u00b7ter", "Dei\u00b7ne", "lie\u00b7be", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor dem Altare legtest in die meine,", "tokens": ["Vor", "dem", "Al\u00b7ta\u00b7re", "leg\u00b7test", "in", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ich f\u00fcnf Treppen hoch Dich junges Wesen", "tokens": ["Als", "ich", "f\u00fcnf", "Trep\u00b7pen", "hoch", "Dich", "jun\u00b7ges", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN", "ADJD", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herauftrug in die luftige Hochzeitskammer,", "tokens": ["Her\u00b7auf\u00b7trug", "in", "die", "luf\u00b7ti\u00b7ge", "Hoch\u00b7zeits\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da war ich stolz!", "tokens": ["Da", "war", "ich", "stolz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Viel stolzer als ein m\u00e4chtiger F\u00fcrst,", "tokens": ["Viel", "stol\u00b7zer", "als", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der seine Braut heimf\u00fchrt in goldne S\u00e4le ...", "tokens": ["Der", "sei\u00b7ne", "Braut", "heim\u00b7f\u00fchrt", "in", "gold\u00b7ne", "S\u00e4\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du blinzelst, sch\u00fcttelst kichernd Deine Locken,", "tokens": ["Du", "blin\u00b7zelst", ",", "sch\u00fct\u00b7telst", "ki\u00b7chernd", "Dei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil ich von jenem Tage wieder tr\u00e4ume", "tokens": ["Weil", "ich", "von", "je\u00b7nem", "Ta\u00b7ge", "wie\u00b7der", "tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Vollmondlicht ...", "tokens": ["Im", "Voll\u00b7mond\u00b7licht", "..."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Weil ich die reiche Seligkeit,", "tokens": ["Weil", "ich", "die", "rei\u00b7che", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die damals mir geworden, ganz durchschwelge;", "tokens": ["Die", "da\u00b7mals", "mir", "ge\u00b7wor\u00b7den", ",", "ganz", "durch\u00b7schwel\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VAPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch horche nur, Du blonde Ueberkluge:", "tokens": ["Doch", "hor\u00b7che", "nur", ",", "Du", "blon\u00b7de", "Ue\u00b7ber\u00b7klu\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das H\u00e4uschen, wo als Kinder wir oft spielten,", "tokens": ["Das", "H\u00e4usc\u00b7hen", ",", "wo", "als", "Kin\u00b7der", "wir", "oft", "spiel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "KOUS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schenk' ich Dir einst.", "tokens": ["Schenk'", "ich", "Dir", "einst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Vielleicht schon morgen kommt das Gl\u00fcck herauf", "tokens": ["Viel\u00b7leicht", "schon", "mor\u00b7gen", "kommt", "das", "Gl\u00fcck", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sch\u00fcttet gelbes Gold in unsere H\u00e4nde ...", "tokens": ["Und", "sch\u00fct\u00b7tet", "gel\u00b7bes", "Gold", "in", "un\u00b7se\u00b7re", "H\u00e4n\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vielleicht bleibt es noch fort die kurze Weile", "tokens": ["Viel\u00b7leicht", "bleibt", "es", "noch", "fort", "die", "kur\u00b7ze", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und kommt dann ungesehen angeflogen", "tokens": ["Und", "kommt", "dann", "un\u00b7ge\u00b7se\u00b7hen", "an\u00b7ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ganz ohne Gold ...", "tokens": ["Ganz", "oh\u00b7ne", "Gold", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und doch das ganze Gl\u00fcck! ... Mich d\u00fcnkt, ich h\u00f6r'", "tokens": ["Und", "doch", "das", "gan\u00b7ze", "Gl\u00fcck", "!", "...", "Mich", "d\u00fcnkt", ",", "ich", "h\u00f6r'"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon seinen Fl\u00fcgelschlag in solchen N\u00e4chten,", "tokens": ["Schon", "sei\u00b7nen", "Fl\u00fc\u00b7gel\u00b7schlag", "in", "sol\u00b7chen", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine feine Kinderstimme fl\u00fcstern:", "tokens": ["Und", "ei\u00b7ne", "fei\u00b7ne", "Kin\u00b7der\u00b7stim\u00b7me", "fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald wirst Du mich in Deinen Armen halten,", "tokens": ["Bald", "wirst", "Du", "mich", "in", "Dei\u00b7nen", "Ar\u00b7men", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin das Gl\u00fcck! ...", "tokens": ["Ich", "bin", "das", "Gl\u00fcck", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Bis dahin aber la\u00df mein dunkles Haupt", "tokens": ["Bis", "da\u00b7hin", "a\u00b7ber", "la\u00df", "mein", "dunk\u00b7les", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "ADV", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An Deinen Knien lehnen, la\u00df mich tr\u00e4umen,", "tokens": ["An", "Dei\u00b7nen", "Kni\u00b7en", "leh\u00b7nen", ",", "la\u00df", "mich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meine Zauberwelt entz\u00fcckt versunken,", "tokens": ["In", "mei\u00b7ne", "Zau\u00b7ber\u00b7welt", "ent\u00b7z\u00fcckt", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umwoben von geheimni\u00dfvollen M\u00e4chten", "tokens": ["Um\u00b7wo\u00b7ben", "von", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7len", "M\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Vollmondlicht.", "tokens": ["I