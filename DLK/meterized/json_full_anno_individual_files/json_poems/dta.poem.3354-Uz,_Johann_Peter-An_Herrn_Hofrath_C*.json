{"dta.poem.3354": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath C*", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Freunde! h\u00f6ret mich, die ihr die Sch\u00f6nheit nen-", "tokens": ["Ihr", "Freun\u00b7de", "!", "h\u00f6\u00b7ret", "mich", ",", "die", "ihr", "die", "Sch\u00f6n\u00b7heit", "nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "net,", "tokens": ["net", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "F\u00fcr ihre Rechte k\u00e4mpft, und sie vielleicht nicht kennet!", "tokens": ["F\u00fcr", "ih\u00b7re", "Rech\u00b7te", "k\u00e4mpft", ",", "und", "sie", "viel\u00b7leicht", "nicht", "ken\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es lacht auf ihrer Stirn die Einfalt der Natur:", "tokens": ["Es", "lacht", "auf", "ih\u00b7rer", "Stirn", "die", "Ein\u00b7falt", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Sie ist auch nackend sch\u00f6n; nicht sch\u00f6n im Purpur nur.", "tokens": ["Sie", "ist", "auch", "na\u00b7ckend", "sch\u00f6n", ";", "nicht", "sch\u00f6n", "im", "Pur\u00b7pur", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$.", "PTKNEG", "ADJD", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein bunter Hurenschmuck ist falscher Sch\u00f6nheit eigen:", "tokens": ["Ein", "bun\u00b7ter", "Hu\u00b7ren\u00b7schmuck", "ist", "fal\u00b7scher", "Sch\u00f6n\u00b7heit", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die gleisst von Flittergold, und will sich immer zeigen;", "tokens": ["Die", "gleisst", "von", "Flit\u00b7ter\u00b7gold", ",", "und", "will", "sich", "im\u00b7mer", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "$,", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und will vorwitzig stolz, auf Stelzen sich erh\u00f6hn,", "tokens": ["Und", "will", "vor\u00b7wit\u00b7zig", "stolz", ",", "auf", "Stel\u00b7zen", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADJD", "$,", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem Winde sich vertraun, und auf den Wolken gehn.", "tokens": ["Dem", "Win\u00b7de", "sich", "ver\u00b7traun", ",", "und", "auf", "den", "Wol\u00b7ken", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Rien n'est beau que le Vrai, le Vrai seul est ai-\nmable,\nIl doit briller par tout &amp; m&#234;me dans la Fable.\nBoileau Das Wahre nur gef\u00e4llt; und wollt ihr w\u00fcrdig", "tokens": ["Ri\u00b7en", "n'\u00b7est", "be\u00b7au", "que", "le", "Vrai", ",", "le", "Vrai", "seul", "est", "ai", "mab\u00b7le", ",", "Il", "doit", "bril\u00b7ler", "par", "tout", "&amp;", "m", "&#234;", "me", "dans", "la", "Fab\u00b7le", ".", "Boi\u00b7le\u00b7au", "Das", "Wah\u00b7re", "nur", "ge\u00b7f\u00e4llt", ";", "und", "wollt", "ihr", "w\u00fcr\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "NE", "ART", "ADJA", "ADV", "VVPP", "$.", "KON", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "dichten,", "tokens": ["dich\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "So mu\u00df die Dichtung nicht auch die Natur vernich-", "tokens": ["So", "mu\u00df", "die", "Dich\u00b7tung", "nicht", "auch", "die", "Na\u00b7tur", "ver\u00b7nich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ten.", "tokens": ["ten", "."], "token_info": ["word", "punct"], "pos": ["FM", "$."], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Oft fliegt sie schw\u00e4rmend auf; allein verflieget sich,", "tokens": ["Oft", "fliegt", "sie", "schw\u00e4r\u00b7mend", "auf", ";", "al\u00b7lein", "ver\u00b7flie\u00b7get", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wird nicht wunderbar, nur abentheuerlich.", "tokens": ["Und", "wird", "nicht", "wun\u00b7der\u00b7bar", ",", "nur", "a\u00b7bent\u00b7heu\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In L\u00e4ndern voller Lichts, in anfgekl\u00e4rten Zeiten,", "tokens": ["In", "L\u00e4n\u00b7dern", "vol\u00b7ler", "Lichts", ",", "in", "anf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Soll wider die Vernunft allein die Dichtkunst strei-", "tokens": ["Soll", "wi\u00b7der", "die", "Ver\u00b7nunft", "al\u00b7lein", "die", "Dicht\u00b7kunst", "strei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "ten?", "tokens": ["ten", "?"], "token_info": ["word", "punct"], "pos": ["FM", "$."], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Wie? dieses Himmelskind schm\u00fcckt p\u00f6belhaften Wahn,", "tokens": ["Wie", "?", "die\u00b7ses", "Him\u00b7mel\u00b7skind", "schm\u00fcckt", "p\u00f6\u00b7bel\u00b7haf\u00b7ten", "Wahn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PDAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Pflanzt alten Jrrthum fort und pflanzet neuen an?", "tokens": ["Pflanzt", "al\u00b7ten", "Jrr\u00b7thum", "fort", "und", "pflan\u00b7zet", "neu\u00b7en", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit M\u00e4hrchen spielt allein die lachende Satire:", "tokens": ["Mit", "M\u00e4hr\u00b7chen", "spielt", "al\u00b7lein", "die", "la\u00b7chen\u00b7de", "Sa\u00b7ti\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Die hohe Muse weis, was ihrem Ernst geb\u00fchre.", "tokens": ["Die", "ho\u00b7he", "Mu\u00b7se", "weis", ",", "was", "ih\u00b7rem", "Ernst", "ge\u00b7b\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dem Scherze wird verziehn, der eine Thorheit wagt:", "tokens": ["Dem", "Scher\u00b7ze", "wird", "ver\u00b7ziehn", ",", "der", "ei\u00b7ne", "Thor\u00b7heit", "wagt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Doch der wird ausgezischt, der sie im Ernste sagt.", "tokens": ["Doch", "der", "wird", "aus\u00b7ge\u00b7zischt", ",", "der", "sie", "im", "Erns\u00b7te", "sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "VVPP", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht Sch\u00f6nheit einer Art mu\u00df aller Orten lachen:", "tokens": ["Nicht", "Sch\u00f6n\u00b7heit", "ei\u00b7ner", "Art", "mu\u00df", "al\u00b7ler", "Or\u00b7ten", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ART", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was immer wiederkommt wird endlich m\u00fcde machen.", "tokens": ["Was", "im\u00b7mer", "wie\u00b7der\u00b7kommt", "wird", "end\u00b7lich", "m\u00fc\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer immer mahlt und mahlt, und ieden M\u00fccken-", "tokens": ["Wer", "im\u00b7mer", "mahlt", "und", "mahlt", ",", "und", "ie\u00b7den", "M\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,", "KON", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Fu\u00df", "tokens": ["Fu\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "In sein Gem\u00e4hlde bringt, mahlt uns zum Ueberdru\u00df.", "tokens": ["In", "sein", "Ge\u00b7m\u00e4hl\u00b7de", "bringt", ",", "mahlt", "uns", "zum", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Sch\u00fcler der Natur verlangt nicht stets zu gl\u00e4n-", "tokens": ["Der", "Sch\u00fc\u00b7ler", "der", "Na\u00b7tur", "ver\u00b7langt", "nicht", "stets", "zu", "gl\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "zen:", "tokens": ["zen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Er l\u00e4\u00dft ein lebhaft Licht an sanfte Schatten gr\u00e4nzen.", "tokens": ["Er", "l\u00e4\u00dft", "ein", "leb\u00b7haft", "Licht", "an", "sanf\u00b7te", "Schat\u00b7ten", "gr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es blendet unser Aug ein steter Sonnenschein:", "tokens": ["Es", "blen\u00b7det", "un\u00b7ser", "Aug", "ein", "ste\u00b7ter", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Lorsque nous demandons des choses qui n