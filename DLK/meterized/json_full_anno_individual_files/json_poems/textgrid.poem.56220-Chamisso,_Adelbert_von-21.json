{"textgrid.poem.56220": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "21", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei stark, du meine M\u00e4nnin, reiche mir", "tokens": ["Sei", "stark", ",", "du", "mei\u00b7ne", "M\u00e4n\u00b7nin", ",", "rei\u00b7che", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und weihe, sie ber\u00fchrend, meine Waffen;", "tokens": ["Und", "wei\u00b7he", ",", "sie", "be\u00b7r\u00fch\u00b7rend", ",", "mei\u00b7ne", "Waf\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht t\u00f6richt gilt's die Welt mehr umzuschaffen,", "tokens": ["Nicht", "t\u00f6\u00b7richt", "gilt's", "die", "Welt", "mehr", "um\u00b7zu\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "ART", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sei stark, f\u00fcr Recht und Ordnung k\u00e4mpfen wir.", "tokens": ["Sei", "stark", ",", "f\u00fcr", "Recht", "und", "Ord\u00b7nung", "k\u00e4mp\u00b7fen", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bricht selbstverschuldet Unheil auf ein Land,", "tokens": ["Bricht", "selbst\u00b7ver\u00b7schul\u00b7det", "Un\u00b7heil", "auf", "ein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und kr\u00e4chzet mahnend links am Weg der Rabe,", "tokens": ["Und", "kr\u00e4ch\u00b7zet", "mah\u00b7nend", "links", "am", "Weg", "der", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird ihm verderblich seine Sehergabe,", "tokens": ["Wird", "ihm", "ver\u00b7derb\u00b7lich", "sei\u00b7ne", "Se\u00b7her\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm gibt des Unheils Schuld der Unverstand.", "tokens": ["Ihm", "gibt", "des", "Un\u00b7heils", "Schuld", "der", "Un\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es hob sich wider mich der Toren Zunft,", "tokens": ["Es", "hob", "sich", "wi\u00b7der", "mich", "der", "To\u00b7ren", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie st\u00fcrmten auf mich ein, mich zu zerrei\u00dfen;", "tokens": ["Sie", "st\u00fcrm\u00b7ten", "auf", "mich", "ein", ",", "mich", "zu", "zer\u00b7rei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich, Rabe, schrie: die schwangre Zeit will krei\u00dfen! \u2013", "tokens": ["Ich", ",", "Ra\u00b7be", ",", "schrie", ":", "die", "schwang\u00b7re", "Zeit", "will", "krei\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "$.", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun bebt die Welt bei ihrer Niederkunft.", "tokens": ["Nun", "bebt", "die", "Welt", "bei", "ih\u00b7rer", "Nie\u00b7der\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das haben ja die Kinder schon gewu\u00dft,", "tokens": ["Das", "ha\u00b7ben", "ja", "die", "Kin\u00b7der", "schon", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und jene haben doch das Wort gesprochen;", "tokens": ["Und", "je\u00b7ne", "ha\u00b7ben", "doch", "das", "Wort", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun ist der Tag des Blutes angebrochen;", "tokens": ["Nun", "ist", "der", "Tag", "des", "Blu\u00b7tes", "an\u00b7ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Erz umg\u00fcrte sich jedwede Brust.", "tokens": ["Mit", "Erz", "um\u00b7g\u00fcr\u00b7te", "sich", "jed\u00b7we\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wir ziehen trauernd in die M\u00e4nnerschlacht,", "tokens": ["Wir", "zie\u00b7hen", "trau\u00b7ernd", "in", "die", "M\u00e4n\u00b7ner\u00b7schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcber Tr\u00fcmmer k\u00e4mpfen wir und Leichen.", "tokens": ["Und", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mer", "k\u00e4mp\u00b7fen", "wir", "und", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fluch \u00fcber sie, die uns den \u00d6lzweig reichen", "tokens": ["Fluch", "\u00fc\u00b7ber", "sie", ",", "die", "uns", "den", "\u00d6l\u00b7zweig", "rei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschm\u00e4hend sahn, und Krieg uns zugebracht!", "tokens": ["Ver\u00b7schm\u00e4\u00b7hend", "sahn", ",", "und", "Krieg", "uns", "zu\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Fluch \u00fcber sie! denn losgerissen st\u00fcrzt", "tokens": ["Fluch", "\u00fc\u00b7ber", "sie", "!", "denn", "los\u00b7ge\u00b7ris\u00b7sen", "st\u00fcrzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$.", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Anwachsend die Lauvin' und schafft Verderben.", "tokens": ["An\u00b7wach\u00b7send", "die", "Lau\u00b7vin'", "und", "schafft", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "F\u00fcr Recht und Ordnung gilt's annoch zu sterben \u2013", "tokens": ["F\u00fcr", "Recht", "und", "Ord\u00b7nung", "gilt's", "an\u00b7noch", "zu", "ster\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer wei\u00df, wie morgen sich der Knoten sch\u00fcrzt?", "tokens": ["Wer", "wei\u00df", ",", "wie", "mor\u00b7gen", "sich", "der", "Kno\u00b7ten", "sch\u00fcrzt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Zwietracht auf erk\u00e4mpftem Boden mag", "tokens": ["In", "Zwiet\u00b7racht", "auf", "er\u00b7k\u00e4mpf\u00b7tem", "Bo\u00b7den", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich leicht die Schar zerspalten der Genossen;", "tokens": ["Sich", "leicht", "die", "Schar", "zer\u00b7spal\u00b7ten", "der", "Ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die heut um mich den Heldenkreis geschlossen,", "tokens": ["Die", "heut", "um", "mich", "den", "Hel\u00b7den\u00b7kreis", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind Feinde mir vielleicht am n\u00e4chsten Tag.", "tokens": ["Sind", "Fein\u00b7de", "mir", "viel\u00b7leicht", "am", "n\u00e4chs\u00b7ten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich werde stehen, wo ich soll und darf,", "tokens": ["Ich", "wer\u00b7de", "ste\u00b7hen", ",", "wo", "ich", "soll", "und", "darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "PWAV", "PPER", "VMFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und fallen, mu\u00df es sein, wo Edle starben,", "tokens": ["Und", "fal\u00b7len", ",", "mu\u00df", "es", "sein", ",", "wo", "Ed\u00b7le", "star\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "VMFIN", "PPER", "VAINF", "$,", "PWAV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr Recht und Ordnung wehen meine Farben,", "tokens": ["F\u00fcr", "Recht", "und", "Ord\u00b7nung", "we\u00b7hen", "mei\u00b7ne", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Recht und Ordnung ist der Tod nicht scharf.", "tokens": ["F\u00fcr", "Recht", "und", "Ord\u00b7nung", "ist", "der", "Tod", "nicht", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ich deck euch k\u00e4mpfend mit dem eignen Leib,", "tokens": ["Ich", "deck", "euch", "k\u00e4mp\u00b7fend", "mit", "dem", "eig\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Umarme mich noch einmal, la\u00df das Weinen,", "tokens": ["Um\u00b7ar\u00b7me", "mich", "noch", "ein\u00b7mal", ",", "la\u00df", "das", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bring her mir meine beiden armen Kleinen,", "tokens": ["Bring", "her", "mir", "mei\u00b7ne", "bei\u00b7den", "ar\u00b7men", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "PPER", "PPOSAT", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nun \u2013 \u2013 Leb wohl, du vielgeliebtes Weib.", "tokens": ["Und", "nun", "\u2013", "\u2013",