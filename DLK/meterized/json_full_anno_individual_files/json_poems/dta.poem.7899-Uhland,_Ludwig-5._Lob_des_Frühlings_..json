{"dta.poem.7899": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5.  Lob des Fr\u00fchlings .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Saatengr\u00fcn, Veilchenduft,", "tokens": ["Saa\u00b7ten\u00b7gr\u00fcn", ",", "Veil\u00b7chen\u00b7duft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Lerchenwirbel, Amselschlag,", "tokens": ["Ler\u00b7chen\u00b7wir\u00b7bel", ",", "Am\u00b7sel\u00b7schlag", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnenregen, linde Luft!", "tokens": ["Son\u00b7nen\u00b7re\u00b7gen", ",", "lin\u00b7de", "Luft", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich solche Worte singe,", "tokens": ["Wenn", "ich", "sol\u00b7che", "Wor\u00b7te", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Braucht es dann noch gro\u00dfer Dinge,", "tokens": ["Braucht", "es", "dann", "noch", "gro\u00b7\u00dfer", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich zu preisen, Fr\u00fchlingstag?", "tokens": ["Dich", "zu", "prei\u00b7sen", ",", "Fr\u00fch\u00b7lings\u00b7tag", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}