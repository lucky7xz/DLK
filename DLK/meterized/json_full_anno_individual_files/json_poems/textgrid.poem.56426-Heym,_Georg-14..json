{"textgrid.poem.56426": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Minuten gehn. Es schaut der Steppensohn", "tokens": ["Mi\u00b7nu\u00b7ten", "gehn", ".", "Es", "schaut", "der", "Step\u00b7pen\u00b7sohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins Rauchmeer weit, das ihm das Land verh\u00fcllt.", "tokens": ["Ins", "Rauch\u00b7meer", "weit", ",", "das", "ihm", "das", "Land", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da blinkt es auf. Da: Waffen. Speer und Schild.", "tokens": ["Da", "blinkt", "es", "auf", ".", "Da", ":", "Waf\u00b7fen", ".", "Speer", "und", "Schild", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "$.", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdie Griechen sind's. Sie sind am Lager schon.\u00ab", "tokens": ["\u00bb", "die", "Grie\u00b7chen", "sind'", "s.", "Sie", "sind", "am", "La\u00b7ger", "schon", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVIMP", "PPER", "VAFIN", "APPRART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er sc