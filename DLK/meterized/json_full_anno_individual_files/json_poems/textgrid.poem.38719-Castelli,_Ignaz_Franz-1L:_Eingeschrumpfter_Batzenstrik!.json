{"textgrid.poem.38719": {"metadata": {"author": {"name": "Castelli, Ignaz Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eingeschrumpfter Batzenstrik!", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eingeschrumpfter Batzenstrik!", "tokens": ["Ein\u00b7ge\u00b7schrumpf\u00b7ter", "Bat\u00b7zen\u00b7strik", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zipfel nur von einem St\u00fck!", "tokens": ["Zip\u00b7fel", "nur", "von", "ei\u00b7nem", "St\u00fck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Miserabler, schlechter Tropf,", "tokens": ["Mi\u00b7se\u00b7rab\u00b7ler", ",", "schlech\u00b7ter", "Tropf", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Warum h\u00e4ngst denn stets den Kopf?", "tokens": ["Wa\u00b7rum", "h\u00e4ngst", "denn", "stets", "den", "Kopf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist doch ein erb\u00e4rmlich Wesen,", "tokens": ["Bist", "doch", "ein", "er\u00b7b\u00e4rm\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Anders bist doch sonst gewesen.", "tokens": ["An\u00b7ders", "bist", "doch", "sonst", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFI