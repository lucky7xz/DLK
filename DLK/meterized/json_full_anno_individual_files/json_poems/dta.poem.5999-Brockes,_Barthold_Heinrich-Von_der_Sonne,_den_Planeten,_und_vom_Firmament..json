{"dta.poem.5999": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint der Sonnen Gluht ein reger Creys zu seyn,", "tokens": ["Es", "scheint", "der", "Son\u00b7nen", "Gluht", "ein", "re\u00b7ger", "Creys", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch sie wird alles warm, durch sie wird alles licht.", "tokens": ["Durch", "sie", "wird", "al\u00b7les", "warm", ",", "durch", "sie", "wird", "al\u00b7les", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ADJD", "$,", "APPR", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie schickt aus ihrem Schoos ein Feur, das hell und rein,", "tokens": ["Sie", "schickt", "aus", "ih\u00b7rem", "Schoos", "ein", "Feur", ",", "das", "hell", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und durch den weiten Raum durchsicht\u2019ger L\u00fcffte bricht.", "tokens": ["Und", "durch", "den", "wei\u00b7ten", "Raum", "durch\u00b7sicht'\u00b7ger", "L\u00fcff\u00b7te", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mau sieht ohn Unterla\u00df sie ihre Kr\u00e4ffte dr\u00fccken", "tokens": ["Mau", "sieht", "ohn", "Un\u00b7ter\u00b7la\u00df", "sie", "ih\u00b7re", "Kr\u00e4ff\u00b7te", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Wasser, Erd\u2019 und Lufft, die ihre Gluht durchdringt,", "tokens": ["In", "Was\u00b7ser", ",", "Erd'", "und", "Lufft", ",", "die", "ih\u00b7re", "Gluht", "durch\u00b7dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und auf der C\u00f6rper Meng, die sich durch sie nur schm\u00fccken;", "tokens": ["Und", "auf", "der", "C\u00f6r\u00b7per", "Meng", ",", "die", "sich", "durch", "sie", "nur", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NE", "$,", "PRELS", "PRF", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Als denen sie allein so Licht als Sch\u00f6nheit bringt.", "tokens": ["Als", "de\u00b7nen", "sie", "al\u00b7lein", "so", "Licht", "als", "Sch\u00f6n\u00b7heit", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PPER", "ADV", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}