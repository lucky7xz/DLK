{"textgrid.poem.39115": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr k\u00f6nnt nicht uns verstehen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr k\u00f6nnt nicht uns verstehen", "tokens": ["Ihr", "k\u00f6nnt", "nicht", "uns", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wir nicht euren Rat:", "tokens": ["Und", "wir", "nicht", "eu\u00b7ren", "Rat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan, so la\u00dft uns gehen", "tokens": ["Wo\u00b7hlan", ",", "so", "la\u00dft", "uns", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein jeder seinen Pfad.", "tokens": ["Ein", "je\u00b7der", "sei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr legt die Stirn in Falten,", "tokens": ["Ihr", "legt", "die", "Stirn", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr nennt euch selbst die Alten,", "tokens": ["Ihr", "nennt", "euch", "selbst", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die N\u00fcchternen, die Kalten:", "tokens": ["Die", "N\u00fcch\u00b7ter\u00b7nen", ",", "die", "Kal\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wir sind jung und wir sind frisch", "tokens": ["Und", "wir", "sind", "jung", "und", "wir", "sind", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wir sind rasch und wir sind risch,", "tokens": ["Und", "wir", "sind", "rasch", "und", "wir", "sind", "risch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das kann nicht Friede halten.", "tokens": ["Das", "kann", "nicht", "Frie\u00b7de", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir wollen euch nicht z\u00fcrnen,", "tokens": ["Wir", "wol\u00b7len", "euch", "nicht", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ade, ihr alten Herrn!", "tokens": ["A\u00b7de", ",", "ihr", "al\u00b7ten", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor euren kahlen Stirnen", "tokens": ["Vor", "eu\u00b7ren", "kah\u00b7len", "Stir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beugt unser Knie sich gern.", "tokens": ["Beugt", "un\u00b7ser", "Knie", "sich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch sagt, vor unsern Locken,", "tokens": ["Doch", "sagt", ",", "vor", "un\u00b7sern", "Lo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor unsers Flaumes Flocken,", "tokens": ["Vor", "un\u00b7sers", "Flau\u00b7mes", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Warum steht ihr erschrocken?", "tokens": ["Wa\u00b7rum", "steht", "ihr", "er\u00b7schro\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Auch euer Haupt war einmal braun,", "tokens": ["Auch", "eu\u00b7er", "Haupt", "war", "ein\u00b7mal", "braun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auch euer Auge konnte taun;", "tokens": ["Auch", "eu\u00b7er", "Au\u00b7ge", "konn\u00b7te", "taun", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nun aber ist es trocken.", "tokens": ["Nun", "a\u00b7ber", "ist", "es", "tro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr habt ihn l\u00e4ngst verloren,", "tokens": ["Ihr", "habt", "ihn", "l\u00e4ngst", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Blick f\u00fcr unsre Welt,", "tokens": ["Den", "Blick", "f\u00fcr", "uns\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch d\u00fcnkt ein Spott der Toren,", "tokens": ["Euch", "d\u00fcnkt", "ein", "Spott", "der", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was uns die Seele schwellt.", "tokens": ["Was", "uns", "die", "See\u00b7le", "schwellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr m\u00f6gt nur immer sagen,", "tokens": ["Ihr", "m\u00f6gt", "nur", "im\u00b7mer", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kopfsch\u00fctteln nur und fragen,", "tokens": ["Kopf\u00b7sch\u00fct\u00b7teln", "nur", "und", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bedauern und beklagen:", "tokens": ["Be\u00b7dau\u00b7ern", "und", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Uns packt es an und rei\u00dft es fort,", "tokens": ["Uns", "packt", "es", "an", "und", "rei\u00dft", "es", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nun sind wir hier, nun sind wir dort,", "tokens": ["Nun", "sind", "wir", "hier", ",", "nun", "sind", "wir", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir wollen einmal wagen.", "tokens": ["Wir", "wol\u00b7len", "ein\u00b7mal", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Lebt wohl! \u2013 Zum letzten Male", "tokens": ["Lebt", "wohl", "!", "\u2013", "Zum", "letz\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "$(", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kreuzt unsre Bahn sich hier:", "tokens": ["Kreuzt", "uns\u00b7re", "Bahn", "sich", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr geht gemach im Tale,", "tokens": ["Ihr", "geht", "ge\u00b7mach", "im", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Klippen wandern wir.", "tokens": ["Auf", "Klip\u00b7pen", "wan\u00b7dern", "wir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ruht aus in Abendgluten,", "tokens": ["Ruht", "aus", "in", "A\u00b7bend\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beim Murmeln k\u00fchler Fluten,", "tokens": ["Beim", "Mur\u00b7meln", "k\u00fch\u00b7ler", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie eure V\u00e4ter ruhten:", "tokens": ["Wie", "eu\u00b7re", "V\u00e4\u00b7ter", "ruh\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denkt nie, da\u00df ihr einst selber so", "tokens": ["Denkt", "nie", ",", "da\u00df", "ihr", "einst", "sel\u00b7ber", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wart jugendfrisch und jugendfroh \u2013!", "tokens": ["Wart", "ju\u00b7gend\u00b7frisch", "und", "ju\u00b7gend\u00b7froh", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Herz m\u00fc\u00dft euch ja bluten.", "tokens": ["Das", "Herz", "m\u00fc\u00dft", "euch", "ja", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du aber, Reich der Jugend,", "tokens": ["Du", "a\u00b7ber", ",", "Reich", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steig auf, du ewig jung,", "tokens": ["Steig", "auf", ",", "du", "e\u00b7wig", "jung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du G\u00f6ttereich der Tugend", "tokens": ["Du", "G\u00f6t\u00b7ter\u00b7eich", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NE", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und der Begeisterung!", "tokens": ["Und", "der", "Be\u00b7geis\u00b7te\u00b7rung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und sollten wir verderben,", "tokens": ["Und", "soll\u00b7ten", "wir", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wir wollen f\u00fcr dich werben,", "tokens": ["Wir", "wol\u00b7len", "f\u00fcr", "dich", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Zukunft soll dich erben!", "tokens": ["Die", "Zu\u00b7kunft", "soll", "dich", "er\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Alter mag im Lehnstuhl ruhn:", "tokens": ["Das", "Al\u00b7ter", "mag", "im", "Lehn\u00b7stuhl", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch will Gott uns was Gutes tun,", "tokens": ["Doch", "will", "Gott", "uns", "was", "Gu\u00b7tes", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "PPER", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So la\u00df er jung uns sterben!", "tokens": ["So", "la\u00df", "er", "jung", "uns", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr k\u00f6nnt nicht uns verstehen", "tokens": ["Ihr", "k\u00f6nnt", "nicht", "uns", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wir nicht euren Rat:", "tokens": ["Und", "wir", "nicht", "eu\u00b7ren", "Rat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohlan, so la\u00dft uns gehen", "tokens": ["Wo\u00b7hlan", ",", "so", "la\u00dft", "uns", "ge\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein jeder seinen Pfad.", "tokens": ["Ein", "je\u00b7der", "sei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr legt die Stirn in Falten,", "tokens": ["Ihr", "legt", "die", "Stirn", "in", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr nennt euch selbst die Alten,", "tokens": ["Ihr", "nennt", "euch", "selbst", "die", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die N\u00fcchternen, die Kalten:", "tokens": ["Die", "N\u00fcch\u00b7ter\u00b7nen", ",", "die", "Kal\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wir sind jung und wir sind frisch", "tokens": ["Und", "wir", "sind", "jung", "und", "wir", "sind", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wir sind rasch und wir sind risch,", "tokens": ["Und", "wir", "sind", "rasch", "und", "wir", "sind", "risch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das kann nicht Friede halten.", "tokens": ["Das", "kann", "nicht", "Frie\u00b7de", "hal\u00b7ten", "."], "token_info": ["word", "wo