{"dta.poem.6965": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seel' ist in dem Leib so lang er Athem f\u00fchrt:", "tokens": ["Die", "Seel'", "ist", "in", "dem", "Leib", "so", "lang", "er", "A\u00b7them", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, ist das Instrument, der C\u00f6rper, erst versehret,", "tokens": ["Doch", ",", "ist", "das", "Inst\u00b7ru\u00b7ment", ",", "der", "C\u00f6r\u00b7per", ",", "erst", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Geistern ausgeleert, verwirret und zerst\u00f6hret;", "tokens": ["Von", "Geis\u00b7tern", "aus\u00b7ge\u00b7leert", ",", "ver\u00b7wir\u00b7ret", "und", "zer\u00b7st\u00f6h\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So werden sie von ihr nicht mehr regiert.", "tokens": ["So", "wer\u00b7den", "sie", "von", "ihr", "nicht", "mehr", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sie nicht w\u00fcrcken kan im C\u00f6rper, der vergeht,", "tokens": ["Wenn", "sie", "nicht", "w\u00fcr\u00b7cken", "kan", "im", "C\u00f6r\u00b7per", ",", "der", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ohn\u2019 ihren Untergang zugleich mit zu empfinden;", "tokens": ["Ohn'", "ih\u00b7ren", "Un\u00b7ter\u00b7gang", "zu\u00b7gleich", "mit", "zu", "emp\u00b7fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sucht sie zuletzt sich zu entbinden", "tokens": ["Sucht", "sie", "zu\u00b7letzt", "sich", "zu", "ent\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von einem Instrument, das ferner nicht besteht.", "tokens": ["Von", "ei\u00b7nem", "Inst\u00b7ru\u00b7ment", ",", "das", "fer\u00b7ner", "nicht", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}