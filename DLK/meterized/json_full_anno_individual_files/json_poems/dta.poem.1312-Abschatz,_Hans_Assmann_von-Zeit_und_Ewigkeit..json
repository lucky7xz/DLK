rd", "word", "word", "word", "punct"], "pos": ["PWS", "KON", "NN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In tieffster Berge finstrer Scho\u00df", "tokens": ["In", "tieffs\u00b7ter", "Ber\u00b7ge", "finst\u00b7rer", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt sich Crystall und Silber blo\u00df:", "tokens": ["Giebt", "sich", "Crys\u00b7tall", "und", "Sil\u00b7ber", "blo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "KON", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Vernunfft forscht nicht mit F\u00fcrwitz aus", "tokens": ["Ver\u00b7nunfft", "forscht", "nicht", "mit", "F\u00fcr\u00b7witz", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "APPR", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ewigkeit verborgnes Hau\u00df.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ver\u00b7borg\u00b7nes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Trau/ Seele/ dieser N\u00e4rrin nicht/", "tokens": ["Trau", "/", "See\u00b7le", "/", "die\u00b7ser", "N\u00e4r\u00b7rin", "nicht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "PDAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie dir hier viel Zeit verspricht/", "tokens": ["Wenn", "sie", "dir", "hier", "viel", "Zeit", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"],