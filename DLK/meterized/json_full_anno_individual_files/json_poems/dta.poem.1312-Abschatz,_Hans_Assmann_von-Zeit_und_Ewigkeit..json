{"dta.poem.1312": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Zeit und Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Stunde d\u00fcstrer Mitternacht/", "tokens": ["Zur", "Stun\u00b7de", "d\u00fcst\u00b7rer", "Mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alles schl\u00e4fft/ mein Auge wacht/", "tokens": ["Wenn", "al\u00b7les", "schl\u00e4fft", "/", "mein", "Au\u00b7ge", "wacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweg\u2019 ich/ wie die Zeit wegeilt/", "tokens": ["Er\u00b7weg'", "ich", "/", "wie", "die", "Zeit", "we\u00b7geilt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die unser kurtzes Leben theilt.", "tokens": ["Die", "un\u00b7ser", "kurt\u00b7zes", "Le\u00b7ben", "theilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Tag ist lang/ wenn Schmertz und Noth", "tokens": ["Ein", "Tag", "ist", "lang", "/", "wenn", "Schmertz", "und", "Noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird unser hartes Wochen-Brod:", "tokens": ["Wird", "un\u00b7ser", "har\u00b7tes", "Wo\u00b7chen\u00b7Brod", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie schwer die Angst und Arbeit sey/", "tokens": ["Wie", "schwer", "die", "Angst", "und", "Ar\u00b7beit", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geht Woch und Tag doch bald f\u00fcrbey.", "tokens": ["Geht", "Woch", "und", "Tag", "doch", "bald", "f\u00fcr\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Monat theilet sich das Jahr/", "tokens": ["In", "Mo\u00b7nat", "thei\u00b7let", "sich", "das", "Jahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wird man unverhofft gewahr/", "tokens": ["Doch", "wird", "man", "un\u00b7ver\u00b7hofft", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie dieser k\u00f6mmt und jener weicht/", "tokens": ["Wie", "die\u00b7ser", "k\u00f6mmt", "und", "je\u00b7ner", "weicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "KON", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bi\u00df Jahr und Leben mit verstreicht.", "tokens": ["Bi\u00df", "Jahr", "und", "Le\u00b7ben", "mit", "ver\u00b7streicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Trau/ Seele/ keiner Stunde nicht!", "tokens": ["Trau", "/", "See\u00b7le", "/", "kei\u00b7ner", "Stun\u00b7de", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du weist nicht/ wenn das Leben bricht/", "tokens": ["Du", "weist", "nicht", "/", "wenn", "das", "Le\u00b7ben", "bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimmst doch durch die kurtze Zeit", "tokens": ["Und", "nimmst", "doch", "durch", "die", "kurt\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg zur langen Ewigkeit.", "tokens": ["Den", "Weg", "zur", "lan\u00b7gen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Tag hat sein geseztes Ziel/", "tokens": ["Ein", "Tag", "hat", "sein", "ge\u00b7sez\u00b7tes", "Ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ihm die Sonne g\u00f6nnen will/", "tokens": ["Das", "ihm", "die", "Son\u00b7ne", "g\u00f6n\u00b7nen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer aber mi\u00dft den langen Tag", "tokens": ["Wer", "a\u00b7ber", "mi\u00dft", "den", "lan\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der keinen Abend finden mag?", "tokens": ["Der", "kei\u00b7nen", "A\u00b7bend", "fin\u00b7den", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wir schreiben nach des Monden Lauff", "tokens": ["Wir", "schrei\u00b7ben", "nach", "des", "Mon\u00b7den", "Lauff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zahl der Jahres-Wochen auff;", "tokens": ["Die", "Zahl", "der", "Jah\u00b7res\u00b7Wo\u00b7chen", "auff", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist der uns zu rechnen wei\u00df", "tokens": ["Wer", "ist", "der", "uns", "zu", "rech\u00b7nen", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Woch ohn Ende rundten Krei\u00df?", "tokens": ["Der", "Woch", "ohn", "En\u00b7de", "rund\u00b7ten", "Krei\u00df", "?"], "token_inf