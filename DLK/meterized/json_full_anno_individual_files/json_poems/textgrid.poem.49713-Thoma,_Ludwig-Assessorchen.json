{"textgrid.poem.49713": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Assessorchen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bezirksamtsassessorchen, nicht wahr?", "tokens": ["Ein", "Be\u00b7zirks\u00b7amt\u00b7sas\u00b7ses\u00b7sor\u00b7chen", ",", "nicht", "wahr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Schreibt mit krummen Fingern das ganze Jahr;", "tokens": ["Schreibt", "mit", "krum\u00b7men", "Fin\u00b7gern", "das", "gan\u00b7ze", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sein Horiz\u00f6ntchen \u2013 na ja, ihr wi\u00dft,", "tokens": ["Sein", "Ho\u00b7ri\u00b7z\u00f6nt\u00b7chen", "\u2013", "na", "ja", ",", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ITJ", "ADV", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie ein Assessorhoriz\u00f6ntchen ist.", "tokens": ["Wie", "ein", "As\u00b7ses\u00b7sor\u00b7ho\u00b7ri\u00b7z\u00f6nt\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zuerst der Komment, dann Examennot,", "tokens": ["Zu\u00b7erst", "der", "Kom\u00b7ment", ",", "dann", "E\u00b7xa\u00b7men\u00b7not", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann die Sorge um das t\u00e4gliche Brot,", "tokens": ["Dann", "die", "Sor\u00b7ge", "um", "das", "t\u00e4g\u00b7li\u00b7che", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Die eigene Meinung b\u00e4nglich versteckt", "tokens": ["Die", "ei\u00b7ge\u00b7ne", "Mei\u00b7nung", "b\u00e4ng\u00b7lich", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und immer korrekt \u2013 und immer korrekt.", "tokens": ["Und", "im\u00b7mer", "kor\u00b7rekt", "\u2013", "und", "im\u00b7mer", "kor\u00b7rekt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$(", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ein bi\u00dfchen Angst und ein bi\u00dfchen Schi\u00df", "tokens": ["Ein", "bi\u00df\u00b7chen", "Angst", "und", "ein", "bi\u00df\u00b7chen", "Schi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Vor jedem Karrierehindernis.", "tokens": ["Vor", "je\u00b7dem", "Kar\u00b7rie\u00b7re\u00b7hin\u00b7der\u00b7nis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sich k\u00fcmmerlich k\u00fcmmern den ganzen Tag,", "tokens": ["Sich", "k\u00fcm\u00b7mer\u00b7lich", "k\u00fcm\u00b7mern", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Ob der Vorgesetzte ihn leiden mag,", "tokens": ["Ob", "der", "Vor\u00b7ge\u00b7setz\u00b7te", "ihn", "lei\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Und ob er ihn endlich qualifiziert,", "tokens": ["Und", "ob", "er", "ihn", "end\u00b7lich", "qua\u00b7li\u00b7fi\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da\u00df Assessorchen ein Amtm\u00e4nnchen wird!", "tokens": ["Da\u00df", "As\u00b7ses\u00b7sor\u00b7chen", "ein", "Amt\u00b7m\u00e4nn\u00b7chen", "wird", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Da habt ihr das ganze Konglomerat,", "tokens": ["Da", "habt", "ihr", "das", "gan\u00b7ze", "Kon\u00b7glo\u00b7me\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Da habt ihr so ziemlich den ganzen Salat", "tokens": ["Da", "habt", "ihr", "so", "ziem\u00b7lich", "den", "gan\u00b7zen", "Sa\u00b7lat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.17": {"text": "Gemischten Inhalts im kleinen Gehirn", "tokens": ["Ge\u00b7mischten", "In\u00b7halts", "im", "klei\u00b7nen", "Ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Hinter der hohen Assessorstirn.", "tokens": ["Hin\u00b7ter", "der", "ho\u00b7hen", "As\u00b7ses\u00b7sors\u00b7tirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Doch wenn ihr glaubt, so ein Herrchen wird", "tokens": ["Doch", "wenn", "ihr", "glaubt", ",", "so", "ein", "Herr\u00b7chen", "wird"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Nur f\u00fcr den sp\u00e4tren Dienst pr\u00e4pariert", "tokens": ["Nur", "f\u00fcr", "den", "sp\u00e4t\u00b7ren", "Dienst", "pr\u00e4\u00b7pa\u00b7riert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Und hat ein \u00c4mtchen von kleinem Gewicht,", "tokens": ["Und", "hat", "ein", "\u00c4mt\u00b7chen", "von", "klei\u00b7nem", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Ihr Herren, da irrt ihr, so ist es nicht.", "tokens": ["Ihr", "Her\u00b7ren", ",", "da", "irrt", "ihr", ",", "so", "ist", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Trotz Horiz\u00f6ntchen \u2013 ei h\u00f6rt doch nur!", "tokens": ["Trotz", "Ho\u00b7ri\u00b7z\u00f6nt\u00b7chen", "\u2013", "ei", "h\u00f6rt", "doch", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "NE", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Assessorchen hat die Pressezensur!", "tokens": ["As\u00b7ses\u00b7sor\u00b7chen", "hat", "die", "Pres\u00b7se\u00b7zen\u00b7sur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Beschn\u00fcffelt, verbietet und konfisziert,", "tokens": ["Be\u00b7schn\u00fcf\u00b7felt", ",", "ver\u00b7bie\u00b7tet", "und", "kon\u00b7fis\u00b7ziert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Bestimmt h\u00f6chstselbst, was gelesen wird,", "tokens": ["Be\u00b7stimmt", "h\u00f6chst\u00b7selbst", ",", "was", "ge\u00b7le\u00b7sen", "wird", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PRELS", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Bewacht als ein treuer Gewissensrat", "tokens": ["Be\u00b7wacht", "als", "ein", "treu\u00b7er", "Ge\u00b7wis\u00b7sens\u00b7rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Gesellschaft, Kirche und unsern Staat.", "tokens": ["Ge\u00b7sell\u00b7schaft", ",", "Kir\u00b7che", "und", "un\u00b7sern", "Staat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Ja, ja, ihr Leute! Und wenn euch bangt,", "tokens": ["Ja", ",", "ja", ",", "ihr", "Leu\u00b7te", "!", "Und", "wenn", "euch", "bangt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPOSAT", "NN", "$.", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Da\u00df sein Gehirnchen daf\u00fcr nicht langt,", "tokens": ["Da\u00df", "sein", "Ge\u00b7hirn\u00b7chen", "da\u00b7f\u00fcr", "nicht", "langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PAV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "So sollt ihr denken: \u00bbDem Vaterland", "tokens": ["So", "sollt", "ihr", "den\u00b7ken", ":", "\u00bb", "Dem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "$(", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Ist wenig geholfen mit viel Verstand,", "tokens": ["Ist", "we\u00b7nig", "ge\u00b7hol\u00b7fen", "mit", "viel", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.33": {"text": "Gesinnungst\u00fcchtig, so voll und ganz,", "tokens": ["Ge\u00b7sin\u00b7nungs\u00b7t\u00fcch\u00b7tig", ",", "so", "voll", "und", "ganz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Ist immer nur eines \u2013 die Ignoranz.\u00ab", "tokens": ["Ist", "im\u00b7mer", "nur", "ei\u00b7nes", "\u2013", "die", "Ig\u00b7no\u00b7ranz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "$(", "ART", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ein Bezirksamtsassessorchen, nicht wahr?", "tokens": ["Ein", "Be\u00b7zirks\u00b7amt\u00b7sas\u00b7ses\u00b7sor\u00b7chen", ",", "nicht", "wahr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Schreibt mit krummen Fingern das ganze Jahr;", "tokens": ["Schreibt", "mit", "krum\u00b7men", "Fin\u00b7gern", "das", "gan\u00b7ze", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sein Horiz\u00f6ntchen \u2013 na ja, ihr wi\u00dft,", "tokens": ["Sein", "Ho\u00b7ri\u00b7z\u00f6nt\u00b7chen", "\u2013", "na", "ja", ",", "ihr", "wi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ITJ", "ADV", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie ein Assessorhoriz\u00f6ntchen ist.", "tokens": ["Wie", "ein", "As\u00b7ses\u00b7sor\u00b7ho\u00b7ri\u00b7z\u00f6nt\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zuerst der Komment, dann Examennot,", "tokens": ["Zu\u00b7erst", "der", "Kom\u00b7ment", ","