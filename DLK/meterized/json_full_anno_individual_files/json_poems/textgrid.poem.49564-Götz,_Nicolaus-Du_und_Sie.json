{"textgrid.poem.49564": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Du und Sie", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Galathea, wohin flog sie, die goldne Zeit,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "wo\u00b7hin", "flog", "sie", ",", "die", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da du, ohne Lakey und Putz,", "tokens": ["Da", "du", ",", "oh\u00b7ne", "La\u00b7key", "und", "Putz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Abends, einzig geschm\u00fcckt mit deinen Grazien,", "tokens": ["A\u00b7bends", ",", "ein\u00b7zig", "ge\u00b7schm\u00fcckt", "mit", "dei\u00b7nen", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "In der Fallje", "tokens": ["In", "der", "Fall\u00b7je"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-++", "measure": "unknown.measure.tri"}, "line.5": {"text": "Froh bey meinem Salat, den in Ambrosia", "tokens": ["Froh", "bey", "mei\u00b7nem", "Sa\u00b7lat", ",", "den", "in", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NE"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Deine Reize verwandelten,", "tokens": ["Dei\u00b7ne", "Rei\u00b7ze", "ver\u00b7wan\u00b7del\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.7": {"text": "Warfst du damahls dich selbst, fr\u00f6hlicher Laune voll,", "tokens": ["Warfst", "du", "da\u00b7mahls", "dich", "selbst", ",", "fr\u00f6h\u00b7li\u00b7cher", "Lau\u00b7ne", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADV", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-++--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "In des gl\u00fccklichen J\u00fcnglings Arm,", "tokens": ["In", "des", "gl\u00fcck\u00b7li\u00b7chen", "J\u00fcng\u00b7lings", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Der, betrogen von dir, g\u00e4nzlich sich dir ergab.", "tokens": ["Der", ",", "be\u00b7tro\u00b7gen", "von", "dir", ",", "g\u00e4nz\u00b7lich", "sich", "dir", "er\u00b7gab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "PPER", "$,", "ADJD", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Damahls schenkten die G\u00f6tter dir", "tokens": ["Da\u00b7mahls", "schenk\u00b7ten", "die", "G\u00f6t\u00b7ter", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Rang und Sch\u00e4tze noch nicht; aber an ihrer Statt,", "tokens": ["Rang", "und", "Sch\u00e4t\u00b7ze", "noch", "nicht", ";", "a\u00b7ber", "an", "ih\u00b7rer", "Statt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PTKNEG", "$.", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Was ein M\u00e4dchen unsch\u00e4tzbar macht:", "tokens": ["Was", "ein", "M\u00e4d\u00b7chen", "un\u00b7sch\u00e4tz\u00b7bar", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Einen lachenden Witz, herzliche Z\u00e4rtlichkeit,", "tokens": ["Ei\u00b7nen", "la\u00b7chen\u00b7den", "Witz", ",", "herz\u00b7li\u00b7che", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.14": {"text": "Eine Brust, wie die Milch so wei\u00df,", "tokens": ["Ei\u00b7ne", "Brust", ",", "wie", "die", "Milch", "so", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Und zwey Augen, verliebt, gro\u00df und verf\u00fchrerisch.", "tokens": ["Und", "zwey", "Au\u00b7gen", ",", "ver\u00b7liebt", ",", "gro\u00df", "und", "ver\u00b7f\u00fch\u00b7re\u00b7risch", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Wer mit solchen entz\u00fcckenden", "tokens": ["Wer", "mit", "sol\u00b7chen", "ent\u00b7z\u00fc\u00b7cken\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.17": {"text": "Reizen w\u00e4re kein Schalk? Holdester Gegenstand", "tokens": ["Rei\u00b7zen", "w\u00e4\u00b7re", "kein", "Schalk", "?", "Hol\u00b7des\u00b7ter", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "NN", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Meiner Liebe, du warst es auch!", "tokens": ["Mei\u00b7ner", "Lie\u00b7be", ",", "du", "warst", "es", "auch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.19": {"text": "Und ich liebete dich, Amor vergebe mir's!", "tokens": ["Und", "ich", "lie\u00b7be\u00b7te", "dich", ",", "A\u00b7mor", "ver\u00b7ge\u00b7be", "mir's", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "NE", "VVFIN", "NE", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Darum, wahrlich! nicht weniger!", "tokens": ["Da\u00b7rum", ",", "wahr\u00b7lich", "!", "nicht", "we\u00b7ni\u00b7ger", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "ADV", "$.", "PTKNEG", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Diesem Leben voll Lust gleichet ihr jetziges", "tokens": ["Die\u00b7sem", "Le\u00b7ben", "voll", "Lust", "glei\u00b7chet", "ihr", "jet\u00b7zi\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Reich mit Ehren gekr\u00f6ntes nicht!", "tokens": ["Reich", "mit", "Eh\u00b7ren", "ge\u00b7kr\u00f6n\u00b7tes", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Jener Schweizer, Madam, wei\u00df wie das Schneegebirg',", "tokens": ["Je\u00b7ner", "Schwei\u00b7zer", ",", "Ma\u00b7dam", ",", "wei\u00df", "wie", "das", "Schnee\u00b7ge\u00b7bir\u00b7g'", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und breitschultrig, wie Herkules,", "tokens": ["Und", "breit\u00b7schult\u00b7rig", ",", "wie", "Her\u00b7ku\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NE", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Der, in ihrem Pallast, l\u00fcgend, am Thore sitzt,", "tokens": ["Der", ",", "in", "ih\u00b7rem", "Pal\u00b7last", ",", "l\u00fc\u00b7gend", ",", "am", "Tho\u00b7re", "sitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PPOSAT", "NN", "$,", "VVPP", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ein symbolisches Bild der Zeit,", "tokens": ["Ein", "sym\u00b7bo\u00b7li\u00b7sches", "Bild", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Schreckt mit drohendem Blick, jetzo der l\u00e4chelnden", "tokens": ["Schreckt", "mit", "dro\u00b7hen\u00b7dem", "Blick", ",", "jet\u00b7zo", "der", "l\u00e4\u00b7cheln\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Amoretten und Grazien", "tokens": ["A\u00b7mo\u00b7ret\u00b7ten", "und", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Leichte Truppen hinweg. Sch\u00fcchtern umflattern sie", "tokens": ["Leich\u00b7te", "Trup\u00b7pen", "hin\u00b7weg", ".", "Sch\u00fcch\u00b7tern", "um\u00b7flat\u00b7tern", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Jene Balken von Zedernholz", "tokens": ["Je\u00b7ne", "Bal\u00b7ken", "von", "Ze\u00b7dern\u00b7holz"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Ihres Alkovs nicht mehr. Ehemahls schl\u00fcpften sie,", "tokens": ["Ih\u00b7res", "Al\u00b7kovs", "nicht", "mehr", ".", "E\u00b7he\u00b7mahls", "schl\u00fcpf\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "$.", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Einem Schwarme von Tauben gleich,", "tokens": ["Ei\u00b7nem", "Schwar\u00b7me", "von", "Tau\u00b7ben", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Oft durch's Fenster hinein; scherzten und trippelten", "tokens": ["Oft", "durch's", "Fens\u00b7ter", "hin\u00b7ein", ";", "scherz\u00b7ten", "und", "trip\u00b7pel\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$.", "VVFIN", "KON", "VVFIN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Um ihr jugendlich Bettchen her.", "tokens": ["Um", "ihr", "ju\u00b7gend\u00b7lich", "Bett\u00b7chen", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wahrlich, gn\u00e4dige Frau, diese lebendigen", "tokens": ["Wahr\u00b7lich", ",", "gn\u00e4\u00b7di\u00b7ge", "Frau", ",", "die\u00b7se", "le\u00b7ben\u00b7di\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PDAT", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Persianischen Teppiche;", "tokens": ["Per\u00b7si\u00b7a\u00b7ni\u00b7schen", "Tep\u00b7pi\u00b7che", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-++-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dieses Silbergeschirr, manches Praxiteles", "tokens": ["Die\u00b7ses", "Sil\u00b7ber\u00b7ge\u00b7schirr", ",", "man\u00b7ches", "Pra\u00b7xi\u00b7te\u00b7les"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Kunstwerk; diese hellgl\u00e4nzenden", "tokens": ["Kunst\u00b7werk", ";", "die\u00b7se", "hell\u00b7gl\u00e4n\u00b7zen\u00b7den"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PDAT", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Kabinette, worinn Frankreich die sinischen", "tokens": ["Ka\u00b7bi\u00b7net\u00b7te", ",", "wo\u00b7rinn", "Fran\u00b7kreich", "die", "si\u00b7ni\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "NE", "ART", "ADJA"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "K\u00fcnstler alle besch\u00e4mete;", "tokens": ["K\u00fcnst\u00b7ler", "al\u00b7le", "be\u00b7sch\u00e4\u00b7me\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVFIN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Diese Betten von Mohr; diese japanischen", "tokens": ["Die\u00b7se", "Bet\u00b7ten", "von", "Mohr", ";", "die\u00b7se", "ja\u00b7pa\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NE", "$.", "PDAT", "ADJA"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Prunkgef\u00e4\u00dfe, zerbrechliche", "tokens": ["Prunk\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", ",", "zer\u00b7brech\u00b7li\u00b7che"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.9": {"text": "Wunder menschlicher Kunst; diese demantenen", "tokens": ["Wun\u00b7der", "menschli\u00b7cher", "Kunst", ";", "die\u00b7se", "de\u00b7man\u00b7te\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "PDS", "VVINF"], "meter": "+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Ohrgeh\u00e4nge, Gestirnen gleich", "tokens": ["Ohr\u00b7ge\u00b7h\u00e4n\u00b7ge", ",", "Ge\u00b7stir\u00b7nen", "gleich"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Stralenstreuend bey Nacht; dieser bezaubernde", "tokens": ["Stra\u00b7len\u00b7streu\u00b7end", "bey", "Nacht", ";", "die\u00b7ser", "be\u00b7zau\u00b7bern\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "PDAT", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Staat und Hochmuth zusammen ist", "tokens": ["Staat", "und", "Hoch\u00b7muth", "zu\u00b7sam\u00b7men", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP", "VAFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Eines Ku\u00dfes nicht werth, den du mir Gl\u00fccklichen", "tokens": ["Ei\u00b7nes", "Ku\u00b7\u00dfes", "nicht", "werth", ",", "den", "du", "mir", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$,", "PRELS", "PPER", "PPER", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "In der Jugend gegeben hast.", "tokens": ["In", "der", "Ju\u00b7gend", "ge\u00b7ge\u00b7ben", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Galathea, wohin flog sie, die goldne Zeit,", "tokens": ["Ga\u00b7la\u00b7thea", ",", "wo\u00b7hin", "flog", "sie", ",", "die", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-++--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da du, ohne Lakey und Putz,", "tokens": ["Da", "du", ",", "oh\u00b7ne", "La\u00b7key", "und", "Putz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Abends, einzig geschm\u00fcckt mit deinen Grazien,", "tokens": ["A\u00b7bends", ",", "ein\u00b7zig", "ge\u00b7schm