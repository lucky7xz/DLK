{"textgrid.poem.33200": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2175.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Vater, der GOTT im ungrund, \u2013 ", "tokens": ["Dein", "Va\u00b7ter", ",", "der", "GoTT", "im", "un\u00b7grund", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "ADJD", "$,", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der sel'ge Sch\u00f6pfer \u2013 nicht all's verd\u00fcrb.", "tokens": ["Der", "sel'\u00b7ge", "Sch\u00f6p\u00b7fer", "\u2013", "nicht", "all's", "ver\u00b7d\u00fcrb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "O Gott! du tieffe \u2013 dr\u00fcber alle sinnen.", "tokens": ["O", "Gott", "!", "du", "tief\u00b7fe", "\u2013", "dr\u00fc\u00b7ber", "al\u00b7le", "sin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "ADJA", "$(", "PAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was bin ich denn euch, \u2013 allm\u00e4chtige Menschheit bekenn'.", "tokens": ["Was", "bin", "ich", "denn", "euch", ",", "\u2013", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Menschheit", "be\u00b7kenn'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PPER", "$,", "$(", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "O Heiliger Geist, \u2013 gesungen. Hallel.", "tokens": ["O", "Hei\u00b7li\u00b7ger", "Geist", ",", "\u2013", "ge\u00b7sun\u00b7gen", ".", "Hal\u00b7lel", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "$(", "VVPP", "$.", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Er will uns allzeit \u2013 seiner macht.", "tokens": ["Er", "will", "uns", "all\u00b7zeit", "\u2013", "sei\u00b7ner", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$(", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bis die Christin \u2013 fleisch und gebein.", "tokens": ["Bis", "die", "Chris\u00b7tin", "\u2013", "fleisch", "und", "ge\u00b7bein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ADJD", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Drum wird man oft geregt, \u2013 Vater in den h\u00f6hn!", "tokens": ["Drum", "wird", "man", "oft", "ge\u00b7regt", ",", "\u2013", "Va\u00b7ter", "in", "den", "h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "VVPP", "$,", "$(", "NN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}}, "stanza.9": {"line.1": {"text": "Dein Vater, der GOTT im ungrund, \u2013 ", "tokens": ["Dein", "Va\u00b7ter", ",", "der", "GoTT", "im", "un\u00b7grund", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "APPRART", "ADJD", "$,", "$("], "meter":