": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Selbsucht nur verdammt allein.", "tokens": ["Die", "Selb\u00b7sucht", "nur", "ver\u00b7dammt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn du den Himmel hast in dir,", "tokens": ["Wenn", "du", "den", "Him\u00b7mel", "hast", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist dir Tod und Leben gleich.", "tokens": ["So", "ist", "dir", "Tod", "und", "Le\u00b7ben", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast du nicht den Himmel hier,", "tokens": ["Und", "hast", "du", "nicht", "den", "Him\u00b7mel", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was n\u00fctzt dir dort das Himmelreich?", "tokens": ["Was", "n\u00fctzt", "dir", "dort", "das", "Him\u00b7mel\u00b7reich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lieb' etwas hier und bet' es an,", "tokens": ["Lieb'", "et\u00b7was", "hier", "und", "bet'", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verg\u00f6ttre nur dich selber nicht \u2013", "tokens": ["Ver\u00b7g\u00f6t\u00b7tre", "nur", "dich", "sel\u00b7ber", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir brach der Eigenliebe Wahn,", "tokens": ["Mir", "brach", "der", "Ei\u00b7gen\u00b7lie\u00b7be", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich dir sah ins Angesicht.", "tokens": ["Als", "ich", "dir", "sah", "ins", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hast mit deiner Locken Band", "tokens": ["Du", "hast", "mit", "dei\u00b7ner", "Lo\u00b7cken", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ichheit Fesseln abgestr\u00fcpft,", "tokens": ["Der", "Ich\u00b7heit", "Fes\u00b7seln", "ab\u00b7ge\u00b7str\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an der Seelen Vaterland", "tokens": ["Und", "an", "der", "See\u00b7len", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinen Blicken mich gekn\u00fcpft.", "tokens": ["Mit", "dei\u00b7nen", "Bli\u00b7cken", "mich", "ge\u00b7kn\u00fcpft", "."], "to