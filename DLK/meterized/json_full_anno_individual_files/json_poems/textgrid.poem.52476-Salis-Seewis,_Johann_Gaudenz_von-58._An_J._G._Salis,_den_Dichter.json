{"textgrid.poem.52476": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "58. An J. G. Salis, den Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, die Nachtigall schweigt! Wer singet im Thale nun f\u00fcrder!", "tokens": ["Sie\u00b7he", ",", "die", "Nach\u00b7ti\u00b7gall", "schweigt", "!", "Wer", "sin\u00b7get", "im", "Tha\u00b7le", "nun", "f\u00fcr\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "V\u00f6gelein singen genug, aber nicht eines wie sie! \u2013", "tokens": ["V\u00f6\u00b7ge\u00b7lein", "sin\u00b7gen", "ge\u00b7nug", ",", "a\u00b7ber", "nicht", "ei\u00b7nes", "wie", "sie", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADV", "PTKNEG", "PIS", "KOKOM", "PPER", "$.", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Du auch, Meister des Liedes im Alpengel\u00e4nde, du schweigest!", "tokens": ["Du", "auch", ",", "Meis\u00b7ter", "des", "Lie\u00b7des", "im", "Al\u00b7pen\u00b7ge\u00b7l\u00e4n\u00b7de", ",", "du", "schwei\u00b7gest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--+--++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Viel' zwar singen dir nach, aber nicht einer wie du! \u2013", "tokens": ["Viel'", "zwar", "sin\u00b7gen", "dir", "nach", ",", "a\u00b7ber", "nicht", "ei\u00b7ner", "wie", "du", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "PTKNEG", "PIS", "KOKOM", "PPER", "$.", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "\u00bbkehrte die Nachtigall doch!\u00ab \u2013 so rufen im Thale die Hirten.", "tokens": ["\u00bb", "kehr\u00b7te", "die", "Nach\u00b7ti\u00b7gall", "doch", "!", "\u00ab", "\u2013", "so", "ru\u00b7fen", "im", "Tha\u00b7le", "die", "Hir\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ADV", "$.", "$(", "$(", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "\u00bbsinge noch, Salis, ein Lied!\u00ab \u2013 hallt es im Alpengel\u00e4nd.", "tokens": ["\u00bb", "sin\u00b7ge", "noch", ",", "Sa\u00b7lis", ",", "ein", "Lied", "!", "\u00ab", "\u2013", "hallt", "es", "im", "Al\u00b7pen\u00b7ge\u00b7l\u00e4nd", "."], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "NE", "$,", "ART", "NN", "$.", "$(", "$(", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe, die Nachtigall schweigt! Wer singet im Thale nun f\u00fcrder!", "tokens": ["Sie\u00b7he", ",", "die", "Nach\u00b7ti\u00b7gall", "schweigt", "!", "Wer", "sin\u00b7get", "im", "Tha\u00b7le", "nun", "f\u00fcr\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "$