{"dta.poem.9916": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsrer f\u00fcnffe m\u00e4uler sind voll-mit menschenfleisch gef\u00fcllet/", "tokens": ["Uns\u00b7rer", "f\u00fcnf\u00b7fe", "m\u00e4u\u00b7ler", "sind", "voll\u00b7mit", "men\u00b7schen\u00b7fleisch", "ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "VVPP", "$("], "meter": "+-+-+--++--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Niemals wird der hunger uns mehr/ als wen\u0303 es kalt/ gestillet/", "tokens": ["Nie\u00b7mals", "wird", "der", "hun\u00b7ger", "uns", "mehr", "/", "als", "we\u00f1", "es", "kalt", "/", "ge\u00b7stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "PPER", "ADV", "$(", "KOKOM", "KOUS", "PPER", "ADJD", "$(", "VVPP", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wen\u0303 ein bruder uns verlassen/ werden wir gar schlecht geacht/", "tokens": ["We\u00f1", "ein", "bru\u00b7der", "uns", "ver\u00b7las\u00b7sen", "/", "wer\u00b7den", "wir", "gar", "schlecht", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "$(", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Thun/ wenn man von uns begehret/ alles/ was die hand sonst", "tokens": ["Thun", "/", "wenn", "man", "von", "uns", "be\u00b7ge\u00b7hret", "/", "al\u00b7les", "/", "was", "die", "hand", "sonst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "PIS", "APPR", "PPER", "VVFIN", "$(", "PIS", "$(", "PWS", "ART", "NN", "ADV"], "meter": "+---+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "(macht.", "tokens": ["(", "macht", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}