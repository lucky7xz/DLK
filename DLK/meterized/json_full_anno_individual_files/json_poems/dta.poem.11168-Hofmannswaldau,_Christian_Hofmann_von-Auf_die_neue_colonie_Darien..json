{"dta.poem.11168": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die neue colonie Darien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So macht die neue welt noch immer neue grillen,", "tokens": ["So", "macht", "die", "neu\u00b7e", "welt", "noch", "im\u00b7mer", "neu\u00b7e", "gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man die kasten will auf ihren k\u00fcsten f\u00fcllen.", "tokens": ["Da\u00df", "man", "die", "kas\u00b7ten", "will", "auf", "ih\u00b7ren", "k\u00fcs\u00b7ten", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wirds, ihr Spanier! um euer erbtheil stehn?", "tokens": ["Wie", "wirds", ",", "ihr", "Spa\u00b7nier", "!", "um", "eu\u00b7er", "erbt\u00b7heil", "stehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Schotten werden euch nun in die schoten gehn.", "tokens": ["Die", "Schot\u00b7ten", "wer\u00b7den", "euch", "nun", "in", "die", "scho\u00b7ten", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APP