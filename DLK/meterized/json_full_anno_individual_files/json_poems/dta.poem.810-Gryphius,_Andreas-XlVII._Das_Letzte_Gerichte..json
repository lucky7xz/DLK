{"dta.poem.810": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.  \n Das Letzte Gerichte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Avff Todten! auff! die welt verkracht in letztem brande! ", "tokens": ["Avff", "Tod\u00b7ten", "!", "auff", "!", "die", "welt", "ver\u00b7kracht", "in", "letz\u00b7tem", "bran\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sternen Heer vergeht! der Mond ist dunckel-rott/", "tokens": ["Der", "Ster\u00b7nen", "Heer", "ver\u00b7geht", "!", "der", "Mond", "ist", "dun\u00b7ckel\u00b7rott", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonn\u2019 ohn allen schein! Auff/ Jhr die grab vnd kott", "tokens": ["Die", "Sonn'", "ohn", "al\u00b7len", "schein", "!", "Auff", "/", "Ihr", "die", "grab", "vnd", "kott"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIS", "PTKVZ", "$.", "APPR", "$(", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff! jhr die Erd vnd See vnd Hellen hilt zu pfande!", "tokens": ["Auff", "!", "jhr", "die", "Erd", "vnd", "See", "vnd", "Hel\u00b7len", "hilt", "zu", "pfan\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPER", "ART", "NN", "KON", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr die jhr lebt komm\u2019t an: der HERR/ der vor in schande", "tokens": ["Ihr", "die", "jhr", "lebt", "kom\u00b7m't", "an", ":", "der", "HeRR", "/", "der", "vor", "in", "schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PPER", "VVFIN", "VVFIN", "PTKVZ", "$.", "ART", "NN", "$(", "ART", "APPR", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Sich richten lie\u00df/ erscheint/ vor Jhm laufft flam\u0303\u2019vnd noth", "tokens": ["Sich", "rich\u00b7ten", "lie\u00df", "/", "er\u00b7scheint", "/", "vor", "Jhm", "laufft", "flam\u0303'v\u00b7nd", "noth"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "VVFIN", "$(", "VVFIN", "$(", "APPR", "PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey Jhm steht Majest\u00e4tt/ nach jhm/ folgt blitz vnd todt/", "tokens": ["Bey", "Jhm", "steht", "Ma\u00b7jes\u00b7t\u00e4tt", "/", "nach", "jhm", "/", "folgt", "blitz", "vnd", "todt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "$(", "APPR", "PPER", "$(", "VVFIN", "NE", "KON", "ADJD", "$("], "meter": "-+----+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vmb jhn/ mehr Eherubim als Sand an Pontus strande.", "tokens": ["Vmb", "jhn", "/", "mehr", "E\u00b7he\u00b7ru\u00b7bim", "als", "Sand", "an", "Pon\u00b7tus", "stran\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$(", "PIAT", "NN", "KOUS", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}}, "stanza.3": {"line.1": {"text": "Wie lieblich spricht Er an/ die seine Recht\u2019 erkohren.", "tokens": ["Wie", "lieb\u00b7lich", "spricht", "Er", "an", "/", "die", "sei\u00b7ne", "Recht'", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$(", "ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schrecklich donnert Er/ auff diese die verlohren", "tokens": ["Wie", "schreck\u00b7lich", "don\u00b7nert", "Er", "/", "auff", "die\u00b7se", "die", "ver\u00b7loh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$(", "APPR", "PDAT", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnwiderrufflich wortt/ kommt Freunde! Feinde fliht!", "tokens": ["Vn\u00b7wi\u00b7der\u00b7ruff\u00b7lich", "wortt", "/", "kommt", "Freun\u00b7de", "!", "Fein\u00b7de", "fliht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAPP", "$(", "VVFIN", "NN", "$.", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Der Himmel schleu\u00dft sich auff! O GOtt! welch fr\u00f6lich", "tokens": ["Der", "Him\u00b7mel", "schleu\u00dft", "sich", "auff", "!", "O", "Gott", "!", "welch", "fr\u00f6\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$.", "NE", "NN", "$.", "PWAT", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "fcheiden! ", "tokens": ["fchei\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die Erden reist entzwey. Welch weh/ welch schrecklich leid\u1ebd.", "tokens": ["Die", "Er\u00b7den", "reist", "ent\u00b7zwey", ".", "Welch", "weh", "/", "welch", "schreck\u00b7lich", "lei\u00b7d\u1ebd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PIAT", "NN", "$(", "PWAT", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weh/ weh dem/ der verdamm\u2019t: wol dem der ", "tokens": ["Weh", "/", "weh", "dem", "/", "der", "ver\u00b7dam\u00b7m't", ":", "wol", "dem", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "ADV", "ART", "$(", "ART", "VVFIN", "$.", "ADV", "ART", "ART"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}}}}