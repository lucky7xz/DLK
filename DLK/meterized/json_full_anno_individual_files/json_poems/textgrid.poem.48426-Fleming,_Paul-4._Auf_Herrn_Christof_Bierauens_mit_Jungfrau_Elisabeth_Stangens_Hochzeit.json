{"textgrid.poem.48426": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf Herrn Christof Bierauens mit Jungfrau Elisabeth Stangens Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne wolte gleich ietzt aus den Fischen schreiten,", "tokens": ["Die", "Son\u00b7ne", "wol\u00b7te", "gleich", "ietzt", "aus", "den", "Fi\u00b7schen", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Himmel stund erstarrt, die wei\u00dfen Wolken speiten", "tokens": ["der", "Him\u00b7mel", "stund", "er\u00b7starrt", ",", "die", "wei\u00b7\u00dfen", "Wol\u00b7ken", "spei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die d\u00fcrre Flut, den Schnee; die Erde war ganz greis", "tokens": ["die", "d\u00fcr\u00b7re", "Flut", ",", "den", "Schnee", ";", "die", "Er\u00b7de", "war", "ganz", "greis"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und runzlicht an der Haut; die Fluten hatten Eis,", "tokens": ["und", "runz\u00b7licht", "an", "der", "Haut", ";", "die", "Flu\u00b7ten", "hat\u00b7ten", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Felder Flocken um; zur Zeit, wenn Mars nicht kriegen,", "tokens": ["die", "Fel\u00b7der", "Flo\u00b7cken", "um", ";", "zur", "Zeit", ",", "wenn", "Mars", "nicht", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$.", "APPRART", "NN", "$,", "KOUS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie er gern wolte, kan, mu\u00df in Quartieren liegen,", "tokens": ["wie", "er", "gern", "wol\u00b7te", ",", "kan", ",", "mu\u00df", "in", "Quar\u00b7tie\u00b7ren", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "VMFIN", "$,", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da ihm denn auch ist wol. \u2013 Wiewol man itzt gewohnt,", "tokens": ["da", "ihm", "denn", "auch", "ist", "wol", ".", "\u2013", "Wie\u00b7wol", "man", "itzt", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADV", "$.", "$(", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df man bei Winters auch des Feindes nicht verschont,", "tokens": ["da\u00df", "man", "bei", "Win\u00b7ters", "auch", "des", "Fein\u00b7des", "nicht", "ver\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "ADV", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "wie das mein Teutsches Land gelernet hat von Norden,", "tokens": ["wie", "das", "mein", "Teut\u00b7sches", "Land", "ge\u00b7ler\u00b7net", "hat", "von", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der kriegerischen Welt. Wir sind Soldaten worden", "tokens": ["der", "krie\u00b7ge\u00b7ri\u00b7schen", "Welt", ".", "Wir", "sind", "Sol\u00b7da\u00b7ten", "wor\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "NN", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und gehn den Ahnen gleich. So lange kriegen wir", "tokens": ["und", "gehn", "den", "Ah\u00b7nen", "gleich", ".", "So", "lan\u00b7ge", "krie\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und kriegen minder doch als so viel nichts daf\u00fcr,", "tokens": ["und", "krie\u00b7gen", "min\u00b7der", "doch", "als", "so", "viel", "nichts", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "KOKOM", "ADV", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "verkriegen Gut und Geist. \u2013 Nun, eben dieser Tage", "tokens": ["ver\u00b7krie\u00b7gen", "Gut", "und", "Geist", ".", "\u2013", "Nun", ",", "e\u00b7ben", "die\u00b7ser", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$.", "$(", "ADV", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "begab sichs, da\u00df Gott Mars auch in der Ruhe lage,", "tokens": ["be\u00b7gab", "sichs", ",", "da\u00df", "Gott", "Mars", "auch", "in", "der", "Ru\u00b7he", "la\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "NN", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "sein Hauptquartier war hier. Frau Venus, wie man wei\u00df,", "tokens": ["sein", "Haupt\u00b7quar\u00b7tier", "war", "hier", ".", "Frau", "Ve\u00b7nus", ",", "wie", "man", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$.", "NN", "NE", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "pflegt nicht fern' ab zu sein. Es friere noch solch Eis,", "tokens": ["pflegt", "nicht", "fern'", "ab", "zu", "sein", ".", "Es", "frie\u00b7re", "noch", "solch", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "PTKVZ", "PTKZU", "VAINF", "$.", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "es drehe wie es will, sie l\u00e4\u00dft sich nichts erhalten,", "tokens": ["es", "dre\u00b7he", "wie", "es", "will", ",", "sie", "l\u00e4\u00dft", "sich", "nichts", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PRF", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "reist ihren Buhlen nach, versperrt den lahmen Alten", "tokens": ["reist", "ih\u00b7ren", "Buh\u00b7len", "nach", ",", "ver\u00b7sperrt", "den", "lah\u00b7men", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und l\u00e4\u00dft ihn h\u00e4mmern wol. Wie denn der gute Man", "tokens": ["und", "l\u00e4\u00dft", "ihn", "h\u00e4m\u00b7mern", "wol", ".", "Wie", "denn", "der", "gu\u00b7te", "Man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ADV", "$.", "PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "itzt so viel hat zu tun, da\u00df er nicht schlafen kan:", "tokens": ["itzt", "so", "viel", "hat", "zu", "tun", ",", "da\u00df", "er", "nicht", "schla\u00b7fen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "er soll, wei\u00df nicht wie viel der Harnsche fertig haben", "tokens": ["er", "soll", ",", "wei\u00df", "nicht", "wie", "viel", "der", "Harn\u00b7sche", "fer\u00b7tig", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PTKNEG", "KOKOM", "ADV", "ART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "bald auf den ersten Mai. Indessen kan sich laben", "tokens": ["bald", "auf", "den", "ers\u00b7ten", "Mai", ".", "In\u00b7des\u00b7sen", "kan", "sich", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "die Venus, wie sie will. So viel Zeit hat er nicht,", "tokens": ["die", "Ve\u00b7nus", ",", "wie", "sie", "will", ".", "So", "viel", "Zeit", "hat", "er", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VMFIN", "$.", "ADV", "PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "da\u00df er seh' eins darnach, ob sie noch brenne Liecht,", "tokens": ["da\u00df", "er", "seh'", "eins", "dar\u00b7nach", ",", "ob", "sie", "noch", "bren\u00b7ne", "Liecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIS", "PAV", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "ob sie entschlummert sei, ob sie sei extra gangen.", "tokens": ["ob", "sie", "ent\u00b7schlum\u00b7mert", "sei", ",", "ob", "sie", "sei", "ex\u00b7tra", "gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Zu dem so hat er auch nicht so ein gro\u00df Verlangen", "tokens": ["Zu", "dem", "so", "hat", "er", "auch", "nicht", "so", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "mit ihr verliebt zu tun. Sie h\u00e4lt ihn auch nicht gro\u00df,", "tokens": ["mit", "ihr", "ver\u00b7liebt", "zu", "tun", ".", "Sie", "h\u00e4lt", "ihn", "auch", "nicht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "PTKZU", "VVINF", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "im Fall' sie liegen kan in eines Andern Scho\u00df,", "tokens": ["im", "Fall'", "sie", "lie\u00b7gen", "kan", "in", "ei\u00b7nes", "An\u00b7dern", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "und der es besser kan. Gleich als sie nun vernommen,", "tokens": ["und", "der", "es", "bes\u00b7ser", "kan", ".", "Gleich", "als", "sie", "nun", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "VMFIN", "$.", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "da\u00df ihr geliebter Freund allhier sei angekommen,", "tokens": ["da\u00df", "ihr", "ge\u00b7lieb\u00b7ter", "Freund", "all\u00b7hier", "sei", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "der eben auch nach ihr ein solch Verlangen trug,", "tokens": ["der", "e\u00b7ben", "auch", "nach", "ihr", "ein", "solch", "Ver\u00b7lan\u00b7gen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "als wie sie wol nach ihm, sprach sie: Nun hab' ich Fug;", "tokens": ["als", "wie", "sie", "wol", "nach", "ihm", ",", "sprach", "sie", ":", "Nun", "hab'", "ich", "Fug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ADV", "APPR", "PPER", "$,", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "mein Sohn, du solst mit mir! Lauf, spanne vor die Tauben!", "tokens": ["mein", "Sohn", ",", "du", "solst", "mit", "mir", "!", "Lauf", ",", "span\u00b7ne", "vor", "die", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "$.", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Ihr andern bringt mir Schmuck, Geschmeide, R\u00f6cke, Schauben", "tokens": ["Ihr", "an\u00b7dern", "bringt", "mir", "Schmuck", ",", "Ge\u00b7schmei\u00b7de", ",", "R\u00f6\u00b7cke", ",", "Schau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "und was mein Sch\u00f6nstes ist! Die G\u00f6ttin setzt sich auf", "tokens": ["und", "was", "mein", "Sch\u00f6ns\u00b7tes", "ist", "!", "Die", "G\u00f6t\u00b7tin", "setzt", "sich", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "und f\u00e4hrt also davon: die helle Zunft folgt drauf.", "tokens": ["und", "f\u00e4hrt", "al\u00b7so", "da\u00b7von", ":", "die", "hel\u00b7le", "Zunft", "folgt", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$.", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Sonne wolte gleich ietzt aus den Fischen schreiten,", "tokens": ["Die", "Son\u00b7ne", "wol\u00b7te", "gleich", "ietzt", "aus", "den", "Fi\u00b7schen", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Himmel stund erstarrt, die wei\u00dfen Wolken speiten", "tokens": ["der", "Him\u00b7mel", "stund", "er\u00b7starrt", ",", "die", "wei\u00b7\u00dfen", "Wol\u00b7ken", "spei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die d\u00fcrre Flut, den Schnee; die Erde war ganz greis", "tokens": ["die", "d\u00fcr\u00b7re", "Flut", ",", "den", "Schnee", ";", "die", "Er\u00b7de", "war", "ganz", "greis"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und runzlicht an der Haut; die Fluten hatten Eis,", "tokens": ["und", "runz\u00b7licht", "an", "der", "Haut", ";", "die", "Flu\u00b7ten", "hat\u00b7ten", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die Felder Flocken um; zur Zeit, wenn Mars nicht kriegen,", "tokens": ["die", "Fel\u00b7der", "Flo\u00b7cken", "um", ";", "zur", "Zeit", ",", "wenn", "Mars", "nicht", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$.", "APPRART", "NN", "$,", "KOUS", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie er gern wolte, kan, mu\u00df in Quartieren liegen,", "tokens": ["wie", "er", "gern", "wol\u00b7te", ",", "kan", ",", "mu\u00df", "in", "Quar\u00b7tie\u00b7ren", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "VMFIN", "$,", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "da ihm denn auch ist wol. \u2013 Wiewol man itzt gewohnt,", "tokens": ["da", "ihm", "denn", "au