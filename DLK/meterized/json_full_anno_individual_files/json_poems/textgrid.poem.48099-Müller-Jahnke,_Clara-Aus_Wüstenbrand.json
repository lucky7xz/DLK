{"textgrid.poem.48099": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Aus W\u00fcstenbrand", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, den ich ahnte in den heiligen Stunden,", "tokens": ["Du", ",", "den", "ich", "ahn\u00b7te", "in", "den", "hei\u00b7li\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "da meine Sehnsucht nach den H\u00f6hen flog, \u2013", "tokens": ["da", "mei\u00b7ne", "Sehn\u00b7sucht", "nach", "den", "H\u00f6\u00b7hen", "flog", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "den ich gesucht und den ich nicht gefunden,", "tokens": ["den", "ich", "ge\u00b7sucht", "und", "den", "ich", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "ART", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "du, dessen Hauch wie Sturmwind mich bewegt,", "tokens": ["du", ",", "des\u00b7sen", "Hauch", "wie", "Sturm\u00b7wind", "mich", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "KOKOM", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der aus des Hinduf\u00fcrsten Purpurg\u00e4rten", "tokens": ["der", "aus", "des", "Hin\u00b7du\u00b7f\u00fcrs\u00b7ten", "Pur\u00b7pur\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den Bl\u00fctenstaub in Schiras Kelche tr\u00e4gt, \u2013", "tokens": ["den", "Bl\u00fc\u00b7ten\u00b7staub", "in", "Schi\u00b7ras", "Kel\u00b7che", "tr\u00e4gt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "du, dessen Lied mir im Geraun der Wogen", "tokens": ["du", ",", "des\u00b7sen", "Lied", "mir", "im", "Ge\u00b7raun", "der", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit Schmeichellauten in die Seele sang,", "tokens": ["mit", "Schmei\u00b7chel\u00b7lau\u00b7ten", "in", "die", "See\u00b7le", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du, der mir rief und der mir stets gelogen . . . ..", "tokens": ["du", ",", "der", "mir", "rief", "und", "der", "mir", "stets", "ge\u00b7lo\u00b7gen", ".", ".", ".", ".."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "KON", "ART", "PPER", "ADV", "VVPP", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "den ich in Mittern\u00e4chten toller Lust", "tokens": ["den", "ich", "in", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten", "tol\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "auf meinen Br\u00fcsten zu verbrennen w\u00e4hnte,", "tokens": ["auf", "mei\u00b7nen", "Br\u00fcs\u00b7ten", "zu", "ver\u00b7bren\u00b7nen", "w\u00e4hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "o du, o du, von dem ich ", "tokens": ["o", "du", ",", "o", "du", ",", "von", "dem", "ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["FM", "PPER", "$,", "FM", "PPER", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u2013 Schau her! Am Steinger\u00f6ll und Dorngeheg", "tokens": ["\u2013", "Schau", "her", "!", "Am", "Stein\u00b7ge\u00b7r\u00f6ll", "und", "Dorn\u00b7ge\u00b7heg"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "PTKVZ", "$.", "APPRART", "NN", "KON", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "flattern die Fetzen meines Pilgerkleides.", "tokens": ["flat\u00b7tern", "die", "Fet\u00b7zen", "mei\u00b7nes", "Pil\u00b7ger\u00b7klei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "blutige Spuren zeichnen meinen Weg.", "tokens": ["blu\u00b7ti\u00b7ge", "Spu\u00b7ren", "zeich\u00b7nen", "mei\u00b7nen", "Weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Auf meinem Scheitel liegt der Staub der W\u00fcste,", "tokens": ["Auf", "mei\u00b7nem", "Schei\u00b7tel", "liegt", "der", "Staub", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mein Auge sp\u00e4ht, von Glut und Tr\u00e4nen blind,", "tokens": ["mein", "Au\u00b7ge", "sp\u00e4ht", ",", "von", "Glut", "und", "Tr\u00e4\u00b7nen", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "vergeblich aus nach der verhei\u00dfnen K\u00fcste.", "tokens": ["ver\u00b7geb\u00b7lich", "aus", "nach", "der", "ver\u00b7hei\u00df\u00b7nen", "K\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Von finstern Schroffen, die kein Fu\u00df erklomm,", "tokens": ["Von", "fins\u00b7tern", "Schrof\u00b7fen", ",", "die", "kein", "Fu\u00df", "er\u00b7klomm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aus brennendem Sand, den K\u00f6nig Tod sich w\u00e4hlte", "tokens": ["aus", "bren\u00b7nen\u00b7dem", "Sand", ",", "den", "K\u00f6\u00b7nig", "Tod", "sich", "w\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "NN", "PRF", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "zum Sommersitz, klingt mir dein Lockruf: \u00bbKomm!\u00ab", "tokens": ["zum", "Som\u00b7mer\u00b7sitz", ",", "klingt", "mir", "dein", "Lock\u00b7ruf", ":", "\u00bb", "Komm", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und diesen Weg, von Jammerlaut und Fluchen", "tokens": ["Und", "die\u00b7sen", "Weg", ",", "von", "Jam\u00b7mer\u00b7laut", "und", "Flu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "erf\u00fcllt, vom Blut Verlorener ged\u00fcngt,", "tokens": ["er\u00b7f\u00fcllt", ",", "vom", "Blut", "Ver\u00b7lo\u00b7re\u00b7ner", "ge\u00b7d\u00fcngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.3": {"text": "ich mu\u00df ihn wandern, denn ich mu\u00df dich suchen! \u2013", "tokens": ["ich", "mu\u00df", "ihn", "wan\u00b7dern", ",", "denn", "ich", "mu\u00df", "dich", "su\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "$,", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O du, der mich wie blinkend Glas durchschaut,", "tokens": ["O", "du", ",", "der", "mich", "wie", "blin\u00b7kend", "Glas", "durch\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPER", "KOKOM", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der auf den trotzigen Tr\u00fcmmern meines Wollens", "tokens": ["der", "auf", "den", "trot\u00b7zi\u00b7gen", "Tr\u00fcm\u00b7mern", "mei\u00b7nes", "Wol\u00b7lens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "der eignen St\u00e4rke Hochburg lachend baut:", "tokens": ["der", "eig\u00b7nen", "St\u00e4r\u00b7ke", "Hoch\u00b7burg", "la\u00b7chend", "baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "wenn dich ein Frauenscho\u00df in Leid geboren,", "tokens": ["wenn", "dich", "ein", "Frau\u00b7en\u00b7scho\u00df", "in", "Leid", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenn eine Mutter liebend dich gek\u00fc\u00dft,", "tokens": ["wenn", "ei\u00b7ne", "Mut\u00b7ter", "lie\u00b7bend", "dich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "so la\u00df dich finden, sei mir nicht verloren!", "tokens": ["so", "la\u00df", "dich", "fin\u00b7den", ",", "sei", "mir", "nicht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So la\u00df dich finden: deiner harrt die Kraft,", "tokens": ["So", "la\u00df", "dich", "fin\u00b7den", ":", "dei\u00b7ner", "harrt", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$.", "PPOSAT", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die Kraft der H\u00f6hen, die da Freiheit spendet,", "tokens": ["die", "Kraft", "der", "H\u00f6\u00b7hen", ",", "die", "da", "Frei\u00b7heit", "spen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Kraft der Tiefen, die da Leben schafft!", "tokens": ["die", "Kraft", "der", "Tie\u00b7fen", ",", "die", "da", "Le\u00b7ben", "schafft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wenn schon dein Schatten mir ein flutend Leben,", "tokens": ["Wenn", "schon", "dein", "Schat\u00b7ten", "mir", "ein", "flu\u00b7tend", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dein blasses Abbild mir ein H\u00f6hengl\u00fcck", "tokens": ["dein", "blas\u00b7ses", "Ab\u00b7bild", "mir", "ein", "H\u00f6\u00b7hen\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und aller Schmerzen Herrlichkeit gegeben \u2013", "tokens": ["und", "al\u00b7ler", "Schmer\u00b7zen", "Herr\u00b7lich\u00b7keit", "ge\u00b7ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "so komm du selbst! Komm eh die Kraft versagt", "tokens": ["so", "komm", "du", "selbst", "!", "Komm", "eh", "die", "Kraft", "ver\u00b7sagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und \u00fcber Bestien, die mein Herzblut trinken,", "tokens": ["und", "\u00fc\u00b7ber", "Be\u00b7sti\u00b7en", ",", "die", "mein", "Herz\u00b7blut", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "der fahle W\u00fcstenhimmel tagt . . . . . . .", "tokens": ["der", "fah\u00b7le", "W\u00fcs\u00b7ten\u00b7him\u00b7mel", "tagt", ".", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Du, den ich ahnte in den heiligen Stunden,", "tokens": ["Du", ",", "den", "ich", "ahn\u00b7te", "in", "den", "hei\u00b7li\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "APPR", "A