{"textgrid.poem.45851": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbeine Liebe ist der andern wert\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umh\u00fcllst Du Dich mit Ernst und tiefem Schweigen", "tokens": ["Um\u00b7h\u00fcllst", "Du", "Dich", "mit", "Ernst", "und", "tie\u00b7fem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sprichst von K\u00e4lte und von nicht verstehn", "tokens": ["Und", "sprichst", "von", "K\u00e4l\u00b7te", "und", "von", "nicht", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPR", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sehe ich Dianen vor mir stehn", "tokens": ["So", "se\u00b7he", "ich", "Di\u00b7a\u00b7nen", "vor", "mir", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmermehr wird sich ihr Sinn erweichen,", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "wird", "sich", "ihr", "Sinn", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch lieber m\u00f6cht ich Ph\u00f6ben Dich vergleichen", "tokens": ["Doch", "lie\u00b7ber", "m\u00f6cht", "ich", "Ph\u00f6\u00b7ben", "Dich", "ver\u00b7glei\u00b7chen"], "token_