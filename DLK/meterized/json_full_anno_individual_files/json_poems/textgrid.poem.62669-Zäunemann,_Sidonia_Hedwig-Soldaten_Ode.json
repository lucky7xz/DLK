{"textgrid.poem.62669": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Soldaten Ode", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Br\u00fcder! Ihr Br\u00fcder!", "tokens": ["Ihr", "Br\u00fc\u00b7der", "!", "Ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seyd lustig mit mir!", "tokens": ["Seyd", "lus\u00b7tig", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Werft B\u00fccher, Papir", "tokens": ["Werft", "B\u00fc\u00b7cher", ",", "Pa\u00b7pir"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "NN", "$,", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Darnieder, Darnieder;", "tokens": ["Dar\u00b7nie\u00b7der", ",", "Dar\u00b7nie\u00b7der", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PAV", "$,", "PAV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich suche stat der Dinte", "tokens": ["Ich", "su\u00b7che", "stat", "der", "Din\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blut, Degen, Helm und Flinte,", "tokens": ["Blut", ",", "De\u00b7gen", ",", "Helm", "und", "Flin\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "R\u00e4um das Herz dem Mavor ein,", "tokens": ["R\u00e4um", "das", "Herz", "dem", "Ma\u00b7vor", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So kan ich vergn\u00fcget seyn.", "tokens": ["So", "kan", "ich", "ver\u00b7gn\u00fc\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mars schenket, Mars schenket", "tokens": ["Mars", "schen\u00b7ket", ",", "Mars", "schen\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vergn\u00fcgen und Lust,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "und", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Seele und Brust", "tokens": ["Die", "See\u00b7le", "und", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Gedenket, gedenket:", "tokens": ["Ge\u00b7den\u00b7ket", ",", "ge\u00b7den\u00b7ket", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich will mit Martis Waffen", "tokens": ["Ich", "will", "mit", "Mar\u00b7tis", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir reiche Beute schaffen:", "tokens": ["Mir", "rei\u00b7che", "Beu\u00b7te", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da mein Herze in der Stadt", "tokens": ["Da", "mein", "Her\u00b7ze", "in", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenig sonst zu hoffen hat.", "tokens": ["We\u00b7nig", "sonst", "zu", "hof\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sechs Batzen, sechs Batzen", "tokens": ["Sechs", "Bat\u00b7zen", ",", "sechs", "Bat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "$,", "CARD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Giebt dieser ohn Streit,", "tokens": ["Giebt", "die\u00b7ser", "ohn", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der auf dem Ro\u00df reit't.", "tokens": ["Der", "auf", "dem", "Ro\u00df", "reit'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Drum schwatzen, drum schwatzen", "tokens": ["Drum", "schwat\u00b7zen", ",", "drum", "schwat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PAV", "VVINF", "$,", "PAV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Lippen: Ich will reiten", "tokens": ["Die", "Lip\u00b7pen", ":", "Ich", "will", "rei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VMFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ohn Geld, bey Krieges-Leuten.", "tokens": ["Ohn", "Geld", ",", "bey", "Krie\u00b7ge\u00b7sLeu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ey! was frag ich nach dem Pferd,", "tokens": ["Ey", "!", "was", "frag", "ich", "nach", "dem", "Pferd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bin ich doch des Esels werth.", "tokens": ["Bin", "ich", "doch", "des", "E\u00b7sels", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Soldaten, Soldaten", "tokens": ["Sol\u00b7da\u00b7ten", ",", "Sol\u00b7da\u00b7ten"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind munter und froh,", "tokens": ["Sind", "mun\u00b7ter", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf Betten und Stroh.", "tokens": ["Auf", "Bet\u00b7ten", "und", "Stroh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie rathen, sie rathen,", "tokens": ["Sie", "ra\u00b7then", ",", "sie", "ra\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie sie in andern St\u00e4dtgen", "tokens": ["Wie", "sie", "in", "an\u00b7dern", "St\u00e4dt\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch wieder andre M\u00e4dgen,", "tokens": ["Auch", "wie\u00b7der", "and\u00b7re", "M\u00e4d\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sich zum Zeitvertreib ersehn,", "tokens": ["Sich", "zum", "Zeit\u00b7ver\u00b7treib", "er\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zu ihnen spiele gehn.", "tokens": ["Und", "zu", "ih\u00b7nen", "spie\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bey Pfeifen, bey Pfeifen", "tokens": ["Bey", "Pfei\u00b7fen", ",", "bey", "Pfei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Kriegt mancher am Tag", "tokens": ["Kriegt", "man\u00b7cher", "am", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sto\u00df, Ruthen und Schlag,", "tokens": ["Sto\u00df", ",", "Ru\u00b7then", "und", "Schlag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sie greifen, sie greifen", "tokens": ["Sie", "grei\u00b7fen", ",", "sie", "grei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Trummeln mit viel Freuden.", "tokens": ["Die", "Trum\u00b7meln", "mit", "viel", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es weichet Gram und Leiden,", "tokens": ["Es", "wei\u00b7chet", "Gram", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Giebt nur stets der Officier", "tokens": ["Giebt", "nur", "stets", "der", "Of\u00b7fi\u00b7cier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Den Soldaten Geld zu Bier.", "tokens": ["Den", "Sol\u00b7da\u00b7ten", "Geld", "zu", "Bier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit siegen, mit siegen,", "tokens": ["Mit", "sie\u00b7gen", ",", "mit", "sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wird alles vollbracht.", "tokens": ["Wird", "al\u00b7les", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer dieses betracht,", "tokens": ["Wer", "die\u00b7ses", "be\u00b7tracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mu\u00df springen, mu\u00df springen:", "tokens": ["Mu\u00df", "sprin\u00b7gen", ",", "mu\u00df", "sprin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Denn der Soldaten Wesen", "tokens": ["Denn", "der", "Sol\u00b7da\u00b7ten", "We\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat Lustbarkeit erlesen;", "tokens": ["Hat", "Lust\u00b7bar\u00b7keit", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "An dem Morgen, bey der Ruh", "tokens": ["An", "dem", "Mor\u00b7gen", ",", "bey", "der", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gehet alles lustig zu.", "tokens": ["Ge\u00b7het", "al\u00b7les", "lus\u00b7tig", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenns heiset, wenns heiset:", "tokens": ["Wenns", "hei\u00b7set", ",", "wenns", "hei\u00b7set", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "KOUS", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf! ziehet ins Feld!", "tokens": ["Auf", "!", "zie\u00b7het", "ins", "Feld", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Erbeutet euch Geld!", "tokens": ["Er\u00b7beu\u00b7tet", "euch", "Geld", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da preiset, da preiset", "tokens": ["Da", "prei\u00b7set", ",", "da", "prei\u00b7set"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Der Mund die frohen Stunden,", "tokens": ["Der", "Mund", "die", "fro\u00b7hen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die sich darzu gefunden,", "tokens": ["Die", "sich", "dar\u00b7zu", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da nunmehr das Gl\u00fccke bl\u00fcht,", "tokens": ["Da", "nun\u00b7mehr", "das", "Gl\u00fc\u00b7cke", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil man seinen Feind bezieht.", "tokens": ["Weil", "man", "sei\u00b7nen", "Feind", "be\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Marschieret, marschieret,", "tokens": ["Mar\u00b7schie\u00b7ret", ",", "mar\u00b7schie\u00b7ret", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Und k\u00e4mpfet mit Flei\u00df:", "tokens": ["Und", "k\u00e4mp\u00b7fet", "mit", "Flei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So folget der Prei\u00df.", "tokens": ["So", "fol\u00b7get", "der", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Vollf\u00fchret, vollf\u00fchret", "tokens": ["Voll\u00b7f\u00fch\u00b7ret", ",", "voll\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Was Mavor angefangen,", "tokens": ["Was", "Ma\u00b7vor", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So wird man siegend prangen:", "tokens": ["So", "wird", "man", "sie\u00b7gend", "pran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und man tr\u00e4gt die Ehren-Kron,", "tokens": ["Und", "man", "tr\u00e4gt", "die", "Eh\u00b7ren\u00b7Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu der Feinde Schmach davon.", "tokens": ["Zu", "der", "Fein\u00b7de", "Schmach", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir bleiben, wir bleiben", "to