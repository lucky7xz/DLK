{"textgrid.poem.66816": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbach, bleibt uns vom Leibe doch meilenweit", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach, bleibt uns vom Leibe doch meilenweit", "tokens": ["\u00bb", "ach", ",", "bleibt", "uns", "vom", "Lei\u00b7be", "doch", "mei\u00b7len\u00b7weit"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "VVFIN", "PPER", "APPRART", "NN", "ADV", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit euren lumpigen Gaben!", "tokens": ["Mit", "eu\u00b7ren", "lum\u00b7pi\u00b7gen", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen keine Wohlt\u00e4tigkeit,", "tokens": ["Wir", "wol\u00b7len", "kei\u00b7ne", "Wohl\u00b7t\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gerechtigkeit wollen wir haben.", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "wol\u00b7len", "wir", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Ihr d\u00fcnkt euch noch gut, philanthropisch, human:", "tokens": ["Ihr", "d\u00fcnkt", "euch", "noch", "gut", ",", "phil\u00b7an\u00b7th\u00b7ro\u00b7pisch", ",", "hu\u00b7man", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADJD", "$,