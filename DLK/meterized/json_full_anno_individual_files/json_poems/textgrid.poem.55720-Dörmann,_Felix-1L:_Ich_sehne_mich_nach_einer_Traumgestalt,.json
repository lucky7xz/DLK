{"textgrid.poem.55720": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehne mich nach einer Traumgestalt,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehne mich nach einer Traumgestalt,", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "ei\u00b7ner", "Traum\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach einem unber\u00fchrten, keuschen Wesen,", "tokens": ["Nach", "ei\u00b7nem", "un\u00b7be\u00b7r\u00fchr\u00b7ten", ",", "keu\u00b7schen", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das noch im Buch der S\u00fcnde nicht gelesen,", "tokens": ["Das", "noch", "im", "Buch", "der", "S\u00fcn\u00b7de", "nicht", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Wollust nicht einmal im Geist umkrallt.", "tokens": ["Das", "Wol\u00b7lust", "nicht", "ein\u00b7mal", "im", "Geist", "um\u00b7krallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In ihrer Seele m\u00fc\u00dfte Mitleid wohnen", "tokens": ["In", "ih\u00b7rer", "See\u00b7le", "m\u00fc\u00df\u00b7te", "Mit\u00b7leid", "woh\u00b7nen"], "token_info": ["wo