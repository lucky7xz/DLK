ine.6": {"text": "Du suchst, du pflegst die kranken Kinder.", "tokens": ["Du", "suchst", ",", "du", "pflegst", "die", "kran\u00b7ken", "Kin\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gib mir dein Herz, mach meins auch warm,", "tokens": ["Gib", "mir", "dein", "Herz", ",", "mach", "meins", "auch", "warm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wann ich sterb', dich mein erbarm!", "tokens": ["Und", "wann", "ich", "sterb'", ",", "dich", "mein", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "PPER", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "O Gottes Herz, Barmherzigkeit,", "tokens": ["O", "Got\u00b7tes", "Herz", ",", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein sanfter Trost in h\u00f6chstem Leid,", "tokens": ["Mein", "sanf\u00b7ter", "Trost", "in", "h\u00f6chs\u00b7tem", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-