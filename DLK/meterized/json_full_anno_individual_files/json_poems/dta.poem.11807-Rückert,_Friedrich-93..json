{"dta.poem.11807": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "93.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es geht ein schmaler Weg hin zwischen Strom und Klippe,", "tokens": ["Es", "geht", "ein", "schma\u00b7ler", "Weg", "hin", "zwi\u00b7schen", "Strom", "und", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Wandrer mittendurch geht mit verlechzter Lippe.", "tokens": ["Ein", "Wand\u00b7rer", "mit\u00b7ten\u00b7durch", "geht", "mit", "ver\u00b7lechz\u00b7ter", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Durst zu l\u00f6schen, k\u00f6nnt' er hier am Strome nippen,", "tokens": ["Den", "Durst", "zu", "l\u00f6\u00b7schen", ",", "k\u00f6nnt'", "er", "hier", "am", "Stro\u00b7me", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an den Beeren dort, die wachsen auf den Klippen.", "tokens": ["Und", "an", "den", "Bee\u00b7ren", "dort", ",", "die", "wach\u00b7sen", "auf", "den", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch doppelte Gefahr droht her von Strom und Klippe,", "tokens": ["Doch", "dop\u00b7pel\u00b7te", "Ge\u00b7fahr", "droht", "her", "von", "Strom", "und", "Klip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lieber weiter geht er mit verlechzter Lippe.", "tokens": ["Und", "lie\u00b7ber", "wei\u00b7ter", "geht", "er", "mit", "ver\u00b7lechz\u00b7ter", "Lip\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn unten lauscht im Schilf des Stroms ein Krokodill,", "tokens": ["Denn", "un\u00b7ten", "lauscht", "im", "Schilf", "des", "Stroms", "ein", "Kro\u00b7ko\u00b7dill", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und oben im Geb\u00fcsch der Klipp' ein Tieger still.", "tokens": ["Und", "o\u00b7ben", "im", "Ge\u00b7b\u00fcsch", "der", "Klipp'", "ein", "Tie\u00b7ger", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn der Wandrer still und schnell nicht geht die Bahn,", "tokens": ["Und", "wenn", "der", "Wand\u00b7rer", "still", "und", "schnell", "nicht", "geht", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKVZ", "KON", "ADJD", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00e4llt hier Krokodill und dort ihn Tieger an.", "tokens": ["So", "f\u00e4llt", "hier", "Kro\u00b7ko\u00b7dill", "und", "dort", "ihn", "Tie\u00b7ger", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "KON", "ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er denkt: w\u00e4r' ich der Hund, der gleiche Sorge f\u00fchlt", "tokens": ["Er", "denkt", ":", "w\u00e4r'", "ich", "der", "Hund", ",", "der", "glei\u00b7che", "Sor\u00b7ge", "f\u00fchlt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit gleichem Durst, und ihn am Strom im Laufen k\u00fchlt.", "tokens": ["Mit", "glei\u00b7chem", "Durst", ",", "und", "ihn", "am", "Strom", "im", "Lau\u00b7fen", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "PPER", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "W\u00e4r' ich das V\u00f6gelein, das auf der Klippe nascht,", "tokens": ["W\u00e4r'", "ich", "das", "V\u00f6\u00b7ge\u00b7lein", ",", "das", "auf", "der", "Klip\u00b7pe", "nascht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' Aengste, da\u00df nach ihm der gro\u00dfe W\u00fcrger hascht.", "tokens": ["Ohn'", "A\u00b7engs\u00b7te", ",", "da\u00df", "nach", "ihm", "der", "gro\u00b7\u00dfe", "W\u00fcr\u00b7ger", "hascht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.8": {"line.1": {"text": "W\u00e4r ich der Gott des Orts! den Wanderern zum Segen", "tokens": ["W\u00e4r", "ich", "der", "Gott", "des", "Orts", "!", "den", "Wan\u00b7de\u00b7rern", "zum", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$.", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchrt' ich das Krokodill dem Tieger selbst entgegen;", "tokens": ["F\u00fchrt'", "ich", "das", "Kro\u00b7ko\u00b7dill", "dem", "Tie\u00b7ger", "selbst", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Da\u00df aneinander selbst sie stumpften ihren Zahn,", "tokens": ["Da\u00df", "an\u00b7ein\u00b7an\u00b7der", "selbst", "sie", "stumpf\u00b7ten", "ih\u00b7ren", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sicher k\u00fcnftig gieng' ein Wandrer diese Bahn.", "tokens": ["Und", "si\u00b7cher", "k\u00fcnf\u00b7tig", "gieng'", "ein", "Wand\u00b7rer", "die\u00b7se", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}