mal", "tokens": ["wenn", "die", "B\u00fcr\u00b7ger", "auch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "gottlos, wie der Adel t\u00e4ten!", "tokens": ["gott\u00b7los", ",", "wie", "der", "A\u00b7del", "t\u00e4\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nein, das Volk, das Volk mu\u00df beten,", "tokens": ["Nein", ",", "das", "Volk", ",", "das", "Volk", "mu\u00df", "be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "beten \u2013 oder doch so tun.", "tokens": ["be\u00b7ten", "\u2013", "o\u00b7der", "doch", "so", "tun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, wenn ich es recht ermesse,", "tokens": ["Ja", ",", "wenn", "ich", "es", "recht", "er\u00b7mes\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kann vielleicht sogar die Presse", "tokens": ["kann", "viel\u00b7leicht", "so\u00b7gar", "die", "Pres\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "f\u00fcr Beamte und Noblesse", "tokens": ["f\u00fcr", "Be\u00b7am\u00b7te", "und", "Nob\u00b7les\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "schon ein wenig freier sein.", "tokens": ["schon", "ein", "we\u00b7nig", "frei\u00b7er", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber f\u00fcr die andern? Nein!", "tokens": ["A\u00b7ber", "f\u00fcr", "die", "an\u00b7dern", "?", "Nein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein f\u00fcrwahr, das Volk mu\u00df schweigen.", "tokens": ["Nein", "f\u00fcr\u00b7wahr", ",", "das", "Volk", "mu\u00df", "schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer gehorchen will, sei stumm;", "tokens": ["Wer", "ge\u00b7hor\u00b7chen", "will", ",", "sei", "stumm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "schweigend wird das Publikum", "tokens": ["schwei\u00b7gend", "wird", "das", "Pub\u00b7li\u00b7kum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "stets sich am loyalsten zeigen:", "tokens": ["stets", "sich", "am", "lo\u00b7yals\u00b7ten", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "