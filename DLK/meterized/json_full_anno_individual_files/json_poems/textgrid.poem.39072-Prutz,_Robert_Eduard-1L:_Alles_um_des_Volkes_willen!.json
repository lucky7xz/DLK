{"textgrid.poem.39072": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alles um des Volkes willen!", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles um des Volkes willen!", "tokens": ["Al\u00b7les", "um", "des", "Vol\u00b7kes", "wil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seht, ich lache selbst im stillen", "tokens": ["seht", ",", "ich", "la\u00b7che", "selbst", "im", "stil\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "dieser Bibeln und Postillen", "tokens": ["die\u00b7ser", "Bi\u00b7beln", "und", "Pos\u00b7til\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und da\u00df man so gl\u00e4ubig ist:", "tokens": ["und", "da\u00df", "man", "so", "gl\u00e4u\u00b7big", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich, f\u00fcr mich, bin Atheist!", "tokens": ["Ich", ",", "f\u00fcr", "mich", ",", "bin", "A\u00b7theist", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPER", "$,", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Doch das Volk, das Volk mu\u00df glauben!", "tokens": ["Doch", "das", "Volk", ",", "das", "Volk", "mu\u00df", "glau\u00b7ben", "!"], "token