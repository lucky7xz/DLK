{"textgrid.poem.53365": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[wir haben, Herr, ein festes Wort]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben, Herr, ein festes Wort,", "tokens": ["Wir", "ha\u00b7ben", ",", "Herr", ",", "ein", "fes\u00b7tes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darumb uns keiner Hellen Mord", "tokens": ["Da\u00b7rumb", "uns", "kei\u00b7ner", "Hel\u00b7len", "Mord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ewigkeit wird bringen,", "tokens": ["In", "E\u00b7wig\u00b7keit", "wird", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du uns kennest allerseit", "tokens": ["Da\u00df", "du", "uns", "ken\u00b7nest", "al\u00b7ler\u00b7seit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was Vngemach und Hertzeleid", "tokens": ["Was", "Vn\u00b7ge\u00b7mach", "und", "Hert\u00b7ze\u00b7leid"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Je auff uns m\u00f6chte dringen.", "tokens": ["Je", "auff", "uns", "m\u00f6ch\u00b7te", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sonst gehst du warlich mit uns umb", "tokens": ["Sonst", "gehst", "du", "war\u00b7lich", "mit", "uns", "umb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wunderseltzam und so krumm,", "tokens": ["So", "wun\u00b7der\u00b7selt\u00b7zam", "und", "so", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df Trost und Raht uns fliehen,", "tokens": ["Da\u00df", "Trost", "und", "Raht", "uns", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd wir bey der geh\u00e4ufften Last", "tokens": ["Vnd", "wir", "bey", "der", "ge\u00b7h\u00e4uff\u00b7ten", "Last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Auch deine Sorge f\u00fcr uns fast", "tokens": ["Auch", "dei\u00b7ne", "Sor\u00b7ge", "f\u00fcr", "uns", "fast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Zweiffel m\u00f6chten ziehen.", "tokens": ["In", "Zweif\u00b7fel", "m\u00f6ch\u00b7ten", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zeuchst du nun die Verheissung ein,", "tokens": ["Zeuchst", "du", "nun", "die", "Ver\u00b7heis\u00b7sung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergissest gn\u00e4dig mehr zu seyn?", "tokens": ["Ver\u00b7gis\u00b7sest", "gn\u00e4\u00b7dig", "mehr", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4ssest von dir lesen:", "tokens": ["Du", "l\u00e4s\u00b7sest", "von", "dir", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie du vor Alters deiner Schar,", "tokens": ["Wie", "du", "vor", "Al\u00b7ters", "dei\u00b7ner", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die dir vertrawet, in Gefahr", "tokens": ["Die", "dir", "ver\u00b7tra\u00b7wet", ",", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Barmhertzig bist gewesen.", "tokens": ["Barm\u00b7hert\u00b7zig", "bist", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie stellst du dich denn jetzund an?", "tokens": ["Wie", "stellst", "du", "dich", "denn", "je\u00b7tzund", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist uns wie ein frembder Mann", "tokens": ["Du", "bist", "uns", "wie", "ein", "fremb\u00b7der", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nur wil f\u00f6rter gehen,", "tokens": ["Der", "nur", "wil", "f\u00f6r\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bist wie ein Riese, der nicht Raht,", "tokens": ["Bist", "wie", "ein", "Rie\u00b7se", ",", "der", "nicht", "Raht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht Hertz noch Krafft zu helffen hat", "tokens": ["Nicht", "Hertz", "noch", "Krafft", "zu", "helf\u00b7fen", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "NN", "PTKZU", "VVINF", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Angst und Noht entstehen.", "tokens": ["Wenn", "Angst", "und", "Noht", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schaw wie sich unsre Tr\u00fcbsal n\u00e4hrt!", "tokens": ["Schaw", "wie", "sich", "uns\u00b7re", "Tr\u00fcb\u00b7sal", "n\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hertz ist uns fast auffgezehrt", "tokens": ["Das", "Hertz", "ist", "uns", "fast", "auff\u00b7ge\u00b7zehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Sorgen, die uns nagen,", "tokens": ["Von", "Sor\u00b7gen", ",", "die", "uns", "na\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du st\u00fcrmst so grawsam zu uns ein,", "tokens": ["Du", "st\u00fcrmst", "so", "graw\u00b7sam", "zu", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd schl\u00e4gst uns, wie man in gmein", "tokens": ["Vnd", "schl\u00e4gst", "uns", ",", "wie", "man", "in", "gmein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PIS", "APPR", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Pflegt seinen Feind zu schlagen.", "tokens": ["Pflegt", "sei\u00b7nen", "Feind", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erzeig uns deine G\u00fcte doch,", "tokens": ["Er\u00b7zeig", "uns", "dei\u00b7ne", "G\u00fc\u00b7te", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist ja unser Vater noch", "tokens": ["Du", "bist", "ja", "un\u00b7ser", "Va\u00b7ter", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So, wie vor alten Zeiten,", "tokens": ["So", ",", "wie", "vor", "al\u00b7ten", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Setz unsre Hoffnung nicht in Spott,", "tokens": ["Setz", "uns\u00b7re", "Hoff\u00b7nung", "nicht", "in", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd sey auch ferner uns ein GOTT", "tokens": ["Vnd", "sey", "auch", "fer\u00b7ner", "uns", "ein", "GoTT"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von nah' und nicht von weiten.", "tokens": ["Von", "nah'", "und", "nicht", "von", "wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "PTKNEG", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wend endlich unser Noht-Geschrey,", "tokens": ["Wend", "end\u00b7lich", "un\u00b7ser", "Noht\u00b7Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohn uns mit Trost und Rettung bey,", "tokens": ["Wohn", "uns", "mit", "Trost", "und", "Ret\u00b7tung", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN"