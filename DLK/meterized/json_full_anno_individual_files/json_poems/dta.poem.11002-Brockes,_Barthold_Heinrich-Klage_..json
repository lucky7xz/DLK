{"dta.poem.11002": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Klage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem sch\u00f6nen Blumen-Garten, worin auf einem Blumen-\nBeet,", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Blu\u00b7men\u00b7Gar\u00b7ten", ",", "wo\u00b7rin", "auf", "ei\u00b7nem", "Blu\u00b7men", "Beet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "APPR", "ART", "TRUNC", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "In tausendfach gef\u00e4rbtem Glanz, theils niedrig, und zum Theil", "tokens": ["In", "tau\u00b7send\u00b7fach", "ge\u00b7f\u00e4rb\u00b7tem", "Glanz", ",", "theils", "nied\u00b7rig", ",", "und", "zum", "Theil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein wundersch\u00f6nes Blumen-Heer, in seiner sch\u00f6nsten Zierde,", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "Blu\u00b7men\u00b7Heer", ",", "in", "sei\u00b7ner", "sch\u00f6ns\u00b7ten", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Ging ", "tokens": ["Ging"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Unachtsam hin und her spatzieren.", "tokens": ["U\u00b7nacht\u00b7sam", "hin", "und", "her", "spat\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "So wenig der gef\u00e4rbte Schmelz, als des Geruchs gew\u00fcrz-", "tokens": ["So", "we\u00b7nig", "der", "ge\u00b7f\u00e4rb\u00b7te", "Schmelz", ",", "als", "des", "Ge\u00b7ruchs", "ge\u00b7w\u00fcrz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Vermochten den gesch\u00e4fftgen Hund, noch minder Chrysidor zu", "tokens": ["Ver\u00b7moch\u00b7ten", "den", "ge\u00b7sch\u00e4fft\u00b7gen", "Hund", ",", "noch", "min\u00b7der", "Chry\u00b7si\u00b7dor", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "NE", "APPR"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "Sie wu\u00dften nichts von ihrer Zierde, nichts von der Balsams-", "tokens": ["Sie", "wu\u00df\u00b7ten", "nichts", "von", "ih\u00b7rer", "Zier\u00b7de", ",", "nichts", "von", "der", "Bal\u00b7sams"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$,", "PIS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Ich sa\u00df in einer Sommerlaube, sah heimlich beyder Hand-", "tokens": ["Ich", "sa\u00df", "in", "ei\u00b7ner", "Som\u00b7mer\u00b7lau\u00b7be", ",", "sah", "heim\u00b7lich", "bey\u00b7der", "Han\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Betr\u00fcbte mich, da\u00df aller Schmuck ein Menschen-Herz nicht", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "mich", ",", "da\u00df", "al\u00b7ler", "Schmuck", "ein", "Men\u00b7schen\u00b7Herz", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dachte mit fast bitterm Gram; wie kann doch dieses", "tokens": ["Und", "dach\u00b7te", "mit", "fast", "bit\u00b7term", "Gram", ";", "wie", "kann", "doch", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$.", "PWAV", "VMFIN", "ADV", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zuletzt gab die Betr\u00fcbni\u00df mir dar\u00fcber die Gedanken ein:", "tokens": ["Zu\u00b7letzt", "gab", "die", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "mir", "da\u00b7r\u00fc\u00b7ber", "die", "Ge\u00b7dan\u00b7ken", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.13": {"text": "Ein Mensch, der seine Pflichten kennt, mu\u00df billig \u00fcber das", "tokens": ["Ein", "Mensch", ",", "der", "sei\u00b7ne", "Pflich\u00b7ten", "kennt", ",", "mu\u00df", "bil\u00b7lig", "\u00fc\u00b7ber", "das"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Der Menschen, mit den blo\u00df f\u00fcr sie so sch\u00f6n erschaffnen Blu-", "tokens": ["Der", "Men\u00b7schen", ",", "mit", "den", "blo\u00df", "f\u00fcr", "sie", "so", "sch\u00f6n", "er\u00b7schaff\u00b7nen", "Blu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "APPR", "PPER", "ADV", "ADJD", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Wie viele Menschen gehn, wie Hunde, auch mit", "tokens": ["Wie", "vie\u00b7le", "Men\u00b7schen", "gehn", ",", "wie", "Hun\u00b7de", ",", "auch", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,", "PWAV", "NN", "$,", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "den sch\u00f6nsten Blumen um!", "tokens": ["den", "sch\u00f6ns\u00b7ten", "Blu\u00b7men", "um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Wie oder soll ich lieber sagen, mit einem Mitleid-vollem Grimm:", "tokens": ["Wie", "o\u00b7der", "soll", "ich", "lie\u00b7ber", "sa\u00b7gen", ",", "mit", "ei\u00b7nem", "Mit\u00b7lei\u00b7dvol\u00b7lem", "Grimm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "VMFIN", "PPER", "ADV", "VVINF", "$,", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.18": {"text": "Es geht ein Hund, es geht ein Thier,", "tokens": ["Es", "geht", "ein", "Hund", ",", "es", "geht", "ein", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit dem so sch\u00f6nen Schmuck der Erden, den Blu-", "tokens": ["Mit", "dem", "so", "sch\u00f6\u00b7nen", "Schmuck", "der", "Er\u00b7den", ",", "den", "Blu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "men, eben um, als wir?", "tokens": ["men", ",", "e\u00b7ben", "um", ",", "als", "wir", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}