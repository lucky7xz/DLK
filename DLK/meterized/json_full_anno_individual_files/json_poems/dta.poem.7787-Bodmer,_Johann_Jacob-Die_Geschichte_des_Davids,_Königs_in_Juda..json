{"dta.poem.7787": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mitde\u00df da\u00df dieses nun also bey uns geschicht,", "tokens": ["Mit\u00b7de\u00df", "da\u00df", "die\u00b7ses", "nun", "al\u00b7so", "bey", "uns", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Erf\u00e4hrt man in der Stadt von diesen Wunderdingen,", "tokens": ["Er\u00b7f\u00e4hrt", "man", "in", "der", "Stadt", "von", "die\u00b7sen", "Wun\u00b7der\u00b7din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Joel weis f\u00fcr Angst nicht was er soll f\u00fcrbringen,", "tokens": ["Der", "Joel", "weis", "f\u00fcr", "Angst", "nicht", "was", "er", "soll", "f\u00fcr\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "APPR", "NN", "PTKNEG", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Er eilt gen Bersaba ganz eilig eh es tagt,", "tokens": ["Er", "eilt", "gen", "Ber\u00b7sa\u00b7ba", "ganz", "ei\u00b7lig", "eh", "es", "tagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADV", "ADJD", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn der scheut Tag und Licht den sein Gewissen plagt.", "tokens": ["Denn", "der", "scheut", "Tag", "und", "Licht", "den", "sein", "Ge\u00b7wis\u00b7sen", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "KON", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie dieses kam f\u00fcr Kis, rieth der man sollte schweigen,", "tokens": ["Wie", "die\u00b7ses", "kam", "f\u00fcr", "Kis", ",", "rieth", "der", "man", "soll\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "APPR", "NE", "$,", "VVFIN", "ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und hierinn diese Ehr dem Samuel erzeigen.", "tokens": ["Und", "hie\u00b7rinn", "die\u00b7se", "Ehr", "dem", "Sa\u00b7muel", "er\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und ward es drum vertuscht, da\u00df diese lose That", "tokens": ["Und", "ward", "es", "drum", "ver\u00b7tuscht", ",", "da\u00df", "die\u00b7se", "lo\u00b7se", "That"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PAV", "VVPP", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kein Mensch nach dieser Zeit wie du erfahren hat.", "tokens": ["Kein", "Mensch", "nach", "die\u00b7ser", "Zeit", "wie", "du", "er\u00b7fah\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "KOKOM", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Abenar lie\u00df sein Weib in dem Gef\u00e4ngni\u00df richten,", "tokens": ["A\u00b7be\u00b7nar", "lie\u00df", "sein", "Weib", "in", "dem", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Und weil die Schuld am Tag, so lie\u00df sich leichtlich schlichten", "tokens": ["Und", "weil", "die", "Schuld", "am", "Tag", ",", "so", "lie\u00df", "sich", "leicht\u00b7lich", "schlich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Ha\u00df den ihre Freund mit uns darum gef\u00fchrt.", "tokens": ["Der", "Ha\u00df", "den", "ih\u00b7re", "Freund", "mit", "uns", "da\u00b7rum", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPOSAT", "NN", "APPR", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie dancken Gott, da\u00df es nicht weiter wird ber\u00fchrt.", "tokens": ["Sie", "dan\u00b7cken", "Gott", ",", "da\u00df", "es", "nicht", "wei\u00b7ter", "wird", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hierauf beschliessen sie mit aller St\u00e4mme Willen,", "tokens": ["Hier\u00b7auf", "be\u00b7schlies\u00b7sen", "sie", "mit", "al\u00b7ler", "St\u00e4m\u00b7me", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das Unrecht, so im Land ergehet, gar zu stillen,", "tokens": ["Das", "Un\u00b7recht", ",", "so", "im", "Land", "er\u00b7ge\u00b7het", ",", "gar", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df ihme Samuel soll w\u00e4hlen einen Mann,", "tokens": ["Da\u00df", "ih\u00b7me", "Sa\u00b7muel", "soll", "w\u00e4h\u00b7len", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Der da ein K\u00f6nig sey und besser richten kan,", "tokens": ["Der", "da", "ein", "K\u00f6\u00b7nig", "sey", "und", "bes\u00b7ser", "rich\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "KON", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann alle Heiden ja nachlebten solcher Weise.", "tokens": ["Dann", "al\u00b7le", "Hei\u00b7den", "ja", "nach\u00b7leb\u00b7ten", "sol\u00b7cher", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Schlu\u00df der wird gemacht, sie heben an die Reise,", "tokens": ["Der", "Schlu\u00df", "der", "wird", "ge\u00b7macht", ",", "sie", "he\u00b7ben", "an", "die", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nach Ramath kommen sie, und Samuel vernimmt", "tokens": ["Nach", "Ra\u00b7math", "kom\u00b7men", "sie", ",", "und", "Sa\u00b7muel", "ver\u00b7nimmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "KON", "NE", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mit Schmertzen die\u00df Gewerb, doch weil es so bestimmt,", "tokens": ["Mit", "Schmert\u00b7zen", "die\u00df", "Ge\u00b7werb", ",", "doch", "weil", "es", "so", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "NN", "$,", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und er sah, da\u00df es Gott wollt haben, sollt geschehen", "tokens": ["Und", "er", "sah", ",", "da\u00df", "es", "Gott", "wollt", "ha\u00b7ben", ",", "sollt", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "VMFIN", "VAINF", "$,", "VMFIN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.23": {"text": "Der St\u00e4mme ihr Begehrn; worauf sie von ihm gehen.", "tokens": ["Der", "St\u00e4m\u00b7me", "ihr", "Be\u00b7gehrn", ";", "wo\u00b7rauf", "sie", "von", "ihm", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "PWAV", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}