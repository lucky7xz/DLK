{"textgrid.poem.42348": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Zimmer hat nur sp\u00e4rliches Ger\u00e4t.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Zimmer hat nur sp\u00e4rliches Ger\u00e4t.", "tokens": ["Das", "Zim\u00b7mer", "hat", "nur", "sp\u00e4r\u00b7li\u00b7ches", "Ge\u00b7r\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Herde glimmert ein verkohltes Scheit.", "tokens": ["Im", "Her\u00b7de", "glim\u00b7mert", "ein", "ver\u00b7kohl\u00b7tes", "Scheit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichgiltige Lippen murmeln ein Gebet.", "tokens": ["Gleich\u00b7gil\u00b7ti\u00b7ge", "Lip\u00b7pen", "mur\u00b7meln", "ein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es stirbt ein Mann. Vom Turme schl\u00e4gt die Zeit.", "tokens": ["Es", "stirbt", "ein", "Mann", ".", "Vom", "Tur\u00b7me", "schl\u00e4gt", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er hat nicht Weib, nicht Kind. Kein Schluchzen t\u00f6nt.", "tokens": ["Er", "hat", "nicht", "Weib", ",", "nicht", "Kind", ".", "Kein", "Schluch\u00b7zen", "t\u00