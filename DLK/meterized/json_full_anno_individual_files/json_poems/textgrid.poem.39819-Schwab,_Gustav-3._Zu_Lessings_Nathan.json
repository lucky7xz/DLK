{"textgrid.poem.39819": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. Zu Lessings Nathan", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist die Werkstatt, drin die sichre Waffe,", "tokens": ["Wo", "ist", "die", "Werk\u00b7statt", ",", "drin", "die", "sich\u00b7re", "Waf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Geschaffen wird? Nicht wenig liegt daran,", "tokens": ["Ge\u00b7schaf\u00b7fen", "wird", "?", "Nicht", "we\u00b7nig", "liegt", "da\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "PTKNEG", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu Schutz und Trutz es t\u00fcchtig zu besitzen,", "tokens": ["Zu", "Schutz", "und", "Trutz", "es", "t\u00fcch\u00b7tig", "zu", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum Angriff scharf, doch ehrlich, giftig nicht,", "tokens": ["Zum", "An\u00b7griff", "scharf", ",", "doch", "ehr\u00b7lich", ",", "gif\u00b7tig", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum Schirme fest und sicher, doch nicht plump.", "tokens": ["Zum", "Schir\u00b7me", "fest", "und", "si\u00b7cher", ",", "doch", "nicht", "plump", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADJD", "$,", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es recht zu schmieden, ist die gro\u00dfe Kunst,", "tokens": ["Es", "recht", "zu", "schmie\u00b7den", ",", "ist", "die", "gro\u00b7\u00dfe", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ist unsrer Zeit fast einziges Bestreben,", "tokens": ["Ist", "uns\u00b7rer", "Zeit", "fast", "ein\u00b7zi\u00b7ges", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Denn nicht mehr auf des Degens Spitze nur,", "tokens": ["Denn", "nicht", "mehr", "auf", "des", "De\u00b7gens", "Spit\u00b7ze", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch auf der Lippen Schneide ruht die Welt.", "tokens": ["Auch", "auf", "der", "Lip\u00b7pen", "Schnei\u00b7de", "ruht", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Heros, der in k\u00fchnem Redestreit", "tokens": ["Der", "He\u00b7ros", ",", "der", "in", "k\u00fch\u00b7nem", "Re\u00b7des\u00b7treit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit neu erfundnen K\u00fcnsten angef\u00fchrt", "tokens": ["Mit", "neu", "er\u00b7fund\u00b7nen", "K\u00fcns\u00b7ten", "an\u00b7ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das k\u00e4mpfende Jahrhundert, dem das unsre,", "tokens": ["Das", "k\u00e4mp\u00b7fen\u00b7de", "Jahr\u00b7hun\u00b7dert", ",", "dem", "das", "uns\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein unruhvolles Kind, entsprossen ist,", "tokens": ["Ein", "un\u00b7ruh\u00b7vol\u00b7les", "Kind", ",", "ent\u00b7spros\u00b7sen", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der gro\u00dfe ", "tokens": ["Der", "gro\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Manch blankes Wurfgescho\u00df fliegt gl\u00e4nzend jetzt", "tokens": ["Manch", "blan\u00b7kes", "Wurf\u00b7ge\u00b7scho\u00df", "fliegt", "gl\u00e4n\u00b7zend", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Her\u00fcber und hin\u00fcber durch die Luft,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "und", "hin\u00b7\u00fc\u00b7ber", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Doch wie Apoll, der Fernhintreffende,", "tokens": ["Doch", "wie", "A\u00b7poll", ",", "der", "Fern\u00b7hin\u00b7tref\u00b7fen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "$,", "ART", "NN", "$,"], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.11": {"text": "Nicht stets den Bogen spannt und manchesmal", "tokens": ["Nicht", "stets", "den", "Bo\u00b7gen", "spannt", "und", "man\u00b7ches\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Muse Schlummer mit den Saiten weckt:", "tokens": ["Der", "Mu\u00b7se", "Schlum\u00b7mer", "mit", "den", "Sai\u00b7ten", "weckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So hat auch ", "tokens": ["So", "hat", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Nicht stets gebraucht, es ward auch ihm zum Liede,", "tokens": ["Nicht", "stets", "ge\u00b7braucht", ",", "es", "ward", "auch", "ihm", "zum", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$,", "PPER", "VAFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Es ward zum weltgestaltenden Gedicht.", "tokens": ["Es", "ward", "zum", "welt\u00b7ge\u00b7stal\u00b7ten\u00b7den", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Zwar hielt er selbst sich, der Bescheidene,", "tokens": ["Zwar", "hielt", "er", "selbst", "sich", ",", "der", "Be\u00b7schei\u00b7de\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PRF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.17": {"text": "F\u00fcr keinen Sch\u00f6pfer, keine Dichterkraft:", "tokens": ["F\u00fcr", "kei\u00b7nen", "Sch\u00f6p\u00b7fer", ",", "kei\u00b7ne", "Dich\u00b7ter\u00b7kraft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbnicht f\u00fchl' ich in mir die lebend'ge Quelle,\u00ab", "tokens": ["\u00bb", "nicht", "f\u00fchl'", "ich", "in", "mir", "die", "le\u00b7ben\u00b7d'\u00b7ge", "Quel\u00b7le", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKNEG", "VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$,", "$("], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Sprach er, \u00bbdie aufw\u00e4rts dringt mit eignem Trieb,", "tokens": ["Sprach", "er", ",", "\u00bb", "die", "auf\u00b7w\u00e4rts", "dringt", "mit", "eig\u00b7nem", "Trieb", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "$(", "ART", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und in so reichen, frischen, reinen Stralen", "tokens": ["Und", "in", "so", "rei\u00b7chen", ",", "fri\u00b7schen", ",", "rei\u00b7nen", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Aufschie\u00dft an's Licht; durch Druckwerk nur und Rohr", "tokens": ["Auf\u00b7schie\u00dft", "an's", "Licht", ";", "durch", "Druck\u00b7werk", "nur", "und", "Rohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$.", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Pre\u00dft sich's bei mir herauf.\u00ab So sprach der Mann,", "tokens": ["Pre\u00dft", "sich's", "bei", "mir", "her\u00b7auf", ".", "\u00ab", "So", "sprach", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und sandt' aus seinem Geist der Dichtung Quell,", "tokens": ["Und", "sandt'", "aus", "sei\u00b7nem", "Geist", "der", "Dich\u00b7tung", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der sprudelnd heut, nach sechzig Jahren, quillt.", "tokens": ["Der", "spru\u00b7delnd", "heut", ",", "nach", "sech\u00b7zig", "Jah\u00b7ren", ",", "quillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "$,", "APPR", "CARD", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Wir fragen nicht: Ist es ein Wasserfall,", "tokens": ["Wir", "fra\u00b7gen", "nicht", ":", "Ist", "es", "ein", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Der in die tiefe Heimat niedereilt,", "tokens": ["Der", "in", "die", "tie\u00b7fe", "Hei\u00b7mat", "nie\u00b7de\u00b7reilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ist es ein Springquell, den die Absicht schuf?", "tokens": ["Ist", "es", "ein", "Spring\u00b7quell", ",", "den", "die", "Ab\u00b7sicht", "schuf", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Uns ist's Natur. Und w\u00e4r es Gartenkunst:", "tokens": ["Uns", "ist's", "Na\u00b7tur", ".", "Und", "w\u00e4r", "es", "Gar\u00b7ten\u00b7kunst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "KON", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Zur sch\u00f6nen Wildni\u00df ward f\u00fcr uns die Kunst;", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Wild\u00b7ni\u00df", "ward", "f\u00fcr", "uns", "die", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ein heil'ger Hain ist seine Poesie,", "tokens": ["Ein", "heil'\u00b7ger", "Hain", "ist", "sei\u00b7ne", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Aus dem des deutschen Geistes Welle rauscht.", "tokens": ["Aus", "dem", "des", "deut\u00b7schen", "Geis\u00b7tes", "Wel\u00b7le", "rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch dieser ", "tokens": ["Auch", "die\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADV", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ist Leben, wie's die rechte Dichtung ist.", "tokens": ["Ist", "Le\u00b7ben", ",", "wie's", "die", "rech\u00b7te", "Dich\u00b7tung", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Gleichni\u00df von den Ringen funkelt noch", "tokens": ["Sein", "Gleich\u00b7ni\u00df", "von", "den", "Rin\u00b7gen", "fun\u00b7kelt", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Rubinenhell, erfreut, erbittert noch,", "tokens": ["Ru\u00b7bi\u00b7nen\u00b7hell", ",", "er\u00b7freut", ",", "er\u00b7bit\u00b7tert", "noch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum Sinnen und zum Zweifel weckt es noch.", "tokens": ["Zum", "Sin\u00b7nen", "und", "zum", "Zwei\u00b7fel", "weckt", "es", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch warum davon sprechen, wenn sein Wort,", "tokens": ["Doch", "wa\u00b7rum", "da\u00b7von", "spre\u00b7chen", ",", "wenn", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PAV", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein eigenes, nur harrt, von unsern Lippen,", "tokens": ["Sein", "ei\u00b7ge\u00b7nes", ",", "nur", "harrt", ",", "von", "un\u00b7sern", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein theueres Verm\u00e4chtni\u00df, auszugehn?", "tokens": ["Ein", "theu\u00b7e\u00b7res", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", ",", "aus\u00b7zu\u00b7gehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gew\u00e4hrt ihm Stille, diesem ernsten Wort;", "tokens": ["Ge\u00b7w\u00e4hrt", "ihm", "Stil\u00b7le", ",", "die\u00b7sem", "erns\u00b7ten", "Wort", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "