{"dta.poem.17700": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "322.  Die Seeligkeit ist in dem h\u00f6chsten  \n Gutt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Mensch kanseelig seyn/ als in dem h\u00f6chsten Gutt;", "tokens": ["Kein", "Mensch", "kan\u00b7see\u00b7lig", "seyn", "/", "als", "in", "dem", "h\u00f6chs\u00b7ten", "Gutt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VAINF", "$(", "KOKOM", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie da\u00df mans dann verl\u00e4st/ und\u2019s kleine suchen thut?", "tokens": ["Wie", "da\u00df", "mans", "dann", "ver\u00b7l\u00e4st", "/", "un\u00b7d's", "klei\u00b7ne", "su\u00b7chen", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ADV", "VVFIN", "$(", "KON", "ADJA", "VVINF", "VVFIN", "$."], "meter": "--+--++-+-+-+", "measure": "anapaest.di.plus"}}}}}