{"dta.poem.6044": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dies ist vom Paralell ein ziemlich gleiches Bild.", "tokens": ["Dies", "ist", "vom", "Pa\u00b7ra\u00b7lell", "ein", "ziem\u00b7lich", "glei\u00b7ches", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erden Oeffnungen verstatten", "tokens": ["Der", "Er\u00b7den", "Oeff\u00b7nun\u00b7gen", "ver\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den C\u00f6rpern, womit Lufft und Himmel angef\u00fcllt,", "tokens": ["Den", "C\u00f6r\u00b7pern", ",", "wo\u00b7mit", "Lufft", "und", "Him\u00b7mel", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}