{"textgrid.poem.50739": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verschlossen und dunkel ist um und um", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschlossen und dunkel ist um und um", "tokens": ["Ver\u00b7schlos\u00b7sen", "und", "dun\u00b7kel", "ist", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "APPR", "KON", "KOUI"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein winterlich Herze zu schauen,", "tokens": ["Mein", "win\u00b7ter\u00b7lich", "Her\u00b7ze", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch innen, da ist es erleuchtet und hell,", "tokens": ["Doch", "in\u00b7nen", ",", "da", "ist", "es", "er\u00b7leuch\u00b7tet", "und", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da dehnen sich gr\u00fcnende Auen.", "tokens": ["Da", "deh\u00b7nen", "sich", "gr\u00fc\u00b7nen\u00b7de", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Da stell ich den Fr\u00fchling im kleinen auf", "tokens": ["Da", "stell", "ich", "den", "Fr\u00fch\u00b7ling", "im", "klei\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Roseng\u00e4rten und Bronnen,", "tokens": ["Mit", "Ro\u00b7sen\u00b7g\u00e4r\u00b7ten", "und", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da spann ich ein liebliches Himmelblau aus", "tokens": ["Da", "spann", "ich", "ein", "lieb\u00b7li\u00b7ches", "Him\u00b7mel\u00b7blau", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Regenbogen und Sonnen!", "tokens": ["Mit", "Re\u00b7gen\u00b7bo\u00b7gen", "und", "Son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Da z\u00fcnd ich das Morgen- und Abendrot an", "tokens": ["Da", "z\u00fcnd", "ich", "das", "Mor\u00b7gen", "und", "A\u00b7ben\u00b7drot", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "TRUNC", "KON", "NN", "APPR"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und lasse die Nachtigall schlagen,", "tokens": ["Und", "las\u00b7se", "die", "Nach\u00b7ti\u00b7gall", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da lasse ich singende Jungfr\u00e4ulein gehn,", "tokens": ["Da", "las\u00b7se", "ich", "sin\u00b7gen\u00b7de", "Jung\u00b7fr\u00e4u\u00b7lein", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die meergr\u00fcne Kleider tragen!", "tokens": ["Die", "meer\u00b7gr\u00fc\u00b7ne", "Klei\u00b7der", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dann \u00e4ndr' ich die Szene und lasse mit Macht", "tokens": ["Dann", "\u00e4ndr'", "ich", "die", "Sze\u00b7ne", "und", "las\u00b7se", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den blitzenden Sommer ergl\u00fchen,", "tokens": ["Den", "blit\u00b7zen\u00b7den", "Som\u00b7mer", "er\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich lasse die Schnitter auf Garben ruhn", "tokens": ["Ich", "las\u00b7se", "die", "Schnit\u00b7ter", "auf", "Gar\u00b7ben", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und blutrote Mohnfelder bl\u00fchen.", "tokens": ["Und", "blut\u00b7ro\u00b7te", "Mohn\u00b7fel\u00b7der", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Und dann durchschneid ich mit Wetterschein", "tokens": ["Und", "dann", "durch\u00b7schneid", "ich", "mit", "Wet\u00b7ter\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Herz und f\u00fcll es mit St\u00fcrmen,", "tokens": ["Mein", "Herz", "und", "f\u00fcll", "es", "mit", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df Schiffe und Mannschaft zu Grunde gehn,", "tokens": ["La\u00df", "Schif\u00b7fe", "und", "Mann\u00b7schaft", "zu", "Grun\u00b7de", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dann \u00bbFeuer\u00ab von Bergen und T\u00fcrmen!", "tokens": ["Dann", "\u00bb", "Feu\u00b7er", "\u00ab", "von", "Ber\u00b7gen", "und", "T\u00fcr\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Hei, Revolutionen und Mordgeschrei,", "tokens": ["Hei", ",", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7o\u00b7nen", "und", "Mord\u00b7ge\u00b7schrei", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit Galgen und Guillotinen,", "tokens": ["Mit", "Gal\u00b7gen", "und", "Guil\u00b7lo\u00b7ti\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gek\u00f6pfte K\u00f6nige, wahnsinnig Volk,", "tokens": ["Ge\u00b7k\u00f6pf\u00b7te", "K\u00f6\u00b7ni\u00b7ge", ",", "wahn\u00b7sin\u00b7nig", "Volk", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Konvente und H\u00f6llenmaschinen!", "tokens": ["Kon\u00b7ven\u00b7te", "und", "H\u00f6l\u00b7len\u00b7ma\u00b7schi\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Nun ist mein Herze der Gr\u00e8veplatz,", "tokens": ["Nun", "ist", "mein", "Her\u00b7ze", "der", "Gr\u00e8\u00b7ve\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Voll P\u00f6bel und blutige Leichen;", "tokens": ["Voll", "P\u00f6\u00b7bel", "und", "blu\u00b7ti\u00b7ge", "Lei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich sehe mich selber im dicksten Tro\u00df", "tokens": ["Ich", "se\u00b7he", "mich", "sel\u00b7ber", "im", "dicks\u00b7ten", "Tro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Erschrocken und totenbla\u00df schleichen.", "tokens": ["Er\u00b7schro\u00b7cken", "und", "to\u00b7ten\u00b7bla\u00df", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Der Draht ist gebrochen an meiner Figur,", "tokens": ["Der", "Draht", "ist", "ge\u00b7bro\u00b7chen", "an", "mei\u00b7ner", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich kann mich nicht mehr entziehen;", "tokens": ["Ich", "kann", "mich", "nicht", "mehr", "ent\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es wird mir bang, und ich lasse das Bild", "tokens": ["Es", "wird", "mir", "bang", ",", "und", "ich", "las\u00b7se", "das", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit all seinen Schrecken entfliehen.", "tokens": ["Mit", "all", "sei\u00b7nen", "Schre\u00b7cken", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich sch\u00fcttle die Tannen wie Besenreis", "tokens": ["Ich", "sch\u00fctt\u00b7le", "die", "Tan\u00b7nen", "wie", "Be\u00b7sen\u00b7reis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und fege das Laub von den W\u00e4ldern,", "tokens": ["Und", "fe\u00b7ge", "das", "Laub", "von", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich l\u00f6sche am Himmel die Sterne aus", "tokens": ["Ich", "l\u00f6\u00b7sche", "am", "Him\u00b7mel", "die", "Ster\u00b7ne", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und senge das Gras auf den Feldern.", "tokens": ["Und", "sen\u00b7ge", "das", "Gras", "auf", "den", "Fel\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wenn alles erstorben und stille ist,", "tokens": ["Wenn", "al\u00b7les", "ers\u00b7tor\u00b7ben", "und", "stil\u00b7le", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "KON", "ADJA", "VAFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.2": {"text": "Dann trag ich mich selber zu Grabe", "tokens": ["Dann", "trag", "ich", "mich", "sel\u00b7ber", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und stecke ein schwarzes Kreuzlein darauf,", "tokens": ["Und", "ste\u00b7cke", "ein", "schwar\u00b7zes", "Kreuz\u00b7lein", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ich selbsten geschnitzelt habe.", "tokens": ["Das", "ich", "selbs\u00b7ten", "ge\u00b7schnit\u00b7zelt", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "So spiel ich den langen Winter hindurch;", "tokens": ["So", "spiel", "ich", "den", "lan\u00b7gen", "Win\u00b7ter", "hin\u00b7durch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch wenn die Maiblumen sprossen,", "tokens": ["Doch", "wenn", "die", "Maib\u00b7lu\u00b7men", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zerbrech ich das gl\u00e4serne Puppenspiel", "tokens": ["Zer\u00b7brech", "ich", "das", "gl\u00e4\u00b7ser\u00b7ne", "Pup\u00b7pen\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und mache den Dichter im gro\u00dfen!", "tokens": ["Und", "ma\u00b7che", "den", "Dich\u00b7ter", "im", "gro\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.12": {"line.1": {"text": "Verschlossen und dunkel ist um und um", "tokens": ["Ver\u00b7schlos\u00b7sen", "und", "dun\u00b7kel", "ist", "um", "und", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "APPR", "KON", "KOUI"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mein winterlich Herze zu schauen,", "tokens": ["Mein", "win\u00b7ter\u00b7lich", "Her\u00b7ze", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch innen, da ist es erleuchtet und hell,", "tokens": ["Doch", "in\u00b7nen", ",", "da", "ist", "es", "er\u00b7leuch\u00b7tet", "und", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da dehnen sich gr\u00fcnende Auen.", "tokens": ["Da", "deh\u00b7nen", "sich", "gr\u00fc\u00b7nen\u00b7de", "Au\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.13": {"line.1": {"text": "Da stell ich den Fr\u00fchling im kleinen auf", "tokens": ["Da", "stell", "ich", "den", "Fr\u00fch\u00b7ling", "im", "klei\u00b7nen", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "APPR"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit Roseng\u00e4rten und Bronnen,", "tokens": ["Mit", "Ro\u00b7sen\u00b7g\u00e4r\u00b7ten", "und", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da spann ich ein liebliches Himmelblau aus", "tokens": ["Da", "spann", "ich", "ein", "lieb\u00b7li\u00b7ches", "Him\u00b7mel\u00b7blau", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit Regenbogen und Sonnen!", "tokens": ["Mit", "Re\u00b7gen\u00b7bo\u00b7gen", "und", "Son\u00b7nen", "!"], "token