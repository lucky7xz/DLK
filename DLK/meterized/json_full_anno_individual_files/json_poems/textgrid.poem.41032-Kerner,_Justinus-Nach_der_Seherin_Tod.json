{"textgrid.poem.41032": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Seherin Tod", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb' wohl! was ich dir hab' zu danken,", "tokens": ["Leb'", "wohl", "!", "was", "ich", "dir", "hab'", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PWS", "PPER", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trag' ich im Herzen immerdar.", "tokens": ["Trag'", "ich", "im", "Her\u00b7zen", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schaut mein Innres ohne Wanken", "tokens": ["Es", "schaut", "mein", "Inn\u00b7res", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In geist'ge Tiefen, wunderklar.", "tokens": ["In", "geist'\u00b7ge", "Tie\u00b7fen", ",", "wun\u00b7der\u00b7klar", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo du auch weilst, im Licht, im Schatten,", "tokens": ["Wo", "du", "auch", "weilst", ",", "im", "Licht", ",", "im", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "me