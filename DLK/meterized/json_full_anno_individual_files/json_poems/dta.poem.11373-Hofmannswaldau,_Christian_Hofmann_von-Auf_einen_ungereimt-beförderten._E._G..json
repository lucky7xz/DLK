{"dta.poem.11373": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen ungereimt-bef\u00f6rderten.  \n E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje wunderlich spielt doch das gl\u00fccke,", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "spielt", "doch", "das", "gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ADV", "PDS", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mit denen, die es f\u00f6rdern soll!", "tokens": ["Mit", "de\u00b7nen", ",", "die", "es", "f\u00f6r\u00b7dern", "soll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Corydon geht an der kr\u00fccke;", "tokens": ["Der", "Co\u00b7ry\u00b7don", "geht", "an", "der", "kr\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch klinget alles wohl:", "tokens": ["Und", "den\u00b7noch", "klin\u00b7get", "al\u00b7les", "wohl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Witz, klugheit und verstand sind alle lahm geschossen;", "tokens": ["Witz", ",", "klug\u00b7heit", "und", "ver\u00b7stand", "sind", "al\u00b7le", "lahm", "ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "VVFIN", "VAFIN", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch helffen ihm die narren-possen.", "tokens": ["Doch", "helf\u00b7fen", "ihm", "die", "nar\u00b7ren\u00b7pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie wunderlich spielt doch das gl\u00fccke!", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "spielt", "doch", "das", "gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Jhr leutgen! seht ihn nur recht an!", "tokens": ["Ihr", "leut\u00b7gen", "!", "seht", "ihn", "nur", "recht", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo doch das herrliche geschicke,", "tokens": ["Wo", "doch", "das", "herr\u00b7li\u00b7che", "ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das seinen rang verdienen kan;", "tokens": ["Das", "sei\u00b7nen", "rang", "ver\u00b7die\u00b7nen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df, ihr k\u00f6nnet nichts, als diese w\u00f6rter, sagen:", "tokens": ["Ich", "wei\u00df", ",", "ihr", "k\u00f6n\u00b7net", "nichts", ",", "als", "die\u00b7se", "w\u00f6r\u00b7ter", ",", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "$,", "KOUS", "PDAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den hat das gl\u00fcck empor getragen.", "tokens": ["Den", "hat", "das", "gl\u00fcck", "em\u00b7por", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie wunderlich spielt das gel\u00fccke!", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "spielt", "das", "ge\u00b7l\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Allein, verzeiht mir! irrt euch nicht!", "tokens": ["Al\u00b7lein", ",", "ver\u00b7zeiht", "mir", "!", "irrt", "euch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Corydon hat eine ficke,", "tokens": ["Der", "Co\u00b7ry\u00b7don", "hat", "ei\u00b7ne", "fi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo niemahls saltz und schmaltz gebricht.", "tokens": ["Wo", "nie\u00b7mahls", "saltz", "und", "schmaltz", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer diese sachen kan in seinen beutel suchen,", "tokens": ["Wer", "die\u00b7se", "sa\u00b7chen", "kan", "in", "sei\u00b7nen", "beu\u00b7tel", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der kan des gl\u00fcckes macht verfluchen.", "tokens": ["Der", "kan", "des", "gl\u00fc\u00b7ckes", "macht", "ver\u00b7flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Allein, wie spielt doch da das gl\u00fccke,", "tokens": ["Al\u00b7lein", ",", "wie", "spielt", "doch", "da", "das", "gl\u00fc\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "VVFIN", "ADV", "KOUS", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Corydon die predigt sagt,", "tokens": ["Wenn", "Co\u00b7ry\u00b7don", "die", "pre\u00b7digt", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihr ein gantz halb dutzent st\u00fccke", "tokens": ["Und", "ihr", "ein", "gantz", "halb", "dut\u00b7zent", "st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADV", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihm vergn\u00fcgt nach hause tragt!", "tokens": ["Von", "ihm", "ver\u00b7gn\u00fcgt", "nach", "hau\u00b7se", "tragt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlagt nur den eingang nach in Mayers trost-postillen,", "tokens": ["Schlagt", "nur", "den", "ein\u00b7gang", "nach", "in", "Ma\u00b7yers", "trost\u00b7po\u00b7stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "APPR", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So k\u00f6nnt ihr diesen zweiffel stillen.", "tokens": ["So", "k\u00f6nnt", "ihr", "die\u00b7sen", "zweif\u00b7fel", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie wunderlich spielt doch das gl\u00fccke!", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "spielt", "doch", "das", "gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Dem tritt der pfarr den beicht-stuhl ab,", "tokens": ["Dem", "tritt", "der", "pfarr", "den", "beicht\u00b7stuhl", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und geht mit seiner Peters-picke", "tokens": ["Und", "geht", "mit", "sei\u00b7ner", "Pe\u00b7ter\u00b7spi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freywillig in das todten-grab.", "tokens": ["Frey\u00b7wil\u00b7lig", "in", "das", "tod\u00b7ten\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum? Der herr Johanns will seine tochter haben,", "tokens": ["Wa\u00b7rum", "?", "Der", "herr", "Jo\u00b7hanns", "will", "sei\u00b7ne", "toch\u00b7ter", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NN", "NE", "VMFIN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum last ihn immerhin begraben.", "tokens": ["Drum", "last", "ihn", "im\u00b7mer\u00b7hin", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie wunderlich spielt abers gl\u00fccke,", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "spielt", "a\u00b7bers", "gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wenn sein examen recht besteht,", "tokens": ["Wenn", "sein", "e\u00b7xa\u00b7men", "recht", "be\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obgleich ein elephant und m\u00fccke", "tokens": ["Ob\u00b7gleich", "ein", "e\u00b7le\u00b7phant", "und", "m\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dabey in einem paare geht!", "tokens": ["Da\u00b7bey", "in", "ei\u00b7nem", "paa\u00b7re", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ey! lernet nur, wie er, ein stetes ja-wort sprechen,", "tokens": ["Ey", "!", "ler\u00b7net", "nur", ",", "wie", "er", ",", "ein", "ste\u00b7tes", "ja\u00b7wort", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ADV", "$,", "PWAV", "PPER", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan euch kein patron gebrechen.", "tokens": ["So", "kan", "euch", "kein", "pat\u00b7ron", "ge\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So wunderlich spielt unser gl\u00fccke!", "tokens": ["So", "wun\u00b7der\u00b7lich", "spielt", "un\u00b7ser", "gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Drum nehmt es ja recht wohl in acht:", "tokens": ["Drum", "nehmt", "es", "ja", "recht", "wohl", "in", "acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Damit es euch auch einst erblicke,", "tokens": ["Da\u00b7mit", "es", "euch", "auch", "einst", "er\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn seine gunst Ehrw\u00fcrden macht.", "tokens": ["Wenn", "sei\u00b7ne", "gunst", "Ehr\u00b7w\u00fcr\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Laut geben, predigen, recht glauben und beweiben,", "tokens": ["Laut", "ge\u00b7ben", ",", "pre\u00b7di\u00b7gen", ",", "recht", "glau\u00b7ben", "und", "be\u00b7wei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "VVFIN", "$,", "ADJD", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "So wird euch kein patron vertreiben.", "tokens": ["So", "wird", "euch", "kein", "pat\u00b7ron", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}