{"dta.poem.23698": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Hrn. Professor A. G. Baumgarten  \n in Frankfurth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie der die Tugend liebet,", "tokens": ["Sieh", ",", "wie", "der", "die", "Tu\u00b7gend", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der, als du die Laster schaltest,", "tokens": ["Der", ",", "als", "du", "die", "Las\u00b7ter", "schal\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich schwur: ich will sie hassen.", "tokens": ["Pl\u00f6tz\u00b7lich", "schwur", ":", "ich", "will", "sie", "has\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Kr\u00e4fte deiner Lehren,", "tokens": ["Durch", "die", "Kr\u00e4f\u00b7te", "dei\u00b7ner", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwangst du ihn zur Tugendliebe.", "tokens": ["Zwangst", "du", "ihn", "zur", "Tu\u00b7gend\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O wie schaft man seinen Lehren", "tokens": ["O", "wie", "schaft", "man", "sei\u00b7nen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Solche Kr\u00e4fte, solchen Seegen?", "tokens": ["Sol\u00b7che", "Kr\u00e4f\u00b7te", ",", "sol\u00b7chen", "See\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lehrer, wenn du mich es lehrest,", "tokens": ["Leh\u00b7rer", ",", "wenn", "du", "mich", "es", "leh\u00b7rest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O so will ich M\u00e4dchen zwingen,", "tokens": ["O", "so", "will", "ich", "M\u00e4d\u00b7chen", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df sie pl\u00f6tzlich schweren m\u00fcssen,", "tokens": ["Da\u00df", "sie", "pl\u00f6tz\u00b7lich", "schwe\u00b7ren", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mich zu lieben, wenn ich liebe.", "tokens": ["Mich", "zu", "lie\u00b7ben", ",", "wenn", "ich", "lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}