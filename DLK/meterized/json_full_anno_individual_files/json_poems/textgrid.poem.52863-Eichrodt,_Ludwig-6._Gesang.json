{"textgrid.poem.52863": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6. Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Alte ruft aus dem Himmelreich:", "tokens": ["Und", "der", "Al\u00b7te", "ruft", "aus", "dem", "Him\u00b7mel\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\"ist's trocken drunten? Ich komme sogleich!", "tokens": ["\"", "ist's", "tro\u00b7cken", "drun\u00b7ten", "?", "Ich", "kom\u00b7me", "sog\u00b7leich", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich glaub' es ist gescheidter,", "tokens": ["Ich", "glaub'", "es", "ist", "ge\u00b7scheid\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du legst an den Bogen die Leiter.\"", "tokens": ["Du", "legst", "an", "den", "Bo\u00b7gen", "die", "Lei\u00b7ter", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "\"festhalten! So. Nun Gott sei Dank!", "tokens": ["\"", "fest\u00b7hal\u00b7ten", "!", "So", ".", "Nun", "Gott", "sei", "Dank", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "ADV", "$.", "ADV", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feuchtigkeit macht mir die Engel krank.", "tokens": ["Die", "Feuch\u00b7tig\u00b7keit", "macht", "mir", "die", "En\u00b7gel", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gottlob! jetzt ist's vor\u00fcber!", "tokens": ["Gott\u00b7lob", "!", "jetzt", "ist's", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schenke Dir meine Z\u00fcber!\"", "tokens": ["Ich", "schen\u00b7ke", "Dir", "mei\u00b7ne", "Z\u00fc\u00b7ber", "!", "\""], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon recht, sagt der Noah, das aber ist", "tokens": ["Schon", "recht", ",", "sagt", "der", "Noah", ",", "das", "a\u00b7ber", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "NE", "$,", "PRELS", "ADV", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht viel mit Erlaubni\u00df; ich d\u00e4cht' Ihr wi\u00dft,", "tokens": ["Nicht", "viel", "mit", "Er\u00b7laub\u00b7ni\u00df", ";", "ich", "d\u00e4cht'", "Ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da\u00df Ihr mir die Wirthschaft ruiniret,", "tokens": ["Da\u00df", "Ihr", "mir", "die", "Wirth\u00b7schaft", "ru\u00b7i\u00b7ni\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und da\u00df ich das Vieh Euch salviret.", "tokens": ["Und", "da\u00df", "ich", "das", "Vieh", "Euch", "sal\u00b7vi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\"so bitte Dir schnell eine Gnade aus!", "tokens": ["\"", "so", "bit\u00b7te", "Dir", "schnell", "ei\u00b7ne", "Gna\u00b7de", "aus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Keine Komplimente! Nur grad' heraus!", "tokens": ["Kei\u00b7ne", "Kom\u00b7pli\u00b7men\u00b7te", "!", "Nur", "grad'", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Willst was, das ich verboten?", "tokens": ["Willst", "was", ",", "das", "ich", "ver\u00b7bo\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oder sprich, willst Du jetzt ", "tokens": ["O\u00b7der", "sprich", ",", "willst", "Du", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das w\u00e4r's! versetzet der Schwanenwirth.", "tokens": ["Das", "w\u00e4r's", "!", "ver\u00b7set\u00b7zet", "der", "Schwa\u00b7nen\u00b7wirth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Schwanen wird wieder floriren;", "tokens": ["Der", "Schwa\u00b7nen", "wird", "wie\u00b7der", "flo\u00b7ri\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wollt Ihr mir Rothen spendiren.", "tokens": ["Wollt", "Ihr", "mir", "Ro\u00b7then", "spen\u00b7di\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "\"den sollst Du haben,\" der Herrgott sagt,", "tokens": ["\"", "den", "sollst", "Du", "ha\u00b7ben", ",", "\"", "der", "Herr\u00b7gott", "sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VMFIN", "PPER", "VAFIN", "$,", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch hab' ich noch gar nicht nachgefragt", "tokens": ["Doch", "hab'", "ich", "noch", "gar", "nicht", "nach\u00b7ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nach meinen armen Tr\u00f6pfen,", "tokens": ["Nach", "mei\u00b7nen", "ar\u00b7men", "Tr\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach meinen lieben Gesch\u00f6pfen!\"", "tokens": ["Nach", "mei\u00b7nen", "lie\u00b7ben", "Ge\u00b7sch\u00f6p\u00b7fen", "!", "\""], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "S'ist Alles in Ordnung, Herr Zebraoth!", "tokens": ["S'\u00b7ist", "Al\u00b7les", "in", "Ord\u00b7nung", ",", "Herr", "Zeb\u00b7raoth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "$,", "NN", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nur eines machte mir schwere Noth,", "tokens": ["Nur", "ei\u00b7nes", "mach\u00b7te", "mir", "schwe\u00b7re", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sch\u00f6nen Mammuthsknochen", "tokens": ["Die", "sch\u00f6\u00b7nen", "Mam\u00b7muths\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sind mir leider zerbrochen.", "tokens": ["Die", "sind", "mir", "lei\u00b7der", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\"thut nichts! Es war das ein plumpes Vieh,", "tokens": ["\"", "thut", "nichts", "!", "Es", "war", "das", "ein", "plum\u00b7pes", "Vieh", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schickte sich in die Welt auch nie:", "tokens": ["Und", "schick\u00b7te", "sich", "in", "die", "Welt", "auch", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu gro\u00df waren seine Kiefer ...", "tokens": ["Zu", "gro\u00df", "wa\u00b7ren", "sei\u00b7ne", "Kie\u00b7fer", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was macht das - Ungeziefer?", "tokens": ["Was", "macht", "das", "Un\u00b7ge\u00b7zie\u00b7fer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das Ungeziefer? Der Noah erschrickt,", "tokens": ["Das", "Un\u00b7ge\u00b7zie\u00b7fer", "?", "Der", "Noah", "er\u00b7schrickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NE", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine Floh-Art hatte er heimlich geknickt,", "tokens": ["Ei\u00b7ne", "Floh\u00b7Art", "hat\u00b7te", "er", "heim\u00b7lich", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auch einige Wanzen vertilget,", "tokens": ["Auch", "ei\u00b7ni\u00b7ge", "Wan\u00b7zen", "ver\u00b7til\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Darein Gott der Herr nicht gewill'get.", "tokens": ["Da\u00b7rein", "Gott", "der", "Herr", "nicht", "ge\u00b7will'", "get."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "NN", "ART", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Schon gut! Es ist vorderhand noch genug", "tokens": ["Schon", "gut", "!", "Es", "ist", "vor\u00b7der\u00b7hand", "noch", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vorhanden. Ich billige Deinen Unfug,", "tokens": ["Vor\u00b7han\u00b7den", ".", "Ich", "bil\u00b7li\u00b7ge", "Dei\u00b7nen", "Un\u00b7fug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mu\u00df doch noch mancherlei machen,", "tokens": ["Mu\u00df", "doch", "noch", "man\u00b7cher\u00b7lei", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kelleresel zum Beispiel und Drachen.\"", "tokens": ["Kel\u00b7le\u00b7re\u00b7sel", "zum", "Bei\u00b7spiel", "und", "Dra\u00b7chen", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Dank' sch\u00f6n! so murmelt der Schwanenwirth,", "tokens": ["Dank'", "sch\u00f6n", "!", "so", "mur\u00b7melt", "der", "Schwa\u00b7nen\u00b7wirth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich hab' mich nicht in ihm geirrt,", "tokens": ["Ich", "hab'", "mich", "nicht", "in", "ihm", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bleibt ein Projectenmacher", "tokens": ["Er", "bleibt", "ein", "Pro\u00b7jec\u00b7ten\u00b7ma\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "- Und ich ein uralter Kracher.", "tokens": ["Und", "ich", "ein", "ur\u00b7al\u00b7ter", "Kra\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Dabei guckt er auf und gibt mir ein' Tritt,", "tokens": ["Da\u00b7bei", "guckt", "er", "auf", "und", "gibt", "mir", "ein'", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und merkt, er hat vor sich den Zirkelschmidt,", "tokens": ["Und", "merkt", ",", "er", "hat", "vor", "sich", "den", "Zir\u00b7kelsc\u00b7hmidt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da ruft er siebenmal: oha! -", "tokens": ["Und", "da", "ruft", "er", "sie\u00b7ben\u00b7mal", ":", "o\u00b7ha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$.", "NE", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber jetzt hab' ich Durst wie der Noha.", "tokens": ["A\u00b7ber", "jetzt", "hab'", "ich", "Durst", "wie", "der", "No\u00b7ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.13": {"line.1": {"text": "Der Noah soll leben noch tausend Jahr", "tokens": ["Der", "Noah", "soll", "le\u00b7ben", "noch", "tau\u00b7send", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VMFIN", "VVINF", "ADV", "CARD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und gr\u00fcnden ein manches Mitmenschenpaar,", "tokens": ["Und", "gr\u00fcn\u00b7den", "ein", "man\u00b7ches", "Mit\u00b7men\u00b7schen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die alle Nachkommen haben -", "tokens": ["Die", "al\u00b7le", "Nach\u00b7kom\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unter Andern die Kn\u00f6pflesschwaben!", "tokens": ["Un\u00b7ter", "An\u00b7dern", "die", "Kn\u00f6pf\u00b7les\u00b7schwa\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und der Alte ruft aus dem Himmelreich:", "tokens": ["Und", "der", "Al\u00b7te", "ruft", "aus", "dem", "Him\u00b7mel\u00b7reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\"ist's trocken drunten? Ich komme sogleich!", "tokens": ["\"", "ist's", "tro\u00b7cken", "drun\u00b7ten", "?", "Ich", "kom\u00b7me", "sog\u00b7leich", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich glaub' es ist gescheidter,", "tokens": ["Ich", "glaub'", "es", "ist", "ge\u00b7scheid\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du legst an den Bogen die Leiter.\"", "tokens": ["Du", "legst", "an", "den", "Bo\u00b7gen", "die", "Lei\u00b7ter", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.15": {"line.1": {"text": "\"festhalten! So. Nun Gott sei Dank!", "tokens": ["\"", "fest\u00b7hal\u00b7ten", "!", "So", ".", "Nun", "Gott", "sei", "Dank", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$.", "ADV", "$.", "ADV", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Feuchtigkeit macht mir die Engel krank.", "tokens": ["Die", "Feuch\u00b7tig\u00b7keit", "macht", "mir", "die", "En\u00b7gel", "krank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gottlob! jetzt ist's vor\u00fcber!", "tokens": ["Gott\u00b7lob", "!", "jetzt", "ist's", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schenke Dir meine Z\u00fcber!\"", "tokens": ["Ich", "schen\u00b7ke", "Dir", "mei\u00b7ne", "Z\u00fc\u00b7ber", "!", "\""], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Schon recht, sagt der Noah, das aber ist", "tokens": ["Schon", "recht", ",", "sagt", "der", "Noah", ",", "das", "a\u00b7ber", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ART", "NE", "$,", "PRELS", "ADV", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht viel mit Erlaubni\u00df; ich d\u00e4cht' Ihr wi\u00dft,", "tokens": ["Nicht", "viel", "mit", "Er\u00b7laub\u00b7ni\u00df", ";", "ich", "d\u00e4cht'", "Ihr", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$.", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da\u00df Ihr mir die Wirthschaft ruiniret,", "tokens": ["Da\u00df", "Ihr", "mir", "die", "Wirth\u00b7schaft", "ru\u00b7i\u00b7ni\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und da\u00df ich das Vieh Euch salviret.", "tokens": ["Und", "da\u00df", "ich", "das", "Vieh", "Euch", "sal\u00b7vi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.17": {"line.1": {"text": "\"so bitte Dir schnell eine Gnade aus!", "tokens": ["\"", "so", "bit\u00b7te", "Dir", "schnell", "ei\u00b7ne", "Gna\u00b7de", "aus", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Keine Komplimente! Nur grad' heraus!", "tokens": ["Kei\u00b7ne", "Kom\u00b7pli\u00b7men\u00b7te", "!", "Nur", "grad'", "he\u00b7raus", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Willst was, das ich verboten?", "tokens": ["Willst", "was", ",", "das", "ich", "ver\u00b7bo\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oder sprich, willst Du jetzt ", "tokens": ["O\u00b7der", "sprich", ",", "willst", "Du", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Das w\u00e4r's! versetzet der Schwanenwirth.", "tokens": ["Das", "w\u00e4r's", "!", "ver\u00b7set\u00b7zet", "der", "Schwa\u00b7nen\u00b7wirth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Schwanen wird wieder floriren;", "tokens": ["Der", "Schwa\u00b7nen", "wird", "wie\u00b7der", "flo\u00b7ri\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wollt Ihr mir Rothen spendiren.", "tokens": ["Wollt", "Ihr", "mir", "Ro\u00b7then", "spen\u00b7di\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.19": {"line.1": {"text": "\"den sollst Du haben,\" der Herrgott sagt,", "tokens": ["\"", "den", "sollst", "Du", "ha\u00b7ben", ",", "\"", "der", "Herr\u00b7gott", "sagt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VMFIN", "PPER", "VAFIN", "$,", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch hab' ich noch gar nicht nachgefragt", "tokens": ["Doch", "hab'", "ich", "noch", "gar", "nicht", "nach\u00b7ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nach meinen armen Tr\u00f6pfen,", "tokens": ["Nach", "mei\u00b7nen", "ar\u00b7men", "Tr\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach meinen lieben Gesch\u00f6pfen!\"", "tokens": ["Nach", "mei\u00b7nen", "lie\u00b7ben", "Ge\u00b7sch\u00f6p\u00b7fen", "!", "\""], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.20": {"line.1": {"text": "S'ist Alles in Ordnung, Herr Zebraoth!", "tokens": ["S'\u00b7ist", "Al\u00b7les", "in", "Ord\u00b7nung", ",", "Herr", "Zeb\u00b7raoth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "$,", "NN", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nur eines machte mir schwere Noth,", "tokens": ["Nur", "ei\u00b7nes", "mach\u00b7te", "mir", "schwe\u00b7re", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sch\u00f6nen Mammuthsknochen", "tokens": ["Die", "sch\u00f6\u00b7nen", "Mam\u00b7muths\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sind mir leider zerbrochen.", "tokens": ["Die", "sind", "mir", "lei\u00b7der", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.21": {"line.1": {"text": "\"thut nichts! Es war das ein plumpes Vieh,", "tokens": ["\"", "thut", "nichts", "!", "Es", "war", "das", "ein", "plum\u00b7pes", "Vieh", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PIS", "$.", "PPER", "VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schickte sich in die Welt auch nie:", "tokens": ["Und", "schick\u00b7te", "sich", "in", "die", "Welt", "auch", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zu gro\u00df waren seine Kiefer ...", "tokens": ["Zu", "gro\u00df", "wa\u00b7ren", "sei\u00b7ne", "Kie\u00b7fer", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was macht das - Ungeziefer?", "tokens": ["Was", "macht", "das", "Un\u00b7ge\u00b7zie\u00b7fer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Das Ungeziefer? Der Noah erschrickt,", "tokens": ["Das", "Un\u00b7ge\u00b7zie\u00b7fer", "?", "Der", "Noah", "er\u00b7schrickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NE", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine Floh-Art hatte er heimlich geknickt,", "tokens": ["Ei\u00b7ne", "Floh\u00b7Art", "hat\u00b7te", "er", "heim\u00b7lich", "ge\u00b7knickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Auch einige Wanzen vertilget,", "tokens": ["Auch", "ei\u00b7ni\u00b7ge", "Wan\u00b7zen", "ver\u00b7til\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Darein Gott der Herr nicht gewill'get.", "tokens": ["Da\u00b7rein", "Gott", "der", "Herr", "nicht", "ge\u00b7will'", "get."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "NN", "ART", "NN", "PTKNEG", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.23": {"line.1": {"text": "Schon gut! Es ist vorderhand noch genug", "tokens": ["Schon", "gut", "!", "Es", "ist", "vor\u00b7der\u00b7hand", "noch", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vorhanden. Ich billige Deinen Unfug,", "tokens": ["Vor\u00b7han\u00b7den", ".", "Ich", "bil\u00b7li\u00b7ge", "Dei\u00b7nen", "Un\u00b7fug", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mu\u00df doch noch mancherlei machen,", "tokens": ["Mu\u00df", "doch", "noch", "man\u00b7cher\u00b7lei", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kelleresel zum Beispiel und Drachen.\"", "tokens": ["Kel\u00b7le\u00b7re\u00b7sel", "zum", "Bei\u00b7spiel", "und", "Dra\u00b7chen", ".", "\""], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Dank' sch\u00f6n! so murmelt der Schwanenwirth,", "tokens": ["Dank'", "sch\u00f6n", "!", "so", "mur\u00b7melt", "der", "Schwa\u00b7nen\u00b7wirth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich hab' mich nicht in ihm geirrt,", "tokens": ["Ich", "hab'", "mich", "nicht", "in", "ihm", "ge\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er bleibt ein Projectenmacher", "tokens": ["Er", "bleibt", "ein", "Pro\u00b7jec\u00b7ten\u00b7ma\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "- Und ich ein uralter Kracher.", "tokens": ["Und", "ich", "ein", "ur\u00b7al\u00b7ter", "Kra\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.25": {"line.1": {"text": "Dabei guckt er auf und gibt mir ein' Tritt,", "tokens": ["Da\u00b7bei", "guckt", "er", "auf", "und", "gibt", "mir", "ein'", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und merkt, er hat vor sich den Zirkelschmidt,", "tokens": ["Und", "merkt", ",", "er", "hat", "vor", "sich", "den", "Zir\u00b7kelsc\u00b7hmidt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da ruft er siebenmal: oha! -", "tokens": ["Und", "da", "ruft", "er", "sie\u00b7ben\u00b7mal", ":", "o\u00b7ha", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$.", "NE", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Aber jetzt hab' ich Durst wie der Noha.", "tokens": ["A\u00b7ber", "jetzt", "hab'", "ich", "Durst", "wie", "der", "No\u00b7ha", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.26": {"line.1": {"text": "Der Noah soll leben noch tausend Jahr", "tokens": ["Der", "Noah", "soll", "le\u00b7ben", "noch", "tau\u00b7send", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VMFIN", "VVINF", "ADV", "CARD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und gr\u00fcnden ein manches Mitmenschenpaar,", "tokens": ["Und", "gr\u00fcn\u00b7den", "ein", "man\u00b7ches", "Mit\u00b7men\u00b7schen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die alle Nachkommen haben -", "tokens": ["Die", "al\u00b7le", "Nach\u00b7kom\u00b7men", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unter Andern die Kn\u00f6pflesschwaben!", "tokens": ["Un\u00b7ter", "An\u00b7dern", "die", "Kn\u00f6pf\u00b7les\u00b7schwa\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}