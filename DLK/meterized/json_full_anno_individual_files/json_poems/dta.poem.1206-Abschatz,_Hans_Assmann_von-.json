{"dta.poem.1206": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Flutten/ die du siehst von meinen Augen rinnen/", "tokens": ["Die", "Flut\u00b7ten", "/", "die", "du", "siehst", "von", "mei\u00b7nen", "Au\u00b7gen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lieb-werthe Rosilis/ sind nicht gemeine Thr\u00e4nen/", "tokens": ["Lie\u00b7bwert\u00b7he", "Ro\u00b7si\u00b7lis", "/", "sind", "nicht", "ge\u00b7mei\u00b7ne", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "VAFIN", "PTKNEG", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wie deine G\u00f6ttligkeit wohl irgend m\u00f6chte wehnen!", "tokens": ["Wie", "dei\u00b7ne", "G\u00f6tt\u00b7lig\u00b7keit", "wohl", "ir\u00b7gend", "m\u00f6ch\u00b7te", "weh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo wolt ich solche Str\u00f6m und B\u00e4che fassen k\u00fcnnen?", "tokens": ["Wo", "wolt", "ich", "sol\u00b7che", "Str\u00f6m", "und", "B\u00e4\u00b7che", "fas\u00b7sen", "k\u00fcn\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie werden ausgebrennt vermittelst meiner Sinnen", "tokens": ["Sie", "wer\u00b7den", "aus\u00b7ge\u00b7brennt", "ver\u00b7mit\u00b7telst", "mei\u00b7ner", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Liljen deiner Schos/ von Rosen deiner Wangen/", "tokens": ["Von", "Lil\u00b7jen", "dei\u00b7ner", "Schos", "/", "von", "Ro\u00b7sen", "dei\u00b7ner", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$(", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und m\u00fcssen den Geruch von deiner Gunst erlangen/", "tokens": ["Und", "m\u00fcs\u00b7sen", "den", "Ge\u00b7ruch", "von", "dei\u00b7ner", "Gunst", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem keine Specerey den Prei\u00df wird abgewinnen.", "tokens": ["Dem", "kei\u00b7ne", "Spe\u00b7ce\u00b7rey", "den", "Prei\u00df", "wird", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Liebe giebt die Glutt/ der Ofen steht im Hertzen/", "tokens": ["Die", "Lie\u00b7be", "giebt", "die", "Glutt", "/", "der", "O\u00b7fen", "steht", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der dicken Seufftzer Wind bl\u00e4st mir das Feuer auff/", "tokens": ["Der", "di\u00b7cken", "Seufft\u00b7zer", "Wind", "bl\u00e4st", "mir", "das", "Feu\u00b7er", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Augen Helm verg\u00f6nnt dem Wasser freyen Lauff/", "tokens": ["Der", "Au\u00b7gen", "Helm", "ver\u00b7g\u00f6nnt", "dem", "Was\u00b7ser", "frey\u00b7en", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und weil so hitzig ist die Flamme meiner Schmertzen/", "tokens": ["Und", "weil", "so", "hit\u00b7zig", "ist", "die", "Flam\u00b7me", "mei\u00b7ner", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So m\u00fcssen in die H\u00f6h so viel der D\u00fcnste steigen/", "tokens": ["So", "m\u00fcs\u00b7sen", "in", "die", "H\u00f6h", "so", "viel", "der", "D\u00fcns\u00b7te", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und durch der Augen R\u00f6hr ohn Ende sich verseygen.", "tokens": ["Und", "durch", "der", "Au\u00b7gen", "R\u00f6hr", "ohn", "En\u00b7de", "sich", "ver\u00b7sey\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}