re": "trochaic.tetra"}, "line.23": {"text": "Aber sie wird, wenn sie euch vorbeygeht,", "tokens": ["A\u00b7ber", "sie", "wird", ",", "wenn", "sie", "euch", "vor\u00b7bey\u00b7geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.24": {"text": "S\u00fc\u00dfern Schauer empfinden, sie wird euch", "tokens": ["S\u00fc\u00b7\u00dfern", "Schau\u00b7er", "emp\u00b7fin\u00b7den", ",", "sie", "wird", "euch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "$,", "PPER", "VAFIN", "PPER"], "meter": "+-+--+-++-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Mit ihren Blicken segnen, ihr werdet", "tokens": ["Mit", "ih\u00b7ren", "Bli\u00b7cken", "seg\u00b7nen", ",", "ihr", "wer\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "PPER", "VAFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Gl\u00fccklicher seyn, als ich.", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "seyn", ",", "als", "ich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,", "KOUS", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}