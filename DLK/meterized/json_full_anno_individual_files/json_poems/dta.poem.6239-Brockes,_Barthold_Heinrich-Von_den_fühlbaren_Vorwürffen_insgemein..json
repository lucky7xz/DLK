{"dta.poem.6239": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor allen wird man sich dahin bestreben m\u00fcssen,", "tokens": ["Vor", "al\u00b7len", "wird", "man", "sich", "da\u00b7hin", "be\u00b7stre\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PIS", "PRF", "PAV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wil man die Harmonie der Sinnen unterscheiden,", "tokens": ["Wil", "man", "die", "Har\u00b7mo\u00b7nie", "der", "Sin\u00b7nen", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die einzelnen Bewegungen zu wissen,", "tokens": ["Die", "ein\u00b7zel\u00b7nen", "Be\u00b7we\u00b7gun\u00b7gen", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die wir in unsern C\u00f6rpern leiden,", "tokens": ["Die", "wir", "in", "un\u00b7sern", "C\u00f6r\u00b7pern", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ihre W\u00fcrckung sich in uns erzeigen kan.", "tokens": ["Wie", "ih\u00b7re", "W\u00fcr\u00b7ckung", "sich", "in", "uns", "er\u00b7zei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die C\u00f6rper allzumal, die uns ber\u00fchren,", "tokens": ["Die", "C\u00f6r\u00b7per", "all\u00b7zu\u00b7mal", ",", "die", "uns", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bestehn, aus C\u00f6rperchen, die keine Augen sp\u00fcren,", "tokens": ["Be\u00b7stehn", ",", "aus", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "kei\u00b7ne", "Au\u00b7gen", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NE", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die, auf verschiedne Art vermischt, zusammen kleben.", "tokens": ["Die", ",", "auf", "ver\u00b7schied\u00b7ne", "Art", "ver\u00b7mischt", ",", "zu\u00b7sam\u00b7men", "kle\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn aber unser Blick umsonst sie sucht zu finden;", "tokens": ["Wenn", "a\u00b7ber", "un\u00b7ser", "Blick", "um\u00b7sonst", "sie", "sucht", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So lasset uns dahin im Dencken doch bestreben,", "tokens": ["So", "las\u00b7set", "uns", "da\u00b7hin", "im", "Den\u00b7cken", "doch", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Um jede einzelne Figur", "tokens": ["Um", "je\u00b7de", "ein\u00b7zel\u00b7ne", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Der Theilchen, von dem Stoff in der Natur,", "tokens": ["Der", "Theil\u00b7chen", ",", "von", "dem", "Stoff", "in", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Durch ihre W\u00fcrckung zu ergr\u00fcnden:", "tokens": ["Durch", "ih\u00b7re", "W\u00fcr\u00b7ckung", "zu", "er\u00b7gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dadurch wir denn verm\u00f6gend sind zu fassen,", "tokens": ["Da\u00b7durch", "wir", "denn", "ver\u00b7m\u00f6\u00b7gend", "sind", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVPP", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Durch welchen Druck und Trieb wir uns bewegen lassen.", "tokens": ["Durch", "wel\u00b7chen", "Druck", "und", "Trieb", "wir", "uns", "be\u00b7we\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}