{"dta.poem.7950": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Petrarka .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du von Laura Wahres hast gesungen,", "tokens": ["Wenn", "du", "von", "Lau\u00b7ra", "Wah\u00b7res", "hast", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von hehrem Blick, von himmlischer Geb\u00e4rde: \u2014", "tokens": ["Von", "heh\u00b7rem", "Blick", ",", "von", "himm\u00b7li\u00b7scher", "Ge\u00b7b\u00e4r\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ferne sey, da\u00df angefochten werde,", "tokens": ["Und", "fer\u00b7ne", "sey", ",", "da\u00df", "an\u00b7ge\u00b7foch\u00b7ten", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "KOUS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was dir das innerste Gem\u00fcth durchdrungen! \u2014", "tokens": ["Was", "dir", "das", "in\u00b7ners\u00b7te", "Ge\u00b7m\u00fcth", "durch\u00b7drun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "War sie ein Zweig, im Paradies entsprungen,", "tokens": ["War", "sie", "ein", "Zweig", ",", "im", "Pa\u00b7ra\u00b7dies", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Engel in der irdischen Beschwerde,", "tokens": ["Ein", "En\u00b7gel", "in", "der", "ir\u00b7di\u00b7schen", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein zarter Fremdling auf der rauhen Erde,", "tokens": ["Ein", "zar\u00b7ter", "Fremd\u00b7ling", "auf", "der", "rau\u00b7hen", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der bald zur Heimath sich zur\u00fcckgeschwungen:", "tokens": ["Der", "bald", "zur", "Hei\u00b7math", "sich", "zu\u00b7r\u00fcck\u00b7ge\u00b7schwun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So f\u00fcrcht\u2019 ich, da\u00df auch auf dem goldnen Sterne,", "tokens": ["So", "f\u00fcrcht'", "ich", ",", "da\u00df", "auch", "auf", "dem", "gold\u00b7nen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin du, ein Verkl\u00e4rter, nun gekommen,", "tokens": ["Wo\u00b7hin", "du", ",", "ein", "Ver\u00b7kl\u00e4r\u00b7ter", ",", "nun", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du nimmer das Ersehnte wirst erringen;", "tokens": ["Du", "nim\u00b7mer", "das", "Er\u00b7sehn\u00b7te", "wirst", "er\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn Jene flog inde\u00df zur h\u00f6hern Ferne,", "tokens": ["Denn", "Je\u00b7ne", "flog", "in\u00b7de\u00df", "zur", "h\u00f6\u00b7hern", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie ward in heil\u2019gern Sph\u00e4ren aufgenommen,", "tokens": ["Sie", "ward", "in", "heil'\u00b7gern", "Sph\u00e4\u00b7ren", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder mu\u00dft du Liebesklage singen.", "tokens": ["Und", "wie\u00b7der", "mu\u00dft", "du", "Lie\u00b7bes\u00b7kla\u00b7ge", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}