{"dta.poem.20735": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf die Geduld .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein l\u00e4nger kann ichs nicht ertragen,                 ", "tokens": ["Nein", "l\u00e4n\u00b7ger", "kann", "ichs", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist zu viel, ist gar zu schwer,", "tokens": ["Das", "ist", "zu", "viel", ",", "ist", "gar", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "PIS", "$,", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das m\u00fc\u00dfte mich zu Boden schlagen,", "tokens": ["Das", "m\u00fc\u00df\u00b7te", "mich", "zu", "Bo\u00b7den", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich die St\u00e4rke selber w\u00e4r.", "tokens": ["Wenn", "ich", "die", "St\u00e4r\u00b7ke", "sel\u00b7ber", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich habe die Geduld verloren,", "tokens": ["Ich", "ha\u00b7be", "die", "Ge\u00b7duld", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gro\u00dfe Leidentr\u00e4gerin,", "tokens": ["Die", "gro\u00b7\u00dfe", "Lei\u00b7den\u00b7tr\u00e4\u00b7ge\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die bei mir war, als ich gebohren,", "tokens": ["Die", "bei", "mir", "war", ",", "als", "ich", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VAFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auferzogen worden bin;", "tokens": ["Und", "auf\u00b7er\u00b7zo\u00b7gen", "wor\u00b7den", "bin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die nimmer noch von mir gewichen", "tokens": ["Die", "nim\u00b7mer", "noch", "von", "mir", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In mancher j\u00e4mmerlichen Noth:", "tokens": ["In", "man\u00b7cher", "j\u00e4m\u00b7mer\u00b7li\u00b7chen", "Noth", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach die Geduld ist nun verblichen,", "tokens": ["Ach", "die", "Ge\u00b7duld", "ist", "nun", "ver\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der falsche Milon schlug sie todt.", "tokens": ["Der", "fal\u00b7sche", "Mi\u00b7lon", "schlug", "sie", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit einem Herzverachtungsstreiche", "tokens": ["Mit", "ei\u00b7nem", "Herz\u00b7ver\u00b7ach\u00b7tungs\u00b7strei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward sie getroffen, und mein Herz", "tokens": ["Ward", "sie", "ge\u00b7trof\u00b7fen", ",", "und", "mein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weint Thr\u00e4nen \u00fcber ihrer Leiche", "tokens": ["Weint", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber", "ih\u00b7rer", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erstarret unter seinem Schmerz.", "tokens": ["Er\u00b7star\u00b7ret", "un\u00b7ter", "sei\u00b7nem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der stolze, spr\u00f6de Milon sagte", "tokens": ["Der", "stol\u00b7ze", ",", "spr\u00f6\u00b7de", "Mi\u00b7lon", "sag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Veilchen zu, und t\u00e4uschte mich", "tokens": ["Mir", "Veil\u00b7chen", "zu", ",", "und", "t\u00e4uschte", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PTKVZ", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel Tage lang, so oft ich fragte,", "tokens": ["Viel", "Ta\u00b7ge", "lang", ",", "so", "oft", "ich", "frag\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Aug und Munde k\u00fcmmerlich.", "tokens": ["Mit", "Aug", "und", "Mun\u00b7de", "k\u00fcm\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zuletzt kam er in meine H\u00fctte,", "tokens": ["Zu\u00b7letzt", "kam", "er", "in", "mei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Trug Veilchen bei sich, schenkte sie,", "tokens": ["Trug", "Veil\u00b7chen", "bei", "sich", ",", "schenk\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PRF", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn Ihren Wink, ohn ihre Bitte,", "tokens": ["Ohn", "Ih\u00b7ren", "Wink", ",", "ohn", "ih\u00b7re", "Bit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der kleinen jungen Corally. \u2014", "tokens": ["Der", "klei\u00b7nen", "jun\u00b7gen", "Co\u00b7ral\u00b7ly", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O du Verr\u00e4ther meiner Treue,", "tokens": ["O", "du", "Ver\u00b7r\u00e4\u00b7ther", "mei\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00e4chter meiner Z\u00e4rtlichkeit,", "tokens": ["Ver\u00b7\u00e4ch\u00b7ter", "mei\u00b7ner", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich \u00fcbergebe dich der Reue,", "tokens": ["Ich", "\u00fc\u00b7ber\u00b7ge\u00b7be", "dich", "der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mich der Leidvergessenheit.", "tokens": ["Und", "mich", "der", "Leid\u00b7ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich werde dich noch immer denken,", "tokens": ["Ich", "wer\u00b7de", "dich", "noch", "im\u00b7mer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob du die Seele gleich verwarfst,", "tokens": ["Ob", "du", "die", "See\u00b7le", "gleich", "ver\u00b7warfst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von der du nie mit Goldgeschenken", "tokens": ["Von", "der", "du", "nie", "mit", "Gold\u00b7ge\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sanftes L\u00e4cheln kaufen darfst;", "tokens": ["Ein", "sanf\u00b7tes", "L\u00e4\u00b7cheln", "kau\u00b7fen", "darfst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Auch werd ich stets dich sehen wollen,", "tokens": ["Auch", "werd", "ich", "stets", "dich", "se\u00b7hen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob meine Lieder gleich hinfort", "tokens": ["Ob", "mei\u00b7ne", "Lie\u00b7der", "gleich", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von meiner Liebe schweigen sollen,", "tokens": ["Von", "mei\u00b7ner", "Lie\u00b7be", "schwei\u00b7gen", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihr h\u00f6rst du das letzte Wort.", "tokens": ["Von", "ihr", "h\u00f6rst", "du", "das", "letz\u00b7te", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}