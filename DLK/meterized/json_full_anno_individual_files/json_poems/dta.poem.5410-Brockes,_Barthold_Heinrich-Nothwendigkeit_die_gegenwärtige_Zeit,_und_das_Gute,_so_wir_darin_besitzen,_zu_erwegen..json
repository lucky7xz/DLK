{"dta.poem.5410": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendigkeit die gegenw\u00e4rtige Zeit,  \n und das Gute, so wir darin besitzen,  \n zu erwegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ungl\u00fcckseelig sind wir Menschen, und zwar da-", "tokens": ["Wie", "un\u00b7gl\u00fcck\u00b7see\u00b7lig", "sind", "wir", "Men\u00b7schen", ",", "und", "zwar", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "NN", "$,", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "durch fast blos allein,", "tokens": ["durch", "fast", "blos", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir f\u00fcr das besessne Gute, unbillig, unempfindlich seyn!", "tokens": ["Da\u00df", "wir", "f\u00fcr", "das", "be\u00b7sess\u00b7ne", "Gu\u00b7te", ",", "un\u00b7bil\u00b7lig", ",", "un\u00b7emp\u00b7find\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Die gr\u00f6sten Sch\u00e4tze, die wir haben, sind, wie wir ja", "tokens": ["Die", "gr\u00f6s\u00b7ten", "Sch\u00e4t\u00b7ze", ",", "die", "wir", "ha\u00b7ben", ",", "sind", ",", "wie", "wir", "ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "gestehen m\u00fcssen,", "tokens": ["ge\u00b7ste\u00b7hen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Gesundheit, G\u00fcter, gut Ger\u00fcchte, Bequemlichkeit und", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "G\u00fc\u00b7ter", ",", "gut", "Ge\u00b7r\u00fcch\u00b7te", ",", "Be\u00b7quem\u00b7lich\u00b7keit", "und"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "gut Gewissen;", "tokens": ["gut", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Doch wird uns leider ihr Genu\u00df blos durch Gewohnheit", "tokens": ["Doch", "wird", "uns", "lei\u00b7der", "ihr", "Ge\u00b7nu\u00df", "blos", "durch", "Ge\u00b7wohn\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "so entrissen,", "tokens": ["so", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Da\u00df, da wir nicht daran gedencken, uns der Besitz gantz", "tokens": ["Da\u00df", ",", "da", "wir", "nicht", "da\u00b7ran", "ge\u00b7den\u00b7cken", ",", "uns", "der", "Be\u00b7sitz", "gantz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PTKNEG", "PAV", "VVINF", "$,", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "unbewust", "tokens": ["un\u00b7be\u00b7wust"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Und wir sie leider gar nicht f\u00fchlen, als in derselbigen", "tokens": ["Und", "wir", "sie", "lei\u00b7der", "gar", "nicht", "f\u00fch\u00b7len", ",", "als", "in", "der\u00b7sel\u00b7bi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$,", "KOUS", "APPR", "PDS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Verlust.", "tokens": ["Ver\u00b7lust", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Die Ursach ist leicht zu ergr\u00fcnden: Wir sind vom Sch\u00f6-", "tokens": ["Die", "Ur\u00b7sach", "ist", "leicht", "zu", "er\u00b7gr\u00fcn\u00b7den", ":", "Wir", "sind", "vom", "Sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "pfer so gemacht,", "tokens": ["pfer", "so", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Da\u00df des Genusses bester Theil in anders nichts besteht, als", "tokens": ["Da\u00df", "des", "Ge\u00b7nus\u00b7ses", "bes\u00b7ter", "Theil", "in", "an\u00b7ders", "nichts", "be\u00b7steht", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "APPR", "ADV", "PIS", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Dencken;", "tokens": ["Den\u00b7cken", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Doch wir bem\u00fch\u2019n uns leider nicht, der Seelen Kraft", "tokens": ["Doch", "wir", "be\u00b7m\u00fch'n", "uns", "lei\u00b7der", "nicht", ",", "der", "See\u00b7len", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "darauf zu lencken,", "tokens": ["da\u00b7rauf", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Wodurch zugleich der Danck verschwindet. Da dieses", "tokens": ["Wo\u00b7durch", "zu\u00b7gleich", "der", "Danck", "ver\u00b7schwin\u00b7det", ".", "Da", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVPP", "$.", "KOUS", "PDAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "nun unstreitig wahr,", "tokens": ["nun", "un\u00b7strei\u00b7tig", "wahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "So wird zugleich der gantze Fehler durch solche Wahr-", "tokens": ["So", "wird", "zu\u00b7gleich", "der", "gant\u00b7ze", "Feh\u00b7ler", "durch", "sol\u00b7che", "Wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "heit offenbahr.", "tokens": ["heit", "of\u00b7fen\u00b7bahr", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}