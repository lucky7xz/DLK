{"dta.poem.5242": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Das Harz-Moos,  \n als Herr Dohmdechant Freyherr Spiegel  \n zum Diesenberg etwas Moos vom  \n Harzgeb\u00fcrge mitgebracht hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Gott zeigt in seiner Sch\u00f6pfung-Werke,", "tokens": ["Gott", "zeigt", "in", "sei\u00b7ner", "Sch\u00f6p\u00b7fung\u00b7Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber unserm Haupt, sich auf der Erde gro\u00df;", "tokens": ["Sich", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", ",", "sich", "auf", "der", "Er\u00b7de", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,", "PRF", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er gab der Sonne Glut, er gab dem L\u00f6wen St\u00e4rke,", "tokens": ["Er", "gab", "der", "Son\u00b7ne", "Glut", ",", "er", "gab", "dem", "L\u00f6\u00b7wen", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bildete das kleinste Moos,", "tokens": ["Und", "bil\u00b7de\u00b7te", "das", "kleins\u00b7te", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das an dem Harzberg w\u00e4chst, fein zweigigt wie Cypresse,", "tokens": ["Das", "an", "dem", "Harz\u00b7berg", "w\u00e4chst", ",", "fein", "zwei\u00b7gigt", "wie", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Voll kleiner Knospen, untersprengt", "tokens": ["Voll", "klei\u00b7ner", "Knos\u00b7pen", ",", "un\u00b7ter\u00b7sprengt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mit etwas R\u00f6the, so, wie junger M\u00e4dchen Bl\u00e4sse", "tokens": ["Mit", "et\u00b7was", "R\u00f6\u00b7the", ",", "so", ",", "wie", "jun\u00b7ger", "M\u00e4d\u00b7chen", "Bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "$,", "PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Im Antlitz sich mit roth vermengt,", "tokens": ["Im", "Ant\u00b7litz", "sich", "mit", "roth", "ver\u00b7mengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn sie der J\u00fcngling angeblicket;", "tokens": ["Wenn", "sie", "der", "J\u00fcng\u00b7ling", "an\u00b7ge\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Flur, der Garten und der Wald", "tokens": ["Die", "Flur", ",", "der", "Gar\u00b7ten", "und", "der", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und selbst die H\u00fcgel sind geschm\u00fccket.", "tokens": ["Und", "selbst", "die", "H\u00fc\u00b7gel", "sind", "ge\u00b7schm\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch andre Blumen sterben bald,", "tokens": ["Doch", "and\u00b7re", "Blu\u00b7men", "ster\u00b7ben", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das fein gebaute Moos bleibt, wenn sie schon gestorben,", "tokens": ["Das", "fein", "ge\u00b7bau\u00b7te", "Moos", "bleibt", ",", "wenn", "sie", "schon", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Tief unter Schnee noch unverdorben.", "tokens": ["Tief", "un\u00b7ter", "Schnee", "noch", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wie \u00e4hnlich ist es mir! tief lag ich unter Gram", "tokens": ["Wie", "\u00e4hn\u00b7lich", "ist", "es", "mir", "!", "tief", "lag", "ich", "un\u00b7ter", "Gram"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PPER", "$.", "ADJD", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Viel schwere Jahre lang, und als mein Winter kam,", "tokens": ["Viel", "schwe\u00b7re", "Jah\u00b7re", "lang", ",", "und", "als", "mein", "Win\u00b7ter", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "$,", "KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da stand ich unverwelkt und fieng erst an zu gr\u00fcnen.", "tokens": ["Da", "stand", "ich", "un\u00b7ver\u00b7welkt", "und", "fi\u00b7eng", "erst", "an", "zu", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ich muste, wie das Moos, dem Gl\u00fcck zum weichen", "tokens": ["Ich", "mus\u00b7te", ",", "wie", "das", "Moos", ",", "dem", "Gl\u00fcck", "zum", "wei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Tritt,", "tokens": ["Tritt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Dem Thoren zur Verachtung dienen.", "tokens": ["Dem", "Tho\u00b7ren", "zur", "Ver\u00b7ach\u00b7tung", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Einst sterb ich! Doch mein Lied geht nicht zum", "tokens": ["Einst", "sterb", "ich", "!", "Doch", "mein", "Lied", "geht", "nicht", "zum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Grabe mit!", "tokens": ["Gra\u00b7be", "mit", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}