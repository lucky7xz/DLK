{"textgrid.poem.32153": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "6. \u00dcberschrift der Verdammnis", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist ein ewge Nacht, man wei\u00df von keinem Lachen,", "tokens": ["Hier", "ist", "ein", "ew\u00b7ge", "Nacht", ",", "man", "wei\u00df", "von", "kei\u00b7nem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Jammer, Ach und Weh: ach, ewig sein verlorn!", "tokens": ["Ein", "Jam\u00b7mer", ",", "Ach", "und", "Weh", ":", "ach", ",", "e\u00b7wig", "sein", "ver\u00b7lorn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "KON", "NN", "$.", "XY", "$,", "ADJD", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird immerfort geschrien und: w\u00e4rn wir nie geborn!", "tokens": ["Wird", "im\u00b7mer\u00b7fort", "ge\u00b7schri\u00b7en", "und", ":", "w\u00e4rn", "wir", "nie", "ge\u00b7born", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "$.", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Beineben h\u00f6rt man nichts als Donnern, Hageln, Krachen,", "tokens": ["Bei\u00b7ne\u00b7ben", "h\u00f6rt", "man", "nichts", "als", "Don\u00b7nern", ",", "Ha\u00b7geln", ",", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PIS", "KOKOM", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Man sieht den Basilisk mit Kr\u00f6ten, Schlangen, Drachen", "tokens": ["Man", "sieht", "den", "Ba\u00b7si\u00b7lisk", "mit", "Kr\u00f6\u00b7ten", ",", "Schlan\u00b7gen", ",", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und tausend Ungeheur. Man ist vor K\u00e4lt erfrorn", "tokens": ["Und", "tau\u00b7send", "Un\u00b7ge\u00b7heur", ".", "Man", "ist", "vor", "K\u00e4lt", "er\u00b7frorn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "$.", "PIS", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und schmelzt vor gro\u00dfer Glut, man schilt sich Narrn und Torn", "tokens": ["Und", "schmelzt", "vor", "gro\u00b7\u00dfer", "Glut", ",", "man", "schilt", "sich", "Narrn", "und", "Torn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "PIS", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kommt doch nimmermehr aus diesem Teufelsrachen.", "tokens": ["Und", "kommt", "doch", "nim\u00b7mer\u00b7mehr", "aus", "die\u00b7sem", "Teu\u00b7fel\u00b7sra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man stirbt und stirbt doch nie, man liegt im ewgen Tod,", "tokens": ["Man", "stirbt", "und", "stirbt", "doch", "nie", ",", "man", "liegt", "im", "ew\u00b7gen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$,", "PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man w\u00fctet, tobt und z\u00fcrnt, man flucht und l\u00e4stert Gott.", "tokens": ["Man", "w\u00fc\u00b7tet", ",", "tobt", "und", "z\u00fcrnt", ",", "man", "flucht", "und", "l\u00e4s\u00b7tert", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PIS", "VVFIN", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man bei\u00dft und hadert sich, man lebt wie Hund und Katzen,", "tokens": ["Man", "bei\u00dft", "und", "ha\u00b7dert", "sich", ",", "man", "lebt", "wie", "Hund", "und", "Kat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man mu\u00df sich ewiglich mit allen Teufeln kratzen.", "tokens": ["Man", "mu\u00df", "sich", "e\u00b7wig\u00b7lich", "mit", "al\u00b7len", "Teu\u00b7feln", "krat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man frisset H\u00fcttenrauch, Pech, Schwefel, Teufelsmist:", "tokens": ["Man", "fris\u00b7set", "H\u00fct\u00b7ten\u00b7rauch", ",", "Pech", ",", "Schwe\u00b7fel", ",", "Teu\u00b7fels\u00b7mist", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach S\u00fcnder! tu doch Bu\u00df, eh du darinnen bist. ", "tokens": ["Ach", "S\u00fcn\u00b7der", "!", "tu", "doch", "Bu\u00df", ",", "eh", "du", "da\u00b7rin\u00b7nen", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "NE", "KON", "NN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier ist ein ewge Nacht, man wei\u00df von keinem Lachen,", "tokens": ["Hier", "ist", "ein", "ew\u00b7ge", "Nacht", ",", "man", "wei\u00df", "von", "kei\u00b7nem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PIS", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Jammer, Ach und Weh: ach, ewig sein verlorn!", "tokens": ["Ein", "Jam\u00b7mer", ",", "Ach", "und", "Weh", ":", "ach", ",", "e\u00b7wig", "sein", "ver\u00b7lorn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "KON", "NN", "$.", "XY", "$,", "ADJD", "VAINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird immerfort geschrien und: w\u00e4rn wir nie geborn!", "tokens": ["Wird", "im\u00b7mer\u00b7fort", "ge\u00b7schri\u00b7en", "und", ":", "w\u00e4rn", "wir", "nie", "ge\