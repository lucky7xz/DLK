{"dta.poem.8745": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV .  \n  Der Seelenwanderer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Dich beklag' ich, armes Kerzchen, da\u00df zum Nichts dein", "tokens": ["Dich", "be\u00b7klag'", "ich", ",", "ar\u00b7mes", "Kerz\u00b7chen", ",", "da\u00df", "zum", "Nichts", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "APPRART", "PIS", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Aber Antwort gab die Kerze, dieses h\u00f6rt' ich voll Ver-", "tokens": ["A\u00b7ber", "Ant\u00b7wort", "gab", "die", "Ker\u00b7ze", ",", "die\u00b7ses", "h\u00f6rt'", "ich", "voll", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Ueberhebe nicht dich also, denn auch ich war einst was", "tokens": ["Ue\u00b7ber\u00b7he\u00b7be", "nicht", "dich", "al\u00b7so", ",", "denn", "auch", "ich", "war", "einst", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPER", "ADV", "$,", "KON", "ADV", "PPER", "VAFIN", "ADV", "PWS"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Starb ich, modert' ich, doch wieder wuchs ich aus dem", "tokens": ["Starb", "ich", ",", "mo\u00b7dert'", "ich", ",", "doch", "wie\u00b7der", "wuchs", "ich", "aus", "dem"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "PPER", "$,", "ADV", "ADV", "VVFIN", "PPER", "APPR", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Kam ein Bienchen, naschte flei\u00dfig, nuzte mich im Korb", "tokens": ["Kam", "ein", "Bien\u00b7chen", ",", "naschte", "flei\u00b7\u00dfig", ",", "nuz\u00b7te", "mich", "im", "Korb"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ward ich Wachs, woraus man endlich diese Kerze nun", "tokens": ["Ward", "ich", "Wachs", ",", "wo\u00b7raus", "man", "end\u00b7lich", "die\u00b7se", "Ker\u00b7ze", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "$,", "PWAV", "PIS", "ADV", "PDAT", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Staub und Erde mu\u00dft du werden, ich verzehre mich im", "tokens": ["Staub", "und", "Er\u00b7de", "mu\u00dft", "du", "wer\u00b7den", ",", "ich", "ver\u00b7zeh\u00b7re", "mich", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VAINF", "$,", "PPER", "VVFIN", "PPER", "APPRART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}