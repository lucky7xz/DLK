{"textgrid.poem.28324": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als C\u00e6nis hie\u00df C\u00e6nis, da war sie ein Weib,", "tokens": ["Als", "C\u00e6\u00b7nis", "hie\u00df", "C\u00e6\u00b7nis", ",", "da", "war", "sie", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "NE", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da lidte, da th\u00e4te, was weibisch, ihr Leib;", "tokens": ["Da", "lid\u00b7te", ",", "da", "th\u00e4\u00b7te", ",", "was", "wei\u00b7bisch", ",", "ihr", "Leib", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da C\u00e6nis hie\u00df C\u00e6neus, da war sie ein Mann,", "tokens": ["Da", "C\u00e6\u00b7nis", "hie\u00df", "C\u00e6\u00b7neus", ",", "da", "war", "sie", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "NE", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dem Schwerter nichts hatten, dem Spiesse nichts an;", "tokens": ["Dem", "Schwer\u00b7ter", "nichts", "hat\u00b7ten", ",", "dem", "Spies\u00b7se", "nichts", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "$,", "ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Der gleich eine M\u00e4mm, eh er feste wird, heist,", "tokens": ["Der", "gleich", "ei\u00b7ne", "M\u00e4mm", ",", "eh", "er", "fes\u00b7te", "wird", ",", "heist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADJA", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der wird, wann er feste wird, Ritter gepreist.", "tokens": ["Der", "wird", ",", "wann", "er", "fes\u00b7te", "wird", ",", "Rit\u00b7ter", "ge\u00b7preist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "ADJA", "VAFIN", "$,", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Als C\u00e6nis hie\u00df C\u00e6nis, da war sie ein Weib,", "tokens": ["Als", "C\u00e6\u00b7nis", "hie\u00df", "C\u00e6\u00b7nis", ",", "da", "war", "sie", "ein", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "NE", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da lidte, da th\u00e4te, was weibisch, ihr Leib;", "tokens": ["Da", "lid\u00b7te", ",", "da", "th\u00e4\u00b7te", ",", "was", "wei\u00b7bisch", ",", "ihr", "Leib", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PRELS", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da C\u00e6nis hie\u00df C\u00e6neus, da war sie ein Mann,", "tokens": ["Da", "C\u00e6\u00b7nis", "hie\u00df", "