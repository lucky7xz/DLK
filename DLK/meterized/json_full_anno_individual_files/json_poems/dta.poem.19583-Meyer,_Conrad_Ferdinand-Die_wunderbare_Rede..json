{"dta.poem.19583": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die wunderbare Rede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Appierstra\u00dfe zieht ein Heer", "tokens": ["Auf", "der", "Ap\u00b7pier\u00b7stra\u00b7\u00dfe", "zieht", "ein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schnellen Schrittes, weit umw\u00f6lkt von Staub.", "tokens": ["Schnel\u00b7len", "Schrit\u00b7tes", ",", "weit", "um\u00b7w\u00f6lkt", "von", "Staub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wei\u00df am Horizont das H\u00e4usermeer \u2014", "tokens": ["Wei\u00df", "am", "Ho\u00b7ri\u00b7zont", "das", "H\u00e4u\u00b7ser\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "\u201erom ist morgen euer!\u201c zeigt Sever.", "tokens": ["\u201e", "rom", "ist", "mor\u00b7gen", "eu\u00b7er", "!", "\u201c", "zeigt", "Se\u00b7ver", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ADV", "PPOSAT", "$.", "$(", "VVFIN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "\u201eflieget, Adler! Sto\u00dft auf euren Raub!\u201c", "tokens": ["\u201e", "flie\u00b7get", ",", "Ad\u00b7ler", "!", "Sto\u00dft", "auf", "eu\u00b7ren", "Raub", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Morgen? Rom sorgt sich um morgen nicht.", "tokens": ["Mor\u00b7gen", "?", "Rom", "sorgt", "sich", "um", "mor\u00b7gen", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "VVFIN", "PRF", "APPR", "ADV", "PTKNEG", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u201edie Gladiatoren spielen heut!\u201c", "tokens": ["\u201e", "die", "Gla\u00b7di\u00b7a\u00b7to\u00b7ren", "spie\u00b7len", "heut", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weiber schm\u00fccken sich. Orestes ficht!", "tokens": ["Wei\u00b7ber", "schm\u00fc\u00b7cken", "sich", ".", "O\u00b7res\u00b7tes", "ficht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$.", "NE", "PTKVZ", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Manch unheimlich brennend Augenlicht", "tokens": ["Manch", "un\u00b7heim\u00b7lich", "bren\u00b7nend", "Au\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "VVPP", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Blitzt im Spiegel den die Sklavin beut.", "tokens": ["Blitzt", "im", "Spie\u00b7gel", "den", "die", "Skla\u00b7vin", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "S\u00e4nften hasten zum Theater schon,", "tokens": ["S\u00e4nf\u00b7ten", "has\u00b7ten", "zum", "The\u00b7a\u00b7ter", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von Gewitterwolken \u00fcberjagt,", "tokens": ["Von", "Ge\u00b7wit\u00b7ter\u00b7wol\u00b7ken", "\u00fc\u00b7ber\u00b7jagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schw\u00fcle Blicke, die wie Fackeln lohn,", "tokens": ["Schw\u00fc\u00b7le", "Bli\u00b7cke", ",", "die", "wie", "Fa\u00b7ckeln", "lohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "KOKOM", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Finst'rer Brauen ungeduldig Drohn \u2014", "tokens": ["Finst'\u00b7rer", "Brau\u00b7en", "un\u00b7ge\u00b7dul\u00b7dig", "Drohn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ueber Dach und Zinne ragt empor", "tokens": ["Ue\u00b7ber", "Dach", "und", "Zin\u00b7ne", "ragt", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Himmelhoch ein riesenstarker Bau,", "tokens": ["Him\u00b7mel\u00b7hoch", "ein", "rie\u00b7sen\u00b7star\u00b7ker", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der ein Volk empf\u00e4ngt durch manches Thor.", "tokens": ["Der", "ein", "Volk", "emp\u00b7f\u00e4ngt", "durch", "man\u00b7ches", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hinter seinem Mauerkranz hervor", "tokens": ["Hin\u00b7ter", "sei\u00b7nem", "Mau\u00b7er\u00b7kranz", "her\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Steigt es schwarz und schw\u00e4rzer auf im Blau.", "tokens": ["Steigt", "es", "schwarz", "und", "schw\u00e4r\u00b7zer", "auf", "im", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Drinnen dr\u00e4ngen sie sich Sitz an Sitz,", "tokens": ["Drin\u00b7nen", "dr\u00e4n\u00b7gen", "sie", "sich", "Sitz", "an", "Sitz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Jede Stufe strotzt und wogt und schwillt.", "tokens": ["Je\u00b7de", "Stu\u00b7fe", "strotzt", "und", "wogt", "und", "schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der B\u00fchne z\u00fcngeln hell und spitz", "tokens": ["Auf", "der", "B\u00fch\u00b7ne", "z\u00fcn\u00b7geln", "hell", "und", "spitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "KON", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kurze Schwerter. Schimmernd flirrt ein Blitz", "tokens": ["Kur\u00b7ze", "Schwer\u00b7ter", ".", "Schim\u00b7mernd", "flirrt", "ein", "Blitz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und ein erster Sprudel Blutes quillt.", "tokens": ["Und", "ein", "ers\u00b7ter", "Spru\u00b7del", "Blu\u00b7tes", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Starren Blickes, bla\u00df vor Leidenschaft,", "tokens": ["Star\u00b7ren", "Bli\u00b7ckes", ",", "bla\u00df", "vor", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lauert vorgeneigt die R\u00f6merin", "tokens": ["Lau\u00b7ert", "vor\u00b7ge\u00b7neigt", "die", "R\u00f6\u00b7me\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf die Sterbewunde \u2014 Eine gafft", "tokens": ["Auf", "die", "Ster\u00b7be\u00b7wun\u00b7de", "Ei\u00b7ne", "gafft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcstern. Eine sinnt d\u00e4monenhaft.", "tokens": ["L\u00fcs\u00b7tern", ".", "Ei\u00b7ne", "sinnt", "d\u00e4\u00b7mo\u00b7nen\u00b7haft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Eine lauscht mit hartem M\u00f6rdersinn.", "tokens": ["Ei\u00b7ne", "lauscht", "mit", "har\u00b7tem", "M\u00f6r\u00b7der\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "An der rasch gedrehten Klingen Spiel", "tokens": ["An", "der", "rasch", "ge\u00b7dreh\u00b7ten", "Klin\u00b7gen", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Haften Seelen gierig, ohne Zahl \u2014", "tokens": ["Haf\u00b7ten", "See\u00b7len", "gie\u00b7rig", ",", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$,", "KOUI", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Traf der Sto\u00df? Er sa\u00df. Ein Fechter fiel,", "tokens": ["Traf", "der", "Sto\u00df", "?", "Er", "sa\u00df", ".", "Ein", "Fech\u00b7ter", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4lzt sich um im Sand und ist am Ziel", "tokens": ["W\u00e4lzt", "sich", "um", "im", "Sand", "und", "ist", "am", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "NN", "KON", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Nach der kurz empfundnen Sterbequal.", "tokens": ["Nach", "der", "kurz", "emp\u00b7fund\u00b7nen", "Ster\u00b7be\u00b7qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Mark und Herz ersch\u00fctternd gellt ein Schrei!", "tokens": ["Mark", "und", "Herz", "er\u00b7sch\u00fct\u00b7ternd", "gellt", "ein", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dort auf dem Balkon ein Weib im Traum!", "tokens": ["Dort", "auf", "dem", "Bal\u00b7kon", "ein", "Weib", "im", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um die Schultern wehn die Haare frei", "tokens": ["Um", "die", "Schul\u00b7tern", "wehn", "die", "Haa\u00b7re", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und als ob sie die Sibylle sei,", "tokens": ["Und", "als", "ob", "sie", "die", "Si\u00b7byl\u00b7le", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ruft sie ehern durch den vollen Raum:", "tokens": ["Ruft", "sie", "e\u00b7hern", "durch", "den", "vol\u00b7len", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "\u201ewehe morgen! Fechter, du bist todt!", "tokens": ["\u201e", "we\u00b7he", "mor\u00b7gen", "!", "Fech\u00b7ter", ",", "du", "bist", "todt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "$.", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gute Fahrt! Dir thun sie nichts zu leid!", "tokens": ["Gu\u00b7te", "Fahrt", "!", "Dir", "thun", "sie", "nichts", "zu", "leid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PIS", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Morgen wehe! Horch! Die Tuba droht!", "tokens": ["Mor\u00b7gen", "we\u00b7he", "!", "Horch", "!", "Die", "Tu\u00b7ba", "droht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$.", "NE", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eine weite Flamme weht und loht!", "tokens": ["Ei\u00b7ne", "wei\u00b7te", "Flam\u00b7me", "weht", "und", "loht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wehe! Sie zerrei\u00dfen mir das Kleid!\u201c", "tokens": ["We\u00b7he", "!", "Sie", "zer\u00b7rei\u00b7\u00dfen", "mir", "das", "Kleid", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "In das Morgen blickt sie voller Graun,", "tokens": ["In", "das", "Mor\u00b7gen", "blickt", "sie", "vol\u00b7ler", "Graun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schaudernd wie vor Blutes tiefem Strom,", "tokens": ["Schau\u00b7dernd", "wie", "vor", "Blu\u00b7tes", "tie\u00b7fem", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn ihr Auge kann das K\u00fcnft'ge schaun \u2014", "tokens": ["Denn", "ihr", "Au\u00b7ge", "kann", "das", "K\u00fcnft'\u00b7ge", "schaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Es ist keine von den ird'schen Fraun!", "tokens": ["Es", "ist", "kei\u00b7ne", "von", "den", "ird'\u00b7schen", "Fraun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Es ist Rom! Es ist die G\u00f6ttin Rom!", "tokens": ["Es", "ist", "Rom", "!", "Es", "ist", "die", "G\u00f6t\u00b7tin", "Rom", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Vor dem Volk auf hoher Stufe ragt", "tokens": ["Vor", "dem", "Volk", "auf", "ho\u00b7her", "Stu\u00b7fe", "ragt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Rom die Herrin in versteintem Schmerz,", "tokens": ["Rom", "die", "Her\u00b7rin", "in", "ver\u00b7stein\u00b7tem