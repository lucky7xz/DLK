{"textgrid.poem.24993": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Fu\u00df der Alpen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neu klimmt der Fr\u00fchling auf die H\u00f6hn,", "tokens": ["Neu", "klimmt", "der", "Fr\u00fch\u00b7ling", "auf", "die", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Gletscher auf den Firnen krachen,", "tokens": ["Die", "Glet\u00b7scher", "auf", "den", "Fir\u00b7nen", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Lawine l\u00e4\u00dft der F\u00f6hn", "tokens": ["Und", "die", "La\u00b7wi\u00b7ne", "l\u00e4\u00dft", "der", "F\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ihrer Sommerlust erwachen;", "tokens": ["Zu", "ih\u00b7rer", "Som\u00b7mer\u00b7lust", "er\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Donner ihres Sturzes hallt", "tokens": ["Der", "Don\u00b7ner", "ih\u00b7res", "Stur\u00b7zes", "hallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch Thal und Schluchten hin von Spalt zu Spalt.", "tokens": ["Durch", "Thal", "und