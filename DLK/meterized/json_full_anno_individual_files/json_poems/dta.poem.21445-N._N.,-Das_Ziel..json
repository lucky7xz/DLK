{"dta.poem.21445": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Das Ziel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Schon als ich noch ein Knabe war, zog es mich hin zu ander\u2019m Stern,", "tokens": ["Schon", "als", "ich", "noch", "ein", "Kna\u00b7be", "war", ",", "zog", "es", "mich", "hin", "zu", "an\u00b7der'm", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Tiefhei\u00dfes Sehnen fa\u00dfte mich, doch blieb mir die Erf\u00fcllung fern.", "tokens": ["Tief\u00b7hei\u00b7\u00dfes", "Seh\u00b7nen", "fa\u00df\u00b7te", "mich", ",", "doch", "blieb", "mir", "die", "Er\u00b7f\u00fcl\u00b7lung", "fern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Ich fieberte all\u2019 meine Tag\u2019. Oft st\u00fcrmt\u2019 ich in das Feld hinaus \u2026", "tokens": ["Ich", "fie\u00b7ber\u00b7te", "all'", "mei\u00b7ne", "Tag'", ".", "Oft", "st\u00fcrmt'", "ich", "in", "das", "Feld", "hin\u00b7aus", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APZR", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Der br\u00fcnstige Leib verk\u00fchlte sich in Regenschaum und Sturmgebraus.", "tokens": ["Der", "br\u00fcns\u00b7ti\u00b7ge", "Leib", "ver\u00b7k\u00fchl\u00b7te", "sich", "in", "Re\u00b7gen\u00b7schaum", "und", "Sturm\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Der Seele Schrei: ich h\u00f6rte ihn in tausendstimmigen Melodien,", "tokens": ["Der", "See\u00b7le", "Schrei", ":", "ich", "h\u00f6r\u00b7te", "ihn", "in", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7gen", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Ich sah auf dunklen Fittichen die todten Leidgenossen zieh\u2019n.", "tokens": ["Ich", "sah", "auf", "dunk\u00b7len", "Fit\u00b7ti\u00b7chen", "die", "tod\u00b7ten", "Leid\u00b7ge\u00b7nos\u00b7sen", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Die ewige D\u00e4mmerung zerstob: die Nebel theilten sich zu Hauf\u2019,", "tokens": ["Die", "e\u00b7wi\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "zer\u00b7stob", ":", "die", "Ne\u00b7bel", "theil\u00b7ten", "sich", "zu", "Hauf'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Lichtfremde Welten thaten sich vor meinen Geisteraugen auf.", "tokens": ["Licht\u00b7frem\u00b7de", "Wel\u00b7ten", "tha\u00b7ten", "sich", "vor", "mei\u00b7nen", "Geis\u00b7ter\u00b7au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.9": {"text": "Nicht Lust noch Schmerz barg mehr die Brust: zu Ende war gek\u00e4mpft", "tokens": ["Nicht", "Lust", "noch", "Schmerz", "barg", "mehr", "die", "Brust", ":", "zu", "En\u00b7de", "war", "ge\u00b7k\u00e4mpft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "ADV", "ART", "NN", "$.", "APPR", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Das All war ich: ich war das All: so ward mir Friede in der Nacht.", "tokens": ["Das", "All", "war", "ich", ":", "ich", "war", "das", "All", ":", "so", "ward", "mir", "Frie\u00b7de", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}