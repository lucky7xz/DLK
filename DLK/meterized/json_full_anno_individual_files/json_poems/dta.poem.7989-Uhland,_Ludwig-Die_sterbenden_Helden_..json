{"dta.poem.7989": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbenden Helden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Der D\u00e4nen Schwerdter dr\u00e4ngen Schwedens Heer", "tokens": ["Der", "D\u00e4\u00b7nen", "Schwerd\u00b7ter", "dr\u00e4n\u00b7gen", "Schwe\u00b7dens", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum wilden Meer.", "tokens": ["Zum", "wil\u00b7den", "Meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Wagen klirren fern, es blinkt der Stahl", "tokens": ["Die", "Wa\u00b7gen", "klir\u00b7ren", "fern", ",", "es", "blinkt", "der", "Stahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Mondenstral.", "tokens": ["Im", "Mon\u00b7dens\u00b7tral", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da liegen, sterbend, auf dem Leichenfeld", "tokens": ["Da", "lie\u00b7gen", ",", "ster\u00b7bend", ",", "auf", "dem", "Lei\u00b7chen\u00b7feld"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der sch\u00f6ne Sven und Ulf, der graue Held.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Sven", "und", "Ulf", ",", "der", "grau\u00b7e", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}