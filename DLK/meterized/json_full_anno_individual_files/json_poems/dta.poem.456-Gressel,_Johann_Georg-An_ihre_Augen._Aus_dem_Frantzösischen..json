{"dta.poem.456": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An ihre Augen.  \n Aus dem Frantz\u00f6sischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was solte mein Gesicht der Sterne-Lauff beschauen/", "tokens": ["Was", "sol\u00b7te", "mein", "Ge\u00b7sicht", "der", "Ster\u00b7ne\u00b7Lauff", "be\u00b7schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf den Jrrlichts-Schein ihm sein Geheimni\u00df bauen?", "tokens": ["Und", "auf", "den", "Jrr\u00b7lichts\u00b7Schein", "ihm", "sein", "Ge\u00b7heim\u00b7ni\u00df", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Solt ich mein Ungel\u00fcck und Gl\u00fcck daraus ersehen?", "tokens": ["Solt", "ich", "mein", "Un\u00b7ge\u00b7l\u00fcck", "und", "Gl\u00fcck", "da\u00b7raus", "er\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nein! ", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "In deinen Augen wird mein Gl\u00fcck und Ungl\u00fcck stehen/", "tokens": ["In", "dei\u00b7nen", "Au\u00b7gen", "wird", "mein", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn da trifft alles wohl/ was ich ersehen/ ein.", "tokens": ["Denn", "da", "trifft", "al\u00b7les", "wohl", "/", "was", "ich", "er\u00b7se\u00b7hen", "/", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV", "$(", "PWS", "PPER", "VVINF", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}