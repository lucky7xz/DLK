{"textgrid.poem.60316": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es irrt durch schwanke Wasserh\u00fcgel", "tokens": ["Es", "irrt", "durch", "schwan\u00b7ke", "Was\u00b7ser\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im weiten, windbewegten Meer", "tokens": ["Im", "wei\u00b7ten", ",", "wind\u00b7be\u00b7weg\u00b7ten", "Meer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schmetterling mit mattem Fl\u00fcgel", "tokens": ["Ein", "Schmet\u00b7ter\u00b7ling", "mit", "mat\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und todes\u00e4ngstlich hin und her.", "tokens": ["Und", "to\u00b7de\u00b7s\u00e4ngst\u00b7lich", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihn triebs vom trauten Bl\u00fctenstrande", "tokens": ["Ihn", "triebs", "vom", "trau\u00b7ten", "Bl\u00fc\u00b7ten\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Meeresfremde fern hinaus;", "tokens": ["Zur", "Mee\u00b7res\u00b7frem\u00b7de", "fern", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom scherzend holden Fr\u00fchlingstande", "tokens": ["Vom", "scher\u00b7zend", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7tan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins ernste, kalte Flutgebraus.", "tokens": ["Ins", "erns\u00b7te", ",", "kal\u00b7te", "Flut\u00b7ge\u00b7braus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf glattgestreckte, sanfte Wogen", "tokens": ["Auf", "glatt\u00b7ge\u00b7streck\u00b7te", ",", "sanf\u00b7te", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hatt ihm das Meergras tr\u00fcgerisch", "tokens": ["Hatt", "ihm", "das", "Meer\u00b7gras", "tr\u00fc\u00b7ge\u00b7risch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel sch\u00f6nre Wiesen hingelogen,", "tokens": ["Viel", "sch\u00f6n\u00b7re", "Wie\u00b7sen", "hin\u00b7ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie westgeschaukelt, blumenfrisch.", "tokens": ["Wie", "west\u00b7ge\u00b7schau\u00b7kelt", ",", "blu\u00b7men\u00b7frisch", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihm war am Strand das leise Fl\u00fcstern", "tokens": ["Ihm", "war", "am", "Strand", "das", "lei\u00b7se", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von West und Bl\u00fcte nicht genug,", "tokens": ["Von", "West", "und", "Bl\u00fc\u00b7te", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es trieb hinaus ihn, w\u00e4hlig l\u00fcstern,", "tokens": ["Es", "trieb", "hin\u00b7aus", "ihn", ",", "w\u00e4h\u00b7lig", "l\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APZR", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu wagen einen weitern Flug.", "tokens": ["Zu", "wa\u00b7gen", "ei\u00b7nen", "wei\u00b7tern", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kaum aber war vom Strand geflogen", "tokens": ["Kaum", "a\u00b7ber", "war", "vom", "Strand", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fr\u00fchlings ungeduldges Kind:", "tokens": ["Des", "Fr\u00fch\u00b7lings", "un\u00b7ge\u00b7duld\u00b7ges", "Kind", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kam sausend hinter ihm gezogen", "tokens": ["Kam", "sau\u00b7send", "hin\u00b7ter", "ihm", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ri\u00df ihn fort der b\u00f6se Wind;", "tokens": ["Und", "ri\u00df", "ihn", "fort", "der", "b\u00f6\u00b7se", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Stets weiter fort von seines Lebens", "tokens": ["Stets", "wei\u00b7ter", "fort", "von", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu fr\u00fch verlornem Heimatgl\u00fcck;", "tokens": ["Zu", "fr\u00fch", "ver\u00b7lor\u00b7nem", "Hei\u00b7mat\u00b7gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schwache Flattrer ringt vergebens", "tokens": ["Der", "schwa\u00b7che", "Flatt\u00b7rer", "ringt", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem verschm\u00e4hten Strand zur\u00fcck.", "tokens": ["Nach", "dem", "ver\u00b7schm\u00e4h\u00b7ten", "Strand", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Von ihrem Schiffe Wandersleute", "tokens": ["Von", "ih\u00b7rem", "Schif\u00b7fe", "Wan\u00b7ders\u00b7leu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit wehmutsvollem L\u00e4cheln sehn", "tokens": ["Mit", "weh\u00b7muts\u00b7vol\u00b7lem", "L\u00e4\u00b7cheln", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die zierlich leichte Wellenbeute,", "tokens": ["Die", "zier\u00b7lich", "leich\u00b7te", "Wel\u00b7len\u00b7beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den armen Schmetterling vergehn.", "tokens": ["Den", "ar\u00b7men", "Schmet\u00b7ter\u00b7ling", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O Faust, o Faust, du Mann des Fluches!", "tokens": ["O", "Faust", ",", "o", "Faust", ",", "du", "Mann", "des", "Flu\u00b7ches", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der arme Schmetterling bist du!", "tokens": ["Der", "ar\u00b7me", "Schmet\u00b7ter\u00b7ling", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inmitten Sturms und Wogenbruches", "tokens": ["In\u00b7mit\u00b7ten", "Sturms", "und", "Wo\u00b7gen\u00b7bru\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wankst du dem Untergange zu.", "tokens": ["Wankst", "du", "dem", "Un\u00b7ter\u00b7gan\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du wagtest, eh der Tod dich gr\u00fc\u00dfte,", "tokens": ["Du", "wag\u00b7test", ",", "eh", "der", "Tod", "dich", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vorflatternd dich ins Geistermeer;", "tokens": ["Vor\u00b7flat\u00b7ternd", "dich", "ins", "Geis\u00b7ter\u00b7meer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du gehst verloren in der W\u00fcste,", "tokens": ["Du", "gehst", "ver\u00b7lo\u00b7ren", "in", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von wannen keine Wiederkehr.", "tokens": ["Von", "wan\u00b7nen", "kei\u00b7ne", "Wie\u00b7der\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wohl schauen dich die Geisterscharen,", "tokens": ["Wohl", "schau\u00b7en", "dich", "die", "Geis\u00b7ter\u00b7scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbarmen l\u00e4chelnd deinem Leid;", "tokens": ["Er\u00b7bar\u00b7men", "l\u00e4\u00b7chelnd", "dei\u00b7nem", "Leid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch m\u00fcssen sie vor\u00fcberfahren,", "tokens": ["Doch", "m\u00fcs\u00b7sen", "sie", "vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fortsteuernd durch die Ewigkeit.", "tokens": ["Fort\u00b7steu\u00b7ernd", "durch", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es irrt durch schwanke Wasserh\u00fcgel", "tokens": ["Es", "irrt", "durch", "schwan\u00b7ke", "Was\u00b7ser\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im weiten, windbewegten Meer", "tokens": ["Im", "wei\u00b7ten", ",", "wind\u00b7be\u00b7weg\u00b7ten", "Meer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schmetterling mit mattem Fl\u00fcgel", "tokens": ["Ein", "Schmet\u00b7ter\u00b7ling", "mit", "mat\u00b7tem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und todes\u00e4ngstlich hin und her.", "tokens": ["Und", "to\u00b7de\u00b7s\u00e4ngst\u00b7lich", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+",