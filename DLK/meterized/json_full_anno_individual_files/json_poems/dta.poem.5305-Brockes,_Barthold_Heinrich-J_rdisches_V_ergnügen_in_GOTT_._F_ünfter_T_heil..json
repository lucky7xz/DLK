{"dta.poem.5305": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich deucht, als ob ich sie,", "tokens": ["Mich", "deucht", ",", "als", "ob", "ich", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In schwartzer Luft, die blos durch Schnee-Gest\u00f6ber grau,", "tokens": ["In", "schwart\u00b7zer", "Luft", ",", "die", "blos", "durch", "Schnee\u00b7Ge\u00b7st\u00f6\u00b7ber", "grau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf tausend Art bereits besch\u00e4ftigt schau;", "tokens": ["Auf", "tau\u00b7send", "Art", "be\u00b7reits", "be\u00b7sch\u00e4f\u00b7tigt", "schau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie, mit starrer Hand, und mit verwegner M\u00fch,", "tokens": ["Wie", "sie", ",", "mit", "star\u00b7rer", "Hand", ",", "und", "mit", "ver\u00b7weg\u00b7ner", "M\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich, zwischen Eis-Gebirg-und abgerissnen Schollen,", "tokens": ["Sich", ",", "zwi\u00b7schen", "Eis\u00b7Ge\u00b7bir\u00b7g\u00b7und", "ab\u00b7ge\u00b7riss\u00b7nen", "Schol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die krachend \u00fcberall in strengen Strudeln rollen,", "tokens": ["Die", "kra\u00b7chend", "\u00fc\u00b7be\u00b7rall", "in", "stren\u00b7gen", "Stru\u00b7deln", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit mehr als t\u00f6dtlicher Gefahr, begeben,", "tokens": ["Mit", "mehr", "als", "t\u00f6dt\u00b7li\u00b7cher", "Ge\u00b7fahr", ",", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "KOKOM", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und, in entstandnem Sturm, bey Rasen, W\u00fcten, Sau-", "tokens": ["Und", ",", "in", "ent\u00b7stand\u00b7nem", "Sturm", ",", "bey", "Ra\u00b7sen", ",", "W\u00fc\u00b7ten", ",", "Sau"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "$,", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "sen", "tokens": ["sen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Der Winde, beym Gebr\u00fcll, Geknirsch, Geheul und", "tokens": ["Der", "Win\u00b7de", ",", "beym", "Ge\u00b7br\u00fcll", ",", "Ge\u00b7knirsch", ",", "Ge\u00b7heul", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Brausen", "tokens": ["Brau\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Der Wellen, zwischen Meer- und Wasser-Wundern", "tokens": ["Der", "Wel\u00b7len", ",", "zwi\u00b7schen", "Meer", "und", "Was\u00b7ser\u00b7Wun\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "schweben.", "tokens": ["schwe\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Geliebter Leser! la\u00df uns nun,", "tokens": ["Ge\u00b7lieb\u00b7ter", "Le\u00b7ser", "!", "la\u00df", "uns", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVIMP", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dem Schreck-Bild\u2019, unserm Stand entgegen setzen:", "tokens": ["Dem", "Schreck\u00b7Bild'", ",", "un\u00b7serm", "Stand", "ent\u00b7ge\u00b7gen", "set\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du kannst in Sicherheit auf deinem Bette ruhn,", "tokens": ["Du", "kannst", "in", "Si\u00b7cher\u00b7heit", "auf", "dei\u00b7nem", "Bet\u00b7te", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du kannst in Feld\u2019 und Wald\u2019 auf Blumen dich ergetzen,", "tokens": ["Du", "kannst", "in", "Feld'", "und", "Wald'", "auf", "Blu\u00b7men", "dich", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Du kannst, in warmer Luft, wenn laue Winde wehn,", "tokens": ["Du", "kannst", ",", "in", "war\u00b7mer", "Luft", ",", "wenn", "lau\u00b7e", "Win\u00b7de", "wehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf einem sichern Boden gehn.", "tokens": ["Auf", "ei\u00b7nem", "si\u00b7chern", "Bo\u00b7den", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ach, la\u00df uns dieses denn doch vor ein Gl\u00fccke sch\u00e4tzen!", "tokens": ["Ach", ",", "la\u00df", "uns", "die\u00b7ses", "denn", "doch", "vor", "ein", "Gl\u00fc\u00b7cke", "sch\u00e4t\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PDAT", "KON", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach la\u00df uns oft den Unterscheid besehn,", "tokens": ["Ach", "la\u00df", "uns", "oft", "den", "Un\u00b7ter\u00b7scheid", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und in Erk\u00e4nntlichkeit, des Sch\u00f6pfers Huld erh\u00f6hn!", "tokens": ["Und", "in", "Er\u00b7k\u00e4nnt\u00b7lich\u00b7keit", ",", "des", "Sch\u00f6p\u00b7fers", "Huld", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Indessen w\u00fcnschen wir den Arbeit-seel\u2019gen Leuten,", "tokens": ["In\u00b7des\u00b7sen", "w\u00fcn\u00b7schen", "wir", "den", "Ar\u00b7beit\u00b7seel'\u00b7gen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Auf ihrer schl\u00fcpfrichen, beschwerlich-rauhen Bahn,", "tokens": ["Auf", "ih\u00b7rer", "schl\u00fcpf\u00b7ri\u00b7chen", ",", "be\u00b7schwer\u00b7lich\u00b7rau\u00b7hen", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zu ihrer Reise Gl\u00fcck von gantzen Hertzen an,", "tokens": ["Zu", "ih\u00b7rer", "Rei\u00b7se", "Gl\u00fcck", "von", "gant\u00b7zen", "Hert\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df sie, was sie gesucht, in Ueberflu\u00df erbeuten!", "tokens": ["Da\u00df", "sie", ",", "was", "sie", "ge\u00b7sucht", ",", "in", "Ue\u00b7berf\u00b7lu\u00df", "er\u00b7beu\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}