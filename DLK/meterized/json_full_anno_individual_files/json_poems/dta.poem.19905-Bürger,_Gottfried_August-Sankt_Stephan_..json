{"dta.poem.19905": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Stephan .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Sankt Stephan war ein Gottesman;               ", "tokens": ["Sankt", "Ste\u00b7phan", "war", "ein", "Got\u00b7tes\u00b7man", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Geist berathen,", "tokens": ["Von", "Got\u00b7tes", "Geist", "be\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der durch den Glauben Kraft gewan,", "tokens": ["Der", "durch", "den", "Glau\u00b7ben", "Kraft", "ge\u00b7wan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu hohen Wunderthaten.", "tokens": ["Zu", "ho\u00b7hen", "Wun\u00b7der\u00b7tha\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch seines Glaubens Wunderkraft,", "tokens": ["Doch", "sei\u00b7nes", "Glau\u00b7bens", "Wun\u00b7der\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Himmelswissenschaft", "tokens": ["Und", "sei\u00b7ne", "Him\u00b7mels\u00b7wis\u00b7sen\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verdros die Schulgelehrten,", "tokens": ["Ver\u00b7dros", "die", "Schul\u00b7ge\u00b7lehr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Erdenweisheit ehrten.", "tokens": ["Die", "Er\u00b7den\u00b7weis\u00b7heit", "ehr\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und die Gelehrten stritten scharf", "tokens": ["Und", "die", "Ge\u00b7lehr\u00b7ten", "strit\u00b7ten", "scharf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und waren ihm zuwider;", "tokens": ["Und", "wa\u00b7ren", "ihm", "zu\u00b7wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein die Himmelsweisheit warf", "tokens": ["Al\u00b7lein", "die", "Him\u00b7mels\u00b7weis\u00b7heit", "warf"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Irdische danieder.", "tokens": ["Die", "Ir\u00b7di\u00b7sche", "da\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ihr besch\u00e4mter Hochmut san", "tokens": ["Und", "ihr", "be\u00b7sch\u00e4m\u00b7ter", "Hoch\u00b7mut", "san"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Rache an dem Gottesman.", "tokens": ["Auf", "Ra\u00b7che", "an", "dem", "Got\u00b7tes\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihn zu verleumden, dungen", "tokens": ["Ihn", "zu", "ver\u00b7leum\u00b7den", ",", "dun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie falscher Zeugen Zungen.", "tokens": ["Sie", "fal\u00b7scher", "Zeu\u00b7gen", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und gegen ihn in Aufruhr trat", "tokens": ["Und", "ge\u00b7gen", "ihn", "in", "Auf\u00b7ruhr", "trat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die J\u00fcdische Gemeinde.", "tokens": ["Die", "J\u00fc\u00b7di\u00b7sche", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald ris ihn vor den hohen Rath", "tokens": ["Bald", "ris", "ihn", "vor", "den", "ho\u00b7hen", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rachgier seiner Feinde.", "tokens": ["Die", "Rach\u00b7gier", "sei\u00b7ner", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die falschen Zeugen stiegen auf", "tokens": ["Die", "fal\u00b7schen", "Zeu\u00b7gen", "stie\u00b7gen", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und logen: Dieser h\u00f6rt nicht auf,", "tokens": ["Und", "lo\u00b7gen", ":", "Die\u00b7ser", "h\u00f6rt", "nicht", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PDS", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum str\u00e4flichsten Exempel,", "tokens": ["Zum", "str\u00e4f\u00b7lichs\u00b7ten", "Ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zu l\u00e4stern Gott und Tempel.", "tokens": ["Zu", "l\u00e4s\u00b7tern", "Gott", "und", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201esein Jesus, schm\u00e4ht er, w\u00fcrde nun", "tokens": ["\u201e", "sein", "Je\u00b7sus", ",", "schm\u00e4ht", "er", ",", "w\u00fcr\u00b7de", "nun"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPOSAT", "NE", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Tempels Dienst zerst\u00f6ren;", "tokens": ["Des", "Tem\u00b7pels", "Dienst", "zer\u00b7st\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hinweg die Sazung Mosis thun,", "tokens": ["Hin\u00b7weg", "die", "Sa\u00b7zung", "Mo\u00b7sis", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und andre Sitte lehren.\u201e \u2014", "tokens": ["Und", "and\u00b7re", "Sit\u00b7te", "leh\u00b7ren", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Starr sah der ganze Rath ihn an;", "tokens": ["Starr", "sah", "der", "gan\u00b7ze", "Rath", "ihn", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch Er, mit Unschuld angethan,", "tokens": ["Doch", "Er", ",", "mit", "Un\u00b7schuld", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Troz dem, was sie bezeugten,", "tokens": ["Troz", "dem", ",", "was", "sie", "be\u00b7zeug\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schien Engeln gleich zu leuchten.", "tokens": ["Schien", "En\u00b7geln", "gleich", "zu", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201enun sprich! Ist dem also?\u201e began", "tokens": ["\u201e", "nun", "sprich", "!", "Ist", "dem", "al\u00b7so", "?", "\u201e", "be\u00b7gan"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["$(", "ADV", "ADJD", "$.", "VAFIN", "ART", "ADV", "$.", "$(", "NE"], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Hohepriester endlich.", "tokens": ["Der", "Ho\u00b7he\u00b7pries\u00b7ter", "end\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hub er frei zu reden an,", "tokens": ["Da", "hub", "er", "frei", "zu", "re\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und deutete verst\u00e4ndlich", "tokens": ["Und", "deu\u00b7te\u00b7te", "ver\u00b7st\u00e4nd\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der heiligen Propheten Sin,", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Pro\u00b7phe\u00b7ten", "Sin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was der Herr von Anbegin,", "tokens": ["Und", "was", "der", "Herr", "von", "An\u00b7be\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Juda\u2019s Heil und Frommen,", "tokens": ["Zu", "Ju\u00b7da's", "Heil", "und", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gered\u2019t und unternommen.", "tokens": ["Ge\u00b7red't", "und", "un\u00b7ter\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201edoch, Unbeschnitt\u2019ne, fuhr er fort,", "tokens": ["\u201e", "doch", ",", "Un\u00b7be\u00b7schnitt'\u00b7ne", ",", "fuhr", "er", "fort", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Herzen und an Ohren!", "tokens": ["An", "Her\u00b7zen", "und", "an", "Oh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An Euch war Gottes That und Wort", "tokens": ["An", "Euch", "war", "Got\u00b7tes", "That", "und", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von je und je verloren.", "tokens": ["Von", "je", "und", "je", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eur Stolz, der sich der Zucht entreist,", "tokens": ["Eur", "Stolz", ",", "der", "sich", "der", "Zucht", "en\u00b7treist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stets widerstrebt er Gottes Geist.", "tokens": ["Stets", "wi\u00b7der\u00b7strebt", "er", "Got\u00b7tes", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr, so wie eure V\u00e4ter,", "tokens": ["Ihr", ",", "so", "wie", "eu\u00b7re", "V\u00e4\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Seyd M\u00f6rder und Verr\u00e4ter!", "tokens": ["Seyd", "M\u00f6r\u00b7der", "und", "Ver\u00b7r\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nent mir Propheten, die sie nicht", "tokens": ["Nent", "mir", "Pro\u00b7phe\u00b7ten", ",", "die", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "PRELS", "PPER", "PTKNEG"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Verfolgt und hingerichtet,", "tokens": ["Ver\u00b7folgt", "und", "hin\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn sie aus g\u00f6ttlichem Gesicht", "tokens": ["Wenn", "sie", "aus", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Heilands Kunft berichtet;", "tokens": ["Des", "Hei\u00b7lands", "Kunft", "be\u00b7rich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Heilands, welchen eur Verrath", "tokens": ["Des", "Hei\u00b7lands", ",", "wel\u00b7chen", "eur", "Ver\u00b7rath"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Tode jezt gekreuzigt hat.", "tokens": ["Zu", "To\u00b7de", "jezt", "ge\u00b7kreu\u00b7zigt", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr wist zwar Gottes Willen;", "tokens": ["Ihr", "wist", "zwar", "Got\u00b7tes", "Wil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch wolt ihn nie erf\u00fcllen.\u201e \u2014", "tokens": ["Doch", "wolt", "ihn", "nie", "er\u00b7f\u00fcl\u00b7len", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und horch! ein dumpfer L\u00e4rm erschol.", "tokens": ["Und", "horch", "!", "ein", "dum\u00b7pfer", "L\u00e4rm", "er\u00b7schol", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es knirschte das Get\u00fcmmel.", "tokens": ["Es", "knirschte", "das", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Er aber ward des Geistes vol,", "tokens": ["Er", "a\u00b7ber", "ward", "des", "Geis\u00b7tes", "vol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und blikt\u2019 empor gen Himmel,", "