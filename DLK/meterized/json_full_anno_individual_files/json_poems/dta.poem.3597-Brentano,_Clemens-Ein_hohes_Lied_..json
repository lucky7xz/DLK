{"dta.poem.3597": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Ein hohes Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz das schwebt in Freudenspur,               ", "tokens": ["Mein", "Herz", "das", "schwebt", "in", "Freu\u00b7den\u00b7spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenk ich, wie die Kreatur", "tokens": ["Ge\u00b7denk", "ich", ",", "wie", "die", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Zweiheit ist gebildet;", "tokens": ["In", "Zwei\u00b7heit", "ist", "ge\u00b7bil\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des sey gelobt der Sch\u00f6pfer weis',", "tokens": ["Des", "sey", "ge\u00b7lobt", "der", "Sch\u00f6p\u00b7fer", "weis'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der uns erschuf im Paradeis,", "tokens": ["Der", "uns", "er\u00b7schuf", "im", "Pa\u00b7ra\u00b7deis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erschuf jungfr\u00e4ulichs Bilde,", "tokens": ["Er\u00b7schuf", "jung\u00b7fr\u00e4u\u00b7lichs", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die er da einem J\u00fcngling gab,", "tokens": ["Die", "er", "da", "ei\u00b7nem", "J\u00fcng\u00b7ling", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den er gemacht aus Erden;", "tokens": ["Den", "er", "ge\u00b7macht", "aus", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Darum dien jezt ich junger Knab", "tokens": ["Da\u00b7rum", "di\u00b7en", "jezt", "ich", "jun\u00b7ger", "Knab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PPER", "ADJA", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.10": {"text": "Wohl einer Jungfrau werthe.", "tokens": ["Wohl", "ei\u00b7ner", "Jung\u00b7frau", "wert\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ihr hohes Lob, das will ich ihr verk\u00fcnden,", "tokens": ["Ihr", "ho\u00b7hes", "Lob", ",", "das", "will", "ich", "ihr", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PDS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ob ich es m\u00f6gt durchgr\u00fcnden,", "tokens": ["Ob", "ich", "es", "m\u00f6gt", "durch\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Nach meines Herzens Gier,", "tokens": ["Nach", "mei\u00b7nes", "Her\u00b7zens", "Gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ob ich gefiel auch ihr.", "tokens": ["Ob", "ich", "ge\u00b7fiel", "auch", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}