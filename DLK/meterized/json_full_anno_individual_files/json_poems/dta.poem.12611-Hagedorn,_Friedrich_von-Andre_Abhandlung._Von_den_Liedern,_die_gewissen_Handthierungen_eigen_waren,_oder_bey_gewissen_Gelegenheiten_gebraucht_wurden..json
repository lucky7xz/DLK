{"dta.poem.12611": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Andre Abhandlung.  \n Von den Liedern, die gewissen Handthierungen eigen waren,  \n oder bey gewissen Gelegenheiten gebraucht wurden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlafe s\u00fc\u00df, geliebtes Paar,", "tokens": ["Schla\u00b7fe", "s\u00fc\u00df", ",", "ge\u00b7lieb\u00b7tes", "Paar", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaft, geliebte Herzen,", "tokens": ["Schlaft", ",", "ge\u00b7lieb\u00b7te", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Frey von Unruh und Gefahr,", "tokens": ["Frey", "von", "Un\u00b7ruh", "und", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Frey von Sorg und Schmerzen.", "tokens": ["Frey", "von", "Sorg", "und", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Liebe Kinder, gute Nacht!", "tokens": ["Lie\u00b7be", "Kin\u00b7der", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafet, liebe Br\u00fcder,", "tokens": ["Schla\u00b7fet", ",", "lie\u00b7be", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schlafet gl\u00fccklich ein, erwacht", "tokens": ["Schla\u00b7fet", "gl\u00fcck\u00b7lich", "ein", ",", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen gl\u00fccklich wieder.", "tokens": ["Mor\u00b7gen", "gl\u00fcck\u00b7lich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}