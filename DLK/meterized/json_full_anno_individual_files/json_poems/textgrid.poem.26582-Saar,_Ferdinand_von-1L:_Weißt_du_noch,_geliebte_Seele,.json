{"textgrid.poem.26582": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wei\u00dft du noch, geliebte Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du noch, geliebte Seele,", "tokens": ["Wei\u00dft", "du", "noch", ",", "ge\u00b7lieb\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie wir einst \u2013 hell schien die Sonne \u2013", "tokens": ["Wie", "wir", "einst", "\u2013", "hell", "schien", "die", "Son\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$(", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Leis und sch\u00fcchtern uns ergingen", "tokens": ["Leis", "und", "sch\u00fcch\u00b7tern", "uns", "er\u00b7gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Garten eines Klosters?", "tokens": ["In", "dem", "Gar\u00b7ten", "ei\u00b7nes", "Klos\u00b7ters", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Offen stand die kleine Pforte \u2013", "tokens": ["Of\u00b7fen", "stand", "die", "klei\u00b7ne", "Pfor\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir waren eingetreten,", "tokens": ["Und", "wir", "wa\u00b7ren", "ein\u00b7ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tief im Herzen angemuthet", "tokens": ["Tief", "im", "Her\u00b7zen", "an\u00b7ge\u00b7mu\u00b7thet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Ortes Ruh' und Frieden.", "tokens": ["Von", "des", "Or\u00b7tes", "Ruh'", "und", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Herbst schon war es. An den Birken", "tokens": ["Herbst", "schon", "war", "es", ".", "An", "den", "Bir\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldig schimmerten die Bl\u00e4tter;", "tokens": ["Gol\u00b7dig", "schim\u00b7mer\u00b7ten", "die", "Bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Astern bl\u00fchten, Georginen \u2013", "tokens": ["As\u00b7tern", "bl\u00fch\u00b7ten", ",", "Ge\u00b7or\u00b7gi\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NE", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier und dort auch blasse Rosen.", "tokens": ["Hier", "und", "dort", "auch", "blas\u00b7se", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wir schritten wie verloren", "tokens": ["Und", "wir", "schrit\u00b7ten", "wie", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In weltfernen Einsamkeiten;", "tokens": ["In", "welt\u00b7fer\u00b7nen", "Ein\u00b7sam\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Niemand sah uns \u2013 doch wir schlugen", "tokens": ["Nie\u00b7mand", "sah", "uns", "\u2013", "doch", "wir", "schlu\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$(", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor uns selbst die Augen nieder.", "tokens": ["Vor", "uns", "selbst", "die", "Au\u00b7gen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Reiner waren wir gekommen,", "tokens": ["Rei\u00b7ner", "wa\u00b7ren", "wir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiner waren wir gegangen", "tokens": ["Rei\u00b7ner", "wa\u00b7ren", "wir", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die M\u00f6nche, die inzwischen", "tokens": ["Als", "die", "M\u00f6n\u00b7che", ",", "die", "in\u00b7zwi\u00