{"dta.poem.14333": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eund was weint ihr, Vater und Mutter, um mich?", "tokens": ["\u201e", "und", "was", "weint", "ihr", ",", "Va\u00b7ter", "und", "Mut\u00b7ter", ",", "um", "mich", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,", "KOUI", "PPER", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In einem viel sch\u00f6neren Garten bin ich,", "tokens": ["In", "ei\u00b7nem", "viel", "sch\u00f6\u00b7ne\u00b7ren", "Gar\u00b7ten", "bin", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der ist so gro\u00df und weit und wunderbar,", "tokens": ["Der", "ist", "so", "gro\u00df", "und", "weit", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Viel Blumen steh'n dort von Golde klar", "tokens": ["Viel", "Blu\u00b7men", "steh'n", "dort", "von", "Gol\u00b7de", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und sch\u00f6ne Kindlein mit Fl\u00fcgeln schwingen", "tokens": ["Und", "sch\u00f6\u00b7ne", "Kin\u00b7dlein", "mit", "Fl\u00fc\u00b7geln", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf und nieder sich drauf und singen. \u2014", "tokens": ["Auf", "und", "nie\u00b7der", "sich", "drauf", "und", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "KON", "PTKVZ", "PRF", "PAV", "KON", "VVFIN", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Die kenn' ich gar wohl aus der Fr\u00fchlingszeit,", "tokens": ["Die", "kenn'", "ich", "gar", "wohl", "aus", "der", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie sie zogen \u00fcber Berge und Th\u00e4ler weit", "tokens": ["Wie", "sie", "zo\u00b7gen", "\u00fc\u00b7ber", "Ber\u00b7ge", "und", "Th\u00e4\u00b7ler", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}