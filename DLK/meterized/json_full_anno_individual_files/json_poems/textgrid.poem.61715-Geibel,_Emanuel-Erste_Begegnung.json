{"textgrid.poem.61715": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Erste Begegnung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich war sie als Kind, schwarz\u00e4ugig; schimmernde Bl\u00e4sse,", "tokens": ["Lieb\u00b7lich", "war", "sie", "als", "Kind", ",", "schwarz\u00b7\u00e4u\u00b7gig", ";", "schim\u00b7mern\u00b7de", "Bl\u00e4s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "KOUS", "NN", "$,", "ADJD", "$.", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wie sie die Perle dir zeigt, lag ihr um Wangen und Stirn,", "tokens": ["Wie", "sie", "die", "Per\u00b7le", "dir", "zeigt", ",", "lag", "ihr", "um", "Wan\u00b7gen", "und", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df fremdartig sie fast im Kreise der blonden Geschwister", "tokens": ["Da\u00df", "fremd\u00b7ar\u00b7tig", "sie", "fast", "im", "Krei\u00b7se", "der", "blon\u00b7den", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+---+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie ein s\u00fcdlich Gew\u00e4chs unter den heimischen stand.", "tokens": ["Wie", "ein", "s\u00fcd\u00b7lich", "Ge\u00b7w\u00e4chs", "un\u00b7ter", "den", "hei\u00b7mi\u00b7schen", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber ich sah sie zuerst elfj\u00e4hrig am Ufer des Meeres,", "tokens": ["A\u00b7ber", "ich", "sah", "sie", "zu\u00b7erst", "elf\u00b7j\u00e4h\u00b7rig", "am", "U\u00b7fer", "des", "Mee\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Da sie vom Bad heimkam in der Gespielinnen Schwarm,", "tokens": ["Da", "sie", "vom", "Bad", "heim\u00b7kam", "in", "der", "Ge\u00b7spie\u00b7lin\u00b7nen", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NE", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--++-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Froh des k\u00f6stlichen Tags; denn im Seewind rauschte die Brandung", "tokens": ["Froh", "des", "k\u00f6st\u00b7li\u00b7chen", "Tags", ";", "denn", "im", "See\u00b7wind", "rauschte", "die", "Bran\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "$.", "KON", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Hoch, und im sonnigen Blau flatterte wei\u00dfes Gew\u00f6lk.", "tokens": ["Hoch", ",", "und", "im", "son\u00b7ni\u00b7gen", "Blau", "flat\u00b7ter\u00b7te", "wei\u00b7\u00dfes", "Ge\u00b7w\u00f6lk", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "APPRART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Leicht wie ein Rehlein sprang sie dahin, lang flog ihr das dunkle", "tokens": ["Leicht", "wie", "ein", "Reh\u00b7lein", "sprang", "sie", "da\u00b7hin", ",", "lang", "flog", "ihr", "das", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PAV", "$,", "ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Haar, zum Trocknen gel\u00f6st, \u00fcber die H\u00fcften herab.", "tokens": ["Haar", ",", "zum", "Trock\u00b7nen", "ge\u00b7l\u00f6st", ",", "\u00fc\u00b7ber", "die", "H\u00fcf\u00b7ten", "her\u00b7ab", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Doch mich r\u00fchrte die feine Gestalt, mich r\u00fchrte des Auges", "tokens": ["Doch", "mich", "r\u00fchr\u00b7te", "die", "fei\u00b7ne", "Ge\u00b7stalt", ",", "mich", "r\u00fchr\u00b7te", "des", "Au\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Ahnungsseliger Glanz, der wie ein R\u00e4tsel mich zog;", "tokens": ["Ah\u00b7nungs\u00b7se\u00b7li\u00b7ger", "Glanz", ",", "der", "wie", "ein", "R\u00e4t\u00b7sel", "mich", "zog", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Und wie J\u00fcnglinge sind, die blitzschnell jeder Empfindung", "tokens": ["Und", "wie", "J\u00fcng\u00b7lin\u00b7ge", "sind", ",", "die", "blitz\u00b7schnell", "je\u00b7der", "Emp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "VAFIN", "$,", "PRELS", "ADJD", "PIAT", "NN"], "meter": "--++-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Folgen, befl\u00fcgelten Schritts eilt' ich der Lieblichen nach,", "tokens": ["Fol\u00b7gen", ",", "be\u00b7fl\u00fc\u00b7gel\u00b7ten", "Schritts", "eilt'", "ich", "der", "Lieb\u00b7li\u00b7chen", "nach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Und von hinten sie leis an den zierlichen Schultern ergreifend,", "tokens": ["Und", "von", "hin\u00b7ten", "sie", "leis", "an", "den", "zier\u00b7li\u00b7chen", "Schul\u00b7tern", "er\u00b7grei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Lehnt' ich im Scherz ihr Haupt sacht an die Brust mir empor.", "tokens": ["Lehnt'", "ich", "im", "Scherz", "ihr", "Haupt", "sacht", "an", "die", "Brust", "mir", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "Aber sie machte sich los, und tief aus schattigen Wimpern", "tokens": ["A\u00b7ber", "sie", "mach\u00b7te", "sich", "los", ",", "und", "tief", "aus", "schat\u00b7ti\u00b7gen", "Wim\u00b7pern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Unbeschreiblichen Blicks schaute sie lange mich an", "tokens": ["Un\u00b7be\u00b7schreib\u00b7li\u00b7chen", "Blicks", "schau\u00b7te", "sie", "lan\u00b7ge", "mich", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Vorwurfsvoll und freundlich zugleich. Da zuckte das Herz mir,", "tokens": ["Vor\u00b7wurfs\u00b7voll", "und", "freund\u00b7lich", "zu\u00b7gleich", ".", "Da", "zuck\u00b7te", "das", "Herz", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADV", "$.", "ADV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Wie in des Weidmanns Hand \u00fcber verborgenem Quell", "tokens": ["Wie", "in", "des", "Weid\u00b7manns", "Hand", "\u00fc\u00b7ber", "ver\u00b7bor\u00b7ge\u00b7nem", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Pl\u00f6tzlich die Rute sich r\u00fchrt. Nicht wei\u00df ich, war es der Blick nur,", "tokens": ["Pl\u00f6tz\u00b7lich", "die", "Ru\u00b7te", "sich", "r\u00fchrt", ".", "Nicht", "wei\u00df", "ich", ",", "war", "es", "der", "Blick", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PRF", "VVFIN", "$.", "PTKNEG", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "War es ein Zukunftshauch, was mir die Seele bewegt?", "tokens": ["War", "es", "ein", "Zu\u00b7kunfts\u00b7hauch", ",", "was", "mir", "die", "See\u00b7le", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+----+-+--+", "measure": "iambic.tetra.chol"}, "line.23": {"text": "Doch wie ein Tr\u00e4umender schritt ich hinaus in die D\u00fcnen, und lang noch", "tokens": ["Doch", "wie", "ein", "Tr\u00e4u\u00b7men\u00b7der", "schritt", "ich", "hin\u00b7aus", "in", "die", "D\u00fc\u00b7nen", ",", "und", "lang", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "APZR", "APPR", "ART", "NN", "$,", "KON", "ADJD", "ADV"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Dacht' ich des lieblichen Kinds, das ich am Hafen gesehn.", "tokens": ["Dacht'", "ich", "des", "lieb\u00b7li\u00b7chen", "Kinds", ",", "das", "ich", "am", "Ha\u00b7fen", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Lieblich war sie als Kind, schwarz\u00e4ugig; schimmernde Bl\u00e4sse,", "tokens": ["Lieb\u00b7lich", "war", "sie", "als", "Kind", ",", "schwarz\u00b7\u00e4u\u00b7gig", ";", "schim\u00b7mern\u00b7de", "Bl\u00e4s\u00b