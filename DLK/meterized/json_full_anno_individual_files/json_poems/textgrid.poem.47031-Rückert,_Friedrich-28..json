{"textgrid.poem.47031": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da steht sie nun, o da\u00df ihr stehn sie s\u00e4het,", "tokens": ["Da", "steht", "sie", "nun", ",", "o", "da\u00df", "ihr", "stehn", "sie", "s\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie meine Hand sich fest in ihre drucket,", "tokens": ["Wie", "mei\u00b7ne", "Hand", "sich", "fest", "in", "ih\u00b7re", "dru\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "ADJD", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie dr\u00fcber keine Miene nur verzucket", "tokens": ["Sie", "dr\u00fc\u00b7ber", "kei\u00b7ne", "Mie\u00b7ne", "nur", "ver\u00b7zu\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "PIAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unbek\u00fcmmert ringshin horcht und sp\u00e4het.", "tokens": ["Und", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "rings\u00b7hin", "horcht", "und", "sp\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Hund, der drau\u00dfen bellt, der Hahn, der kr\u00e4het,", "tokens": ["Der", "Hund", ",", "der", "drau\u00b7\u00dfen", "bellt", ",", "der", "Hahn", ",", "der", "kr\u00e4\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das M\u00e4uschen, das still in der Ecke spuket,", "tokens": ["Das", "M\u00e4u\u00b7schen", ",", "das", "still", "in", "der", "E\u00b7cke", "spu\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Sperling, der durchs offne Fenster gucket,", "tokens": ["Der", "Sper\u00b7ling", ",", "der", "durchs", "off\u00b7ne", "Fens\u00b7ter", "gu\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts so gering, so klein, da\u00df sie's verschm\u00e4het.", "tokens": ["Nichts", "so", "ge\u00b7ring", ",", "so", "klein", ",", "da\u00df", "sie's", "ver\u00b7schm\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn sie mu\u00df alles mit den Augen sehen,", "tokens": ["Denn", "sie", "mu\u00df", "al\u00b7les", "mit", "den", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "mea