{"textgrid.poem.36596": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gespr\u00e4ch mit sich selbst", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chloe will mit ihren Blicken", "tokens": ["Chloe", "will", "mit", "ih\u00b7ren", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich bestricken,", "tokens": ["Mich", "be\u00b7stri\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Aber sie bestrickt mich nicht!", "tokens": ["A\u00b7ber", "sie", "be\u00b7strickt", "mich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr will ich, gleich schlauen Fischen,", "tokens": ["Ihr", "will", "ich", ",", "gleich", "schlau\u00b7en", "Fi\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Wohl entwischen,", "tokens": ["Wohl", "ent\u00b7wi\u00b7schen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Aus den Netzen im Gesicht!", "tokens": ["Aus", "den", "Net\u00b7zen", "im", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00e4nger soll sie mich nicht