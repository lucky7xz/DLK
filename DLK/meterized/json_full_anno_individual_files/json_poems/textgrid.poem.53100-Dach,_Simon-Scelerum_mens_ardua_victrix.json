{"textgrid.poem.53100": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Scelerum mens ardua victrix", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was suchstu, sch\u00e4ndliche Begier?", "tokens": ["Was", "suchs\u00b7tu", ",", "sch\u00e4nd\u00b7li\u00b7che", "Be\u00b7gier", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Seelen Feind, du Wunder-Thier,", "tokens": ["Du", "See\u00b7len", "Feind", ",", "du", "Wun\u00b7der\u00b7Thier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich armen abermal zu f\u00e4llen?", "tokens": ["Mich", "ar\u00b7men", "a\u00b7ber\u00b7mal", "zu", "f\u00e4l\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geh' eilends, packe dich von mir!", "tokens": ["Geh'", "ei\u00b7lends", ",", "pa\u00b7cke", "dich", "von", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Geh auff den finstern Grund der Hellen,", "tokens": ["Geh", "auff", "den", "fins\u00b7tern", "Grund", "der", "Hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den b\u00f6sen Geistern nachzustellen!", "tokens": ["Den", "b\u00f6\u00b7sen", "Geis\u00b7tern", "nach\u00b7zu\u00b7stel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kenne deine Tyranney,", "tokens": ["Ich", "ken\u00b7ne", "dei\u00b7ne", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe deiner Heucheley", "tokens": ["Ich", "ha\u00b7be", "dei\u00b7ner", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Schaden vormals beygepflichtet,", "tokens": ["Mit", "Scha\u00b7den", "vor\u00b7mals", "bey\u00b7ge\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin jetzt noch kaum derselben frey;", "tokens": ["Bin", "jetzt", "noch", "kaum", "der\u00b7sel\u00b7ben", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab' auff das mein Thun gerichtet,", "tokens": ["Ich", "hab'", "auff", "das", "mein", "Thun", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was deine Geilheit gantz vernichtet.", "tokens": ["Was", "dei\u00b7ne", "Geil\u00b7heit", "gantz", "ver\u00b7nich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auff, meine Seele, nimm in acht,", "tokens": ["Auff", ",", "mei\u00b7ne", "See\u00b7le", ",", "nimm", "in", "acht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPOSAT", "NN", "$,", "VVIMP", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie embsig dein Verderben wacht!", "tokens": ["Wie", "emb\u00b7sig", "dein", "Ver\u00b7der\u00b7ben", "wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df dich mit nichten vbert\u00e4uben!", "tokens": ["La\u00df", "dich", "mit", "nich\u00b7ten", "vbert\u00b7\u00e4u\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIS", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sey auff dein Ampt vnd Pflicht bedacht!", "tokens": ["Sey", "auff", "dein", "Ampt", "vnd", "Pflicht", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du must den Feind jetzt hintertreiben,", "tokens": ["Du", "must", "den", "Feind", "jetzt", "hin\u00b7ter\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihn in der ersten Bl\u00fct' auffreiben", "tokens": ["Ihn", "in", "der", "ers\u00b7ten", "Bl\u00fct'", "auf\u00b7frei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du, vnsere Gebieterin,", "tokens": ["Du", ",", "vn\u00b7se\u00b7re", "Ge\u00b7bie\u00b7te\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des gantzen Menschen K\u00f6nigin,", "tokens": ["Des", "gant\u00b7zen", "Men\u00b7schen", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lass den Begierden nicht den Willen!", "tokens": ["Lass", "den", "Be\u00b7gier\u00b7den", "nicht", "den", "Wil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dempf eilends den verkehrten Sinn!", "tokens": ["Dempf", "ei\u00b7lends", "den", "ver\u00b7kehr\u00b7ten", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein viehisch Toben bald zu stillen", "tokens": ["Sein", "vie\u00b7hisch", "To\u00b7ben", "bald", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PIAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mustu mit gutem Raht mich f\u00fcllen.", "tokens": ["Mus\u00b7tu", "mit", "gu\u00b7tem", "Raht", "mich", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du bist vergebens nicht erh\u00f6ht:", "tokens": ["Du", "bist", "ver\u00b7ge\u00b7bens", "nicht", "er\u00b7h\u00f6ht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein' Herrschafft vnd Verwaltung steht", "tokens": ["Dein'", "Herr\u00b7schafft", "vnd", "Ver\u00b7wal\u00b7tung", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im H\u00e4upt, als einem Schloss, erhaben;", "tokens": ["Im", "H\u00e4upt", ",", "als", "ei\u00b7nem", "Schloss", ",", "er\u00b7ha\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schaw, wie der Sinnen Frevel geht", "tokens": ["Schaw", ",", "wie", "der", "Sin\u00b7nen", "Fre\u00b7vel", "geht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd trotzet deiner Wei\u00dfheit Gaben!", "tokens": ["Vnd", "trot\u00b7zet", "dei\u00b7ner", "Wei\u00df\u00b7heit", "Ga\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie vngez\u00e4hmt die Hengste traben!", "tokens": ["Wie", "vn\u00b7ge\u00b7z\u00e4hmt", "die", "Hengs\u00b7te", "tra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Verz\u00e4un' jhm seine wilde Flucht", "tokens": ["Ver\u00b7z\u00e4un'", "jhm", "sei\u00b7ne", "wil\u00b7de", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd tritt jhn vnter deine Zucht!", "tokens": ["Vnd", "tritt", "jhn", "vn\u00b7ter", "dei\u00b7ne", "Zucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst kriegstu schaden im Gewissen,", "tokens": ["Sonst", "kriegs\u00b7tu", "scha\u00b7den", "im", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestattest du jhm, was er sucht,", "tokens": ["Ge\u00b7stat\u00b7test", "du", "jhm", ",", "was", "er", "sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirst neben jhm mit fortgerissen;", "tokens": ["Wirst", "ne\u00b7ben", "jhm", "mit", "fort\u00b7ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Es steht am meisten dir zu b\u00fcssen.", "tokens": ["Es", "steht", "am", "meis\u00b7ten", "dir", "zu", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "PIS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was suchstu, sch\u00e4ndliche Begier?", "token