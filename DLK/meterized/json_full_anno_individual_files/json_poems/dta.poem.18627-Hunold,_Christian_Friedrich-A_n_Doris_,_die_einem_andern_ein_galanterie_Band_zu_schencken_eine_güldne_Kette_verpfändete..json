{"dta.poem.18627": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n   Doris ,  die einem andern ein  \n  galanterie   Band zu schencken/ eine  \n g\u00fcldne Kette verpf\u00e4ndete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Klage nicht/ da\u00df dich mein Geist", "tokens": ["Kla\u00b7ge", "nicht", "/", "da\u00df", "dich", "mein", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun nicht mehr die Liebste heist:", "tokens": ["Nun", "nicht", "mehr", "die", "Liebs\u00b7te", "heist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Doris ich thu keine S\u00fcnden/", "tokens": ["Do\u00b7ris", "ich", "thu", "kei\u00b7ne", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Treu wird nicht verletzt/", "tokens": ["Mei\u00b7ne", "Treu", "wird", "nicht", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Du kanst mich nicht weiter binden/", "tokens": ["Du", "kanst", "mich", "nicht", "wei\u00b7ter", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Denn die Ketten ist versetzt.", "tokens": ["Denn", "die", "Ket\u00b7ten", "ist", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}