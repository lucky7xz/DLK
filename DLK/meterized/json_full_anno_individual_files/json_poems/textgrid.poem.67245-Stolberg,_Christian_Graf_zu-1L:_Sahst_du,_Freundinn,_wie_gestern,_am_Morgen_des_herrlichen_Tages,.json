{"textgrid.poem.67245": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sahst du, Freundinn, wie gestern, am Morgen des herrlichen Tages,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sahst du, Freundinn, wie gestern, am Morgen des herrlichen Tages,", "tokens": ["Sahst", "du", ",", "Freun\u00b7dinn", ",", "wie", "ge\u00b7stern", ",", "am", "Mor\u00b7gen", "des", "herr\u00b7li\u00b7chen", "Ta\u00b7ges", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "ADJD", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Als der Fels und der Strom Zeuge des Bundes uns war,", "tokens": ["Als", "der", "Fels", "und", "der", "Strom", "Zeu\u00b7ge", "des", "Bun\u00b7des", "uns", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "NN", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sahst du die Nebelschleier, die uns umwallten? sie h\u00fcllten", "tokens": ["Sahst", "du", "die", "Ne\u00b7bel\u00b7schlei\u00b7er", ",", "die", "uns", "um\u00b7wall\u00b7ten", "?", "sie", "h\u00fcll\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gold und Purpur uns ein, l\u00f6schten den \u00f6stlichen Strahl.", "tokens": ["Gold", "und", "Pur\u00b7pur", "uns", "ein", ",", "l\u00f6schten", "den", "\u00f6st\u00b7li\u00b7chen", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Lange schwebte die Wolke, sie sank und jegliches Gr\u00e4schen", "tokens": ["Lan\u00b7ge", "schweb\u00b7te", "die", "Wol\u00b7ke", ",", "sie", "sank", "und", "jeg\u00b7li\u00b7ches", "Gr\u00e4sc\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Neigte sich, von der Last weinender Tropfen beschwert.", "tokens": ["Neig\u00b7te", "sich", ",", "von", "der", "Last", "wei\u00b7nen\u00b7der", "Trop\u00b7fen", "be\u00b7schwert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Da erschien die Sonne, da bl\u00fchte mit leuchtenden Sternen", "tokens": ["Da", "er\u00b7schien", "die", "Son\u00b7ne", ",", "da", "bl\u00fch\u00b7te", "mit", "leuch\u00b7ten\u00b7den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Halm und Blum' und Geb\u00fcsch, jegliches Gr\u00e4schen ein Stern!", "tokens": ["Halm", "und", "Blum'", "und", "Ge\u00b7b\u00fcsch", ",", "jeg\u00b7li\u00b7ches", "Gr\u00e4sc\u00b7hen", "ein", "Stern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Freundinn, so enth\u00fcllen sich uns die Nebel, es wandelt", "tokens": ["Freun\u00b7dinn", ",", "so", "ent\u00b7h\u00fcl\u00b7len", "sich", "uns", "die", "Ne\u00b7bel", ",", "es", "wan\u00b7delt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "So sich in Perlen die Saat unserer Thr\u00e4nen dereinst.", "tokens": ["So", "sich", "in", "Per\u00b7len", "die", "Saat", "un\u00b7se\u00b7rer", "Thr\u00e4\u00b7nen", "de\u00b7reinst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "APPR", "NN", "ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Viele weintest du, Edle, der bittersten Thr\u00e4nen, wie strahlend", "tokens": ["Vie\u00b7le", "wein\u00b7test", "du", ",", "Ed\u00b7le", ",", "der", "bit\u00b7ters\u00b7ten", "Thr\u00e4\u00b7nen", ",", "wie", "strah\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ADJA", "$,", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.12": {"text": "Flammet, o Siegerinn, einst deiner Belohnungen Kranz!", "tokens": ["Flam\u00b7met", ",", "o", "Sie\u00b7ge\u00b7rinn", ",", "einst", "dei\u00b7ner", "Be\u00b7loh\u00b7nun\u00b7gen", "Kranz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "ADV", "PPOS