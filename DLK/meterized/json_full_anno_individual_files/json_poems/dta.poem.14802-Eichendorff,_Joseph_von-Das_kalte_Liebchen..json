{"dta.poem.14802": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kalte Liebchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Fieberschauer mich durchbeben.", "tokens": ["Er", ".", "Fie\u00b7ber\u00b7schau\u00b7er", "mich", "durch\u00b7be\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie. Wahnsinn bringt der Todten Ku\u00df. \u2014", "tokens": ["Sie", ".", "Wahn\u00b7sinn", "bringt", "der", "Tod\u00b7ten", "Ku\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "$.", "NN", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er. Weh! es bricht mein junges Leben!", "tokens": ["Er", ".", "Weh", "!", "es", "bricht", "mein", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie. Mit ins Grab hinunter mu\u00df.", "tokens": ["Sie", ".", "Mit", "ins", "Grab", "hin\u00b7un\u00b7ter", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "APPR", "APPRART", "NN", "APPO", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}