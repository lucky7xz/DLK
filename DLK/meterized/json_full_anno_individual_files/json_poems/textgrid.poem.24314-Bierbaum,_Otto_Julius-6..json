{"textgrid.poem.24314": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00f6wenmaske aus schwarzem Granit,", "tokens": ["Die", "L\u00f6\u00b7wen\u00b7mas\u00b7ke", "aus", "schwar\u00b7zem", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die du mir heute geschenkt hast,", "tokens": ["Die", "du", "mir", "heu\u00b7te", "ge\u00b7schenkt", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mi\u00dfbrauch ich zum Tintenfa\u00df.", "tokens": ["Mi\u00df\u00b7brauch", "ich", "zum", "Tin\u00b7ten\u00b7fa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ehemals spie sie,", "tokens": ["E\u00b7he\u00b7mals", "spie", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Es sind gewi\u00df vier Jahrhunderte her", "tokens": ["Es", "sind", "ge\u00b7wi\u00df", "vier", "Jahr\u00b7hun\u00b7der\u00b7te", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "(weshalb Marzocco nicht mehr ganz komplett ist),", "tokens": ["(", "we\u00b7shalb", "Mar\u00b7zoc\u00b7co", "nicht", "mehr", "ganz", "kom\u00b7plett", "ist", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "NE", "PTKNEG", "ADV", "ADV", "ADJD", "VAFIN", "$(", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Aus diesem rundge\u00f6ffneten Maule", "tokens": ["Aus", "die\u00b7sem", "rund\u00b7ge\u00f6ff\u00b7ne\u00b7ten", "Mau\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wasser, wer wei\u00df wo, in ein wei\u00dfes Becken.", "tokens": ["Was\u00b7ser", ",", "wer", "wei\u00df", "wo", ",", "in", "ein", "wei\u00b7\u00dfes", "Be\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Das stand gewi\u00df in einem sch\u00f6nen Garten,", "tokens": ["Das", "stand", "ge\u00b7wi\u00df", "in", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und manchmal kam Madonna Gemma", "tokens": ["Und", "manch\u00b7mal", "kam", "Ma\u00b7don\u00b7na", "Gem\u00b7ma"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "NE", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und hielt die wei\u00dfen, hei\u00dfen H\u00e4nde unter", "tokens": ["Und", "hielt", "die", "wei\u00b7\u00dfen", ",", "hei\u00b7\u00dfen", "H\u00e4n\u00b7de", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "A