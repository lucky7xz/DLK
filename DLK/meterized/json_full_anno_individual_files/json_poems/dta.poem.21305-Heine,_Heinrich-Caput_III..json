{"dta.poem.21305": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das mahnt an das Mittelalter so sch\u00f6n,", "tokens": ["Das", "mahnt", "an", "das", "Mit\u00b7tel\u00b7al\u00b7ter", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An Edelknechte und Knappen,", "tokens": ["An", "E\u00b7del\u00b7knech\u00b7te", "und", "Knap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die in dem Herzen getragen die Treu", "tokens": ["Die", "in", "dem", "Her\u00b7zen", "ge\u00b7tra\u00b7gen", "die", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "ART", "NN"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und auf dem Hintern ein Wappen.", "tokens": ["Und", "auf", "dem", "Hin\u00b7tern", "ein", "Wap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Das mahnt an Kreuzzug und Turney,", "tokens": ["Das", "mahnt", "an", "Kreuz\u00b7zug", "und", "Tur\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An Minne und frommes Dienen,", "tokens": ["An", "Min\u00b7ne", "und", "from\u00b7mes", "Die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "An die ungedruckte Glaubenszeit,", "tokens": ["An", "die", "un\u00b7ge\u00b7druck\u00b7te", "Glau\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo noch keine Zeitung erschienen.", "tokens": ["Wo", "noch", "kei\u00b7ne", "Zei\u00b7tung", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, ja, der Helm gef\u00e4llt mir, er zeugt", "tokens": ["Ja", ",", "ja", ",", "der", "Helm", "ge\u00b7f\u00e4llt", "mir", ",", "er", "zeugt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Vom allerh\u00f6chsten Witze!", "tokens": ["Vom", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Wit\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein k\u00f6niglicher Einfall war\u2019s!", "tokens": ["Ein", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Ein\u00b7fall", "wa\u00b7r's", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es fehlt nicht die Pointe, die Spitze!", "tokens": ["Es", "fehlt", "nicht", "die", "Poin\u00b7te", ",", "die", "Spit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Nur f\u00fcrcht\u2019 ich, wenn ein Gewitter entsteht,", "tokens": ["Nur", "f\u00fcrcht'", "ich", ",", "wenn", "ein", "Ge\u00b7wit\u00b7ter", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Zieht leicht so eine Spitze", "tokens": ["Zieht", "leicht", "so", "ei\u00b7ne", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Herab auf Euer romantisches Haupt", "tokens": ["Her\u00b7ab", "auf", "Eu\u00b7er", "ro\u00b7man\u00b7ti\u00b7sches", "Haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Himmels modernste Blitze! \u2013 \u2013", "tokens": ["Des", "Him\u00b7mels", "mo\u00b7derns\u00b7te", "Blit\u00b7ze", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}