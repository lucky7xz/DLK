{"dta.poem.12998": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Florettens antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leanders kleiner brief ist mir zu h\u00e4nden kommen,", "tokens": ["Le\u00b7an\u00b7ders", "klei\u00b7ner", "brief", "ist", "mir", "zu", "h\u00e4n\u00b7den", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leanders, welcher stets was nettes dichten kan.", "tokens": ["Le\u00b7an\u00b7ders", ",", "wel\u00b7cher", "stets", "was", "net\u00b7tes", "dich\u00b7ten", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "PWS", "ADJA", "ADJA", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn weil ein kluger geist bey dir den sitz genommen,", "tokens": ["Denn", "weil", "ein", "klu\u00b7ger", "geist", "bey", "dir", "den", "sitz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zeigt dein kleiner brief von dir was gro\u00dfes an.", "tokens": ["So", "zeigt", "dein", "klei\u00b7ner", "brief", "von", "dir", "was", "gro\u00b7\u00dfes", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "APPR", "PPER", "PIS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch was schertzest du vom grab und letzten z\u00fcgen?", "tokens": ["Je\u00b7doch", "was", "schert\u00b7zest", "du", "vom", "grab", "und", "letz\u00b7ten", "z\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "APPRART", "NN", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was h\u00e4lt\u2019st du mich vor dir, so ich niemalen bin?", "tokens": ["Was", "h\u00e4lt'st", "du", "mich", "vor", "dir", ",", "so", "ich", "nie\u00b7ma\u00b7len", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "ADV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sollt ich die \u00e4rtztin seyn, so m\u00fcstest du erliegen,", "tokens": ["Sollt", "ich", "die", "\u00e4rtz\u00b7tin", "seyn", ",", "so", "m\u00fcs\u00b7test", "du", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "VAINF", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn meiner verse krafft sinckt vor den deinen hin.", "tokens": ["Denn", "mei\u00b7ner", "ver\u00b7se", "krafft", "sinckt", "vor", "den", "dei\u00b7nen", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wilst du ja hinfort noch an Floretten schreiben,", "tokens": ["Und", "wilst", "du", "ja", "hin\u00b7fort", "noch", "an", "Flo\u00b7ret\u00b7ten", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So sinne selbst was aus, wovon der inhalt sey.", "tokens": ["So", "sin\u00b7ne", "selbst", "was", "aus", ",", "wo\u00b7von", "der", "in\u00b7halt", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PWS", "PTKVZ", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mir will von meiner hand niemals ein brief bekleiben,", "tokens": ["Mir", "will", "von", "mei\u00b7ner", "hand", "nie\u00b7mals", "ein", "brief", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Es trage denn zuvor dein kiel die w\u00fcrckung bey.", "tokens": ["Es", "tra\u00b7ge", "denn", "zu\u00b7vor", "dein", "kiel", "die", "w\u00fcr\u00b7ckung", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}