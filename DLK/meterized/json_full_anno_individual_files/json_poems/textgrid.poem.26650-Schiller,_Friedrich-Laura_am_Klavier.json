{"textgrid.poem.26650": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Laura am Klavier", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dein Finger durch die Saiten meistert \u2013", "tokens": ["Wenn", "dein", "Fin\u00b7ger", "durch", "die", "Sai\u00b7ten", "meis\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Laura, itzt zur Statue entgeistert,", "tokens": ["Lau\u00b7ra", ",", "itzt", "zur", "Sta\u00b7tue", "ent\u00b7geis\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Itzt entk\u00f6rpert steh ich da.", "tokens": ["Itzt", "ent\u00b7k\u00f6r\u00b7pert", "steh", "ich", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du gebietest \u00fcber Tod und Leben,", "tokens": ["Du", "ge\u00b7bie\u00b7test", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "M\u00e4chtig wie von tausend Nervgeweben", "tokens": ["M\u00e4ch\u00b7tig", "wie", "von", "tau\u00b7send", "Nerv\u00b7ge\u00b7we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seelen fordert Philadelphia; \u2013", "tokens": ["See\u00b7len", "for\u00b7dert", "Phi\u00b7la\u00b7del\u00b7phia", ";", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "NE", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ehrerbietig leiser rauschen", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "lei\u00b7ser", "rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann die L\u00fcfte, dir zu lauschen;", "tokens": ["Dann", "die", "L\u00fcf\u00b7te", ",", "dir", "zu", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hingeschmiedet zum Gesang,", "tokens": ["Hin\u00b7ge\u00b7schmie\u00b7det", "zum", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehn im ewgen Wirbelgang,", "tokens": ["Stehn", "im", "ew\u00b7gen", "Wir\u00b7bel\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einzuziehn die Wonnef\u00fclle,", "tokens": ["Ein\u00b7zu\u00b7ziehn", "die", "Won\u00b7ne\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lauschende Naturen stille,", "tokens": ["Lau\u00b7schen\u00b7de", "Na\u00b7tu\u00b7ren", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zauberin! mit T\u00f6nen, wie", "tokens": ["Zau\u00b7be\u00b7rin", "!", "mit", "T\u00f6\u00b7nen", ",", "wie"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$.", "APPR", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich mit Blicken, zwingst du sie.", "tokens": ["Mich", "mit", "Bli\u00b7cken", ",", "zwingst", "du", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seelenvolle Harmonien wimmeln,", "tokens": ["See\u00b7len\u00b7vol\u00b7le", "Har\u00b7mo\u00b7ni\u00b7en", "wim\u00b7meln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein woll\u00fcstig Ungest\u00fcm,", "tokens": ["Ein", "wol\u00b7l\u00fcs\u00b7tig", "Un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Saiten, wie aus ihren Himmeln", "tokens": ["Aus", "den", "Sai\u00b7ten", ",", "wie", "aus", "ih\u00b7ren", "Him\u00b7meln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Neugeborne Seraphim;", "tokens": ["Neu\u00b7ge\u00b7bor\u00b7ne", "Se\u00b7ra\u00b7phim", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie, des Chaos Riesenarm entronnen,", "tokens": ["Wie", ",", "des", "Chaos", "Rie\u00b7se\u00b7narm", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aufgejagt vom Sch\u00f6pfungssturm die Sonnen", "tokens": ["Auf\u00b7ge\u00b7jagt", "vom", "Sch\u00f6p\u00b7fungs\u00b7sturm", "die", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Funkend fuhren aus der Finsternus,", "tokens": ["Fun\u00b7kend", "fuh\u00b7ren", "aus", "der", "Fins\u00b7ter\u00b7nus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Str\u00f6mt der goldne Saitengu\u00df.", "tokens": ["Str\u00f6mt", "der", "gold\u00b7ne", "Sai\u00b7ten\u00b7gu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieblich itzt wie \u00fcber bunten Kieseln", "tokens": ["Lieb\u00b7lich", "itzt", "wie", "\u00fc\u00b7ber", "bun\u00b7ten", "Kie\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Silberhelle Fluten rieseln, \u2013", "tokens": ["Sil\u00b7ber\u00b7hel\u00b7le", "Flu\u00b7ten", "rie\u00b7seln", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Majest\u00e4tisch pr\u00e4chtig nun", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "pr\u00e4ch\u00b7tig", "nun"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie des Donners Orgelton,", "tokens": ["Wie", "des", "Don\u00b7ners", "Or\u00b7gel\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrmend von hinnen itzt, wie sich von Felsen", "tokens": ["St\u00fcr\u00b7mend", "von", "hin\u00b7nen", "itzt", ",", "wie", "sich", "von", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADV", "ADV", "$,", "PWAV", "PRF", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Rauschende, sch\u00e4umende Gie\u00dfb\u00e4che w\u00e4lzen,", "tokens": ["Rau\u00b7schen\u00b7de", ",", "sch\u00e4u\u00b7men\u00b7de", "Gie\u00df\u00b7b\u00e4\u00b7che", "w\u00e4l\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "Holdes Ges\u00e4usel bald,", "tokens": ["Hol\u00b7des", "Ge\u00b7s\u00e4u\u00b7sel", "bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Schmeichlerisch linde,", "tokens": ["Schmeich\u00b7le\u00b7risch", "lin\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wie durch den Espenwald", "tokens": ["Wie", "durch", "den", "Es\u00b7pen\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Buhlende Winde,", "tokens": ["Buh\u00b7len\u00b7de", "Win\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Schwerer nun und melancholisch d\u00fcster,", "tokens": ["Schwe\u00b7rer", "nun", "und", "me\u00b7lan\u00b7cho\u00b7lisch", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Wie durch toter W\u00fcsten Schauernachtgefl\u00fcster,", "tokens": ["Wie", "durch", "to\u00b7ter", "W\u00fcs\u00b7ten", "Schau\u00b7er\u00b7nacht\u00b7ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Wo verlornes Heulen schweift,", "tokens": ["Wo", "ver\u00b7lor\u00b7nes", "Heu\u00b7len", "schweift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Tr\u00e4nenwellen der Cocytus schleift.", "tokens": ["Tr\u00e4\u00b7nen\u00b7wel\u00b7len", "der", "Co\u00b7cy\u00b7tus", "schleift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "M\u00e4dchen, sprich! Ich frage, gib mir Kunde:", "tokens": ["M\u00e4d\u00b7chen", ",", "sprich", "!", "Ich", "fra\u00b7ge", ",", "gib", "mir", "Kun\u00b7de", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$.", "PPER", "VVFIN", "$,", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehst mit h\u00f6hern Geistern du im Bunde?", "tokens": ["Stehst", "mit", "h\u00f6\u00b7hern", "Geis\u00b7tern", "du", "im", "Bun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ists die Sprache, l\u00fcg mir nicht,", "tokens": ["Ists", "die", "Spra\u00b7che", ",", "l\u00fcg", "mir", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man in Elysen spricht?", "tokens": ["Die", "man", "in", "E\u00b7ly\u00b7sen", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von dem Auge weg der Schleier!", "tokens": ["Von", "dem", "Au\u00b7ge", "weg", "der", "Schlei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starre Riegel von dem Ohr!", "tokens": ["Star\u00b7re", "Rie\u00b7gel", "von", "dem", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4dchen! Ha! schon atm ich freier,", "tokens": ["M\u00e4d\u00b7chen", "!", "Ha", "!", "schon", "atm", "ich", "frei\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ITJ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4utert mich \u00e4therisch Feuer?", "tokens": ["L\u00e4u\u00b7tert", "mich", "\u00e4t\u00b7he\u00b7risch", "Feu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Tragen Wirbel mich empor? \u2013 \u2013", "tokens": ["Tra\u00b7gen", "Wir\u00b7bel", "mich", "em\u00b7por", "?", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Neuer Geister Sonnensitze", "tokens": ["Neu\u00b7er", "Geis\u00b7ter", "Son\u00b7nen\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winken durch zerri\u00dfner Himmel Ritze \u2013", "tokens": ["Win\u00b7ken", "durch", "zer\u00b7ri\u00df\u00b7ner", "Him\u00b7mel", "Rit\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberm Grabe Morgenrot!", "tokens": ["\u00dc\u00b7berm", "Gra\u00b7be", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weg, ihr Sp\u00f6tter, mit Insektenwitze!", "tokens": ["Weg", ",", "ihr", "Sp\u00f6t\u00b7ter", ",", "mit", "In\u00b7sek\u00b7ten\u00b7wit\u00b7ze", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Weg! Es ist ein Gott \u2013 \u2013 \u2013 \u2013", "tokens": ["Weg", "!", "Es", "ist", "ein", "Gott", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "$(", "$(", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanz