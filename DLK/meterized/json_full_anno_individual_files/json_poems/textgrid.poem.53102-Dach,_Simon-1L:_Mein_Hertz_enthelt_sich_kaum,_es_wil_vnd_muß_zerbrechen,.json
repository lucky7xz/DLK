ord", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.24": {"line.1": {"text": "Trawr' ich wo, oder Sie, so zeige deine Wangen", "tokens": ["Tra\u00b7wr'", "ich", "wo", ",", "o\u00b7der", "Sie", ",", "so", "zei\u00b7ge", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWAV", "$,", "KON", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Erblasst, als werest du auch neben vns in Noht:", "tokens": ["Er\u00b7blasst", ",", "als", "we\u00b7rest", "du", "auch", "ne\u00b7ben", "vns", "in", "Noht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Steht's wol vmb Sie vnd mich, so solt, du g\u00fcldne, prangen", "tokens": ["Steht's", "wol", "vmb", "Sie", "vnd", "mich", ",", "so", "solt", ",", "du", "g\u00fcld\u00b7ne", ",", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "APPR", "PPER", "KON", "PPER", "$,", "ADV", "VMFIN", "$,", "PPER", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit deinem besten Glantz gemahlet Rosen-roht.", "tokens": ["Mit", "dei\u00b7nem", "bes\u00b7ten", "Glantz", "ge\u00b7mah\u00b7let", "Ro\u00b7sen\u00b7roht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.25": {"line.1": {"text": "Vnd wo mir je mein Lieb wil etwas sagen lassen,", "tokens": ["Vnd", "wo", "mir", "je", "mein", "Lieb", "wil", "et\u00b7was", "sa\u00b7gen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schick dein Liebes-Volck f\u00fcr jhren zarten Mund,", "tokens": ["So", "schick", "dein", "Lie\u00b7bes\u00b7Volck", "f\u00fcr", "jhren", "zar\u00b7ten", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die meiner Liebsten Red' in jhre K\u00f6cher fassen,", "tokens": ["Die", "mei\u00b7ner", "Liebs\u00b7ten", "Red'", "in", "jhre", "K\u00f6\u00b7cher", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd thun sie nachmals mir vom hohen Himmel kunt.", "tokens": ["Vnd", "thun", "sie", "nach\u00b7mals", "mir", "vom", "ho\u00b7hen", "Him\u00b7mel", "kunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.26": {"line.1": {"text": "Mu\u00df gleich das wilde Meer vns von einander trennen,", "tokens": ["Mu\u00df", "gleich", "das", "wil\u00b7de", "Meer", "vns", "von", "ein\u00b7an\u00b7der", "tren\u00b7