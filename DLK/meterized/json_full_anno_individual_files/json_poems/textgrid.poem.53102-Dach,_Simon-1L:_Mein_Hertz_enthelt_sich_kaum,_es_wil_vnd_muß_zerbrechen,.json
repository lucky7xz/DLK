{"textgrid.poem.53102": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Hertz enthelt sich kaum, es wil vnd mu\u00df zerbrechen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertz enthelt sich kaum, es wil vnd mu\u00df zerbrechen,", "tokens": ["Mein", "Hertz", "en\u00b7thelt", "sich", "kaum", ",", "es", "wil", "vnd", "mu\u00df", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "$,", "PPER", "VMFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Geist geht in der jrr' Vnd kennt sich selbst nicht wol,", "tokens": ["Mein", "Geist", "geht", "in", "der", "jrr'", "Vnd", "kennt", "sich", "selbst", "nicht", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich nicht wei\u00df, mein Lieb, wenn ich euch werde sprechen,", "tokens": ["Weil", "ich", "nicht", "wei\u00df", ",", "mein", "Lieb", ",", "wenn", "ich", "euch", "wer\u00b7de", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem ich jetzt so weit von hinnen ziehen sol.", "tokens": ["In\u00b7dem", "ich", "jetzt", "so", "weit", "von", "hin\u00b7nen", "zie\u00b7hen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Winde, kehret vmb vnd stellt euch mir zuwider,", "tokens": ["Ihr", "Win\u00b7de", ",", "keh\u00b7ret", "vmb", "vnd", "stellt", "euch", "mir", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "KON", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df da\u00df ich Sie, gleich wie Sie mich gesegnet hat!", "tokens": ["Bi\u00df", "da\u00df", "ich", "Sie", ",", "gleich", "wie", "Sie", "mich", "ge\u00b7seg\u00b7net", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "$,", "ADV", "KOKOM", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihr Segel haltet an, legt ewren Hochmut nieder!", "tokens": ["Ihr", "Se\u00b7gel", "hal\u00b7tet", "an", ",", "legt", "ew\u00b7ren", "Hoch\u00b7mut", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir letzen vns noch erst, vnd weinen vns recht satt.", "tokens": ["Wir", "let\u00b7zen", "vns", "noch", "erst", ",", "vnd", "wei\u00b7nen", "vns", "recht", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lasst ab, mein' Argine, vnd schonet ewrer Thr\u00e4nen,", "tokens": ["Lasst", "ab", ",", "mein'", "Ar\u00b7gi\u00b7ne", ",", "vnd", "scho\u00b7net", "ew\u00b7rer", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was schw\u00e4cht Ihr ewr Gesicht, ich mu\u00df doch endlich fort!", "tokens": ["Was", "schw\u00e4cht", "Ihr", "ewr", "Ge\u00b7sicht", ",", "ich", "mu\u00df", "doch", "end\u00b7lich", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr jhr weint, je mehr werd' ich mich nach euch sehnen.", "tokens": ["Je", "mehr", "jhr", "weint", ",", "je", "mehr", "werd'", "ich", "mich", "nach", "euch", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vnd irren ohne Trost dort vmb den frembden Port.", "tokens": ["Vnd", "ir\u00b7ren", "oh\u00b7ne", "Trost", "dort", "vmb", "den", "fremb\u00b7den", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich wil in meine Seel' ein kleines Hau\u00df Euch bawen,", "tokens": ["Ich", "wil", "in", "mei\u00b7ne", "Seel'", "ein", "klei\u00b7nes", "Hau\u00df", "Euch", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "In welches Ewre stets sol eingeschlossen seyn,", "tokens": ["In", "wel\u00b7ches", "Ew\u00b7re", "stets", "sol", "ein\u00b7ge\u00b7schlos\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd wil hergegen Euch auch meine Seele trawen,", "tokens": ["Vnd", "wil", "her\u00b7ge\u00b7gen", "Euch", "auch", "mei\u00b7ne", "See\u00b7le", "tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die hebt Euch auff vnd schliesst sie ewrer Seelen ein.", "tokens": ["Die", "hebt", "Euch", "auff", "vnd", "schliesst", "sie", "ew\u00b7rer", "See\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Kein Thr\u00e4nlein fleusset jetzt von ewren bleichen Wangen,", "tokens": ["Kein", "Thr\u00e4n\u00b7lein", "fleus\u00b7set", "jetzt", "von", "ew\u00b7ren", "blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd mu\u00df kein Seufftzer auch au\u00df Ewrem Hertzen gehn,", "tokens": ["Vnd", "mu\u00df", "kein", "Seufft\u00b7zer", "auch", "au\u00df", "Ew\u00b7rem", "Hert\u00b7zen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe sie mit flei\u00df zur Beylag' auffgefangen,", "tokens": ["Ich", "ha\u00b7be", "sie", "mit", "flei\u00df", "zur", "Beylag'", "auff\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd lasse meine Seel hiemit gef\u00fcllet stehn.", "tokens": ["Vnd", "las\u00b7se", "mei\u00b7ne", "Seel", "hie\u00b7mit", "ge\u00b7f\u00fcl\u00b7let", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PAV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die sollen mit mir ziehn durch Wetter, Wind vnd Wellen,", "tokens": ["Die", "sol\u00b7len", "mit", "mir", "ziehn", "durch", "Wet\u00b7ter", ",", "Wind", "vnd", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nehme sie f\u00fcr Euch zu meiner Liebsten an,", "tokens": ["Ich", "neh\u00b7me", "sie", "f\u00fcr", "Euch", "zu", "mei\u00b7ner", "Liebs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff da\u00df sie ewer Bild mir stets vor Augen stellen,", "tokens": ["Auff", "da\u00df", "sie", "e\u00b7wer", "Bild", "mir", "stets", "vor", "Au\u00b7gen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd tragen, was ich selbst nicht mit mir nehmen kan.", "tokens": ["Vnd", "tra\u00b7gen", ",", "was", "ich", "selbst", "nicht", "mit", "mir", "neh\u00b7men", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit jhnen wil ich mich besprechen vnd ergetzen,", "tokens": ["Mit", "jh\u00b7nen", "wil", "ich", "mich", "be\u00b7spre\u00b7chen", "vnd", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sollen seyn mein Trost in Noht vnd Trawrigkeit,", "tokens": ["Sie", "sol\u00b7len", "seyn", "mein", "Trost", "in", "Noht", "vnd", "Traw\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Gl\u00fcck, kein b\u00f6ser Fall sol mir die\u00df Volck verletzen,", "tokens": ["Kein", "Gl\u00fcck", ",", "kein", "b\u00f6\u00b7ser", "Fall", "sol", "mir", "die\u00df", "Volck", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Sturm vnd wilde Fluth, auch keiner Winde Streit.", "tokens": ["Kein", "Sturm", "vnd", "wil\u00b7de", "Fluth", ",", "auch", "kei\u00b7ner", "Win\u00b7de", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$,", "ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Kein frembdes Weib sol sie durch jhre Gunst vertreiben,", "tokens": ["Kein", "fremb\u00b7des", "Weib", "sol", "sie", "durch", "jhre", "Gunst", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sollen (hilfft mir Gott gesund hie zu euch her)", "tokens": ["Sie", "sol\u00b7len", "(", "hilfft", "mir", "Gott", "ge\u00b7sund", "hie", "zu", "euch", "her", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VVFIN", "PPER", "NN", "ADJD", "ADV", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bezeugen meine Trew vnd mein Best\u00e4ndig-bleiben,", "tokens": ["Be\u00b7zeu\u00b7gen", "mei\u00b7ne", "Trew", "vnd", "mein", "Be\u00b7st\u00e4n\u00b7dig\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd sagen, wie ich nie ein ander Lieb begehr.", "tokens": ["Vnd", "sa\u00b7gen", ",", "wie", "ich", "nie", "ein", "an\u00b7der", "Lieb", "be\u00b7gehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihr werdet selbst alsdann es an mir k\u00f6nnen schliessen,", "tokens": ["Ihr", "wer\u00b7det", "selbst", "als\u00b7dann", "es", "an", "mir", "k\u00f6n\u00b7nen", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dieses ewer Pfand durch meiner Augen Bach", "tokens": ["Wenn", "die\u00b7ses", "e\u00b7wer", "Pfand", "durch", "mei\u00b7ner", "Au\u00b7gen", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Au\u00df Lieb' vnd Fr\u00f6licheit euch wird entgegen fliessen,", "tokens": ["Au\u00df", "Lieb'", "vnd", "Fr\u00f6\u00b7lic\u00b7heit", "euch", "wird", "ent\u00b7ge\u00b7gen", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VAFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd r\u00fchmen meinen Sinn, mein Lieben vor vnd nach.", "tokens": ["Vnd", "r\u00fch\u00b7men", "mei\u00b7nen", "Sinn", ",", "mein", "Lie\u00b7ben", "vor", "vnd", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mit dem Bedinge nun geh' ich von euch zu scheiden,", "tokens": ["Mit", "dem", "Be\u00b7din\u00b7ge", "nun", "geh'", "ich", "von", "euch", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du, Venus, die du vns zusammen hast gef\u00fchrt,", "tokens": ["Du", ",", "Ve\u00b7nus", ",", "die", "du", "vns", "zu\u00b7sam\u00b7men", "hast", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Komm abentlich zu stewr mit deinem Licht vns beyden!", "tokens": ["Komm", "a\u00b7bent\u00b7lich", "zu", "stewr", "mit", "dei\u00b7nem", "Licht", "vns", "bey\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was mich vnd Sie betrifft, werd auch an dir gesp\u00fchrt!", "tokens": ["Was", "mich", "vnd", "Sie", "be\u00b7tr\u00b7ifft", ",", "werd", "auch", "an", "dir", "ge\u00b7sp\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Trawr' ich wo, oder Sie, so zeige deine Wangen", "tokens": ["Tra\u00b7wr'", "ich", "wo", ",", "o\u00b7der", "Sie", ",", "so", "zei\u00b7ge", "dei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWAV", "$,", "KON", "PPER", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Erblasst, als werest du auch neben vns in Noht:", "tokens": ["Er\u00b7blasst", ",", "als", "we\u00b7rest", "du", "auch", "ne\u00b7ben", "vns", "in", "Noht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Steht's wol vmb Sie vnd mich, so solt, du g\u00fcldne, prangen", "tokens": ["Steht's", "wol", "vmb", "Sie", "vnd", "mich", ",", "so", "solt", ",", "du", "g\u00fcld\u00b7ne", ",", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "APPR", "PPER", "KON", "PPER", "$,", "ADV", "VMFIN", "$,", "PPER", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit deinem besten Glantz gemahlet Rosen-roht.", "tokens": ["Mit", "dei\u00b7nem", "bes\u00b7ten", "Glantz", "ge\u00b7mah\u00b7let", "Ro\u00b7sen\u00b7roht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Vnd wo mir je mein Lieb wil etwas sagen lassen,", "tokens": ["Vnd", "wo", "mir", "je", "mein", "Lieb", "wil", "et\u00b7was", "sa\u00b7gen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VMFIN", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schick dein Liebes-Volck f\u00fcr jhren zarten Mund,", "tokens": ["So", "schick", "dein", "Lie\u00b7bes\u00b7Volck", "f\u00fcr", "jhren", "zar\u00b7ten", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die meiner Liebsten Red' in jhre K\u00f6cher fassen,", "tokens": ["Die", "mei\u00b7ner", "Liebs\u00b7ten", "Red'", "in", "jhre", "K\u00f6\u00b7cher", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd thun sie nachmals mir vom hohen Himmel kunt.", "tokens": ["Vnd", "thun", "sie", "nach\u00b7mals", "mir", "vom", "ho\u00b7hen", "Him\u00b7mel", "kunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Mu\u00df gleich das wilde Meer vns von einander trennen,", "tokens": ["Mu\u00df", "gleich", "das", "wil\u00b7de", "Meer", "vns", "von", "ein\u00b7an\u00b7der", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "PPER", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wollen wir durch dich dennoch beysammen seyn,", "tokens": ["So", "wol\u00b7len", "wir", "durch", "dich", "den\u00b7noch", "bey\u00b7sam\u00b7men", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd vnser beyder Thun vnd Leben stets erkennen,", "tokens": ["Vnd", "vn\u00b7ser", "bey\u00b7der", "Thun", "vnd", "Le\u00b7ben", "stets", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vns frewen in dem Gl\u00fcck vnd tr\u00f6sten in der Pein.", "tokens": ["Vns", "fre\u00b7wen", "in", "dem", "Gl\u00fcck", "vnd", "tr\u00f6s\u00b7ten", "in", "der", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Mein Hertz enthelt sich kaum, es wil vnd mu\u00df zerbrechen,", "tokens": ["Mein", "Hertz", "en\u00b7thelt", "sich", "kaum", ",", "es", "wil", "vnd", "mu\u00df", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ADV", "$,", "PPER", "VMFIN", "KON", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Geist geht in der jrr' Vnd kennt sich selbst nicht wol,", "tokens": ["Mein", "Geist", "geht", "in", "der", "jrr'", "Vnd", "kennt", "sich", "selbst", "nicht", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich nicht wei\u00df, mein Lieb, wenn ich euch werde sprechen,", "tokens": ["Weil", "ich", "nicht", "wei\u00df", ",", "mein", "Lieb", ",", "wenn", "ich", "euch", "wer\u00b7de", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem ich jetzt so weit von hinnen ziehen sol.", "tokens": ["In\u00b7dem", "ich", "jetzt", "so", "weit", "von", "hin\u00b7nen", "zie\u00b7hen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Ihr Winde, kehret vmb vnd stellt euch mir zuwider,", "tokens": ["Ihr", "Win\u00b7de", ",", "keh\u00b7ret", "vmb", "vnd", "stellt", "euch", "mir", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "KON", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bi\u00df da\u00df ich Sie, gleich wie Sie mich gesegnet hat!", "tokens": ["Bi\u00df", "da\u00df", "ich", "Sie", ",", "gleich", "wie", "Sie", "mich", "ge\u00b7seg\u00b7net", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "$,", "ADV", "KOKOM", "PPER", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ihr Segel haltet an, legt ewren Hochmut nieder!", "tokens": ["Ihr", "Se\u00b7gel", "hal\u00b7tet", "an", ",", "legt", "ew\u00b7ren", "Hoch\u00b7mut", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir letzen vns noch erst, vnd weinen vns recht satt.", "tokens": ["Wir", "let\u00b7zen", "vns", "noch", "erst", ",", "vnd", "wei\u00b7nen", "vns", "recht", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Lasst ab, mein' Argine, vnd schonet ewrer Thr\u00e4nen,", "tokens": ["Lasst", "ab", ",", "mein'", "Ar\u00b7gi\u00b7ne", ",", "vnd", "scho\u00b7net", "ew\u00b7rer", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was schw\u00e4cht Ihr ewr Gesicht, ich mu\u00df doch endlich fort!", "tokens": ["Was", "schw\u00e4cht", "Ihr", "ewr", "Ge\u00b7sicht", ",", "ich", "mu\u00df", "doch", "end\u00b7lich", "fort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr jhr weint, je mehr werd' ich mich nach euch sehnen.", "tokens": ["Je", "mehr", "jhr", "weint", ",", "je", "mehr", "werd'", "ich", "mich", "nach", "euch", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vnd irren ohne Trost dort vmb den frembden Port.", "tokens": ["Vnd", "ir\u00b7ren", "oh\u00b7ne", "Trost", "dort", "vmb", "den", "fremb\u00b7den", "Port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Ich wil in meine Seel' ein kleines Hau\u00df Euch bawen,", "tokens": ["Ich", "wil", "in", "mei\u00b7ne", "Seel'", "ein", "klei\u00b7nes", "Hau\u00df", "Euch", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "In welches Ewre stets sol eingeschlossen seyn,", "tokens": ["In", "wel\u00b7ches", "Ew\u00b7re", "stets", "sol", "ein\u00b7ge\u00b7schlos\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd wil hergegen Euch auch meine Seele trawen,", "tokens": ["Vnd", "wil", "her\u00b7ge\u00b7gen", "Euch", "auch", "mei\u00b7ne", "See\u00b7le", "tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die hebt Euch auff vnd schliesst sie ewrer Seelen ein.", "tokens": ["Die", "hebt", "Euch", "auff", "vnd", "schliesst", "sie", "ew\u00b7rer", "See\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Kein Thr\u00e4nlein fleusset jetzt von ewren bleichen Wangen,", "tokens": ["Kein", "Thr\u00e4n\u00b7lein", "fleus\u00b7set", "jetzt", "von", "ew\u00b7ren", "blei\u00b7chen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd mu\u00df kein Seufftzer auch au\u00df Ewrem Hertzen gehn,", "tokens": ["Vnd", "mu\u00df", "kein", "Seufft\u00b7zer", "auch", "au\u00df", "Ew\u00b7rem", "Hert\u00b7zen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe sie mit flei\u00df zur Beylag' auffgefangen,", "tokens": ["Ich", "ha\u00b7be", "sie", "mit", "flei\u00df", "zur", "Beylag'", "auff\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vnd lasse meine Seel hiemit gef\u00fcllet stehn.", "tokens": ["Vnd", "las\u00b7se", "mei\u00b7ne", "Seel", "hie\u00b7mit", "ge\u00b7f\u00fcl\u00b7let", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PAV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Die sollen mit mir ziehn durch Wetter, Wind vnd Wellen,", "tokens": ["Die", "sol\u00b7len", "mit", "mir", "ziehn", "durch", "Wet\u00b7ter", ",", "Wind", "vnd", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nehme sie f\u00fcr Euch zu meiner Liebsten an,", "tokens": ["Ich", "neh\u00b7me", "sie", "f\u00fcr", "Euch", "zu", "mei\u00b7ner", "Liebs\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff da\u00df sie ewer Bild mir stets vor Augen stellen,", "tokens": ["Auff", "da\u00df", "sie", "e\u00b7wer", "Bild", "mir", "stets", "vor", "Au\u00b7gen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd tragen, was ich selbst nicht mit mir nehmen kan.", "tokens": ["Vnd", "tra\u00b7gen", ",", "was", "ich", "selbst", "nicht", "mit", "mir", "neh\u00b7men", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Mit jhnen wil ich mich besprechen vnd ergetzen,", "tokens": ["Mit", "jh\u00b7nen", "wil", "ich", "mich", "be\u00b7spre\u00b7chen", "vnd", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sollen seyn mein Trost in Noht vnd Trawrigkeit,", "tokens": ["Sie", "sol\u00b7len", "seyn", "mein", "Trost", "in", "Noht", "vnd", "Traw\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VAINF", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Gl\u00fcck, kein b\u00f6ser Fall sol mir die\u00df Volck verletzen,", "tokens": ["Kein", "Gl\u00fcck", ",", "kein", "b\u00f6\u00b7ser", "Fall", "sol", "mir", "die\u00df", "Volck", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Sturm vnd wilde Fluth, auch keiner Winde Streit.", "tokens": ["Kein", "Sturm", "vnd", "wil\u00b7de", "Fluth", ",", "auch", "kei\u00b7ner", "Win\u00b7de", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "ADJA", "NN", "$,", "ADV", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "Kein frembdes Weib sol sie durch jhre Gunst vertreiben,", "tokens": ["Kein", "fremb\u00b7des", "Weib", "sol", "sie", "durch", "jhre", "Gunst", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie sollen (hilfft mir Gott gesund hie zu euch her)", "tokens": ["Sie", "sol\u00b7len", "(", "hilfft", "mir", "Gott", "ge\u00b7sund", "hie", "zu", "euch", "her", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "VVFIN", "PPER", "NN", "ADJD", "ADV", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bezeugen meine Trew vnd mein Best\u00e4ndig-bleiben,", "tokens": ["Be\u00b7zeu\u00b7gen", "mei\u00b7ne", "Trew", "vnd", "mein", "Be\u00b7st\u00e4n\u00b7dig\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd sagen, wie ich nie ein ander Lieb begehr.", "tokens": ["Vnd", "sa\u00b7gen", ",", "wie", "ich", "nie", "ein", "an\u00b7der", "Lieb", "be\u00b7gehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "PPER", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Ihr werdet selbst alsdann es an mir k\u00f6nnen schliessen,", "tokens": ["Ihr", "wer\u00b7det", "selbst", "als\u00b7dann", "es", "an", "mir", "k\u00f6n\u00b7nen", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dieses ewer Pfand durch meiner Augen Bach", "tokens": ["Wenn", "die\u00b7ses", "e\u00b7wer", "Pfand", "durch", "mei\u00b7ner", "Au\u00b7gen", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Au\u00df Lieb' vnd Fr\u00f6licheit euch wird entgegen fliessen,", "tokens": ["Au\u00df", "Lieb'", "vnd", "Fr\u00f6\u00b7lic\u00b7heit", "euch", "wird", "ent\u00b7ge\u00b7gen", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VAFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd r\u00fchmen meinen Sinn, mein Lieben