{"textgrid.poem.63646": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob in der argen Welt, wie gute Christen", "tokens": ["Ob", "in", "der", "ar\u00b7gen", "Welt", ",", "wie", "gu\u00b7te", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beteuern, alles sich zum Besten wende,", "tokens": ["Be\u00b7teu\u00b7ern", ",", "al\u00b7les", "sich", "zum", "Bes\u00b7ten", "wen\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob sie nur wert sei, da\u00df sie eilig ende,", "tokens": ["Ob", "sie", "nur", "wert", "sei", ",", "da\u00df", "sie", "ei\u00b7lig", "en\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach eurem Credo, werte Pessimisten,", "tokens": ["Nach", "eu\u00b7rem", "Cre\u00b7do", ",", "wer\u00b7te", "Pes\u00b7si\u00b7mis\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob zwischen dem Erfreulichen und Tristen", "tokens": ["Ob", "zwi\u00b7schen", "dem", "Er\u00b7freu\u00b7li\u00b7chen", "und", "Tris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "In goldner Mitte sich der Ausgleich f\u00e4nde:", "tokens": ["In", "gold\u00b7ner", "Mit\u00b7te", "sich", "der", "Aus\u00b7gleich", "f\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern sei's von mir, da\u00df ich mich unterst\u00e4nde", "tokens": ["Fern", "sei's", "von", "mir", ",", "da\u00df", "ich", "mich", "un\u00b7ter\u00b7st\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schiedsrichterlichen Spruchs bei solchen Zwisten.", "tokens": ["Schieds\u00b7rich\u00b7ter\u00b7li\u00b7chen", "Spruchs", "bei", "sol\u00b7chen", "Zwis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hab', indes ich wandelt' hier auf Erden,", "tokens": ["Ich", "hab'", ",", "in\u00b7des", "ich", "wan\u00b7delt'", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom S\u00fc\u00dfesten und Bittersten genossen", "tokens": ["Vom", "S\u00fc\u00b7\u00dfes\u00b7ten", "und", "Bit\u00b7ters\u00b7ten", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP"], "meter": "-+-+