{"dta.poem.20763": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als sie eine zum Scherz verfertigte Ordre  \n an den Kanzeleidirektor Brandhorst, und ein Exe-  \n cutoriale auf einem Tisch fand.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da Mandloff, der einmal die Kanzelei regiert,               ", "tokens": ["Da", "Mand\u00b7loff", ",", "der", "ein\u00b7mal", "die", "Kan\u00b7ze\u00b7lei", "re\u00b7giert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jetzt vom ganzen Kreis zu Potsdam Rechnung f\u00fchrt,", "tokens": ["Und", "jetzt", "vom", "gan\u00b7zen", "Kreis", "zu", "Pots\u00b7dam", "Rech\u00b7nung", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da dieser, wie es uns vom Kammersekretaire", "tokens": ["Da", "die\u00b7ser", ",", "wie", "es", "uns", "vom", "Kam\u00b7mer\u00b7se\u00b7kre\u00b7tai\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "$,", "PWAV", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "B*mann er\u00f6ffnet ward, so eigenn\u00fctzig w\u00e4re", "tokens": ["B", "*", "mann", "er\u00b7\u00f6ff\u00b7net", "ward", ",", "so", "ei\u00b7gen\u00b7n\u00fct\u00b7zig", "w\u00e4\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["XY", "XY", "NN", "VVPP", "VAFIN", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ein poetisch Buch, das man ihm zugestellt,", "tokens": ["Und", "ein", "po\u00b7e\u00b7tisch", "Buch", ",", "das", "man", "ihm", "zu\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schon seit geraumer Zeit unartig vorenth\u00e4lt,", "tokens": ["Schon", "seit", "ge\u00b7rau\u00b7mer", "Zeit", "un\u00b7ar\u00b7tig", "vor\u00b7ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So oft man ihn auch schon erinnert, und gesagt:", "tokens": ["So", "oft", "man", "ihn", "auch", "schon", "e\u00b7rin\u00b7nert", ",", "und", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "ADV", "ADV", "VVFIN", "$,", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df man es haben will \u2014 Als wird er hart verklagt,", "tokens": ["Da\u00df", "man", "es", "ha\u00b7ben", "will", "Als", "wird", "er", "hart", "ver\u00b7klagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAINF", "VMFIN", "$(", "KOUS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und nicht entschuldiget. Es wird zugleich befohlen", "tokens": ["Und", "nicht", "ent\u00b7schul\u00b7di\u00b7get", ".", "Es", "wird", "zu\u00b7gleich", "be\u00b7foh\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch Execution das Buch von ihm zu holen.", "tokens": ["Durch", "Ex\u00b7e\u00b7cu\u00b7ti\u00b7on", "das", "Buch", "von", "ihm", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Wird zu ihm hingeschickt, er soll mit Ueberflu\u00df", "tokens": ["Wird", "zu", "ihm", "hin\u00b7ge\u00b7schickt", ",", "er", "soll", "mit", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sich speisen lassen, ja, er soll ihm gar nichts schenken,", "tokens": ["Sich", "spei\u00b7sen", "las\u00b7sen", ",", "ja", ",", "er", "soll", "ihm", "gar", "nichts", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "VVINF", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey Tische la\u00df er sich mit gutem Bischof tr\u00e4nken,", "tokens": ["Bey", "Ti\u00b7sche", "la\u00df", "er", "sich", "mit", "gu\u00b7tem", "Bi\u00b7schof", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und weiche eher nicht, bis er das Buch gewi\u00df", "tokens": ["Und", "wei\u00b7che", "e\u00b7her", "nicht", ",", "bis", "er", "das", "Buch", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In seinen H\u00e4nden hat; auch mach\u2019 er \u00fcberdies", "tokens": ["In", "sei\u00b7nen", "H\u00e4n\u00b7den", "hat", ";", "auch", "mach'", "er", "\u00fc\u00b7ber\u00b7dies"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dem Mandloff noch bekannt, wie viel er sich entzogen,", "tokens": ["Dem", "Mand\u00b7loff", "noch", "be\u00b7kannt", ",", "wie", "viel", "er", "sich", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PWAV", "PIS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df er die Billigkeit nicht ganz genau erwogen!", "tokens": ["Da\u00df", "er", "die", "Bil\u00b7lig\u00b7keit", "nicht", "ganz", "ge\u00b7nau", "er\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Manch nagelneu Gedicht von ungemeiner Art", "tokens": ["Manch", "na\u00b7gel\u00b7neu", "Ge\u00b7dicht", "von", "un\u00b7ge\u00b7mei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist hier bekannt gemacht, das ihm verschwiegen ward:", "tokens": ["Ist", "hier", "be\u00b7kannt", "ge\u00b7macht", ",", "das", "ihm", "ver\u00b7schwie\u00b7gen", "ward", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wird er k\u00fcnftig nicht die B\u00fccher wieder geben,", "tokens": ["Und", "wird", "er", "k\u00fcnf\u00b7tig", "nicht", "die", "B\u00fc\u00b7cher", "wie\u00b7der", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "PTKNEG", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So soll er ohne Wein und ohne Liebe leben.", "tokens": ["So", "soll", "er", "oh\u00b7ne", "Wein", "und", "oh\u00b7ne", "Lie\u00b7be", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}