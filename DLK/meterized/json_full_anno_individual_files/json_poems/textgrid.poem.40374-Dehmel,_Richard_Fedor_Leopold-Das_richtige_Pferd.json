{"textgrid.poem.40374": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Das richtige Pferd", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer schenkt mir ein lebendiges Pferd,", "tokens": ["Wer", "schenkt", "mir", "ein", "le\u00b7ben\u00b7di\u00b7ges", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "mein Schaukelpferd ist garnichts wert,", "tokens": ["mein", "Schau\u00b7kel\u00b7pferd", "ist", "gar\u00b7nichts", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es hat so steife Beine;", "tokens": ["es", "hat", "so", "stei\u00b7fe", "Bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es stampft nicht, fri\u00dft nicht, wiehert nicht,", "tokens": ["es", "stampft", "nicht", ",", "fri\u00dft", "nicht", ",", "wie\u00b7hert", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "und macht solch ledernes Gesicht,", "tokens": ["und", "macht", "solch", "le\u00b7der\u00b7nes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "es wei\u00df 