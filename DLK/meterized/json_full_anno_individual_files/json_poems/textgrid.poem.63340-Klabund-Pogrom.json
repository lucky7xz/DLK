{"textgrid.poem.63340": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Pogrom", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Sonntag f\u00e4llt ein kleines Wort im Dom,", "tokens": ["Am", "Sonn\u00b7tag", "f\u00e4llt", "ein", "klei\u00b7nes", "Wort", "im", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Montag rollt es wachsend durch die Gasse,", "tokens": ["Am", "Mon\u00b7tag", "rollt", "es", "wach\u00b7send", "durch", "die", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Dienstag spricht man schon vom Rassenhasse, Am Mittwoch rauscht und raschelt es: Pogrom!", "tokens": ["Am", "Diens\u00b7tag", "spricht", "man", "schon", "vom", "Ras\u00b7sen\u00b7has\u00b7se", ",", "Am", "Mitt\u00b7woch", "rauscht", "und", "ra\u00b7schelt", "es", ":", "Pog\u00b7rom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+-+--+-+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Donnerstag wei\u00df man es ganz bestimmt:", "tokens": ["Am", "Don\u00b7ners\u00b7tag", "wei\u00df", "man", "es", "ganz", "be\u00b7stimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "po