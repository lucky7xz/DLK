{"dta.poem.15603": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bitte, wollet mir nur Seel' und Leib nicht scheiden;", "tokens": ["Ich", "bit\u00b7te", ",", "wol\u00b7let", "mir", "nur", "Seel'", "und", "Leib", "nicht", "schei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertragen lasset sich, sogut es geht, die beiden.", "tokens": ["Ver\u00b7tra\u00b7gen", "las\u00b7set", "sich", ",", "so\u00b7gut", "es", "geht", ",", "die", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "$,", "VVFIN", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich bitte, macht nicht wei\u00df dem eingebildten Ding,", "tokens": ["Ich", "bit\u00b7te", ",", "macht", "nicht", "wei\u00df", "dem", "ein\u00b7ge\u00b7bild\u00b7ten", "Ding", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Seel', es sei der Leib f\u00fcr sie viel zu gering.", "tokens": ["Der", "Seel'", ",", "es", "sei", "der", "Leib", "f\u00fcr", "sie", "viel", "zu", "ge\u00b7ring", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Setzt ihr nicht in den Kopf, da\u00df gut nur sei das Gute,", "tokens": ["Setzt", "ihr", "nicht", "in", "den", "Kopf", ",", "da\u00df", "gut", "nur", "sei", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,", "KOUS", "ADJD", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sie vollbringt, und nicht auch etwas lieg' im Blute.", "tokens": ["Das", "sie", "voll\u00b7bringt", ",", "und", "nicht", "auch", "et\u00b7was", "lieg'", "im", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bringt ihr den Wahn nicht bei, da\u00df ihrem Adel sei", "tokens": ["Bringt", "ihr", "den", "Wahn", "nicht", "bei", ",", "da\u00df", "ih\u00b7rem", "A\u00b7del", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts angemessen als zu werden Leibes frei.", "tokens": ["Nichts", "an\u00b7ge\u00b7mes\u00b7sen", "als", "zu", "wer\u00b7den", "Lei\u00b7bes", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "KOKOM", "PTKZU", "VAINF", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Beweiset ihr vielmehr, da\u00df ihr nicht minder noth", "tokens": ["Be\u00b7wei\u00b7set", "ihr", "viel\u00b7mehr", ",", "da\u00df", "ihr", "nicht", "min\u00b7der", "noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Leib ist als sie ihm, und Gott es so gebot.", "tokens": ["Der", "Leib", "ist", "als", "sie", "ihm", ",", "und", "Gott", "es", "so", "ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOUS", "PPER", "PPER", "$,", "KON", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Macht ihr begreiflich, da\u00df sie selber haben mu\u00df,", "tokens": ["Macht", "ihr", "be\u00b7greif\u00b7lich", ",", "da\u00df", "sie", "sel\u00b7ber", "ha\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "VAINF", "VMFIN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wenn sie nicht lahm will seyn, zum Handeln Hand und Fu\u00df.", "tokens": ["Wenn", "sie", "nicht", "lahm", "will", "seyn", ",", "zum", "Han\u00b7deln", "Hand", "und", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PTKVZ", "VMFIN", "VAINF", "$,", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Erkl\u00e4ret ihr, da\u00df sie den Leib nur soll verkl\u00e4ren,", "tokens": ["Er\u00b7kl\u00e4\u00b7ret", "ihr", ",", "da\u00df", "sie", "den", "Leib", "nur", "soll", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um den verkl\u00e4rten mitzunehmen zu den Sf\u00e4ren;", "tokens": ["Um", "den", "ver\u00b7kl\u00e4r\u00b7ten", "mit\u00b7zu\u00b7neh\u00b7men", "zu", "den", "Sf\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Weil ohne Leib sich dort zurecht nicht w\u00fcrde finden,", "tokens": ["Weil", "oh\u00b7ne", "Leib", "sich", "dort", "zu\u00b7recht", "nicht", "w\u00fcr\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PRF", "ADV", "VVFIN", "PTKNEG", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch ihre Seligkeit die Seele ganz empfinden.", "tokens": ["Noch", "ih\u00b7re", "Se\u00b7lig\u00b7keit", "die", "See\u00b7le", "ganz", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}