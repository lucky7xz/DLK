{"textgrid.poem.33973": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewiger, der du in Tiefen wohnest,", "tokens": ["E\u00b7wi\u00b7ger", ",", "der", "du", "in", "Tie\u00b7fen", "woh\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die der j\u00fcngst geborene Gedanke,", "tokens": ["Die", "der", "j\u00fcngst", "ge\u00b7bo\u00b7re\u00b7ne", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.3": {"text": "Der, weil du allein Gedanken sendest,", "tokens": ["Der", ",", "weil", "du", "al\u00b7lein", "Ge\u00b7dan\u00b7ken", "sen\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kaum den Weg von dir zu mir durchmessen,", "tokens": ["Kaum", "den", "Weg", "von", "dir", "zu", "mir", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn er r\u00fcckw\u00e4rts blickt, nur schwindelnd nachmi\u00dft,", "tokens": ["Wenn", "er", "r\u00fcck\u00b7w\u00e4rts", "blickt", ",", "nur", "schwin\u00b7delnd", "nach\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Ewiger, vernimm in dieser Stunde", "tokens": ["E\u00b7wi\u00b7ger", ",", "ver\u00b7nimm", "in", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Meines bang bewegten Herzens Flehen!", "tokens": ["Mei\u00b7nes", "bang", "be\u00b7weg\u00b7ten", "Her\u00b7zens", "Fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4umt vielleicht in einer niedern H\u00fctte", "tokens": ["Tr\u00e4umt", "viel\u00b7leicht", "in", "ei\u00b7ner", "nie\u00b7dern", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Irgendwo ein Kind, in dessen Seele", "tokens": ["Ir\u00b7gend\u00b7wo", "ein", "Kind", ",", "in", "des\u00b7sen", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jene Kraft des sch\u00f6pferischen Bildens", "tokens": ["Je\u00b7ne", "Kraft", "des", "sch\u00f6p\u00b7fe\u00b7ri\u00b7schen", "Bil\u00b7dens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen Menschen liehest, heimlich schlummert,", "tokens": ["Dei\u00b7nen", "Men\u00b7schen", "lie\u00b7hest", ",", "heim\u00b7lich", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der J\u00fcngling, der dies Kind geworden,", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "der", "dies", "Kind", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PDS", "NN", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schl\u00e4gt, von Armuth hart bedr\u00e4ngt und Rohheit,", "tokens": ["Schl\u00e4gt", ",", "von", "Ar\u00b7muth", "hart", "be\u00b7dr\u00e4ngt", "und", "Roh\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Einst ein Auge, das vor starren Thr\u00e4nen", "tokens": ["Einst", "ein", "Au\u00b7ge", ",", "das", "vor", "star\u00b7ren", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Deine Sterne l\u00e4ngst nicht mehr gesehen,", "tokens": ["Dei\u00b7ne", "Ster\u00b7ne", "l\u00e4ngst", "nicht", "mehr", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Auf zu dir und stammelt ohne Worte:", "tokens": ["Auf", "zu", "dir", "und", "stam\u00b7melt", "oh\u00b7ne", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Luft, mein Vater, da\u00df ich nicht ersticke,", "tokens": ["Luft", ",", "mein", "Va\u00b7ter", ",", "da\u00df", "ich", "nicht", "er\u00b7sti\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Eh' ich f\u00fcr mein Leben dich bezahlte!", "tokens": ["Eh'", "ich", "f\u00fcr", "mein", "Le\u00b7ben", "dich", "be\u00b7zahl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Send' ihm dann den Edelsten entgegen,", "tokens": ["Send'", "ihm", "dann", "den", "E\u00b7dels\u00b7ten", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Der, zufrieden, ein geweihtes Leben", "tokens": ["Der", ",", "zu\u00b7frie\u00b7den", ",", "ein", "ge\u00b7weih\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Aus dem Bann zu l\u00f6sen, ihm die Hand reicht,", "tokens": ["Aus", "dem", "Bann", "zu", "l\u00f6\u00b7sen", ",", "ihm", "die", "Hand", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und belohnt ist, wenn er wieder athmet,", "tokens": ["Und", "be\u00b7lohnt", "ist", ",", "wenn", "er", "wie\u00b7der", "ath\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wie ein Wand'rer die verstopfte Quelle", "tokens": ["Wie", "ein", "Wan\u00b7d'\u00b7rer", "die", "ver\u00b7stopf\u00b7te", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Freundlich reinigt, und f\u00fcr seine M\u00fche", "tokens": ["Freund\u00b7lich", "rei\u00b7nigt", ",", "und", "f\u00fcr", "sei\u00b7ne", "M\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Als der Erste trinkt und weiter schreitet.", "tokens": ["Als", "der", "Ers\u00b7te", "trinkt", "und", "wei\u00b7ter", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Kannst du aber keinen Solchen senden,", "tokens": ["Kannst", "du", "a\u00b7ber", "kei\u00b7nen", "Sol\u00b7chen", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "So verschlie\u00dfe dich vor seinem Stammeln,", "tokens": ["So", "ver\u00b7schlie\u00b7\u00dfe", "dich", "vor", "sei\u00b7nem", "Stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Denn die Kraft, die eine Welt beleben", "tokens": ["Denn", "die", "Kraft", ",", "die", "ei\u00b7ne", "Welt", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Oder eine Welt verj\u00fcngen k\u00f6nnte,", "tokens": ["O\u00b7der", "ei\u00b7ne", "Welt", "ver\u00b7j\u00fcn\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Wird, in seiner Brust zur\u00fcckgehalten,", "tokens": ["Wird", ",", "in", "sei\u00b7ner", "Brust", "zu\u00b7r\u00fcck\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Langsam, aber sicher, ihn verzehren,", "tokens": ["Lang\u00b7sam", ",", "a\u00b7ber", "si\u00b7cher", ",", "ihn", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Und dann mag er mit dem All sich mischen,", "tokens": ["Und", "dann", "mag", "er", "mit", "dem", "All", "sich", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Bis, verst\u00e4rkt in langer Ruhepause,", "tokens": ["Bis", ",", "ver\u00b7st\u00e4rkt", "in", "lan\u00b7ger", "Ru\u00b7he\u00b7pau\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ihn die eig'ne Schwere wieder abl\u00f6s't", "tokens": ["Ihn", "die", "eig'\u00b7ne", "Schwe\u00b7re", "wie\u00b7der", "ab\u00b7l\u00f6s't"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und ihm neu das Thor zum Dasein aufsprengt.", "tokens": ["Und", "ihm", "neu", "das", "Thor", "zum", "Da\u00b7sein", "auf\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Also bet' ich, weil ich schmerzlich w\u00fcnsche,", "tokens": ["Al\u00b7so", "bet'", "ich", ",", "weil", "ich", "schmerz\u00b7lich", "w\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df f\u00fcr mich, als ich geboren wurde,", "tokens": ["Da\u00df", "f\u00fcr", "mich", ",", "als", "ich", "ge\u00b7bo\u00b7ren", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So ein edler Mensch gebetet h\u00e4tte.", "tokens": ["So", "ein", "ed\u00b7ler", "Mensch", "ge\u00b7be\u00b7tet", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ewiger, der du in Tiefen wohnest,", "tokens": ["E\u00b7wi\u00b7ger", ",", "der", "du", "in", "Tie\u00b7fen", "woh\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die der j\u00fcngst geborene Gedanke,", "tokens": ["Die", "der", "j\u00fcngst", "ge\u00b7bo\u00b7re\u00b7ne", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}, "line.3": {"text": "Der, weil du allein Gedanken sendest,", "tokens": ["Der", ",", "weil", "du", "al\u00b7lein", "Ge\u00b7dan\u00b7ken", "sen\u00b7dest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kaum den Weg von dir zu mir durchmessen,", "tokens": ["Kaum", "den", "Weg", "von", "dir", "zu", "mir", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn er r\u00fcckw\u00e4rts blickt, nur schwindelnd nachmi\u00dft,", "tokens": ["Wenn", "er", "r\u00fcck\u00b7w\u00e4rts", "blickt", ",", "nur", "schwin\u00b7delnd", "nach\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Ewiger, vernimm in dieser Stunde", "tokens": ["E\u00b7wi\u00b7ger", ",", "ver\u00b7nimm", "in", "die\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "ADJD", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Meines bang bewegten Herzens Flehen!", "tokens": ["Mei\u00b7nes", "bang", "be\u00b7weg\u00b7ten", "Her\u00b7zens", "Fle\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Tr\u00e4umt vielleicht in einer niedern H\u00fctte", "tokens": ["Tr\u00e4umt", "viel\u00b7leicht", "in", "ei\u00b7ner", "nie\u00b7dern", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Irgendwo ein Kind, in dessen Seele", "tokens": ["Ir\u00b7gend\u00b7wo", "ein", "Kind", ",", "in", "des\u00b7sen", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jene Kraft des sch\u00f6pferischen Bildens", "tokens": ["Je\u00b7ne", "Kraft", "des", "sch\u00f6p\u00b7fe\u00b7ri\u00b7schen", "Bil\u00b7dens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deinen Menschen liehest, heimlich schlummert,", "tokens": ["Dei\u00b7nen", "Men\u00b7schen", "lie\u00b7hest", ",", "heim\u00b7lich", "schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der J\u00fcngling, der dies Kind geworden,", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "der", "dies", "Kind", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PDS", "NN", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schl\u00e4gt, von Armuth hart bedr\u00e4ngt und Rohheit,", "tokens": ["Schl\u00e4gt", ",", "von", "Ar\u00b7muth", "hart", "be\u00b7dr\u00e4ngt", "und", "Roh\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Einst ein Auge, das vor starren Thr\u00e4nen", "tokens": ["Einst", "ein", "Au\u00b7ge", ",", "das", "vor", "star\u00b7ren", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Deine Sterne l\u00e4ngst nicht mehr gesehen,", "tokens": ["Dei\u00b7ne", "Ster\u00b7ne", "l\u00e4ngst", "nicht", "mehr", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Auf zu dir und stammelt ohne Worte:", "tokens": ["Auf", "zu", "dir", "und", "stam\u00b7melt", "oh\u00b7ne", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPER", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Luft, mein Vater, da\u00df ich nicht ersticke,", "tokens": ["Luft", ",", "mein", "Va\u00b7ter", ",", "da\u00df", "ich", "nicht", "er\u00b7sti\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Eh' ich f\u00fcr mein Leben dich bezahlte!", "tokens": ["Eh'", "ich", "f\u00fcr", "mein", "Le\u00b7ben", "dich", "be\u00b7zahl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Send' ihm dann den Edelsten entgegen,", "tokens": ["Send'", "ihm", "dann", "den", "E\u00b7dels\u00b7ten", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Der, zufrieden, ein geweihtes Leben", "tokens": ["Der", ",", "zu\u00b7frie\u00b7den", ",", "ein", "ge\u00b7weih\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Aus dem Bann zu l\u00f6sen, ihm die Hand reicht,", "tokens": ["Aus", "dem", "Bann", "zu", "l\u00f6\u00b7sen", ",", "ihm", "die", "Hand", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Und belohnt ist, wenn er wieder athmet,", "tokens": ["Und", "be\u00b7lohnt", "ist", ",", "wenn", "er", "wie\u00b7der", "ath\u00b7met", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Wie ein Wand'rer die verstopfte Quelle", "tokens": ["Wie", "ein", "Wan\u00b7d'\u00b7rer", "die", "ver\u00b7stopf\u00b7te", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Freundlich reinigt, und f\u00fcr seine M\u00fche", "tokens": ["Freund\u00b7lich", "rei\u00b7nigt", ",", "und", "f\u00fcr", "sei\u00b7ne", "M\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Als der Erste trinkt und weiter schreitet.", "tokens": ["Als", "der", "Ers\u00b7te", "trinkt", "und", "wei\u00b7ter", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Kannst du aber keinen Solchen senden,", "tokens": ["Kannst", "du", "a\u00b7ber", "kei\u00b7nen", "Sol\u00b7chen", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "So verschlie\u00dfe dich vor seinem Stammeln,", "tokens": ["So", "ver\u00b7schlie\u00b7\u00dfe", "dich", "vor", "sei\u00b7nem", "Stam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Denn die Kraft, die eine Welt beleben", "tokens": ["Denn", "die", "Kraft", ",", "die", "ei\u00b7ne", "Welt", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Oder eine Welt verj\u00fcngen k\u00f6nnte,", "tokens": ["O\u00b7der", "ei\u00b7ne", "Welt", "ver\u00b7j\u00fcn\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Wird, in seiner Brust zur\u00fcckgehalten,", "tokens": ["Wird", ",", "in", "sei\u00b7ner", "Brust", "zu\u00b7r\u00fcck\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Langsam, aber sicher, ihn verzehren,", "tokens": ["Lang\u00b7sam", ",", "a\u00b7ber", "si\u00b7cher", ",", "ihn", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Und dann mag er mit dem All sich mischen,", "tokens": ["Und", "dann", "mag", "er", "mit", "dem", "All", "sich", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Bis, verst\u00e4rkt in langer Ruhepause,", "tokens": ["Bis", ",", "ver\u00b7st\u00e4rkt", "in", "lan\u00b7ger", "Ru\u00b7he\u00b7pau\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ihn die eig'ne Schwere wieder abl\u00f6s't", "tokens": ["Ihn", "die", "eig'\u00b7ne", "Schwe\u00b7re", "wie\u00b7der", "ab\u00b7l\u00f6s't"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und ihm neu das Thor zum Dasein aufsprengt.", "tokens": ["Und", "ihm", "neu", "das", "Thor", "zum", "Da\u00b7sein", "auf\u00b7sprengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Also bet' ich, weil ich schmerzlich w\u00fcnsche,", "tokens": ["Al\u00b7so", "bet'", "ich", ",", "weil", "ich", "schmerz\u00b7lich", "w\u00fcn\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df f\u00fcr mich, als ich geboren wurde,", "tokens": ["Da\u00df", "f\u00fcr", "mich", ",", "als", "ich", "ge\u00b7bo\u00b7ren", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "So ein edler Mensch gebetet h\u00e4tte.", "tokens": ["So", "ein", "ed\u00b7ler", "Mensch", "ge\u00b7be\u00b7tet", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}