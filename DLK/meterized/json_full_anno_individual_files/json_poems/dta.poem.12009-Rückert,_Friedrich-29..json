{"dta.poem.12009": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Raum? die dir vom Sinn gesetzten Schranken.", "tokens": ["Was", "ist", "der", "Raum", "?", "die", "dir", "vom", "Sinn", "ge\u00b7setz\u00b7ten", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PRELS", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist die Zeit? der Flu\u00df der Ding' und der Gedanken.", "tokens": ["Was", "ist", "die", "Zeit", "?", "der", "Flu\u00df", "der", "Ding'", "und", "der", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allgegenwart des Orts, Allgegenwart der Zeit!", "tokens": ["All\u00b7ge\u00b7gen\u00b7wart", "des", "Orts", ",", "All\u00b7ge\u00b7gen\u00b7wart", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo ruht von hier und dort, von jetzt und einst der Streit?", "tokens": ["Wo", "ruht", "von", "hier", "und", "dort", ",", "von", "jetzt", "und", "einst", "der", "Streit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADV", "KON", "ADV", "$,", "APPR", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Gott, wo alles ruht, wo einst die Zeit geruht,", "tokens": ["In", "Gott", ",", "wo", "al\u00b7les", "ruht", ",", "wo", "einst", "die", "Zeit", "ge\u00b7ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh in des Raumes Bett hervorbrach ihre Flut.", "tokens": ["Eh", "in", "des", "Rau\u00b7mes", "Bett", "her\u00b7vor\u00b7brach", "ih\u00b7re", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Und wo in Gott dich senkt Entz\u00fcckung oder Traum,", "tokens": ["Und", "wo", "in", "Gott", "dich", "senkt", "Ent\u00b7z\u00fc\u00b7ckung", "o\u00b7der", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da steht dir still die Zeit, und gibt dich frei der Raum.", "tokens": ["Da", "steht", "dir", "still", "die", "Zeit", ",", "und", "gibt", "dich", "frei", "der", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}