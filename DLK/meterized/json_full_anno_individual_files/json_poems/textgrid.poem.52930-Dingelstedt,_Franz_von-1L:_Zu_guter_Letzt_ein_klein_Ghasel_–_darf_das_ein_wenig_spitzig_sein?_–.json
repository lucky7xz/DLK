{"textgrid.poem.52930": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu guter Letzt ein klein Ghasel \u2013 darf das ein wenig spitzig sein? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu guter Letzt ein klein Ghasel \u2013 darf das ein wenig spitzig sein? \u2013", "tokens": ["Zu", "gu\u00b7ter", "Letzt", "ein", "klein", "Gha\u00b7sel", "\u2013", "darf", "das", "ein", "we\u00b7nig", "spit\u00b7zig", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJD", "NN", "$(", "VMFIN", "PDS", "ART", "PIS", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ein K\u00f6nig, spricht's bescheiden aus, ein K\u00f6nig soll nicht witzig sein;", "tokens": ["Ein", "K\u00f6\u00b7nig", ",", "spricht's", "be\u00b7schei\u00b7den", "aus", ",", "ein", "K\u00f6\u00b7nig", "soll", "nicht", "wit\u00b7zig", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,", "ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Das Wortspiel und den Calembourg lass er den Journalisten,", "tokens": ["Das", "Wort\u00b7spiel", "und", "den", "Ca\u00b7lem\u00b7bourg", "lass", "er", "den", "Jour\u00b7na\u00b7lis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Das Fluchen seinen F\u00e4hnderichs: ein K\u00f6nig soll nicht hitzig sein.", "tokens": ["Das", "Flu\u00b7chen", "sei\u00b7nen", "F\u00e4hn\u00b7de\u00b7richs", ":", "ein", "K\u00f6\u00b7nig", "soll", "nicht", "hit\u00b7zig", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Auch sorg er, wie ein Schuldespot, sich nicht um J\u00fcden-Namen,", "tokens": ["Auch", "sorg", "er", ",", "wie", "ein", "Schul\u00b7des\u00b7pot", ",", "sich", "nicht", "um", "J\u00fc\u00b7den\u00b7Na\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PRF", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Wer wird denn grausam gegen Schmul und strenge gegen Itzig sein?", "tokens": ["Wer", "wird", "denn", "grau\u00b7sam", "ge\u00b7gen", "Schmul", "und", "stren\u00b7ge", "ge\u00b7gen", "It\u00b7zig", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "KON", "VVFIN", "APPR", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Ein K\u00f6nig sei Original und stehe auf sich selber:", "tokens": ["Ein", "K\u00f6\u00b7nig", "sei", "O\u00b7rig\u00b7i\u00b7nal", "und", "ste\u00b7he", "auf", "sich", "sel\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "VVFIN", "APPR", "PRF", "ADV", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text"