{"textgrid.poem.49957": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Philomele", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tonleiter\u00e4hnlich steiget dein Klaggesang", "tokens": ["Ton\u00b7lei\u00b7te\u00b7r\u00e4hn\u00b7lich", "stei\u00b7get", "dein", "Klag\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vollschwellend auf, wie wenn man Bouteillen f\u00fcllt:", "tokens": ["Voll\u00b7schwel\u00b7lend", "auf", ",", "wie", "wenn", "man", "Bou\u00b7teil\u00b7len", "f\u00fcllt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KOKOM", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es steigt und steigt im Hals der Flasche \u2013", "tokens": ["Es", "steigt", "und", "steigt", "im", "Hals", "der", "Fla\u00b7sche", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh, und das liebliche Na\u00df sch\u00e4umt \u00fcber.", "tokens": ["Sieh", ",", "und", "das", "lieb\u00b7li\u00b7che", "Na\u00df", "sch\u00e4umt", "\u00fc\u00b7ber", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ART", "ADJA", "NN", "VVFIN", "APPR", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O S\u00e4ngerin, dir m\u00f6cht ich ein Liedchen weihn,", "tokens": ["O", "S\u00e4n\u00b7ge\u00b7rin", ",", "dir", "m\u00f6cht", "ich", "ein", "Lied\u00b7chen", "weihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Voll Lieb und Sehnsucht! aber ich stocke schon;", "tokens": ["Voll", "Lieb", "und", "Sehn\u00b7sucht", "!", "a\u00b7ber", "ich", "sto\u00b7cke", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$.", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach, mein unselig Gleichnis regt mir", "tokens": ["Ach", ",", "mein", "un\u00b7se\u00b7lig", "Gleich\u00b7nis", "regt", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Pl\u00f6tzlich den Durst und mein Gaumen lechzet.", "tokens": ["Pl\u00f6tz\u00b7lich", "den", "Durst", "und", "mein", "Gau\u00b7men", "lech\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Verzeih! im J\u00e4gerschl\u00f6\u00dfchen ist frisches Bier", "tokens": ["Ver\u00b7zeih", "!", "im", "J\u00e4\u00b7ger\u00b7schl\u00f6\u00df\u00b7chen", "ist", "fri\u00b7sches", "Bier"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "APPRART", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Kegelabend heut: ich versprach es halb", "tokens": ["Und", "Ke\u00b7ge\u00b7la\u00b7bend", "heut", ":", "ich", "ver\u00b7sprach