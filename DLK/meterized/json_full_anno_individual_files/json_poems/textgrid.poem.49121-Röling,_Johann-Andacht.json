s": ["VAFIN", "PPER", "PIS", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn so werd' ich gleichfalls rein,", "tokens": ["Denn", "so", "werd'", "ich", "gleich\u00b7falls", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was Reines geben.", "tokens": ["Und", "was", "Rei\u00b7nes", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "O erw\u00fcnschtes Leben,", "tokens": ["O", "er\u00b7w\u00fcnschtes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O begl\u00fcckter Stand f\u00fcr hier,", "tokens": ["O", "be\u00b7gl\u00fcck\u00b7ter", "Stand", "f\u00fcr", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn doch sterb' und fall' ich dir?", "tokens": ["Wenn", "doch", "sterb'", "und", "fall'", "ich", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Vater, Sohn und heilge Flamm,", "tokens": ["Va\u00b7ter", ",", "Sohn", "und", "heil\u00b7ge", "Flamm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pffer, Tr\u00f6ster, Br\u00e4utigam,", "tokens": ["Sch\u00f6pf\u00b7fer", ",", "Tr\u00f6s\u00b7ter", ",", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Grosses Eins in dreyen!", "tokens": ["Gros\u00b7ses", "Eins", "in", "drey\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "CARD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie mu\u00df ich mich freuen,", "tokens": ["Wie", "mu\u00df", "ich", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn ihr, Haupt der Ewigkeit,", "tokens": ["Wenn", "ihr", ",", "Haupt", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Seelen G\u00e4ste seyd.", "tokens": ["Mei\u00b7ner", "See\u00b7len", "G\u00e4s\u00b7te", "seyd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ich betr\u00fcbter Erdenklo\u00df", "tokens": ["Ich", "be\u00b7tr\u00fcb\u00b7ter", "Er\u00b7den\u00b7klo\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin der gantzen Gottheit Schoo\u00df;", "tokens": ["Bin", "der", "gant\u00b7zen", "Got\u00b7theit", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die Lust und Ehre,", "tokens": ["F\u00fcr", "die", "Lust", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auch wie kurtz sie w\u00e4re,", "tokens": ["Auch", "wie", "kurtz", "sie", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ging' ich nicht auf Ewigseyn", "tokens": ["Ging'", "ich", "nicht", "auf", "E\u00b7wi\u00b7gseyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Erden Herrschafft ein.", "tokens": ["Al\u00b7ler", "Er\u00b7den", "Herr\u00b7schafft", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Sey, o Seele, frey, und froh,", "tokens": ["Sey", ",", "o", "See\u00b7le", ",", "frey", ",", "und", "froh", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein und Gottes allzeit so,", "tokens": ["Dein", "und", "Got\u00b7tes", "all\u00b7zeit", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stopff dir Aug' und Ohren;", "tokens": ["Stopff", "dir", "Aug'", "und", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn du die verloren,", "tokens": ["Wenn", "du", "die", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn so h\u00f6rst und siehstu recht,", "tokens": ["Denn", "so", "h\u00f6rst", "und", "sieh\u00b7stu", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Drinn dich Aug' und Ohr itzt schw\u00e4cht.", "tokens": ["Drinn", "dich", "Aug'", "und", "Ohr", "itzt", "schw\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}