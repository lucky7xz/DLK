{"textgrid.poem.49121": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fromme Seele, la\u00df die Welt;", "tokens": ["From\u00b7me", "See\u00b7le", ",", "la\u00df", "die", "Welt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deines Sch\u00f6pffers heiligs Zelt,", "tokens": ["Dei\u00b7nes", "Sch\u00f6pf\u00b7fers", "hei\u00b7ligs", "Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum du pflegst beten,", "tokens": ["Wa\u00b7rum", "du", "pflegst", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Heist dich einzutreten;", "tokens": ["Heist", "dich", "ein\u00b7zu\u00b7tre\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sey bedachtsam, wie man ist,", "tokens": ["Sey", "be\u00b7dacht\u00b7sam", ",", "wie", "man", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PWAV", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn man seinen K\u00f6nig gr\u00fcst.", "tokens": ["Wenn", "man", "sei\u00b7nen", "K\u00f6\u00b7nig", "gr\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebster Gott, ich seh' in mir", "tokens": ["Liebs\u00b7ter", "Gott", ",", "ich", "seh'", "in", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich und alle deine Zier,", "tokens": ["Dich", "und", "al\u00b7le", "dei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Thron und Himmel", "tokens": ["Dei\u00b7nen", "Thron", "und", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und das Jauchz-Get\u00fcmmel,", "tokens": ["Und", "das", "Jauch\u00b7zGe\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das der frohen Engel Schaar", "tokens": ["Das", "der", "fro\u00b7hen", "En\u00b7gel", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Um dich treibet immerdar.", "tokens": ["Um", "dich", "trei\u00b7bet", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ich kan, sing' ich mit ein,", "tokens": ["Was", "ich", "kan", ",", "sing'", "ich", "mit", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber doch, was kan es seyn?", "tokens": ["A\u00b7ber", "doch", ",", "was", "kan", "es", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich begunt im B\u00f6sen,", "tokens": ["Ich", "be\u00b7gunt", "im", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So ist noch mein Wesen,", "tokens": ["So", "ist", "noch", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie k\u00f6mmt dessen Tichterey", "tokens": ["Wie", "k\u00f6mmt", "des\u00b7sen", "Tich\u00b7te\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PDS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihrem heiligen Heilig bey?", "tokens": ["Ih\u00b7rem", "hei\u00b7li\u00b7gen", "Hei\u00b7lig", "bey", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Werd' ich eins bey ihnen sein,", "tokens": ["Werd'", "ich", "eins", "bey", "ih\u00b7nen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn so werd' ich gleichfalls rein,", "tokens": ["Denn", "so", "werd'", "ich", "gleich\u00b7falls", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was Reines geben.", "tokens": ["Und", "was", "Rei\u00b7nes", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "O erw\u00fcnschtes Leben,", "tokens": ["O", "er\u00b7w\u00fcnschtes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O begl\u00fcckter Stand f\u00fcr hier,", "tokens": ["O", "be\u00b7gl\u00fcck\u00b7ter", "Stand", "f\u00fcr", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn doch sterb' und fall' ich dir?", "tokens": ["Wenn", "doch", "sterb'", "und", "fall'", "ich", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Vater, Sohn und heilge Flamm,", "tokens": ["Va\u00b7ter", ",", "Sohn", "und", "heil\u00b7ge", "Flamm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pffer, Tr\u00f6ster, Br\u00e4utigam,", "tokens": ["Sch\u00f6pf\u00b7fer", ",", "Tr\u00f6s\u00b7ter", ",", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Grosses Eins in dreyen!", "tokens": ["Gros\u00b7ses", "Eins", "in", "drey\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "CARD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wie mu\u00df ich mich freuen,", "tokens": ["Wie", "mu\u00df", "ich", "mich", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn ihr, Haupt der Ewigkeit,", "tokens": ["Wenn", "ihr", ",", "Haupt", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Seelen G\u00e4ste seyd.", "tokens": ["Mei\u00b7ner", "See\u00b7len", "G\u00e4s\u00b7te", "seyd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich betr\u00fcbter Erdenklo\u00df", "tokens": ["Ich", "be\u00b7tr\u00fcb\u00b7ter", "Er\u00b7den\u00b7klo\u00df"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin der gantzen Gottheit Schoo\u00df;", "tokens": ["Bin", "der", "gant\u00b7zen", "Got\u00b7theit", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr die Lust und Ehre,", "tokens": ["F\u00fcr", "die", "Lust", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auch wie kurtz sie w\u00e4re,", "tokens": ["Auch", "wie", "kurtz", "sie", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ging' ich nicht auf Ewigseyn", "tokens": ["Ging'", "ich", "nicht", "auf", "E\u00b7wi\u00b7gseyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aller Erden Herrschafft ein.", "tokens": ["Al\u00b7ler", "Er\u00b7den", "Herr\u00b7schafft", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sey, o Seele, frey, und froh,", "tokens": ["Sey", ",", "o", "See\u00b7le", ",", "frey", ",", "und", "froh", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "ADJD", "$,", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dein und Gottes allzeit so,", "tokens": ["Dein", "und", "Got\u00b7tes", "all\u00b7zeit", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stopff dir Aug' und Ohren;", "tokens": ["Stopff", "dir", "Aug'", "und", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn du die verloren,", "tokens": ["Wenn", "du", "die", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn so h\u00f6rst und siehstu recht,", "tokens": 