{"textgrid.poem.44384": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[f\u00fcr Hedwig Sonnleithner zum Namensfeste ihres Vaters]", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie suchten manche Stunden", "tokens": ["Sie", "such\u00b7ten", "man\u00b7che", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In B\u00fcchern, sch\u00f6n gebunden,", "tokens": ["In", "B\u00fc\u00b7chern", ",", "sch\u00f6n", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach Spr\u00fcchen auf dein Fest,", "tokens": ["Nach", "Spr\u00fc\u00b7chen", "auf", "dein", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und haben nichts gefunden,", "tokens": ["Und", "ha\u00b7ben", "nichts", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was sich nur h\u00f6ren l\u00e4\u00dft.", "tokens": ["Was", "sich", "nur", "h\u00f6\u00b7ren", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Allein um dir zu sagen:", "tokens": ["Al\u00b7lein", "um", "dir", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Papa, ich liebe Dich!", "tokens": ["Pa\u00b7pa", ",", "ich", "lie\u00b7be", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Darf ich nicht andre fragen,", "tokens": ["Darf", "ich", "nicht", "and\u00b7re", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das kann ich schon f\u00fcr mich!", "tokens": ["Das", "kann", "ich", "schon", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie suchten manche Stunden", "tokens": ["Sie", "such\u00b7ten", "man\u00b7che", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In B\u00fcchern, sch\u00f6n gebunden,", "tokens": ["In", "B\u00fc\u00b7chern", ",", "sch\u00f6n", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach Spr\u00fcchen auf dein Fest,", "tokens": ["Nach", "Spr\u00fc\u00b7chen", "auf", "dein", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und haben nichts gefunden,", "tokens": ["Und", "ha\u00b7ben", "nichts", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "VVPP", "