{"textgrid.poem.38299": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Schreibstunde", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bat ein Bauer ein T\u00f6chterlein,", "tokens": ["Es", "bat", "ein", "Bau\u00b7er", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df es doch th\u00e4te den Willen sein;", "tokens": ["Da\u00df", "es", "doch", "th\u00e4\u00b7te", "den", "Wil\u00b7len", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er bot ihr Silber und rothes Gold,", "tokens": ["Er", "bot", "ihr", "Sil\u00b7ber", "und", "ro\u00b7thes", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df sie ihn lieb h\u00e4tt und heirathen sollt,", "tokens": ["Da\u00df", "sie", "ihn", "lieb", "h\u00e4tt", "und", "hei\u00b7ra\u00b7then", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gar \u00f6ffentlich.", "tokens": ["Gar", "\u00f6f\u00b7fent\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Als ein Studente das hat erh\u00f6rt,", "tokens": ["Als", "ein", "Stu\u00b7den\u00b7te", "das", "hat", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Er seinem Haus den R\u00fccken kehrt;", "tokens": ["Er", "sei\u00b7nem", "Haus", "den", "R\u00fc\u00b7cken", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kam vor der Jungfrauen ihre Th\u00fcr,", "tokens": ["Kam", "vor", "der", "Jung\u00b7frau\u00b7en", "ih\u00b7re", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und klopft mit seinem Finger daf\u00fcr,", "tokens": ["Und", "klopft", "mit", "sei\u00b7nem", "Fin\u00b7ger", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Gar heimlich.", "tokens": ["Gar", "heim\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Die Jungfrau im Arm auf dem Bette lag,", "tokens": ["Die", "Jung\u00b7frau", "im", "Arm", "auf", "dem", "Bet\u00b7te", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und zum Studenten ganz leise sprach:", "tokens": ["Und", "zum", "Stu\u00b7den\u00b7ten", "ganz", "lei\u00b7se", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist jemand draussen, begehret mein,", "tokens": ["Ist", "je\u00b7mand", "draus\u00b7sen", ",", "be\u00b7ge\u00b7hret", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$,", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der zieh das Schn\u00fcrlein und komm herein", "tokens": ["Der", "zieh", "das", "Schn\u00fcr\u00b7lein", "und", "komm", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gar heimlich.", "tokens": ["Gar", "heim\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Als das der Bauer doch hat geh\u00f6rt,", "tokens": ["Als", "das", "der", "Bau\u00b7er", "doch", "hat", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Hause sein er den R\u00fccken kehrt;", "tokens": ["Dem", "Hau\u00b7se", "sein", "er", "den", "R\u00fc\u00b7cken", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und kam vor der Jungfrauen Th\u00fcr,", "tokens": ["Und", "kam", "vor", "der", "Jung\u00b7frau\u00b7en", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er klopft mit seinem Stiefel daf\u00fcr", "tokens": ["Er", "klopft", "mit", "sei\u00b7nem", "Stie\u00b7fel", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Gar \u00f6ffentlich.", "tokens": ["Gar", "\u00f6f\u00b7fent\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Die Jungfrau war in Freuden wach,", "tokens": ["Die", "Jung\u00b7frau", "war", "in", "Freu\u00b7den", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und zu dem Bauern da lachend sprach:", "tokens": ["Und", "zu", "dem", "Bau\u00b7ern", "da", "la\u00b7chend", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist jemand da, der begehrt hinein,", "tokens": ["Ist", "je\u00b7mand", "da", ",", "der", "be\u00b7gehrt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,", "PRELS", "VVPP", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der such sich ein ander Jungfr\u00e4ulein", "tokens": ["Der", "such", "sich", "ein", "an\u00b7der", "Jung\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gar heimlich.", "tokens": ["Gar", "heim\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Wer ists, der heut uns dies Liedlein sang?", "tokens": ["Wer", "ists", ",", "der", "heut", "uns", "dies", "Lied\u00b7lein", "sang", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "PPER", "PDS", "NN", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Ein freyer Studente ist er genannt;", "tokens": ["Ein", "frey\u00b7er", "Stu\u00b7den\u00b7te", "ist", "er", "ge\u00b7nannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er lehrt der Jungfrau Lesen und Schreiben,", "tokens": ["Er", "lehrt", "der", "Jung\u00b7frau", "Le\u00b7sen", "und", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Braucht dazu weder Feder noch Kreiden,", "tokens": ["Braucht", "da\u00b7zu", "we\u00b7der", "Fe\u00b7der", "noch", "Krei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gar heimlich.", "tokens": ["Gar", "heim\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": 