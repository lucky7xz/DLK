{"textgrid.poem.44533": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seid ihr so arm in eurem eignen Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr so arm in eurem eignen Haus,", "tokens": ["Seid", "ihr", "so", "arm", "in", "eu\u00b7rem", "eig\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ihr Ger\u00e4te borgt aus fremden, fernen?", "tokens": ["Da\u00df", "ihr", "Ge\u00b7r\u00e4\u00b7te", "borgt", "aus", "frem\u00b7den", ",", "fer\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spricht das Gef\u00fchl nicht eignen Inhalt aus,", "tokens": ["Spricht", "das", "Ge\u00b7f\u00fchl", "nicht", "eig\u00b7nen", "In\u00b7halt", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wie solls im Fremden sich zu finden lernen?", "tokens": ["Wie", "solls", "im", "Frem\u00b7den", "sich", "zu", "fin\u00b7den", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was heut geschehn, preis ich dem Lied nicht an", "tokens": ["Was", "heut", "ge\u00b7schehn", ",", "preis", "ich", "dem", "Lied", "nicht", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Gegenw\u00e4rtges hab ich nie besungen,", "tokens": ["Und", "Ge\u00b7gen\u00b7w\u00e4rt\u00b7ges", "hab", "ich", "nie", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ist, ist dem Bed\u00fcrfnis untertan,", "tokens": ["Was", "ist", ",", "ist", "dem", "Be\u00b7d\u00fcrf\u00b7nis", "un\u00b7ter\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergangnes, weil verkl\u00e4rt, ziemt Dichterzungen,", "tokens": ["Ver\u00b7gang\u00b7nes", ",", "weil", "ver\u00b7kl\u00e4rt", ",", "ziemt", "Dich\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "VVPP", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch die Empfindung, die dem Liede lauscht,", "tokens": ["Doch", "die", "Emp\u00b7fin\u00b7dung", ",", "die", "dem", "Lie\u00b7de", "lauscht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie ist von heut und ist mit dir geboren,", "tokens": ["Sie", "ist", "von", "heut", "und", "ist", "mit", "dir", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "KON", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sich dein Selbst mit keinem andern tauscht,", "tokens": ["Wie", "sich", "dein", "Selbst", "mit", "kei\u00b7nem", "an\u00b7dern", "tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPOSAT", "NN", "APPR", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist, was du selbst nicht f\u00fchlst, f\u00fcr dich verloren.", "tokens": ["Ist", ",", "was", "du", "selbst", "nicht", "f\u00fchlst", ",", "f\u00fcr", "dich", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Anteil liegt in Sachen, nicht im Wort,", "tokens": ["Der", "An\u00b7teil", "liegt", "in", "Sa\u00b7chen", ",", "nicht", "im", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Mitleid wecken nur verwandte Schmerzen,", "tokens": ["Dein", "Mit\u00b7leid", "we\u00b7cken", "nur", "ver\u00b7wand\u00b7te", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erbt auch der Geist durch die Geschlechter fort,", "tokens": ["Erbt", "auch", "der", "Geist", "durch", "die", "Ge\u00b7schlech\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sich selber Grab und Wiege sind die Herzen.", "tokens": ["Sich", "sel\u00b7ber", "Grab", "und", "Wie\u00b7ge", "sind", "die", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn anders ich in meinen Tagen sang,", "tokens": ["Wenn", "an\u00b7ders", "ich", "in", "mei\u00b7nen", "Ta\u00b7gen", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als \u00c4schylos, erreichbar wohl f\u00fcr keinen,", "tokens": ["Als", "\u00c4\u00b7schy\u00b7los", ",", "er\u00b7reich\u00b7bar", "wohl", "f\u00fcr", "kei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJD", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wars, weil ein andres Echo mir erklang", "tokens": ["Wars", ",", "weil", "ein", "and\u00b7res", "E\u00b7cho", "mir", "er\u00b7klang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus meiner H\u00f6rer Brust, als ihm aus seinen.", "tokens": ["Aus", "mei\u00b7ner", "H\u00f6\u00b7rer", "Brust", ",", "als", "ihm", "aus", "sei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und ihr nach zwei Jahrtausend Zwischenraum,", "tokens": ["Und", "ihr", "nach", "zwei", "Jahr\u00b7tau\u00b7send", "Zwi\u00b7schen\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "CARD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Widerspiel von meines Volkes Leben,", "tokens": ["Das", "Wi\u00b7der\u00b7spiel", "von", "mei\u00b7nes", "Vol\u00b7kes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wollt, was das Wissen euch verdeutlicht kaum,", "tokens": ["Wollt", ",", "was", "das", "Wis\u00b7sen", "euch", "ver\u00b7deut\u00b7licht", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Mitgef\u00fchl als weiche Nahrung geben?", "tokens": ["Dem", "Mit\u00b7ge\u00b7f\u00fchl", "als", "wei\u00b7che", "Nah\u00b7rung", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ehrt ihr mich, wohl, so eignet mich euch an,", "tokens": ["Ehrt", "ihr", "mich", ",", "wohl", ",", "so", "eig\u00b7net", "mich", "euch", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcllt eure Adern straff mit meinem Blute,", "tokens": ["F\u00fcllt", "eu\u00b7re", "A\u00b7dern", "straff", "mit", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und so gest\u00e4rkt, tut, wie ich selbst getan:", "tokens": ["Und", "so", "ge\u00b7st\u00e4rkt", ",", "tut", ",", "wie", "ich", "selbst", "ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erzeugt das euch Gem\u00e4\u00dfe und das Gute.", "tokens": ["Er\u00b7zeugt", "das", "euch", "Ge\u00b7m\u00e4\u00b7\u00dfe", "und", "das", "Gu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und k\u00f6nnt nicht ihrs, noch denen ihr vertraut,", "tokens": ["Und", "k\u00f6nnt", "nicht", "ihrs", ",", "noch", "de\u00b7nen", "ihr", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "$,", "ADV", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So weint und klagt im h\u00e4rnen B\u00fc\u00dferhemde,", "tokens": ["So", "weint", "und", "klagt", "im", "h\u00e4r\u00b7nen", "B\u00fc\u00b7\u00dfer\u00b7hem\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht da\u00df ihr stolz auf Mitgeborne schaut,", "tokens": ["Nicht", "da\u00df", "ihr", "stolz", "auf", "Mit\u00b7ge\u00b7bor\u00b7ne", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil ihr euch angeheuchelt habt das Fremde.", "tokens": ["Weil", "ihr", "euch", "an\u00b7ge\u00b7heu\u00b7chelt", "habt", "das", "Frem\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Dem aber, der euch deutelt Neu und Alt,", "tokens": ["Dem", "a\u00b7ber", ",", "der", "euch", "deu\u00b7telt", "Neu", "und", "Alt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sagt nur, es sein die schlechtsten der Insekten,", "tokens": ["Sagt", "nur", ",", "es", "sein", "die", "schlechts\u00b7ten", "der", "In\u00b7sek\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "PPOSAT", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die ihre Eier, weil sie selbst zu kalt,", "tokens": ["Die", "ih\u00b7re", "Ei\u00b7er", ",", "weil", "sie", "selbst", "zu", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In fremde K\u00f6rper auszubr\u00fcten legten.", "tokens": ["In", "frem\u00b7de", "K\u00f6r\u00b7per", "aus\u00b7zu\u00b7br\u00fc\u00b7ten", "leg\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Wer Leben schafft, das seiner Zeit geh\u00f6rt,", "tokens": ["Wer", "Le\u00b7ben", "schafft", ",", "das", "sei\u00b7ner", "Zeit", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4rs auch im Raum und durch die Zeit begrenzter,", "tokens": ["W\u00e4rs", "auch", "im", "Raum", "und", "durch", "die", "Zeit", "be\u00b7grenz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "KON", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tat mehr, als wer zum Sabbath auf beschw\u00f6rt", "tokens": ["Tat", "mehr", ",", "als", "wer", "zum", "Sab\u00b7ba\u00b7th", "auf", "be\u00b7schw\u00f6rt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "PWS", "APPRART", "NN", "APPR", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Schatten von Gespenstern f\u00fcr Gespenster.", "tokens": ["Die", "Schat\u00b7ten", "von", "Ge\u00b7spens\u00b7tern", "f\u00fcr", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, 