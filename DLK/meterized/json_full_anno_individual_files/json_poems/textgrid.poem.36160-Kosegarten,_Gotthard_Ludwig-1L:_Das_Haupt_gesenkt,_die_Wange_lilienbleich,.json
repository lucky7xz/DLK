{"textgrid.poem.36160": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Haupt gesenkt, die Wange lilienbleich,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Haupt gesenkt, die Wange lilienbleich,", "tokens": ["Das", "Haupt", "ge\u00b7senkt", ",", "die", "Wan\u00b7ge", "li\u00b7li\u00b7en\u00b7bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sa\u00df Zidli staar und stumm. In halbverwehrter T\u00f6ne", "tokens": ["Sa\u00df", "Zid\u00b7li", "staar", "und", "stumm", ".", "In", "halb\u00b7ver\u00b7wehr\u00b7ter", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["XY", "XY", "XY", "KON", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewimmer trug man ihn, den Einzigen der S\u00f6hne,", "tokens": ["Ge\u00b7wim\u00b7mer", "trug", "man", "ihn", ",", "den", "Ein\u00b7zi\u00b7gen", "der", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ins freundenlose Todtenreich.", "tokens": ["Ins", "freun\u00b7den\u00b7lo\u00b7se", "Tod\u00b7ten\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dumpf hallte noch der Tr\u00e4ger letzter Tritt", "tokens": ["Dumpf", "hall\u00b7te", "noch", "der", "Tr\u00e4\u00b7ger", "letz\u00b7ter", "Tritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Mutter in das Ohr, und perlenhelle Thr\u00e4nen", "tokens": ["Der", "Mut\u00b7ter", "in", "das", "Ohr", ",", "und", "per\u00b7len\u00b7hel\u00b7le", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entst\u00fcrzten ihrem Aug'. \u00bbSo \u2013 ewig du mein Sehnen,\u00ab", "tokens": ["Ent\u00b7st\u00fcrz\u00b7ten", "ih\u00b7rem", "Aug'", ".", "\u00bb", "So", "\u2013", "e\u00b7wig", "du", "mein", "Seh\u00b7nen", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$(", "ADV", "$(", "ADJD", "PPER", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Rief Zidli, \u00bbwallst du mit?", "tokens": ["Rief", "Zid\u00b7li", ",", "\u00bb", "wallst", "du", "mit", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "$(", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du wallest mit, und wie ein stablos Rohr", "tokens": ["Du", "wal\u00b7lest", "mit", ",", "und", "wie", "ein", "stab\u00b7los", "Rohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der W\u00fcste beuget mich der Sturmwind grasser Schmerzen,", "tokens": ["Der", "W\u00fcs\u00b7te", "beu\u00b7get", "mich", "der", "Sturm\u00b7wind", "gras\u00b7ser", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Wandrer tritt auf mich mit mitleidlosem Herzen;", "tokens": ["Der", "Wand\u00b7rer", "tritt", "auf", "mich", "mit", "mit\u00b7leid\u00b7lo\u00b7sem", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Denn nimmer hebst du mich empor.\u00ab", "tokens": ["Denn", "nim\u00b7mer", "hebst", "du", "mich", "em\u00b7por", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie sank zur\u00fcck. Der Lippen Purpurroth", "tokens": ["Sie", "sank", "zu\u00b7r\u00fcck", ".", "Der", "Lip\u00b7pen", "Pur\u00b7pur\u00b7roth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erblich. Ihr letzter Laut erstarb. Und d\u00fcstres Schweigen", "tokens": ["Er\u00b7blich", ".", "Ihr", "letz\u00b7ter", "Laut", "er\u00b7starb", ".", "Und", "d\u00fcst\u00b7res", "Schwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PPOSAT", "ADJA", "APPR", "VVFIN", "$.", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Umh\u00fcllt sie, \u00f6de Ruh', und lebensm\u00fcdes Neigen", "tokens": ["Um\u00b7h\u00fcllt", "sie", ",", "\u00f6\u00b7de", "Ruh'", ",", "und", "le\u00b7bens\u00b7m\u00fc\u00b7des", "Nei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hinab in Nacht und Grab und Tod.", "tokens": ["Hin\u00b7ab", "in", "Nacht", "und", "Grab", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Indessen war bei schwuler Sommerluft", "tokens": ["In\u00b7des\u00b7sen", "war", "bei", "schwu\u00b7ler", "Som\u00b7mer\u00b7luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Feierleichenzug bis vor das Thor gekommen.", "tokens": ["Der", "Fei\u00b7er\u00b7lei\u00b7chen\u00b7zug", "bis", "vor", "das", "Thor", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Chor von J\u00fcnglingen, von Jungfrau'n und von frommen", "tokens": ["Ein", "Chor", "von", "J\u00fcng\u00b7lin\u00b7gen", ",", "von", "Jung\u00b7frau'n", "und", "von", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "ADJA"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wallfahrern wallte mit zur Gruft.", "tokens": ["Wall\u00b7fah\u00b7rern", "wall\u00b7te", "mit", "zur", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hoch ragte sie in wei\u00dfem Leichgewand", "tokens": ["Hoch", "rag\u00b7te", "sie", "in", "wei\u00b7\u00dfem", "Leich\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Empor, den Schlaf umkr\u00e4nzt, die sch\u00f6ne J\u00fcnglingsleiche \u2013", "tokens": ["Em\u00b7por", ",", "den", "Schlaf", "um\u00b7kr\u00e4nzt", ",", "die", "sch\u00f6\u00b7ne", "J\u00fcng\u00b7lings\u00b7lei\u00b7che", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mag, ich mag nicht sehn der Wangen Schauerbleiche,", "tokens": ["Ich", "mag", ",", "ich", "mag", "nicht", "sehn", "der", "Wan\u00b7gen", "Schau\u00b7er\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht sehn die lebenlose Hand.", "tokens": ["Nicht", "sehn", "die", "le\u00b7ben\u00b7lo\u00b7se", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und hold und hehr, von J\u00fcngern rings umdr\u00e4ngt,", "tokens": ["Und", "hold", "und", "hehr", ",", "von", "J\u00fcn\u00b7gern", "rings", "um\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Naht Jesus Christus sich voll Hoheit und voll Milde.", "tokens": ["Naht", "Je\u00b7sus", "Chris\u00b7tus", "sich", "voll", "Ho\u00b7heit", "und", "voll", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "PRF", "ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sieht die Leiche, sieht die Mutter, h\u00f6rt das wilde", "tokens": ["Er", "sieht", "die", "Lei\u00b7che", ",", "sieht", "die", "Mut\u00b7ter", ",", "h\u00f6rt", "das", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gejammer, das die Wolken sprengt.", "tokens": ["Ge\u00b7jam\u00b7mer", ",", "das", "die", "Wol\u00b7ken", "sprengt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sein g\u00f6ttlich Herz zerschmilzt. Sein nasser Blick", "tokens": ["Sein", "g\u00f6tt\u00b7lich", "Herz", "zer\u00b7schmilzt", ".", "Sein", "nas\u00b7ser", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schau't flehend himmelan. Er hei\u00dft die Tr\u00e4ger stehen,", "tokens": ["Schau't", "fle\u00b7hend", "him\u00b7me\u00b7lan", ".", "Er", "hei\u00dft", "die", "Tr\u00e4\u00b7ger", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ruft den todten Funken aus der Himmel H\u00f6hen", "tokens": ["Und", "ruft", "den", "tod\u00b7ten", "Fun\u00b7ken", "aus", "der", "Him\u00b7mel", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus seines Vaters Schoo\u00df' zur\u00fcck.", "tokens": ["Aus", "sei\u00b7nes", "Va\u00b7ters", "Schoo\u00df'", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Funke kehret \u2013 glimmet \u2013 lodert \u2013 sieh!", "tokens": ["Der", "Fun\u00b7ke", "keh\u00b7ret", "\u2013", "glim\u00b7met", "\u2013", "lo\u00b7dert", "\u2013", "sieh", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "$(", "VVFIN", "$(", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In helle Flammen auf \u2013 der Todte lebt! Ich sehe", "tokens": ["In", "hel\u00b7le", "Flam\u00b7men", "auf", "\u2013", "der", "Tod\u00b7te", "lebt", "!", "Ich", "se\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "$(", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An seiner Mutter Hals ihn st\u00fcrzen! \u2013 Wonne! Wehe!", "tokens": ["An", "sei\u00b7ner", "Mut\u00b7ter", "Hals", "ihn", "st\u00fcr\u00b7zen", "!", "\u2013", "Won\u00b7ne", "!", "We\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "VVINF", "$.", "$(", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schweig, Saitenspiel, du singst es nie!", "tokens": ["Schweig", ",", "Sai\u00b7ten\u00b7spiel", ",", "du", "singst", "es", "nie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Schaaren staunen! starren! \u2013 Pl\u00f6tzlich st\u00fcrmt", "tokens": ["Die", "Schaa\u00b7ren", "stau\u00b7nen", "!", "star\u00b7ren", "!", "\u2013", "Pl\u00f6tz\u00b7lich", "st\u00fcrmt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "VVFIN", "$.", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Freude Donnerruf empor zum Himmelbogen.", "tokens": ["Der", "Freu\u00b7de", "Don\u00b7ner\u00b7ruf", "em\u00b7por", "zum", "Him\u00b7mel\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wilde Jubel ras't und braust, wie Meereswogen,", "tokens": ["Der", "wil\u00b7de", "Ju\u00b7bel", "ras't", "und", "braust", ",", "wie", "Mee\u00b7res\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie die Kraft des Orkans th\u00fcrmt.", "tokens": ["Wenn", "sie", "die", "Kraft", "des", "Or\u00b7kans", "th\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Das Haupt gesenkt, die Wange lilienbleich,", "tokens": ["Das", "Haupt", "ge\u00b7senkt", ",", "die", "Wan\u00b7ge", "li\u00b7li\u00b7en\u00b7bleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sa\u00df Zidli staar und stumm. In halbverwehrter T\u00f6ne", "tokens": ["Sa\u00df", "Zid\u00b7li", "staar", "und", "stumm", ".", "In", "halb\u00b7ver\u00b7wehr\u00b7ter", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["XY", "XY", "XY", "KON", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gewimmer trug man ihn, den Einzigen der S\u00f6hne,", "tokens": ["Ge\u00b7wim\u00b7mer", "trug", "man", "ihn", ",", "den", "Ein\u00b7zi\u00b7gen", "der", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ins freundenlose Todtenreich.", "tokens": ["Ins", "freun\u00b7den\u00b7lo\u00b7se", "Tod\u00b7ten\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-