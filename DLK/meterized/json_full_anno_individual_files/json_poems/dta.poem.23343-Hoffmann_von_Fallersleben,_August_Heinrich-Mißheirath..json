{"dta.poem.23343": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mi\u00dfheirath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Haltet rein das edle Blut!", "tokens": ["Hal\u00b7tet", "rein", "das", "ed\u00b7le", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mein Vater oft gesagt.", "tokens": ["Hat", "mein", "Va\u00b7ter", "oft", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! was nun mein Enkel thut!", "tokens": ["Ach", "!", "was", "nun", "mein", "En\u00b7kel", "thut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! dem Himmel sei's geklagt!", "tokens": ["Ach", "!", "dem", "Him\u00b7mel", "sei's", "ge\u00b7klagt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine B\u00fcrgerliche frei'n!", "tokens": ["Ei\u00b7ne", "B\u00fcr\u00b7ger\u00b7li\u00b7che", "frei'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, das ist f\u00fcrwahr zu arg!", "tokens": ["Nein", ",", "das", "ist", "f\u00fcr\u00b7wahr", "zu", "arg", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! das wird ein Nagel sein", "tokens": ["Ach", "!", "das", "wird", "ein", "Na\u00b7gel", "sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "NE", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz gewi\u00df zu meinem Sarg!", "tokens": ["Ganz", "ge\u00b7wi\u00df", "zu", "mei\u00b7nem", "Sarg", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also sprach der Gro\u00dfpapa,", "tokens": ["Al\u00b7so", "sprach", "der", "Gro\u00df\u00b7pa\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Ahnen an der Wand", "tokens": ["Und", "die", "Ah\u00b7nen", "an", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nickten gleichsam alle: ja!", "tokens": ["Nick\u00b7ten", "gleich\u00b7sam", "al\u00b7le", ":", "ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "PIS", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ob's jeder mitempfand.", "tokens": ["Als", "ob's", "je\u00b7der", "mi\u00b7temp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der gute Junker nahm", "tokens": ["Und", "der", "gu\u00b7te", "Jun\u00b7ker", "nahm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch zur Frau das B\u00fcrgerblut,", "tokens": ["Doch", "zur", "Frau", "das", "B\u00fcr\u00b7ger\u00b7blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der arme Junker kam", "tokens": ["Und", "der", "ar\u00b7me", "Jun\u00b7ker", "kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So zu gro\u00dfem Geld und Gut.", "tokens": ["So", "zu", "gro\u00b7\u00dfem", "Geld", "und", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und erf\u00fcllt von Lieb' und Dank", "tokens": ["Und", "er\u00b7f\u00fcllt", "von", "Lieb'", "und", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand der Alte sich darein;", "tokens": ["Fand", "der", "Al\u00b7te", "sich", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er der sonst nur Wasser trank,", "tokens": ["Er", "der", "sonst", "nur", "Was\u00b7ser", "trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trank anjetzo nur noch Wein.", "tokens": ["Trank", "an\u00b7je\u00b7tzo", "nur", "noch", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eine B\u00fcrgerliche frei'n,", "tokens": ["Ei\u00b7ne", "B\u00fcr\u00b7ger\u00b7li\u00b7che", "frei'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand er jetzt nicht mehr so arg,", "tokens": ["Fand", "er", "jetzt", "nicht", "mehr", "so", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der gute B\u00fcrgerwein", "tokens": ["Doch", "der", "gu\u00b7te", "B\u00fcr\u00b7ger\u00b7wein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward ein Nagel ihm zum Sarg.", "tokens": ["Ward", "ein", "Na\u00b7gel", "ihm", "zum", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}