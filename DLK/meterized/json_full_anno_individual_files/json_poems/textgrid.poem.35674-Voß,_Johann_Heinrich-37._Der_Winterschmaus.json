{"textgrid.poem.35674": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "37. Der Winterschmaus", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schneidender Ostorkan aus Sibirien saust am Doppelfenster;", "tokens": ["Schnei\u00b7den\u00b7der", "Os\u00b7tor\u00b7kan", "aus", "Si\u00b7bi\u00b7ri\u00b7en", "saust", "am", "Dop\u00b7pel\u00b7fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Bepackt mit Feurung knarrt im Frost die Lastfuhr.", "tokens": ["Be\u00b7packt", "mit", "Feu\u00b7rung", "knarrt", "im", "Frost", "die", "Last\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weder den Schnee durchklingelt ein Schlittener, noch umschwebt ein L\u00e4ufer", "tokens": ["We\u00b7der", "den", "Schnee", "durch\u00b7klin\u00b7gelt", "ein", "Schlit\u00b7te\u00b7ner", ",", "noch", "um\u00b7schwebt", "ein", "L\u00e4u\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+-+--+--+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Mit Stahl der Eisbahn blankgefegten Marmor.", "tokens": ["Mit", "Stahl", "der", "Eis\u00b7bahn", "blank\u00b7ge\u00b7feg\u00b7ten", "Mar\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einzelne traben im Sturm, wie gefittiget; auch das arme M\u00e4gdlein", "tokens": ["Ein\u00b7zel\u00b7ne", "tra\u00b7ben", "im", "Sturm", ",", "wie", "ge\u00b7fit\u00b7ti\u00b7get", ";", "auch", "das", "ar\u00b7me", "M\u00e4gd\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,", "PWAV", "VVPP", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-+-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Knirrt rasches Fu\u00dftritts, Haub' und Mantel haltend.", "tokens": ["Knirrt", "ra\u00b7sches", "Fu\u00df\u00b7tritts", ",", "Haub'", "und", "Man\u00b7tel", "hal\u00b7tend", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selbst im Stalle der Hahn traurt kl\u00f6sterlich, kr\u00e4het kaum, und duldet,", "tokens": ["Selbst", "im", "Stal\u00b7le", "der", "Hahn", "traurt", "kl\u00f6s\u00b7ter\u00b7lich", ",", "kr\u00e4\u00b7het", "kaum", ",", "und", "dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "$,", "VVFIN", "ADV", "$,", "KON", "VVFIN", "$,"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gelockt zum Futter, Kr\u00e4h' als Gast und Sperling.", "tokens": ["Ge\u00b7lockt", "zum", "Fut\u00b7ter", ",", "Kr\u00e4h'", "als", "Gast", "und", "Sper\u00b7ling", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "NN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stolberg, trotz dem Orkan, wie er wintere, komm in falber Wildschur,", "tokens": ["Stol\u00b7berg", ",", "trotz", "dem", "Or\u00b7kan", ",", "wie", "er", "win\u00b7te\u00b7re", ",", "komm", "in", "fal\u00b7ber", "Wild\u00b7schur", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Dem B\u00e4renturban dicken Dampf entatmend;", "tokens": ["Dem", "B\u00e4\u00b7ren\u00b7tur\u00b7ban", "di\u00b7cken", "Dampf", "en\u00b7tat\u00b7mend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Keck in Vlie\u00df und Karosse begleite dich unsre Frau Sophia,", "tokens": ["Keck", "in", "Vlie\u00df", "und", "Ka\u00b7ros\u00b7se", "be\u00b7glei\u00b7te", "dich", "uns\u00b7re", "Frau", "So\u00b7phia", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und deine Jungfrau'n, eingemummt in Rauchwerk.", "tokens": ["Und", "dei\u00b7ne", "Jung\u00b7frau'n", ",", "ein\u00b7ge\u00b7mummt", "in", "Rauch\u00b7werk", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lenz hier w\u00e4rmt das Gemach, und Heiterkeit. Lenz umgr\u00fcnt das Fenster,", "tokens": ["Lenz", "hier", "w\u00e4rmt", "das", "Ge\u00b7mach", ",", "und", "Hei\u00b7ter\u00b7keit", ".", "Lenz", "um\u00b7gr\u00fcnt", "das", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "$.", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und h\u00f6hnt des Frostes blumenhaft Gegaukel.", "tokens": ["Und", "h\u00f6hnt", "des", "Fros\u00b7tes", "blu\u00b7men\u00b7haft", "Ge\u00b7gau\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lenz in dem K\u00e4fige singt der Kanarier, froh des krausen Kohles,", "tokens": ["Lenz", "in", "dem", "K\u00e4\u00b7fi\u00b7ge", "singt", "der", "Ka\u00b7na\u00b7rier", ",", "froh", "des", "krau\u00b7sen", "Koh\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Woran Krystall in heller Sonn' ihm funkelt.", "tokens": ["Wo\u00b7ran", "Krys\u00b7tall", "in", "hel\u00b7ler", "Sonn'", "ihm", "fun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Froh, wie in bl\u00fchender B\u00e4um' Umd\u00e4mmerung, klingt der Feiergl\u00e4ser", "tokens": ["Froh", ",", "wie", "in", "bl\u00fc\u00b7hen\u00b7der", "B\u00e4um'", "Um\u00b7d\u00e4m\u00b7me\u00b7rung", ",", "klingt", "der", "Fei\u00b7er\u00b7gl\u00e4\u00b7ser"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "APPR", "ADJA", "NN", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+--+--+-+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gel\u00e4ut mit Gl\u00fcckwunsch um die Hirtentafel.", "tokens": ["Ge\u00b7l\u00e4ut", "mit", "Gl\u00fcck\u00b7wunsch", "um", "die", "Hir\u00b7ten\u00b7ta\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Manches Gesangs Nachhall aus Jonia, mancher Laut vom Tibris,", "tokens": ["Man\u00b7ches", "Ge\u00b7sangs", "Nach\u00b7hall", "aus", "Jo\u00b7nia", ",", "man\u00b7cher", "Laut", "vom", "Tib\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "APPR", "NE", "$,", "PIAT", "APPR", "APPRART", "NE", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Wo junger Fr\u00fchling ewig bl\u00fcht, umweht uns,", "tokens": ["Wo", "jun\u00b7ger", "Fr\u00fch\u00b7ling", "e\u00b7wig", "bl\u00fcht", ",", "um\u00b7weht", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit herzengendes Grams Aufheiterung. Eine Ros' auch spiegelt", "tokens": ["Mit", "her\u00b7zen\u00b7gen\u00b7des", "Grams", "Auf\u00b7hei\u00b7te\u00b7rung", ".", "Ei\u00b7ne", "Ros'", "auch", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$.", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "In d