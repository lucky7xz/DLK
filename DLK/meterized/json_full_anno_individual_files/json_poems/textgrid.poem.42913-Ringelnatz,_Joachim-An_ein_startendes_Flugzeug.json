{"textgrid.poem.42913": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "An ein startendes Flugzeug", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da stehst du in n\u00e4chster N\u00e4he", "tokens": ["Da", "stehst", "du", "in", "n\u00e4chs\u00b7ter", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor mir, stumm, starr, dumm und grau.", "tokens": ["Vor", "mir", ",", "stumm", ",", "starr", ",", "dumm", "und", "grau", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Torkle davon, du listige Kr\u00e4he,", "tokens": ["Tor\u00b7kle", "da\u00b7von", ",", "du", "lis\u00b7ti\u00b7ge", "Kr\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "T\u00f6ff t\u00f6ff und surr und dann auf in das Blau.", "tokens": ["T\u00f6ff", "t\u00f6ff", "und", "surr", "und", "dann", "auf", "in", "das", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NE", "KON", "ADV", "APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wei\u00df ich doch, da\u00df du ganz genau wei\u00dft,", "tokens": ["Wei\u00df", "ich", "doch", ",", "da\u00df", "du", "ganz", "ge\u00b7nau", "wei\u00dft", ","], "token_info": ["wo