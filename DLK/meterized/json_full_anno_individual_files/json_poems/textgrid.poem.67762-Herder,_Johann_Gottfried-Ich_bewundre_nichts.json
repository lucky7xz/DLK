{"textgrid.poem.67762": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ich bewundre nichts", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, ich bewundre nichts! In unsrer kleinen Welt", "tokens": ["Nein", ",", "ich", "be\u00b7wund\u00b7re", "nichts", "!", "In", "uns\u00b7rer", "klei\u00b7nen", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PIS", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist jedes Kleine gro\u00df, das gro\u00df ins Auge f\u00e4llt.", "tokens": ["Ist", "je\u00b7des", "Klei\u00b7ne", "gro\u00df", ",", "das", "gro\u00df", "ins", "Au\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "ADJD", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In unsrer Welt von Staub ist Alles Gold, was gl\u00e4nzet,", "tokens": ["In", "uns\u00b7rer", "Welt", "von", "Staub", "ist", "Al\u00b7les", "Gold", ",", "was", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "PIAT", "NN", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Lorbeer jedes Kraut, wenn es den Gro\u00dfen kr\u00e4nzet.", "tokens": ["Und", "Lor\u00b7beer", "je\u00b7des", "Kraut", ",", "wenn", "es", "den", "Gro\u00b7\u00dfen", "kr\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In unsrer w\u00fcsten Welt ist gro\u00df, was selten ist,", "tokens": ["In", "uns\u00b7rer", "w\u00fcs\u00b7ten", "Welt", "ist", "gro\u00df", ",", "was", "sel\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist jedes Neue sch\u00f6n, ist werth, was man vermi\u00dft.", "tokens": ["Ist", "je\u00b7des", "Neu\u00b7e", "sch\u00f6n", ",", "ist", "werth", ",", "was", "man", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "ADJD", "$,", "VAFIN", "ADJD", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In unsrer Schattenwelt gl\u00e4nzt das, was nur nicht blendet,", "tokens": ["In", "uns\u00b7rer", "Schat\u00b7ten\u00b7welt", "gl\u00e4nzt", "das", ",", "was", "nur", "nicht", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDS", "$,", "PRELS", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wenn ein Meteor nur glei\u00dft und nur das Auge schonet,", "tokens": ["Und", "wenn", "ein", "Me\u00b7te\u00b7or", "nur", "glei\u00dft", "und", "nur", "das", "Au\u00b7ge", "scho\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVFIN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Als Sonne starrt es gleich der laute P\u00f6bel an,", "tokens": ["Als", "Son\u00b7ne", "starrt", "es", "gleich", "der", "lau\u00b7te", "P\u00f6\u00b7bel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und Kinder beten es als unsern Herrn Gott an.", "tokens": ["Und", "Kin\u00b7der", "be\u00b7ten", "es", "als", "un\u00b7sern", "Herrn", "Gott", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Freund, ich ging durch die Welt; so weit ich sie erblickte,", "tokens": ["Freund", ",", "ich", "ging", "durch", "die", "Welt", ";", "so", "weit", "ich", "sie", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sah ich, was mich zerstreut', fand nie, was mich entz\u00fcckte,", "tokens": ["Sah", "ich", ",", "was", "mich", "zer\u00b7streut'", ",", "fand", "nie", ",", "was", "mich", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measur