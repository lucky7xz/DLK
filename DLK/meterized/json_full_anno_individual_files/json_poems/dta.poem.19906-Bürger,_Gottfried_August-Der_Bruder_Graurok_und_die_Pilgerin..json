{"dta.poem.19906": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der                \n  Bruder Graurok und die Pilgerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Pilgerm\u00e4del, jung und sch\u00f6n,               ", "tokens": ["Ein", "Pil\u00b7ger\u00b7m\u00e4\u00b7del", ",", "jung", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wallt\u2019 auf ein Kloster zu.", "tokens": ["Wallt'", "auf", "ein", "Klos\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie zog das Gl\u00f6klein an dem Thor;", "tokens": ["Sie", "zog", "das", "Gl\u00f6\u00b7klein", "an", "dem", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bruder Graurok trat hervor,", "tokens": ["Ein", "Bru\u00b7der", "Grau\u00b7rok", "trat", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Halbbarfus ohne Schuh.", "tokens": ["Halb\u00b7bar\u00b7fus", "oh\u00b7ne", "Schuh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sprach: \u201eGelobt sey Jesus Christ!\u201e \u2014", "tokens": ["Sie", "sprach", ":", "\u201e", "Ge\u00b7lobt", "sey", "Je\u00b7sus", "Christ", "!", "\u201e"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVPP", "VAFIN", "NE", "NE", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ein Ewigkeit!\u201e sprach er.", "tokens": ["\u201e", "in", "E\u00b7wig\u00b7keit", "!", "\u201e", "sprach", "er", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$.", "$(", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gar wunderseltsam ihm geschah;", "tokens": ["Gar", "wun\u00b7der\u00b7selt\u00b7sam", "ihm", "ge\u00b7schah", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und als er ihr ins Auge sah,", "tokens": ["Und", "als", "er", "ihr", "ins", "Au\u00b7ge", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schlug sein Herz noch mehr.", "tokens": ["Da", "schlug", "sein", "Herz", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Pilgerin mit leisem Ton,", "tokens": ["Die", "Pil\u00b7ge\u00b7rin", "mit", "lei\u00b7sem", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vol holder Sch\u00fcchternheit:", "tokens": ["Vol", "hol\u00b7der", "Sch\u00fcch\u00b7tern\u00b7heit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eehrw\u00fcrdiger, o meldet mir,", "tokens": ["\u201e", "ehr\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "o", "mel\u00b7det", "mir", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "FM", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weilt nicht mein Herzgeliebter hier", "tokens": ["Weilt", "nicht", "mein", "Herz\u00b7ge\u00b7lieb\u00b7ter", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Klostereinsamkeit?\u201e \u2014", "tokens": ["In", "Klos\u00b7ter\u00b7ein\u00b7sam\u00b7keit", "?", "\u201e"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ekind Gottes, wie sol kentlich mir", "tokens": ["\u201e", "kind", "Got\u00b7tes", ",", "wie", "sol", "kent\u00b7lich", "mir"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "NN", "NN", "$,", "PWAV", "PIS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herzgeliebter seyn?\u201e \u2014", "tokens": ["Dein", "Herz\u00b7ge\u00b7lieb\u00b7ter", "seyn", "?", "\u201e"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAINF", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201each! An dem gr\u00f6bsten h\u00e4rnen Rok,", "tokens": ["\u201e", "ach", "!", "An", "dem", "gr\u00f6bs\u00b7ten", "h\u00e4r\u00b7nen", "Rok", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "An Geissel, Gurt, und Weidenstok,", "tokens": ["An", "Geis\u00b7sel", ",", "Gurt", ",", "und", "Wei\u00b7dens\u00b7tok", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die seinen Leib kastei\u2019n.", "tokens": ["Die", "sei\u00b7nen", "Leib", "kas\u00b7tei'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "PPOSAT", "NN", "NE", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201enoch mehr an Wuchs und Angesicht,", "tokens": ["\u201e", "noch", "mehr", "an", "Wuchs", "und", "An\u00b7ge\u00b7sicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Morgenrot im Mai,", "tokens": ["Wie", "Mor\u00b7gen\u00b7rot", "im", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am goldnen Ringellockenhaar,", "tokens": ["Am", "gold\u00b7nen", "Rin\u00b7gel\u00b7lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am himmelblauen Augenpaar,", "tokens": ["Am", "him\u00b7mel\u00b7blau\u00b7en", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So freundlich, lieb und treu!\u201e \u2014", "tokens": ["So", "freund\u00b7lich", ",", "lieb", "und", "treu", "!", "\u201e"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201ekind Gottes, o wie l\u00e4ngst dahin!", "tokens": ["\u201e", "kind", "Got\u00b7tes", ",", "o", "wie", "l\u00e4ngst", "da\u00b7hin", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "FM", "KOKOM", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4ngst todt und tief verschart!", "tokens": ["L\u00e4ngst", "todt", "und", "tief", "ver\u00b7schart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gr\u00e4schen s\u00e4uselt dr\u00fcber her;", "tokens": ["Das", "Gr\u00e4sc\u00b7hen", "s\u00e4u\u00b7selt", "dr\u00fc\u00b7ber", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Stein von Marmel dr\u00fckt ihn schwer;", "tokens": ["Ein", "Stein", "von", "Mar\u00b7mel", "dr\u00fckt", "ihn", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4ngst todt und tief verschart!", "tokens": ["L\u00e4ngst", "todt", "und", "tief", "ver\u00b7schart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Siehst dort, in Immergr\u00fcn verh\u00fclt,", "tokens": ["Siehst", "dort", ",", "in", "Im\u00b7mer\u00b7gr\u00fcn", "ver\u00b7h\u00fclt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Zellenfenster nicht?", "tokens": ["Das", "Zel\u00b7len\u00b7fens\u00b7ter", "nicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wohnt\u2019 und weint\u2019 er, und verkam,", "tokens": ["Da", "wohnt'", "und", "weint'", "er", ",", "und", "ver\u00b7kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch seines M\u00e4dels Schuld, vor Gram,", "tokens": ["Durch", "sei\u00b7nes", "M\u00e4\u00b7dels", "Schuld", ",", "vor", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verl\u00f6schend, wie ein Licht.", "tokens": ["Ver\u00b7l\u00f6\u00b7schend", ",", "wie", "ein", "Licht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sechs Junggeselchen, schlank und fein,", "tokens": ["Sechs", "Jung\u00b7ge\u00b7sel\u00b7chen", ",", "schlank", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Trauersang und Klang,", "tokens": ["Bei", "Trau\u00b7er\u00b7sang", "und", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie trugen seine Baar\u2019 ans Grab;", "tokens": ["Sie", "tru\u00b7gen", "sei\u00b7ne", "Baa\u00b7r'", "ans", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und manche Z\u00e4re ran hinab,", "tokens": ["Und", "man\u00b7che", "Z\u00e4\u00b7re", "ran", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem sein Sarg versank.\u201e \u2014", "tokens": ["In\u00b7dem", "sein", "Sarg", "ver\u00b7sank", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201eo weh! O weh! So bist du hin?", "tokens": ["\u201e", "o", "weh", "!", "O", "weh", "!", "So", "bist", "du", "hin", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "NE", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bist todt und tief verschart? \u2014", "tokens": ["Bist", "todt", "und", "tief", "ver\u00b7schart", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun brich, o Herz, die Schuld war dein!", "tokens": ["Nun", "brich", ",", "o", "Herz", ",", "die", "Schuld", "war", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "FM", "NN", "$,", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4rst du, wie sein Marmelstein,", "tokens": ["Und", "w\u00e4rst", "du", ",", "wie", "sein", "Mar\u00b7mel\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4rst dennoch nicht zu hart.\u201e \u2014", "tokens": ["W\u00e4rst", "den\u00b7noch", "nicht", "zu", "hart", ".", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKA", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u201egedult, Kind Gottes, weine nicht:", "tokens": ["\u201e", "ge\u00b7dult", ",", "Kind", "Got\u00b7tes", ",", "wei\u00b7ne", "nicht", ":"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVPP", "$,", "NN", "NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun bete desto mehr!", "tokens": ["Nun", "be\u00b7te", "des\u00b7to", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergebner Gram zerspelt das Herz;", "tokens": ["Ver\u00b7geb\u00b7ner", "Gram", "zer\u00b7spelt", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Augenlicht verlischt von Schmerz;", "tokens": ["Das", "Au\u00b7gen\u00b7licht", "ver\u00b7lischt", "von", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum weine nicht so sehr!\u201e \u2014", "tokens": ["Drum", "wei\u00b7ne", "nicht", "so", "sehr", "!", "\u201e"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "ADV", "ADV", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u201eo nein, Ehrw\u00fcrdiger, o nein!", "tokens": ["\u201e", "o", "nein", ",", "Ehr\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "o", "nein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "NN", "$,", "FM", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verdamme nicht mein Leid!", "tokens": ["Ver\u00b7dam\u00b7me", "nicht", "mein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn meines Herzens Lust war Er;", "tokens": ["Denn", "mei\u00b7nes", "Her\u00b7zens", "Lust", "war", "Er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lebt und liebt kein J\u00fcngling mehr,", "tokens": ["So", "lebt", "und", "liebt", "kein", "J\u00fcng\u00b7ling", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf Erden weit und breit.", "tokens": ["Auf", "Er\u00b7den", "weit", "und", "breit", "."], "token_info": ["word", "word", "