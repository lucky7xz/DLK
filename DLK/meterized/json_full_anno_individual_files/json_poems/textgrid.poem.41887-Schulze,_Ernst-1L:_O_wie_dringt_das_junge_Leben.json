{"textgrid.poem.41887": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wie dringt das junge Leben", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie dringt das junge Leben", "tokens": ["O", "wie", "dringt", "das", "jun\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4ftig mir durch Sinn und Herz!", "tokens": ["Kr\u00e4f\u00b7tig", "mir", "durch", "Sinn", "und", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles f\u00fchl' ich gl\u00fchn und streben,", "tokens": ["Al\u00b7les", "f\u00fchl'", "ich", "gl\u00fchn", "und", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchle doppelt Lust und Schmerz.", "tokens": ["F\u00fch\u00b7le", "dop\u00b7pelt", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fruchtlos such' ich euch zu halten,", "tokens": ["Frucht\u00b7los", "such'", "ich", "euch", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geister meiner regen Brust;", "tokens": ["Geis\u00b7ter", "mei\u00b7ner", "re\u00b7gen", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nach Gefallen m\u00f6gt ihr walten,", "tokens": ["Nach", "Ge\u00b7fal\u00b7len", "m\u00f6gt", "ihr", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sey's zum Leide, sey's zur Lust.", "tokens": ["Sey's", "zum", "Lei\u00b7de", ",", "sey's", "zur", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lodre nur, gewalt'ge Liebe,", "tokens": ["Lod\u00b7re", "nur", ",", "ge\u00b7walt'\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6her lodre nur empor!", "tokens": ["H\u00f6\u00b7her", "lod\u00b7re", "nur", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Brecht, ihr vollen Bl\u00fcthentriebe,", "tokens": ["Brecht", ",", "ihr", "vol\u00b7len", "Bl\u00fc\u00b7then\u00b7trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4chtig schwellend nur hervor!", "tokens": ["M\u00e4ch\u00b7tig", "schwel\u00b7lend", "nur", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag das Herz sich blutig f\u00e4rben,", "tokens": ["Mag", "das", "Herz", "sich", "blu\u00b7tig", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag's vergehn in rascher Pein;", "tokens": ["Mag's", "ver\u00b7gehn", "in", "ra\u00b7scher", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lieber will ich ganz verderben", "tokens": ["Lie\u00b7ber", "will", "ich", "ganz", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als nur halb lebendig seyn.", "tokens": ["Als", "nur", "halb", "le\u00b7ben\u00b7dig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses Zagen, dieses Sehnen,", "tokens": ["Die\u00b7ses", "Za\u00b7gen", ",", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die Brust vergeblich schwellt,", "tokens": ["Das", "die", "Brust", "ver\u00b7geb\u00b7lich", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Seufzer, diese Thr\u00e4nen,", "tokens": ["Die\u00b7se", "Seuf\u00b7zer", ",", "die\u00b7se", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Stolz gefangen h\u00e4lt,", "tokens": ["Die", "der", "Stolz", "ge\u00b7fan\u00b7gen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses schmerzlich eitle Ringen,", "tokens": ["Die\u00b7ses", "schmerz\u00b7lich", "eit\u00b7le", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses K\u00e4mpfen ohne Kraft,", "tokens": ["Die\u00b7ses", "K\u00e4mp\u00b7fen", "oh\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Hoffnung und Vollbringen,", "tokens": ["Oh\u00b7ne", "Hoff\u00b7nung", "und", "Voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat mein bestes Mark erschlafft.", "tokens": ["Hat", "mein", "bes\u00b7tes", "Mark", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lieber wecke rasch und muthig,", "tokens": ["Lie\u00b7ber", "we\u00b7cke", "rasch", "und", "mut\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlachtruf, den entschlaf'nen Sinn!", "tokens": ["Schlach\u00b7truf", ",", "den", "ent\u00b7schlaf'\u00b7nen", "Sinn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lange tr\u00e4umt' ich, lange ruht' ich,", "tokens": ["Lan\u00b7ge", "tr\u00e4umt'", "ich", ",", "lan\u00b7ge", "ruht'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gab der Kette lang mich hin;", "tokens": ["Gab", "der", "Ket\u00b7te", "lang", "mich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier ist H\u00f6lle nicht, noch Himmel,", "tokens": ["Hier", "ist", "H\u00f6l\u00b7le", "nicht", ",", "noch", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PTKNEG", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weder Frost ist hier, noch Gluth!", "tokens": ["We\u00b7der", "Frost", "ist", "hier", ",", "noch", "Gluth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf in's feindliche Get\u00fcmmel,", "tokens": ["Auf", "in's", "feind\u00b7li\u00b7che", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00fcstig weiter durch die Fluth!", "tokens": ["R\u00fcs\u00b7tig", "wei\u00b7ter", "durch", "die", "Fluth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df noch einmal Wunsch und Wagen,", "tokens": ["Da\u00df", "noch", "ein\u00b7mal", "Wunsch", "und", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Zorn und Liebe, Wohl und Weh", "tokens": ["Zorn", "und", "Lie\u00b7be", ",", "Wohl", "und", "Weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Wellen um mich schlagen", "tokens": ["Ih\u00b7re", "Wel\u00b7len", "um", "mich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Lebens wilder See;", "tokens": ["Auf", "des", "Le\u00b7bens", "wil\u00b7der", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich k\u00fchn im tapfern Streite", "tokens": ["Und", "ich", "k\u00fchn", "im", "tap\u00b7fern", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Mit dem Strom, der mich entrafft,", "tokens": ["Mit", "dem", "Strom", ",", "der", "mich", "en\u00b7trafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Selber meinen Nachen leite,", "tokens": ["Sel\u00b7ber", "mei\u00b7nen", "Na\u00b7chen", "lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Freudig in gepr\u00fcfter Kraft.", "tokens": ["Freu\u00b7dig", "in", "ge\u00b7pr\u00fcf\u00b7ter", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "O wie dringt das junge Leben", "tokens": ["O", "wie", "dringt", "das", "jun\u00b7ge", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4ftig mir durch Sinn und Herz!", "tokens": ["Kr\u00e4f\u00b7tig", "mir", "durch", "Sinn", "und", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles f\u00fchl' ich gl\u00fchn und streben,", "tokens": ["Al\u00b7les", "f\u00fchl'", "ich", "gl\u00fchn", "und", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchle doppelt Lust und Schmerz.", "tokens": ["F\u00fch\u00b7le", "dop\u00b7pelt", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fruchtlos such' ich euch zu halten,", "tokens": ["Frucht\u00b7los", "such'", "ich", "euch", "z