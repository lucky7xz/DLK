{"textgrid.poem.25423": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Owen", "genre": "verse", "period": "N.A.", "pub_year": 1654, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus meinen Augen quillt des kalten Nilus fluth", "tokens": ["Aus", "mei\u00b7nen", "Au\u00b7gen", "quillt", "des", "kal\u00b7ten", "Ni\u00b7lus", "fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und aus dem Hertzen flammt des Berges Aethna Gluth:", "tokens": ["und", "aus", "dem", "Hert\u00b7zen", "flammt", "des", "Ber\u00b7ges", "A\u00b7e\u00b7th\u00b7na", "Gluth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein jedes pflegt zugleich mit aller Macht zu k\u00e4mpffen/", "tokens": ["Ein", "je\u00b7des", "pflegt", "zu\u00b7gleich", "mit", "al\u00b7ler", "Macht", "zu", "k\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ADV", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und dennoch kan die Fluth die Feuers-gluth nicht d\u00e4mpffen;", "tokens": ["und", "den\u00b7noch", "kan", "die", "Fluth", "die", "Feu\u00b7er\u00b7sgluth", "nicht", "d\u00e4mpf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch wieder von der Gluth das Wasser trocknen aus;", "tokens": ["Noch", "wie\u00b7der", "von", "der", "Gluth", "das", "Was\u00b7ser", "trock\u00b7nen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ART", "NN", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie seyn zwey Feinde sonst/ doch werden Freunde daraus.", "tokens": ["Sie", "seyn", "zwey", "Fein\u00b7de", "sonst", "/", "doch", "wer\u00b7den", "Freun\u00b7de", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "CARD", "NN", "ADV", "$(", "ADV", "VAFIN", "NN", "PAV", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Aus meinen Augen quillt des kalten Nilus fluth", "tokens": ["Aus", "mei\u00b7nen", "Au\u00b7gen", "quillt", "des", "kal\u00b7ten", "Ni\u00b7lus", "fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und aus dem Hertzen flammt des Berges Aethna Gluth:", "tokens": ["und", "aus", "dem", "Hert\u00b7zen", "flammt", "des", "Ber\u00b7ges", "A\u00b7e\u00b7th\u00b7na", "Gluth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ein jedes pflegt zugleich mit aller Macht zu k\u00e4mpffen/", "tokens": ["Ein", "je\u00b7des", "pflegt", "zu\u00b7gleich", "mit", "al\u00b7ler", "Macht", "zu", "k\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "wor