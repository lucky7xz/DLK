{"dta.poem.253": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verehret die schwartzbraune  Statire.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du G\u00f6ttinn meiner Brust;", "tokens": ["Du", "G\u00f6t\u00b7tinn", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Strahlen/ die die Sonne machet", "tokens": ["Strah\u00b7len", "/", "die", "die", "Son\u00b7ne", "ma\u00b7chet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie an dem Himmel lachet", "tokens": ["Wenn", "sie", "an", "dem", "Him\u00b7mel", "la\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sieht man an mit Lust;", "tokens": ["Die", "sieht", "man", "an", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn ihre Gluht/ und angenehme Sch\u00f6ne/", "tokens": ["Denn", "ih\u00b7re", "Gluht", "/", "und", "an\u00b7ge\u00b7neh\u00b7me", "Sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Augen gleicht/ nach welchen ich mich sehne.", "tokens": ["Den", "Au\u00b7gen", "gleicht", "/", "nach", "wel\u00b7chen", "ich", "mich", "seh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPR", "PWAT", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das schwartze krause Haar", "tokens": ["Das", "schwart\u00b7ze", "krau\u00b7se", "Haar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit den Augen ist verschwistert/", "tokens": ["Mit", "den", "Au\u00b7gen", "ist", "ver\u00b7schwis\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Pech die Nacht verd\u00fcstert/", "tokens": ["De\u00b7ren", "Pech", "die", "Nacht", "ver\u00b7d\u00fcs\u00b7tert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie befloret war.", "tokens": ["Wenn", "sie", "be\u00b7flo\u00b7ret", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So wie der Blitz/ den schwartz Gew\u00f6lcke heget/", "tokens": ["So", "wie", "der", "Blitz", "/", "den", "schwartz", "Ge\u00b7w\u00f6l\u00b7cke", "he\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So auch dein Strahl die Hertzen niederschl\u00e4get.", "tokens": ["So", "auch", "dein", "Strahl", "die", "Hert\u00b7zen", "nie\u00b7der\u00b7schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die angenehme Nacht", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht die braunen Wolcken siehen/", "tokens": ["Macht", "die", "brau\u00b7nen", "Wol\u00b7cken", "sie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dran die Sternen sich erh\u00f6hen", "tokens": ["Dran", "die", "Ster\u00b7nen", "sich", "er\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit ungemeiner Pracht/", "tokens": ["Mit", "un\u00b7ge\u00b7mei\u00b7ner", "Pracht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie den Geist durch ihre Zier entz\u00fccket/", "tokens": ["Da\u00df", "sie", "den", "Geist", "durch", "ih\u00b7re", "Zier", "ent\u00b7z\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn er die Lufft so angenehm erblicket.", "tokens": ["Wenn", "er", "die", "Lufft", "so", "an\u00b7ge\u00b7nehm", "er\u00b7bli\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So auch ", "tokens": ["So", "auch"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Du ihr gleichende ", "tokens": ["Du", "ihr", "glei\u00b7chen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Die ich stets im Hertzen f\u00fchre/", "tokens": ["Die", "ich", "stets", "im", "Hert\u00b7zen", "f\u00fch\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Entz\u00fcckend machst entz\u00fcndt.", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "machst", "ent\u00b7z\u00fcndt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die braune Nacht/ die/ Sch\u00f6nste/ dich bekr\u00f6net/", "tokens": ["Die", "brau\u00b7ne", "Nacht", "/", "die", "/", "Sch\u00f6ns\u00b7te", "/", "dich", "be\u00b7kr\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "$(", "NN", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das helle Licht der weissen ", "tokens": ["Das", "hel\u00b7le", "Licht", "der", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein weisses Kind ersehn", "tokens": ["Ein", "weis\u00b7ses", "Kind", "er\u00b7sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Macht nicht gleich die Hertzen brennen/", "tokens": ["Macht", "nicht", "gleich", "die", "Hert\u00b7zen", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schwartzen Strahlen k\u00f6nnen", "tokens": ["A\u00b7ber", "schwart\u00b7zen", "Strah\u00b7len", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht leicht wir widerstehn.", "tokens": ["Nicht", "leicht", "wir", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Tages-Schein den weissen ", "tokens": ["Des", "Ta\u00b7ges\u00b7Schein", "den", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der vor der Nacht der braunen Sch\u00f6nen weichet.", "tokens": ["Der", "vor", "der", "Nacht", "der", "brau\u00b7nen", "Sch\u00f6\u00b7nen", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die sch\u00f6nen Augen sind", "tokens": ["Die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bey dir gleich dem gr\u00f6\u00dften Wunder/", "tokens": ["Bey", "dir", "gleich", "dem", "gr\u00f6\u00df\u00b7ten", "Wun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhr Schw\u00e4rtze ist der Zunder/", "tokens": ["Ihr", "Schw\u00e4rt\u00b7ze", "ist", "der", "Zun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Hertz und Seel entz\u00fcndt:", "tokens": ["Der", "Hertz", "und", "Seel", "ent\u00b7z\u00fcndt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sind die Nacht/ und leuchten doch wie Sonnen/", "tokens": ["Sie", "sind", "die", "Nacht", "/", "und", "leuch\u00b7ten", "doch", "wie", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "VVFIN", "ADV", "KOKOM", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil ", "tokens": ["Weil"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Der schwartze ", "tokens": ["Der", "schwart\u00b7ze"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wird mit grosser M\u00fch gebrochen;", "tokens": ["Wird", "mit", "gros\u00b7ser", "M\u00fch", "ge\u00b7bro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gold mu\u00df man aus Schlacken kochen", "tokens": ["Gold", "mu\u00df", "man", "aus", "Schla\u00b7cken", "ko\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PIS", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die gleichfals dunckel seyn;", "tokens": ["Die", "gleich\u00b7fals", "dun\u00b7ckel", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dennoch sind sie hoch und werth geachtet/", "tokens": ["Und", "den\u00b7noch", "sind", "sie", "hoch", "und", "werth", "ge\u00b7ach\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df jedermann sie zu besitzen trachtet.", "tokens": ["Da\u00df", "je\u00b7der\u00b7mann", "sie", "zu", "be\u00b7sit\u00b7zen", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So steigt man auch mein Kind", "tokens": ["So", "steigt", "man", "auch", "mein", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach den Kirschen in die H\u00f6he", "tokens": ["Nach", "den", "Kir\u00b7schen", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Baumes schlancke J\u00e4he/", "tokens": ["Auf", "des", "Bau\u00b7mes", "schlan\u00b7cke", "J\u00e4\u00b7he", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die da erschwartzet sind;", "tokens": ["Die", "da", "er\u00b7schwart\u00b7zet", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil ihre Frucht viel s\u00fcsser und gesunder/", "tokens": ["Weil", "ih\u00b7re", "Frucht", "viel", "s\u00fcs\u00b7ser", "und", "ge\u00b7sun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als die/ so da im Schatten sind darunter.", "tokens": ["Als", "die", "/", "so", "da", "im", "Schat\u00b7ten", "sind", "da\u00b7run\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ADV", "ADV", "APPRART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}