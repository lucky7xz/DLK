{"dta.poem.20771": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An meine Freundin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Deine Seele giebt mir immer               ", "tokens": ["Dei\u00b7ne", "See\u00b7le", "giebt", "mir", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Neues Feuer, machet mich", "tokens": ["Neu\u00b7es", "Feu\u00b7er", ",", "ma\u00b7chet", "mich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer neu verliebt in dich,", "tokens": ["Im\u00b7mer", "neu", "ver\u00b7liebt", "in", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der helle Sternenschimmer", "tokens": ["Wie", "der", "hel\u00b7le", "Ster\u00b7nen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der sch\u00f6nen Winternacht;", "tokens": ["In", "der", "sch\u00f6\u00b7nen", "Win\u00b7ter\u00b7nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Fr\u00fchling mich ger\u00fchrter", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", "mich", "ge\u00b7r\u00fchr\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die sch\u00f6ne Rose macht;", "tokens": ["Durch", "die", "sch\u00f6\u00b7ne", "Ro\u00b7se", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie der gr\u00fcne Baum gezierter", "tokens": ["Wie", "der", "gr\u00fc\u00b7ne", "Baum", "ge\u00b7zier\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch die Frucht mir mehr gef\u00e4llt;", "tokens": ["Durch", "die", "Frucht", "mir", "mehr", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So gef\u00e4llst du mir vor allen,", "tokens": ["So", "ge\u00b7f\u00e4llst", "du", "mir", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und wirst noch in jener Welt", "tokens": ["Und", "wirst", "noch", "in", "je\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Bei den Engeln mir gefallen.", "tokens": ["Bei", "den", "En\u00b7geln", "mir", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}