{"dta.poem.12913": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die verliebte verwandlung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast, o Liebe! mich erst in ein reh verkehrt,", "tokens": ["Du", "hast", ",", "o", "Lie\u00b7be", "!", "mich", "erst", "in", "ein", "reh", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seines j\u00e4gers pfeil in zarter brust getragen:", "tokens": ["Das", "sei\u00b7nes", "j\u00e4\u00b7gers", "pfeil", "in", "zar\u00b7ter", "brust", "ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hernach in einen schwan, der sich zu tode singt:", "tokens": ["Her\u00b7nach", "in", "ei\u00b7nen", "schwan", ",", "der", "sich", "zu", "to\u00b7de", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dann in eine blum\u2019, in die die flamme dringt,", "tokens": ["Und", "dann", "in", "ei\u00b7ne", "blum'", ",", "in", "die", "die", "flam\u00b7me", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So von der sonne kommt, und allen safft verzehrt:", "tokens": ["So", "von", "der", "son\u00b7ne", "kommt", ",", "und", "al\u00b7len", "safft", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hierauf verlohr\u2019 ich mich in einen thr\u00e4nen-regen:", "tokens": ["Hier\u00b7auf", "verl\u00b7ohr'", "ich", "mich", "in", "ei\u00b7nen", "thr\u00e4\u00b7nen\u00b7re\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und itzund mu\u00df ich, mir zur pein,", "tokens": ["Und", "it\u00b7zund", "mu\u00df", "ich", ",", "mir", "zur", "pein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein salamander seyn,", "tokens": ["Ein", "sa\u00b7la\u00b7man\u00b7der", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Der in der strahlen-glut, so Daphnens augen hegen,", "tokens": ["Der", "in", "der", "strah\u00b7len\u00b7glut", ",", "so", "Daph\u00b7nens", "au\u00b7gen", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ADV", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verschmachtet, und doch auch sein schmachtend hertze n\u00e4hrt.", "tokens": ["Ver\u00b7schmach\u00b7tet", ",", "und", "doch", "auch", "sein", "schmach\u00b7tend", "hert\u00b7ze", "n\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ADV", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jedennoch wolt\u2019 ich mich im minsten nicht beklagen,", "tokens": ["Je\u00b7den\u00b7noch", "wolt'", "ich", "mich", "im", "mins\u00b7ten", "nicht", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn deine wunder-macht,", "tokens": ["Wenn", "dei\u00b7ne", "wun\u00b7der\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die mir bisher nichts s\u00fc\u00dfes zugedacht,", "tokens": ["Die", "mir", "bis\u00b7her", "nichts", "s\u00fc\u00b7\u00dfes", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PIS", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mir endlich noch die gunst gew\u00e4hrte,", "tokens": ["Mir", "end\u00b7lich", "noch", "die", "gunst", "ge\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und mich vor meinem tod in Daphnens schatz verkehrte.", "tokens": ["Und", "mich", "vor", "mei\u00b7nem", "tod", "in", "Daph\u00b7nens", "schatz", "ver\u00b7kehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}