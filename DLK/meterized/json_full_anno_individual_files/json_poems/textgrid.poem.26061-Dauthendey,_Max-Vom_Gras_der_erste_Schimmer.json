{"textgrid.poem.26061": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Vom Gras der erste Schimmer", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Gras der erste Schimmer,", "tokens": ["Vom", "Gras", "der", "ers\u00b7te", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als fiel vom gr\u00fcnen Seidenkleid meiner Liebsten", "tokens": ["Als", "fiel", "vom", "gr\u00fc\u00b7nen", "Sei\u00b7den\u00b7kleid", "mei\u00b7ner", "Liebs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf den braunen Wegrand ein gr\u00fcner Glimmer.", "tokens": ["Auf", "den", "brau\u00b7nen", "Weg\u00b7rand", "ein", "gr\u00fc\u00b7ner", "Glim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bald gehen ihre und meine Schuhe ohne Ziel", "tokens": ["Bald", "ge\u00b7hen", "ih\u00b7re", "und", "mei\u00b7ne", "Schu\u00b7he", "oh\u00b7ne", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Durch die gr\u00fcne Ruhe im Feld immer weiter, immer,", "tokens": ["Durch", "die", "gr\u00fc\u00b7ne", "Ru\u00b7he", "im", "Feld", "im\u00b7mer", "wei\u00b7ter", ",", "im\u00b7mer", ","], "token_info": ["word", "word", "word