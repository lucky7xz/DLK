{"dta.poem.16218": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "272.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So lange du noch kanst err\u00f6then und erblassen,", "tokens": ["So", "lan\u00b7ge", "du", "noch", "kanst", "er\u00b7r\u00f6\u00b7then", "und", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du von menschlichen Gef\u00fchlen nicht verlassen.", "tokens": ["Bist", "du", "von", "menschli\u00b7chen", "Ge\u00b7f\u00fch\u00b7len", "nicht", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nie m\u00f6gen menschliche Gef\u00fchle dir entweichen", "tokens": ["Nie", "m\u00f6\u00b7gen", "menschli\u00b7che", "Ge\u00b7f\u00fch\u00b7le", "dir", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Soweit, da\u00df du nicht kanst err\u00f6then und erbleichen!", "tokens": ["So\u00b7weit", ",", "da\u00df", "du", "nicht", "kanst", "er\u00b7r\u00f6\u00b7then", "und", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erbleichen macht dich Furcht, err\u00f6then macht dich Scham,", "tokens": ["Er\u00b7blei\u00b7chen", "macht", "dich", "Furcht", ",", "er\u00b7r\u00f6\u00b7then", "macht", "dich", "Scham", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "VVINF", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Furcht die vorm B\u00f6sen kommt, und Scham die nach ihm kam.", "tokens": ["Furcht", "die", "vorm", "B\u00f6\u00b7sen", "kommt", ",", "und", "Scham", "die", "nach", "ihm", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPRART", "NN", "VVFIN", "$,", "KON", "NN", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur wenn du diese Furcht und Scham in dir zu t\u00f6dten", "tokens": ["Nur", "wenn", "du", "die\u00b7se", "Furcht", "und", "Scham", "in", "dir", "zu", "t\u00f6d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PDAT", "NN", "KON", "NN", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vermagst, wirst du nicht mehr erblassen und err\u00f6then.", "tokens": ["Ver\u00b7magst", ",", "wirst", "du", "nicht", "mehr", "er\u00b7blas\u00b7sen", "und", "er\u00b7r\u00f6\u00b7then", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "PTKNEG", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer nicht das B\u00f6se kennt, erbla\u00dft, err\u00f6thet nicht,", "tokens": ["Wer", "nicht", "das", "B\u00f6\u00b7se", "kennt", ",", "er\u00b7bla\u00dft", ",", "er\u00b7r\u00f6t\u00b7het", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Thier am Boden hier, der Siddha dort im Licht.", "tokens": ["Das", "Thier", "am", "Bo\u00b7den", "hier", ",", "der", "Sidd\u00b7ha", "dort", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vom Thiere fern, kanst du nicht an den Siddha reichen,", "tokens": ["Vom", "Thie\u00b7re", "fern", ",", "kanst", "du", "nicht", "an", "den", "Sidd\u00b7ha", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deswegen Furcht und Scham dich wechselnd \u00fcberschleichen.", "tokens": ["Des\u00b7we\u00b7gen", "Furcht", "und", "Scham", "dich", "wech\u00b7selnd", "\u00fc\u00b7bersc\u00b7hlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du kanst dem Thiere nicht, noch auch dem Siddha gleichen,", "tokens": ["Du", "kanst", "dem", "Thie\u00b7re", "nicht", ",", "noch", "auch", "dem", "Sidd\u00b7ha", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "$,", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dagegen wechselt dein Err\u00f6then mit Erbleichen.", "tokens": ["Da\u00b7ge\u00b7gen", "wech\u00b7selt", "dein", "Er\u00b7r\u00f6\u00b7then", "mit", "Er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "O f\u00fcrchte dich nur nicht, noch sch\u00e4me dich der Zeichen", "tokens": ["O", "f\u00fcrch\u00b7te", "dich", "nur", "nicht", ",", "noch", "sch\u00e4\u00b7me", "dich", "der", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschlichkeit im Schamerr\u00f6then, Furchterbleichen!", "tokens": ["Der", "Menschlich\u00b7keit", "im", "Scha\u00b7mer\u00b7r\u00f6\u00b7then", ",", "Furch\u00b7ter\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}