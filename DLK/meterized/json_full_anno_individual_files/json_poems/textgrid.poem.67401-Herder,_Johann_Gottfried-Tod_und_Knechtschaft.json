{"textgrid.poem.67401": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Tod und Knechtschaft", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seiner kleinen Philomele", "tokens": ["Sei\u00b7ner", "klei\u00b7nen", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sang aus tiefer, voller Seele", "tokens": ["Sang", "aus", "tie\u00b7fer", ",", "vol\u00b7ler", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJD", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr A\u00ebdon noch sein Lied,", "tokens": ["Ihr", "A\u00eb\u00b7don", "noch", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als er droben einen Geier,", "tokens": ["Als", "er", "dro\u00b7ben", "ei\u00b7nen", "Gei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drunten einen Vogelsteller", "tokens": ["Drun\u00b7ten", "ei\u00b7nen", "Vo\u00b7gel\u00b7stel\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schweben und anschleichen sieht.", "tokens": ["Schwe\u00b7ben", "und", "an\u00b7schlei\u00b7chen", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbauf! Geliebte, auf! und w\u00e4hle!", "tokens": ["\u00bb", "auf", "!", "Ge\u00b7lieb\u00b7te", ",", "auf", "!", "und", "w\u00e4h\u00b7le", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "$,", "PTKVZ", "$.", "KON", "VVIMP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Siehe, siehe, was uns droht,", "tokens": ["Sie\u00b7he", ",", "sie\u00b7he", ",", "was", "uns", "droht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.9": {"text": "Unten Knechtschaft, oben Tod!\u00ab", "tokens": ["Un\u00b7ten", "Knecht\u00b7schaft", ",", "o\u00b7ben", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "\u00bbfrisch gew\u00e4hlt!\u00ab sprach Philomele;", "tokens": ["\u00bb", "frisch", "ge\u00b7w\u00e4hlt", "!", "\u00ab", "sprach", "Phi\u00b7lo\u00b7me\u00b7le", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVPP", "$.", "$(", "VVFIN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00bbungetrennet s\u00fc\u00dfen Tod!\u00ab", "tokens": ["\u00bb", "un\u00b7ge\u00b7tren\u00b7net", "s\u00fc\u00b7\u00dfen", "Tod", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seiner kleinen Philomele", "tokens": ["Sei\u00b7ner", "klei\u00b7nen", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.te