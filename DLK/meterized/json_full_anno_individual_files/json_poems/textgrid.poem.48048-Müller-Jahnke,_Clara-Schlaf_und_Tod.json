{"textgrid.poem.48048": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Schlaf und Tod", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df und wonnesam ist der Schlaf. \u2013", "tokens": ["S\u00fc\u00df", "und", "won\u00b7ne\u00b7sam", "ist", "der", "Schlaf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "In der strengen Schule des Lebens,", "tokens": ["In", "der", "stren\u00b7gen", "Schu\u00b7le", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "wo gleich unverst\u00e4ndigen Kindern", "tokens": ["wo", "gleich", "un\u00b7ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "wir die krausen, verworrenen R\u00e4tsel", "tokens": ["wir", "die", "krau\u00b7sen", ",", "ver\u00b7wor\u00b7re\u00b7nen", "R\u00e4t\u00b7sel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "m\u00fchsam zusammenbuchstabieren", "tokens": ["m\u00fch\u00b7sam", "zu\u00b7sam\u00b7men\u00b7buch\u00b7sta\u00b7bie\u00b7ren"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "aber nimmer den Sinn erforschen,", "tokens": ["a\u00b7ber", "nim\u00b7mer", "den", "Sinn", "er\u00b7for\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "wo der Schmerz mit ehernem Griffel", "tokens": ["wo", "der", "Schmerz", "mit", "e\u00b7her\u00b7nem", "Grif\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Runen auf unsere Stirnen schreibt,", "tokens": ["Ru\u00b7nen", "auf", "un\u00b7se\u00b7re", "Stir\u00b7nen", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "d\u00fcnkt der Schlaf die Erholungsstunde", "tokens": ["d\u00fcnkt", "der", "Schlaf", "die", "Er\u00b7ho\u00b7lungs\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "mir, die s\u00fc\u00dfe, k\u00f6stliche Pause,", "tokens": ["mir", ",", "die", "s\u00fc\u00b7\u00dfe", ",", "k\u00f6st\u00b7li\u00b7che", "Pau\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "da die verschlossene T\u00fcre aufspringt", "tokens": ["da", "die", "ver\u00b7schlos\u00b7se\u00b7ne", "T\u00fc\u00b7re", "auf\u00b7springt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "und statt dumpfigen B\u00fccherstaubes", "tokens": ["und", "statt", "dump\u00b7fi\u00b7gen", "B\u00fc\u00b7cher\u00b7stau\u00b7bes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sonnenstrahlen und Luft wir atmen . . .", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len", "und", "Luft", "wir", "at\u00b7men", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVINF", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "s\u00fc\u00df und wonnesam ist der Schlaf. \u2013", "tokens": ["s\u00fc\u00df", "und", "won\u00b7ne\u00b7sam", "ist", "der", "Schlaf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Schlaf ist Vergessen, ist die Befreiung", "tokens": ["Schlaf", "ist", "Ver\u00b7ges\u00b7sen", ",", "ist", "die", "Be\u00b7frei\u00b7ung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$,", "VAFIN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "von all den lastenden, qu\u00e4lenden Sorgen", "tokens": ["von", "all", "den", "las\u00b7ten\u00b7den", ",", "qu\u00e4\u00b7len\u00b7den", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "um des Daseins traurige Narrheit,", "tokens": ["um", "des", "Da\u00b7seins", "trau\u00b7ri\u00b7ge", "Nar\u00b7rheit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "um der Zukunft lichtloses Dunkel,", "tokens": ["um", "der", "Zu\u00b7kunft", "licht\u00b7lo\u00b7ses", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.5": {"text": "um das eine, selige Gl\u00fcck,", "tokens": ["um", "das", "ei\u00b7ne", ",", "se\u00b7li\u00b7ge", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "das gleich silbernen Wasserwogen", "tokens": ["das", "gleich", "sil\u00b7ber\u00b7nen", "Was\u00b7ser\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "meines Lebens dornige W\u00fcste", "tokens": ["mei\u00b7nes", "Le\u00b7bens", "dor\u00b7ni\u00b7ge", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "noch mit bl\u00fchenden Blumen schm\u00fcckte,", "tokens": ["noch", "mit", "bl\u00fc\u00b7hen\u00b7den", "Blu\u00b7men", "schm\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "und nun haltlos wie Regentropfen", "tokens": ["und", "nun", "halt\u00b7los", "wie", "Re\u00b7gen\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "mir in der zitternden Hand zerrinnt. \u2013", "tokens": ["mir", "in", "der", "zit\u00b7tern\u00b7den", "Hand", "zer\u00b7rinnt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00df und wonnesam ist der Schlaf,", "tokens": ["S\u00fc\u00df", "und", "won\u00b7ne\u00b7sam", "ist", "der", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "aber eines noch d\u00e4ucht mich s\u00fc\u00dfer:", "tokens": ["a\u00b7ber", "ei\u00b7nes", "noch", "d\u00e4ucht", "mich", "s\u00fc\u00b7\u00dfer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "nicht das Vergessen nur, \u2013 das Vergehen!", "tokens": ["nicht", "das", "Ver\u00b7ges\u00b7sen", "nur", ",", "\u2013", "das", "Ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$,", "$(", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nicht das Ausruhen, \u2013 nein, die Ruhe!", "tokens": ["Nicht", "das", "Aus\u00b7ru\u00b7hen", ",", "\u2013", "nein", ",", "die", "Ru\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "$(", "PTKANT", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sei willkommen mir, goldene Stunde,", "tokens": ["Sei", "will\u00b7kom\u00b7men", "mir", ",", "gol\u00b7de\u00b7ne", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "die den Sch\u00fcler gereiften Sinnes", "tokens": ["die", "den", "Sch\u00fc\u00b7ler", "ge\u00b7reif\u00b7ten", "Sin\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "aus der dr\u00fcckenden Mauern Enge", "tokens": ["aus", "der", "dr\u00fc\u00b7cken\u00b7den", "Mau\u00b7ern", "En\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NE"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u00fcber die Schwelle hinaus in lichte", "tokens": ["\u00fc\u00b7ber", "die", "Schwel\u00b7le", "hin\u00b7aus", "in", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APZR", "APPR", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "sonnendurchstrahlte Weiten f\u00fchrt \u2013 \u2013", "tokens": ["son\u00b7nen\u00b7durch\u00b7strahl\u00b7te", "Wei\u00b7ten", "f\u00fchrt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$(", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Sei gesegnet, du G\u00f6tterbote,", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "du", "G\u00f6t\u00b7ter\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "der auf rauschenden Adlerschwingen", "tokens": ["der", "auf", "rau\u00b7schen\u00b7den", "Ad\u00b7ler\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "meine Seele aus Nacht und Dunkel", "tokens": ["mei\u00b7ne", "See\u00b7le", "aus", "Nacht", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "aufw\u00e4rts tr\u00e4gt zu den fernen H\u00f6hn,", "tokens": ["auf\u00b7w\u00e4rts", "tr\u00e4gt", "zu", "den", "fer\u00b7nen", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "wo aus goldenem Schacht des Gl\u00fcckes", "tokens": ["wo", "aus", "gol\u00b7de\u00b7nem", "Schacht", "des", "Gl\u00fc\u00b7ckes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "nie versiegende Quellen sprudeln! \u2013", "tokens": ["nie", "ver\u00b7sie\u00b7gen\u00b7de", "Quel\u00b7len", "spru\u00b7deln", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Dreimal s\u00fc\u00dfer ist Schlaf, denn Wachen,", "tokens": ["Drei\u00b7mal", "s\u00fc\u00b7\u00dfer", "ist", "Schlaf", ",", "denn", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "aber das S\u00fc\u00dfeste ist der Tod.", "tokens": ["a\u00b7ber", "das", "S\u00fc\u00b7\u00dfes\u00b7te", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "