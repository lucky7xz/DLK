{"textgrid.poem.48731": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die warme Fr\u00fchlingsluft macht ihren Himmel klar.", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die warme Fr\u00fchlingsluft macht ihren Himmel klar.", "tokens": ["Die", "war\u00b7me", "Fr\u00fch\u00b7lings\u00b7luft", "macht", "ih\u00b7ren", "Him\u00b7mel", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seht, wie das g\u00fcldne Liecht der Sonnen heller blicket!", "tokens": ["Seht", ",", "wie", "das", "g\u00fcld\u00b7ne", "Liecht", "der", "Son\u00b7nen", "hel\u00b7ler", "bli\u00b7cket", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Felder schwangre Scho\u00df ist zur Geburt geschicket;", "tokens": ["Der", "Fel\u00b7der", "schwang\u00b7re", "Scho\u00df", "ist", "zur", "Ge\u00b7burt", "ge\u00b7schi\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die gr\u00fcne See geht auf, die Quelle springen gar", "tokens": ["die", "gr\u00fc\u00b7ne", "See", "geht", "auf", ",", "die", "Quel\u00b7le", "sprin\u00b7gen", "gar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,", "ART",