{"textgrid.poem.44298": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf den Tod einer Grille", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Musen, h\u00fcllet mir die Leier,", "tokens": ["Mu\u00b7sen", ",", "h\u00fcl\u00b7let", "mir", "die", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sonst nur der Freud erklang,", "tokens": ["Die", "sonst", "nur", "der", "Freud", "er\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "In der Trauer dunkeln Schleier,", "tokens": ["In", "der", "Trau\u00b7er", "dun\u00b7keln", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagend halle mein Gesang!", "tokens": ["Kla\u00b7gend", "hal\u00b7le", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwermutsvoll, in dumpfen T\u00f6nen", "tokens": ["Schwer\u00b7muts\u00b7voll", ",", "in", "dum\u00b7pfen", "T\u00f6\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weine, holde Elegie,", "tokens": ["Wei\u00b7ne", ",", "hol\u00b7de", "E\u00b7le\u00b7gie", ","], "token_info": ["word", "punct", "word", "word", "punct"], "