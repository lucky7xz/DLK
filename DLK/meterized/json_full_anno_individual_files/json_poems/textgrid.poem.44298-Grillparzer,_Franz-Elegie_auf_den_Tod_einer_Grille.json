cck!", "tokens": ["Nim\u00b7mer", "keh\u00b7ret", "er", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Schlafe denn, da dich mein Kummer", "tokens": ["Schla\u00b7fe", "denn", ",", "da", "dich", "mein", "Kum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Nimmermehr zum Leben ruft,", "tokens": ["Nim\u00b7mer\u00b7mehr", "zum", "Le\u00b7ben", "ruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Schlafe denn den Todesschlummer,", "tokens": ["Schla\u00b7fe", "denn", "den", "To\u00b7des\u00b7schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Ruhe sanft in d\u00fcstrer Gruft!", "tokens": ["Ru\u00b7he", "sanft", "in", "d\u00fcst\u00b7rer", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}