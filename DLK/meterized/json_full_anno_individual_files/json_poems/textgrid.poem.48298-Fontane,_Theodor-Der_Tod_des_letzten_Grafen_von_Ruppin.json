en_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Pfeiler im hohen Chore.", "tokens": ["Am", "Pfei\u00b7ler", "im", "ho\u00b7hen", "Cho\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.18": {"line.1": {"text": "Die alte Gr\u00e4fin murmelte still:", "tokens": ["Die", "al\u00b7te", "Gr\u00e4\u00b7fin", "mur\u00b7mel\u00b7te", "still", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbnun mu\u00df ich wollen, was ich nicht will,", "tokens": ["\u00bb", "nun", "mu\u00df", "ich", "wol\u00b7len", ",", "was", "ich", "nicht", "will", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VMFIN", "$,", "PWS", "PPER", "PTKNEG", "VMFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und leben \u2013 ich, die ", "tokens": ["Und", "le\u00b7ben", "\u2013", "ich", ",", "die"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$(", "PPER", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}}}}}