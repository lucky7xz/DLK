{"textgrid.poem.33260": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Pfaden, dunkel, voller Grausen,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Pfaden, dunkel, voller Grausen,", "tokens": ["Auf", "Pfa\u00b7den", ",", "dun\u00b7kel", ",", "vol\u00b7ler", "Grau\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nur b\u00f6se Engel hausen,", "tokens": ["Wo", "nur", "b\u00f6\u00b7se", "En\u00b7gel", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ein D\u00e4mon, ", "tokens": ["Wo", "ein", "D\u00e4\u00b7mon", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Auf schwarzem Thron die Fl\u00fcgel spannt,", "tokens": ["Auf", "schwar\u00b7zem", "Thron", "die", "Fl\u00fc\u00b7gel", "spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus letztem d\u00fcsterm Thule fand", "tokens": ["Aus", "letz\u00b7tem", "d\u00fcs\u00b7term", "Thu\u00b7le", "fand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich j\u00fcngst erst her in dieses Land \u2013", "tokens": ["Ich", "j\u00fcngst", "erst", "her", "in", "die\u00b7ses", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aus Zauberreich, so wild und weit,", "tokens": ["Aus", "Zau\u00b7ber\u00b7reich", ",", "so", "wild", "und", "weit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Fern von ", "tokens": ["Fern", "von"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ewig bodenlose Schl\u00fcnde,", "tokens": ["E\u00b7wig", "bo\u00b7den\u00b7lo\u00b7se", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kl\u00fcfte, Schl\u00fcfte ohne Gr\u00fcnde,", "tokens": ["Kl\u00fcf\u00b7te", ",", "Schl\u00fcf\u00b7te", "oh\u00b7ne", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbegrenzte Wassermassen,", "tokens": ["Un\u00b7be\u00b7grenz\u00b7te", "Was\u00b7ser\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich nie in Ufer fassen,", "tokens": ["Die", "sich", "nie", "in", "U\u00b7fer", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4lder, die kein Ende nehmen,", "tokens": ["W\u00e4l\u00b7der", ",", "die", "kein", "En\u00b7de", "neh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die \u2013 titanenhafte Schemen \u2013", "tokens": ["Die", "\u2013", "ti\u00b7ta\u00b7nen\u00b7haf\u00b7te", "Sche\u00b7men", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tropfend stehn in Nebeltau,", "tokens": ["Trop\u00b7fend", "stehn", "in", "Ne\u00b7bel\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Endlos wuchtend, endlos grau!", "tokens": ["End\u00b7los", "wuch\u00b7tend", ",", "end\u00b7los", "grau", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Berge, endlos niederfallend,", "tokens": ["Ber\u00b7ge", ",", "end\u00b7los", "nie\u00b7der\u00b7fal\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meere, in kein Ufer wallend,", "tokens": ["Mee\u00b7re", ",", "in", "kein", "U\u00b7fer", "wal\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Meere, die urewig fluten,", "tokens": ["Mee\u00b7re", ",", "die", "u\u00b7re\u00b7wig", "flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.12": {"text": "Himmel, die urewig gluten,", "tokens": ["Him\u00b7mel", ",", "die", "u\u00b7re\u00b7wig", "glu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Weiher, die unendlich breiten", "tokens": ["Wei\u00b7her", ",", "die", "un\u00b7end\u00b7lich", "brei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Stummer Wasser Einsamkeiten,", "tokens": ["Stum\u00b7mer", "Was\u00b7ser", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die in Tod und Stille liegen", "tokens": ["Die", "in", "Tod", "und", "Stil\u00b7le", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und den Schnee der Lilie wiegen.", "tokens": ["Und", "den", "Schnee", "der", "Li\u00b7lie", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Bei den Weihern, die da breiten", "tokens": ["Bei", "den", "Wei\u00b7hern", ",", "die", "da", "brei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stummer Wasser Einsamkeiten,", "tokens": ["Stum\u00b7mer", "Was\u00b7ser", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in Tod und Trauer liegen", "tokens": ["Die", "in", "Tod", "und", "Trau\u00b7er", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Schnee der Lilie wiegen;", "tokens": ["Und", "den", "Schnee", "der", "Li\u00b7lie", "wie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Bei den Bergen, bei den Fl\u00fcssen,", "tokens": ["Bei", "den", "Ber\u00b7gen", ",", "bei", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die so ruhlos murmeln m\u00fcssen;", "tokens": ["Die", "so", "ruh\u00b7los", "mur\u00b7meln", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bei den W\u00e4ldern, bei den S\u00fcmpfen,", "tokens": ["Bei", "den", "W\u00e4l\u00b7dern", ",", "bei", "den", "S\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo bei schwarzverfaulten St\u00fcmpfen", "tokens": ["Wo", "bei", "schwarz\u00b7ver\u00b7faul\u00b7ten", "St\u00fcmp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Molch und Kr\u00f6te lauernd schleichen;", "tokens": ["Molch", "und", "Kr\u00f6\u00b7te", "lau\u00b7ernd", "schlei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Bei den Pfuhlen und den Teichen,", "tokens": ["Bei", "den", "Pfuh\u00b7len", "und", "den", "Tei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo gefr\u00e4\u00dfige D\u00e4monen", "tokens": ["Wo", "ge\u00b7fr\u00e4\u00b7\u00dfi\u00b7ge", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gierig bei den Leichen wohnen;", "tokens": ["Gie\u00b7rig", "bei", "den", "Lei\u00b7chen", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Bei den tr\u00fcben S\u00fcndenquellen,", "tokens": ["Bei", "den", "tr\u00fc\u00b7ben", "S\u00fcn\u00b7den\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die in giftigen D\u00fcnsten schwellen \u2013", "tokens": ["Die", "in", "gif\u00b7ti\u00b7gen", "D\u00fcns\u00b7ten", "schwel\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Trifft der Wandrer voller Bangen", "tokens": ["Trifft", "der", "Wand\u00b7rer", "vol\u00b7ler", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alles, was schon lang vergangen:", "tokens": ["Al\u00b7les", ",", "was", "schon", "lang", "ver\u00b7gan\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Totenhemden, die sich bl\u00e4hen,", "tokens": ["To\u00b7ten\u00b7hem\u00b7den", ",", "die", "sich", "bl\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Schemen, die aus Schatten sp\u00e4hen,", "tokens": ["Sche\u00b7men", ",", "die", "aus", "Schat\u00b7ten", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Freunde, lang schon aus dem Leben,", "tokens": ["Freun\u00b7de", ",", "lang", "schon", "aus", "dem", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Erd \u2013 und Himmel \u00fcbergeben.", "tokens": ["Erd", "\u2013", "und", "Him\u00b7mel", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcr das Herz voll tausend Wehen", "tokens": ["F\u00fcr", "das", "Herz", "voll", "tau\u00b7send", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es hier ein friedvoll Gehen \u2013", "tokens": ["Ist", "es", "hier", "ein", "fried\u00b7voll", "Ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr den Geist, den Schatten bannt,", "tokens": ["F\u00fcr", "den", "Geist", ",", "den", "Schat\u00b7ten", "bannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's ein paradiesisch Land!", "tokens": ["Ist's", "ein", "pa\u00b7ra\u00b7die\u00b7sisch", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wer wandert durch dies Grauen,", "tokens": ["Doch", "wer", "wan\u00b7dert", "durch", "dies", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wage niemals aufzuschauen,", "tokens": ["Wa\u00b7ge", "nie\u00b7mals", "auf\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie den schwachen Blick zu heben", "tokens": ["Nie", "den", "schwa\u00b7chen", "Blick", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In das Weben und das Beben,", "tokens": ["In", "das", "We\u00b7ben", "und", "das", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Senke das bewimpert Lid,", "tokens": ["Sen\u00b7ke", "das", "be\u00b7wim\u00b7pert", "Lid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df es kein Geheimnis sieht.", "tokens": ["Da\u00df", "es", "kein", "Ge\u00b7heim\u00b7nis", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "So des K\u00f6nigs Machtbefehle.", "tokens": ["So", "des", "K\u00f6\u00b7nigs", "Macht\u00b7be\u00b7feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und so darf die tr\u00fcbe Seele", "tokens": ["Und", "so", "darf", "die", "tr\u00fc\u00b7be", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hier nur im Vor\u00fcbergehen", "tokens": ["Hier", "nur", "im", "Vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Durch getr\u00fcbte Gl\u00e4ser sehen.", "tokens": ["Durch", "ge\u00b7tr\u00fcb\u00b7te", "Gl\u00e4\u00b7ser", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf Pfaden, dunkel, voller Grausen,", "token