{"textgrid.poem.32722": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der verliebte Bauer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so traurig, bin so still!", "tokens": ["Ich", "bin", "so", "trau\u00b7rig", ",", "bin", "so", "still", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein ganzer Mut ist hin!", "tokens": ["Mein", "gan\u00b7zer", "Mut", "ist", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn ach! die kleine Fieke will", "tokens": ["Denn", "ach", "!", "die", "klei\u00b7ne", "Fie\u00b7ke", "will"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir nimmer aus dem Sinn!", "tokens": ["Mir", "nim\u00b7mer", "aus", "dem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bald pfeif' ich was, bald sing' ich was,", "tokens": ["Bald", "pfeif'", "ich", "was", ",", "bald", "sing'", "ich", "was", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein' es doch nicht so;", "tokens": ["Und", "mein'", "es", "doch", "nicht", "so", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mache bei den Bauren Spa\u00df,", "tokens": ["Ich", "ma\u00b7che", "bei", "den", "Bau\u00b7ren", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bin doch nimmer froh.", "tokens": ["Und", "bin", "doch", "nim\u00b7mer", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist ein Leben voller Qual,", "tokens": ["Es", "ist", "ein", "Le\u00b7ben", "vol\u00b7ler", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man sich so vergafft,", "tokens": ["Wenn", "man", "sich", "so", "ver\u00b7gafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sich doch nicht ein einzigmal", "tokens": ["Und", "sich", "doch", "nicht", "ein", "ein\u00b7zig\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Reden Luft verschafft!", "tokens": ["Durch", "Re\u00b7den", "Luft", "ver\u00b7schafft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich wollt' es ihr so oft gestehn,", "tokens": ["Ich", "wollt'", "es", "ihr", "so", "oft", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hab' es nie gethan.", "tokens": ["Und", "hab'", "es", "nie", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist, als m\u00fc\u00dft' ich gleich vergehn,", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00dft'", "ich", "gleich", "ver\u00b7gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh' ich sie darum an.", "tokens": ["Seh'", "ich", "sie", "da\u00b7rum", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich h\u00e4rme sichtbarlich mich ab;", "tokens": ["Ich", "h\u00e4r\u00b7me", "sicht\u00b7bar\u00b7lich", "mich", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch Fieke tanzt und lacht!", "tokens": ["Doch", "Fie\u00b7ke", "tanzt", "und", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und senkte mich der Harm ins Grab,", "tokens": ["Und", "senk\u00b7te", "mich", "der", "Harm", "ins", "Grab", ","], "token_info": ["word"