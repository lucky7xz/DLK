{"dta.poem.10561": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Sturm .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00fcthet der Sturm,", "tokens": ["Es", "w\u00fct\u00b7het", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und er peitscht die Well'n,", "tokens": ["Und", "er", "peitscht", "die", "Well'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Wellen, wuthsch\u00e4umend und b\u00e4umend,", "tokens": ["Und", "die", "Wel\u00b7len", ",", "wuth\u00b7sch\u00e4u\u00b7mend", "und", "b\u00e4u\u00b7mend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Th\u00fcrmen sich auf, und es wogen lebendig", "tokens": ["Th\u00fcr\u00b7men", "sich", "auf", ",", "und", "es", "wo\u00b7gen", "le\u00b7ben\u00b7dig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PRF", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ADJD"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Die wei\u00dfen Wasserberge,", "tokens": ["Die", "wei\u00b7\u00dfen", "Was\u00b7ser\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und das Schifflein erklimmt sie", "tokens": ["Und", "das", "Schif\u00b7flein", "er\u00b7klimmt", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hastig m\u00fchsam,", "tokens": ["Has\u00b7tig", "m\u00fch\u00b7sam", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Und pl\u00f6tzlich st\u00fcrzt es hinab", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "st\u00fcrzt", "es", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "In schwarze, weitg\u00e4hnende Fluthabgr\u00fcnde \u2014", "tokens": ["In", "schwar\u00b7ze", ",", "weit\u00b7g\u00e4h\u00b7nen\u00b7de", "Fluth\u00b7ab\u00b7gr\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "O Meer!", "tokens": ["O", "Meer", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Mutter der Sch\u00f6nheit, der Schaumentstiegenen!", "tokens": ["Mut\u00b7ter", "der", "Sch\u00f6n\u00b7heit", ",", "der", "Schau\u00b7ment\u00b7stie\u00b7ge\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Gro\u00dfmutter der Liebe! schone meiner!", "tokens": ["Gro\u00df\u00b7mut\u00b7ter", "der", "Lie\u00b7be", "!", "scho\u00b7ne", "mei\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPOSAT", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon flattert, leichenwitternd,", "tokens": ["Schon", "flat\u00b7tert", ",", "lei\u00b7chen\u00b7wit\u00b7ternd", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die wei\u00dfe, gespenstische M\u00f6ve,", "tokens": ["Die", "wei\u00b7\u00dfe", ",", "ge\u00b7spens\u00b7ti\u00b7sche", "M\u00f6\u00b7ve", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und wetzt an dem Mastbaum den Schnabel", "tokens": ["Und", "wetzt", "an", "dem", "Mast\u00b7baum", "den", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und lechzt, voll Fra\u00dfbegier, nach dem Mund,", "tokens": ["Und", "lechzt", ",", "voll", "Fra\u00df\u00b7be\u00b7gier", ",", "nach", "dem", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der vom Ruhm deiner Tochter ert\u00f6nt,", "tokens": ["Der", "vom", "Ruhm", "dei\u00b7ner", "Toch\u00b7ter", "er\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und lechzt nach dem Herzen,", "tokens": ["Und", "lechzt", "nach", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das dein Enkel, der kleine Schalk,", "tokens": ["Das", "dein", "En\u00b7kel", ",", "der", "klei\u00b7ne", "Schalk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zum Spielzeug erw\u00e4hlt.", "tokens": ["Zum", "Spiel\u00b7zeug", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Vergebens mein Bitten und Flehn!", "tokens": ["Ver\u00b7ge\u00b7bens", "mein", "Bit\u00b7ten", "und", "Flehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mein Rufen verhallt im tosenden Sturm,", "tokens": ["Mein", "Ru\u00b7fen", "ver\u00b7hallt", "im", "to\u00b7sen\u00b7den", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Schlachtl\u00e4rm der Winde;", "tokens": ["Im", "Schlacht\u00b7l\u00e4rm", "der", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Es brau\u00dft und pfeift und prasselt und heult,", "tokens": ["Es", "brau\u00dft", "und", "pfeift", "und", "pras\u00b7selt", "und", "heult", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie ein Tollhaus von T\u00f6nen!", "tokens": ["Wie", "ein", "Toll\u00b7haus", "von", "T\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und zwischendurch h\u00f6r' ich vernehmbar", "tokens": ["Und", "zwi\u00b7schen\u00b7durch", "h\u00f6r'", "ich", "ver\u00b7nehm\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "VVFIN", "PPER", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Lockende Harfenlaute,", "tokens": ["Lo\u00b7cken\u00b7de", "Har\u00b7fen\u00b7lau\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Sehnsuchtwilden Gesang,", "tokens": ["Sehn\u00b7sucht\u00b7wil\u00b7den", "Ge\u00b7sang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Seelenschmelzend und seelenzerrei\u00dfend,", "tokens": ["See\u00b7len\u00b7schmel\u00b7zend", "und", "see\u00b7len\u00b7zer\u00b7rei\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Und ich erkenne die Stimme.", "tokens": ["Und", "ich", "er\u00b7ken\u00b7ne", "die", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Fern an schottischer Felsenk\u00fcste,", "tokens": ["Fern", "an", "schot\u00b7ti\u00b7scher", "Fel\u00b7sen\u00b7k\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo das graue Schl\u00f6\u00dflein hinausragt", "tokens": ["Wo", "das", "grau\u00b7e", "Schl\u00f6\u00df\u00b7lein", "hin\u00b7aus\u00b7ragt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ueber die brandende See,", "tokens": ["Ue\u00b7ber", "die", "bran\u00b7den\u00b7de", "See", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Dort am hochgew\u00f6lbten Fenster,", "tokens": ["Dort", "am", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Steht eine sch\u00f6ne, kranke Frau,", "tokens": ["Steht", "ei\u00b7ne", "sch\u00f6\u00b7ne", ",", "kran\u00b7ke", "Frau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zartdurchsichtig und marmorbla\u00df,", "tokens": ["Zart\u00b7durch\u00b7sich\u00b7tig", "und", "mar\u00b7mor\u00b7bla\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und sie spielt die Harfe und singt,", "tokens": ["Und", "sie", "spielt", "die", "Har\u00b7fe", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und der Wind durchw\u00fchlt ihre langen Locken,", "tokens": ["Und", "der", "Wind", "durch\u00b7w\u00fchlt", "ih\u00b7re", "lan\u00b7gen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und tr\u00e4gt ihr dunkles Lied", "tokens": ["Und", "tr\u00e4gt", "ihr", "dunk\u00b7les", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ueber das weite, st\u00fcrmende Meer.", "tokens": ["Ue\u00b7ber", "das", "wei\u00b7te", ",", "st\u00fcr\u00b7men\u00b7de", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}