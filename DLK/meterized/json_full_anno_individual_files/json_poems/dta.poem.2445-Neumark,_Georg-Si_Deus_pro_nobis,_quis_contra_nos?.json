{"dta.poem.2445": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Si Deus pro nobis, quis contra nos?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan man wei\u00df auch den nicht gnugsam zuerheben", "tokens": ["Man", "kan", "man", "wei\u00df", "auch", "den", "nicht", "gnug\u00b7sam", "zu\u00b7er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PIS", "VVFIN", "ADV", "ART", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Gott aus Liebe strafft; Dr\u00fcm wegre dich doch", "tokens": ["Den", "Gott", "aus", "Lie\u00b7be", "strafft", ";", "Dr\u00fcm", "weg\u00b7re", "dich", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "nicht/", "tokens": ["nicht", "/"], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$("], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Du mein betr\u00fcbtes Hertz wenn Gottes Angesicht", "tokens": ["Du", "mein", "be\u00b7tr\u00fcb\u00b7tes", "Hertz", "wenn", "Got\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dir etwas herbe scheint dich willig zu ergeben/", "tokens": ["Dir", "et\u00b7was", "her\u00b7be", "scheint", "dich", "wil\u00b7lig", "zu", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "VVFIN", "PRF", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Halt nur ged\u00fcltig aus halt felsenfesten Stand:", "tokens": ["Halt", "nur", "ge\u00b7d\u00fcl\u00b7tig", "aus", "halt", "fel\u00b7sen\u00b7fes\u00b7ten", "Stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADJD", "APPR", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn wenn die Linke schl\u00e4gt/ so heilt die rechte Hand.", "tokens": ["Denn", "wenn", "die", "Lin\u00b7ke", "schl\u00e4gt", "/", "so", "heilt", "die", "rech\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}