{"dta.poem.9624": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er sahe sie wider die gewohnheit bey noch  \n gantz fr\u00fchem morgen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Melinde/ deine gunst will mich zu fr\u00fch begl\u00fccken/", "tokens": ["Me\u00b7lin\u00b7de", "/", "dei\u00b7ne", "gunst", "will", "mich", "zu", "fr\u00fch", "be\u00b7gl\u00fc\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VMFIN", "PRF", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein schlumrend auge war vom schlaffe kaum erwacht/", "tokens": ["Mein", "schlum\u00b7rend", "au\u00b7ge", "war", "vom", "schlaf\u00b7fe", "kaum", "er\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "APPRART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da kont es allbereits an deiner pracht erblicken", "tokens": ["Da", "kont", "es", "all\u00b7be\u00b7reits", "an", "dei\u00b7ner", "pracht", "er\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein etwas/ so dich selbst der sonnen \u00e4hnlich macht.", "tokens": ["Ein", "et\u00b7was", "/", "so", "dich", "selbst", "der", "son\u00b7nen", "\u00e4hn\u00b7lich", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$(", "ADV", "PPER", "ADV", "ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sah\u2019 ein lodrend feur aus deinen augen blitzen/", "tokens": ["Ich", "sah'", "ein", "lod\u00b7rend", "feur", "aus", "dei\u00b7nen", "au\u00b7gen", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und einen solchen brand/ der sonnen nur geb\u00fchrt;", "tokens": ["Und", "ei\u00b7nen", "sol\u00b7chen", "brand", "/", "der", "son\u00b7nen", "nur", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "$(", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein feur/ das durch und durch die geister kan erhitzen/", "tokens": ["Ein", "feur", "/", "das", "durch", "und", "durch", "die", "geis\u00b7ter", "kan", "er\u00b7hit\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "KON", "APPR", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sternen-gleiche krafft in seinen strahlen f\u00fchrt.", "tokens": ["Und", "ster\u00b7nen\u00b7glei\u00b7che", "krafft", "in", "sei\u00b7nen", "strah\u00b7len", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hegt nun dein heisses aug\u2019 der sonnen glut und flammen/", "tokens": ["Hegt", "nun", "dein", "heis\u00b7ses", "aug'", "der", "son\u00b7nen", "glut", "und", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Trifft glantz und w\u00fcrckung selbst so reichlich bey dir ein/", "tokens": ["Trifft", "glantz", "und", "w\u00fcr\u00b7ckung", "selbst", "so", "reich\u00b7lich", "bey", "dir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "ADJD", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wirstu warlich nicht ein k\u00fchnes wort verdammen/", "tokens": ["So", "wirs\u00b7tu", "war\u00b7lich", "nicht", "ein", "k\u00fch\u00b7nes", "wort", "ver\u00b7dam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das dich mit h\u00f6chstem recht heist meine sonne seyn.", "tokens": ["Das", "dich", "mit", "h\u00f6chs\u00b7tem", "recht", "heist", "mei\u00b7ne", "son\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die sonne bringt den tag/ der tag giebt freud und leben;", "tokens": ["Die", "son\u00b7ne", "bringt", "den", "tag", "/", "der", "tag", "giebt", "freud", "und", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Melinde/ glaube mir/ du bist mein sonnen-licht/", "tokens": ["Me\u00b7lin\u00b7de", "/", "glau\u00b7be", "mir", "/", "du", "bist", "mein", "son\u00b7nen\u00b7licht", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Drum kanstu anders nichts als anmuth von dir geben/", "tokens": ["Drum", "kans\u00b7tu", "an\u00b7ders", "nichts", "als", "an\u00b7muth", "von", "dir", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PIS", "KOKOM", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn dein befeurter glantz durch beyde augen bricht.", "tokens": ["Wenn", "dein", "be\u00b7fe\u00b7ur\u00b7ter", "glantz", "durch", "bey\u00b7de", "au\u00b7gen", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Und wie der sonnen glut die gantze welt erhitzet/", "tokens": ["Und", "wie", "der", "son\u00b7nen", "glut", "die", "gant\u00b7ze", "welt", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und alle flammen auch durch ihre krafft besiegt;", "tokens": ["Und", "al\u00b7le", "flam\u00b7men", "auch", "durch", "ih\u00b7re", "krafft", "be\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So weistu/ wann dein feur auff unsre hertzen blitzet/", "tokens": ["So", "weis\u00b7tu", "/", "wann", "dein", "feur", "auff", "uns\u00b7re", "hert\u00b7zen", "blit\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWAV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df geist und seele selbst durch solchen brand erliegt.", "tokens": ["Da\u00df", "geist", "und", "see\u00b7le", "selbst", "durch", "sol\u00b7chen", "brand", "er\u00b7liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hat in der alten welt bey den beth\u00f6rten heyden", "tokens": ["Hat", "in", "der", "al\u00b7ten", "welt", "bey", "den", "be\u00b7th\u00f6r\u00b7ten", "hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Der sonnen/ als eim gott/ ein g\u00f6ttlich lob geh\u00f6rt;", "tokens": ["Der", "son\u00b7nen", "/", "als", "eim", "gott", "/", "ein", "g\u00f6tt\u00b7lich", "lob", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "ART", "NN", "$(", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Wie viel mit gr\u00f6sserm recht kan man auff erden leiden/", "tokens": ["Wie", "viel", "mit", "gr\u00f6s\u00b7serm", "recht", "kan", "man", "auff", "er\u00b7den", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN", "VMFIN", "PIS", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Da\u00df du als g\u00f6ttin wirst von aller welt verehrt.", "tokens": ["Da\u00df", "du", "als", "g\u00f6t\u00b7tin", "wirst", "von", "al\u00b7ler", "welt", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NE", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn la\u00df die sonne gleich den krey\u00df des himmels zieren/", "tokens": ["Denn", "la\u00df", "die", "son\u00b7ne", "gleich", "den", "krey\u00df", "des", "him\u00b7mels", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "La\u00df ihren glantz auch gehn weit \u00fcber sternen-pracht;", "tokens": ["La\u00df", "ih\u00b7ren", "glantz", "auch", "gehn", "weit", "\u00fc\u00b7ber", "ster\u00b7nen\u00b7pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So wird sie warlich dir auch nicht den ruhm entf\u00fchren/", "tokens": ["So", "wird", "sie", "war\u00b7lich", "dir", "auch", "nicht", "den", "ruhm", "ent\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df dich der himmel selbst zur irrdschen sonne macht.", "tokens": ["Da\u00df", "dich", "der", "him\u00b7mel", "selbst", "zur", "irrd\u00b7schen", "son\u00b7ne", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Genug! ich kan nicht wohl von deinen sonnen sprechen/", "tokens": ["Ge\u00b7nug", "!", "ich", "kan", "nicht", "wohl", "von", "dei\u00b7nen", "son\u00b7nen", "spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein sterblich auge wird durch solchen glantz verblendt;", "tokens": ["Ein", "sterb\u00b7lich", "au\u00b7ge", "wird", "durch", "sol\u00b7chen", "glantz", "ver\u00b7blendt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nur la\u00df ihr feuer nicht zu hefftig auff mich stechen/", "tokens": ["Nur", "la\u00df", "ihr", "feu\u00b7er", "nicht", "zu", "heff\u00b7tig", "auff", "mich", "ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sonst glaube f\u00fcr gewi\u00df/ da\u00df sich mein leben end.", "tokens": ["Sonst", "glau\u00b7be", "f\u00fcr", "ge\u00b7wi\u00df", "/", "da\u00df", "sich", "mein", "le\u00b7ben", "end", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "$(", "KOUS", "PRF", "PPOSAT", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}