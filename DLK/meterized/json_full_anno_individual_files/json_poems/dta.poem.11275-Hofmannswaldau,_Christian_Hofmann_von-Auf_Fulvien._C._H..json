{"dta.poem.11275": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Fulvien.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4gt die Fulvia die br\u00fcste ziemlich blos,", "tokens": ["Es", "tr\u00e4gt", "die", "Ful\u00b7via", "die", "br\u00fcs\u00b7te", "ziem\u00b7lich", "blos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ART", "ADJA", "ADV", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und \u00fcber di\u00df entbl\u00f6st sie heimlich auch die schos;", "tokens": ["Und", "\u00fc\u00b7ber", "di\u00df", "ent\u00b7bl\u00f6st", "sie", "heim\u00b7lich", "auch", "die", "schos", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum sagen viel gewi\u00df, da\u00df man um dieser sachen", "tokens": ["Drum", "sa\u00b7gen", "viel", "ge\u00b7wi\u00df", ",", "da\u00df", "man", "um", "die\u00b7ser", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr auch in kurtzer zeit den r\u00fccken blos wird machen.", "tokens": ["Ihr", "auch", "in", "kurt\u00b7zer", "zeit", "den", "r\u00fc\u00b7cken", "blos", "wird", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "ART", "VVFIN", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}