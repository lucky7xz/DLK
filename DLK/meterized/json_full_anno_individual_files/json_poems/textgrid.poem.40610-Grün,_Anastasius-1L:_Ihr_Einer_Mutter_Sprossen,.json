word", "word"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und mahnt mit strengem Sinn:", "tokens": ["Und", "mahnt", "mit", "stren\u00b7gem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbwas nie du kannst erreichen,", "tokens": ["\u00bb", "was", "nie", "du", "kannst", "er\u00b7rei\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Thor, la\u00df fahren hin!\u00ab", "tokens": ["Du", "Thor", ",", "la\u00df", "fah\u00b7ren", "hin", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "VVIMP", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Spruch sei hoch zu loben,", "tokens": ["Der", "Spruch", "sei", "hoch", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das B\u00fcrschlein unten schwor,", "tokens": ["Das", "B\u00fcr\u00b7schlein", "un\u00b7ten", "schwor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Blick doch blieb erhoben", "tokens": ["Sein", "Blick", "doch", "blieb", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Sternlein nach wie vor.", "tokens": ["Zum", "Stern\u00b7lein", "nac