{"textgrid.poem.36631": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Grenadier", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was macht der alte Grenadier?", "tokens": ["Was", "macht", "der", "al\u00b7te", "Gre\u00b7na\u00b7di\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der macht nicht viel, der sitzt", "tokens": ["Der", "macht", "nicht", "viel", ",", "der", "sitzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Ofen, ohne Schlachtbegier,", "tokens": ["Am", "O\u00b7fen", ",", "oh\u00b7ne", "Schlacht\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Kopf gest\u00fctzt!", "tokens": ["Den", "al\u00b7ten", "Kopf", "ge\u00b7st\u00fctzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der badet sein betagtes Haar", "tokens": ["Der", "ba\u00b7det", "sein", "be\u00b7tag\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einer Thr\u00e4nenflut!", "tokens": ["In", "ei\u00b7ner", "Thr\u00e4\u00b7nen\u00b7flut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-