{"dta.poem.20376": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ob Ludewig gr\u00f6sser sey als  \n Alexander.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man spricht/ da\u00df du so gro\u00df als Alexander bist/", "tokens": ["Man", "spricht", "/", "da\u00df", "du", "so", "gro\u00df", "als", "A\u00b7lex\u00b7an\u00b7der", "bist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "KOKOM", "NE", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und manche meynen gar da\u00df dieser kleiner ist.", "tokens": ["Und", "man\u00b7che", "mey\u00b7nen", "gar", "da\u00df", "die\u00b7ser", "klei\u00b7ner", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "KOUS", "PDAT", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun ist es zwar gewi\u00df. Ihr habet gleiche gaben/", "tokens": ["Nun", "ist", "es", "zwar", "ge\u00b7wi\u00df", ".", "Ihr", "ha\u00b7bet", "glei\u00b7che", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$.", "PPER", "VAFIN", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ehrsucht trifft bey dir wie bey den Griechen ein;", "tokens": ["Die", "ehr\u00b7sucht", "trifft", "bey", "dir", "wie", "bey", "den", "Grie\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PPER", "KOKOM", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch Alexander mu\u00df nothwendig gr\u00f6sser seyn:", "tokens": ["Doch", "A\u00b7lex\u00b7an\u00b7der", "mu\u00df", "noth\u00b7wen\u00b7dig", "gr\u00f6s\u00b7ser", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VMFIN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn jener hatte schon/ was du noch erst wilst haben.", "tokens": ["Denn", "je\u00b7ner", "hat\u00b7te", "schon", "/", "was", "du", "noch", "erst", "wilst", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "$(", "PWS", "PPER", "ADV", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}