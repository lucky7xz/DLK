{"textgrid.poem.58724": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Kreuze nur die Liebe triumphiert,", "tokens": ["Im", "Kreu\u00b7ze", "nur", "die", "Lie\u00b7be", "tri\u00b7um\u00b7phiert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo Schmerz, wo Dornen uns durchbohren,", "tokens": ["Wo", "Schmerz", ",", "wo", "Dor\u00b7nen", "uns", "durch\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So hat's mein Jesus selbst erkoren,", "tokens": ["So", "hat's", "mein", "Je\u00b7sus", "selbst", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ist der eigne Weg, wodurch Gott Seelen f\u00fchrt.", "tokens": ["Dies", "ist", "der", "eig\u00b7ne", "Weg", ",", "wo\u00b7durch", "Gott", "See\u00b7len", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie Jesus geht, so folgt ihm unverr\u00fcckt,", "tokens": ["Wie", "Je\u00b7sus", "geht", ",", "so", "folgt", "ihm", "un\u00b7ver\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "