{"dta.poem.21198": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welchem heiligen Entz\u00fccken", "tokens": ["Mit", "wel\u00b7chem", "hei\u00b7li\u00b7gen", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ich die Demuth nicht erblicken,", "tokens": ["Mu\u00df", "ich", "die", "De\u00b7muth", "nicht", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, Pilgrime, mit so viel Andacht spricht!", "tokens": ["Die", ",", "Pil\u00b7gri\u00b7me", ",", "mit", "so", "viel", "An\u00b7dacht", "spricht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Erhebt dann das Gesicht,", "tokens": ["Er\u00b7hebt", "dann", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und \u00fcberschaut erfreut", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7haut", "er\u00b7freut"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den Schauplatz der erhabensten Geschichte \u2014", "tokens": ["Den", "Schau\u00b7platz", "der", "er\u00b7ha\u00b7bens\u00b7ten", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Bestralt vom Sonnenlichte", "tokens": ["Be\u00b7stralt", "vom", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ragt Tabor dort aus dem Gew\u00f6lk hervor;", "tokens": ["Ragt", "Ta\u00b7bor", "dort", "aus", "dem", "Ge\u00b7w\u00f6lk", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Viel n\u00e4her streckt sein Haupt Moria hier empor!", "tokens": ["Viel", "n\u00e4\u00b7her", "streckt", "sein", "Haupt", "Mo\u00b7ria", "hier", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "NE", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Und unter ihm der Oelberg, dessen H\u00f6hen,", "tokens": ["Und", "un\u00b7ter", "ihm", "der", "O\u00b7el\u00b7berg", ",", "des\u00b7sen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NE", "$,", "PRELAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Messias, dich im blutgen Schwei\u00df gesehen.", "tokens": ["Mes\u00b7si\u00b7as", ",", "dich", "im", "blut\u00b7gen", "Schwei\u00df", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRF", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Gethsemane! die schw\u00e4rzste Mitternacht", "tokens": ["Geth\u00b7se\u00b7ma\u00b7ne", "!", "die", "schw\u00e4rz\u00b7ste", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Ward hier vom Gottmensch durchgewacht.", "tokens": ["Ward", "hier", "vom", "Gott\u00b7mensch", "durch\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier drang der Mordsucht Fackel auf ihn ein;", "tokens": ["Hier", "drang", "der", "Mord\u00b7sucht", "Fa\u00b7ckel", "auf", "ihn", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Den Misseth\u00e4tern gleich ward er hinweggebracht.", "tokens": ["Den", "Mis\u00b7set\u00b7h\u00e4\u00b7tern", "gleich", "ward", "er", "hin\u00b7weg\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und endlich starb der Fromme, der Gerechte,", "tokens": ["Und", "end\u00b7lich", "starb", "der", "From\u00b7me", ",", "der", "Ge\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Allhier auf Golgatha f\u00fcr S\u00fcnder und f\u00fcr Knechte.", "tokens": ["All\u00b7hier", "auf", "Gol\u00b7ga\u00b7tha", "f\u00fcr", "S\u00fcn\u00b7der", "und", "f\u00fcr", "Knech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}