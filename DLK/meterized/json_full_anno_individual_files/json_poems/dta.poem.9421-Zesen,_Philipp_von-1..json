{"dta.poem.9421": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.85", "pl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nste? wie? weistu nicht wo ich itzt weide?", "tokens": ["Sch\u00f6ns\u00b7te", "?", "wie", "?", "weis\u00b7tu", "nicht", "wo", "ich", "itzt", "wei\u00b7de", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "$.", "VVFIN", "PTKNEG", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ey so verf\u00fcge dich balde dort hin/", "tokens": ["Ey", "so", "ver\u00b7f\u00fc\u00b7ge", "dich", "bal\u00b7de", "dort", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Schaue die spuren auf breunlicher heyde/", "tokens": ["Schau\u00b7e", "die", "spu\u00b7ren", "auf", "breun\u00b7li\u00b7cher", "hey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Weide die Ziegen woselbsten ich bin.", "tokens": ["Wei\u00b7de", "die", "Zie\u00b7gen", "wo\u00b7selbs\u00b7ten", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "VAFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Ich gleiche der menge/", "tokens": ["Ich", "glei\u00b7che", "der", "men\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem Wagen-gepr\u00e4nge", "tokens": ["Dem", "Wa\u00b7gen\u00b7ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Des Pharao/ Dich:", "tokens": ["Des", "Pha\u00b7rao", "/", "Dich", ":"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Das Liebliche blicken", "tokens": ["Das", "Lieb\u00b7li\u00b7che", "bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "In g\u00fcldenen st\u00fccken", "tokens": ["In", "g\u00fcl\u00b7de\u00b7nen", "st\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Zwinget/ zu preisen die Liebeste/ Mich.", "tokens": ["Zwin\u00b7get", "/", "zu", "prei\u00b7sen", "die", "Lie\u00b7bes\u00b7te", "/", "Mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "PTKZU", "VVFIN", "ART", "NN", "$(", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Sihe! wie lieblich in g\u00fcldenen Spangen/", "tokens": ["Si\u00b7he", "!", "wie", "lieb\u00b7lich", "in", "g\u00fcl\u00b7de\u00b7nen", "Span\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00f6nste/ dein Backen-roth l\u00e4chlet erf\u00fcr/", "tokens": ["Sch\u00f6ns\u00b7te", "/", "dein", "Ba\u00b7cken\u00b7roth", "l\u00e4ch\u00b7let", "er\u00b7f\u00fcr", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VVFIN", "NE", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Schaue die Ketten am Halse dier prangen/", "tokens": ["Schau\u00b7e", "die", "Ket\u00b7ten", "am", "Hal\u00b7se", "dier", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Schaue dich selbsten in F\u00fcrstlicher zier.", "tokens": ["Schau\u00b7e", "dich", "selbs\u00b7ten", "in", "F\u00fcrst\u00b7li\u00b7cher", "zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wier la\u00dfen dier machen", "tokens": ["Wier", "la\u00b7\u00dfen", "dier", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Noch andere sachen", "tokens": ["Noch", "an\u00b7de\u00b7re", "sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zu mehren die Pracht;", "tokens": ["Zu", "meh\u00b7ren", "die", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Hinf\u00fcro sol prangen", "tokens": ["Hin\u00b7f\u00fc\u00b7ro", "sol", "pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Mit sch\u00f6neren Spangen", "tokens": ["Mit", "sch\u00f6\u00b7ne\u00b7ren", "Span\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Meine Geliebte/ nach der ich getracht.", "tokens": ["Mei\u00b7ne", "Ge\u00b7lieb\u00b7te", "/", "nach", "der", "ich", "ge\u00b7tracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}