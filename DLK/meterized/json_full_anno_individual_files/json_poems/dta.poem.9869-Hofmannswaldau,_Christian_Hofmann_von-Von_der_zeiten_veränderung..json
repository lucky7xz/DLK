{"dta.poem.9869": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der zeiten ver\u00e4nderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die zeiten m\u00fcssen sich vergleichen/", "tokens": ["Die", "zei\u00b7ten", "m\u00fcs\u00b7sen", "sich", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil alle zeiten fl\u00fcchtig seyn/", "tokens": ["Weil", "al\u00b7le", "zei\u00b7ten", "fl\u00fcch\u00b7tig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tritt wiederum der fr\u00fchling ein/", "tokens": ["Tritt", "wie\u00b7de\u00b7rum", "der", "fr\u00fch\u00b7ling", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So pflegt der winter ihm zu weichen/", "tokens": ["So", "pflegt", "der", "win\u00b7ter", "ihm", "zu", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vergeht des sommers heisser brand/", "tokens": ["Ver\u00b7geht", "des", "som\u00b7mers", "heis\u00b7ser", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So k\u00f6mmt des herbstes k\u00fchler stand.", "tokens": ["So", "k\u00f6mmt", "des", "herbs\u00b7tes", "k\u00fch\u00b7ler", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum wolt ihr die zeiten hassen/", "tokens": ["Wa\u00b7rum", "wolt", "ihr", "die", "zei\u00b7ten", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr/ die ihr seht der zeiten streit?", "tokens": ["Ihr", "/", "die", "ihr", "seht", "der", "zei\u00b7ten", "streit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil euch in unvergn\u00fcgsamkeit", "tokens": ["Weil", "euch", "in", "un\u00b7ver\u00b7gn\u00fcg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lentz/ sommer/ herbst und winter lassen/", "tokens": ["Lentz", "/", "som\u00b7mer", "/", "herbst", "und", "win\u00b7ter", "las\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADV", "$(", "ADV", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil aller zeiten lauff verrancht/", "tokens": ["Weil", "al\u00b7ler", "zei\u00b7ten", "lauff", "ver\u00b7rancht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eh\u2019 iemand ihrer recht gebraucht.", "tokens": ["Eh'", "ie\u00b7mand", "ih\u00b7rer", "recht", "ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was zeigt uns dieses wechsels schatten?", "tokens": ["Was", "zeigt", "uns", "die\u00b7ses", "wech\u00b7sels", "schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein bild der unbest\u00e4ndigkeit;", "tokens": ["Ein", "bild", "der", "un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein werck der unvollkommenheit:", "tokens": ["Ein", "werck", "der", "un\u00b7voll\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil nicht der himmel will verstatten/", "tokens": ["Weil", "nicht", "der", "him\u00b7mel", "will", "ver\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df etwas hier soll ewig stehn/", "tokens": ["Da\u00df", "et\u00b7was", "hier", "soll", "e\u00b7wig", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das mit den zeiten soll vergehn.", "tokens": ["Das", "mit", "den", "zei\u00b7ten", "soll", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Jhr \u00e4ndert ja der zeiten s\u00f6hne/", "tokens": ["Ihr", "\u00e4n\u00b7dert", "ja", "der", "zei\u00b7ten", "s\u00f6h\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der zeiten t\u00f6chter/ allezeit/", "tokens": ["Der", "zei\u00b7ten", "t\u00f6ch\u00b7ter", "/", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Indem ihr macht aus freude leid/", "tokens": ["In\u00b7dem", "ihr", "macht", "aus", "freu\u00b7de", "leid", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was gleich ist/ krum; was arg ist/ sch\u00f6ne;", "tokens": ["Was", "gleich", "ist", "/", "krum", ";", "was", "arg", "ist", "/", "sch\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$(", "VVIMP", "$.", "PWS", "ADJD", "VAFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was starck ist/ schwach; was kalt ist/ hei\u00df;", "tokens": ["Was", "starck", "ist", "/", "schwach", ";", "was", "kalt", "ist", "/", "hei\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$(", "VVFIN", "$.", "PWS", "ADJD", "VAFIN", "$(", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was klein ist/ gro\u00df; was schwartz ist/ wei\u00df.", "tokens": ["Was", "klein", "ist", "/", "gro\u00df", ";", "was", "schwartz", "ist", "/", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$(", "ADJD", "$.", "PWS", "ADJD", "VAFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein jahr ist unser gantzes leben:", "tokens": ["Ein", "jahr", "ist", "un\u00b7ser", "gant\u00b7zes", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der fr\u00fchling h\u00e4lt der jugend art/", "tokens": ["Der", "fr\u00fch\u00b7ling", "h\u00e4lt", "der", "ju\u00b7gend", "art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sommer/ der verliebte paart/", "tokens": ["Der", "som\u00b7mer", "/", "der", "ver\u00b7lieb\u00b7te", "paart", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan sch\u00f6ne frucht zu wiegen geben/", "tokens": ["Kan", "sch\u00f6\u00b7ne", "frucht", "zu", "wie\u00b7gen", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der herbst nimt an den kr\u00e4fften ab/", "tokens": ["Der", "herbst", "nimt", "an", "den", "kr\u00e4ff\u00b7ten", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der winter legt uns in das grab.", "tokens": ["Der", "win\u00b7ter", "legt", "uns", "in", "das", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}