{"dta.poem.5810": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es senckte GOTT, doch in gewisser Maasse nur,", "tokens": ["Es", "senck\u00b7te", "GoTT", ",", "doch", "in", "ge\u00b7wis\u00b7ser", "Maas\u00b7se", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ADV", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In die Materie Ruh und Bewegung ein.", "tokens": ["In", "die", "Ma\u00b7te\u00b7rie", "Ruh", "und", "Be\u00b7we\u00b7gung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und Seine Macht verschafft, da\u00df sie in der Natur", "tokens": ["Und", "Sei\u00b7ne", "Macht", "ver\u00b7schafft", ",", "da\u00df", "sie", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In steter Meng und Gleichheit seyn.", "tokens": ["In", "ste\u00b7ter", "Meng", "und", "Gleich\u00b7heit", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kan man leicht hiebey entdecken", "tokens": ["Doch", "kan", "man", "leicht", "hie\u00b7bey", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df, da so viele Ding all\u2019Augenblick sich \u00e4ndern,", "tokens": ["Da\u00df", ",", "da", "so", "vie\u00b7le", "Ding", "all'\u00b7Au\u00b7gen\u00b7blick", "sich", "\u00e4n\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ADV", "PIAT", "NN", "PIAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch ihr Bewegen sich auf andre m\u00fc\u00df\u2019 erstrecken.", "tokens": ["Auch", "ihr", "Be\u00b7we\u00b7gen", "sich", "auf", "and\u00b7re", "m\u00fc\u00df'", "er\u00b7stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "APPR", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein C\u00f6rper giebt es bald, empf\u00e4nget es auch leicht,", "tokens": ["Ein", "C\u00f6r\u00b7per", "giebt", "es", "bald", ",", "emp\u00b7f\u00e4n\u00b7get", "es", "auch", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da er denn stets so viel verlieret,", "tokens": ["Da", "er", "denn", "stets", "so", "viel", "ver\u00b7lie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als wie er einen andern reicht.", "tokens": ["Als", "wie", "er", "ei\u00b7nen", "an\u00b7dern", "reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch diesen lieblichen Zusammenklang,", "tokens": ["Durch", "die\u00b7sen", "lieb\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.12": {"text": "Und herrlichen Zusammenhang", "tokens": ["Und", "herr\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7hang"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wird stets in der Natur derselbe Stand versp\u00fcret.", "tokens": ["Wird", "stets", "in", "der", "Na\u00b7tur", "der\u00b7sel\u00b7be", "Stand", "ver\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man sieht, da\u00df alles sich ver\u00e4ndert und vertreibet,", "tokens": ["Man", "sieht", ",", "da\u00df", "al\u00b7les", "sich", "ver\u00b7\u00e4n\u00b7dert", "und", "ver\u00b7trei\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man sieht, da\u00df alles immer bleibet.", "tokens": ["Man", "sieht", ",", "da\u00df", "al\u00b7les", "im\u00b7mer", "blei\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}