{"textgrid.poem.65727": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von sch\u00f6nsten Wesen w\u00fcnschen wir Vermehrung,", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von sch\u00f6nsten Wesen w\u00fcnschen wir Vermehrung,", "tokens": ["Von", "sch\u00f6ns\u00b7ten", "We\u00b7sen", "w\u00fcn\u00b7schen", "wir", "Ver\u00b7meh\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Damit der Sch\u00f6nheit Ros' unsterblich sei,", "tokens": ["Da\u00b7mit", "der", "Sch\u00f6n\u00b7heit", "Ros'", "uns\u00b7terb\u00b7lich", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NE", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, wenn das Reife stirbt durch Zeitverheerung,", "tokens": ["Und", ",", "wenn", "das", "Rei\u00b7fe", "stirbt", "durch", "Zeit\u00b7ver\u00b7hee\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Bild in zarten Erben sich erneu'.", "tokens": ["Sein", "Bild", "in", "zar\u00b7ten", "Er\u00b7ben", "sich", "er\u00b7neu'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch du, in eigner Augen Schein begn\u00fcgt,", "tokens": ["Doch", "du", ",", "in", "eig\u00b7ner", "Au\u00b7gen", "Schein", "be\u00b7gn\u00fcgt", ","