{"dta.poem.10176": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Eis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch gestern deckt', als wie ein Spiegel-Glas,", "tokens": ["Noch", "ge\u00b7stern", "deckt'", ",", "als", "wie", "ein", "Spie\u00b7gel\u00b7Glas", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.2": {"text": "Enteis\u2019ter Alster-Flu\u00df, das Eis dein reges Na\u00df;", "tokens": ["Ent\u00b7eis'\u00b7ter", "Als\u00b7ter\u00b7Flu\u00df", ",", "das", "Eis", "dein", "re\u00b7ges", "Na\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heut\u2019 ist es weg, verschwunden, und sein prangen,", "tokens": ["Heut'", "ist", "es", "weg", ",", "ver\u00b7schwun\u00b7den", ",", "und", "sein", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,", "KON", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4r\u2019 es nie gewesen, gantz vergangen?", "tokens": ["Als", "w\u00e4r'", "es", "nie", "ge\u00b7we\u00b7sen", ",", "gantz", "ver\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VAPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Indem ich also bey mir fragte;", "tokens": ["In\u00b7dem", "ich", "al\u00b7so", "bey", "mir", "frag\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam es mir f\u00fcr,", "tokens": ["Kam", "es", "mir", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPR", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Als ob die Alster mir", "tokens": ["Als", "ob", "die", "Als\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur Antwort sagte:", "tokens": ["Zur", "Ant\u00b7wort", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}