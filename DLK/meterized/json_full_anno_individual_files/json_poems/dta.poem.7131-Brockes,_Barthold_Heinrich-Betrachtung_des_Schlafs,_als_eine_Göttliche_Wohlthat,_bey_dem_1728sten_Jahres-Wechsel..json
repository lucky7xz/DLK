{"dta.poem.7131": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns zeigt demnach ein Traum weit mehr, als man gedencket;", "tokens": ["Uns", "zeigt", "dem\u00b7nach", "ein", "Traum", "weit", "mehr", ",", "als", "man", "ge\u00b7den\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "NN", "ADJD", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verdienet folglich auch, da\u00df man Verstand und Witz", "tokens": ["Ver\u00b7die\u00b7net", "folg\u00b7lich", "auch", ",", "da\u00df", "man", "Ver\u00b7stand", "und", "Witz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "$,", "KOUS", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf ihn ein wenig mehr, als man gewohnt ist, lencket,", "tokens": ["Auf", "ihn", "ein", "we\u00b7nig", "mehr", ",", "als", "man", "ge\u00b7wohnt", "ist", ",", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "PIS", "ADV", "$,", "KOUS", "PIS", "VVPP", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil ihr Wesen uns mehr, als man glaubet, n\u00fctz.", "tokens": ["Die\u00b7weil", "ihr", "We\u00b7sen", "uns", "mehr", ",", "als", "man", "glau\u00b7bet", ",", "n\u00fctz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}