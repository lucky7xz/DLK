{"textgrid.poem.52202": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ode, welche den 9. Sept. abgesungen worden, als den 12. darauf der gl\u00fcckliche Entsatz von Wien erfolgte", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilf, Himmel! welch' ein grauses Schrecken", "tokens": ["Hilf", ",", "Him\u00b7mel", "!", "welch'", "ein", "grau\u00b7ses", "Schre\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht unser werthes Vaterland", "tokens": ["Sucht", "un\u00b7ser", "wert\u00b7hes", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Rauch und Flammen zu bedecken,", "tokens": ["Mit", "Rauch", "und", "Flam\u00b7men", "zu", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sp\u00fcre nichts als Mord und Brand.", "tokens": ["Ich", "sp\u00fc\u00b7re", "nichts", "als", "Mord", "und", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo ist der Friede hingeflogen?", "tokens": ["Wo", "ist", "der", "Frie\u00b7de", "hin\u00b7ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat denn der Scythen Pfeil' und Bogen,", "tokens": ["Hat", "denn", "der", "Scyt\u00b7hen", "Pfeil'", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Mechmets ungeheures Schwerd", "tokens": ["Und", "Mech\u00b7mets", "un\u00b7ge\u00b7heu\u00b7res", "Schwerd"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der L\u00e4nder Sicherheit verzehrt.", "tokens": ["Der", "L\u00e4n\u00b7der", "Si\u00b7cher\u00b7heit", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach H\u00f6chster! dem die Seraphinen", "tokens": ["Ach", "H\u00f6chs\u00b7ter", "!", "dem", "die", "Se\u00b7ra\u00b7phi\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "ART", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Zittern zu Gebote stehn,", "tokens": ["Mit", "Zit\u00b7tern", "zu", "Ge\u00b7bo\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sol Deutschland frembden V\u00f6lckern dienen,", "tokens": ["Sol", "Deutschland", "fremb\u00b7den", "V\u00f6l\u00b7ckern", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und k\u00fcnftig schwer gefesselt gehn?", "tokens": ["Und", "k\u00fcnf\u00b7tig", "schwer", "ge\u00b7fes\u00b7selt", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was s\u00e4umen sich die Legionen,", "tokens": ["Was", "s\u00e4u\u00b7men", "sich", "die", "Le\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die in dem hohen Himmel wohnen?", "tokens": ["Die", "in", "dem", "ho\u00b7hen", "Him\u00b7mel", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nimt denn dein' unverk\u00fcrtzte Macht", "tokens": ["Nimt", "denn", "dein'", "un\u00b7ver\u00b7k\u00fcrtz\u00b7te", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Christen Winseln nicht in acht?", "tokens": ["Der", "Chris\u00b7ten", "Win\u00b7seln", "nicht", "in", "acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du h\u00f6rst es wol: doch unsre S\u00fcnden,", "tokens": ["Du", "h\u00f6rst", "es", "wol", ":", "doch", "uns\u00b7re", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die deines Zornes Feur erregt,", "tokens": ["Die", "dei\u00b7nes", "Zor\u00b7nes", "Feur", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und t\u00e4glich deinen Grimm entz\u00fcnden,", "tokens": ["Und", "t\u00e4g\u00b7lich", "dei\u00b7nen", "Grimm", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind Ursach, da\u00df der Feind uns schl\u00e4gt!", "tokens": ["Sind", "Ur\u00b7sach", ",", "da\u00df", "der", "Feind", "uns", "schl\u00e4gt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Uppigkeit, die geilen L\u00fcste", "tokens": ["Die", "Up\u00b7pig\u00b7keit", ",", "die", "gei\u00b7len", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vollf\u00fchren izt die Traur-Ger\u00fcste,", "tokens": ["Voll\u00b7f\u00fch\u00b7ren", "izt", "die", "Traur\u00b7Ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und unser Stolz und Ubermutt", "tokens": ["Und", "un\u00b7ser", "Stolz", "und", "U\u00b7ber\u00b7mutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geust Oel in diese Krieges-Glutt.", "tokens": ["Geust", "O\u00b7el", "in", "die\u00b7se", "Krie\u00b7ge\u00b7sGlutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sol der Himmel H\u00fclffe senden,", "tokens": ["Wie", "sol", "der", "Him\u00b7mel", "H\u00fclf\u00b7fe", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Indem man Schuld auf Schulden h\u00e4uft?", "tokens": ["In\u00b7dem", "man", "Schuld", "auf", "Schul\u00b7den", "h\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sol er Straf' und Plagen wenden,", "tokens": ["Wie", "sol", "er", "Straf'", "und", "Pla\u00b7gen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ieder ins Verderben l\u00e4uft.", "tokens": ["Wenn", "ie\u00b7der", "ins", "Ver\u00b7der\u00b7ben", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach Deutschland wasche dich mit Z\u00e4hren,", "tokens": ["Ach", "Deutschland", "wa\u00b7sche", "dich", "mit", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wofern dir Gott sol Heil gew\u00e4hren,", "tokens": ["Wo\u00b7fern", "dir", "Gott", "sol", "Heil", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zeuch deinen Hochmutts-Purpur aus,", "tokens": ["Zeuch", "dei\u00b7nen", "Hoch\u00b7mutts\u00b7Pur\u00b7pur", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und lege dich in Asch und Graus.", "tokens": ["Und", "le\u00b7ge", "dich", "in", "Asch", "und", "Graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Komm, falle mit geschwinder Busse,", "tokens": ["Komm", ",", "fal\u00b7le", "mit", "ge\u00b7schwin\u00b7der", "Bus\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern du noch zu retten bist,", "tokens": ["Wo\u00b7fern", "du", "noch", "zu", "ret\u00b7ten", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem dreymal grossen Gott zu Fusse,", "tokens": ["Dem", "drey\u00b7mal", "gros\u00b7sen", "Gott", "zu", "Fus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bitt' umb eine kurtze Frist.", "tokens": ["Und", "bitt'", "umb", "ei\u00b7ne", "kurt\u00b7ze", "Frist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, zeig' ihm seines Sohnes Wunden,", "tokens": ["Komm", ",", "zeig'", "ihm", "sei\u00b7nes", "Soh\u00b7nes", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch die er sich mit dir verbunden,", "tokens": ["Durch", "die", "er", "sich", "mit", "dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Jesus dich nicht sch\u00fctzen kan,", "tokens": ["Wo", "Je\u00b7sus", "dich", "nicht", "sch\u00fct\u00b7zen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So ist es bald mit dir gethan.", "tokens": ["So", "ist", "es", "bald", "mit", "dir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er kan, Er wil, Er wird auch sch\u00fctzen,", "tokens": ["Er", "kan", ",", "Er", "wil", ",", "Er", "wird", "auch", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wofern du dich nur bessern magst,", "tokens": ["Wo\u00b7fern", "du", "dich", "nur", "bes\u00b7sern", "magst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird die schwachen Kr\u00e4fte st\u00fctzen,", "tokens": ["Er", "wird", "die", "schwa\u00b7chen", "Kr\u00e4f\u00b7te", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern du zeitlich nach ihm fragst.", "tokens": ["Wo\u00b7fern", "du", "zeit\u00b7lich", "nach", "ihm", "fragst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn alle, die den Herren lieben,", "tokens": ["Denn", "al\u00b7le", ",", "die", "den", "Her\u00b7ren", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die kan kein Feind, kein Schwerd betr\u00fcben.", "tokens": ["Die", "kan", "kein", "Feind", ",", "kein", "Schwerd", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie tretten Kummer und Verdru\u00df", "tokens": ["Sie", "tret\u00b7ten", "Kum\u00b7mer", "und", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gro\u00dfm\u00fcttig unter ihren Fu\u00df.", "tokens": ["Gro\u00df\u00b7m\u00fct\u00b7tig", "un\u00b7ter", "ih\u00b7ren", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Jesu, dessen hohen Nahmen,", "tokens": ["Herr", "Je\u00b7su", ",", "des\u00b7sen", "ho\u00b7hen", "Nah\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Saracene h\u00f6hnen wil,", "tokens": ["Der", "Sa\u00b7ra\u00b7ce\u00b7ne", "h\u00f6h\u00b7nen", "wil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerstreu den ungerathnen Saamen,", "tokens": ["Zer\u00b7streu", "den", "un\u00b7ge\u00b7rath\u00b7nen", "Saa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und setze seinem Grimm ein Ziel.", "tokens": ["Und", "set\u00b7ze", "sei\u00b7nem", "Grimm", "ein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm, Jesus! h\u00f6r' aus unser Sehnen,", "tokens": ["Komm", ",", "Je\u00b7sus", "!", "h\u00f6r'", "aus", "un\u00b7ser", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, trockne deines Volckes Thr\u00e4nen;", "tokens": ["Komm", ",", "trock\u00b7ne", "dei\u00b7nes", "Vol\u00b7ckes", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"