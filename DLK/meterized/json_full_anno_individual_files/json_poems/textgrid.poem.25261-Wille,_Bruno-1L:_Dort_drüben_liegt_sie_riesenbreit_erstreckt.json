{"textgrid.poem.25261": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort dr\u00fcben liegt sie/ riesenbreit erstreckt", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort dr\u00fcben liegt sie/ riesenbreit erstreckt", "tokens": ["Dort", "dr\u00fc\u00b7ben", "liegt", "sie", "/", "rie\u00b7sen\u00b7breit", "er\u00b7streckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$(", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und vielgezackt zum Wolkengrau gereckt:", "tokens": ["Und", "viel\u00b7ge\u00b7zackt", "zum", "Wol\u00b7ken\u00b7grau", "ge\u00b7reckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die steinern fahle Stadt/ von hunderttausend", "tokens": ["Die", "stei\u00b7nern", "fah\u00b7le", "Stadt", "/", "von", "hun\u00b7dert\u00b7tau\u00b7send"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "APPR", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tagwerken murrend und erbrausend.", "tokens": ["Tag\u00b7wer\u00b7ken", "mur\u00b7rend", "und", "er\u00b7brau\u00b7send", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein Dunst umh\u00fcllt die D\u00e4cher, ru\u00dfig, bleiern:", "tokens": ["Ein", "Dunst", "um\u00b7h\u00fcllt", "die", "D\u00e4\u00b7cher", ",", "ru\u00b7\u00dfig", ",", "blei\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Schlote Ausgeburt/ die noch nicht feiern.", "tokens": ["Der", "Schlo\u00b7te", "Aus\u00b7ge\u00b7burt", "/", "die", "noch", "nicht", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und doch schon murmeln von der Vesperstunde", "tokens": ["Und", "doch", "schon", "mur\u00b7meln", "von", "der", "Ves\u00b7per\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die d\u00fcstern T\u00fcrme mit dem Glockenmunde.", "tokens": ["Die", "d\u00fcs\u00b7tern", "T\u00fcr\u00b7me", "mit", "dem", "Glo\u00b7cken\u00b7mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie dort der H\u00e4userwall, der Vorstadt-Rumpf,", "tokens": ["Wie", "dort", "der", "H\u00e4u\u00b7ser\u00b7wall", ",", "der", "Vor\u00b7stadt\u00b7Rumpf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus f\u00fcnfgezeilten Fenstern stumpf", "tokens": ["Aus", "f\u00fcnf\u00b7ge\u00b7zeil\u00b7ten", "Fens\u00b7tern", "stumpf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Her\u00fcberstarrt zum braunen Ackergrund,", "tokens": ["Her\u00b7\u00fc\u00b7bers\u00b7tarrt", "zum", "brau\u00b7nen", "A\u00b7cker\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo, schmutzigrot die Mauern,", "tokens": ["Wo", ",", "schmut\u00b7zi\u00b7grot", "die", "Mau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zwei qualmende Fabriken kauern.", "tokens": ["Zwei", "qual\u00b7men\u00b7de", "Fab\u00b7ri\u00b7ken", "kau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Horch, die Maschine heult das Vesperzeichen.", "tokens": ["Horch", ",", "die", "Ma\u00b7schi\u00b7ne", "heult", "das", "Ves\u00b7per\u00b7zei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Da rinnt aus dem Fabrikentor", "tokens": ["Da", "rinnt", "aus", "dem", "Fab\u00b7ri\u00b7ken\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein langer Zug von Arbeitsvolk", "tokens": ["Ein", "lan\u00b7ger", "Zug", "von", "Ar\u00b7beits\u00b7volk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Ackerweg dahin, zur Stadt.", "tokens": ["Den", "A\u00b7cker\u00b7weg", "da\u00b7hin", ",", "zur", "Stadt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sieh, die H\u00e4userstirnen r\u00f6tet matt", "tokens": ["Und", "sieh", ",", "die", "H\u00e4u\u00b7ser\u00b7stir\u00b7nen", "r\u00f6\u00b7tet", "matt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Abendwolken Widerschein.", "tokens": ["Der", "Ab\u00b7end\u00b7wol\u00b7ken", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf einmal quillt der Feuerball herein", "tokens": ["Auf", "ein\u00b7mal", "quillt", "der", "Feu\u00b7er\u00b7ball", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus einem Wolkenri\u00df und \u00fcberflutet", "tokens": ["Aus", "ei\u00b7nem", "Wol\u00b7ken\u00b7ri\u00df", "und", "\u00fc\u00b7berf\u00b7lu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Landschaft, da\u00df sie golden glutet.", "tokens": ["Die", "Land\u00b7schaft", ",", "da\u00df", "sie", "gol\u00b7den", "glu\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Zaubertat! Die Stadt mit ihrem Dunst", "tokens": ["O", "Zau\u00b7ber\u00b7tat", "!", "Die", "Stadt", "mit", "ih\u00b7rem", "Dunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Liegt nun verkl\u00e4rt, von Purpurduft umflossen:", "tokens": ["Liegt", "nun", "ver\u00b7kl\u00e4rt", ",", "von", "Pur\u00b7pur\u00b7duft", "um\u00b7flos\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein H\u00fcgel, drum in ungest\u00fcmer Brunst,", "tokens": ["Ein", "H\u00fc\u00b7gel", ",", "drum", "in", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Brunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus grauem Dorn, blutrote Rosen sprossen.", "tokens": ["Aus", "grau\u00b7em", "Dorn", ",", "blut\u00b7ro\u00b7te", "Ro\u00b7sen", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und sieh nur, wie die Scheibenzeilen strahlen,", "tokens": ["Und", "sieh", "nur", ",", "wie", "die", "Schei\u00b7ben\u00b7zei\u00b7len", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit rotem Blitz das Sonnenfeuer malen!", "tokens": ["Mit", "ro\u00b7tem", "Blitz", "das", "Son\u00b7nen\u00b7feu\u00b7er", "ma\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie alle H\u00e4user, alle Fensteraugen,", "tokens": ["Wie", "al\u00b7le", "H\u00e4u\u00b7ser", ",", "al\u00b7le", "Fens\u00b7ter\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit hei\u00dfem Durst die Purpurquelle saugen", "tokens": ["Mit", "hei\u00b7\u00dfem", "Durst", "die", "Pur\u00b7pur\u00b7quel\u00b7le", "sau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und saugend immer lichter sich verkl\u00e4ren/", "tokens": ["Und", "sau\u00b7gend", "im\u00b7mer", "lich\u00b7ter", "sich", "ver\u00b7kl\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJD", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ob sie fluchbeladne Schl\u00f6sser w\u00e4ren,", "tokens": ["Als", "ob", "sie", "fluch\u00b7be\u00b7lad\u00b7ne", "Schl\u00f6s\u00b7ser", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die f\u00fcr ein karges Weilchen von der b\u00f6sen", "tokens": ["Die", "f\u00fcr", "ein", "kar\u00b7ges", "Weil\u00b7chen", "von", "der", "b\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verw\u00fcnschung sich erl\u00f6sen.", "tokens": ["Ver\u00b7w\u00fcn\u00b7schung", "sich", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und sie betrachtend voller Staunen,", "tokens": ["Und", "sie", "be\u00b7trach\u00b7tend", "vol\u00b7ler", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r ich die H\u00e4user gramvoll raunen:", "tokens": ["H\u00f6r", "ich", "die", "H\u00e4u\u00b7ser", "gram\u00b7voll", "rau\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbverwunschene Schl\u00f6sser, verfluchte Mauern,", "tokens": ["\u00bb", "ver\u00b7wun\u00b7sche\u00b7ne", "Schl\u00f6s\u00b7ser", ",", "ver\u00b7fluch\u00b7te", "Mau\u00b7ern", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach wohl, das sind wir! M\u00fcssen ja trauern", "tokens": ["Ach", "wohl", ",", "das", "sind", "wir", "!", "M\u00fcs\u00b7sen", "ja", "trau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "PDS", "VAFIN", "PPER", "$.", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In d\u00fcstrer \u00d6de jahraus jahrein,", "tokens": ["In", "d\u00fcst\u00b7rer", "\u00d6\u00b7de", "ja\u00b7hraus", "ja\u00b7hrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hilfloses Grauen im lahmen Gebein.", "tokens": ["Hilf\u00b7lo\u00b7ses", "Grau\u00b7en", "im", "lah\u00b7men", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Durch Kerkerr\u00e4ume Gespenster poltern,", "tokens": ["Durch", "Ker\u00b7ker\u00b7r\u00e4u\u00b7me", "Ge\u00b7spens\u00b7ter", "pol\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Viel arme Menschenseelen zu foltern,", "tokens": ["Viel", "ar\u00b7me", "Men\u00b7schen\u00b7see\u00b7len", "zu", "fol\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit teuflischen Zangen, mit D\u00fcrsten und Fasten,", "tokens": ["Mit", "teuf\u00b7li\u00b7schen", "Zan\u00b7gen", ",", "mit", "D\u00fcrs\u00b7ten", "und", "Fas\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Mit knechtischen Ketten, unmenschlichen Lasten.", "tokens": ["Mit", "knech\u00b7ti\u00b7schen", "Ket\u00b7ten", ",", "un\u00b7menschli\u00b7chen", "Las\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Auf faulem Stroh die Armut kauert,", "tokens": ["Auf", "fau\u00b7lem", "Stroh", "die", "Ar\u00b7mut", "kau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verzehrt von Fieber und frostdurchschauert;", "tokens": ["Ver\u00b7zehrt", "von", "Fie\u00b7ber", "und", "frost\u00b7durch\u00b7schau\u00b7ert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Auge irrt,", "tokens": ["Das", "Au\u00b7ge", "irrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es ringen die H\u00e4nde.", "tokens": ["Es", "rin\u00b7gen", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch fledermausig", "tokens": ["Doch", "fle\u00b7der\u00b7mau\u00b7sig"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Sorge schwirrt", "tokens": ["Die", "Sor\u00b7ge", "schwirrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Um unsere grausig", "tokens": ["Um", "un\u00b7se\u00b7re", "grau\u00b7sig"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Verdammten W\u00e4nde ...", "tokens": ["Ver\u00b7damm\u00b7ten", "W\u00e4n\u00b7de", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Fluch und kein Ende!", "tokens": ["Fluch", "und", "kein", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.r