{"dta.poem.10535": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "In dem Dome zu Corduva", "tokens": ["In", "dem", "Do\u00b7me", "zu", "Cor\u00b7du\u00b7va"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Stehen S\u00e4ulen, dreizehnhundert,", "tokens": ["Ste\u00b7hen", "S\u00e4u\u00b7len", ",", "drei\u00b7zehn\u00b7hun\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "CARD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dreizehnhundert Riesens\u00e4ulen", "tokens": ["Drei\u00b7zehn\u00b7hun\u00b7dert", "Rie\u00b7sen\u00b7s\u00e4u\u00b7len"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tragen die gewalt'ge Kuppel.", "tokens": ["Tra\u00b7gen", "die", "ge\u00b7walt'\u00b7ge", "Kup\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und auf S\u00e4ulen, Kuppel, W\u00e4nden,", "tokens": ["Und", "auf", "S\u00e4u\u00b7len", ",", "Kup\u00b7pel", ",", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn von oben sich bis unten", "tokens": ["Ziehn", "von", "o\u00b7ben", "sich", "bis", "un\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "PRF", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Corans arab'sche Spr\u00fcche,", "tokens": ["Des", "Co\u00b7rans", "a\u00b7rab'\u00b7sche", "Spr\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Klug und blumenhaft verschlungen.", "tokens": ["Klug", "und", "blu\u00b7men\u00b7haft", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mohrenk\u00f6n'ge bauten weiland", "tokens": ["Moh\u00b7ren\u00b7k\u00f6n'\u00b7ge", "bau\u00b7ten", "wei\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Haus zu Allahs Ruhme,", "tokens": ["Die\u00b7ses", "Haus", "zu", "Al\u00b7lahs", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch hat Alles sich verwandelt", "tokens": ["Doch", "hat", "Al\u00b7les", "sich", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "PRF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Zeiten dunkelm Strudel.", "tokens": ["In", "der", "Zei\u00b7ten", "dun\u00b7kelm", "Stru\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf dem Thurme, wo der Th\u00fcrmer", "tokens": ["Auf", "dem", "Thur\u00b7me", ",", "wo", "der", "Th\u00fcr\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zum Gebete aufgerufen,", "tokens": ["Zum", "Ge\u00b7be\u00b7te", "auf\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt sich jetzt der Christenglocken", "tokens": ["Hebt", "sich", "jetzt", "der", "Chris\u00b7ten\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Melancholisches Gesumme.", "tokens": ["Me\u00b7lan\u00b7cho\u00b7li\u00b7sches", "Ge\u00b7sum\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf den Stufen, wo die Gl\u00e4ub'gen", "tokens": ["Auf", "den", "Stu\u00b7fen", ",", "wo", "die", "Gl\u00e4ub'\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Prophetenwort gesungen,", "tokens": ["Das", "Pro\u00b7phe\u00b7ten\u00b7wort", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigen jetzt die Glatzenpf\u00e4fflein", "tokens": ["Zei\u00b7gen", "jetzt", "die", "Glat\u00b7zen\u00b7pf\u00e4f\u00b7flein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihrer Messe fades Wunder.", "tokens": ["Ih\u00b7rer", "Mes\u00b7se", "fa\u00b7des", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und das ist ein Drehn und Winden", "tokens": ["Und", "das", "ist", "ein", "Drehn", "und", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ART", "CARD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor den buntbemalten Puppen,", "tokens": ["Vor", "den", "bunt\u00b7be\u00b7mal\u00b7ten", "Pup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das bl\u00f6ckt und dampft und klingelt,", "tokens": ["Und", "das", "bl\u00f6ckt", "und", "dampft", "und", "klin\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die dummen Kerzen funkeln.", "tokens": ["Und", "die", "dum\u00b7men", "Ker\u00b7zen", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In dem Dome zu Corduva", "tokens": ["In", "dem", "Do\u00b7me", "zu", "Cor\u00b7du\u00b7va"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Steht Almansor ben Abdullah,", "tokens": ["Steht", "Al\u00b7man\u00b7sor", "ben", "Ab\u00b7dul\u00b7lah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "All die S\u00e4ulen still betrachtend,", "tokens": ["All", "die", "S\u00e4u\u00b7len", "still", "be\u00b7trach\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die stillen Worte murmelnd:", "tokens": ["Und", "die", "stil\u00b7len", "Wor\u00b7te", "mur\u00b7melnd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u201eo, ihr S\u00e4ulen, stark und riesig,", "tokens": ["\u201e", "o", ",", "ihr", "S\u00e4u\u00b7len", ",", "stark", "und", "rie\u00b7sig", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst geschm\u00fcckt zu Allahs Ruhme,", "tokens": ["Einst", "ge\u00b7schm\u00fcckt", "zu", "Al\u00b7lahs", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzo m\u00fc\u00dft Ihr dienend huld'gen", "tokens": ["Jet\u00b7zo", "m\u00fc\u00dft", "Ihr", "die\u00b7nend", "huld'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem verha\u00dften Christenthume!", "tokens": ["Dem", "ver\u00b7ha\u00df\u00b7ten", "Chris\u00b7ten\u00b7thu\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201eihr bequemt Euch in die Zeiten,", "tokens": ["\u201e", "ihr", "be\u00b7quemt", "Euch", "in", "die", "Zei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Ihr tragt die Last geduldig; \u2014", "tokens": ["Und", "Ihr", "tragt", "die", "Last", "ge\u00b7dul\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, da mu\u00df ja wohl der Schw\u00e4ch're", "tokens": ["Ei", ",", "da", "mu\u00df", "ja", "wohl", "der", "Schw\u00e4ch'\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch viel leichter sich beruh'gen.\u201c", "tokens": ["Noch", "viel", "leich\u00b7ter", "sich", "be\u00b7ruh'\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADJD", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und sein Haupt, mit heiterm Antlitz,", "tokens": ["Und", "sein", "Haupt", ",", "mit", "hei\u00b7term", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt Almansor ben Abdullah", "tokens": ["Beugt", "Al\u00b7man\u00b7sor", "ben", "Ab\u00b7dul\u00b7lah"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ueber den gezierten Taufstein,", "tokens": ["Ue\u00b7ber", "den", "ge\u00b7zier\u00b7ten", "Tauf\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In dem Dome zu Corduva.", "tokens": ["In", "dem", "Do\u00b7me", "zu", "Cor\u00b7du\u00b7va", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}