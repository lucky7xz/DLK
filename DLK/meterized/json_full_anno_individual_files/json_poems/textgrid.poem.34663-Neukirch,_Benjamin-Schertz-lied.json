], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht bald erfahren will/", "tokens": ["Nicht", "bald", "er\u00b7fah\u00b7ren", "will", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der meide nur zwey sachen/", "tokens": ["Der", "mei\u00b7de", "nur", "zwey", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "CARD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die alles traurig machen;", "tokens": ["Die", "al\u00b7les", "trau\u00b7rig", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein weib und karten-spiel.", "tokens": ["Ein", "weib", "und", "kar\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}