{"textgrid.poem.34663": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "Schertz-lied", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind das nicht narren-possen?", "tokens": ["Sind", "das", "nicht", "nar\u00b7ren\u00b7pos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein vater hat beschlossen/", "tokens": ["Mein", "va\u00b7ter", "hat", "be\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich soll ein m\u00e4gdgen freyn:", "tokens": ["Ich", "soll", "ein", "m\u00e4gd\u00b7gen", "freyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was aber kan auff erden", "tokens": ["Was", "a\u00b7ber", "kan", "auff", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl v\u00f6ller von beschwerden", "tokens": ["Wohl", "v\u00f6l\u00b7ler", "von", "be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als junge m\u00e4nner seyn?", "tokens": ["Als", "jun\u00b7ge", "m\u00e4n\u00b7ner", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn wie man seidne decken", "tokens": ["Denn", "wie", "man", "seid\u00b7ne", "de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar selten ohne flecken", "tokens": ["Gar", "sel\u00b7ten", "oh\u00b7ne", "fle\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ohne fehler findt/", "tokens": ["Und", "oh\u00b7ne", "feh\u00b7ler", "findt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So ist die kuh auch immer", "tokens": ["So", "ist", "die", "kuh", "auch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Bey unserm frauen-zimmer", "tokens": ["Bey", "un\u00b7serm", "frau\u00b7en\u00b7zim\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auff einem auge blind.", "tokens": ["Auff", "ei\u00b7nem", "au\u00b7ge", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6ne kehrt die feinde", "tokens": ["Die", "sch\u00f6\u00b7ne", "kehrt", "die", "fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch schw\u00e4ger zwar in freunde;", "tokens": ["Durch", "schw\u00e4\u00b7ger", "zwar", "in", "freun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch steht sie mir nicht an:", "tokens": ["Doch", "steht", "sie", "mir", "nicht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich die tummen h\u00f6rner", "tokens": ["Weil", "ich", "die", "tum\u00b7men", "h\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wenig/ als die d\u00f6rner/", "tokens": ["So", "we\u00b7nig", "/", "als", "die", "d\u00f6r\u00b7ner", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am kopffe leiden kan.", "tokens": ["Am", "kopf\u00b7fe", "lei\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die garstigen/ die stincken/", "tokens": ["Die", "gars\u00b7ti\u00b7gen", "/", "die", "stin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und pflegen offt zu hincken/", "tokens": ["Und", "pfle\u00b7gen", "offt", "zu", "hin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr bestes ist das geld:", "tokens": ["Ihr", "bes\u00b7tes", "ist", "das", "geld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Inzwischen gl\u00e4ntzt die stirne", "tokens": ["I\u00b7nzwi\u00b7schen", "gl\u00e4ntzt", "die", "stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie eine faule birne/", "tokens": ["Wie", "ei\u00b7ne", "fau\u00b7le", "bir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die von dem baume f\u00e4llt.", "tokens": ["Die", "von", "dem", "bau\u00b7me", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die klugen/ die befehlen/", "tokens": ["Die", "klu\u00b7gen", "/", "die", "be\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wollen stets erwehlen/", "tokens": ["Und", "wol\u00b7len", "stets", "er\u00b7weh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was man beginnen soll:", "tokens": ["Was", "man", "be\u00b7gin\u00b7nen", "soll", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die tumme bleibt geschossen/", "tokens": ["Die", "tum\u00b7me", "bleibt", "ge\u00b7schos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und macht mit ihren possen", "tokens": ["Und", "macht", "mit", "ih\u00b7ren", "pos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das gantze zimmer voll.", "tokens": ["Das", "gant\u00b7ze", "zim\u00b7mer", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die reichen sind nur prahler/", "tokens": ["Die", "rei\u00b7chen", "sind", "nur", "prah\u00b7ler", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schliessen ihre thaler", "tokens": ["Und", "schlies\u00b7sen", "ih\u00b7re", "tha\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In schwere kasten ein:", "tokens": ["In", "schwe\u00b7re", "kas\u00b7ten", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bey armen kans nicht fehlen/", "tokens": ["Bey", "ar\u00b7men", "kans", "nicht", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie m\u00fcssen manches stehlen/", "tokens": ["Sie", "m\u00fcs\u00b7sen", "man\u00b7ches", "steh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und m\u00e4nnern untreu seyn.", "tokens": ["Und", "m\u00e4n\u00b7nern", "un\u00b7treu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die dicken seyn beschwerlich/", "tokens": ["Die", "di\u00b7cken", "seyn", "be\u00b7schwer\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zur geburt gef\u00e4hrlich:", "tokens": ["Und", "zur", "ge\u00b7burt", "ge\u00b7f\u00e4hr\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum mag ich auch nicht dran.", "tokens": ["Drum", "mag", "ich", "auch", "nicht", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die armen w\u00e4ren besser/", "tokens": ["Die", "ar\u00b7men", "w\u00e4\u00b7ren", "bes\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nur da\u00df man solche fresser", "tokens": ["Nur", "da\u00df", "man", "sol\u00b7che", "fres\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PIAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wohl ern\u00e4hren kan.", "tokens": ["Nicht", "wohl", "er\u00b7n\u00e4h\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die fromme mu\u00df auff erden", "tokens": ["Die", "from\u00b7me", "mu\u00df", "auff", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VMFIN", "APPR", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Der m\u00e4gde n\u00e4rrin werden/", "tokens": ["Der", "m\u00e4g\u00b7de", "n\u00e4r\u00b7rin", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bringet nur beschwer.", "tokens": ["Und", "brin\u00b7get", "nur", "be\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die b\u00f6sen sind der teuffel/", "tokens": ["Die", "b\u00f6\u00b7sen", "sind", "der", "teuf\u00b7fel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und kommen ohne zweiffel", "tokens": ["Und", "kom\u00b7men", "oh\u00b7ne", "zweif\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner mutter her.", "tokens": ["Von", "sei\u00b7ner", "mut\u00b7ter", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch h\u00e4nget manchem weibe", "tokens": ["Doch", "h\u00e4n\u00b7get", "man\u00b7chem", "wei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich nichts an ihrem leibe", "tokens": ["Gleich", "nichts", "an", "ih\u00b7rem", "lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von diesen fehlern an/", "tokens": ["Von", "die\u00b7sen", "feh\u00b7lern", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So hat sie doch die tugend/", "tokens": ["So", "hat", "sie", "doch", "die", "tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie bey ihrer jugend", "tokens": ["Da\u00df", "sie", "bey", "ih\u00b7rer", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.6": {"text": "Nicht viel verschweigen kan.", "tokens": ["Nicht", "viel", "ver\u00b7schwei\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum wer die narren-plagen", "tokens": ["Drum", "wer", "die", "nar\u00b7ren\u00b7pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PWS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bey seinen jungen tagen", "tokens": ["Bey", "sei\u00b7nen", "jun\u00b7gen", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht bald erfahren will/", "tokens": ["Nicht", "bald", "er\u00b7fah\u00b7ren", "will", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der meide nur zwey sachen/", "tokens": ["Der", "m