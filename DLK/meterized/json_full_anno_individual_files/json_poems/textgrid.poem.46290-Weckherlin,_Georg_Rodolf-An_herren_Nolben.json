{"textgrid.poem.46290": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An herren Nolben", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Nolb, vermeinest du dan dich,", "tokens": ["Herr", "Nolb", ",", "ver\u00b7mei\u00b7nest", "du", "dan", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "weil ich, wie du an mich begehret,", "tokens": ["weil", "ich", ",", "wie", "du", "an", "mich", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "dich eines loblieds nicht gewehret,", "tokens": ["dich", "ei\u00b7nes", "lob\u00b7li\u00b7eds", "nicht", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "nu r\u00e4chend, und verleumdend mich,", "tokens": ["nu", "r\u00e4\u00b7chend", ",", "und", "ver\u00b7leum\u00b7dend", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KON", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mich andrer leut lob zu berauben?", "tokens": ["mich", "an\u00b7drer", "leut", "lob", "zu", "be\u00b7rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Glaub du, wan ich schon solt von dir", "tokens": ["Glaub", "du", ",", "wan", "ich", "schon", "solt", "von", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "ADV", "VMFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "vil schreiben, so w\u00fcrd man doch mir,", "tokens": ["vil", "schrei\u00b7ben", ",", "so", "w\u00fcrd", "man", "doch", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "VAFIN", "PIS", "ADV", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "wie man auch dir von mir, nichts glauben.", "tokens": ["wie", "man", "auch", "dir", "von", "mir", ",", "nichts", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPER", "APPR", "PPER", "$,", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Nolb, vermeinest du dan dich,", "tokens": ["Herr", "Nolb", ",", "ver\u00b7mei\u00b7nest", "du", "dan", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "weil ich, wie du an mich begehret,", "tokens": ["weil", "ich", ",", "wie", "du", "an", "mich", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "dich eines loblieds nicht gewehret,", "tokens": ["dich", "ei\u00b7nes", "lob\u00b7li\u00b7eds", "nicht", "ge\u00b7weh\u00b7ret", ","], "token_inf