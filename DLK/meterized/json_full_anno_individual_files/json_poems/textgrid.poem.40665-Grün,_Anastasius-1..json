{"textgrid.poem.40665": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Hirtenkinder, Knab' und M\u00e4dchen, spielen", "tokens": ["Zwei", "Hir\u00b7ten\u00b7kin\u00b7der", ",", "Knab'", "und", "M\u00e4d\u00b7chen", ",", "spie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Felsen bei erloschner Feuerstelle,", "tokens": ["Am", "Fel\u00b7sen", "bei", "er\u00b7lo\u00b7schner", "Feu\u00b7er\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die glatte Steinwand zeigt in Sonnenhelle", "tokens": ["Die", "glat\u00b7te", "Stein\u00b7wand", "zeigt", "in", "Son\u00b7nen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Schatten von zwei kindlichen Profilen.", "tokens": ["Die", "Schat\u00b7ten", "von", "zwei", "kind\u00b7li\u00b7chen", "Pro\u00b7fi\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Schwester Anmut fesselt den Gespielen", "tokens": ["Der", "Schwes\u00b7ter", "An\u00b7mut", "fes\u00b7selt", "den", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN"],