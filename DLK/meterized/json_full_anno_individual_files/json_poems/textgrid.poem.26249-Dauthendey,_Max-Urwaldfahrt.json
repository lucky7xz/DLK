{"textgrid.poem.26249": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Urwaldfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Auto rattert laut bergan,", "tokens": ["Das", "Au\u00b7to", "rat\u00b7tert", "laut", "ber\u00b7gan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Bambus dunkelt mir die Stra\u00dfe.", "tokens": ["Und", "Bam\u00b7bus", "dun\u00b7kelt", "mir", "die", "Stra\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein H\u00e4hnlein kaum noch fliehen kann.", "tokens": ["Ein", "H\u00e4hn\u00b7lein", "kaum", "noch", "flie\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Kopf guckt aus dem Strohgelasse,", "tokens": ["Ein", "Kopf", "guckt", "aus", "dem", "Stroh\u00b7ge\u00b7las\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sundanese staunt uns an.", "tokens": ["Der", "Sun\u00b7da\u00b7ne\u00b7se", "staunt", "uns", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Reisfeldwasser Frauen stehn,", "tokens": ["Im", "Reis\u00b7feld\u00b7was\u00b7ser", "Frau\u00b7en", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und junge Br\u00fcste fromm sich runden.", "tokens": ["Und", "jun\u00b7ge", "Br\u00fcs\u00b7te", "fromm", "sich", "run\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00fctter nur ihr Kindlein sehn,", "tokens": ["Die", "M\u00fct\u00b7ter", "nur", "ihr", "Kin\u00b7dlein", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Liebesurwald nackt gefunden.", "tokens": ["Im", "Lie\u00b7be\u00b7sur\u00b7wald", "nackt", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es kommen D\u00f6rfer und vergehn.", "tokens": ["Es", "kom\u00b7men", "D\u00f6r\u00b7fer", "und", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wagen, lautlos, sinkt zum Tal.", "tokens": ["Der", "Wa\u00b7gen", ",", "laut\u00b7los", ",", "sinkt", "zum", "Tal", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Abgrund rollt mit schroffem Gru\u00dfe", "tokens": ["Im", "Ab\u00b7grund", "rollt", "mit", "schrof\u00b7fem", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hin durch der roten Erde Saal", "tokens": ["Hin", "durch", "der", "ro\u00b7ten", "Er\u00b7de", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Leib von einem Urwaldflusse.", "tokens": ["Der", "Leib", "von", "ei\u00b7nem", "Ur\u00b7wald\u00b7flus\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Weg zum Tode ist hier schmal.", "tokens": ["Der", "Weg", "zum", "To\u00b7de", "ist", "hier", "schmal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Wagen \u00fcberholt den Schaum,", "tokens": ["Der", "Wa\u00b7gen", "\u00fc\u00b7berh\u00b7olt", "den", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der rund um Lavabl\u00f6cke flutet.", "tokens": ["Der", "rund", "um", "La\u00b7va\u00b7bl\u00f6\u00b7cke", "flu\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Knapp streift er hin am Felsensaum.", "tokens": ["Knapp", "streift", "er", "hin", "am", "Fel\u00b7sen\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Manchmal die Hupe heulend tutet,", "tokens": ["Manch\u00b7mal", "die", "Hu\u00b7pe", "heu\u00b7lend", "tu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann hallt der T\u00e4ler gr\u00fcner Raum.", "tokens": ["Dann", "hallt", "der", "T\u00e4\u00b7ler", "gr\u00fc\u00b7ner", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Urwaldkraut das Stahltier kennt,", "tokens": ["Das", "Ur\u00b7wald\u00b7kraut", "das", "Stahl\u00b7tier", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das donnernd kommt und, schnell verschwunden,", "tokens": ["Das", "don\u00b7nernd", "kommt", "und", ",", "schnell", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Laut rasselnd um die H\u00e4nge rennt,", "tokens": ["Laut", "ras\u00b7selnd", "um", "die", "H\u00e4n\u00b7ge", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und das die Eile hier erfunden,", "tokens": ["Und", "das", "die", "Ei\u00b7le", "hier", "er\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Ruhe nie die Eile nennt.", "tokens": ["Wo", "Ru\u00b7he", "nie", "die", "Ei\u00b7le", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Bl\u00fcten vom Trompetenstrauch,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "vom", "Trom\u00b7pe\u00b7ten\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Baumfarren, alle, sie erschrecken,", "tokens": ["Baum\u00b7far\u00b7ren", ",", "al\u00b7le", ",", "sie", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt knatternd der metallne Bauch", "tokens": ["Kommt", "knat\u00b7ternd", "der", "me\u00b7tall\u00b7ne", "Bauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Tieres durch die Teestrauchstrecken", "tokens": ["Des", "Tie\u00b7res", "durch", "die", "Tees\u00b7trauch\u00b7stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Baumvolk, schw\u00e4lend Gift und Rauch.", "tokens": ["Zum", "Baum\u00b7volk", ",", "schw\u00e4\u00b7lend", "Gift", "und", "Rauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Urwaldbl\u00f6cke nie begreifen das Ziel.", "tokens": ["Die", "Ur\u00b7wald\u00b7bl\u00f6\u00b7cke", "nie", "be\u00b7grei\u00b7fen", "das", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie wissen nichts von Menschenlaunen.", "tokens": ["Sie", "wis\u00b7sen", "nichts", "von", "Men\u00b7schen\u00b7lau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei steifer Bl\u00e4tter altem Spiel,", "tokens": ["Bei", "stei\u00b7fer", "Bl\u00e4t\u00b7ter", "al\u00b7tem", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei alten Spr\u00fcchen, die sie raunen,", "tokens": ["Bei", "al\u00b7ten", "Spr\u00fc\u00b7chen", ",", "die", "sie", "rau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ist ihnen Eile viel zu viel.", "tokens": ["Ist", "ih\u00b7nen", "Ei\u00b7le", "viel", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Urwaldseele schl\u00e4ft auf Daunen.", "tokens": ["Die", "Ur\u00b7wald\u00b7see\u00b7le", "schl\u00e4ft", "auf", "Dau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Auto r