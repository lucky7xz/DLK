{"textgrid.poem.49937": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "11. Verzicht", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleistift nahmen wir mit und Zeichenpapier und das Rei\u00dfbrett;", "tokens": ["Blei\u00b7stift", "nah\u00b7men", "wir", "mit", "und", "Zei\u00b7chen\u00b7pa\u00b7pier", "und", "das", "Rei\u00df\u00b7brett", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "KON", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber wie sch\u00f6n ist der Tag! und wir verd\u00fcrben ihn so?", "tokens": ["A\u00b7ber", "wie", "sch\u00f6n", "ist", "der", "Tag", "!", "und", "wir", "ver\u00b7d\u00fcr\u00b7ben", "ihn", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Beinah d\u00e4cht ich, wir lie\u00dfen es gar, wir schaun und genie\u00dfen!", "tokens": ["Bei\u00b7nah", "d\u00e4cht", "ich", ",", "wir", "lie\u00b7\u00dfen", "es", "gar", ",", "wir", "schaun", "und", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJD", "KON", "VVINF", "$."], "meter": "-++--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenig verliert ihr, und nichts wahrlich verlieret die Kunst.", "tokens": ["We\u00b7nig", "ver\u00b7liert", "ihr", ",", "und", "nichts", "wahr\u00b7lich", "ver\u00b7lie\u00b7ret", "die", "Kunst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "PIS", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "H\u00e4tt ich auch endlich mein Blatt vom Gasthaus an und der Kirche", "tokens": ["H\u00e4tt", "ich", "auch", "end\u00b7lich", "mein", "Blatt", "vom", "Gast\u00b7haus", "an", "und", "der", "Kir\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "KON", "ART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Bis zur M\u00fchle herab fertiggekritzelt \u2013 was ist's?", "tokens": ["Bis", "zur", "M\u00fch\u00b7le", "her\u00b7ab", "fer\u00b7tig\u00b7ge\u00b7krit\u00b7zelt", "\u2013", "was", "ist's", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADV", "ADJD", "$(", "PWS", "VAFIN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Hinter den licht durchbrochenen Turm, wer malt mir dies s\u00fc\u00dfe,", "tokens": ["Hin\u00b7ter", "den", "licht", "durch\u00b7bro\u00b7che\u00b7nen", "Turm", ",", "wer", "malt", "mir", "dies", "s\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Schimmernde Blau, und wer rundum das warme Gebirg?", "tokens": ["Schim\u00b7mern\u00b7de", "Blau", ",", "und", "wer", "run\u00b7dum", "das", "war\u00b7me", "Ge\u00b7birg", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PWS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "\u2013 Nein! Wo ich k\u00fcnftig auch sei, f\u00fcrwahr mit geschlossenen Augen", "tokens": ["\u2013", "Nein", "!", "Wo", "ich", "k\u00fcnf\u00b7tig", "auch", "sei", ",", "f\u00fcr\u00b7wahr", "mit", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$.", "PWAV", "PPER", "ADJD", "ADV", "VAFIN", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Seh ich dies Ganze vor mir, wie es kein Bildchen uns gibt.", "tokens": ["Seh", "ich", "dies", "Gan\u00b7ze", "vor", "mir", ",", "wie", "es", "kein", "Bild\u00b7chen", "uns", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "APPR", "PPER", "$,", "PWAV", "PPER", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Bleistift nahmen wir mit und Zeichenpapier und das Rei\u00dfbrett;", "tokens": ["Blei\u00b7stift", "nah\u00b7men", "wir", "mit", "und", "Zei\u00b7chen\u00b7pa\u00b7pier", "und", "das", "Rei\u00df\u00b7brett", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "KON", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber wie sch\u00f6n ist der Tag! und wir verd\u00fcrben ihn so?", "tokens": ["A\u00b7ber", "wie", "sch\u00f6n", "ist", "der", "Tag", "!", "und", "wir", "ver\u00b7d\u00fcr\u00b7ben", "ihn", "so", "?"], "token_info": ["word", "word", "word", "wo