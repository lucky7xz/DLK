{"textgrid.poem.57219": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Lied", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Gna im Fluge, jugendlich ungest\u00fcm,", "tokens": ["Wie", "Gna", "im", "Flu\u00b7ge", ",", "ju\u00b7gend\u00b7lich", "un\u00b7ge\u00b7st\u00fcm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und stolz, als reichten mir aus Iduna's Gold", "tokens": ["Und", "stolz", ",", "als", "reich\u00b7ten", "mir", "aus", "I\u00b7du\u00b7na's", "Gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die G\u00f6tter, sing' ich meine Freunde", "tokens": ["Die", "G\u00f6t\u00b7ter", ",", "sing'", "ich", "mei\u00b7ne", "Freun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Feyrend in k\u00fchnerem Bardenliede.", "tokens": ["Fey\u00b7rend", "in", "k\u00fch\u00b7ne\u00b7rem", "Bar\u00b7den\u00b7lie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Willst du zu Strophen werden, o Haingesang?", "tokens": ["Willst", "du", "zu", "Stro\u00b7phen", "wer\u00b7den", ",", "o", "Hain\u00b7ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VAINF", "$,", "FM", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Willst du gesetzlos, Ossians Schwunge gleich,", "tokens": ["Willst", "du", "ge\u00b7setz\u00b7los", ",", "Os\u00b7si\u00b7ans", "Schwun\u00b7ge", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "$,", "NE", "NN", "ADV", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Gleich Ullers Tanz auf Meerkrystalle,", "tokens": ["Gleich", "Ul\u00b7lers", "Tanz", "auf", "Meer\u00b7krys\u00b7tal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frey aus der Seele des Dichters schweben?", "tokens": ["Frey", "aus", "der", "See\u00b7le", "des", "Dich\u00b7ters", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Wasser Hebrus w\u00e4lzten mit Adlereil", "tokens": ["Die", "Was\u00b7ser", "Heb\u00b7rus", "w\u00e4lz\u00b7ten", "mit", "Ad\u00b7ler\u00b7eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des Zelten Leyer, welche die W\u00e4lder zwang,", "tokens": ["Des", "Zel\u00b7ten", "Le\u00b7yer", ",", "wel\u00b7che", "die", "W\u00e4l\u00b7der", "zwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dass sie ihr folgten, die den Felsen", "tokens": ["Dass", "sie", "ihr", "folg\u00b7ten", ",", "die", "den", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Taumeln, und wandeln aus Wolken lehrte.", "tokens": ["Tau\u00b7meln", ",", "und", "wan\u00b7deln", "aus", "Wol\u00b7ken", "lehr\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "So floss der Hebrus. Schattenbes\u00e4nftiger,", "tokens": ["So", "floss", "der", "Heb\u00b7rus", ".", "Schat\u00b7ten\u00b7be\u00b7s\u00e4nf\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$.", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit fortgerissen folgte dein fliehend Haupt", "tokens": ["Mit", "fort\u00b7ge\u00b7ris\u00b7sen", "folg\u00b7te", "dein", "flie\u00b7hend", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll Bluts, mit todter Stirn, der Leyer", "tokens": ["Voll", "Bluts", ",", "mit", "tod\u00b7ter", "Stirn", ",", "der", "Le\u00b7yer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hoch im Get\u00f6se gest\u00fcrzter Wogen.", "tokens": ["Hoch", "im", "Ge\u00b7t\u00f6\u00b7se", "ge\u00b7st\u00fcrz\u00b7ter", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "So floss der Waldstrom hin nach dem Ozean!", "tokens": ["So", "floss", "der", "Wald\u00b7strom", "hin", "nach", "dem", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So fliesst mein Lied auch, stark, und gedankenvoll.", "tokens": ["So", "fliesst", "mein", "Lied", "auch", ",", "stark", ",", "und", "ge\u00b7dan\u00b7ken\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dess spott' ich, der's mit Kl\u00fcglingsblicken", "tokens": ["Dess", "spott'", "ich", ",", "der's", "mit", "Kl\u00fcg\u00b7lings\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6ret, und kalt von der Glosse triefet.", "tokens": ["H\u00f6\u00b7ret", ",", "und", "kalt", "von", "der", "Glos\u00b7se", "trie\u00b7fet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Den segne, Lied, ihn segne bey festlichem", "tokens": ["Den", "seg\u00b7ne", ",", "Lied", ",", "ihn", "seg\u00b7ne", "bey", "fest\u00b7li\u00b7chem"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entgegengehn, mit Freudenbegr\u00fcssungen,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7gehn", ",", "mit", "Freu\u00b7den\u00b7be\u00b7gr\u00fcs\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcber Wingolfs hohe Schwelle", "tokens": ["Der", "\u00fc\u00b7ber", "Win\u00b7golfs", "ho\u00b7he", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiter, im Haine gekr\u00e4nzt, hereintritt.", "tokens": ["Hei\u00b7ter", ",", "im", "Hai\u00b7ne", "ge\u00b7kr\u00e4nzt", ",", "her\u00b7ein\u00b7tritt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Dein Barde wartet. Liebling der sanften Hlyn,", "tokens": ["Dein", "Bar\u00b7de", "war\u00b7tet", ".", "Lieb\u00b7ling", "der", "sanf\u00b7ten", "Hlyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo bliebst du? k\u00f6mst du von dem begeisternden", "tokens": ["Wo", "bliebst", "du", "?", "k\u00f6mst", "du", "von", "dem", "be\u00b7geis\u00b7tern\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ach\u00e4erh\u00e4mus? oder k\u00f6mst du", "tokens": ["A\u00b7ch\u00e4\u00b7er\u00b7h\u00e4\u00b7mus", "?", "o\u00b7der", "k\u00f6mst", "du"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "KON", "VVFIN", "PPER"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von den unsterblichen sieben H\u00fcgeln?", "tokens": ["Von", "den", "uns\u00b7terb\u00b7li\u00b7chen", "sie\u00b7ben", "H\u00fc\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "CARD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Wo Scipionen, Flakkus und Tullius,", "tokens": ["Wo", "Sci\u00b7pi\u00b7o\u00b7nen", ",", "Flak\u00b7kus", "und", "Tul\u00b7li\u00b7us", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Urenkel denkend, t\u00f6nender sprach, und sang,", "tokens": ["Ur\u00b7en\u00b7kel", "den\u00b7kend", ",", "t\u00f6\u00b7nen\u00b7der", "sprach", ",", "und", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJA", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo Maro mit dem Kapitole", "tokens": ["Wo", "Ma\u00b7ro", "mit", "dem", "Ka\u00b7pi\u00b7to\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Um die Unsterblichkeit muthig zankte!", "tokens": ["Um", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "mut\u00b7hig", "zank\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Voll sichres Stolzes, sah er die Ewigkeit", "tokens": ["Voll", "sich\u00b7res", "Stol\u00b7zes", ",", "sah", "er", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des hohen Marmors: Tr\u00fcmmer wirst einst du seyn,", "tokens": ["Des", "ho\u00b7hen", "Mar\u00b7mors", ":", "Tr\u00fcm\u00b7mer", "wirst", "einst", "du", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VAFIN", "ADV", "PPER", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Staub dann, und dann des Sturms Gespiele,", "tokens": ["Staub", "dann", ",", "und", "dann", "des", "Sturms", "Ge\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Du Kapitol! und du Gott der Donner!", "tokens": ["Du", "Ka\u00b7pi\u00b7tol", "!", "und", "du", "Gott", "der", "Don\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$.", "KON", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wie oder z\u00f6gerst du von des Albion", "tokens": ["Wie", "o\u00b7der", "z\u00f6\u00b7gerst", "du", "von", "des", "Al\u00b7bi\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eiland her\u00fcber? Liebe sie, Ebert, nur!", "tokens": ["Ei\u00b7land", "her\u00b7\u00fc\u00b7ber", "?", "Lie\u00b7be", "sie", ",", "E\u00b7bert", ",", "nur", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "PPER", "$,", "NE", "$,", "ADV", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sie sind auch deutsches Stamms, Urs\u00f6hne", "tokens": ["Sie", "sind", "auch", "deut\u00b7sches", "Stamms", ",", "Ur\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jener, die k\u00fchn mit der Woge kamen!", "tokens": ["Je\u00b7ner", ",", "die", "k\u00fchn", "mit", "der", "Wo\u00b7ge", "ka\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+++-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.11": {"line.1": {"text": "Sey mir gegr\u00fcsset! Immer gew\u00fcnscht k\u00f6mst du,", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fcs\u00b7set", "!", "Im\u00b7mer", "ge\u00b7w\u00fcnscht", "k\u00f6mst", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "ADV", "VVPP", "VVFIN", "PPER", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo du auch herk\u00f6mst, Liebling der sanften Hlyn!", "tokens": ["Wo", "du", "auch", "her\u00b7k\u00f6mst", ",", "Lieb\u00b7ling", "der", "sanf\u00b7ten", "Hlyn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vom Tybris lieb, sehr lieb vom H\u00e4mus!", "tokens": ["Vom", "Tyb\u00b7ris", "lieb", ",", "sehr", "lieb", "vom", "H\u00e4\u00b7mus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADJD", "$,", "ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieb von Britanniens stolzem Eiland,", "tokens": ["Lieb", "von", "Bri\u00b7tan\u00b7ni\u00b7ens", "stol\u00b7zem", "Ei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Allein geliebter, wenn du voll Vaterlands", "tokens": ["Al\u00b7lein", "ge\u00b7lieb\u00b7ter", ",", "wenn", "du", "voll", "Va\u00b7ter\u00b7lands"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus jenen Hainen k\u00f6mst, wo der Barden Chor", "tokens": ["Aus", "je\u00b7nen", "Hai\u00b7nen", "k\u00f6mst", ",", "wo", "der", "Bar\u00b7den", "Chor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Braga singet, wo die Telyn", "tokens": ["Mit", "Bra\u00b7ga", "sin\u00b7get", ",", "wo", "die", "Te\u00b7lyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "T\u00f6nt zu dem Fluge des deutschen Liedes.", "tokens": ["T\u00f6nt", "zu", "dem", "Flu\u00b7ge", "des", "deut\u00b7schen", "Lie\u00b7des", "."], "token_