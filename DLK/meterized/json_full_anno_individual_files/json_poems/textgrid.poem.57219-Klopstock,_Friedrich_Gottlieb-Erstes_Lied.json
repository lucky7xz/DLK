text": "Des Zelten Leyer, welche die W\u00e4lder zwang,", "tokens": ["Des", "Zel\u00b7ten", "Le\u00b7yer", ",", "wel\u00b7che", "die", "W\u00e4l\u00b7der", "zwang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dass sie ihr folgten, die den Felsen", "tokens": ["Dass", "sie", "ihr", "folg\u00b7ten", ",", "die", "den", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Taumeln, und wandeln aus Wolken lehrte.", "tokens": ["Tau\u00b7meln", ",", "und", "wan\u00b7deln", "aus", "Wol\u00b7ken", "lehr\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.19": {"line.1": {"text": "So floss der Hebrus. Schattenbes\u00e4nftiger,", "tokens": ["So", "floss", "der", "Heb\u00b7rus", ".", "Schat\u00b7ten\u00b7be\u00b7s\u00e4nf\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$.", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit fortgerissen folgte dein fliehend Haupt", "tokens": ["Mit", "fort\u00b7ge\u00b7ris\u00b7sen", "folg\u00b7te", "dein", "flie\u00b7hend", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll Bluts, mit todter Stirn, der Leyer", "tokens": ["Voll", "Bluts", ",", "mit", "tod\u00b7ter", "Stirn", ",", "der", "Le\u00b7yer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Hoch im Get\u00f6se gest\u00fcrzter Wogen.", "tokens": ["Hoch", "im", "Ge\u00b7t\u00f6\u00b7se", "ge\u00b7st\u00fcrz\u00b7ter", "Wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "So floss der Waldstrom hin nach dem Ozean!", "tokens": ["So", "floss", "der", "Wald\u00b7strom", "hin", "nach", "dem", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So fliesst mein Lied auch, stark, und gedankenvoll.", "tokens": ["So", "fliesst", "mein", "Lied", "auch", ",", "stark", ",", "und", "ge\u00b7dan\u00b7ken\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dess spott' ich, der's mit Kl\u00fcglingsblicken", "tokens": ["Dess", "spott'", "ich", ",", "der's", "mit", "Kl\u00fcg\u00b7lings\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6ret, und kalt von der Glosse triefet.", "tokens": ["H\u00f6\u00b7ret", ",", "und", "kalt", "von", "der", "Glos\u00b7se", "trie\u00b7fet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Den segne, Lied, ihn segne bey festlichem", "tokens": ["Den", "seg\u00b7ne", ",", "Lied", ",", "ihn", "seg\u00b7ne", "bey", "fest\u00b7li\u00b7chem"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entgegengehn, mit Freudenbegr\u00fcssungen,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7gehn", ",", "mit", "Freu\u00b7den\u00b7be\u00b7gr\u00fcs\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcber Wingolfs hohe Schwelle", "tokens": ["Der", "\u00fc\u00b7ber", "Win\u00b7golfs", "ho\u00b7he", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heiter, im Haine gekr\u00e4nzt, hereintritt.", "tokens": ["Hei\u00b7ter", ",", "im", "Hai\u00b7ne", "ge\u00b7kr\u00e4nzt", ",", "her\u00b7ein\u00b7tritt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.22": {"line.1": {"text": "Dein Barde wartet. Liebling der sanften Hlyn,", "tokens": ["Dein", "Bar\u00b7de", "war\u00b7tet", ".", "Lieb\u00b7ling", "der", "sanf\u00b7ten", "Hlyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.rela