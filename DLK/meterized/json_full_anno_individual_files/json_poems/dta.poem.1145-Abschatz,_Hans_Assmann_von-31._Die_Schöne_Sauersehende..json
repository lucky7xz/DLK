{"dta.poem.1145": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "31. Die Sch\u00f6ne Sauersehende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Das \u00e4userliche Thun zeigt meistens vom Gem\u00fctte/ ", "tokens": ["Das", "\u00e4u\u00b7ser\u00b7li\u00b7che", "Thun", "zeigt", "meis\u00b7tens", "vom", "Ge\u00b7m\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was angebohren ist/ wird unvermerckt geweist;", "tokens": ["Was", "an\u00b7ge\u00b7boh\u00b7ren", "ist", "/", "wird", "un\u00b7ver\u00b7merckt", "ge\u00b7weist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "$(", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beherrschet nur den Leib ein freundlich-edler Geist/", "tokens": ["Be\u00b7herr\u00b7schet", "nur", "den", "Leib", "ein", "freund\u00b7lich\u00b7ed\u00b7ler", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So zeiget sich wohl auch von aussen seine G\u00fctte.", "tokens": ["So", "zei\u00b7get", "sich", "wohl", "auch", "von", "aus\u00b7sen", "sei\u00b7ne", "G\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ADV", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer aber witzig ist/ bemeistert das Gebl\u00fctte/", "tokens": ["Wer", "a\u00b7ber", "wit\u00b7zig", "ist", "/", "be\u00b7meis\u00b7tert", "das", "Ge\u00b7bl\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verf\u00e4lscht das Gla\u00df/ das sonst der Sinnen Spiegel heist.", "tokens": ["Ver\u00b7f\u00e4lscht", "das", "Gla\u00df", "/", "das", "sonst", "der", "Sin\u00b7nen", "Spie\u00b7gel", "heist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "PDS", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht alles finstr\u2019 ist Bley/ nicht alles Gold/ was glei\u00dft:", "tokens": ["Nicht", "al\u00b7les", "fin\u00b7str'", "ist", "Bley", "/", "nicht", "al\u00b7les", "Gold", "/", "was", "glei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "VAFIN", "NN", "$(", "PTKNEG", "PIAT", "NN", "$(", "PWS", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Offt wohnt ein lindes Hertz in einer rauhen H\u00fctte.", "tokens": ["Offt", "wohnt", "ein", "lin\u00b7des", "Hertz", "in", "ei\u00b7ner", "rau\u00b7hen", "H\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Zu viele Freundligkeit bringt Eckel und Verdru\u00df/", "tokens": ["Zu", "vie\u00b7le", "Freund\u00b7lig\u00b7keit", "bringt", "E\u00b7ckel", "und", "Ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Gall erweckt und reizt des Honigs Uberflu\u00df/", "tokens": ["Die", "Gall", "er\u00b7weckt", "und", "reizt", "des", "Ho\u00b7nigs", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch saure Speisen wird die Essens-Lust ersetzet.", "tokens": ["Durch", "sau\u00b7re", "Spei\u00b7sen", "wird", "die", "Es\u00b7sens\u00b7Lust", "er\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den allerstrengsten Durst bestillt das saure Bier;", "tokens": ["Den", "al\u00b7ler\u00b7strengs\u00b7ten", "Durst", "be\u00b7stillt", "das", "sau\u00b7re", "Bier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was keine m\u00fche kost/ taugt weder mir noch dir/", "tokens": ["Was", "kei\u00b7ne", "m\u00fc\u00b7he", "kost", "/", "taugt", "we\u00b7der", "mir", "noch", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ADJA", "NN", "$(", "VVFIN", "KON", "PPER", "ADV", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verwehrtes Kleinod wird am meisten werth gesch\u00e4tzet.", "tokens": ["Ver\u00b7wehr\u00b7tes", "Klei\u00b7nod", "wird", "am", "meis\u00b7ten", "werth", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKA", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}