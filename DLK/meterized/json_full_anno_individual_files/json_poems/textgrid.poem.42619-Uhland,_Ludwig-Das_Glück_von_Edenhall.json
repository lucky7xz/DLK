{"textgrid.poem.42619": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck von Edenhall", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Edenhall der junge Lord", "tokens": ["Von", "E\u00b7den\u00b7hall", "der", "jun\u00b7ge", "Lord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft schmettern Festtrommetenschall,", "tokens": ["L\u00e4\u00dft", "schmet\u00b7tern", "Fest\u00b7trom\u00b7me\u00b7ten\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hebt sich an des Tisches Bord", "tokens": ["Er", "hebt", "sich", "an", "des", "Ti\u00b7sches", "Bord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruft in trunkner G\u00e4ste Schwall:", "tokens": ["Und", "ruft", "in", "trunk\u00b7ner", "G\u00e4s\u00b7te", "Schwall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbnun her mit dem Gl\u00fccke von Edenhall!\u00ab", "tokens": ["\u00bb", "nun", "her", "mit", "dem", "Gl\u00fc\u00b7cke", "von", "E\u00b7den\u00b7hall", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APZR", "APPR", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Schenk vernimmt ungern den Spruch,", "tokens": ["Der", "Schenk", "ver\u00b7nimmt", "un\u00b7gern", "den", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Hauses \u00e4ltester Vassall,", "tokens": ["Des", "Hau\u00b7ses", "\u00e4l\u00b7tes\u00b7ter", "Vas\u00b7sall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nimmt z\u00f6gernd aus dem seidnen Tuch", "tokens": ["Nimmt", "z\u00f6\u00b7gernd", "aus", "dem", "seid\u00b7nen", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das hohe Trinkglas von Kristall,", "tokens": ["Das", "ho\u00b7he", "Trink\u00b7glas", "von", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie nennen's: ", "tokens": ["Sie", "nen\u00b7nen's", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Darauf der Lord: \u00bbDem Glas zum Preis", "tokens": ["Da\u00b7rauf", "der", "Lord", ":", "\u00bb", "Dem", "Glas", "zum", "Preis"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "$.", "$(", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schenk roten ein aus Portugal!\u00ab", "tokens": ["Schenk", "ro\u00b7ten", "ein", "aus", "Por\u00b7tu\u00b7gal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit H\u00e4ndezittern gie\u00dft der Greis,", "tokens": ["Mit", "H\u00e4n\u00b7de\u00b7zit\u00b7tern", "gie\u00dft", "der", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und purpurn Licht wird \u00fcberall,", "tokens": ["Und", "pur\u00b7purn", "Licht", "wird", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es strahlt aus dem Gl\u00fccke von Edenhall.", "tokens": ["Es", "strahlt", "aus", "dem", "Gl\u00fc\u00b7cke", "von", "E\u00b7den\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Da spricht der Lord und schwingt's dabei:", "tokens": ["Da", "spricht", "der", "Lord", "und", "schwingt's", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdies Glas von leuchtendem Kristall", "tokens": ["\u00bb", "dies", "Glas", "von", "leuch\u00b7ten\u00b7dem", "Kris\u00b7tall"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gab meinem Ahn am Quell die Fei,", "tokens": ["Gab", "mei\u00b7nem", "Ahn", "am", "Quell", "die", "Fei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drein schrieb sie: kommt dies Glas zu Fall,", "tokens": ["Drein", "schrieb", "sie", ":", "kommt", "dies", "Glas", "zu", "Fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PDS", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr wohl dann, o Gl\u00fcck von Edenhall!", "tokens": ["Fahr", "wohl", "dann", ",", "o", "Gl\u00fcck", "von", "E\u00b7den\u00b7hall", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "FM", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Kelchglas ward zum Los mit Fug", "tokens": ["Ein", "Kelchg\u00b7las", "ward", "zum", "Los", "mit", "Fug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem freud'gen Stamm von Edenhall;", "tokens": ["Dem", "freu\u00b7d'\u00b7gen", "Stamm", "von", "E\u00b7den\u00b7hall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wir schl\u00fcrfen gern in vollem Zug,", "tokens": ["Wir", "schl\u00fcr\u00b7fen", "gern", "in", "vol\u00b7lem", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir l\u00e4uten gern mit lautem Schall;", "tokens": ["Wir", "l\u00e4u\u00b7ten", "gern", "mit", "lau\u00b7tem", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sto\u00dft an mit dem Gl\u00fccke von Edenhall!\u00ab", "tokens": ["Sto\u00dft", "an", "mit", "dem", "Gl\u00fc\u00b7cke", "von", "E\u00b7den\u00b7hall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Erst klingt es milde, tief und voll,", "tokens": ["Erst", "klingt", "es", "mil\u00b7de", ",", "tief", "und", "voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleich dem Gesang der Nachtigall,", "tokens": ["Gleich", "dem", "Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wie des Waldstroms laut Geroll,", "tokens": ["Dann", "wie", "des", "Wald\u00b7stroms", "laut", "Ge\u00b7roll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zuletzt erdr\u00f6hnt wie Donnerhall", "tokens": ["Zu\u00b7letzt", "er\u00b7dr\u00f6hnt", "wie", "Don\u00b7ner\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das herrliche Gl\u00fcck von Edenhall.", "tokens": ["Das", "herr\u00b7li\u00b7che", "Gl\u00fcck", "von", "E\u00b7den\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbzum Horte nimmt ein k\u00fchn Geschlecht", "tokens": ["\u00bb", "zum", "Hor\u00b7te", "nimmt", "ein", "k\u00fchn", "Ge\u00b7schlecht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich den zerbrechlichen Kristall;", "tokens": ["Sich", "den", "zer\u00b7brech\u00b7li\u00b7chen", "Kris\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--++", "measure": "dactylic.di.plus"}, "line.3": {"text": "Er dauert l\u00e4nger schon als recht,", "tokens": ["Er", "dau\u00b7ert", "l\u00e4n\u00b7ger", "schon", "als", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sto\u00dft an! mit diesem kr\u00e4ft'gen Prall", "tokens": ["Sto\u00dft", "an", "!", "mit", "die\u00b7sem", "kr\u00e4ft'\u00b7gen", "Prall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Versuch ich das Gl\u00fcck von Edenhall.\u00ab", "tokens": ["Ver\u00b7such", "ich", "das", "Gl\u00fcck", "von", "E\u00b7den\u00b7hall", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Und als das Trinkglas gellend springt,", "tokens": ["Und", "als", "das", "Trink\u00b7glas", "gel\u00b7lend", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Springt das Gew\u00f6lb mit j\u00e4hem Knall,", "tokens": ["Springt", "das", "Ge\u00b7w\u00f6lb", "mit", "j\u00e4\u00b7hem", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und aus dem Ri\u00df die Flamme dringt;", "tokens": ["Und", "aus", "dem", "Ri\u00df", "die", "Flam\u00b7me", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die G\u00e4ste sind zerstoben all", "tokens": ["Die", "G\u00e4s\u00b7te", "sind", "zer\u00b7sto\u00b7ben", "all"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit dem brechenden Gl\u00fccke von Edenhall.", "tokens": ["Mit", "dem", "bre\u00b7chen\u00b7den", "Gl\u00fc\u00b7cke", "von", "E\u00b7den\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Er st\u00fcrmt der Feind mit Brand und Mord,", "tokens": ["Er", "st\u00fcrmt", "der", "Feind", "mit", "Brand", "und", "Mord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in d