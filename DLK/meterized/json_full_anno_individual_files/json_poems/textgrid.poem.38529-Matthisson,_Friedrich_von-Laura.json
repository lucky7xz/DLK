{"textgrid.poem.38529": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Laura", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tag voll Himmel! da, aus Laura's Bliken,", "tokens": ["Tag", "voll", "Him\u00b7mel", "!", "da", ",", "aus", "Lau\u00b7ra's", "Bli\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$.", "ADV", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir der Liebe heiligstes Entz\u00fcken", "tokens": ["Mir", "der", "Lie\u00b7be", "hei\u00b7ligs\u00b7tes", "Ent\u00b7z\u00fc\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In die tiefger\u00fchrte Seele drang!", "tokens": ["In", "die", "tief\u00b7ge\u00b7r\u00fchr\u00b7te", "See\u00b7le", "drang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und, von ihrem Zauber hingerissen,", "tokens": ["Und", ",", "von", "ih\u00b7rem", "Zau\u00b7ber", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ich der Frommen, unter Feuerk\u00fcssen,", "tokens": ["Ich", "der", "From\u00b7men", ",", "un\u00b7ter", "Feu\u00b7er\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "An den s\u00fc\u00dfbeklomnen Busen sank!", "tokens": ["An", "den", "s\u00fc\u00df\u00b7be\u00b7klom\u00b7nen", "Bu\u00b7sen", "sank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "O! wie da, im seligsten Get\u00fcmmel", "tokens": ["O", "!", "wie", "da", ",", "im", "se\u00b7ligs\u00b7ten", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Niegef\u00fchlter Freuden, Erd' und Himmel", "tokens": ["Nie\u00b7ge\u00b7f\u00fchl\u00b7ter", "Freu\u00b7den", ",", "Erd'", "und", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mir Begl\u00fcktem rings umher vergieng!", "tokens": ["Mir", "Be\u00b7gl\u00fck\u00b7tem", "rings", "um\u00b7her", "ver\u00b7gieng", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sanfte Glut durch meine Adern gl\u00fchte,", "tokens": ["Sanf\u00b7te", "Glut", "durch", "mei\u00b7ne", "A\u00b7dern", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aus dem hocherfreuten Auge spr\u00fchte,", "tokens": ["Aus", "dem", "ho\u00b7cher\u00b7freu\u00b7ten", "Au\u00b7ge", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn es schmachtend an dem ihren hieng!", "tokens": ["Wenn", "es", "schmach\u00b7tend", "an", "dem", "ih\u00b7ren", "hieng", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Goldner sah' ich Wolken sich bes\u00e4umen,", "tokens": ["Gold\u00b7ner", "sah'", "ich", "Wol\u00b7ken", "sich", "be\u00b7s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jedes Bl\u00e4tchen auf den Fr\u00fchlingsb\u00e4umen", "tokens": ["Je\u00b7des", "Bl\u00e4t\u00b7chen", "auf", "den", "Fr\u00fch\u00b7lings\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schien zu fl\u00fcstern: Sie ist ewig dein!", "tokens": ["Schien", "zu", "fl\u00fcs\u00b7tern", ":", "Sie", "ist", "e\u00b7wig", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcklicher, in solcher Taumelf\u00fclle,", "tokens": ["Gl\u00fck\u00b7li\u00b7cher", ",", "in", "sol\u00b7cher", "Tau\u00b7mel\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Werd' ich, nach verst\u00e4ubter Erdenh\u00fclle,", "tokens": ["Werd'", "ich", ",", "nach", "ver\u00b7st\