{"dta.poem.17178": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "29.  Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb ist wie der Tod: sie t\u00f6dtet meine Sinnen/", "tokens": ["Die", "Lieb", "ist", "wie", "der", "Tod", ":", "sie", "t\u00f6d\u00b7tet", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie brichet mir das Hertz/ un\u0304 f\u00fchrt den Geist von hin\u0304en.", "tokens": ["Sie", "bri\u00b7chet", "mir", "das", "Hertz", "/", "un\u0304", "f\u00fchrt", "den", "Geist", "von", "hin\u00b7\u0304en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}