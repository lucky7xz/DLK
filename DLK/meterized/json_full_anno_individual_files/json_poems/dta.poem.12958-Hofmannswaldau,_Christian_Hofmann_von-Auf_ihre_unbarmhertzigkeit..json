{"dta.poem.12958": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre unbarmhertzigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll denn mein heisses ach und weinen gar nichts taugen?", "tokens": ["Soll", "denn", "mein", "heis\u00b7ses", "ach", "und", "wei\u00b7nen", "gar", "nichts", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "XY", "KON", "VVFIN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ein theurer zoll des hertzens und der augen,", "tokens": ["Es", "ist", "ein", "theu\u00b7rer", "zoll", "des", "hert\u00b7zens", "und", "der", "au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJD", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der wie dein harter grimm von tag zu tage steigt.", "tokens": ["Der", "wie", "dein", "har\u00b7ter", "grimm", "von", "tag", "zu", "ta\u00b7ge", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "PPOSAT", "ADJA", "ADJD", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn deinen strengen sinn nun selbst die zeit nicht beugt,", "tokens": ["Wenn", "dei\u00b7nen", "stren\u00b7gen", "sinn", "nun", "selbst", "die", "zeit", "nicht", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kan es nicht mehr lange w\u00e4hren,", "tokens": ["So", "kan", "es", "nicht", "mehr", "lan\u00b7ge", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es mu\u00df sich meine brust in den Vesuvius,", "tokens": ["Es", "mu\u00df", "sich", "mei\u00b7ne", "brust", "in", "den", "Ve\u00b7su\u00b7vius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Der augen nasser \u00fcberflu\u00df", "tokens": ["Der", "au\u00b7gen", "nas\u00b7ser", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich in ein meer, dein sinn in einen fels verkehren.", "tokens": ["Sich", "in", "ein", "meer", ",", "dein", "sinn", "in", "ei\u00b7nen", "fels", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}