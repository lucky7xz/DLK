{"textgrid.poem.57540": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "April", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Laut fl\u00f6tet der Wind durch den Haselnu\u00dfstrauch,", "tokens": ["Laut", "fl\u00f6\u00b7tet", "der", "Wind", "durch", "den", "Ha\u00b7sel\u00b7nu\u00df\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schneeflocken durchwirbeln den Hain,", "tokens": ["Schnee\u00b7flo\u00b7cken", "durch\u00b7wir\u00b7beln", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald Hagel, bald Regen und eisiger Hauch,", "tokens": ["Bald", "Ha\u00b7gel", ",", "bald", "Re\u00b7gen", "und", "ei\u00b7si\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bald lachendster Lenzsonnenschein.", "tokens": ["Bald", "la\u00b7chends\u00b7ter", "Lenz\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich wei\u00df ja, da\u00df kurz dieser Sonnenblick dauert,", "tokens": ["Ich", "wei\u00df", "ja", ",", "da\u00df", "kurz", "die\u00b7ser", "Son\u00b7nen\u00b7blick", "dau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df Hagel und Regen und Schneefall schon lauert", "tokens": ["Da\u00df", "Ha\u00b7gel", "und", "Re\u00b7gen", "und", "Schnee\u00b7fall", "schon", "lau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "ADV", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und Nordwinds erstarrendes Wehn,", "tokens": ["Und", "Nord\u00b7winds", "er\u00b7star\u00b7ren\u00b7des", "Wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Und dennoch mich freudige Hoffnung durchschauert,", "tokens": ["Und", "den\u00b7noch", "mich", "freu\u00b7di\u00b7ge", "Hoff\u00b7nung", "durch\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Es ist ja so sch\u00f6n, ja so fr\u00fchlingshaft sch\u00f6n.", "tokens": ["Es", "ist", "ja", "so", "sch\u00f6n", ",", "ja", "so", "fr\u00fch\u00b7lings\u00b7haft", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Erfriern auch die Veilchen, die gestern erbl\u00fcht,", "tokens": ["Er\u00b7fri\u00b7ern", "auch", "die", "Veil\u00b7chen", ",", "die", "ge\u00b7stern", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verstummt auch der Fink in dem Wald \u2013", "tokens": ["Ver\u00b7stummt", "auch", "der", "Fink", "in", "dem", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So lieb ich, April dich, in meinem Gem\u00fct", "tokens": ["So", "lieb", "ich", ",", "Ap\u00b7ril", "dich", ",", "in", "mei\u00b7nem", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "$,", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ist's auch heute warm, morgen kalt.", "tokens": ["Ist's", "auch", "heu\u00b7te", "warm", ",", "mor\u00b7gen", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Auch dich hatt' ich lieb, die so oft mich belogen,", "tokens": ["Auch", "dich", "hatt'", "ich", "lieb", ",", "die", "so", "oft", "mich", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "So oft mich mit Lachen und Weinen betrogen,", "tokens": ["So", "oft", "mich", "mit", "La\u00b7chen", "und", "Wei\u00b7nen", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Dich M\u00e4del, trotz Falschheit und Lug,", "tokens": ["Dich", "M\u00e4\u00b7del", ",", "trotz", "Falschheit", "und", "Lug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ja, Zauberkraft war's, die zu dir mich gezogen,", "tokens": ["Ja", ",", "Zau\u00b7ber\u00b7kraft", "wa\u00b7r's", ",", "die", "zu", "dir", "mich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja Trug, doch berauschender, seliger Trug.", "tokens": ["Ja", "Trug", ",", "doch", "be\u00b7rau\u00b7schen\u00b7der", ",", "se\u00b7li\u00b7ger", "Trug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Schon lange ist's her, schon manch langes Jahr,", "tokens": ["Schon", "lan\u00b7ge", "ist's", "her", ",", "schon", "manch", "lan\u00b7ges", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "PTKVZ", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab' immer gern deiner gedacht,", "tokens": ["Hab'", "im\u00b7mer", "gern", "dei\u00b7ner", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du rosige Wange, du goldhelles Haar,", "tokens": ["Du", "ro\u00b7si\u00b7ge", "Wan\u00b7ge", ",", "du", "gold\u00b7hel\u00b7les", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du Auge, voll tiefblauer Pracht,", "tokens": ["Du", "Au\u00b7ge", ",", "voll", "tief\u00b7blau\u00b7er", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Lippen, wie konntet ihr lachen und schmollen,", "tokens": ["Ihr", "Lip\u00b7pen", ",", "wie", "konn\u00b7tet", "ihr", "la\u00b7chen", "und", "schmol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ihr Augen, wie konntet ihr strahlen und grollen,", "tokens": ["Ihr", "Au\u00b7gen", ",", "wie", "konn\u00b7tet", "ihr", "strah\u00b7len", "und", "grol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bald H\u00f6llenpein spenden und bald Paradies,", "tokens": ["Bald", "H\u00f6l\u00b7len\u00b7pein", "spen\u00b7den", "und", "bald", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "KON", "ADV", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Was half mir mein besseres Wissen und Wollen,", "tokens": ["Was", "half", "mir", "mein", "bes\u00b7se\u00b7res", "Wis\u00b7sen", "und", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ja L\u00fcge und Trug war's, doch s\u00fc\u00df, ach so s\u00fc\u00df.", "tokens": ["Ja", "L\u00fc\u00b7ge", "und", "Trug", "wa\u00b7r's", ",", "doch", "s\u00fc\u00df", ",", "ach", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "ADJD", "$,", "ITJ", "ADV", "ADJD", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich weine den Blumen des Herzens nicht nach,", "tokens": ["Ich", "wei\u00b7ne", "den", "Blu\u00b7men", "des", "Her\u00b7zens", "nicht", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon morgen erbl\u00fcht neues Gl\u00fcck,", "tokens": ["Schon", "mor\u00b7gen", "er\u00b7bl\u00fcht", "neu\u00b7es", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn auch der Nordwind die Lenzbl\u00fcten brach,", "tokens": ["Und", "wenn", "auch", "der", "Nord\u00b7wind", "die", "Lenz\u00b7bl\u00fc\u00b7ten", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein Jahr und sie kehren zur\u00fcck.", "tokens": ["Ein", "Jahr", "und", "sie", "keh\u00b7ren", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ja Hagel und Regen und Sonne und Schneien,", "tokens": ["Ja", "Ha\u00b7gel", "und", "Re\u00b7gen", "und", "Son\u00b7ne", "und", "Schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und Wechsel von Trauer, von Lust und Bereuen,", "tokens": ["Und", "Wech\u00b7sel", "von", "Trau\u00b7er", ",", "von", "Lust", "und", "Be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bald jauchzend, bald d\u00fcster und still,", "tokens": ["Bald", "jauch\u00b7zend", ",", "bald", "d\u00fcs\u00b7ter", "und", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Die Lust nicht verachten, die Schmerzen nicht scheuen,", "tokens": ["Die", "Lust", "nicht", "ver\u00b7ach\u00b7ten", ",", "die", "Schmer\u00b7zen", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Ich lieb euch, falsch M\u00e4dchen und falscher April.", "tokens": ["Ich", "lieb", "euch", ",", "falsch", "M\u00e4d\u00b7chen", "und", "fal\u00b7scher", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Laut fl\u00f6tet der Wind durch den Haselnu\u00dfstrauch,", "tokens": ["Laut", "fl\u00f6\u00b7tet", "der", "Wind", "durch", "den", "Ha\u00b7sel\u00b7nu\u00df\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schneeflocken durchwirbeln den Hain,", "tokens": ["Schnee\u00b7flo\u00b7cken", "durch\u00b7wir\u00b7beln", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald Hagel, bald Regen und eisiger Hauch,", "tokens": ["Bald", "Ha\u00b7gel", ",", "bald", "Re\u00b7gen", "und", "ei\u00b7si\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bald lachendster Lenzsonnenschein.", "tokens": ["Bald", "la\u00b7chends\u00b7ter", "Lenz\u00b7son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich wei\u00df ja, da\u00df kurz dieser Sonnenblick dauert,", "tokens": ["Ich", "wei\u00df", "ja", ",", "da\u00df", "kurz", "die\u00b7ser", "Son\u00b7nen\u00b7blick", "dau\u00b7ert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da\u00df Hagel und Regen und Schneefall schon lauert", "tokens": ["Da\u00df", "Ha\u00b7gel", "und", "Re\u00b7gen", "und", "Schnee\u00b7fall", "schon", "lau\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "ADV", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und Nordwinds erstarrendes Wehn,", "tokens": ["Und", "Nord\u00b7winds", "er\u00b7star\u00b7ren\u00b7des", "Wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Und dennoch mich freudige Hoffnung durchschauert,", "tokens": ["Und", "den\u00b7noch", "mich", "freu\u00b7di\u00b7ge", "Hoff\u00b7nung", "durch\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Es ist ja so sch\u00f6n, ja so fr\u00fchlingshaft sch\u00f6n.", "tokens": ["Es", "ist", "ja", "so", "sch\u00f6n", ",", "ja", "so", "fr\u00fch\u00b7lings\u00b7haft", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Erfriern auch die Veilchen, die gestern erbl\u00fcht,", "tokens": ["Er\u00b7fri\u00b7ern", "auch", "die", "Veil\u00b7chen", ",", "die", "ge\u00b7stern", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verstummt auch der Fink in dem Wald \u2013", "tokens": ["Ver\u00b7stummt", "auch", "der", "Fink", "in", "dem", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So lieb ich, April dich, in meinem Gem\u00fct", "tokens": ["So", "lieb", "ich", ",", "Ap\u00b7ril", "dich", ",", "in", "mei\u00b7nem", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "$,", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ist's auch heute warm, morgen kalt.", "tokens": ["Ist's", "auch", "heu\u00b7te", "warm", ",", "mor\u00b7gen", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Auch dich hatt' ich lieb, die so oft mich belogen,", "tokens": ["Auch", "dich", "hatt'", "ich", "lieb", ",", "die", "so", "oft", "mich", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "So oft mich mit Lachen und Weinen betrogen,", "tokens": ["So", "oft", "mich", "mit", "La\u00b7chen", "und", "Wei\u00b7nen", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Dich M\u00e4del, trotz Falschheit und Lug,", "tokens": ["Dich", "M\u00e4\u00b7del", ",", "trotz", "Falschheit", "und", "Lug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ja, Zauberkraft war's, die zu dir mich gezogen,", "tokens": ["Ja", ",", "Zau\u00b7ber\u00b7kraft", "wa\u00b7r's", ",", "die", "zu", "dir", "mich", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "$,", "PRELS", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja Trug, doch berauschender, seliger Trug.", "tokens": ["Ja", "Trug", ",", "doch", "be\u00b7rau\u00b7schen\u00b7der", ",", "se\u00b7li\u00b7ger", "Trug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Schon lange ist's her, schon manch langes Jahr,", "tokens": ["Schon", "lan\u00b7ge", "ist's", "her", ",", "schon", "manch", "lan\u00b7ges", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "PTKVZ", "$,", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hab' immer gern deiner gedacht,", "tokens": ["Hab'", "im\u00b7mer", "gern", "dei\u00b7ner", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PPOSAT", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du rosige Wange, du goldhelles Haar,", "tokens": ["Du", "ro\u00b7si\u00b7ge", "Wan\u00b7ge", ",", "du", "gold\u00b7hel\u00b7les", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du Auge, voll tiefblauer Pracht,", "tokens": ["Du", "Au\u00b7ge", ",", "voll", "tief\u00b7blau\u00b7er", "Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Lippen, wie konntet ihr lachen und schmollen,", "tokens": ["Ihr", "Lip\u00b7pen", ",", "wie", "konn\u00b7tet", "ihr", "la\u00b7chen", "und", "schmol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ihr Augen, wie konntet ihr strahlen und grollen,", "tokens": ["Ihr", "Au\u00b7gen", ",", "wie", "konn\u00b7tet", "ihr", "strah\u00b7len", "und", "grol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bald H\u00f6llenpein spenden und bald Paradies,", "tokens": ["Bald", "H\u00f6l\u00b7len\u00b7pein", "spen\u00b7den", "und", "bald", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "KON", "ADV", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Was half mir mein besseres Wissen und Wollen,", "tokens": ["Was", "half", "mir", "mein", "bes\u00b7se\u00b7res", "Wis\u00b7sen", "und", "Wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ja L\u00fcge und Trug war's, doch s\u00fc\u00df, ach so s\u00fc\u00df.", "tokens": ["Ja", "L\u00fc\u00b7ge", "und", "Trug", "wa\u00b7r's", ",", "doch", "s\u00fc\u00df", ",", "ach", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "VAFIN", "$,", "ADV", "ADJD", "$,", "ITJ", "ADV", "ADJD", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich weine den Blumen des Herzens nicht nach,", "tokens": ["Ich", "wei\u00b7ne", "den", "Blu\u00b7men", "des", "Her\u00b7zens", "nicht", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Schon morgen erbl\u00fcht neues Gl\u00fcck,", "tokens": ["Schon", "mor\u00b7gen", "er\u00b7bl\u00fcht", "neu\u00b7es", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn auch der Nordwind die Lenzbl\u00fcten brach,", "tokens": ["Und", "wenn", "auch", "der", "Nord\u00b7wind", "die", "Lenz\u00b7bl\u00fc\u00b7ten", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein Jahr und sie kehren zur\u00fcck.", "tokens": ["Ein", "Jahr", "und", "sie", "keh\u00b7ren", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ja Hagel und Regen und Sonne und Schneien,", "tokens": ["Ja", "Ha\u00b7gel", "und", "Re\u00b7gen", "und", "Son\u00b7ne", "und", "Schnei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und Wechsel von Trauer, von Lust und Bereuen,", "tokens": ["Und", "Wech\u00b7sel", "von", "Trau\u00b7er", ",", "von", "Lust", "und", "Be\u00b7reu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Bald jauchzend, bald d\u00fcster und still,", "tokens": ["Bald", "jauch\u00b7zend", ",", "bald", "d\u00fcs\u00b7ter", "und", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Die Lust nicht verachten, die Schmerzen nicht scheuen,", "tokens": ["Die", "Lust", "nicht", "ver\u00b7ach\u00b7ten", ",", "die", "Schmer\u00b7zen", "nicht", "scheu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Ich lieb euch, falsch M\u00e4dchen und falscher April.", "tokens": ["Ich", "lieb", "euch", ",", "falsch", "M\u00e4d\u00b7chen", "und", "fal\u00b7scher", "Ap\u00b7ril", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}