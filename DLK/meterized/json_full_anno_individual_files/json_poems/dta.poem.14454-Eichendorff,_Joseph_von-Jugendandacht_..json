{"dta.poem.14454": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wie ich schreibe hier, den Schmerz zu stillen,", "tokens": ["Und", "wie", "ich", "schrei\u00b7be", "hier", ",", "den", "Schmerz", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' ich den Engel schon die Riegel l\u00f6sen,", "tokens": ["F\u00fchl'", "ich", "den", "En\u00b7gel", "schon", "die", "Rie\u00b7gel", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kann vor Gl\u00e4nzung nicht mehr weiter klagen.", "tokens": ["Und", "kann", "vor", "Gl\u00e4n\u00b7zung", "nicht", "mehr", "wei\u00b7ter", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}