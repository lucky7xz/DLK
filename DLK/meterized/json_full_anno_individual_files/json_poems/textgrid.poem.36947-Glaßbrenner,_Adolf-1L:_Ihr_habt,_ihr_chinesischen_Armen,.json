{"textgrid.poem.36947": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr habt, ihr chinesischen Armen,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt, ihr chinesischen Armen,", "tokens": ["Ihr", "habt", ",", "ihr", "chi\u00b7ne\u00b7si\u00b7schen", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Holz so gro\u00dfe Noth;", "tokens": ["An", "Holz", "so", "gro\u00b7\u00dfe", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch klappern noch die Z\u00e4hne", "tokens": ["Euch", "klap\u00b7pern", "noch", "die", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Reis und trocknem Brot!", "tokens": ["Bei", "Reis", "und", "trock\u00b7nem", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da hab' ich, euch, Chinesen!", "tokens": ["Da", "hab'", "ich", ",", "euch", ",", "Chi\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "$,", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Gesonnen hin und her,", "tokens": ["Ge\u00b7son\u00b7nen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": [