{"dta.poem.16036": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, nothwendig hat das Beste Gott gemacht,", "tokens": ["Du", "sagst", ",", "noth\u00b7wen\u00b7dig", "hat", "das", "Bes\u00b7te", "Gott", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht besser konnte seyn die Welt hervorgebracht.", "tokens": ["Nicht", "bes\u00b7ser", "konn\u00b7te", "seyn", "die", "Welt", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPOSAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn dem Allm\u00e4chtigen, Allg\u00fctigen, Allweisen,", "tokens": ["Denn", "dem", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", ",", "All\u00b7g\u00fc\u00b7ti\u00b7gen", ",", "All\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Geziemt das Beste nur aus des Denkbaren Kreisen.", "tokens": ["Ge\u00b7ziemt", "das", "Bes\u00b7te", "nur", "aus", "des", "Denk\u00b7ba\u00b7ren", "Krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Nicht einmal willst du ihm, dem Allerfreisten, g\u00f6nnen", "tokens": ["Nicht", "ein\u00b7mal", "willst", "du", "ihm", ",", "dem", "Al\u00b7ler\u00b7freis\u00b7ten", ",", "g\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "PPER", "$,", "ART", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Freiheit, da\u00df ers auch hab' anders machen k\u00f6nnen!", "tokens": ["Die", "Frei\u00b7heit", ",", "da\u00df", "ers", "auch", "hab'", "an\u00b7ders", "ma\u00b7chen", "k\u00f6n\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PIS", "ADV", "VAFIN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich aber sage dir, was mir ein Dichter sagte,", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", "dir", ",", "was", "mir", "ein", "Dich\u00b7ter", "sag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den ich um den Verhalt des h\u00f6chsten Dichters fragte.", "tokens": ["Den", "ich", "um", "den", "Ver\u00b7halt", "des", "h\u00f6chs\u00b7ten", "Dich\u00b7ters", "frag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er sprach: die Laien h\u00e4lt ein Vorurteil gebunden,", "tokens": ["Er", "sprach", ":", "die", "Lai\u00b7en", "h\u00e4lt", "ein", "Vor\u00b7ur\u00b7teil", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ein vollkommnes Werk sie haben vorgefunden,", "tokens": ["Wenn", "ein", "voll\u00b7komm\u00b7nes", "Werk", "sie", "ha\u00b7ben", "vor\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zu meinen, da\u00df es gar nicht anders k\u00f6nne seyn,", "tokens": ["Zu", "mei\u00b7nen", ",", "da\u00df", "es", "gar", "nicht", "an\u00b7ders", "k\u00f6n\u00b7ne", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich am ganzen Bau nicht r\u00fccken lass' ein Stein.", "tokens": ["Und", "sich", "am", "gan\u00b7zen", "Bau", "nicht", "r\u00fc\u00b7cken", "lass'", "ein", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "ADJA", "NN", "PTKNEG", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Am Bau, dem fertigen, ist freilich nichts zu r\u00fccken,", "tokens": ["Am", "Bau", ",", "dem", "fer\u00b7ti\u00b7gen", ",", "ist", "frei\u00b7lich", "nichts", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "$,", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch zur Verfertigung gab es gar viele Br\u00fccken.", "tokens": ["Doch", "zur", "Ver\u00b7fer\u00b7ti\u00b7gung", "gab", "es", "gar", "vie\u00b7le", "Br\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.8": {"line.1": {"text": "Und jeder Dichter wei\u00df, wie gut ihm so die Sachen", "tokens": ["Und", "je\u00b7der", "Dich\u00b7ter", "wei\u00df", ",", "wie", "gut", "ihm", "so", "die", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelungen, da\u00df er sie auch anders konnte machen.", "tokens": ["Ge\u00b7lun\u00b7gen", ",", "da\u00df", "er", "sie", "auch", "an\u00b7ders", "konn\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und macht' er anders sie, ihr stimmtet wieder bei,", "tokens": ["Und", "macht'", "er", "an\u00b7ders", "sie", ",", "ihr", "stimm\u00b7tet", "wie\u00b7der", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dis das Best', und gar kein andres m\u00f6glich sei.", "tokens": ["Da\u00df", "dis", "das", "Best'", ",", "und", "gar", "kein", "and\u00b7res", "m\u00f6g\u00b7lich", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "$,", "KON", "ADV", "PIAT", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Gott der nach seiner Wahl hier macht' ein Bestes so,", "tokens": ["Gott", "der", "nach", "sei\u00b7ner", "Wahl", "hier", "macht'", "ein", "Bes\u00b7tes", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ein andres Bestes macht er irgend anderswo.", "tokens": ["Ein", "and\u00b7res", "Bes\u00b7tes", "macht", "er", "ir\u00b7gend", "an\u00b7ders\u00b7wo", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}