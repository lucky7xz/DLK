{"textgrid.poem.52051": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fiedler und darzu die Dudler sollen dir,", "tokens": ["Die", "Fied\u00b7ler", "und", "dar\u00b7zu", "die", "Dud\u00b7ler", "sol\u00b7len", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PAV", "ART", "NN", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So um die Kretscham Saul ein Dorff voll Bauern jagen,", "tokens": ["So", "um", "die", "Kretsc\u00b7ham", "Saul", "ein", "Dorff", "voll", "Bau\u00b7ern", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "ART", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die St\u00e4dte durch das Land vor ihren Tisch betagen,", "tokens": ["Die", "St\u00e4d\u00b7te", "durch", "das", "Land", "vor", "ih\u00b7ren", "Tisch", "be\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und B\u00fcrgermeister seyn, die du zeuchst andern f\u00fcr.", "tokens": ["Und", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "seyn", ",", "die", "du", "zeuchst", "an\u00b7dern", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAINF", "$,", "PRELS", "PPER", "VVFIN", "PIS", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du sprichst, wo Jungfern stehn, da zeucht man Zapffen auff,", "tokens": ["Du", "sprichst", ",", "wo", "Jung\u00b7fern", "stehn", ",", "da", "zeucht", "man", "Zapf\u00b7fen", "auff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,", "ADV", "VVFIN", "PIS", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kan man St\u00e4dt' und Land verdudeln und verfiedeln:", "tokens": ["So", "kan", "man", "St\u00e4dt'", "und", "Land", "ver\u00b7du\u00b7deln", "und", "ver\u00b7fie\u00b7deln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "NN", "KON", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wird den Erbarn Rath die Noth im kurtzen igeln:", "tokens": ["Wie", "wird", "den", "Er\u00b7barn", "Rath", "die", "Noth", "im", "kurt\u00b7zen", "i\u00b7geln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Feind kommt: Zuvor, bleib: Dann wird ihr Wort seyn: Lauff!", "tokens": ["Der", "Feind", "kommt", ":", "Zu\u00b7vor", ",", "bleib", ":", "Dann", "wird", "ihr", "Wort", "seyn", ":", "Lauff", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "VAINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.i