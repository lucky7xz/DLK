{"dta.poem.12615": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Die Alte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu meiner Zeit", "tokens": ["Zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bestand noch Recht und Billigkeit.", "tokens": ["Be\u00b7stand", "noch", "Recht", "und", "Bil\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wurden auch aus Kindern Leute;", "tokens": ["Da", "wur\u00b7den", "auch", "aus", "Kin\u00b7dern", "Leu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da wurden auch aus Jungfern Br\u00e4ute:", "tokens": ["Da", "wur\u00b7den", "auch", "aus", "Jung\u00b7fern", "Br\u00e4u\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alles mit Bescheidenheit.", "tokens": ["Doch", "al\u00b7les", "mit", "Be\u00b7schei\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ward kein Liebling zum Verr\u00e4ther,", "tokens": ["Es", "ward", "kein", "Lieb\u00b7ling", "zum", "Ver\u00b7r\u00e4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und unsre Jungfern freyten sp\u00e4ter:", "tokens": ["Und", "uns\u00b7re", "Jung\u00b7fern", "frey\u00b7ten", "sp\u00e4\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie reitzten nicht der M\u00fctter Neid.", "tokens": ["Sie", "reitz\u00b7ten", "nicht", "der", "M\u00fct\u00b7ter", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O gute Zeit!", "tokens": ["O", "gu\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zu meiner Zeit", "tokens": ["Zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Befli\u00df man sich der Heimlichkeit.", "tokens": ["Be\u00b7fli\u00df", "man", "sich", "der", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geno\u00df der J\u00fcngling ein Vergn\u00fcgen,", "tokens": ["Ge\u00b7no\u00df", "der", "J\u00fcng\u00b7ling", "ein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So war er dankbar und verschwiegen:", "tokens": ["So", "war", "er", "dank\u00b7bar", "und", "ver\u00b7schwie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und itzt entdeckt ers ungescheut.", "tokens": ["Und", "itzt", "ent\u00b7deckt", "ers", "un\u00b7ge\u00b7scheut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Regung m\u00fctterlicher Triebe,", "tokens": ["Die", "Re\u00b7gung", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der F\u00fcrwitz und der Geist der Liebe", "tokens": ["Der", "F\u00fcr\u00b7witz", "und", "der", "Geist", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00e4hrt oftmals schon ins Fl\u00fcgel-Kleid.", "tokens": ["F\u00e4hrt", "oft\u00b7mals", "schon", "ins", "Fl\u00fc\u00b7gel\u00b7Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O schlimme Zeit!", "tokens": ["O", "schlim\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Zu meiner Zeit", "tokens": ["Zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ward Pflicht und Ordnung nicht entweiht.", "tokens": ["Ward", "Pflicht", "und", "Ord\u00b7nung", "nicht", "ent\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mann ward, wie es sich geb\u00fchret,", "tokens": ["Der", "Mann", "ward", ",", "wie", "es", "sich", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einer lieben Frau regieret,", "tokens": ["Von", "ei\u00b7ner", "lie\u00b7ben", "Frau", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz seiner stolzen M\u00e4nnlichkeit!", "tokens": ["Trotz", "sei\u00b7ner", "stol\u00b7zen", "M\u00e4nn\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Fromme herrschte nur gelinder:", "tokens": ["Die", "From\u00b7me", "herrschte", "nur", "ge\u00b7lin\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Uns blieb der Hut und ihm die Kinder.", "tokens": ["Uns", "blieb", "der", "Hut", "und", "ihm", "die", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das war die Mode weit und breit.", "tokens": ["Das", "war", "die", "Mo\u00b7de", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "O gute Zeit!", "tokens": ["O", "gu\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Zu meiner Zeit", "tokens": ["Zu", "mei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "War noch in Ehen Einigkeit.", "tokens": ["War", "noch", "in", "E\u00b7hen", "Ei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jtzt darf der Mann uns fast gebieten,", "tokens": ["Jtzt", "darf", "der", "Mann", "uns", "fast", "ge\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns widersprechen und uns h\u00fcten,", "tokens": ["Uns", "wi\u00b7der\u00b7spre\u00b7chen", "und", "uns", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo man mit Freunden sich erfreut.", "tokens": ["Wo", "man", "mit", "Freun\u00b7den", "sich", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dieser Neuerung im Lande,", "tokens": ["Mit", "die\u00b7ser", "Neu\u00b7e\u00b7rung", "im", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit diesem Fluch im Ehestande", "tokens": ["Mit", "die\u00b7sem", "Fluch", "im", "E\u00b7hes\u00b7tan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hat ein Comet uns l\u00e4ngst bedr\u00e4ut.", "tokens": ["Hat", "ein", "Co\u00b7met", "uns", "l\u00e4ngst", "be\u00b7dr\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "O schlimme Zeit!", "tokens": ["O", "schlim\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}