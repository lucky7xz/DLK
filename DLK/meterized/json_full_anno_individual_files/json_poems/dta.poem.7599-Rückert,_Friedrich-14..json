{"dta.poem.7599": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Perlenmuschel selbst, ganz in die eigne Reinheit", "tokens": ["Die", "Per\u00b7len\u00b7mu\u00b7schel", "selbst", ",", "ganz", "in", "die", "eig\u00b7ne", "Rein\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschlossen, theilet doch des Meeres Allgemeinheit.", "tokens": ["Ver\u00b7schlos\u00b7sen", ",", "thei\u00b7let", "doch", "des", "Mee\u00b7res", "All\u00b7ge\u00b7mein\u00b7heit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "An ihrer Perle Farb' erscheinet, ob sie schwamm", "tokens": ["An", "ih\u00b7rer", "Per\u00b7le", "Fa\u00b7rb'", "er\u00b7schei\u00b7net", ",", "ob", "sie", "schwamm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Flut seicht oder tief, auf Meergras oder Schlamm.", "tokens": ["In", "Flut", "seicht", "o\u00b7der", "tief", ",", "auf", "Meer\u00b7gras", "o\u00b7der", "Schlamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJD", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch ob sie l\u00e4nglicht ward, ei- oder kugelrund,", "tokens": ["Doch", "ob", "sie", "l\u00e4ng\u00b7licht", "ward", ",", "ei", "o\u00b7der", "ku\u00b7gel\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das liegt am Muschelhaus, und nicht am Meeresgrund.", "tokens": ["Das", "liegt", "am", "Mu\u00b7schel\u00b7haus", ",", "und", "nicht", "am", "Mee\u00b7res\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "KON", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ob endlich gr\u00f6\u00dfer, ob sie kleiner selber sei,", "tokens": ["Ob", "end\u00b7lich", "gr\u00f6\u00b7\u00dfer", ",", "ob", "sie", "klei\u00b7ner", "sel\u00b7ber", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "$,", "KOUS", "PPER", "ADJD", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt an der Kraft die von Natur ihr wohnet bei.", "tokens": ["Liegt", "an", "der", "Kraft", "die", "von", "Na\u00b7tur", "ihr", "woh\u00b7net", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "APPR", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Mensch nimmt aus der Welt mehr oder minder Licht,", "tokens": ["Ein", "Mensch", "nimmt", "aus", "der", "Welt", "mehr", "o\u00b7der", "min\u00b7der", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Form aus seinem Stand, und aus sich sein Gewicht.", "tokens": ["Die", "Form", "aus", "sei\u00b7nem", "Stand", ",", "und", "aus", "sich", "sein", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "KON", "APPR", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}