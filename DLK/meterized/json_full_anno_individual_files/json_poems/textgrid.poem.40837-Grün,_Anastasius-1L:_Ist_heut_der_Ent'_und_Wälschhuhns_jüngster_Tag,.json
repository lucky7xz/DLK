{"textgrid.poem.40837": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist heut der Ent' und W\u00e4lschhuhns j\u00fcngster Tag,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist heut der Ent' und W\u00e4lschhuhns j\u00fcngster Tag,", "tokens": ["Ist", "heut", "der", "Ent'", "und", "W\u00e4l\u00b7schhuhns", "j\u00fcngs\u00b7ter", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df rings ihr Kr\u00e4chzen schreit aus Hof und Hag?", "tokens": ["Da\u00df", "rings", "ihr", "Kr\u00e4ch\u00b7zen", "schreit", "aus", "Hof", "und", "Hag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Pflanzer r\u00fcckt zur Wachtparad' von Haus", "tokens": ["Der", "Pflan\u00b7zer", "r\u00fcckt", "zur", "Wacht\u00b7pa\u00b7rad'", "von", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und rupft sich einen Federbusch erst aus!", "tokens": ["Und", "rupft", "sich", "ei\u00b7nen", "Fe\u00b7der\u00b7busch", "erst", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Festtag ist's der Unabh\u00e4ngigkeit!", "tokens": ["Der", "Fest\u00b7tag", "ist's", "der", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vor Pittsburgs Thoren stehn ins Glied gereiht", "tokens": ["Vor", "Pitts\u00b7burgs", "Tho\u00b7ren", "stehn", "ins", "Glied", "ge\u00b7reiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Pflugs, der Werkstatt S\u00f6hne, kriegrisch bunt,", "tokens": ["Des", "Pflugs", ",", "der", "Werk\u00b7statt", "S\u00f6h\u00b7ne", ",", "krie\u00b7grisch", "bunt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Glatzkopf hier, dort J\u00fcngling Rosenmund!", "tokens": ["Der", "Glatz\u00b7kopf", "hier", ",", "dort", "J\u00fcng\u00b7ling", "Ro\u00b7sen\u00b7mund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kopfsch\u00fcttelnd wallt der Hauptmann durch die Reihn", "tokens": ["Kopf\u00b7sch\u00fct\u00b7telnd", "wallt", "der", "Haupt\u00b7mann", "durch", "die", "Reihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Wei\u00df und Kupferfarb' und Gro\u00df und Klein!", "tokens": ["Durch", "Wei\u00df", "und", "Kup\u00b7fer\u00b7fa\u00b7rb'", "und", "Gro\u00df", "und", "Klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Jacke hier, daneben der Talar,", "tokens": ["Die", "Ja\u00b7cke", "hier", ",", "da\u00b7ne\u00b7ben", "der", "Ta\u00b7lar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PAV", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Per\u00fcckenhaupt und wehend Lockenhaar!", "tokens": ["Pe\u00b7r\u00fc\u00b7cken\u00b7haupt", "und", "we\u00b7hend", "Lo\u00b7cken\u00b7haar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da\u00df Gott erbarm'! Ei, Nachbar lieb und werth,", "tokens": ["Da\u00df", "Gott", "er\u00b7barm'", "!", "Ei", ",", "Nach\u00b7bar", "lieb", "und", "werth", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$.", "NN", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr tragt ein gar zu rostig, schartig Schwert!", "tokens": ["Ihr", "tragt", "ein", "gar", "zu", "ros\u00b7tig", ",", "schar\u00b7tig", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "PTKA", "ADJD", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbbei Saratoga trug's mein Vater schon,", "tokens": ["\u00bb", "bei", "Sa\u00b7ra\u00b7to\u00b7ga", "trug's", "mein", "Va\u00b7ter", "schon", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "NE", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Pfirsichbaum stutzt jetzt damit der Sohn!", "tokens": ["Den", "Pfir\u00b7sich\u00b7baum", "stutzt", "jetzt", "da\u00b7mit", "der", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So tr\u00e4gt es stolz, von Sieg und Lenz erw\u00e4hlt,", "tokens": ["So", "tr\u00e4gt", "es", "stolz", ",", "von", "Sieg", "und", "Lenz", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Kriegs und Friedens Scharten sch\u00f6n verm\u00e4hlt,", "tokens": ["Des", "Kriegs", "und", "Frie\u00b7dens", "Schar\u00b7ten", "sch\u00f6n", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie auf des wahren Helden Angesicht", "tokens": ["Wie", "auf", "des", "wah\u00b7ren", "Hel\u00b7den", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Schlacht und Schenke Narb' in Eins sich flicht!\u00ab", "tokens": ["Der", "Schlacht", "und", "Schen\u00b7ke", "Na\u00b7rb'", "in", "Eins", "sich", "flicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "APPR", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "He, Freund, dein Helmbusch spielt gar selt'nen Glanz!", "tokens": ["He", ",", "Freund", ",", "dein", "Helm\u00b7busch", "spielt", "gar", "selt'\u00b7nen", "Glanz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich mein', er wuchs auf eines Hahnen Schwanz!", "tokens": ["Ich", "mein'", ",", "er", "wuchs", "auf", "ei\u00b7nes", "Hah\u00b7nen", "Schwanz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbei, ist der Hahn mir doch kein \u00fcbler Bot',", "tokens": ["\u00bb", "ei", ",", "ist", "der", "Hahn", "mir", "doch", "kein", "\u00fcb\u00b7ler", "Bot'", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "ART", "NN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Ruf und Fl\u00fcgelschlag bringt Morgenroth!\u00ab", "tokens": ["Sein", "Ruf", "und", "Fl\u00fc\u00b7gel\u00b7schlag", "bringt", "Mor\u00b7gen\u00b7roth", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Den Bauch zur\u00fcck, Gevatter, wenn du's kannst!", "tokens": ["Den", "Bauch", "zu\u00b7r\u00fcck", ",", "Ge\u00b7vat\u00b7ter", ",", "wenn", "du's", "kannst", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "NN", "$,", "KOUS", "PIS", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ganze sch\u00f6ne Front verdirbt dein Wanst!", "tokens": ["Die", "gan\u00b7ze", "sch\u00f6\u00b7ne", "Front", "ver\u00b7dirbt", "dein", "Wanst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bber ist nur eine Festung mehr dem Land!", "tokens": ["\u00bb", "er", "ist", "nur", "ei\u00b7ne", "Fes\u00b7tung", "mehr", "dem", "Land", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vertheid'gen soll sie mannlich meine Hand!\u00ab", "tokens": ["Vert\u00b7hei\u00b7d'\u00b7gen", "soll", "sie", "mann\u00b7lich", "mei\u00b7ne", "Hand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Der tr\u00e4gt die Whiskyflasche angeschnallt,", "tokens": ["Der", "tr\u00e4gt", "die", "Whis\u00b7ky\u00b7fla\u00b7sche", "an\u00b7ge\u00b7schnallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie das Osagenweib ihr Kind im Wald!", "tokens": ["Wie", "das", "O\u00b7sa\u00b7gen\u00b7weib", "ihr", "Kind", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "\u00bbwohl eines sch\u00f6nen Kornfelds guter Geist", "tokens": ["\u00bb", "wohl", "ei\u00b7nes", "sch\u00f6\u00b7nen", "Korn\u00b7felds", "gu\u00b7ter", "Geist"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wohnt drin, der mich der Heimat denken hei\u00dft!\u00ab", "tokens": ["Wohnt", "drin", ",", "der", "mich", "der", "Hei\u00b7mat", "den\u00b7ken", "hei\u00dft", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PRELS", "PRF", "ART", "NN", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "He, Fl\u00fcgelmann, dein Zopf erschreckt mich fast,", "tokens": ["He", ",", "Fl\u00fc\u00b7gel\u00b7mann", ",", "dein", "Zopf", "er\u00b7schreckt", "mich", "fast", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Steif und gespenstisch, wie ein kahler Ast!", "tokens": ["Steif", "und", "ge\u00b7spens\u00b7tisch", ",", "wie", "ein", "kah\u00b7ler", "Ast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbund ist's ein Ast, h\u00fcpft wohl ein V\u00f6glein drauf", "tokens": ["\u00bb", "und", "ist's", "ein", "Ast", ",", "h\u00fcpft", "wohl", "ein", "V\u00f6\u00b7glein", "drauf"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VAFIN", "ART", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und spielt ein h\u00fcbsches Lied von Freiheit auf!\u00ab", "tokens": ["Und", "spielt", "ein", "h\u00fcb\u00b7sches", "Lied", "von", "Frei\u00b7heit", "auf", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Heda, we\u00df ist das F\u00fcllen, das dort l\u00e4uft,", "tokens": ["He\u00b7da", ",", "we\u00df", "ist", "das", "F\u00fcl\u00b7len", ",", "das", "dort", "l\u00e4uft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und an des F\u00e4hnrichs brauner Stute s\u00e4uft?", "tokens": ["Und", "an", "des", "F\u00e4hn\u00b7richs", "brau\u00b7ner", "Stu\u00b7te", "s\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbz\u00fcrnt nicht! Wer w\u00e4re doch so schlimm gesinnt,", "tokens": ["\u00bb", "z\u00fcrnt", "nicht", "!", "Wer", "w\u00e4\u00b7re", "doch", "so", "schlimm", "ge\u00b7sinnt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "$.", "PWS", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu trennen gar die Mutter von dem Kind!\u00ab", "tokens": ["Zu", "tren\u00b7nen", "gar", "die", "Mut\u00b7ter", "von", "dem", "Kind", "!", "\u00ab"], "token_info": 