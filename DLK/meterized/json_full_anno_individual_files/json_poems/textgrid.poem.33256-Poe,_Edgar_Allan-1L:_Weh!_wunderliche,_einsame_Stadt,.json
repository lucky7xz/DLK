{"textgrid.poem.33256": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weh! wunderliche, einsame Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weh! wunderliche, einsame Stadt,", "tokens": ["Weh", "!", "wun\u00b7der\u00b7li\u00b7che", ",", "ein\u00b7sa\u00b7me", "Stadt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Drin Tod seinen Thron errichtet hat,", "tokens": ["Drin", "Tod", "sei\u00b7nen", "Thron", "er\u00b7rich\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tief unter des Westens d\u00fcsterer Glut,", "tokens": ["Tief", "un\u00b7ter", "des", "Wes\u00b7tens", "d\u00fcs\u00b7te\u00b7rer", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo S\u00fcnde bei G\u00fcte, wo Schlecht bei Gut", "tokens": ["Wo", "S\u00fcn\u00b7de", "bei", "G\u00fc\u00b7te", ",", "wo", "Schlecht", "bei", "Gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "PWAV", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "In letzter ewiger Ruhe ruht.", "tokens": ["In", "letz\u00b7ter", "e\u00b7wi\u00b7ger", "Ru\u00b7he", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An Schl\u00f6ssern, Alt\u00e4ren und T\u00fcrmen hat", "tokens": ["An", "Schl\u00f6s\u00b7sern", ",", "Al\u00b7t\u00e4\u00b7ren", "und", "T\u00fcr\u00b7men", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VAFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "(zerfre\u00dfnen T\u00fcrmen, die nicht beben!)", "tokens": ["(", "zer\u00b7fre\u00df\u00b7nen", "T\u00fcr\u00b7men", ",", "die", "nicht", "be\u00b7ben", "!", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts Gleiches eine unsrige Stadt.", "tokens": ["Nichts", "Glei\u00b7ches", "ei\u00b7ne", "uns\u00b7ri\u00b7ge", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Von Winden vergessen, die w\u00fchlen und heben,", "tokens": ["Von", "Win\u00b7den", "ver\u00b7ges\u00b7sen", ",", "die", "w\u00fch\u00b7len", "und", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PRELS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Stehn unterm Himmel die Wasser ringsum,", "tokens": ["Stehn", "un\u00b7term", "Him\u00b7mel", "die", "Was\u00b7ser", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Schwerm\u00fctige Wasser, ergeben und stumm.", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Was\u00b7ser", ",", "er\u00b7ge\u00b7ben", "und", "stumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Kein Strahlen vom Himmel kommt herab", "tokens": ["Kein", "Strah\u00b7len", "vom", "Him\u00b7mel", "kommt", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf jener Stadt langn\u00e4chtiges Grab.", "tokens": ["Auf", "je\u00b7ner", "Stadt", "lang\u00b7n\u00e4ch\u00b7ti\u00b7ges", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch steigt ein Licht aus dem Meere herauf,", "tokens": ["Doch", "steigt", "ein", "Licht", "aus", "dem", "Mee\u00b7re", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Str\u00f6mt schweigend an k\u00fchnen Zinnen hinauf,", "tokens": ["Str\u00f6mt", "schwei\u00b7gend", "an", "k\u00fch\u00b7nen", "Zin\u00b7nen", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Hinauf an T\u00fcrmen bis zum Knauf,", "tokens": ["Hin\u00b7auf", "an", "T\u00fcr\u00b7men", "bis", "zum", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hinauf an Pal\u00e4sten, an Zitadellen,", "tokens": ["Hin\u00b7auf", "an", "Pa\u00b7l\u00e4s\u00b7ten", ",", "an", "Zi\u00b7ta\u00b7del\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "An Tempeln hinauf und an Babylonw\u00e4llen,", "tokens": ["An", "Tem\u00b7peln", "hin\u00b7auf", "und", "an", "Ba\u00b7by\u00b7lon\u00b7w\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "KON", "APPR", "NE", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Hinauf an vergessenen Laubeng\u00e4ngen", "tokens": ["Hin\u00b7auf", "an", "ver\u00b7ges\u00b7se\u00b7nen", "Lau\u00b7ben\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "----+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mit eingemei\u00dfelten Fruchtgeh\u00e4ngen,", "tokens": ["Mit", "ein\u00b7ge\u00b7mei\u00b7\u00dfel\u00b7ten", "Frucht\u00b7ge\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Hinauf an manchem Opferstein,", "tokens": ["Hin\u00b7auf", "an", "man\u00b7chem", "Op\u00b7fer\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Auf dessen Friesen zu engem Verein", "tokens": ["Auf", "des\u00b7sen", "Frie\u00b7sen", "zu", "en\u00b7gem", "Ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Verflochten Viola, Violen und Wein.", "tokens": ["Ver\u00b7floch\u00b7ten", "Vi\u00b7o\u00b7la", ",", "Vi\u00b7o\u00b7len", "und", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Stehn unterm Himmel die Wasser ringsum,", "tokens": ["Stehn", "un\u00b7term", "Him\u00b7mel", "die", "Was\u00b7ser", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwerm\u00fctige Wasser, ergeben und stumm.", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ge", "Was\u00b7ser", ",", "er\u00b7ge\u00b7ben", "und", "stumm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Mauern und Schatten wie Nebelduft \u2013", "tokens": ["Die", "Mau\u00b7ern", "und", "Schat\u00b7ten", "wie", "Ne\u00b7bel\u00b7duft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KOKOM", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es scheint, als h\u00e4nge alles in Luft.", "tokens": ["Es", "scheint", ",", "als", "h\u00e4n\u00b7ge", "al\u00b7les", "in", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Vom Turm, der herrschend ragt und droht,", "tokens": ["Vom", "Turm", ",", "der", "herr\u00b7schend", "ragt", "und", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schaut riesenhaft herab der Tod.", "tokens": ["Schaut", "rie\u00b7sen\u00b7haft", "her\u00b7ab", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ge\u00f6ffnete Tempel und Totengr\u00fcfte", "tokens": ["Ge\u00b7\u00f6ff\u00b7ne\u00b7te", "Tem\u00b7pel", "und", "To\u00b7ten\u00b7gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "G\u00e4hnen auf leuchtende Meeresschl\u00fcfte.", "tokens": ["G\u00e4h\u00b7nen", "auf", "leuch\u00b7ten\u00b7de", "Mee\u00b7res\u00b7schl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch nicht die blitzenden Juwelen", "tokens": ["Doch", "nicht", "die", "blit\u00b7zen\u00b7den", "Ju\u00b7we\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In goldner G\u00f6tzen Augenh\u00f6hlen", "tokens": ["In", "gold\u00b7ner", "G\u00f6t\u00b7zen", "Au\u00b7gen\u00b7h\u00f6h\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nicht der reiche Tod verf\u00fchren", "tokens": ["Und", "nicht", "der", "rei\u00b7che", "Tod", "ver\u00b7f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die starren Wasser, sich zu r\u00fchren:", "tokens": ["Die", "star\u00b7ren", "Was\u00b7ser", ",", "sich", "zu", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kein kleinstes Wellchen kommt in Gang", "tokens": ["Kein", "kleins\u00b7tes", "Well\u00b7chen", "kommt", "in", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die gl\u00e4serne Ein\u00f6de entlang;", "tokens": ["Die", "gl\u00e4\u00b7ser\u00b7ne", "Ein\u00b7\u00f6\u00b7de", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPO", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Kein Kr\u00e4useln erinnert, da\u00df weniger leer", "tokens": ["Kein", "Kr\u00e4u\u00b7seln", "e\u00b7rin\u00b7nert", ",", "da\u00df", "we\u00b7ni\u00b7ger", "leer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Von Wind ist irgendein anderes Meer,", "tokens": ["Von", "Wind", "ist", "ir\u00b7gend\u00b7ein", "an\u00b7de\u00b7res", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nichts sagt, da\u00df je ein Wehen war", "tokens": ["Nichts", "sagt", ",", "da\u00df", "je", "ein", "We\u00b7hen", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf Meeren, die weniger grauenhaft klar.", "tokens": ["Auf", "Mee\u00b7ren", ",", "die", "we\u00b7ni\u00b7ger", "grau\u00b7en\u00b7haft", "klar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Doch, oh \u2013 es regt sich leis wie Wind!", "tokens": ["Doch", ",", "oh", "\u2013", "es", "regt", "sich", "leis", "wie", "Wind", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "$(", "PPER", "VVFIN", "PRF", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wellen durch das Wasser rinnt \u2013", "tokens": ["Ein", "Wel\u00b7len", "durch", "das", "Was\u00b7ser", "rinnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob die T\u00fcrme im sachten Sinken", "tokens": ["Als", "ob", "die", "T\u00fcr\u00b7me", "im", "sach\u00b7ten", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die Flut versch\u00f6ben zur Rechten und Linken \u2013", "tokens": ["Die", "Flut", "ver\u00b7sch\u00f6\u00b7ben", "zur", "Rech\u00b7ten", "und", "Lin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als ob schon die Spitzen inmitten des blassen", "tokens": ["Als", "ob", "schon", "die", "Spit\u00b7zen", "in\u00b7mit\u00b7ten", "des", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Himmels L\u00fccken zur\u00fcckgelassen.", "tokens": ["Him\u00b7mels", "L\u00fc\u00b7cken", "zu\u00b7r\u00fcck\u00b7ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ein roteres Glimmen steigt heran \u2013", "tokens": ["Ein", "ro\u00b7te\u00b7res", "Glim\u00b7men", "steigt", "he\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Stunden halten den Atem an \u2013", "tokens": ["Die", "Stun\u00b7den", "hal\u00b7ten", "den", "A\u00b7tem", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und wenn die Stadt hinab, hinab", "tokens": ["Und", "wenn", "die", "Stadt", "hin\u00b7ab", ",", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Von hinnen sinkt mit unirdischem St\u00f6hnen,", "tokens": ["Von", "hin\u00b7nen", "sinkt", "mit", "un\u00b7ir\u00b7di\u00b7schem", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+