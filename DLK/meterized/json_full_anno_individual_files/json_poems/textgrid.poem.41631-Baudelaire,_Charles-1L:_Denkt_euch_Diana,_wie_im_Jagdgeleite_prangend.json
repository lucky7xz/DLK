{"textgrid.poem.41631": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denkt euch Diana, wie im Jagdgeleite prangend", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkt euch Diana, wie im Jagdgeleite prangend", "tokens": ["Denkt", "euch", "Di\u00b7a\u00b7na", ",", "wie", "im", "Jagd\u00b7ge\u00b7lei\u00b7te", "pran\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$,", "PWAV", "APPRART", "NN", "VVPP"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie durch die W\u00e4lder streift und durch das Dickicht fegt,", "tokens": ["Sie", "durch", "die", "W\u00e4l\u00b7der", "streift", "und", "durch", "das", "Di\u00b7ckicht", "fegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Winde Brust und Haar, l\u00e4rmtrunken, nie erbangend,", "tokens": ["Im", "Win\u00b7de", "Brust", "und", "Haar", ",", "l\u00e4rm\u00b7trun\u00b7ken", ",", "nie", "er\u00b7ban\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df nicht im Laufe sie den schnellsten Renner schl\u00e4gt!", "tokens": ["Da\u00df", "nicht", "im", "Lau\u00b7fe", "sie", "den", "schnells\u00b7ten", "Ren\u00b7ner", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKN