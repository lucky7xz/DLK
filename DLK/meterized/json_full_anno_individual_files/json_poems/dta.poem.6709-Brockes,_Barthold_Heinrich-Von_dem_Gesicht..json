{"dta.poem.6709": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und durch die gantze Welt erschallet ihr Gesang.", "tokens": ["Und", "durch", "die", "gant\u00b7ze", "Welt", "er\u00b7schal\u00b7let", "ihr", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von V\u00f6lckern, welche wir so sehr verschieden sehen", "tokens": ["Von", "V\u00f6l\u00b7ckern", ",", "wel\u00b7che", "wir", "so", "sehr", "ver\u00b7schie\u00b7den", "se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Sitten, Sprach\u2019 und Art, und die von weiten Meer", "tokens": ["An", "Sit\u00b7ten", ",", "Sprach'", "und", "Art", ",", "und", "die", "von", "wei\u00b7ten", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den weitgetheilten Strand zur Wohnung \u00fcberkommen;", "tokens": ["Den", "weit\u00b7ge\u00b7theil\u00b7ten", "Strand", "zur", "Woh\u00b7nung", "\u00fc\u00b7ber\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Selbst von den Schaaren", "tokens": ["Selbst", "von", "den", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der allerwildesten Barbaren", "tokens": ["Der", "al\u00b7ler\u00b7wil\u00b7des\u00b7ten", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wird dieses G\u00f6ttliche Concert vernommen.", "tokens": ["Wird", "die\u00b7ses", "G\u00f6tt\u00b7li\u00b7che", "Con\u00b7cert", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}