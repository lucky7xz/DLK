{"textgrid.poem.34121": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unsterbliche und Unbegrabene", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trennt Unsterbliche nur von Unbegrabenen, Freunde,", "tokens": ["Trennt", "U\u00b7nsterb\u00b7li\u00b7che", "nur", "von", "Un\u00b7be\u00b7gra\u00b7be\u00b7nen", ",", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+---+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Alle Unsterblichkeit hat nur ein einziges Maa\u00df!", "tokens": ["Al\u00b7le", "U\u00b7nsterb\u00b7lich\u00b7keit", "hat", "nur", "ein", "ein\u00b7zi\u00b7ges", "Maa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+---+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Das ist unsterblich, was lebt, was unverl\u00f6schliche Funken", "tokens": ["Das", "ist", "uns\u00b7terb\u00b7lich", ",", "was", "lebt", ",", "was", "un\u00b7ver\u00b7l\u00f6\u00b7schli\u00b7che", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "PWS", "VVFIN", "$,", "PWS", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Spr\u00fcht, die noch z\u00fcnden in uns, glaubt mir, das And're ist todt.", "tokens": ["Spr\u00fcht", ",", "die", "noch", "z\u00fcn\u00b7den", "in", "uns", ",", "glaubt", "mir", ",", "das", "An\u00b7d'\u00b7re", "ist", "todt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "PPER", "$,", "PRELS", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--+---+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "So ist Homer unsterblich, und durch den Homer auch Achilles", "tokens": ["So", "ist", "Ho\u00b7mer", "uns\u00b7terb\u00b7lich", ",", "und", "durch", "den", "Ho\u00b7mer", "auch", "A\u00b7chil\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "ADJD", "$,", "KON", "APPR", "ART", "NE", "ADV", "NE"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Aeschylos, Sophokles so, Shakespeare, ja Goethe sogar,", "tokens": ["A\u00b7esc\u00b7hy\u00b7los", ",", "So\u00b7phok\u00b7les", "so", ",", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", ",", "ja", "Goe\u00b7the", "so\u00b7gar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "ADV", "$,", "NE", "$,", "ADV", "NE", "ADV", "$,"], "meter": "+--+-+--+--+--+--+", "measure": "iambic.septa.invert"}, "line.7": {"text": "Aber Napoleon stirbt, wofern ihm ein sp\u00e4t'res Jahrhundert", "tokens": ["A\u00b7ber", "Na\u00b7po\u00b7le\u00b7on", "stirbt", ",", "wo\u00b7fern", "ihm", "ein", "sp\u00e4t'\u00b7res", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Nicht den Dichter erweckt, der ihm das Leben verb\u00fcrgt.", "tokens": ["Nicht", "den", "Dich\u00b7ter", "er\u00b7weckt", ",", "der", "ihm", "das", "Le\u00b7ben", "ver\u00b7b\u00fcrgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVPP", "$,", "PREL