{"textgrid.poem.35185": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1901", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlie\u00df auf das Thor; la\u00df seine Fl\u00fcgel springen;", "tokens": ["Schlie\u00df", "auf", "das", "Thor", ";", "la\u00df", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Z\u00fcnd deine Leuchte an in allen Landen!", "tokens": ["Z\u00fcnd", "dei\u00b7ne", "Leuch\u00b7te", "an", "in", "al\u00b7len", "Lan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist, als h\u00f6rte ich den Ruf erklingen,", "tokens": ["Mir", "ist", ",", "als", "h\u00f6r\u00b7te", "ich", "den", "Ruf", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es sei der Tod zum Leben auferstanden.", "tokens": ["Es", "sei", "der", "Tod", "zum", "Le\u00b7ben", "auf\u00b7er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Breit deine Fluren aus und deine Pfade;", "tokens": ["Breit", "dei\u00b7ne", "Flu\u00b7ren", "aus", "und", "dei\u00b7ne", "Pfa\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df deine Wasser klar und freundlich flie\u00dfen,", "tokens": ["La\u00df", "dei\u00b7ne", "Was\u00b7ser", "klar", "und", "freund\u00b7lich", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und von dem Himmel m\u00f6ge sich die Gnade", "tokens": ["Und", "von", "dem", "Him\u00b7mel", "m\u00f6\u00b7ge", "sich", "die", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Auf Alles, was die Erde tr\u00e4gt, ergie\u00dfen.", "tokens": ["Auf", "Al\u00b7les", ",", "was", "die", "Er\u00b7de", "tr\u00e4gt", ",", "er\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schlie\u00df auf das Thor; es tritt die Menschheit ein;", "tokens": ["Schlie\u00df", "auf", "das", "Thor", ";", "es", "tritt", "die", "Menschheit", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "O, la\u00df ihr diesen Schritt gesegnet sein!", "tokens": ["O", ",", "la\u00df", "ihr", "die\u00b7sen", "Schritt", "ge\u00b7seg\u00b7net", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "PDAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schlie\u00df auf den Schrein, vor dem wir betend knieen,", "tokens": ["Schlie\u00df", "auf", "den", "Schrein", ",", "vor", "dem", "wir", "be\u00b7tend", "kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dem wir die Liebe, die Verehrung zollen,", "tokens": ["Dem", "wir", "die", "Lie\u00b7be", ",", "die", "Ver\u00b7eh\u00b7rung", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wir auf seinen Inhalt doch beziehen", "tokens": ["Die", "wir", "auf", "sei\u00b7nen", "In\u00b7halt", "doch", "be\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht dem Menschenwerke widmen sollen!", "tokens": ["Und", "nicht", "dem", "Men\u00b7schen\u00b7wer\u00b7ke", "wid\u00b7men", "sol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df uns erkennen, was wir nicht erkannten,", "tokens": ["La\u00df", "uns", "er\u00b7ken\u00b7nen", ",", "was", "wir", "nicht", "er\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil uns der Geist die Seele stets verhehlte;", "tokens": ["Weil", "uns", "der", "Geist", "die", "See\u00b7le", "stets", "ver\u00b7hehl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df uns verstehen, was wir nicht verstanden,", "tokens": ["La\u00df", "uns", "ver\u00b7ste\u00b7hen", ",", "was", "wir", "nicht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Weil uns die wahre Liebe nicht beseelte.", "tokens": ["Weil", "uns", "die", "wah\u00b7re", "Lie\u00b7be", "nicht", "be\u00b7seel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schlie\u00df auf den Schrein, und zeig, was er enth\u00e4lt,", "tokens": ["Schlie\u00df", "auf", "den", "Schrein", ",", "und", "zeig", ",", "was", "er", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df mit dem Schleier auch der Irrthum f\u00e4llt!", "tokens": ["Da\u00df", "mit", "dem", "Schlei\u00b7er", "auch", "der", "Irr\u00b7thum", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schlie\u00df auf die Herzen; nirgends stehn sie offen,", "tokens": ["Schlie\u00df", "auf", "die", "Her\u00b7zen", ";", "nir\u00b7gends", "stehn", "sie", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn jedes will nur f\u00fcr sich selbst empfinden,", "tokens": ["Denn", "je\u00b7des", "will", "nur", "f\u00fcr", "sich", "selbst", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch ist es ihr eignes, sch\u00f6nstes Hoffen,", "tokens": ["Und", "doch", "ist", "es", "ihr", "eig\u00b7nes", ",", "sch\u00f6ns\u00b7tes", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df sie in Liebe sich zusammenfinden!", "tokens": ["Da\u00df", "sie", "in", "Lie\u00b7be", "sich", "zu\u00b7sam\u00b7men\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df diese Liebe endlich doch erwachen", "tokens": ["La\u00df", "die\u00b7se", "Lie\u00b7be", "end\u00b7lich", "doch", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aus dem Ich heraus ins Leben steigen,", "tokens": ["Und", "aus", "dem", "Ich", "he\u00b7raus", "ins", "Le\u00b7ben", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Menschen zur gesammten Menschheit machen", "tokens": ["Die", "Men\u00b7schen", "zur", "ge\u00b7samm\u00b7ten", "Menschheit", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und sich als Seele dieses Leibes zeigen.", "tokens": ["Und", "sich", "als", "See\u00b7le", "die\u00b7ses", "Lei\u00b7bes", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOUS", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schlie\u00df auf die Herzen; lehre sie verstehn,", "tokens": ["Schlie\u00df", "auf", "die", "Her\u00b7zen", ";", "leh\u00b7re", "sie", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df alle Pulse nur als einer gehn!", "tokens": ["Da\u00df", "al\u00b7le", "Pul\u00b7se", "nur", "als", "ei\u00b7ner", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "KOUS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schlie\u00df auf das Paradies; gieb es uns wieder!", "tokens": ["Schlie\u00df", "auf", "das", "Pa\u00b7ra\u00b7dies", ";", "gieb", "es", "uns", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVIMP", "PPER", "PRF", "ADV", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir wollen heim; wir wollen Frieden halten.", "tokens": ["Wir", "wol\u00b7len", "heim", ";", "wir", "wol\u00b7len", "Frie\u00b7den", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$.", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Vater ist das Haupt; wir sind die Glieder;", "tokens": ["Der", "Va\u00b7ter", "ist", "das", "Haupt", ";", "wir", "sind", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur seine G\u00fcte soll im Hause walten.", "tokens": ["Nur", "sei\u00b7ne", "G\u00fc\u00b7te", "soll", "im", "Hau\u00b7se", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sei du die Zeit, die uns um ihn versammelt,", "tokens": ["Sei", "du", "die", "Zeit", ",", "die", "uns", "um", "ihn", "ver\u00b7sam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zeig uns der Worte k\u00f6stlichstes auf Erden,", "tokens": ["Zeig", "uns", "der", "Wor\u00b7te", "k\u00f6st\u00b7lichs\u00b7tes", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADJA", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Das unsre Bitte um Vers\u00f6hnung stammelt,", "tokens": ["Das", "uns\u00b7re", "Bit\u00b7te", "um", "Ver\u00b7s\u00f6h\u00b7nung", "stam\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dann wirst du eine Zeit des Edens werden.", "tokens": ["Dann", "wirst", "du", "ei\u00b7ne", "Zeit", "des", "E\u00b7dens", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Schlie\u00df auf das Paradies, das Gottesland,", "tokens": ["Schlie\u00df", "auf", "das", "Pa\u00b7ra\u00b7dies", ",", "das", "Got\u00b7tes\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und sei uns zur Erleuchtung zugesandt!", "tokens": ["Und", "sei", "uns", "zur", "Er\u00b7leuch\u00b7tung", "zu\u00b7ge\u00b7sandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Schlie\u00df auf das Thor; la\u00df sei