{"textgrid.poem.60176": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, o mein Freund, mein Sulzer will ich singen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, o mein Freund, mein Sulzer will ich singen,", "tokens": ["Dir", ",", "o", "mein", "Freund", ",", "mein", "Sul\u00b7zer", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den noch zu oft Empfindungen durchdringen,", "tokens": ["Den", "noch", "zu", "oft", "Emp\u00b7fin\u00b7dun\u00b7gen", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bis zu des Lebens Ueberdru\u00df.", "tokens": ["Bis", "zu", "des", "Le\u00b7bens", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sollst den Gram als Weiser einst bek\u00e4mpfen,", "tokens": ["Du", "sollst", "den", "Gram", "als", "Wei\u00b7ser", "einst", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "KOUS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sonst wird er ganz den sch\u00f6nsten Trieb verd\u00e4mpfen,", "tokens": ["Sonst", "wird", "er", "ganz", "den", "sch\u00f6ns\u00b7ten", "Trieb", "ver\u00b7d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den sanften Trieb zu Lieb und Ku\u00df!", "tokens": ["Den", "sanf\u00b7ten", "Trieb", "zu", "Lieb", "und", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Ernst spricht m\u00e4nnlich dir im Angesichte,", "tokens": ["Der", "Ernst", "spricht", "m\u00e4nn\u00b7lich", "dir", "im", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dein Gespr\u00e4ch voll Nachdruck und Gewichte,", "tokens": ["Und", "dein", "Ge\u00b7spr\u00e4ch", "voll", "Nach\u00b7druck", "und", "Ge\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4hlt nie ein Wort von leichtem Scherz.", "tokens": ["W\u00e4hlt", "nie", "ein", "Wort", "von", "leich\u00b7tem", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie lachst du laut, selbst bey dem L\u00e4cherlichen,", "tokens": ["Nie", "lachst", "du", "laut", ",", "selbst", "bey", "dem", "L\u00e4\u00b7cher\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein L\u00e4cheln aber sagt mit Redner-Spr\u00fcchen,", "tokens": ["Dein", "L\u00e4\u00b7cheln", "a\u00b7ber", "sagt", "mit", "Red\u00b7ner\u00b7Spr\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du habest kein versteintes Herz!", "tokens": ["Du", "ha\u00b7best", "kein", "ver\u00b7stein\u00b7tes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst verl\u00e4ugnen deiner Stirne Falten", "tokens": ["Um\u00b7sonst", "ver\u00b7l\u00e4ug\u00b7nen", "dei\u00b7ner", "Stir\u00b7ne", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Seele Bild; ich kenne die Gestalten", "tokens": ["Der", "See\u00b7le", "Bild", ";", "ich", "ken\u00b7ne", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von den Empfindungen zu gut;", "tokens": ["Von", "den", "Emp\u00b7fin\u00b7dun\u00b7gen", "zu", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "O, mir entwischt nicht was die Menschen f\u00fchlen!", "tokens": ["O", ",", "mir", "ent\u00b7wischt", "nicht", "was", "die", "Men\u00b7schen", "f\u00fch\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PTKNEG", "PWS", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Die Lehrer auf der Weisheit hohen St\u00fchlen,", "tokens": ["Die", "Leh\u00b7rer", "auf", "der", "Weis\u00b7heit", "ho\u00b7hen", "St\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind, so wie Dichter, Fleisch und Blut.", "tokens": ["Sind", ",", "so", "wie", "Dich\u00b7ter", ",", "Fleisch", "und", "Blut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "KOKOM", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Fr\u00fchling kommt mit Reizen f\u00fcr das Herze,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "kommt", "mit", "Rei\u00b7zen", "f\u00fcr", "das", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Deine schwimmt noch halb in seinem Schmerze.", "tokens": ["Das", "Dei\u00b7ne", "schwimmt", "noch", "halb", "in", "sei\u00b7nem", "Schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rei\u00df es heraus und werde dein!", "tokens": ["Rei\u00df", "es", "he\u00b7raus", "und", "wer\u00b7de", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "PPOSAT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ganz dein! dich hat der Gram genug durchdrungen,", "tokens": ["Ganz", "dein", "!", "dich", "hat", "der", "Gram", "ge\u00b7nug", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00f6r' die Natur in ihren Foderungen,", "tokens": ["H\u00f6r'", "die", "Na\u00b7tur", "in", "ih\u00b7ren", "Fo\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und la\u00df die Liebe Herrin seyn!", "tokens": ["Und", "la\u00df", "die", "Lie\u00b7be", "Her\u00b7rin", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vern\u00fcnftig, g\u00f6ttlich, Engeln wohlgef\u00e4llig,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", ",", "g\u00f6tt\u00b7lich", ",", "En\u00b7geln", "wohl\u00b7ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Treu, dauerhaft, mit Tugenden gesellig", "tokens": ["Treu", ",", "dau\u00b7er\u00b7haft", ",", "mit", "Tu\u00b7gen\u00b7den", "ge\u00b7sel\u00b7lig"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War deine Liebe ehedem.", "tokens": ["War", "dei\u00b7ne", "Lie\u00b7be", "e\u00b7he\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird sie jetzt neu dich bewohnen kommen,", "tokens": ["So", "wird", "sie", "jetzt", "neu", "dich", "be\u00b7woh\u00b7nen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und f\u00fcr die Zeit, von Trauren dir genommen,", "tokens": ["Und", "f\u00fcr", "die", "Zeit", ",", "von", "Trau\u00b7ren", "dir", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Macht sie die Zukunft angenehm!", "tokens": ["Macht", "sie", "die", "Zu\u00b7kunft", "an\u00b7ge\u00b7nehm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie kommt vielleicht daher, wo stolzer Friede", "tokens": ["Sie", "kommt", "viel\u00b7leicht", "da\u00b7her", ",", "wo", "stol\u00b7zer", "Frie\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Bergen wohnt, die uns in Hallers Liede", "tokens": ["An", "Ber\u00b7gen", "wohnt", ",", "die", "uns", "in", "Hal\u00b7lers", "Lie\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So pr\u00e4chtig vor dem Auge stehn!", "tokens": ["So", "pr\u00e4ch\u00b7tig", "vor", "dem", "Au\u00b7ge", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie bringet dir den Frieden in den Busen,", "tokens": ["Sie", "brin\u00b7get", "dir", "den", "Frie\u00b7den", "in", "den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und stiller, als die unbesorgten Musen", "tokens": ["Und", "stil\u00b7ler", ",", "als", "die", "un\u00b7be\u00b7sorg\u00b7ten", "Mu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird Lieb und Freude mit dir gehn!", "tokens": ["Wird", "Lieb", "und", "Freu\u00b7de", "mit", "dir", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dir, o mein Freund, mein Sulzer will ich singen,", "tokens": ["Dir", ",", "o", "mein", "Freund", ",", "mein", "Sul\u00b7zer", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+