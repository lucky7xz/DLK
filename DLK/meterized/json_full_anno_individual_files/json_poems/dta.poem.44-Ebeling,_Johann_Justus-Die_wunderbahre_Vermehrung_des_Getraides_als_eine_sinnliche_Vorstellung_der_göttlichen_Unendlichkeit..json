{"dta.poem.44": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n wunderbahre Vermehrung  \n  des Getraides  \n als eine sinnliche Vorstellung der g\u00f6ttlichen  \n Unendlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht ohn bewunderndes Vergn\u00fcgen wird\nman in einem jeden Jahr,", "tokens": ["Nicht", "ohn", "be\u00b7wun\u00b7dern\u00b7des", "Ver\u00b7gn\u00fc\u00b7gen", "wird", "man", "in", "ei\u00b7nem", "je\u00b7den", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VAFIN", "PIS", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Wie das Getraide sich vermehret, auf", "tokens": ["Wie", "das", "Ge\u00b7trai\u00b7de", "sich", "ver\u00b7meh\u00b7ret", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein K\u00f6rnchen das da ausges\u00e4t,", "tokens": ["Ein", "K\u00f6rn\u00b7chen", "das", "da", "aus\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worin die K\u00f6rner zwanzigfach, ja hundertfach sich", "tokens": ["Wo\u00b7rin", "die", "K\u00f6r\u00b7ner", "zwan\u00b7zig\u00b7fach", ",", "ja", "hun\u00b7dert\u00b7fach", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es stekket in den Saamenk\u00f6rnern, so viel verborg-", "tokens": ["Es", "stek\u00b7ket", "in", "den", "Saa\u00b7men\u00b7k\u00f6r\u00b7nern", ",", "so", "viel", "ver\u00b7bor\u00b7g"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df jedes kan mehr Aehren spriessen, wie man", "tokens": ["Da\u00df", "je\u00b7des", "kan", "mehr", "A\u00b7eh\u00b7ren", "spries\u00b7sen", ",", "wie", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "PIAT", "NN", "VVINF", "$,", "PWAV", "PIS"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "In vielen Proben angesehn; Auf eines Halmen", "tokens": ["In", "vie\u00b7len", "Pro\u00b7ben", "an\u00b7ge\u00b7sehn", ";", "Auf", "ei\u00b7nes", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sieht man bisweilen hie und da, ein B\u00fcschel vie-", "tokens": ["Sieht", "man", "bis\u00b7wei\u00b7len", "hie", "und", "da", ",", "ein", "B\u00fc\u00b7schel", "vie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "KON", "ADV", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man sieht dies an als seltne Proben der \u00f6fters spie-", "tokens": ["Man", "sieht", "dies", "an", "als", "selt\u00b7ne", "Pro\u00b7ben", "der", "\u00f6f\u00b7ters", "spie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDS", "PTKVZ", "KOKOM", "ADJA", "NN", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Allein mit meines Geistes Augen, seh ich die Ehr-", "tokens": ["Al\u00b7lein", "mit", "mei\u00b7nes", "Geis\u00b7tes", "Au\u00b7gen", ",", "seh", "ich", "die", "Ehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Der wunderbahren GOttheit an, die uns darin den", "tokens": ["Der", "wun\u00b7der\u00b7bah\u00b7ren", "Got\u00b7theit", "an", ",", "die", "uns", "da\u00b7rin", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PAV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von seiner grossen Sch\u00f6pfers-Kraft hat wollen vor", "tokens": ["Von", "sei\u00b7ner", "gros\u00b7sen", "Sch\u00f6p\u00b7fer\u00b7sKraft", "hat", "wol\u00b7len", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "VMFIN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man z\u00e4hle diese Saamenk\u00f6rner, die aus dem einz-", "tokens": ["Man", "z\u00e4h\u00b7le", "die\u00b7se", "Saa\u00b7men\u00b7k\u00f6r\u00b7ner", ",", "die", "aus", "dem", "einz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "Man streu dieselben in die Aekker; so wird man sie", "tokens": ["Man", "streu", "die\u00b7sel\u00b7ben", "in", "die", "A\u00b7ek\u00b7ker", ";", "so", "wird", "man", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PIS", "PPER"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Man s\u00e4e diese grosse Zahl was wird man nicht in", "tokens": ["Man", "s\u00e4e", "die\u00b7se", "gros\u00b7se", "Zahl", "was", "wird", "man", "nicht", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "ADJA", "NN", "PWS", "VAFIN", "PIS", "PTKNEG", "APPR"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "F\u00fcr eine aufgeh\u00e4ufte Meng aus einem einzgen Korn", "tokens": ["F\u00fcr", "ei\u00b7ne", "auf\u00b7ge\u00b7h\u00e4uf\u00b7te", "Meng", "aus", "ei\u00b7nem", "einz\u00b7gen", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Und welch ein Hauffe wird erwachsen, wenn man", "tokens": ["Und", "welch", "ein", "Hauf\u00b7fe", "wird", "er\u00b7wach\u00b7sen", ",", "wenn", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWAT", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und damit tausend Jahr und dr\u00fcber in seinen Rech-", "tokens": ["Und", "da\u00b7mit", "tau\u00b7send", "Jahr", "und", "dr\u00fc\u00b7ber", "in", "sei\u00b7nen", "Rech"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "CARD", "NN", "KON", "PAV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da stuzt der ganz verschlungne Sinn, bei einem", "tokens": ["Da", "stuzt", "der", "ganz", "ver\u00b7schlung\u00b7ne", "Sinn", ",", "bei", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADV", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Von K\u00f6rnern die doch insgesamt, aus einen Korn", "tokens": ["Von", "K\u00f6r\u00b7nern", "die", "doch", "ins\u00b7ge\u00b7samt", ",", "aus", "ei\u00b7nen", "Korn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "ADV", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Zahl steigt ins Unendliche, es fa\u00dft sie keine", "tokens": ["Die", "Zahl", "steigt", "ins", "Un\u00b7end\u00b7li\u00b7che", ",", "es", "fa\u00dft", "sie", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Man sieht von vielen Millionen ein aufgeh\u00e4uftes", "tokens": ["Man", "sieht", "von", "vie\u00b7len", "Mil\u00b7lion\u00b7en", "ein", "auf\u00b7ge\u00b7h\u00e4uf\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PIAT", "NN", "ART", "ADJA"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Das Auge des Verstandes merkt, bey dem fast", "tokens": ["Das", "Au\u00b7ge", "des", "Ver\u00b7stan\u00b7des", "merkt", ",", "bey", "dem", "fast"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.24": {"text": "Die GOttheit habe ihn gezeigt: Es sey ein Wesen", "tokens": ["Die", "Got\u00b7theit", "ha\u00b7be", "ihn", "ge\u00b7zeigt", ":", "Es", "sey", "ein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das kein Begrif kan \u00fcbersehen, und keine Den-", "tokens": ["Das", "kein", "Be\u00b7grif", "kan", "\u00fc\u00b7ber\u00b7se\u00b7hen", ",", "und", "kei\u00b7ne", "Den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VMFIN", "VVINF", "$,", "KON", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Auch so wie sie sich in Gesch\u00f6pfen des Reiches der", "tokens": ["Auch", "so", "wie", "sie", "sich", "in", "Ge\u00b7sch\u00f6p\u00b7fen", "des", "Rei\u00b7ches", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PPER", "PRF", "APPR", "NN", "ART", "NN", "ART"], "meter": "-+--+--+--+--", "measure": "amphibrach.tetra.plus"}, "line.27": {"text": "O! unerme\u00dflich All! O! GOtt! du bist ein ewger", "tokens": ["O", "!", "un\u00b7er\u00b7me\u00df\u00b7lich", "All", "!", "O", "!", "Gott", "!", "du", "bist", "ein", "ew\u00b7ger"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADJD", "NN", "$.", "NE", "$.", "NN", "$.", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den wir in Unvollkommenheit, nur blos allein be-", "tokens": ["Den", "wir", "in", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", ",", "nur", "blos", "al\u00b7lein", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "$,", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wir denken an des Abgrunds Tieffen, die unsern", "tokens": ["Wir", "den\u00b7ken", "an", "des", "Ab\u00b7grunds", "Tief\u00b7fen", ",", "die", "un\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Den die Unendlichkeit bet\u00e4ubet mit Ehrfurcht gleich", "tokens": ["Den", "die", "Un\u00b7end\u00b7lich\u00b7keit", "be\u00b7t\u00e4u\u00b7bet", "mit", "Ehr\u00b7furcht", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Wir lassen diesen Ausspruch h\u00f6rn: Wer wird sich", "tokens": ["Wir", "las\u00b7sen", "die\u00b7sen", "Aus\u00b7spruch", "h\u00f6rn", ":", "Wer", "wird", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "VVINF", "$.", "PWS", "VAFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ein Wesen das unendlich ist, in der Vernunft Be-", "tokens": ["Ein", "We\u00b7sen", "das", "un\u00b7end\u00b7lich", "ist", ",", "in", "der", "Ver\u00b7nunft", "Be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJD", "VAFIN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jhr Narren die ihr dieses wollet, und euch zu sei-", "tokens": ["Ihr", "Nar\u00b7ren", "die", "ihr", "die\u00b7ses", "wol\u00b7let", ",", "und", "euch", "zu", "sei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "PPER", "PDS", "VMFIN", "$,", "KON", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Bedenket eh ihrs euch erk\u00fchnet, was weislich ein", "tokens": ["Be\u00b7den\u00b7ket", "eh", "ihrs", "euch", "er\u00b7k\u00fch\u00b7net", ",", "was", "weis\u00b7lich", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PRELS", "ADV", "ART"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.35": {"text": "Berechnet mir zuerst die Zahl der K\u00f6rner, die aus", "tokens": ["Be\u00b7rech\u00b7net", "mir", "zu\u00b7erst", "die", "Zahl", "der", "K\u00f6r\u00b7ner", ",", "die", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.36": {"text": "Nach einen angenomnen Saz, so wie sie sich ver-", "tokens": ["Nach", "ei\u00b7nen", "an\u00b7ge\u00b7nom\u00b7nen", "Saz", ",", "so", "wie", "sie", "sich", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So lange als das Feld gebl\u00fchet; so lang die Frucht", "tokens": ["So", "lan\u00b7ge", "als", "das", "Feld", "ge\u00b7bl\u00fc\u00b7het", ";", "so", "lang", "die", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "VVFIN", "$.", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Jhr sagt: Wer kan die Zahlen fassen, sie sind zu", "tokens": ["Ihr", "sagt", ":", "Wer", "kan", "die", "Zah\u00b7len", "fas\u00b7sen", ",", "sie", "sind", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF", "$,", "PPER", "VAFIN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wollan! wie waget ihr euch denn, aus Thorheit", "tokens": ["Wol\u00b7lan", "!", "wie", "wa\u00b7get", "ihr", "euch", "denn", ",", "aus", "Thor\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "PWAV", "VVFIN", "PPER", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.40": {"text": "Des Sch\u00f6pfers unerme\u00dfne Gr\u00f6s, die doch unend-", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "un\u00b7er\u00b7me\u00df\u00b7ne", "Gr\u00f6s", ",", "die", "doch", "un\u00b7en\u00b7d"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}