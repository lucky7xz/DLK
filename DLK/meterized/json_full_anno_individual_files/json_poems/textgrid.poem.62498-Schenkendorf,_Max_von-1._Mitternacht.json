{"textgrid.poem.62498": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1. Mitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es strahlt aus Nebelweiten", "tokens": ["Es", "strahlt", "aus", "Ne\u00b7bel\u00b7wei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ferner heil'ger Schein;", "tokens": ["Ein", "fer\u00b7ner", "heil'\u00b7ger", "Schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Anfang aller Zeiten", "tokens": ["Zu", "An\u00b7fang", "al\u00b7ler", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War Gott, und Gott allein.", "tokens": ["War", "Gott", ",", "und", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da lag er auf den Tiefen,", "tokens": ["Da", "lag", "er", "auf", "den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schwebt er auf der Flut,", "tokens": ["Da", "schwebt", "er", "auf", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Geister alle schliefen,", "tokens": ["Die", "Geis\u00b7ter", "al\u00b7le", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er war das h\u00f6chste Gut.", "tokens": ["Er", "war", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie aus dichter H\u00fclle", "tokens": ["Und", "wie", "aus", "dich\u00b7ter", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Morgensonne steigt,", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat seine Kraft, sein Wille", "tokens": ["Hat", "sei\u00b7ne", "Kraft", ",", "sein", "Wil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den ew'gen Sohn gezeugt.", "tokens": ["Den", "ew'\u00b7gen", "Sohn", "ge\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das war das erste Werde,", "tokens": ["Das", "war", "das", "ers\u00b7te", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das war das erste Wort,", "tokens": ["Das", "war", "das", "ers\u00b7te", "Wort", ","], "token_info": ["word", "word", "wo