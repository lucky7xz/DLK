{"textgrid.poem.53638": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Iste Goethe", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So stand ich nun. Der Stern des n\u00e4chsten Schildes", "tokens": ["So", "stand", "ich", "nun", ".", "Der", "Stern", "des", "n\u00e4chs\u00b7ten", "Schil\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "berief mich hin, die Wohnung schien ertr\u00e4glich.", "tokens": ["be\u00b7rief", "mich", "hin", ",", "die", "Woh\u00b7nung", "schien", "er\u00b7tr\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein M\u00e4dchen kam, des seltensten Gebildes,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "kam", ",", "des", "sel\u00b7ten\u00b7sten", "Ge\u00b7bil\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Licht erleuchtend. Mir ward gleich beh\u00e4glich.", "tokens": ["das", "Licht", "er\u00b7leuch\u00b7tend", ".", "Mir", "ward", "gleich", "be\u00b7h\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hausflur und Treppe sah ich als ein Mildes,", "tokens": ["Haus\u00b7flur", "und", "Trep\u00b7pe", "sah", "ich", "als", "ein", "Mil\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die Zimmerchen erfreuten mich uns\u00e4glich.", "tokens": ["die", "Zim\u00b7mer\u00b7chen", "er\u00b7freu\u00b7ten", "mich", "un\u00b7s\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+---+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein M\u00e4dchen kommt, und weil sie sehr grazi\u00f6s ist, und weil es Abend ist, und weil er ein Mann ist \u2013", "tokens": ["Ein", "M\u00e4d\u00b7chen", "kommt", ",", "und", "weil", "sie", "sehr", "gra\u00b7zi\u00b7\u00f6s", "ist", ",", "und", "weil", "es", "A\u00b7bend", "ist", ",", "und", "weil", "er", "ein", "Mann", "ist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "KON", "KOUS", "PPER", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Den Stuhl umwerfend spring ich auf und fasse", "tokens": ["Den", "Stuhl", "um\u00b7wer\u00b7fend", "spring", "ich", "auf", "und", "fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "VVFIN", "PPER", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das sch\u00f6ne Kind; sie lispelt: \u00bbLasse, lasse!\u00ab", "tokens": ["das", "sch\u00f6\u00b7ne", "Kind", ";", "sie", "lis\u00b7pelt", ":", "\u00bb", "Las\u00b7se", ",", "las\u00b7se", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "$.", "$(", "NN", "$,", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vervielfacht war, was sich f\u00fcr sie bewegte:", "tokens": ["Ver\u00b7viel\u00b7facht", "war", ",", "was", "sich", "f\u00fcr", "sie", "be\u00b7weg\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verstand und Witz und alle Lebensgeister", "tokens": ["Ver\u00b7stand", "und", "Witz", "und", "al\u00b7le", "Le\u00b7bens\u00b7geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er schaudert weg, vorsichtig, leise, leise", "tokens": ["Er", "schau\u00b7dert", "weg", ",", "vor\u00b7sich\u00b7tig", ",", "lei\u00b7se", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "entzieht er sich dem holden Zauberkreise.", "tokens": ["ent\u00b7zieht", "er", "sich", "dem", "hol\u00b7den", "Zau\u00b7ber\u00b7krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sitzt, schreibt:", "tokens": ["Sitzt", ",", "schreibt", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ich nahte mich der heimischen Pforte,", "tokens": ["Ich", "nah\u00b7te", "mich", "der", "hei\u00b7mi\u00b7schen", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "entfernen wollten mich die letzten Stunden.", "tokens": ["ent\u00b7fer\u00b7nen", "woll\u00b7ten", "mich", "die", "letz\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da hab ich nun, am sonderbarsten Orte,", "tokens": ["Da", "hab", "ich", "nun", ",", "am", "son\u00b7der\u00b7bars\u00b7ten", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "mein treues Herz aufs neue dir verbunden.", "tokens": ["mein", "treu\u00b7es", "Herz", "aufs", "neu\u00b7e", "dir", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "ADJA", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Schlusse findest du geheime Worte:", "tokens": ["Zum", "Schlus\u00b7se", "fin\u00b7dest", "du", "ge\u00b7hei\u00b7me", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Krankheit erst bew\u00e4hret den Gesunden,", "tokens": ["Die", "Krank\u00b7heit", "erst", "be\u00b7w\u00e4h\u00b7ret", "den", "Ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dies B\u00fcchlein soll dir manches Gute zeigen,", "tokens": ["Dies", "B\u00fcch\u00b7lein", "soll", "dir", "man\u00b7ches", "Gu\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "das Beste nur mu\u00df ich zuletzt verschweigen.", "tokens": ["das", "Bes\u00b7te", "nur", "mu\u00df", "ich", "zu\u00b7letzt", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und weil zuletzt bei jeder Dichtungsweise", "tokens": ["Und", "weil", "zu\u00b7letzt", "bei", "je\u00b7der", "Dich\u00b7tungs\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Moralien uns ernstlich f\u00f6rdern sollen,", "tokens": ["Mo\u00b7ra\u00b7li\u00b7en", "uns", "ernst\u00b7lich", "f\u00f6r\u00b7dern", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so will auch ich in so beliebtem Gleise", "tokens": ["so", "will", "auch", "ich", "in", "so", "be\u00b7lieb\u00b7tem", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "euch gern bekennen, was die Verse wollen:", "tokens": ["euch", "gern", "be\u00b7ken\u00b7nen", ",", "was", "die", "Ver\u00b7se", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir stolpern wohl auf unsrer Lebensreise,", "tokens": ["Wir", "stol\u00b7pern", "wohl", "auf", "uns\u00b7rer", "Le\u00b7bens\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und doch verm\u00f6gen in der Welt, der tollen,", "tokens": ["und", "doch", "ver\u00b7m\u00f6\u00b7gen", "in", "der", "Welt", ",", "der", "tol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zwei Hebel viel aufs irdische Getriebe:", "tokens": ["zwei", "He\u00b7bel", "viel", "aufs", "ir\u00b7di\u00b7sche", "Ge\u00b7trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sehr viel die Pflicht, unendlich mehr die Liebe!", "tokens": ["Sehr", "viel", "die", "Pflicht", ",", "un\u00b7end\u00b7lich", "mehr", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+