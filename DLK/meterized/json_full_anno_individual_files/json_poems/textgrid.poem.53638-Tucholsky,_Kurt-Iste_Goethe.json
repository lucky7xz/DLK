, "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dies B\u00fcchlein soll dir manches Gute zeigen,", "tokens": ["Dies", "B\u00fcch\u00b7lein", "soll", "dir", "man\u00b7ches", "Gu\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "das Beste nur mu\u00df ich zuletzt verschweigen.", "tokens": ["das", "Bes\u00b7te", "nur", "mu\u00df", "ich", "zu\u00b7letzt", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Und weil zuletzt bei jeder Dichtungsweise", "tokens": ["Und", "weil", "zu\u00b7letzt", "bei", "je\u00b7der", "Dich\u00b7tungs\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Moralien uns ernstlich f\u00f6rdern sollen,", "tokens": ["Mo\u00b7ra\u00b7li\u00b7en", "uns", "ernst\u00b7lich", "f\u00f6r\u00b7dern", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so will auch ich in so beliebtem Gleise", "tokens": ["so", "will", "auch", "ich", "in", "so", "be\u00b7lieb\u00b7tem", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "euch gern bekennen, was die Verse wollen:", "tokens": ["euch", "gern", "be\u00b7ken\u00b7nen", ",", "was", "die", "Ver\u00b7se", "wol\u00b7len", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wir stolpern wohl auf unsrer Lebensreise,", "tokens": ["Wir", "stol\u00b7pern", "wohl", "auf", "uns\u00b7rer", "Le\u00b7bens\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und doch verm\u00f6gen in der Welt, der tollen,", "tokens": ["und", "doch", "ver\u00b7m\u00f6\u00b7gen", "in", "der", "Welt", ",", "der", "tol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "zwei Hebel viel aufs irdische Getriebe:", "tokens": ["zwei", "He\u00b7bel", "viel", "aufs", "ir\u00b7di\u00b7sche", "Ge\u00b7trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sehr viel die Pflicht, unendlich mehr die Liebe!", "tokens": ["Sehr", "viel", "die", "Pflicht", ",", "un\u00b7end\u00b7lich", "mehr", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}