{"textgrid.poem.37749": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lang stand die Kirche klein und enge,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lang stand die Kirche klein und enge,", "tokens": ["Lang", "stand", "die", "Kir\u00b7che", "klein", "und", "en\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von hohen H\u00e4usern fast versteckt,", "tokens": ["Von", "ho\u00b7hen", "H\u00e4u\u00b7sern", "fast", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Gl\u00f6cklein gab nur sanfte Kl\u00e4nge,", "tokens": ["Ihr", "Gl\u00f6c\u00b7klein", "gab", "nur", "sanf\u00b7te", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Reicher ward davon erweckt;", "tokens": ["Kein", "Rei\u00b7cher", "ward", "da\u00b7von", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Handwerksleute, ganz geringe,", "tokens": ["Nur", "Hand\u00b7werks\u00b7leu\u00b7te", ",", "ganz", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gingen alle Sonntag hin,", "tokens": ["Die", "gin\u00b7gen", "al\u00b7le", "Sonn\u00b7tag", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df sie ein heilig Wort durchklinge", "tokens": ["Da\u00df", "sie", "ein", "hei\u00b7lig", "Wort", "durch\u00b7klin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit treuem Muth, mit mildem Sinn.", "tokens": ["Mit", "treu\u00b7em", "Muth", ",", "mit", "mil\u00b7dem", "Sinn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In diesem Kirchlein abgelegen,", "tokens": ["In", "die\u00b7sem", "Kirch\u00b7lein", "ab\u00b7ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar unbeachtet von der Welt,", "tokens": ["Gar", "un\u00b7be\u00b7ach\u00b7tet", "von", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein alter Herr mit gro\u00dfem Segen", "tokens": ["Ein", "al\u00b7ter", "Herr", "mit", "gro\u00b7\u00dfem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit funfzig Jahren Predigt h\u00e4lt;", "tokens": ["Seit", "funf\u00b7zig", "Jah\u00b7ren", "Pre\u00b7digt", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Keiner wu\u00dfte in Pall\u00e4sten,", "tokens": ["Und", "Kei\u00b7ner", "wu\u00df\u00b7te", "in", "Pal\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er so herrlich dient der Stadt,", "tokens": ["Da\u00df", "er", "so", "herr\u00b7lich", "dient", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von seinen armen Sonntagsg\u00e4sten", "tokens": ["Von", "sei\u00b7nen", "ar\u00b7men", "Sonn\u00b7tags\u00b7g\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wird jeder geistig voll und satt.", "tokens": ["Wird", "je\u00b7der", "geis\u00b7tig", "voll", "und", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kommt ein Krieg, macht bang die Reichen;", "tokens": ["Da", "kommt", "ein", "Krieg", ",", "macht", "bang", "die", "Rei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl Mancher h\u00f6rt das Gl\u00f6cklein nun,", "tokens": ["Wohl", "Man\u00b7cher", "h\u00f6rt", "das", "Gl\u00f6c\u00b7klein", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00f6chte sich mit Gott ausgleichen", "tokens": ["Und", "m\u00f6ch\u00b7te", "sich", "mit", "Gott", "aus\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und gleich den armen Leuten thun,", "tokens": ["Und", "gleich", "den", "ar\u00b7men", "Leu\u00b7ten", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und h\u00f6rt des ew'gen Worts Vertrauen,", "tokens": ["Und", "h\u00f6rt", "des", "ew'\u00b7gen", "Worts", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Kirchlein von dem Gottesmann,", "tokens": ["Im", "Kirch\u00b7lein", "von", "dem", "Got\u00b7tes\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchlt ein kr\u00e4ftiges Erbauen", "tokens": ["Und", "f\u00fchlt", "ein", "kr\u00e4f\u00b7ti\u00b7ges", "Er\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00e4hrt im Gl\u00fcck da wieder an.", "tokens": ["Und", "f\u00e4hrt", "im", "Gl\u00fcck", "da", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald rollen nach die hohen Wagen,", "tokens": ["Bald", "rol\u00b7len", "nach", "die", "ho\u00b7hen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer zieht den andern fort,", "tokens": ["Und", "ei\u00b7ner", "zieht", "den", "an\u00b7dern", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Brillen junger Leute sagen:", "tokens": ["Die", "Bril\u00b7len", "jun\u00b7ger", "Leu\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df viel zu sehen sey am Wort.", "tokens": ["Da\u00df", "viel", "zu", "se\u00b7hen", "sey", "am", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKZU", "VVINF", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die bunten Pelze, Federh\u00fcte,", "tokens": ["Die", "bun\u00b7ten", "Pel\u00b7ze", ",", "Fe\u00b7der\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die ziehen hin, auch wenn's zu sp\u00e4t:", "tokens": ["Die", "zie\u00b7hen", "hin", ",", "auch", "wenn's", "zu", "sp\u00e4t", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$,", "ADV", "PIS", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie nennen dies der Kirche Bl\u00fcthe,", "tokens": ["Sie", "nen\u00b7nen", "dies", "der", "Kir\u00b7che", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als ob nun Andacht erst ger\u00e4th.", "tokens": ["Als", "ob", "nun", "An\u00b7dacht", "erst", "ge\u00b7r\u00e4\u00b7th", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie dr\u00e4ngen sich zu allen St\u00fchlen,", "tokens": ["Sie", "dr\u00e4n\u00b7gen", "sich", "zu", "al\u00b7len", "St\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Armen machen willig Platz,", "tokens": ["Die", "Ar\u00b7men", "ma\u00b7chen", "wil\u00b7lig", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie freuen sich, da\u00df Reiche f\u00fchlen,", "tokens": ["Sie", "freu\u00b7en", "sich", ",", "da\u00df", "Rei\u00b7che", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was in dem Alten f\u00fcr ein Schatz;", "tokens": ["Was", "in", "dem", "Al\u00b7ten", "f\u00fcr", "ein", "Schatz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Duft der Werkstatt ganz verschwindet", "tokens": ["Der", "Duft", "der", "Werk\u00b7statt", "ganz", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Wohlger\u00fcchen geistig fein;", "tokens": ["In", "Wohl\u00b7ge\u00b7r\u00fc\u00b7chen", "geis\u00b7tig", "fein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch der Gebildete jetzt findet:", "tokens": ["Doch", "der", "Ge\u00b7bil\u00b7de\u00b7te", "jetzt", "fin\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df arme Leute gar sehr schrein.", "tokens": ["Da\u00df", "ar\u00b7me", "Leu\u00b7te", "gar", "sehr", "schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist voll ihr Herz, ihr Mund geht \u00fcber,", "tokens": ["Ist", "voll", "ihr", "Herz", ",", "ihr", "Mund", "geht", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer singen lernte, singet sacht;", "tokens": ["Wer", "sin\u00b7gen", "lern\u00b7te", ",", "sin\u00b7get", "sacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bequem seyn ist den Reichen lieber,", "tokens": ["Be\u00b7quem", "seyn", "ist", "den", "Rei\u00b7chen", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie nehmen alle St\u00fchl' in Pacht,", "tokens": ["Sie", "neh\u00b7men", "al\u00b7le", "St\u00fchl'", "in", "Pacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das treibt die Armen von den Sitzen,", "tokens": ["Das", "treibt", "die", "Ar\u00b7men", "von", "den", "Sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Mancher ein halbhundert Jahr:", "tokens": ["Die", "Man\u00b7cher", "ein", "halb\u00b7hun\u00b7dert", "Jahr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch die Verj\u00e4hrung zu besitzen", "tokens": ["Durch", "die", "Ver\u00b7j\u00e4h\u00b7rung", "zu", "be\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und zu vererben sicher war.", "tokens": ["Und", "zu", "ver\u00b7er\u00b7ben", "si\u00b7cher", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00e4r Glaube bei euch Reichen m\u00e4chtig,", "tokens": ["W\u00e4r", "Glau\u00b7be", "bei", "euch", "Rei\u00b7chen", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr schafftet Platz f\u00fcr jedermann,", "tokens": ["Ihr", "schaff\u00b7tet", "Platz", "f\u00fcr", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr bautet eine Kirche pr\u00e4chtig,", "tokens": ["Ihr", "bau\u00b7tet", "ei\u00b7ne", "Kir\u00b7che", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kutscher f\u00fchren Steine an,", "tokens": ["Die", "Kut\u00b7scher", "f\u00fch\u00b7ren", "Stei\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr lobt den Plan zur gro\u00dfen Kirche,", "tokens": ["Ihr", "lobt", "den", "Plan", "zur", "gro\u00b7\u00dfen", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch euer Lob regt keinen Stein,", "tokens": ["Doch", "eu\u00b7er", "Lob", "regt", "kei\u00b7nen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {