{"dta.poem.6810": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird, wo man den Blick ins Himmels Tieffe streckt,", "tokens": ["Es", "wird", ",", "wo", "man", "den", "Blick", "ins", "Him\u00b7mels", "Tief\u00b7fe", "streckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PIS", "ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch neue Rechen-Kunst der Lichter Meng\u2019 entdeckt.", "tokens": ["Durch", "neu\u00b7e", "Re\u00b7chen\u00b7Kunst", "der", "Lich\u00b7ter", "Meng'", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man schauet, mit erstaunten Blicken", "tokens": ["Man", "schau\u00b7et", ",", "mit", "er\u00b7staun\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein neues Sternen-Heer die tieffe R\u00fcnde schm\u00fccken.", "tokens": ["Ein", "neu\u00b7es", "Ster\u00b7nen\u00b7Heer", "die", "tief\u00b7fe", "R\u00fcn\u00b7de", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es zeiget unsern Blick ein Glas recht klar und helle", "tokens": ["Es", "zei\u00b7get", "un\u00b7sern", "Blick", "ein", "Glas", "recht", "klar", "und", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von den Planeten fast die kleinste Stelle:", "tokens": ["Von", "den", "Pla\u00b7ne\u00b7ten", "fast", "die", "kleins\u00b7te", "Stel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und um uns nun davon ein heller Licht zu geben;", "tokens": ["Und", "um", "uns", "nun", "da\u00b7von", "ein", "hel\u00b7ler", "Licht", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "ADV", "PAV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verk\u00fcrtzt es ihre Weit\u2019, und wei\u00df den Blick zu heben,", "tokens": ["Ver\u00b7k\u00fcrtzt", "es", "ih\u00b7re", "Weit'", ",", "und", "wei\u00df", "den", "Blick", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df wir die tieffste Tieff\u2019 in schneller Eil durchdringen,", "tokens": ["Da\u00df", "wir", "die", "tieffs\u00b7te", "Tief\u00b7f'", "in", "schnel\u00b7ler", "Eil", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und unsrer Augen Strahl bis zum Saturnus bringen.", "tokens": ["Und", "uns\u00b7rer", "Au\u00b7gen", "Strahl", "bis", "zum", "Sa\u00b7tur\u00b7nus", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}