{"textgrid.poem.37733": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thue doch die Augen auf,", "tokens": ["Thue", "doch", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Liebe Seele, aus dem \u00dcberdrusse,", "tokens": ["Lie\u00b7be", "See\u00b7le", ",", "aus", "dem", "\u00dc\u00b7berd\u00b7rus\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh den Flu\u00df im schnellen Lauf", "tokens": ["Sieh", "den", "Flu\u00df", "im", "schnel\u00b7len", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieh der Wolken ruhend Bild im Flusse!", "tokens": ["Sieh", "der", "Wol\u00b7ken", "ru\u00b7hend", "Bild", "im", "Flus\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Steht ", "tokens": ["Steht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "O so bleibt auch ruhiges Genie\u00dfen,", "tokens": ["O", "so", "bleibt", "auch", "ru\u00b7hi\u00b7ges", "Ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Stehet \u00fcberm Strom der fl\u00fcchtgen Zeit,", "tokens": ["Ste\u00b7het", "\u00fc\u00b7berm", "Strom", "der", "fl\u00fccht\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Schafft sich tr\u00e4umend eine Ewigkeit.", "tokens": ["Schafft", "sich", "tr\u00e4u\u00b7mend", "ei\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Weinet auch die Rebe heut,", "tokens": ["Wei\u00b7net", "auch", "die", "Re\u00b7be", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie mu\u00df gr\u00fcnen, bl\u00fchen, Fr\u00fcchte tragen;", "tokens": ["Sie", "mu\u00df", "gr\u00fc\u00b7nen", ",", "bl\u00fc\u00b7hen", ",", "Fr\u00fcch\u00b7te", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00df der Knospe Heimlichkeit", "tokens": ["La\u00df", "der", "Knos\u00b7pe", "Heim\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem hellen Lichte anfangs zagen;", "tokens": ["Vor", "dem", "hel\u00b7len", "Lich\u00b7te", "an\u00b7fangs", "za\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df sie aufbricht, m\u00f6cht' das Herz ihr brechen,", "tokens": ["Da\u00df", "sie", "auf\u00b7bricht", ",", "m\u00f6cht'", "das", "Herz", "ihr", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Doch sie wird sich bald im Glanze r\u00e4chen,", "tokens": ["Doch", "sie", "wird", "sich", "bald", "im", "Glan\u00b7ze", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wie's ihr geht, so ging's ja aller Welt,", "tokens": ["Wie's", "ihr", "geht", ",", "so", "ging's", "ja", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Liebe Seele, sei zur Lust gesellt!", "tokens": ["Lie\u00b7be", "See\u00b7le", ",", "sei", "zur", "Lust", "ge\u00b7sellt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Thue doch die Augen auf,", "tokens": ["Thue", "doch", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Liebe Seele, aus dem \u00dcberdrusse,", "tokens": ["Lie\u00b7be", "Se