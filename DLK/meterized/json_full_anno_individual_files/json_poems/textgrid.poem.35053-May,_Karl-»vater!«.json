{"textgrid.poem.35053": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbvater!\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm her, und sprich ein einzig Wort,", "tokens": ["Komm", "her", ",", "und", "sprich", "ein", "ein\u00b7zig", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wort, so kinderleicht zu sagen.", "tokens": ["Ein", "Wort", ",", "so", "kin\u00b7der\u00b7leicht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm her, und geh nicht wieder fort;", "tokens": ["Komm", "her", ",", "und", "geh", "nicht", "wie\u00b7der", "fort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du brauchst vor mir ja nicht zu zagen.", "tokens": ["Du", "brauchst", "vor", "mir", "ja", "nicht", "zu", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich warte schon so lange dein;", "tokens": ["Ich", "war\u00b7te", "schon", "so", "lan\u00b7ge", "dein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O la\u00df es nicht vergeblich sein!", "tokens": ["O", "la\u00df", "es", "nicht", "ver\u00b7geb\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du sprachst als Kind dies liebe Wort", "tokens": ["Du", "sprachst", "als", "Kind", "dies", "lie\u00b7be", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft und gern, wenn du gelitten;", "tokens": ["So", "oft", "und", "gern", ",", "wenn", "du", "ge\u00b7lit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ward geh\u00f6rt am rechten Ort:", "tokens": ["Es", "ward", "ge\u00b7h\u00f6rt", "am", "rech\u00b7ten", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Vaterherz lie\u00df sich erbitten.", "tokens": ["Das", "Va\u00b7ter\u00b7herz", "lie\u00df", "sich", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ist dies Wort so klein, so klein,", "tokens": ["Wie", "ist", "dies", "Wort", "so", "klein", ",", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "NN", "ADV", "ADJD"