{"textgrid.poem.55907": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Opfer", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie bl\u00fcht mein Leib aus jeder Ader", "tokens": ["O", "wie", "bl\u00fcht", "mein", "Leib", "aus", "je\u00b7der", "A\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "duftender, seitdem ich dich erkenn;", "tokens": ["duf\u00b7ten\u00b7der", ",", "seit\u00b7dem", "ich", "dich", "er\u00b7kenn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sieh, ich gehe schlanker und gerader,", "tokens": ["sieh", ",", "ich", "ge\u00b7he", "schlan\u00b7ker", "und", "ge\u00b7ra\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und du wartest nur \u2013: wer bist du denn?", "tokens": ["und", "du", "war\u00b7test", "nur", "\u2013", ":", "wer", "bist", "du", "denn", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "$(", "$.", "PWS", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh: ich f\u00fchle, wie ich mich entferne,", "tokens": ["Sieh", ":", "ich", "f\u00fch\u00b7le", ",", "wie", "ich", "mich", "ent\u00b7fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "wie ich Altes, Blatt um Blatt, verlier.", "tokens": ["wie", "ich", "Al\u00b7tes", ",", "Blatt", "um", "Blatt", ",", "ver\u00b7lier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "APPR", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur dein L\u00e4cheln steht wie lauter Sterne", "tokens": ["Nur", "dein", "L\u00e4\u00b7cheln", "steht", "wie", "lau\u00b7ter", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "KOKOM", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00fcber dir und bald auch \u00fcber mir.", "tokens": ["\u00fc\u00b7ber", "dir", "und", "bald", "auch", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Alles was durch meine Kinderjahre", "tokens": ["Al\u00b7les", "was", "durch", "mei\u00b7ne", "Kin\u00b7der\u00b7jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "namenlos noch und wie Wasser gl\u00e4nzt,", "tokens": ["na\u00b7men\u00b7los", "noch", "und", "wie", "Was\u00b7ser", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "will ich nach dir nennen am Altare,"