{"textgrid.poem.56447": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: O weites Land des Sommers und der Winde,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weites Land des Sommers und der Winde,", "tokens": ["O", "wei\u00b7tes", "Land", "des", "Som\u00b7mers", "und", "der", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der reinen Wolken, die dem Wind sich bieten.", "tokens": ["Der", "rei\u00b7nen", "Wol\u00b7ken", ",", "die", "dem", "Wind", "sich", "bie\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo goldener Weizen reift und die Gebinde", "tokens": ["Wo", "gol\u00b7de\u00b7ner", "Wei\u00b7zen", "reift", "und", "die", "Ge\u00b7bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "KON", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des gelben Roggens trocknen in den Mieten.", "tokens": ["Des", "gel\u00b7ben", "Rog\u00b7gens", "trock\u00b7nen", "in", "den", "Mie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Erde d\u00e4mmert von den D\u00fcften allen,", "tokens": ["Die", "Er\u00b7de", "d\u00e4m\u00b7mert", "von", "den", "D\u00fcf\u00b7ten", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von gr\u00fcnen Winden und des Mohnes Farben,", "tokens": ["Von", "gr\u00fc\u00b7nen", "Win\u00b7den", "und", "des", "Moh\u00b7nes", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des schwere K\u00f6pfe auf den Stielen fallen", "tokens": ["Des", "schwe\u00b7re", "K\u00f6p\u00b7fe", "auf", "den", "Stie\u00b7len", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weithin brennen aus den hohen Garben.", "tokens": ["Und", "weit\u00b7hin", "bren\u00b7nen", "aus", "den", "ho\u00b7hen", "Gar\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des Feldwegs Br\u00fccke steigt im halben Bogen,", "tokens": ["Des", "Feld\u00b7wegs", "Br\u00fc\u00b7cke", "steigt", "im", "hal\u00b7ben", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo helle Wellen wei\u00dfe Kiesel feuchten.", "tokens": ["Wo", "hel\u00b7le", "Wel\u00b7len", "wei\u00b7\u00dfe", "Kie\u00b7sel", "feuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wassergr\u00e4ser werden fortgezogen,", "tokens": ["Die", "Was\u00b7ser\u00b7gr\u00e4\u00b7ser", "wer\u00b7den", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die in der Sonne aus dem Bache leuchten.", "tokens": ["Die", "in", "der", "Son\u00b7ne", "aus", "dem", "Ba\u00b7che", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Br\u00fccke schwankt herauf die erste Fahne.", "tokens": ["Die", "Br\u00fc\u00b7cke", "schwankt", "her\u00b7auf", "die", "ers\u00b7te", "Fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie flammt von Gold und Rot. Die Seidenquasten", "tokens": ["Sie", "flammt", "von", "Gold", "und", "Rot", ".", "Die", "Sei\u00b7den\u00b7quas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu beiden Seiten halten Kastellane", "tokens": ["Zu", "bei\u00b7den", "Sei\u00b7ten", "hal\u00b7ten", "Kas\u00b7tel\u00b7la\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im alten Chorrock, dem von Staub verbla\u00dften.", "tokens": ["Im", "al\u00b7ten", "Chor\u00b7rock", ",", "dem", "von", "Staub", "ver\u00b7bla\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Man h\u00f6rt Gesang. Die jungen Priester kommen.", "tokens": ["Man", "h\u00f6rt", "Ge\u00b7sang", ".", "Die", "jun\u00b7gen", "Pries\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Barh\u00e4uptig gehen sie vor den Pr\u00e4laten.", "tokens": ["Bar\u00b7h\u00e4up\u00b7tig", "ge\u00b7hen", "sie", "vor", "den", "Pr\u00e4\u00b7la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zu Fl\u00f6ten schallt der Me\u00dfgesang. Die frommen", "tokens": ["Zu", "Fl\u00f6\u00b7ten", "schallt", "der", "Me\u00df\u00b7ge\u00b7sang", ".", "Die", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alten Lieder wandern durch die Saaten.", "tokens": ["Und", "al\u00b7ten", "Lie\u00b7der", "wan\u00b7dern", "durch", "die", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In wei\u00dfen Kleidchen kommen Kinder singend.", "tokens": ["In", "wei\u00b7\u00dfen", "Kleid\u00b7chen", "kom\u00b7men", "Kin\u00b7der", "sin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie tragen kleine Kr\u00e4nze in den Haaren.", "tokens": ["Sie", "tra\u00b7gen", "klei\u00b7ne", "Kr\u00e4n\u00b7ze", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Knaben, runde Weihrauchkessel schwingend,", "tokens": ["Und", "Kna\u00b7ben", ",", "run\u00b7de", "Weih\u00b7rauch\u00b7kes\u00b7sel", "schwin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Spitzenrock und roten Festtalaren.", "tokens": ["Im", "Spit\u00b7zen\u00b7rock", "und", "ro\u00b7ten", "Fest\u00b7ta\u00b7la\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Kirchenbilder kommen auf Alt\u00e4ren.", "tokens": ["Die", "Kir\u00b7chen\u00b7bil\u00b7der", "kom\u00b7men", "auf", "Al\u00b7t\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mariens Wunden brennen hell im Licht.", "tokens": ["Ma\u00b7ri\u00b7ens", "Wun\u00b7den", "bren\u00b7nen", "hell", "im", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Christus naht, von Blumen bunt, die wehren", "tokens": ["Und", "Chris\u00b7tus", "naht", ",", "von", "Blu\u00b7men", "bunt", ",", "die", "weh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne von dem gelben Holzgesicht.", "tokens": ["Die", "Son\u00b7ne", "von", "dem", "gel\u00b7ben", "Holz\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Im Baldachine gl\u00e4nzt des Bischofs Krone.", "tokens": ["Im", "Bal\u00b7da\u00b7chi\u00b7ne", "gl\u00e4nzt", "des", "Bi\u00b7schofs", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er schreitet singend mit dem heiligen Schrein.", "tokens": ["Er", "schrei\u00b7tet", "sin\u00b7gend", "mit", "dem", "hei\u00b7li\u00b7gen", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der hohe Stimmenschall der Diakone", "tokens": ["Der", "ho\u00b7he", "Stim\u00b7men\u00b7schall", "der", "Dia\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fliegt weit hinaus durch Land und Felderreihn.", "tokens": ["Fliegt", "weit", "hin\u00b7aus", "durch", "Land", "und", "Fel\u00b7der\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APZR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Truhen Glanz weht um die alte Tracht.", "tokens": ["Der", "Tru\u00b7hen", "Glanz", "weht", "um", "die", "al\u00b7te", "Tracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kessel dampfen, drin die Kr\u00e4uter kohlen.", "tokens": ["Die", "Kes\u00b7sel", "damp\u00b7fen", ",", "drin", "die", "Kr\u00e4u\u00b7ter", "koh\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie ziehen durch der weiten Felder Pracht,", "tokens": ["Sie", "zie\u00b7hen", "durch", "der", "wei\u00b7ten", "Fel\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und matter gl\u00e4nzen die vergilbten Stolen.", "tokens": ["Und", "mat\u00b7ter", "gl\u00e4n\u00b7zen", "die", "ver\u00b7gilb\u00b7ten", "Sto\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Zug wird kleiner. Der Gesang verhallt.", "tokens": ["Der", "Zug", "wird", "klei\u00b7ner", ".", "Der", "Ge\u00b7sang", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "ART", "