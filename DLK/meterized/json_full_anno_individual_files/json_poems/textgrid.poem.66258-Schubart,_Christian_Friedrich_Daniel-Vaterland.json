": "Romulus eiserne S\u00f6hne", "tokens": ["Ro\u00b7mu\u00b7lus", "ei\u00b7ser\u00b7ne", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Malmten die V\u00f6lker;", "tokens": ["Malm\u00b7ten", "die", "V\u00f6l\u00b7ker", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du aber packtest Roma, die Trotzerin,", "tokens": ["Du", "a\u00b7ber", "pack\u00b7test", "Ro\u00b7ma", ",", "die", "Trot\u00b7ze\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Am blutigen Haarsch\u00e4del,", "tokens": ["Am", "blu\u00b7ti\u00b7gen", "Haar\u00b7sch\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und tratst der V\u00f6lkerz\u00e4hmerin", "tokens": ["Und", "tratst", "der", "V\u00f6l\u00b7ker\u00b7z\u00e4h\u00b7me\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch und stolz auf den Nacken.", "tokens": ["Hoch", "und", "stolz", "auf", "den", "Na\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Auch Hildebrands weitstrahlende,", "tokens": ["Auch", "Hil\u00b7de\u00b7brands", "weit\u00b7strah\u00b7len\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "$,"], "meter": "-+-+-+--", "measu