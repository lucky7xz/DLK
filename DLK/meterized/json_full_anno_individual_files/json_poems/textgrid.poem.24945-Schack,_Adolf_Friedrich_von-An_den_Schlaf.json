{"textgrid.poem.24945": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcd' ist mein Auge; doch noch immer l\u00e4rmen", "tokens": ["M\u00fcd'", "ist", "mein", "Au\u00b7ge", ";", "doch", "noch", "im\u00b7mer", "l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor meinem Ohre w\u00fcste Lebenskl\u00e4nge;", "tokens": ["Vor", "mei\u00b7nem", "Oh\u00b7re", "w\u00fcs\u00b7te", "Le\u00b7bens\u00b7kl\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch immer seh' ich bunte Bilder schw\u00e4rmen,", "tokens": ["Noch", "im\u00b7mer", "seh'", "ich", "bun\u00b7te", "Bil\u00b7der", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gleich Wolken, die in farbigem Gepr\u00e4nge", "tokens": ["Gleich", "Wol\u00b7ken", ",", "die", "in", "far\u00b7bi\u00b7gem", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Abendrot, das schimmernde, umwallen;", "tokens": ["Das", "A\u00b7ben\u00b7drot", ",", "das", "schim\u00b7mern\u00b7de", ",", "um\u00b7wal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wann flieht das rastlos flutende Gedr\u00e4nge?", "tokens": ["Wann", "flieht", "das", "rast\u00b7los", "flu\u00b7ten\u00b7de", "Ge\u00b7dr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wann wird die laute Wirklichkeit verhallen?", "tokens": ["Wann", "wird", "die", "lau\u00b7te", "Wirk\u00b7lich\u00b7keit", "ver\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was s\u00e4umst du, liebstes von den Zwillingskindern", "tokens": ["Was", "s\u00e4umst", "du", ",", "liebs\u00b7tes", "von", "den", "Zwil\u00b7lings\u00b7kin\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der heil'gen Nacht? Mit deinen Silberschwingen", "tokens": ["Der", "heil'\u00b7gen", "Nacht", "?", "Mit", "dei\u00b7nen", "Sil\u00b7ber\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umf\u00e4chle mich, der Stirne Glut zu lindern!", "tokens": ["Um\u00b7f\u00e4ch\u00b7le", "mich", ",", "der", "Stir\u00b7ne", "Glut", "zu", "lin\u00b7dern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Komm, deine Wiegenlieder mir zu singen,", "tokens": ["Komm", ",", "dei\u00b7ne", "Wie\u00b7gen\u00b7lie\u00b7der", "mir", "zu", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "S\u00fc\u00df, wie die Mutter einst sie sang dem Sohne,", "tokens": ["S\u00fc\u00df", ",", "wie", "die", "Mut\u00b7ter", "einst", "sie", "sang", "dem", "Soh\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mir im goldnen Kelch die Flut zu bringen,", "tokens": ["Und", "mir", "im", "gold\u00b7nen", "Kelch", "die", "Flut", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die traumreich quillt aus dem geweihten Mohne!", "tokens": ["Die", "traum\u00b7reich", "quillt", "aus", "dem", "ge\u00b7weih\u00b7ten", "Moh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O aus der F\u00fclle ihres Zauberscho\u00dfes", "tokens": ["O", "aus", "der", "F\u00fcl\u00b7le", "ih\u00b7res", "Zau\u00b7ber\u00b7scho\u00b7\u00dfes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gab dir die Nacht die besten ihrer Sch\u00e4tze!", "tokens": ["Gab", "dir", "die", "Nacht", "die", "bes\u00b7ten", "ih\u00b7rer", "Sch\u00e4t\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein, wunderbar Knabe, ist ein gro\u00dfes", "tokens": ["Dein", ",", "wun\u00b7der\u00b7bar", "Kna\u00b7be", ",", "ist", "ein", "gro\u00b7\u00dfes"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$,", "ADJD", "NN", "$,", "VAFIN", "ART", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Endloses Land voll weicher Ruhepl\u00e4tze,", "tokens": ["End\u00b7lo\u00b7ses", "Land", "voll", "wei\u00b7cher", "Ru\u00b7he\u00b7pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Voll sanfter H\u00fcgelschwellungen und Auen,", "tokens": ["Voll", "sanf\u00b7ter", "H\u00fc\u00b7gel\u00b7schwel\u00b7lun\u00b7gen", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zu denen durch die gr\u00fcnen Bl\u00e4tternetze", "tokens": ["Zu", "de\u00b7nen", "durch", "die", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter\u00b7net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Mondenstrahlen d\u00e4mmernd niedertauen.", "tokens": ["Die", "Mon\u00b7dens\u00b7trah\u00b7len", "d\u00e4m\u00b7mernd", "nie\u00b7der\u00b7tau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, Villen hast du neben blauen Seen", "tokens": ["Ja", ",", "Vil\u00b7len", "hast", "du", "ne\u00b7ben", "blau\u00b7en", "Seen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und G\u00e4rten, wo an schattenreichen G\u00e4ngen", "tokens": ["Und", "G\u00e4r\u00b7ten", ",", "wo", "an", "schat\u00b7ten\u00b7rei\u00b7chen", "G\u00e4n\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In laub'gen Nischen Marmorbilder stehen,", "tokens": ["In", "laub'\u00b7gen", "Ni\u00b7schen", "Mar\u00b7mor\u00b7bil\u00b7der", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo goldne Fr\u00fcchte von den Aesten h\u00e4ngen,", "tokens": ["Wo", "gold\u00b7ne", "Fr\u00fcch\u00b7te", "von", "den", "A\u00b7es\u00b7ten", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und Duft und Sang und pl\u00e4tschernde Kaskaden,", "tokens": ["Und", "Duft", "und", "Sang", "und", "pl\u00e4t\u00b7schern\u00b7de", "Kas\u00b7ka\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die weithin ihre Silbertropfen sprengen,", "tokens": ["Die", "weit\u00b7hin", "ih\u00b7re", "Sil\u00b7ber\u00b7trop\u00b7fen", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Liebenden zu Sommertr\u00e4umen laden.", "tokens": ["Die", "Lie\u00b7ben\u00b7den", "zu", "Som\u00b7mer\u00b7tr\u00e4u\u00b7men", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O, dein sind goldne Dome, Kuppeld\u00e4cher", "tokens": ["O", ",", "dein", "sind", "gold\u00b7ne", "Do\u00b7me", ",", "Kup\u00b7pel\u00b7d\u00e4\u00b7cher"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PPOSAT", "VAFIN", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Felsenburgen \u00fcber bl\u00fch'nden Thalen,", "tokens": ["Und", "Fel\u00b7sen\u00b7bur\u00b7gen", "\u00fc\u00b7ber", "bl\u00fch'n\u00b7den", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und festgeschm\u00fcckte, luftige Gem\u00e4cher,", "tokens": ["Und", "fest\u00b7ge\u00b7schm\u00fcck\u00b7te", ",", "luf\u00b7ti\u00b7ge", "Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo nie die Lust in funkelnden Pokalen", "tokens": ["Wo", "nie", "die", "Lust", "in", "fun\u00b7keln\u00b7den", "Po\u00b7ka\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Versiegt im Kreis der Damen und der Ritter,", "tokens": ["Ver\u00b7siegt", "im", "Kreis", "der", "Da\u00b7men", "und", "der", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und noch das Minnelied der Proven\u00e7alen", "tokens": ["Und", "noch", "das", "Min\u00b7ne\u00b7lied", "der", "Pro\u00b7ven\u00e7a\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sich schaukelt auf der wohlgestimmten Zither.", "tokens": ["Sich", "schau\u00b7kelt", "auf", "der", "wohl\u00b7ge\u00b7stimm\u00b7ten", "Zi\u00b7ther", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was preis' ich noch? Die waldbekr\u00e4nzten Schluchten,", "tokens": ["Was", "preis'", "ich", "noch", "?", "Die", "wald\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Schluch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchklungen vom Gesang der Nachtigallen?", "tokens": ["Durch\u00b7klun\u00b7gen", "vom", "Ge\u00b7sang", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die frischen Halden an den Meeresbuchten,", "tokens": ["Die", "fri\u00b7schen", "Hal\u00b7den", "an", "den", "Mee\u00b7res\u00b7buch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sanft dem Wogenschlage wiederhallen?", "tokens": ["Die", "sanft", "dem", "Wo\u00b7gen\u00b7schla\u00b7ge", "wie\u00b7der\u00b7hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Grotten, die, durchrauscht von Murmelb\u00e4chen,", "tokens": ["Die", "Grot\u00b7ten", ",", "die", ",", "durc\u00b7hrauscht", "von", "Mur\u00b7mel\u00b7b\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An Tropfstein und an hangenden Krystallen", "tokens": ["An", "Tropf\u00b7stein", "und", "an", "han\u00b7gen\u00b7den", "Krys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "--+-++---+-", "measure": "anapaest.init"}, "line.7": {"text": "Den Tagesstrahl zu ew'gem Zwielicht brechen?", "tokens": ["Den", "Ta\u00b7ges\u00b7strahl", "zu", "ew'\u00b7gem", "Zwie\u00b7licht", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Das all ist dein und mehr \u2013 endlose Minen,", "tokens": ["Das", "all", "ist", "dein", "und", "mehr", "\u2013", "end\u00b7lo\u00b7se", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "PPOSAT", "KON", "ADV", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Geistern \u00fcberwachte Wunderhorte,", "tokens": ["Von", "Geis\u00b7tern", "\u00fc\u00b7ber\u00b7wach\u00b7te", "Wun\u00b7der\u00b7hor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Demant reich und funkelnden Rubinen,", "tokens": ["An", "De\u00b7mant", "reich", "und", "fun\u00b7keln\u00b7den", "Ru\u00b7bi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im Erdenschacht \u2013 \u2013 doch schwach sind meine Worte,", "tokens": ["Im", "Er\u00b7den\u00b7schacht", "\u2013", "\u2013", "doch", "schwach", "sind", "mei\u00b7ne", "Wor\u00b7te", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Wimper sinkt, die grellen Strahlen bleichen;", "tokens": ["Die", "Wim\u00b7per", "sinkt", ",", "die", "grel\u00b7len", "Strah\u00b7len", "blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dank, sch\u00f6ner Knabe! Offen steht die Pforte,", "tokens": ["Dank", ",", "sch\u00f6\u00b7ner", "Kna\u00b7be", "!", "Of\u00b7fen", "steht", "die", "Pfor\u00b7te", ","], "to