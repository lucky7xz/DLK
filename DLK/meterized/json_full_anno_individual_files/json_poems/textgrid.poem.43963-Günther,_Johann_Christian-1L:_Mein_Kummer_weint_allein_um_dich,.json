{"textgrid.poem.43963": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Kummer weint allein um dich,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Kummer weint allein um dich,", "tokens": ["Mein", "Kum\u00b7mer", "weint", "al\u00b7lein", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit mir ist's so verloren,", "tokens": ["Mit", "mir", "ist's", "so", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Umst\u00e4nd \u00fcberweisen mich,", "tokens": ["Die", "Um\u00b7st\u00e4nd", "\u00fc\u00b7ber\u00b7wei\u00b7sen", "mich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sey zur Noth gebohren.", "tokens": ["Ich", "sey", "zur", "Noth", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach, spare Seufzer, Wuntsch und Flehn,", "tokens": ["Ach", ",", "spa\u00b7re", "Seuf\u00b7zer", ",", "Wunt\u00b7sch", "und", "Flehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Du wirst mich wohl nicht wiedersehn", "tokens": ["Du", "wirst", "mich", "wohl", "nicht", "wie\u00b7der\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als etwan in den Auen,", "tokens": ["Als", "et\u00b7wan", "in", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Die Glaub und Hofnung schauen.", "tokens": ["Die", "Glaub", "und", "Hof\u00b7nung", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor diesem, da mir Flei\u00df und Kunst", "tokens": ["Vor", "die\u00b7sem", ",", "da", "mir", "Flei\u00df", "und", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf k\u00fcnftig Gl\u00fccke bl\u00fchte", "tokens": ["Auf", "k\u00fcnf\u00b7tig", "Gl\u00fc\u00b7cke", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und mancher sich um G\u00fcnthers Gunst", "tokens": ["Und", "man\u00b7cher", "sich", "um", "G\u00fcn\u00b7thers", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PRF", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon zum Voraus bem\u00fchte,", "tokens": ["Schon", "zum", "Vo\u00b7raus", "be\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da dacht ich, wider Feind und Neid", "tokens": ["Da", "dacht", "ich", ",", "wi\u00b7der", "Feind", "und", "Neid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Palmen der Best\u00e4ndigkeit", "tokens": ["Die", "Pal\u00b7men", "der", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit selbst erworbnem Seegen", "tokens": ["Mit", "selbst", "er\u00b7worb\u00b7nem", "See\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir noch in Schoos zu legen.", "tokens": ["Dir", "noch", "in", "Schoos", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der gute Vorsaz geht in Wind;", "tokens": ["Der", "gu\u00b7te", "Vor\u00b7saz", "geht", "in", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll im Staube liegen", "tokens": ["Ich", "soll", "im", "Stau\u00b7be", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als das \u00e4rmste Findelkind", "tokens": ["Und", "als", "das", "\u00e4rms\u00b7te", "Fin\u00b7del\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich unter Leuten schmiegen.", "tokens": ["Mich", "un\u00b7ter", "Leu\u00b7ten", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man l\u00e4st mich nicht, man st\u00f6st mich gar", "tokens": ["Man", "l\u00e4st", "mich", "nicht", ",", "man", "st\u00f6st", "mich", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "$,", "PIS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch st\u00fcndlich tiefer in Gefahr", "tokens": ["Noch", "st\u00fcnd\u00b7lich", "tie\u00b7fer", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sucht mein sch\u00f6nstes Leben", "tokens": ["Und", "sucht", "mein", "sch\u00f6ns\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Marter preiszugeben.", "tokens": ["Der", "Mar\u00b7ter", "preis\u00b7zu\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So wird auch wohl mein Alter seyn;", "tokens": ["So", "wird", "auch", "wohl", "mein", "Al\u00b7ter", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin des Klagens m\u00fcde", "tokens": ["Ich", "bin", "des", "Kla\u00b7gens", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mag nichts mehr gen Himmel schreyn", "tokens": ["Und", "mag", "nichts", "mehr", "gen", "Him\u00b7mel", "schreyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als: Herr, nun las im Friede!", "tokens": ["Als", ":", "Herr", ",", "nun", "las", "im", "Frie\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$.", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kraft, Muth und Jugend sind fast hin,", "tokens": ["Kraft", ",", "Muth", "und", "Ju\u00b7gend", "sind", "fast", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daher ich nicht mehr f\u00e4hig bin,", "tokens": ["Da\u00b7her", "ich", "nicht", "mehr", "f\u00e4\u00b7hig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch auserlesne Sachen", "tokens": ["Durch", "au\u00b7ser\u00b7les\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mir Gut und Ruhm zu machen.", "tokens": ["Mir", "Gut", "und", "Ruhm", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm also, liebstes Kind, dein Herz,", "tokens": ["Nimm", "al\u00b7so", ",", "liebs\u00b7tes", "Kind", ",", "dein", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O schweres Wort, zur\u00fccke", "tokens": ["O", "schwe\u00b7res", "Wort", ",", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kehre dich an keinen Schmerz,", "tokens": ["Und", "keh\u00b7re", "dich", "an", "kei\u00b7nen", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Womit ich's wiederschicke;", "tokens": ["Wo\u00b7mit", "ich's", "wie\u00b7der\u00b7schi\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist zu edel und zu treu,", "tokens": ["Es", "ist", "zu", "e\u00b7del", "und", "zu", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als da\u00df es mein Gefehrte sey", "tokens": ["Als", "da\u00df", "es", "mein", "Ge\u00b7fehr\u00b7te", "sey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wegen fremder Plage", "tokens": ["Und", "we\u00b7gen", "frem\u00b7der", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein eignes Heil verschlage.", "tokens": ["Sein", "eig\u00b7nes", "Heil", "ver\u00b7schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du kanst dir durch dies theure Pfand", "tokens": ["Du", "kanst", "dir", "durch", "dies", "theu\u00b7re", "Pfand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was K\u00f6stlichers erwerben,", "tokens": ["Was", "K\u00f6st\u00b7li\u00b7chers", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir mehrt es nur den Jammerstand", "tokens": ["Mir", "mehrt", "es", "nur", "den", "Jam\u00b7mer\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4st mich schwerer sterben;", "tokens": ["Und", "l\u00e4st", "mich", "schwe\u00b7rer", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn weil du mich so z\u00e4rtlich liebst", "tokens": ["Denn", "weil", "du", "mich", "so", "z\u00e4rt\u00b7lich", "liebst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles vor mein Wohlseyn giebst,", "tokens": ["Und", "al\u00b7les", "vor", "mein", "Wohl\u00b7seyn", "giebst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So f\u00fchl ich halbe Leiche", "tokens": ["So", "f\u00fchl", "ich", "hal\u00b7be", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch zweyfach scharfe Streiche.", "tokens": ["Auch", "zwey\u00b7fach", "schar\u00b7fe", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich schwur vor diesem: Nur der Tod,", "tokens": ["Ich", "schwur", "vor", "die\u00b7sem", ":", "Nur", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "$.", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst soll uns wohl nichts trennen;", "tokens": ["Sonst", "soll", "uns", "wohl", "nichts", "tren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verzeih es jezo meiner Noth,", "tokens": ["Ver\u00b7zeih", "es", "je\u00b7zo", "mei\u00b7ner", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kan ich dir nicht g\u00f6nnen;", "tokens": ["Die", "kan", "ich", "dir", "nicht", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich liebe dich zu rein und scharf,", "tokens": ["Ich", "lie\u00b7be", "dich", "zu", "rein", "und", "scharf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKA", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als da\u00df ich noch begehren darf,", "tokens": ["Als", "da\u00df", "ich", "noch", "be\u00b7geh\u00b7ren", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df Lorchen auf der Erde", "tokens": ["Da\u00df", "Lor\u00b7chen", "auf", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch mich zur Wittwen werde.", "tokens": ["Durch", "mich", "zur", "Witt\u00b7wen", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So brich nur Bild und Ring entzwey", "tokens": ["So", "brich", "nur", "Bild", "und", "Ring", "ent\u00b7zwey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADV", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und las die Briefe lodern;", "tokens": ["Und", "las", "die", "Brie\u00b7fe", "lo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich gebe dich dem ersten frey", "tokens": ["Ich", 