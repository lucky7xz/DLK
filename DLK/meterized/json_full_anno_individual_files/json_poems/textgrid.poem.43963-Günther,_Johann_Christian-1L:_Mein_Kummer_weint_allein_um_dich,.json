ord", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als etwan in den Auen,", "tokens": ["Als", "et\u00b7wan", "in", "den", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Die Glaub und Hofnung schauen.", "tokens": ["Die", "Glaub", "und", "Hof\u00b7nung", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vor diesem, da mir Flei\u00df und Kunst", "tokens": ["Vor", "die\u00b7sem", ",", "da", "mir", "Flei\u00df", "und", "Kunst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf k\u00fcnftig Gl\u00fccke bl\u00fchte", "tokens": ["Auf", "k\u00fcnf\u00b7tig", "Gl\u00fc\u00b7cke", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und mancher sich um G\u00fcnthers Gunst", "tokens": ["Und", "man\u00b7cher", "sich", "um", "G\u00fcn\u00b7thers", "Gunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PRF", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon zum Voraus bem\u00fchte,", "tokens": ["Schon", "zum", "Vo\u00b7raus", "be\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da dacht ich, wider Feind und Neid", "tokens": ["Da", "dacht", "ich", ",", "wi\u00b7der", "Feind", "und", "Neid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Palmen der Best\u00e4ndigkeit", "tokens": ["Die", "Pal\u00b7men", "der", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit selbst erworbnem Seegen", "tokens": ["Mit", "selbst", "er\u00b7worb\u00b7nem", "See\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dir noch in Schoos zu legen.", "tokens": ["Dir", "noch", "in", "Schoos", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der gute Vorsaz geht in Wind;", "tokens": ["Der", "gu\u00b7te", "Vor\u00b7saz", "geht", "in", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich soll im Staube liegen", "tokens": ["Ich", "soll", "im", "Stau\u00b7be", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als das \u00e4rmste Findelkind", "tokens": ["Und", "als", "das", "\u00e4rms\u00b7te", "Fin\u00b7del\u00b7kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich unt