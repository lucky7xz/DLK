": ["VVIMP", "PPER", "ADV", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von nun an aus dem Sinne;", "tokens": ["Von", "nun", "an", "aus", "dem", "Sin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mein leztes W\u00fcntschen ist erf\u00fcllt,", "tokens": ["Mein", "lez\u00b7tes", "W\u00fcnt\u00b7schen", "ist", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern ich dies gewinne,", "tokens": ["Wo\u00b7fern", "ich", "dies", "ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df mit der Zeit noch jemand spricht:", "tokens": ["Da\u00df", "mit", "der", "Zeit", "noch", "je\u00b7mand", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Philimen die Ketten bricht,", "tokens": ["Wenn", "Phi\u00b7li\u00b7men", "die", "Ket\u00b7ten", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "So sind's nicht Falschheitstriebe,", "tokens": ["So", "sin\u00b7d's", "nicht", "Falschheit\u00b7strie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er hast sie nur aus Liebe.", "tokens":