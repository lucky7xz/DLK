{"dta.poem.3545": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Abschied im Korbe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Wo gehst du hin du Stolze,               ", "tokens": ["Er", ".", "Wo", "gehst", "du", "hin", "du", "Stol\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hab ich dir gethan;", "tokens": ["Was", "hab", "ich", "dir", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du vorbei thust gehen,", "tokens": ["Da\u00df", "du", "vor\u00b7bei", "thust", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schaust mich gar nicht an.", "tokens": ["Und", "schaust", "mich", "gar", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du schl\u00e4gst die Aeuglein nieder,", "tokens": ["Du", "schl\u00e4gst", "die", "A\u00b7e\u00b7u\u00b7glein", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schaust nicht zu mir her;", "tokens": ["Und", "schaust", "nicht", "zu", "mir", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie wenn ich deines Gleichen", "tokens": ["Wie", "wenn", "ich", "dei\u00b7nes", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Niemals gewesen w\u00e4r.", "tokens": ["Nie\u00b7mals", "ge\u00b7we\u00b7sen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAPP", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Sie. Der Abschied ist geschrieben,               ", "tokens": ["Sie", ".", "Der", "Ab\u00b7schied", "ist", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das K\u00f6rblein ist gemacht;", "tokens": ["Das", "K\u00f6rb\u00b7lein", "ist", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4rst du bei mir geblieben,", "tokens": ["W\u00e4rst", "du", "bei", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "H\u00e4tt ich dich nicht veracht.", "tokens": ["H\u00e4tt", "ich", "dich", "nicht", "ver\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er. Nimm du das K\u00f6rblein mit nach Haus,               ", "tokens": ["Er", ".", "Nimm", "du", "das", "K\u00f6rb\u00b7lein", "mit", "nach", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NE", "PPER", "ART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und leg den Abschied nein;", "tokens": ["Und", "leg", "den", "Ab\u00b7schied", "nein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinf\u00fchro aber lasse brav", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "a\u00b7ber", "las\u00b7se", "brav"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das falsche Lieben seyn.", "tokens": ["Das", "fal\u00b7sche", "Lie\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}