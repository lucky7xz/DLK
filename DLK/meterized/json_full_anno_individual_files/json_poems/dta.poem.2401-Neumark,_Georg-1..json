{"dta.poem.2401": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Jupiter dein' Augen nieder/", "tokens": ["La\u00df", "Ju\u00b7pi\u00b7ter", "dein'", "Au\u00b7gen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Seh an mein hochgequ\u00e4ltes Hertz/", "tokens": ["Seh", "an", "mein", "hoch\u00b7ge\u00b7qu\u00e4l\u00b7tes", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "H\u00f6r Himmel meine Klagelieder", "tokens": ["H\u00f6r", "Him\u00b7mel", "mei\u00b7ne", "Kla\u00b7ge\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vernim doch meinen Liebesschmertz", "tokens": ["Ver\u00b7nim", "doch", "mei\u00b7nen", "Lie\u00b7bes\u00b7schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich nicht kan das Hertz erweichen", "tokens": ["Weil", "ich", "nicht", "kan", "das", "Hertz", "er\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch meiner Liebsten Gunst erreichen.", "tokens": ["Noch", "mei\u00b7ner", "Liebs\u00b7ten", "Gunst", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin ein H\u00fcrt von jungen Jahren/", "tokens": ["Ich", "bin", "ein", "H\u00fcrt", "von", "jun\u00b7gen", "Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ein beliebtes Angesicht", "tokens": ["Hab'", "ein", "be\u00b7lieb\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch wil sie sich mit mir nicht pahren.", "tokens": ["Doch", "wil", "sie", "sich", "mit", "mir", "nicht", "pah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie achtet meiner Trene nicht.", "tokens": ["Sie", "ach\u00b7tet", "mei\u00b7ner", "Tre\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lebe noch in gr\u00fcner Jugend", "tokens": ["Ich", "le\u00b7be", "noch", "in", "gr\u00fc\u00b7ner", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gantz durchsternt mit \u00e4dler ", "tokens": ["Die", "gantz", "durchs\u00b7ternt", "mit", "\u00e4d\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auch hab\u2019 ich noch so manche ", "tokens": ["Auch", "hab'", "ich", "noch", "so", "man\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da man sich reichlich n\u00e4hren kan/", "tokens": ["Da", "man", "sich", "reich\u00b7lich", "n\u00e4h\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manche schattenreiche W\u00e4lder/", "tokens": ["So", "man\u00b7che", "schat\u00b7ten\u00b7rei\u00b7che", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich beliebter ", "tokens": ["Da", "ich", "be\u00b7lieb\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und mit vergn\u00fcgter Anmuht weiden.", "tokens": ["Und", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "An\u00b7muht", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch dennoch k\u00f6mmt die\u00df ", "tokens": ["Noch", "den\u00b7noch", "k\u00f6mmt", "die\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PDS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gar nichtig vor/ und h\u00e4lt es schlecht/", "tokens": ["Gar", "nich\u00b7tig", "vor", "/", "und", "h\u00e4lt", "es", "schlecht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$(", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich leb ihr/ wie ich kan/ zu willen/", "tokens": ["Ich", "leb", "ihr", "/", "wie", "ich", "kan", "/", "zu", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VMFIN", "$(", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dien\u2019 ihr als ihr eigner ", "tokens": ["Ich", "dien'", "ihr", "als", "ihr", "eig\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sieht da\u00df all mein ", "tokens": ["Sie", "sieht", "da\u00df", "all", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Sich nur allein zu ihr mu\u00df richten.", "tokens": ["Sich", "nur", "al\u00b7lein", "zu", "ihr", "mu\u00df", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und gleichwol bin ich ihr zuwieder", "tokens": ["Und", "gleich\u00b7wol", "bin", "ich", "ihr", "zu\u00b7wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mach\u2019 es auch nur wie ich woll\u2019/", "tokens": ["Ich", "mach'", "es", "auch", "nur", "wie", "ich", "woll'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sing\u2019 ihr ja die sch\u00f6nsten ", "tokens": ["Ich", "sing'", "ihr", "ja", "die", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich thu was ein ", "tokens": ["Ich", "thu", "was", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Da\u00df sie die\u00df alles so verkleinet.", "tokens": ["Da\u00df", "sie", "die\u00df", "al\u00b7les", "so", "ver\u00b7klei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie lange wollet ihr noch sehen/", "tokens": ["Wie", "lan\u00b7ge", "wol\u00b7let", "ihr", "noch", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr G\u00f6tter solch\u2019 ", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "solch'"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PIAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie lange wolt ihr noch gestehen", "tokens": ["Wie", "lan\u00b7ge", "wolt", "ihr", "noch", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Sie mir mache solches ", "tokens": ["Da\u00df", "Sie", "mir", "ma\u00b7che", "sol\u00b7ches"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kupido strafe diese Thaten/", "tokens": ["Ku\u00b7pi\u00b7do", "stra\u00b7fe", "die\u00b7se", "Tha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDAT", "NN", "$("], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jm fall du dir und mir willst rahten.", "tokens": ["Jm", "fall", "du", "dir", "und", "mir", "willst", "rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPER", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dich sch\u00e4tzet sie vor einen ", "tokens": ["Dich", "sch\u00e4t\u00b7zet", "sie", "vor", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der nichts als ", "tokens": ["Der", "nichts", "als"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mich wil ", "tokens": ["Mich", "wil"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Mich hochbetr\u00fcbt verliebten Mann.", "tokens": ["Mich", "hoch\u00b7be\u00b7tr\u00fcbt", "ver\u00b7lieb\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich ihr doch mein ", "tokens": ["Da", "ich", "ihr", "doch", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hab\u2019 eigenth\u00fcmlich \u00fcbergeben.", "tokens": ["Hab'", "ei\u00b7gent\u00b7h\u00fcm\u00b7lich", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was hilfft mich doch mein kl\u00e4glichs ", "tokens": ["Was", "hilfft", "mich", "doch", "mein", "kl\u00e4g\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was sag\u2019 ich viel von meiner Brunst?", "tokens": ["Was", "sag'", "ich", "viel", "von", "mei\u00b7ner", "Brunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich ", "tokens": ["Weil", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Da\u00df Sie mir zeig' ihr' Hertzensgunst.", "tokens": ["Da\u00df", "Sie", "mir", "zeig'", "ih\u00b7r'", "Hert\u00b7zens\u00b7gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So mag mein ", "tokens": ["So", "mag", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Auf diesen mattenreichen Haiden.", "tokens": ["Auf", "die\u00b7sen", "mat\u00b7ten\u00b7rei\u00b7chen", "Hai\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hab gute Nacht O ", "tokens": ["Hab", "gu\u00b7te", "Nacht", "O"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Leb wol und d\u00e4nk an meine Treu'/", "tokens": ["Leb", "wol", "und", "d\u00e4nk", "an", "mei\u00b7ne", "Treu'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe da\u00df dein harter Wille", "tokens": ["Ich", "se\u00b7he", "da\u00df", "dein", "har\u00b7ter", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch meinen Tod vergn\u00fcget sey.", "tokens": ["Durch", "mei\u00b7nen", "Tod", "ver\u00b7gn\u00fc\u00b7get", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wolan du wirst im kurtzen sehen", "tokens": ["Wo\u00b7lan", "du", "wirst", "im", "kurt\u00b7zen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "APPRART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was deinetwillen ist geschehen.", "tokens": ["Was", "dei\u00b7net\u00b7wil\u00b7len", "ist", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}