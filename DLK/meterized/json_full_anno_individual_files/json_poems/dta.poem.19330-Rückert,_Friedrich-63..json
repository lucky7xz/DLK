{"dta.poem.19330": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder Glaube h\u00e4lt sich f\u00fcr den einzig wahren,", "tokens": ["Ein", "je\u00b7der", "Glau\u00b7be", "h\u00e4lt", "sich", "f\u00fcr", "den", "ein\u00b7zig", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Kraft kann er auch so nur offenbaren.", "tokens": ["Und", "sei\u00b7ne", "Kraft", "kann", "er", "auch", "so", "nur", "of\u00b7fen\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der einzig wahre nur ist er an seinem Ort,", "tokens": ["Der", "ein\u00b7zig", "wah\u00b7re", "nur", "ist", "er", "an", "sei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht minder aber wahr sind andre hier und dort.", "tokens": ["Nicht", "min\u00b7der", "a\u00b7ber", "wahr", "sind", "and\u00b7re", "hier", "und", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VAFIN", "PIS", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was hat denn nun ein Mensch vom Glaubenswort zu halten?", "tokens": ["Was", "hat", "denn", "nun", "ein", "Mensch", "vom", "Glau\u00b7bens\u00b7wort", "zu", "hal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das seinige f\u00fcr wahr an seinem Ort zu halten.", "tokens": ["Das", "sei\u00b7ni\u00b7ge", "f\u00fcr", "wahr", "an", "sei\u00b7nem", "Ort", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sohn, halt an deinem Ort an deinem Glaubenswort,", "tokens": ["Sohn", ",", "halt", "an", "dei\u00b7nem", "Ort", "an", "dei\u00b7nem", "Glau\u00b7bens\u00b7wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und la\u00df am ihrigen die andern halten dort!", "tokens": ["Und", "la\u00df", "am", "ih\u00b7ri\u00b7gen", "die", "an\u00b7dern", "hal\u00b7ten", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPRART", "NN", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}