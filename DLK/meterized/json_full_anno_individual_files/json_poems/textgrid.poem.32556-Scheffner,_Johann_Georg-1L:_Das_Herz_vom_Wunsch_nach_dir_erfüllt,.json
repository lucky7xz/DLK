 "PPER", "ADJD", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Thr\u00e4nchen zittern steht,", "tokens": ["Ein", "Thr\u00e4n\u00b7chen", "zit\u00b7tern", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sch\u00f6ner, wenn den losen Blick", "tokens": ["Doch", "sch\u00f6\u00b7ner", ",", "wenn", "den", "lo\u00b7sen", "Blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des L\u00e4chelns Reitz erh\u00f6h't.", "tokens": ["Des", "L\u00e4\u00b7chelns", "Reitz", "er\u00b7h\u00f6h'", "t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "NE", "XY", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der freygew\u00f6lbte Busen stieg", "tokens": ["Der", "frey\u00b7ge\u00b7w\u00f6lb\u00b7te", "Bu\u00b7sen", "stieg"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Wollustahndungen,", "tokens": ["Von", "Wol\u00b7lus\u00b7tahn\u00b7dun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die braunen Augen scho\u00dfen Blitz,", "tokens": ["Die", "brau\u00b7nen", "Au\u00b7gen", "scho\u00b7\u00dfen", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Streit zu verk\u00fcndigen.", "tokens": ["Streit", "zu", "ver\u00b7k\u00fcn\u00b7di\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Laut schl\u00e4gt mein Herz von Dir ber\u00fchrt", "tokens": ["Laut", "schl\u00e4gt", "mein", "Herz", "von", "Dir", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hei\u00df, wenn die weiche Hand", "tokens": ["Hei\u00df", ",", "wenn", "die", "wei\u00b7che", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Pfeil auf Amors Bogen legt,", "tokens": ["Den", "Pfeil", "auf", "A\u00b7mors", "Bo\u00b7gen", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fchn die Sehne spannt.", "tokens": ["Und", "k\u00fchn", "die", "Seh\u00b7ne", "spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ha! denn flie\u00dft mir die Seele ganz", "tokens": ["Ha", "!", "denn", "flie\u00dft", "mir", "die", "See\u00b7le", "ganz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mit Amors Pfeil ins Ziel!", "tokens": ["Mit", "A\u00b7mors", "Pfeil", "ins", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach M\u00e4dchen auch Dein Aug bricht dann,", "tokens": ["Ach", "M\u00e4d\u00b7chen", "auch", "Dein", "Aug", "bricht", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spricht Gluth und Gef\u00fchl.", "tokens": ["Und", "spricht", "Gluth", "und", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Entz\u00fcckend zischt Champagnerschaum", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "zischt", "Cham\u00b7pag\u00b7ner\u00b7schaum"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Rande des Pokals,", "tokens": ["Am", "Ran\u00b7de", "des", "Po\u00b7kals", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sch\u00f6nrer Schaum h\u00e4ngt dann am Busch", "tokens": ["Doch", "sch\u00f6n\u00b7rer", "Schaum", "h\u00e4ngt", "dann", "am", "Busch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "des duftgen Wollustthals.", "tokens": ["des", "duft\u00b7gen", "Wol\u00b7lust\u00b7thals", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Kein Tempe, kein Elysium", "tokens": ["Kein", "Tem\u00b7pe", ",", "kein", "E\u00b7ly\u00b7si\u00b7um"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sch\u00f6n wie ", "tokens": ["Ist", "sch\u00f6n", "wie"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hier halten Aug, Gef\u00fchl, Geruch", "tokens": ["Hier", "hal\u00b7ten", "Aug", ",", "Ge\u00b7f\u00fchl", ",", "Ge\u00b7ruch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Berauscht ihr G\u00f6ttermahl.", "tokens": ["Be\u00b7rauscht", "ihr", "G\u00f6t\u00b7ter\u00b7mahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Rosenkleid hat Cypra", "tokens": ["Das", "Ro\u00b7sen\u00b7kleid", "hat", "Cyp\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm angelegt, sie ist", "tokens": ["Ihm", "an\u00b7ge\u00b7legt", ",", "sie", "ist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sch\u00f6pferin des Quells, der aus", "tokens": ["Die", "Sch\u00f6p\u00b7fe\u00b7rin", "des", "Quells", ",", "der", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Muschelgrotte flie\u00dft.", "tokens": ["Der", "Mu\u00b7schel\u00b7grot\u00b7te", "flie\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zehntausendmal sagt dir mein Ku\u00df",