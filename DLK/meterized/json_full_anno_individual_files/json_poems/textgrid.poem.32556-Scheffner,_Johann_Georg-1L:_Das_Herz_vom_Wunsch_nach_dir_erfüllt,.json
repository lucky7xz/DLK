{"textgrid.poem.32556": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Herz vom Wunsch nach dir erf\u00fcllt,", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz vom Wunsch nach dir erf\u00fcllt,", "tokens": ["Das", "Herz", "vom", "Wunsch", "nach", "dir", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erflehte Dich zur\u00fcck,", "tokens": ["Er\u00b7fleh\u00b7te", "Dich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam der Schlaf, und wies im Traum", "tokens": ["Da", "kam", "der", "Schlaf", ",", "und", "wies", "im", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir das erflehte Gl\u00fcck.", "tokens": ["Mir", "das", "er\u00b7fleh\u00b7te", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sah' dich sch\u00f6n wie Grazien,", "tokens": ["Ich", "sah'", "dich", "sch\u00f6n", "wie", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Cypripor sie k\u00fc\u00dft,", "tokens": ["Wenn", "Cyp\u00b7ri\u00b7por", "sie", "k