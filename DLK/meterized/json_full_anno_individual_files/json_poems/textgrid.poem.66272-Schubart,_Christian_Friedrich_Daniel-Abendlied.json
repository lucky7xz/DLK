{"textgrid.poem.66272": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ruht, ihr matten Kr\u00e4fte,", "tokens": ["Nun", "ruht", ",", "ihr", "mat\u00b7ten", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Joche der Gesch\u00e4fte,", "tokens": ["Vom", "Jo\u00b7che", "der", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Menschennacken dr\u00fcckt.", "tokens": ["Das", "Men\u00b7schen\u00b7na\u00b7cken", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schau, wie der Quell der Wonne,", "tokens": ["Schau", ",", "wie", "der", "Quell", "der", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "O Seele, wie die Sonne", "tokens": ["O", "See\u00b7le", ",", "wie", "die", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit rothem Antlitz nach dir blickt.", "tokens": ["Mit", "ro\u00b7them", "Ant\u00b7litz", "nach", "dir", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Noch seh' ich ihre Strahlen", "tokens": ["Noch", "seh'", "ich", "ih\u00b7re", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Abendhimmel malen;", "tokens": ["Den", "A\u00b7bend\u00b7him\u00b7mel", "ma\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch h\u00e4ngt ihr Silberlicht", "tokens": ["Noch", "h\u00e4ngt", "ihr", "Sil\u00b7ber\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "An Bl\u00e4ttern und Gestr\u00e4uchen;", "tokens": ["An", "Bl\u00e4t\u00b7tern", "und", "Ge\u00b7str\u00e4u\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch spiegelt sie in Teichen", "tokens": ["Noch", "spie\u00b7gelt", "sie", "in", "Tei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr blutumstr\u00f6mtes Angesicht.", "tokens": ["Ihr", "blu\u00b7tum\u00b7str\u00f6m\u00b7tes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es streckt sich Berg und H\u00fcgel,", "tokens": ["Es", "streckt", "sich", "Berg", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Vogel f\u00e4rbt die Fl\u00fcgel,", "tokens": ["Der", "Vo\u00b7gel", "f\u00e4rbt", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schwingt sie in Sonnengluth.", "tokens": ["Schwingt", "sie", "in", "Son\u00b7nen\u00b7gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch jetzo geht sie unter,", "tokens": ["Doch", "jet\u00b7zo", "geht", "sie", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Creaturen Wunder,", "tokens": ["Der", "Crea\u00b7tu\u00b7ren", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und malt den Horizont mit Blut.", "tokens": ["Und", "malt", "den", "Ho\u00b7ri\u00b7zont", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geh, Sonne, immer nieder,", "tokens": ["Geh", ",", "Son\u00b7ne", ",", "im\u00b7mer", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du kommst ja morgen wieder.", "tokens": ["Du", "kommst", "ja", "mor\u00b7gen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, leb' ich morgen noch?", "tokens": ["Doch", ",", "leb'", "ich", "mor\u00b7gen", "noch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gott, wie vom Wetterschlage", "tokens": ["Gott", ",", "wie", "vom", "Wet\u00b7ter\u00b7schla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Bin ich von dieser Frage", "tokens": ["Bin", "ich", "von", "die\u00b7ser", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ger\u00fchrt, leb' ich auch morgen noch?", "tokens": ["Ge\u00b7r\u00fchrt", ",", "leb'", "ich", "auch", "mor\u00b7gen", "noch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Vielleicht werd' ich die H\u00f6hen", "tokens": ["Viel\u00b7leicht", "werd'", "ich", "die", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Himmels nimmer sehen,", "tokens": ["Des", "Him\u00b7mels", "nim\u00b7mer", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht mehr der Sterne Lauf.", "tokens": ["Nicht", "mehr", "der", "Ster\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht schon morgen trinket", "tokens": ["Viel\u00b7leicht", "schon", "mor\u00b7gen", "trin\u00b7ket"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonne, eh' sie sinket,", "tokens": ["Die", "Son\u00b7ne", ",", "eh'", "sie", "sin\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Dunst von meiner Leiche auf.", "tokens": ["Den", "Dunst", "von", "mei\u00b7ner", "Lei\u00b7che", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die matten Kr\u00e4fte wanken", "tokens": ["Die", "mat\u00b7ten", "Kr\u00e4f\u00b7te", "wan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beim schrecklichen Gedanken", "tokens": ["Beim", "schreck\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der blo\u00dfen M\u00f6glichkeit:", "tokens": ["Der", "blo\u00b7\u00dfen", "M\u00f6g\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch heute kannst du sterben!", "tokens": ["Noch", "heu\u00b7te", "kannst", "du", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es lau'rt auf dich Verderben,", "tokens": ["Es", "lau'rt", "auf", "dich", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tod und Gericht und Ewigkeit.", "tokens": ["Tod", "und", "Ge\u00b7richt", "und", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "O Gott! ich bin verloren,", "tokens": ["O", "Gott", "!", "ich", "bin", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn nicht in meine Ohren", "tokens": ["Wenn", "nicht", "in", "mei\u00b7ne", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch jene Stimme spricht:", "tokens": ["Auch", "je\u00b7ne", "Stim\u00b7me", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "(schon h\u00f6r' ich sie und bebe", "tokens": ["(", "schon", "h\u00f6r'", "ich", "sie", "und", "be\u00b7be"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht mehr) ", "tokens": ["Nicht", "mehr", ")"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Nun dann, die Thr\u00e4nen flie\u00dfen,", "tokens": ["Nun", "dann", ",", "die", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich sinke zu den F\u00fc\u00dfen", "tokens": ["Ich", "sin\u00b7ke", "zu", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Menschenvaters hin.", "tokens": ["Des", "Men\u00b7schen\u00b7va\u00b7ters", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ach h\u00f6re du mein Flehen,", "tokens": ["Ach", "h\u00f6\u00b7re", "du", "mein", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df mich nicht schlafen gehen,", "tokens": ["La\u00df", "mich", "nicht", "schla\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Mein Gott, bis ich bekehret bin.", "tokens": ["Mein", "Gott", ",", "bis", "ich", "be\u00b7keh\u00b7ret", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Entweicht ihr Todesschmerzen;", "tokens": ["Ent\u00b7weicht", "ihr", "To\u00b7des\u00b7schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon thront in meinem Herzen", "tokens": ["Schon", "thront", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erhabne Seelenruh'.", "tokens": ["Er\u00b7hab\u00b7ne", "See\u00b7len\u00b7ruh'", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich lasse diese St\u00e4tte,", "tokens": ["Ich", "las\u00b7se", "die\u00b7se", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es deckt mich nun mein Bette", "tokens": ["Es", "deckt", "mich", "nun", "mein", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sanft, wie der Vorsicht Fl\u00fcgel, zu.", "tokens": ["Sanft", ",", "wie", "der", "Vor\u00b7sicht", "Fl\u00fc\u00b7gel", ",", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun ruht, ihr matten Kr\u00e4fte,", "tokens": ["Nun", "ruht", ",", "ihr", "mat\u00b7ten", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Joche der Gesch\u00e4fte,", "tokens": ["Vom", "Jo\u00b7che", "der", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN",