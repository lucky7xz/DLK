{"dta.poem.3206": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Freund, um irdischen Gewinnstes Opferung", "tokens": ["Ein", "Freund", ",", "um", "ir\u00b7di\u00b7schen", "Ge\u00b7winns\u00b7tes", "Op\u00b7fe\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Erkauft, ein solcher Kauf ist wohlfeil, scheint's, genung.", "tokens": ["Er\u00b7kauft", ",", "ein", "sol\u00b7cher", "Kauf", "ist", "wohl\u00b7feil", ",", "scheint's", ",", "ge\u00b7nung", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "PIAT", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch was man wohlfeil kauft, ist, sagt das Sprichwort, theuer;", "tokens": ["Doch", "was", "man", "wohl\u00b7feil", "kauft", ",", "ist", ",", "sagt", "das", "Sprich\u00b7wort", ",", "theu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PIS", "ADJD", "VVFIN", "$,", "VAFIN", "$,", "VVFIN", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist ein Freund, den feil mu\u00df machen Gab' und Steuer?", "tokens": ["Was", "ist", "ein", "Freund", ",", "den", "feil", "mu\u00df", "ma\u00b7chen", "Gab'", "und", "Steu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum fein auf deinem Recht besteh', und sei nicht bang;", "tokens": ["Drum", "fein", "auf", "dei\u00b7nem", "Recht", "be\u00b7steh'", ",", "und", "sei", "nicht", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Freund, den das verdrie\u00dft, der ist nicht von Belang.", "tokens": ["Ein", "Freund", ",", "den", "das", "ver\u00b7drie\u00dft", ",", "der", "ist", "nicht", "von", "Be\u00b7lang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "VVFIN", "$,", "PRELS", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}