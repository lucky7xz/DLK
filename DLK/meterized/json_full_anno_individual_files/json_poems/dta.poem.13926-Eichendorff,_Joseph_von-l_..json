{"dta.poem.13926": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch einzeln nicht verhallen", "tokens": ["Doch", "ein\u00b7zeln", "nicht", "ver\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darf, was ich jetzt gedacht.", "tokens": ["Darf", ",", "was", "ich", "jetzt", "ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Jeder meint, von ", "tokens": ["Was", "Je\u00b7der", "meint", ",", "von"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "PIS", "VVFIN", "$,", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sei's freudig auch gebracht!", "tokens": ["Sei's", "freu\u00b7dig", "auch", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}