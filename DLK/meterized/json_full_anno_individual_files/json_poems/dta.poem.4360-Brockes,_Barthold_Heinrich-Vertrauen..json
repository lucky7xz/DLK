{"dta.poem.4360": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vertrauen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Wesen lag von Ewigkeit in einem dunklen Nichts", "tokens": ["Mein", "We\u00b7sen", "lag", "von", "E\u00b7wig\u00b7keit", "in", "ei\u00b7nem", "dunk\u00b7len", "Nichts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "verborgen.", "tokens": ["ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich habe meinen C\u00f6rper nicht, auch meine Seele nicht", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "C\u00f6r\u00b7per", "nicht", ",", "auch", "mei\u00b7ne", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "gemacht.", "tokens": ["ge\u00b7macht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Drum la\u00df ich Den, Der mich, wie, wo, und wenn E", "tokens": ["Drum", "la\u00df", "ich", "Den", ",", "Der", "mich", ",", "wie", ",", "wo", ",", "und", "wenn", "E"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NE", "$,", "ART", "PPER", "$,", "PWAV", "$,", "PWAV", "$,", "KON", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wollt, hervorgebracht,", "tokens": ["wollt", ",", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie es mit mir in dieser Zeit,", "tokens": ["Wie", "es", "mit", "mir", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Tod, auch in der Ewigkeit,", "tokens": ["Jm", "Tod", ",", "auch", "in", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dereinst soll werden, sorgen.", "tokens": ["De\u00b7reinst", "soll", "wer\u00b7den", ",", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "VAINF", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nur einzig Deine Lieb\u2019, o ew\u2019ge Lieb\u2019, allein", "tokens": ["Nur", "ein\u00b7zig", "Dei\u00b7ne", "Lieb'", ",", "o", "ew'\u00b7ge", "Lieb'", ",", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "FM", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Soll in der allgemeinen D\u00e4mmrung, der Meynung Dunst,", "tokens": ["Soll", "in", "der", "all\u00b7ge\u00b7mei\u00b7nen", "D\u00e4mm\u00b7rung", ",", "der", "Mey\u00b7nung", "Dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "des Zweifels Nacht,", "tokens": ["des", "Zwei\u00b7fels", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Und Ungewi\u00dfheit aller Dinge, allein mein Licht und Leit-", "tokens": ["Und", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "al\u00b7ler", "Din\u00b7ge", ",", "al\u00b7lein", "mein", "Licht", "und", "Leit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PIAT", "NN", "$,", "ADV", "PPOSAT", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Stern seyn.", "tokens": ["Stern", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ich glaube, h\u00e4tt\u2019 Er von mir wollen,", "tokens": ["Ich", "glau\u00b7be", ",", "h\u00e4tt'", "Er", "von", "mir", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich ein mehres wissen sollen;", "tokens": ["Da\u00df", "ich", "ein", "meh\u00b7res", "wis\u00b7sen", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00e4tt\u2019 in diesem meinen Leben", "tokens": ["Er", "h\u00e4tt'", "in", "die\u00b7sem", "mei\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir mehr Erkenntni\u00df noch gegeben.", "tokens": ["Mir", "mehr", "Er\u00b7kennt\u00b7ni\u00df", "noch", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}