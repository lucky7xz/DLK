{"textgrid.poem.43503": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsfreude", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An uns auch ist der Ruf ergangen,", "tokens": ["An", "uns", "auch", "ist", "der", "Ruf", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr uns auch soll es Fr\u00fchling sein;", "tokens": ["F\u00fcr", "uns", "auch", "soll", "es", "Fr\u00fch\u00b7ling", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir haben auch ein gro\u00df Verlangen,", "tokens": ["Wir", "ha\u00b7ben", "auch", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im milden Fr\u00fchlingssonnenschein", "tokens": ["Im", "mil\u00b7den", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu fliegen in die Welt hinein,", "tokens": ["Zu", "flie\u00b7gen", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns zu erfreuen auch im Frei'n.", "tokens": ["Uns", "zu", "er\u00b7freu\u00b7en", "auch", "im", "Frei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "PTKZU", "VVINF", "ADV", "APPRART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir wollen scherzen und singen,", "tokens": ["Wir", "wol\u00b7len", "scher\u00b7zen", "und", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und spielen und tanzen und springen,", "tokens": ["Und", "spie\u00b7len", "und", "tan\u00b7zen", "und", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wir wollen pfl\u00fccken Bl\u00fcmelein,", "tokens": ["Wir", "wol\u00b7len", "pfl\u00fc\u00b7cken", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wohlgemuth und fr\u00f6hlich sein.", "tokens": ["Und", "wohl\u00b7ge\u00b7muth", "und", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So la\u00dft uns den Fr\u00fchling, den lieben, s\u00fc\u00dfen,", "tokens": ["So", "la\u00dft", "uns", "den", "Fr\u00fch\u00b7ling", ",", "den", "lie\u00b7ben", ",", "s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure"