{"dta.poem.3297": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Ein Gem\u00e4hlde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh! welche Schilderey!", "tokens": ["Sieh", "!", "wel\u00b7che", "Schil\u00b7de\u00b7rey", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bebl\u00fchmt kein wahrer May,", "tokens": ["Be\u00b7bl\u00fchmt", "kein", "wah\u00b7rer", "May", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jm Schoose der Natur,", "tokens": ["Jm", "Schoo\u00b7se", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O Phyllis! diese Flur?", "tokens": ["O", "Phyl\u00b7lis", "!", "die\u00b7se", "Flur", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein dick Geb\u00fcsch umkr\u00e4nzt", "tokens": ["Ein", "dick", "Ge\u00b7b\u00fcsch", "um\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Quelle, die hier gl\u00e4nzt:", "tokens": ["Die", "Quel\u00b7le", ",", "die", "hier", "gl\u00e4nzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am gr\u00fcnen Ufer hin", "tokens": ["Am", "gr\u00fc\u00b7nen", "U\u00b7fer", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Schl\u00e4ft eine Sch\u00e4ferinn.", "tokens": ["Schl\u00e4ft", "ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie liegt, nur leicht bedeckt,", "tokens": ["Sie", "liegt", ",", "nur", "leicht", "be\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In Bluhmen hingestreckt.", "tokens": ["In", "Bluh\u00b7men", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihren Locken spielt", "tokens": ["Mit", "ih\u00b7ren", "Lo\u00b7cken", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Zephyr, der sie k\u00fchlt;", "tokens": ["Ein", "Ze\u00b7phyr", ",", "der", "sie", "k\u00fchlt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und ihre weisse Brust,", "tokens": ["Und", "ih\u00b7re", "weis\u00b7se", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Schon reif zu schlauer Lust,", "tokens": ["Schon", "reif", "zu", "schlau\u00b7er", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verr\u00e4th sich unterm Flohr,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "sich", "un\u00b7term", "Flohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und wallt im Schlaf empor.", "tokens": ["Und", "wallt", "im", "Schlaf", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sieh diesen Sch\u00e4fer hier,", "tokens": ["Sieh", "die\u00b7sen", "Sch\u00e4\u00b7fer", "hier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der, unbewegt, nach ihr", "tokens": ["Der", ",", "un\u00b7be\u00b7wegt", ",", "nach", "ihr"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "$,", "ADJD", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit weiten Augen sieht:", "tokens": ["Mit", "wei\u00b7ten", "Au\u00b7gen", "sieht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie seine Wange gl\u00fcht!", "tokens": ["Wie", "sei\u00b7ne", "Wan\u00b7ge", "gl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Leib hangt ungeschickt,", "tokens": ["Sein", "Leib", "hangt", "un\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf einen Stab geb\u00fcckt,", "tokens": ["Auf", "ei\u00b7nen", "Stab", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In plumper Stellung hin", "tokens": ["In", "plum\u00b7per", "Stel\u00b7lung", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zur holden Schl\u00e4ferinn.", "tokens": ["Zur", "hol\u00b7den", "Schl\u00e4\u00b7fe\u00b7rinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Wilde f\u00fchlt ein Herz!", "tokens": ["Der", "Wil\u00b7de", "f\u00fchlt", "ein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat ihn der Liebe Scherz,", "tokens": ["Hat", "ihn", "der", "Lie\u00b7be", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Zeugen ihrer Macht,", "tokens": ["Als", "Zeu\u00b7gen", "ih\u00b7rer", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zur Sch\u00f6nen hergebracht?", "tokens": ["Zur", "Sch\u00f6\u00b7nen", "her\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hat schon mehr Verstand;", "tokens": ["Er", "hat", "schon", "mehr", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und wird ganz umgewandt", "tokens": ["Und", "wird", "ganz", "um\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu seinen Schafen gehn,", "tokens": ["Zu", "sei\u00b7nen", "Scha\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nachdem er sie gesehn.", "tokens": ["Nach\u00b7dem", "er", "sie", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}