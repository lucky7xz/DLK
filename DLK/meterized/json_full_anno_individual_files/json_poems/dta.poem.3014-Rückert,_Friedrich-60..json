{"dta.poem.3014": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn jene haben Recht, die in des Lebens Mitte", "tokens": ["Wenn", "je\u00b7ne", "ha\u00b7ben", "Recht", ",", "die", "in", "des", "Le\u00b7bens", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "VAFIN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das B\u00f6se sehn, den Feind lauernd auf Tritt und Schritte;", "tokens": ["Das", "B\u00f6\u00b7se", "sehn", ",", "den", "Feind", "lau\u00b7ernd", "auf", "Tritt", "und", "Schrit\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Seele, Str\u00e4fling-gleich, geschmiedet an den Karren,", "tokens": ["Die", "See\u00b7le", ",", "Str\u00e4f\u00b7ling\u00b7gleich", ",", "ge\u00b7schmie\u00b7det", "an", "den", "Kar\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und allzeit fertig zum Verbrecher oder Narren;", "tokens": ["Und", "all\u00b7zeit", "fer\u00b7tig", "zum", "Ver\u00b7bre\u00b7cher", "o\u00b7der", "Nar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im ungl\u00fcckseligen verh\u00e4ltnislosen Streite", "tokens": ["Im", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7gen", "ver\u00b7h\u00e4lt\u00b7nis\u00b7lo\u00b7sen", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das lichte P\u00fcnktchen mit der breiten Schattenseite:", "tokens": ["Das", "lich\u00b7te", "P\u00fcnkt\u00b7chen", "mit", "der", "brei\u00b7ten", "Schat\u00b7ten\u00b7sei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn das die Weisen sind, so sind wir bl\u00f6de Knaben,", "tokens": ["Wenn", "das", "die", "Wei\u00b7sen", "sind", ",", "so", "sind", "wir", "bl\u00f6\u00b7de", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wir am heitern Schein von au\u00dfen Lust noch haben;", "tokens": ["Die", "wir", "am", "hei\u00b7tern", "Schein", "von", "au\u00b7\u00dfen", "Lust", "noch", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "APPR", "ADV", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df wir nach Blumen gehn, von Kr\u00f6tengift bespritzt,", "tokens": ["Da\u00df", "wir", "nach", "Blu\u00b7men", "gehn", ",", "von", "Kr\u00f6\u00b7ten\u00b7gift", "be\u00b7spritzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach den Fr\u00fcchten sehn, vom innern Wurm beschmitzt.", "tokens": ["Und", "nach", "den", "Fr\u00fcch\u00b7ten", "sehn", ",", "vom", "in\u00b7nern", "Wurm", "be\u00b7schmitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch wenn wir haben Recht, wie Recht wir haben m\u00fc\u00dfen,", "tokens": ["Doch", "wenn", "wir", "ha\u00b7ben", "Recht", ",", "wie", "Recht", "wir", "ha\u00b7ben", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "NN", "$,", "PWAV", "NN", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Sch\u00f6nen uns zu freun, zu laben uns am S\u00fc\u00dfen;", "tokens": ["Am", "Sch\u00f6\u00b7nen", "uns", "zu", "freun", ",", "zu", "la\u00b7ben", "uns", "am", "S\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "So droht es unserem Genusse doch Verst\u00f6rung,", "tokens": ["So", "droht", "es", "un\u00b7se\u00b7rem", "Ge\u00b7nus\u00b7se", "doch", "Ver\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sehn stets jener dort unselige Beth\u00f6rung.", "tokens": ["Zu", "sehn", "stets", "je\u00b7ner", "dort", "un\u00b7se\u00b7li\u00b7ge", "Be\u00b7th\u00f6\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PDS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Alswie ein Wachender ganz aus dem Sinn nicht schlagen", "tokens": ["Als\u00b7wie", "ein", "Wa\u00b7chen\u00b7der", "ganz", "aus", "dem", "Sinn", "nicht", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die dummen Fratzen kann, die ihn im Traume plagen.", "tokens": ["Die", "dum\u00b7men", "Frat\u00b7zen", "kann", ",", "die", "ihn", "im", "Trau\u00b7me", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Und wie ein Denkender im Denken wird gest\u00f6rt,", "tokens": ["Und", "wie", "ein", "Den\u00b7ken\u00b7der", "im", "Den\u00b7ken", "wird", "ge\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er Wahnsinnige mit Ketten rasseln h\u00f6rt.", "tokens": ["Wenn", "er", "Wahn\u00b7sin\u00b7ni\u00b7ge", "mit", "Ket\u00b7ten", "ras\u00b7seln", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Doch wie gesund zum Trotz dem Kranken der Gesunde", "tokens": ["Doch", "wie", "ge\u00b7sund", "zum", "Trotz", "dem", "Kran\u00b7ken", "der", "Ge\u00b7sun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "APPRART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich f\u00fchlt, so f\u00fchle dich mit Gott im Seelengrunde.", "tokens": ["Sich", "f\u00fchlt", ",", "so", "f\u00fch\u00b7le", "dich", "mit", "Gott", "im", "See\u00b7len\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADV", "VVFIN", "PRF", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Arbeitsam, liebevoll, bescheiden und enthaltsam;", "tokens": ["Ar\u00b7beit\u00b7sam", ",", "lie\u00b7be\u00b7voll", ",", "be\u00b7schei\u00b7den", "und", "ent\u00b7halt\u00b7sam", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nicht z\u00fcgel-schrankenlos, in keinem Ding gewaltsam;", "tokens": ["Nicht", "z\u00fc\u00b7gel\u00b7schran\u00b7ken\u00b7los", ",", "in", "kei\u00b7nem", "Ding", "ge\u00b7walt\u00b7sam", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Vertrauend ihm, der dir den Himmelsfunken gab,", "tokens": ["Ver\u00b7trau\u00b7end", "ihm", ",", "der", "dir", "den", "Him\u00b7mels\u00b7fun\u00b7ken", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unverfinstert du ihn tragest \u00fcbers Grab;", "tokens": ["Da\u00df", "un\u00b7ver\u00b7fins\u00b7tert", "du", "ihn", "tra\u00b7gest", "\u00fc\u00b7bers", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "So beut dem Nachtspuk Trotz in lichter Zuversicht,", "tokens": ["So", "beut", "dem", "Nachts\u00b7puk", "Trotz", "in", "lich\u00b7ter", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcrchte als Gespenst dich selbst und andre nicht.", "tokens": ["Und", "f\u00fcrch\u00b7te", "als", "Ge\u00b7spenst", "dich", "selbst", "und", "and\u00b7re", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "PPER", "ADV", "KON", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}