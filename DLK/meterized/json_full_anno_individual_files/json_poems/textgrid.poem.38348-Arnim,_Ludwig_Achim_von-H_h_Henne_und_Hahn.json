{"textgrid.poem.38348": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "H h Henne und Hahn", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Henne fr\u00f6hlich gaggagagt, und macht ein gro\u00df Geschrei,", "tokens": ["Die", "Hen\u00b7ne", "fr\u00f6h\u00b7lich", "gag\u00b7ga\u00b7gagt", ",", "und", "macht", "ein", "gro\u00df", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die B\u00e4urin wei\u00df wohl, was sie sagt, und geht und holt das Ey,", "tokens": ["Die", "B\u00e4u\u00b7rin", "wei\u00df", "wohl", ",", "was", "sie", "sagt", ",", "und", "geht", "und", "holt", "das", "Ey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Der Hahn thut fr\u00fch aufwecken den Knecht und faule Magd,", "tokens": ["Der", "Hahn", "thut", "fr\u00fch", "auf\u00b7we\u00b7cken", "den", "Knecht", "und", "fau\u00b7le", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVIZU", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie thun sich erst recht strecken, und schlafen bis es tagt.", "tokens": ["Sie", "thun", "sich", "erst", "recht", "stre\u00b7cken", ",", "und", "schla\u00b7fen", "bis