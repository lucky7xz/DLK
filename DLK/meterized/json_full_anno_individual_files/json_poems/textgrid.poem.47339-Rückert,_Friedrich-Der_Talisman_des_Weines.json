{"textgrid.poem.47339": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Talisman des Weines", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer trinkt soll reines Herzens sein,", "tokens": ["Wer", "trinkt", "soll", "rei\u00b7nes", "Her\u00b7zens", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Wein ist nicht zu scherzen.", "tokens": ["Mit", "Wein", "ist", "nicht", "zu", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der reine rote Edelstein", "tokens": ["Der", "rei\u00b7ne", "ro\u00b7te", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Veredelt zwar die Herzen;", "tokens": ["Ver\u00b7e\u00b7delt", "zwar", "die", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch die Veredlung geht verloren,", "tokens": ["Doch", "die", "Ver\u00b7ed\u00b7lung", "geht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo nicht ist Edles eingeboren:", "tokens": ["Wo", "nicht", "ist", "Ed\u00b7les", "ein\u00b7ge\u00b7bo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "P