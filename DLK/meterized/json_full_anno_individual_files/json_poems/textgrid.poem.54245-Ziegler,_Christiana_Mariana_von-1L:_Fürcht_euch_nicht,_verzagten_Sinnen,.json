hmecken,", "tokens": ["So", "soll", "er", "doch", "von", "mir", "kein", "K\u00f6rn\u00b7gen", "Wey\u00b7rauch", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich kan ohnm\u00f6glich mich mit kleinen Kindern zecken.", "tokens": ["Ich", "kan", "ohn\u00b7m\u00f6g\u00b7lich", "mich", "mit", "klei\u00b7nen", "Kin\u00b7dern", "ze\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Nein, glaube nicht", "tokens": ["Nein", ",", "glau\u00b7be", "nicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df di\u00df geschicht,", "tokens": ["Da\u00df", "di\u00df", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erspahre ja den Rest von deinen Pfeilen,", "tokens": ["Er\u00b7spah\u00b7re", "ja", "den", "Rest", "von", "dei\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist umsonst,", "tokens": ["Es", "ist", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich kan mein Hertz nicht theilen.", "tokens": ["Ich", "kan", "mein", "Hertz", "nicht", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Musen-Gott soll nur allein", "tokens": ["Der", "Mu\u00b7sen\u00b7Gott", "soll", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Davon der Eigenthums- und Erb-Herr seyn.", "tokens": ["Da\u00b7von", "der", "Ei\u00b7gent\u00b7hums", "und", "Er\u00b7bHerr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "TRUNC", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Pierinnen Sayten-Spiel,", "tokens": ["Der", "Pie\u00b7rin\u00b7nen", "Say\u00b7ten\u00b7Spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Das mir von Jugend auf gefiel,", "tokens": ["Das", "mir", "von", "Ju\u00b7gend", "auf", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kan mir die Zeit,", "tokens": ["Kan", "mir", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "In meiner stillen Einsamkeit,", "tokens": ["In", "mei\u00b7ner", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mehr als zu wohl vertreiben,", "tokens": ["M