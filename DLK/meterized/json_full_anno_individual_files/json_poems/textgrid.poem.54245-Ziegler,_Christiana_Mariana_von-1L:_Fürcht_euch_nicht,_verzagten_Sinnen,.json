{"textgrid.poem.54245": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcrcht euch nicht, verzagten Sinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrcht euch nicht, verzagten Sinnen,", "tokens": ["F\u00fcrcht", "euch", "nicht", ",", "ver\u00b7zag\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Geist entrissen wird:", "tokens": ["Da\u00df", "mein", "Geist", "ent\u00b7ris\u00b7sen", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amor findet kein Geh\u00f6r,", "tokens": ["A\u00b7mor", "fin\u00b7det", "kein", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er auch ein Riese w\u00e4r,", "tokens": ["Wenn", "er", "auch", "ein", "Rie\u00b7se", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So soll er mir doch nicht der Freyheit G\u00fcrtel rauben,", "tokens": ["So", "soll", "er", "mir", "doch", "nicht", "der", "Frey\u00b7heit", "G\u00fcr\u00b7tel", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Arm ist viel zu schwach, ihr k\u00f6nt es sicher glauben.", "tokens": ["Sein", "Arm", "ist", "viel", "zu", "schwach", ",", "ihr", "k\u00f6nt", "es", "si\u00b7cher", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es mag auch alle Welt", "tokens": ["Es", "mag", "auch", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den so beschriehnen Wunder-Held", "tokens": ["Den", "so", "be\u00b7schrieh\u00b7nen", "Wun\u00b7der\u00b7Held"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un\u00fcberwindlich nennen,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird er mich doch nicht zu Boden werffen k\u00f6nnen,", "tokens": ["So", "wird", "er", "mich", "doch", "nicht", "zu", "Bo\u00b7den", "werf\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So reitzend er auch seine Minen,", "tokens": ["So", "reit\u00b7zend", "er", "auch", "sei\u00b7ne", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wodurch er manches Hertz bewegt,", "tokens": ["Wo\u00b7durch", "er", "man\u00b7ches", "Hertz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns offtermahls zu machen pflegt,", "tokens": ["Uns", "off\u00b7ter\u00b7mahls", "zu", "ma\u00b7chen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So m\u00fcssen selbge mir recht zum Gel\u00e4chter dienen.", "tokens": ["So", "m\u00fcs\u00b7sen", "selb\u00b7ge", "mir", "recht", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ich f\u00fcrchte weder Pfeil noch Bogen,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "we\u00b7der", "Pfeil", "noch", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie vielmahl hat er sich nicht schon,", "tokens": ["Wie", "viel\u00b7mahl", "hat", "er", "sich", "nicht", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zu nicht geringen Spott und Hohn,", "tokens": ["Zu", "nicht", "ge\u00b7rin\u00b7gen", "Spott", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In seinen Schu\u00df betrogen?", "tokens": ["In", "sei\u00b7nen", "Schu\u00df", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mein Hertze liegt zu weit entfernt,", "tokens": ["Mein", "Hert\u00b7ze", "liegt", "zu", "weit", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn er nicht besser schiessen lernt,", "tokens": ["Wenn", "er", "nicht", "bes\u00b7ser", "schies\u00b7sen", "lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So wird er nimmermehr,", "tokens": ["So", "wird", "er", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "H\u00f6rt! wie man ihn mu\u00df \u00e4ffen,", "tokens": ["H\u00f6rt", "!", "wie", "man", "ihn", "mu\u00df", "\u00e4f\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "PIS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Mein Hertze treffen.", "tokens": ["Mein", "Hert\u00b7ze", "tref\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Amor, bilde dir nicht ein,", "tokens": ["A\u00b7mor", ",", "bil\u00b7de", "dir", "nicht", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich w\u00fcrde zin\u00dfbar seyn.", "tokens": ["Da\u00df", "ich", "w\u00fcr\u00b7de", "zin\u00df\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird dein Altar, du stummes G\u00f6tzen-Bild,", "tokens": ["Wird", "dein", "Al\u00b7tar", ",", "du", "stum\u00b7mes", "G\u00f6t\u00b7zen\u00b7Bild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von andern gleich mit Opfern angef\u00fcllt,", "tokens": ["Von", "an\u00b7dern", "gleich", "mit", "Op\u00b7fern", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So soll er doch von mir kein K\u00f6rngen Weyrauch schmecken,", "tokens": ["So", "soll", "er", "doch", "von", "mir", "kein", "K\u00f6rn\u00b7gen", "Wey\u00b7rauch", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PIAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich kan ohnm\u00f6glich mich mit kleinen Kindern zecken.", "tokens": ["Ich", "kan", "ohn\u00b7m\u00f6g\u00b7lich", "mich", "mit", "klei\u00b7nen", "Kin\u00b7dern", "ze\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nein, glaube nicht", "tokens": ["Nein", ",", "glau\u00b7be", "nicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df di\u00df geschicht,", "tokens": ["Da\u00df", "di\u00df", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Erspahre ja den Rest von deinen Pfeilen,", "tokens": ["Er\u00b7spah\u00b7re", "ja", "den", "Rest", "von", "dei\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist umsonst,", "tokens": ["Es", "ist", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich kan mein Hertz nicht theilen.", "tokens": ["Ich", "kan", "mein", "Hertz", "nicht", "thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Musen-Gott soll nur allein", "tokens": ["Der", "Mu\u00b7sen\u00b7Gott", "soll", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Davon der Eigenthums- und Erb-Herr seyn.", "tokens": ["Da\u00b7von", "der", "Ei\u00b7gent\u00b7hums", "und", "Er\u00b7bHerr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "TRUNC", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Pierinnen Sayten-Spiel,", "tokens": ["Der", "Pie\u00b7rin\u00b7nen", "Say\u00b7ten\u00b7Spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Das mir von Jugend auf gefiel,", "tokens": ["Das", "mir", "von", "Ju\u00b7gend", "auf", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kan mir die Zeit,", "tokens": ["Kan", "mir", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "In meiner stillen Einsamkeit,", "tokens": ["In", "mei\u00b7ner", "stil\u00b7len", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mehr als zu wohl vertreiben,", "tokens": ["Mehr", "als", "zu", "wohl", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Drum will ich ihm auch treu verbleiben.", "tokens": ["Drum", "will", "ich", "ihm", "auch", "treu", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Euch ihr lieblichen Gespielen,", "tokens": ["Euch", "ihr", "lieb\u00b7li\u00b7chen", "Ge\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt mein Hertze zugethan:", "tokens": ["Bleibt", "mein", "Hert\u00b7ze", "zu\u00b7ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Last uns Crei\u00df und Reihen machen,", "tokens": ["Last", "uns", "Crei\u00df", "und", "Rei\u00b7hen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen, spielen, schertzen, lachen,", "tokens": ["Sin\u00b7gen", ",", "spie\u00b7len", ",", "schert\u00b7zen", ",", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wann Amor uns im Spiel,", "tokens": ["Und", "wann", "A\u00b7mor", "uns", "im", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch sein Blendwerck st\u00f6hren will,", "tokens": ["Durch", "sein", "Blend\u00b7werck", "st\u00f6h\u00b7ren", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen wir dem kleinen Geck, Brunst und K\u00fctzel zu vertreiben,", "tokens": ["Wol\u00b7len", "wir", "dem", "klei\u00b7nen", "Geck", ",", "Brunst", "und", "K\u00fct\u00b7zel", "zu", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Sein vertracktes Haut und Fell mit den sch\u00e4rffsten Nesseln reiben.", "tokens": ["Sein", "ver\u00b7track\u00b7tes", "Haut", "und", "Fell", "mit", "den", "sch\u00e4rffs\u00b7ten", "Nes\u00b7seln", "rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "F\u00fcrcht euch nicht, verzagten Sinnen,", "tokens": ["F\u00fcrcht", "euch", "nicht", ",", "ver\u00b7zag\u00b7ten", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Geist entrissen wird:", "tokens": ["Da\u00df", "mein", "Geist", "ent\u00b7ris\u00b7sen", "wird", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amor findet kein Geh\u00f6r,", "tokens": ["A\u00b7mor", "fin\u00b7det", "kein", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er auch ein Riese w\u00e4r,", "tokens": ["Wenn", "er", "auch", "ein", "Rie\u00b7se", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So soll er mir doch nicht der Freyheit G\u00fcrtel rauben,", "tokens": ["So", "soll", "er", "mir", "doch", "nicht", "der", "Frey\u00b7heit", "G\u00fcr\u00b7tel", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein Arm ist viel zu schwach, ihr k\u00f6nt es sicher glauben.", "tokens": ["Sein", "Arm", "ist", "viel", "zu", "schwach", ",", "ihr", "k\u00f6nt", "es", "si\u00b7cher", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es mag auch alle Welt", "tokens": ["Es", "mag", "auch", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den so beschriehnen Wunder-Held", "tokens": ["Den", "so", "be\u00b7schrieh\u00b7nen", "Wun\u00b7der\u00b7Held"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un\u00fcberwindlich nennen,", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird er mich doch nicht zu Boden werffen k\u00f6nnen,", "tokens": ["So", "wird", "er", "mich", "doch", "nicht", "zu", "Bo\u00b7den", "werf\u00b7fen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So reitzend er auch seine Minen,", "tokens": ["So", "reit\u00b7zend", "er", "auch", "sei\u00b7ne", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wodurch er manches Hertz bewegt,", "tokens": ["Wo\u00b7durch", "er", "man\u00b7ches", "Hertz", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns offtermahls zu machen pflegt,", "tokens": ["Uns", "off\u00b7ter\u00b7mahls", "zu", "ma\u00b7chen", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So m\u00fcssen selbge mir recht zum Gel\u00e4chter dienen.", "tokens": ["So", "m\u00fcs\u00b7sen", "selb\u00b7ge", "mir", "recht", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ich f\u00fcrchte weder Pfeil noch Bogen,", "tokens": ["Ich", "f\u00fcrch\u00b7te", "we\u00b7der", "Pfeil", "noch", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie vielmahl hat er sich nicht schon,", "tokens": ["Wie", "viel\u00b7mahl", "hat", "er", "sich", "nicht", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zu nicht geringen Spott und Hohn,", "tokens": ["Zu", "nicht", "ge\u00b7rin\u00b7gen", "Spott", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In seinen Schu\u00df betrogen?", "tokens": ["In", "sei\u00b7nen", "Schu\u00df", "be\u00b7tro\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Mein Hertze liegt zu weit entfernt,", "tokens": ["Mein", "Hert\u00b7ze", "liegt", "zu", "weit", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn er nicht besser schiessen lernt,", "tokens": ["Wenn", "er", "nicht", "bes\u00b7ser", "schies\u00b7sen", "lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So wird er nimmermehr,", "tokens": ["So", "wird", "er", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "H\u00f6rt! wie man 