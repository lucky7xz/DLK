{"dta.poem.21610": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Warum nur mich?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Im goldnen Abendsonnenstrahl", "tokens": ["Im", "gold\u00b7nen", "A\u00b7bend\u00b7son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fccktes Auge \u00fcberall", "tokens": ["Ent\u00b7z\u00fcck\u00b7tes", "Au\u00b7ge", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die heitre Menge froh durchstreift,", "tokens": ["Die", "heit\u00b7re", "Men\u00b7ge", "froh", "durch\u00b7streift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sch\u00f6nem frei zu Sch\u00f6nem schweift.", "tokens": ["Von", "Sch\u00f6\u00b7nem", "frei", "zu", "Sch\u00f6\u00b7nem", "schweift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch Einer immer folgt mein Muth,", "tokens": ["Doch", "Ei\u00b7ner", "im\u00b7mer", "folgt", "mein", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Einer nur dr\u00e4ngt all mein Blut,", "tokens": ["Nach", "Ei\u00b7ner", "nur", "dr\u00e4ngt", "all", "mein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nur Eine missen k\u00f6nnt ich nie:", "tokens": ["Nur", "Ei\u00b7ne", "mis\u00b7sen", "k\u00f6nnt", "ich", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Warum nur sie? Warum nur sie?", "tokens": ["Wa\u00b7rum", "nur", "sie", "?", "Wa\u00b7rum", "nur", "sie", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$.", "PWAV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tritt einher, so herrscherhaft,", "tokens": ["Sie", "tritt", "ein\u00b7her", ",", "so", "herr\u00b7scher\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als w\u00e4r allein sie Geist und Kraft,", "tokens": ["Als", "w\u00e4r", "al\u00b7lein", "sie", "Geist", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein sie Licht, die andern vielen", "tokens": ["Al\u00b7lein", "sie", "Licht", ",", "die", "an\u00b7dern", "vie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "$,", "PRELS", "PIS", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur St\u00e4ubchen, die die Sonn\u2019 umspielen.", "tokens": ["Nur", "St\u00e4ub\u00b7chen", ",", "die", "die", "Sonn'", "um\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und diese Sonne flammt entz\u00fcndet,", "tokens": ["Und", "die\u00b7se", "Son\u00b7ne", "flammt", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Einen nur ihr Lichtstrahl findet;", "tokens": ["Wenn", "Ei\u00b7nen", "nur", "ihr", "Licht\u00b7strahl", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den trifft sie gl\u00fchend innerlich:", "tokens": ["Den", "trifft", "sie", "gl\u00fc\u00b7hend", "in\u00b7ner\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Warum nur mich? Warum nur mich?", "tokens": ["Wa\u00b7rum", "nur", "mich", "?", "Wa\u00b7rum", "nur", "mich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "$.", "PWAV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}