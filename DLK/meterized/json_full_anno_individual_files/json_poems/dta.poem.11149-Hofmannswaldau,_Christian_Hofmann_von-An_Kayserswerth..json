{"dta.poem.11149": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Kayserswerth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje k\u00f6mmts, da\u00df Franckreich dich zu sch\u00fctzen nicht begehrt?", "tokens": ["Wie", "k\u00f6mmts", ",", "da\u00df", "Fran\u00b7ck\u00b7reich", "dich", "zu", "sch\u00fct\u00b7zen", "nicht", "be\u00b7gehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "KOUS", "NE", "PPER", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die trouppen von Burgund sind wohl sehr schlecht", "tokens": ["Die", "troup\u00b7pen", "von", "Bur\u00b7gund", "sind", "wohl", "sehr", "schlecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ge\u00fcbet.", "tokens": ["ge\u00b7\u00fc\u00b7bet", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Das machts, es ist dein platz, wie es der ausgang giebet,", "tokens": ["Das", "machts", ",", "es", "ist", "dein", "platz", ",", "wie", "es", "der", "aus\u00b7gang", "gie\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht eines k\u00f6niges, nur eines kaysers werth.", "tokens": ["Nicht", "ei\u00b7nes", "k\u00f6\u00b7ni\u00b7ges", ",", "nur", "ei\u00b7nes", "kay\u00b7sers", "werth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}