{"dta.poem.8156": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Noch einmal!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die rothe Rose Leidenschaft;", "tokens": ["Die", "ro\u00b7the", "Ro\u00b7se", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einmal hab' ich schw\u00e4rmerisch", "tokens": ["Noch", "ein\u00b7mal", "hab'", "ich", "schw\u00e4r\u00b7me\u00b7risch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In M\u00e4dchenaugen mich vergafft;", "tokens": ["In", "M\u00e4d\u00b7chen\u00b7au\u00b7gen", "mich", "ver\u00b7gafft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal legt ein junges Herz", "tokens": ["Noch", "ein\u00b7mal", "legt", "ein", "jun\u00b7ges", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An meines seinen starken Schlag;", "tokens": ["An", "mei\u00b7nes", "sei\u00b7nen", "star\u00b7ken", "Schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch einmal weht an meine Stirn'", "tokens": ["Noch", "ein\u00b7mal", "weht", "an", "mei\u00b7ne", "Stirn'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein junihei\u00dfer Sommertag.", "tokens": ["Ein", "ju\u00b7ni\u00b7hei\u00b7\u00dfer", "Som\u00b7mer\u00b7tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}