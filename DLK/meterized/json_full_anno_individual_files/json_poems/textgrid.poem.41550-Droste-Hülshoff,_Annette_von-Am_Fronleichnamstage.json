{"textgrid.poem.41550": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Fronleichnamstage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O fasse Mut, er ist dir nah!", "tokens": ["O", "fas\u00b7se", "Mut", ",", "er", "ist", "dir", "nah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast sein Fleisch, sein heilig Blut", "tokens": ["Du", "hast", "sein", "Fleisch", ",", "sein", "hei\u00b7lig", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Genossen ja.", "tokens": ["Ge\u00b7nos\u00b7sen", "ja", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O meine arme Seele, fasse Mut;", "tokens": ["O", "mei\u00b7ne", "ar\u00b7me", "See\u00b7le", ",", "fas\u00b7se", "Mut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er ist ja dein, er ward dein Fleisch und Blut!", "tokens": ["Er", "ist", "ja", "dein", ",", "er", "ward", "dein", "Fleisch", "und", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht, wie ich sollte reich und warm,", "tokens": ["Nicht", ",", "wie", "ich", "soll\u00b7te", "reich", "und", "warm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "VMFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kam freilich ich zu seinem Mahl:", "tokens": ["Kam", "frei\u00b7lich", "ich", "zu", "sei\u00b7nem", "Mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war ein arm", "tokens": ["Ich", "war", "ein", "arm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zerlumpter Gast, doch zitterte die Qual", "tokens": ["Zer\u00b7lump\u00b7ter", "Gast", ",", "doch", "zit\u00b7ter\u00b7te", "die", "Qual"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In mir des Sehnens; Tr\u00e4nen sonder Zahl", "tokens": ["In", "mir", "des", "Seh\u00b7nens", ";", "Tr\u00e4\u00b7nen", "son\u00b7der", "Zahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hab' ich vergossen in der Angst,", "tokens": ["Hab'", "ich", "ver\u00b7gos\u00b7sen", "in", "der", "Angst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die dennoch Freudeschauer war.", "tokens": ["Die", "den\u00b7noch", "Freu\u00b7de\u00b7schau\u00b7er", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich, warum bangst", "tokens": ["Sprich", ",", "wa\u00b7rum", "bangst"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Du vor der Arzenei so s\u00fc\u00df und klar,", "tokens": ["Du", "vor", "der", "Ar\u00b7ze\u00b7nei", "so", "s\u00fc\u00df", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Leben dir und Frieden bietet dar?", "tokens": ["Die", "Le\u00b7ben", "dir", "und", "Frie\u00b7den", "bie\u00b7tet", "dar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl ist es furchtbar, seinen Gott", "tokens": ["Wohl", "ist", "es", "furcht\u00b7bar", ",", "sei\u00b7nen", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einen mit dem s\u00fcnd'gen Leib;", "tokens": ["Zu", "ei\u00b7nen", "mit", "dem", "s\u00fcn\u00b7d'\u00b7gen", "Leib", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Es klingt wie Spott!", "tokens": ["Es", "klingt", "wie", "Spott", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "O Herr, ich bin ein schwach und wirres Weib,", "tokens": ["O", "Herr", ",", "ich", "bin", "ein", "schwach", "und", "wir\u00b7res", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und st\u00e4rker als die Seele ist der Leib!", "tokens": ["Und", "st\u00e4r\u00b7ker", "als", "die", "See\u00b7le", "ist", "der", "Leib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So hab' ich schuldbeladen dir", "tokens": ["So", "hab'", "ich", "schuld\u00b7be\u00b7la\u00b7den", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen S\u00fcnden mich vereint,", "tokens": ["In", "mei\u00b7nen", "S\u00fcn\u00b7den", "mich", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch riefst du mir", "tokens": ["Doch", "riefst", "du", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So laut wie einem, der um Leben weint:", "tokens": ["So", "laut", "wie", "ei\u00b7nem", ",", "der", "um", "Le\u00b7ben", "weint", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So ist es Gnade, was von oben scheint.", "tokens": ["So", "ist", "es", "Gna\u00b7de", ",", "was", "von", "o\u00b7ben", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PRELS", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und hast du des Verstandes Fluch", "tokens": ["Und", "hast", "du", "des", "Ver\u00b7stan\u00b7des", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meiner Pr\u00fcfung mir gestellt:", "tokens": ["Zu", "mei\u00b7ner", "Pr\u00fc\u00b7fung", "mir", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist ein Trug.", "tokens": ["Er", "ist", "ein", "Trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Doch hast du selber ja, du Herr der Welt,", "tokens": ["Doch", "hast", "du", "sel\u00b7ber", "ja", ",", "du", "Herr", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hast selber den Verf\u00fchrer mir gesellt.", "tokens": ["Hast", "sel\u00b7ber", "den", "Ver\u00b7f\u00fch\u00b7rer", "mir", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Drum trau' ich, da\u00df du dessen nicht", "tokens": ["Drum", "trau'", "ich", ",", "da\u00df", "du", "des\u00b7sen", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen wirst an jenem Tag,", "tokens": ["Ver\u00b7ges\u00b7sen", "wirst", "an", "je\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dein Gericht", "tokens": ["Da\u00df", "dein", "Ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mir sprechen wird: \u00bbDen Irren seh ich nach;", "tokens": ["Mir", "spre\u00b7chen", "wird", ":", "\u00bb", "Den", "Ir\u00b7ren", "seh", "ich", "nach", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "$.", "$(", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Herz war willig, nur der Kopf war schwach!\u00ab", "tokens": ["Das", "Herz", "war", "wil\u00b7lig", ",", "nur", "der", "Kopf", "war", "schwach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "ART", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O fasse Mut, er ist dir nah!", "tokens": ["O", "fas\u00b7se", "Mut", ",", "er", "ist", "dir", "nah", "!"], "token_info": ["word", "word", "word", "punct", "word", "word"