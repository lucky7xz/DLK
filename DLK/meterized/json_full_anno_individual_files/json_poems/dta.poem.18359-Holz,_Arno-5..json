{"dta.poem.18359": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Sich zw\u00f6lfmal um sich selbst gedreht,", "tokens": ["Sich", "zw\u00f6lf\u00b7mal", "um", "sich", "selbst", "ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder f\u00fchlst du deine Wunde", "tokens": ["Und", "wie\u00b7der", "f\u00fchlst", "du", "dei\u00b7ne", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00e4chzst und st\u00f6hnst wie Philoktet!", "tokens": ["Und", "\u00e4chzst", "und", "st\u00f6hnst", "wie", "Phi\u00b7lok\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "KOKOM", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Denn dir, auch dir rollt's durch die Adern", "tokens": ["Denn", "dir", ",", "auch", "dir", "rollt's", "durch", "die", "A\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und durchs Gehirn wie hei\u00dfes Blei;", "tokens": ["Und", "durchs", "Ge\u00b7hirn", "wie", "hei\u00b7\u00dfes", "Blei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gigantisch th\u00fcrmst du deine Quadern,", "tokens": ["Gi\u00b7gan\u00b7tisch", "th\u00fcrmst", "du", "dei\u00b7ne", "Qua\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Gott im Himmel willst du hadern", "tokens": ["Mit", "Gott", "im", "Him\u00b7mel", "willst", "du", "ha\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und deine Seele ringt im Schrei!", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "ringt", "im", "Schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Herz steht wie die Welt in Bl\u00fcthe,", "tokens": ["Dein", "Herz", "steht", "wie", "die", "Welt", "in", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geh\u00fcllt in silbergrauen Dunst,", "tokens": ["Ge\u00b7h\u00fcllt", "in", "sil\u00b7ber\u00b7grau\u00b7en", "Dunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und m\u00e4chtig f\u00fchlst du's im Gem\u00fcthe:", "tokens": ["Und", "m\u00e4ch\u00b7tig", "f\u00fchlst", "du's", "im", "Ge\u00b7m\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist ein Priester deiner Kunst!", "tokens": ["Du", "bist", "ein", "Pries\u00b7ter", "dei\u00b7ner", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lebens goldne Kronen winken,", "tokens": ["Des", "Le\u00b7bens", "gold\u00b7ne", "Kro\u00b7nen", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Rosen stehen wei\u00df und roth;", "tokens": ["Die", "Ro\u00b7sen", "ste\u00b7hen", "wei\u00df", "und", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du f\u00fchlst sie duften, siehst sie blinken,", "tokens": ["Du", "f\u00fchlst", "sie", "duf\u00b7ten", ",", "siehst", "sie", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Doch scheu mu\u00dft du vor\u00fcberhinken,", "tokens": ["Doch", "scheu", "mu\u00dft", "du", "vor\u00b7\u00fc\u00b7berh\u00b7in\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Denn ach, dir fehlt dein t\u00e4glich Brod!", "tokens": ["Denn", "ach", ",", "dir", "fehlt", "dein", "t\u00e4g\u00b7lich", "Brod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Beneidenswerth in Forst und Fluren", "tokens": ["Be\u00b7nei\u00b7dens\u00b7werth", "in", "Forst", "und", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schwein um seine Eichelmast!", "tokens": ["Das", "Schwein", "um", "sei\u00b7ne", "Ei\u00b7chel\u00b7mast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die \u00e4rmste aller Kreaturen", "tokens": ["Die", "\u00e4rms\u00b7te", "al\u00b7ler", "Kre\u00b7a\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist doch ein dichtender Phantast!", "tokens": ["Ist", "doch", "ein", "dich\u00b7ten\u00b7der", "Phan\u00b7tast", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Bettler dort an seiner Kr\u00fccke,", "tokens": ["Der", "Bett\u00b7ler", "dort", "an", "sei\u00b7ner", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist nicht halb so arm wie du...", "tokens": ["Er", "ist", "nicht", "halb", "so", "arm", "wie", "du", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "KOKOM", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir brach dein Himmel w\u00fcst in St\u00fccke,", "tokens": ["Dir", "brach", "dein", "Him\u00b7mel", "w\u00fcst", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Er aber tr\u00e4umt von seinem Gl\u00fccke \u2014", "tokens": ["Er", "a\u00b7ber", "tr\u00e4umt", "von", "sei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "O Gott, nur zu, nur immer zu!", "tokens": ["O", "Gott", ",", "nur", "zu", ",", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PTKVZ", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Licht, das mir ins Hirn gelodert,", "tokens": ["Du", "Licht", ",", "das", "mir", "ins", "Hirn", "ge\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu die alte Litanei?", "tokens": ["Wo\u00b7zu", "die", "al\u00b7te", "Li\u00b7ta\u00b7nei", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist doch so viel hier schon vermodert,", "tokens": ["Ist", "doch", "so", "viel", "hier", "schon", "ver\u00b7mo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O, w\u00e4rst auch du, auch du vorbei!", "tokens": ["O", ",", "w\u00e4rst", "auch", "du", ",", "auch", "du", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ADV", "PPER", "$,", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann w\u00e4r der alte, blinde L\u00e4rmer", "tokens": ["Dann", "w\u00e4r", "der", "al\u00b7te", ",", "blin\u00b7de", "L\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein dunkelbraunes Kl\u00fcmpchen Lehm;", "tokens": ["Ein", "dun\u00b7kel\u00b7brau\u00b7nes", "Kl\u00fcmp\u00b7chen", "Lehm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann w\u00e4r die Welt um einen Schw\u00e4rmer,", "tokens": ["Dann", "w\u00e4r", "die", "Welt", "um", "ei\u00b7nen", "Schw\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Um einen Hirnverr\u00fcckten \u00e4rmer", "tokens": ["Um", "ei\u00b7nen", "Hirn\u00b7ver\u00b7r\u00fcck\u00b7ten", "\u00e4r\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und rollte weiter, wie vordem!", "tokens": ["Und", "roll\u00b7te", "wei\u00b7ter", ",", "wie", "vor\u00b7dem", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}