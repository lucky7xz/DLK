{"dta.poem.2808": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verschweig ein Gl\u00fcck, verbirg ein Ungl\u00fcck, das du hast!", "tokens": ["Ver\u00b7schweig", "ein", "Gl\u00fcck", ",", "ver\u00b7birg", "ein", "Un\u00b7gl\u00fcck", ",", "das", "du", "hast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Gl\u00fcck und Ungl\u00fcck sind die Menschen nur zur Last.", "tokens": ["Im", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "sind", "die", "Men\u00b7schen", "nur", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Noch schlimmer als im Gl\u00fcck der gift'ge Blick des Neiders,", "tokens": ["Noch", "schlim\u00b7mer", "als", "im", "Gl\u00fcck", "der", "gift'\u00b7ge", "Blick", "des", "Nei\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Ungl\u00fcck ist das Wort das frost'ge des Mitleiders.", "tokens": ["Im", "Un\u00b7gl\u00fcck", "ist", "das", "Wort", "das", "frost'\u00b7ge", "des", "Mit\u00b7lei\u00b7ders", "."], "token_info": ["word", "word", "word", "word", "wo