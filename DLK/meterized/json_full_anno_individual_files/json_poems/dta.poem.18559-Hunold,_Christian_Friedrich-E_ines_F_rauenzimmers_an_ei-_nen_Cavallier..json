{"dta.poem.18559": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E ines  F rauenzimmers an ei-  \n nen Cavallier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Schan Thr\u00e4nen/ Dint und Blut in dieser Schrift vermengt/", "tokens": ["Schan", "Thr\u00e4\u00b7nen", "/", "Dint", "und", "Blut", "in", "die\u00b7ser", "Schrift", "ver\u00b7mengt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NN", "KON", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mon c&#339;ur&#8250; ich schreibe dir mit mehr als tausend plagen/", "tokens": ["Mon", "c", "&#339;", "ur", "&#8250;", "ich", "schrei\u00b7be", "dir", "mit", "mehr", "als", "tau\u00b7send", "pla\u00b7gen", "/"], "token_info": ["word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$(", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "KOKOM", "CARD", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Mein trauren hat mich fast zur finstern Grufft gesenckt", "tokens": ["Mein", "trau\u00b7ren", "hat", "mich", "fast", "zur", "fins\u00b7tern", "Grufft", "ge\u00b7senckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ach! Du zwingest mich zu lauter Schmertzens klagen.", "tokens": ["Und", "ach", "!", "Du", "zwin\u00b7gest", "mich", "zu", "lau\u00b7ter", "Schmert\u00b7zens", "kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Hencker meiner Ruh' ist nur dein Unbestand/", "tokens": ["Der", "Hen\u00b7cker", "mei\u00b7ner", "Ruh'", "ist", "nur", "dein", "Un\u00b7be\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Ursprung meiner Qvaal r\u00fchrt von getreuer Liebe.", "tokens": ["Der", "Ur\u00b7sprung", "mei\u00b7ner", "Qvaal", "r\u00fchrt", "von", "ge\u00b7treu\u00b7er", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Hertz verbande sich zu meinen Unterpfand/", "tokens": ["Dein", "Hertz", "ver\u00b7ban\u00b7de", "sich", "zu", "mei\u00b7nen", "Un\u00b7ter\u00b7pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach das es solches auch in Ewigkeit verbliebe!", "tokens": ["Ach", "das", "es", "sol\u00b7ches", "auch", "in", "E\u00b7wig\u00b7keit", "ver\u00b7blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PRELS", "PPER", "PIS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du weist wohl/ welche Kunst mich in dein Netz gef\u00fchrt/", "tokens": ["Du", "weist", "wohl", "/", "wel\u00b7che", "Kunst", "mich", "in", "dein", "Netz", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie deine Schmeicheley mir pflegte liebzukosen:", "tokens": ["Wie", "dei\u00b7ne", "Schmei\u00b7che\u00b7ley", "mir", "pfleg\u00b7te", "lieb\u00b7zu\u00b7ko\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wie scheinbares Gold dein gantzes thun geziert.", "tokens": ["Und", "wie", "schein\u00b7ba\u00b7res", "Gold", "dein", "gant\u00b7zes", "thun", "ge\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJA", "NN", "PPOSAT", "ADJA", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Hoffnung zeigte mir die angenehmsten Rosen.", "tokens": ["Die", "Hoff\u00b7nung", "zeig\u00b7te", "mir", "die", "an\u00b7ge\u00b7nehms\u00b7ten", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Treue gr\u00fcndete sich auff den theursten Eyd;", "tokens": ["Die", "Treu\u00b7e", "gr\u00fcn\u00b7de\u00b7te", "sich", "auff", "den", "theurs\u00b7ten", "Eyd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Geister solten dich in tausend St\u00fccken reisen/", "tokens": ["Die", "Geis\u00b7ter", "sol\u00b7ten", "dich", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "rei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bekr\u00f6nete dich nicht Treu und Best\u00e4ndigkeit/", "tokens": ["Be\u00b7kr\u00f6\u00b7ne\u00b7te", "dich", "nicht", "Treu", "und", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "So solte man dich nicht bey deinen Nahmen heissen.", "tokens": ["So", "sol\u00b7te", "man", "dich", "nicht", "bey", "dei\u00b7nen", "Nah\u00b7men", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Worauf ein Cavallier doch sonsten sehr viel h\u00e4lt", "tokens": ["Wo\u00b7rauf", "ein", "Ca\u00b7val\u00b7lier", "doch", "sons\u00b7ten", "sehr", "viel", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NE", "ADV", "ADV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das schl\u00e4gst du nun in Wind - - Jedoch vergib den Zeilen/", "tokens": ["Das", "schl\u00e4gst", "du", "nun", "in", "Wind", "Je\u00b7doch", "ver\u00b7gib", "den", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$(", "$(", "ADV", "VVIMP", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Wenn Wemuth Schmertz und Grimm sie zu Papiere stellt/", "tokens": ["Wenn", "We\u00b7muth", "Schmertz", "und", "Grimm", "sie", "zu", "Pa\u00b7pie\u00b7re", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NE", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn solche Wunden sind nicht durch Gedult zu heilen/", "tokens": ["Denn", "sol\u00b7che", "Wun\u00b7den", "sind", "nicht", "durch", "Ge\u00b7dult", "zu", "hei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vielleicht da\u00df \u00fcm mein Grab gar bald Cypressen steh'n:", "tokens": ["Viel\u00b7leicht", "da\u00df", "\u00fcm", "mein", "Grab", "gar", "bald", "Cyp\u00b7res\u00b7sen", "steh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NE", "PPOSAT", "NN", "ADV", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ach sollen die der Lust versprochne Myrthen heissen!", "tokens": ["Ach", "sol\u00b7len", "die", "der", "Lust", "ver\u00b7sproch\u00b7ne", "Myr\u00b7then", "heis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "ART", "ART", "NN", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Vielleicht mu\u00df ich den Tod in meinen Armen seh'n:", "tokens": ["Viel\u00b7leicht", "mu\u00df", "ich", "den", "Tod", "in", "mei\u00b7nen", "Ar\u00b7men", "seh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Soll mich mein Br\u00e4utigam mit fauler Erde speisen?", "tokens": ["Soll", "mich", "mein", "Br\u00e4u\u00b7ti\u00b7gam", "mit", "fau\u00b7ler", "Er\u00b7de", "spei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ist meiner Haare Schmuck vor Perlen Staub und Grau\u00df?", "tokens": ["Ist", "mei\u00b7ner", "Haa\u00b7re", "Schmuck", "vor", "Per\u00b7len", "Staub", "und", "Grau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Scheint mir mein Sonnen-Licht in Jammer-vollen Schatten?", "tokens": ["Scheint", "mir", "mein", "Son\u00b7nen\u00b7Licht", "in", "Jam\u00b7mer\u00b7vol\u00b7len", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist Hymens Freuden-Schlo\u00df ein D\u00fcstres W\u00fcrmer-", "tokens": ["Ist", "Hy\u00b7mens", "Freu\u00b7den\u00b7Schlo\u00df", "ein", "D\u00fcst\u00b7res", "W\u00fcr\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Hau\u00df/", "tokens": ["Hau\u00df", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.29": {"text": "Wo Schlangen sich vor dir/ mit meinen Lippen-Gatten?", "tokens": ["Wo", "Schlan\u00b7gen", "sich", "vor", "dir", "/", "mit", "mei\u00b7nen", "Lip\u00b7pen\u00b7Gat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "PPER", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ach! heisset Angst und Schmertz die s\u00fcsse Manna-Kost?", "tokens": ["Ach", "!", "heis\u00b7set", "Angst", "und", "Schmertz", "die", "s\u00fcs\u00b7se", "Man\u00b7na\u00b7Kost", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Soll meiner Thr\u00e4nen-Saltz mir Hochzeit-Nectar messen?", "tokens": ["Soll", "mei\u00b7ner", "Thr\u00e4\u00b7nen\u00b7Saltz", "mir", "Hoch\u00b7zeit\u00b7\u00b7Nec\u00b7tar", "mes\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zerfliesset nun mein Blut in Muscateller-Most?", "tokens": ["Zer\u00b7flies\u00b7set", "nun", "mein", "Blut", "in", "Mus\u00b7ca\u00b7tel\u00b7ler\u00b7Most", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Ist Amors Taffel-Lust/ sein eignes Hertze fressen?", "tokens": ["Ist", "A\u00b7mors", "Taf\u00b7fel\u00b7Lust", "/", "sein", "eig\u00b7nes", "Hert\u00b7ze", "fres\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$(", "PPOSAT", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ach siehe Grausamer! wie mich der herbe Schmertz/", "tokens": ["Ach", "sie\u00b7he", "Grau\u00b7sa\u00b7mer", "!", "wie", "mich", "der", "her\u00b7be", "Schmertz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "NN", "$.", "PWAV", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Zur \u00e4rgsten Folter-Banck und tausend Martern f\u00fchret!", "tokens": ["Zur", "\u00e4rgs\u00b7ten", "Fol\u00b7ter\u00b7Banck", "und", "tau\u00b7send", "Mar\u00b7tern", "f\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach schau' in Blute schwimmt dein vorgeliebtes Hertz!", "tokens": ["Ach", "schau'", "in", "Blu\u00b7te", "schwimmt", "dein", "vor\u00b7ge\u00b7lieb\u00b7tes", "Hertz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie da\u00df die Wehmuth dir nicht deine Seele r\u00fchret!", "tokens": ["Wie", "da\u00df", "die", "Weh\u00b7muth", "dir", "nicht", "dei\u00b7ne", "See\u00b7le", "r\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ist deine Brust noch mehr als harter Felsen Art/", "tokens": ["Ist", "dei\u00b7ne", "Brust", "noch", "mehr", "als", "har\u00b7ter", "Fel\u00b7sen", "Art", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "KOKOM", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df meine Thr\u00e4nen-Flut sie gar nicht kan erweichen?", "tokens": ["Da\u00df", "mei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7Flut", "sie", "gar", "nicht", "kan", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Es l\u00e4sst kein Tygerth\u00fcr/ kein wilder Leopart", "tokens": ["Es", "l\u00e4sst", "kein", "Ty\u00b7gert\u00b7h\u00fcr", "/", "kein", "wil\u00b7der", "Le\u00b7o\u00b7part"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was ihm ", "tokens": ["Was", "ihm"], "token_info": ["word", "word"], "pos": ["PWS", "PPER"], "meter": "--", "measure": "unknown.measure.zero"}, "line.42": {"text": "Ich flehe dich/ mein Schatz/ \u00fcm jene Stunden an/", "tokens": ["Ich", "fle\u00b7he", "dich", "/", "mein", "Schatz", "/", "\u00fcm", "je\u00b7ne", "Stun\u00b7den", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(",