{"textgrid.poem.56007": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Das Gold", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denk es w\u00e4re nicht: es h\u00e4tte m\u00fcssen", "tokens": ["Denk", "es", "w\u00e4\u00b7re", "nicht", ":", "es", "h\u00e4t\u00b7te", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VAFIN", "PTKNEG", "$.", "PPER", "VAFIN", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "endlich in den Bergen sich geb\u00e4ren", "tokens": ["end\u00b7lich", "in", "den", "Ber\u00b7gen", "sich", "ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und sich niederschlagen in den Fl\u00fcssen", "tokens": ["und", "sich", "nie\u00b7der\u00b7schla\u00b7gen", "in", "den", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aus dem Wollen, aus dem G\u00e4ren", "tokens": ["aus", "dem", "Wol\u00b7len", ",", "aus", "dem", "G\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "ihres Willens; aus der Zwang-Idee,", "tokens": ["ih\u00b7res", "Wil\u00b7lens", ";", "aus", "der", "Zwang\u00b7I\u00b7dee", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df ein Erz ist \u00fcber allen Erzen.", "tokens": ["da\u00df", "ein", "Erz", "ist", "\u00fc\u00b7ber", "al\u00b7len", "Er\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weithin warfen sie aus ihren Herzen", "tokens": ["Weit\u00b7hin", "war\u00b7fen", "sie", "aus", "ih\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "immer wieder Mero\u00eb", "tokens": ["im\u00b7mer", "wie\u00b7der", "Me\u00b7ro\u00eb"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "an den Rand der Lande, in den \u00c4ther,", "tokens": ["an", "den", "Rand", "der", "Lan\u00b7de", ",", "in", "den", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcber das Erfahrene hinaus;", "tokens": ["\u00fc\u00b7ber", "das", "Er\u00b7fah\u00b7re\u00b7ne", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "und die S\u00f6hne brachten manchmal sp\u00e4ter", "tokens": ["und", "die", "S\u00f6h\u00b7ne", "brach\u00b7ten", "manch\u00b7mal", "sp\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das Verhei