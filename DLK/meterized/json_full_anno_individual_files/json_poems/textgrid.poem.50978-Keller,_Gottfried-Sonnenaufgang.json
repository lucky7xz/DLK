{"textgrid.poem.50978": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenaufgang", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahre herauf, du kristallener Wagen,", "tokens": ["Fah\u00b7re", "her\u00b7auf", ",", "du", "kris\u00b7tal\u00b7le\u00b7ner", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Klingender Morgen, so frisch und so klar!", "tokens": ["Klin\u00b7gen\u00b7der", "Mor\u00b7gen", ",", "so", "frisch", "und", "so", "klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Seidene Wimpel, vom Oste getragen,", "tokens": ["Sei\u00b7de\u00b7ne", "Wim\u00b7pel", ",", "vom", "Os\u00b7te", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Flattre, du rosige W\u00f6lkleinschar!", "tokens": ["Flatt\u00b7re", ",", "du", "ro\u00b7si\u00b7ge", "W\u00f6lk\u00b7lein\u00b7schar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe die Meere, sie wogen und branden,", "tokens": ["Sie\u00b7he", "die", "Mee\u00b7re", ",", "sie", "wo\u00b7gen", "und", "bran\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber still das Gebirge steht,", "tokens": ["A\u00b7ber", "still", "das", "Ge\u00b7bir\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Tau ist gesprengt auf den funkelnden Landen,", "tokens": ["Tau", "ist", "ge\u00b7sprengt", "auf", "den", "fun\u00b7keln\u00b7den", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Weihbrunn zum heiligen Sonnengebet.", "tokens": ["Weih\u00b7brunn", "zum", "hei\u00b7li\u00b7gen", "Son\u00b7nen\u00b7ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Tausendfach wollen die Blumen entriegeln", "tokens": ["Tau\u00b7send\u00b7fach", "wol\u00b7len", "die", "Blu\u00b7men", "ent\u00b7rie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Aus ihrer Brust den gefangenen Gott;", "tokens": ["Aus", "ih\u00b7rer", "Brust", "den", "ge\u00b7fan\u00b7ge\u00b7nen", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch die vergoldeten Kreuze bespiegeln", "tokens": ["Doch", "die", "ver\u00b7gol\u00b7de\u00b7ten", "Kreu\u00b7ze", "be\u00b7spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich auf den Domen mit glei\u00dfendem Spott.", "tokens": ["Sich", "auf", "den", "Do\u00b7men", "mit", "glei\u00b7\u00dfen\u00b7dem", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Singen nicht Lerchen dort hoch in den L\u00fcften,", "tokens": ["Sin\u00b7gen", "nicht", "Ler\u00b7chen", "dort", "hoch", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwenkend in freiem und fr\u00f6hlichem Zug?", "tokens": ["Schwen\u00b7kend", "in", "frei\u00b7em", "und", "fr\u00f6h\u00b7li\u00b7chem", "Zug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nein, aber aufw\u00e4rtsgeschwungen aus Kl\u00fcften,", "tokens": ["Nein", ",", "a\u00b7ber", "auf\u00b7w\u00e4rts\u00b7ge\u00b7schwun\u00b7gen", "aus", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonnt sich ein kreischender Rabenflug.", "tokens": ["Sonnt", "sich", "ein", "krei\u00b7schen\u00b7der", "Ra\u00b7ben\u00b7flug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Springt nicht ein Fischlein aus silberner Welle,", "tokens": ["Springt", "nicht", "ein", "Fisc\u00b7hlein", "aus", "sil\u00b7ber\u00b7ner", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sich am lieblichen Lichte erfreut?", "tokens": ["Das", "sich", "am", "lieb\u00b7li\u00b7chen", "Lich\u00b7te", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja, 's ist der Hecht, der bewehrte Geselle,", "tokens": ["Ja", ",", "'s", "ist", "der", "Hecht", ",", "der", "be\u00b7wehr\u00b7te", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der den allt\u00e4glichen Raub erneut.", "tokens": ["Der", "den", "all\u00b7t\u00e4g\u00b7li\u00b7chen", "Raub", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Fahre hin\u00fcber auf drehenden Speichen,", "tokens": ["Fah\u00b7re", "hin\u00b7\u00fc\u00b7ber", "auf", "dre\u00b7hen\u00b7den", "Spei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schimmernder Morgen, noch ist es nicht Zeit;", "tokens": ["Schim\u00b7mern\u00b7der", "Mor\u00b7gen", ",", "noch", "ist", "es", "nicht", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Rosige Wimpel, auch ihr m\u00f6gt erbleichen \u2013", "tokens": ["Ro\u00b7si\u00b7ge", "Wim\u00b7pel", ",", "auch", "ihr", "m\u00f6gt", "er\u00b7blei\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "PPER", "VMFIN", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Weh mir, schon weht ihr so bla\u00df und so weit!", "tokens": ["Weh", "mir", ",", "schon", "weht", "ihr", "so", "bla\u00df", "und", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Fahr! Ein Josua tr\u00e4umet auf Erden,", "tokens": ["Fahr", "!", "Ein", "Jo\u00b7sua", "tr\u00e4u\u00b7met", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dem es schon ahnend in Ohren erklingt;", "tokens": ["Dem", "es", "schon", "ah\u00b7nend", "in", "Oh\u00b7ren", "er\u00b7klingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auf springt er einst, in die Z\u00fcgel den Pferden,", "tokens": ["Auf", "springt", "er", "einst", ",", "in", "die", "Z\u00fc\u00b7gel", "den", "Pfer\u00b7den"