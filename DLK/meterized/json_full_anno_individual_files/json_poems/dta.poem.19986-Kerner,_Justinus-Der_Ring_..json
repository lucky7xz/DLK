{"dta.poem.19986": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Ring .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein fremder Kavalier", "tokens": ["Ein", "frem\u00b7der", "Ka\u00b7va\u00b7lier"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Stieg ab vom schwarzen Ro\u00df,", "tokens": ["Stieg", "ab", "vom", "schwar\u00b7zen", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trat in den K\u00f6nigssaal,", "tokens": ["Trat", "in", "den", "K\u00f6\u00b7nigs\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit andern Herren gro\u00df.", "tokens": ["Mit", "an\u00b7dern", "Her\u00b7ren", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Derselbe Kavalier", "tokens": ["Der\u00b7sel\u00b7be", "Ka\u00b7va\u00b7lier"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Trug einen Edelstein,", "tokens": ["Trug", "ei\u00b7nen", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie man noch keinen sah,", "tokens": ["Wie", "man", "noch", "kei\u00b7nen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von wundersamen Schein.", "tokens": ["Von", "wun\u00b7der\u00b7sa\u00b7men", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Stein von hohem Werth", "tokens": ["Ein", "Stein", "von", "ho\u00b7hem", "Werth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In K\u00f6nigs Krone sa\u00df,", "tokens": ["In", "K\u00f6\u00b7nigs", "Kro\u00b7ne", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schien vor diesem er", "tokens": ["Doch", "schien", "vor", "die\u00b7sem", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein mattgeschliffen Glas.", "tokens": ["Ein", "matt\u00b7ge\u00b7schlif\u00b7fen", "Glas", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig bot ihm Gold,", "tokens": ["Der", "K\u00f6\u00b7nig", "bot", "ihm", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er bot ihm Leut und Land,", "tokens": ["Er", "bot", "ihm", "Leut", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch lassen wollt er nicht", "tokens": ["Doch", "las\u00b7sen", "wollt", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den edlen Diamant.", "tokens": ["Den", "ed\u00b7len", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig de\u00df' erbost,", "tokens": ["Der", "K\u00f6\u00b7nig", "de\u00df'", "er\u00b7bost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Spricht zu dem Hauptmann sein:", "tokens": ["Spricht", "zu", "dem", "Haupt\u00b7mann", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bringt mir des Mannes Hand", "tokens": ["Bringt", "mir", "des", "Man\u00b7nes", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Samt seinem Edelstein.", "tokens": ["Samt", "sei\u00b7nem", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Hauptmann reckt das Schwert,", "tokens": ["Der", "Haupt\u00b7mann", "reckt", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Haut nach des Mannes Hand,", "tokens": ["Haut", "nach", "des", "Man\u00b7nes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Doch statt dem Kavalier", "tokens": ["Doch", "statt", "dem", "Ka\u00b7va\u00b7lier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Teufel vor ihm stand.", "tokens": ["Der", "Teu\u00b7fel", "vor", "ihm", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gluth str\u00f6mt aus seinem Ring,", "tokens": ["Gluth", "str\u00f6mt", "aus", "sei\u00b7nem", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zur H\u00f6lle w\u00e4chst der Stein,", "tokens": ["Zur", "H\u00f6l\u00b7le", "w\u00e4chst", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schleu\u00dft Schlo\u00df und K\u00f6nig bald", "tokens": ["Schleu\u00dft", "Schlo\u00df", "und", "K\u00f6\u00b7nig", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Samt allen Dienern ein.", "tokens": ["Samt", "al\u00b7len", "Die\u00b7nern", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}