{"dta.poem.19599": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Jung Tyrrel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejung Tyrrel, fuhrest \u00fcber See?", "tokens": ["\u201e", "jung", "Tyr\u00b7rel", ",", "fuh\u00b7rest", "\u00fc\u00b7ber", "See", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jung Tyrrel, mir willkommen hie!", "tokens": ["Jung", "Tyr\u00b7rel", ",", "mir", "will\u00b7kom\u00b7men", "hie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sahst du so dunkle Forste je?", "tokens": ["Sahst", "du", "so", "dunk\u00b7le", "Fors\u00b7te", "je", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So stolze Forste sahst du nie!", "tokens": ["So", "stol\u00b7ze", "Fors\u00b7te", "sahst", "du", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein englisch Wild erst umgebracht!", "tokens": ["Ein", "eng\u00b7lisch", "Wild", "erst", "um\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann geb' ich dir ein englisch Lehn!\u201c", "tokens": ["Dann", "geb'", "ich", "dir", "ein", "eng\u00b7lisch", "Lehn", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jung Tyrrel, dem das Herze lacht,", "tokens": ["Jung", "Tyr\u00b7rel", ",", "dem", "das", "Her\u00b7ze", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft seine blanken Z\u00e4hne sehn.", "tokens": ["L\u00e4\u00dft", "sei\u00b7ne", "blan\u00b7ken", "Z\u00e4h\u00b7ne", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ewer heut den besten Schu\u00df mir thut,", "tokens": ["\u201e", "wer", "heut", "den", "bes\u00b7ten", "Schu\u00df", "mir", "thut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Achtzehnender mir erlegt,", "tokens": ["Den", "Acht\u00b7ze\u00b7hnen\u00b7der", "mir", "er\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nehme sich als Lehensgut", "tokens": ["Der", "neh\u00b7me", "sich", "als", "Le\u00b7hens\u00b7gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den K\u00f6nigsforst der ihn gehegt!", "tokens": ["Den", "K\u00f6\u00b7nigs\u00b7forst", "der", "ihn", "ge\u00b7hegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zu schw\u00f6r' ich dir's auf diesen Bart,", "tokens": ["Zu", "schw\u00f6r'", "ich", "dir's", "auf", "die\u00b7sen", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der feuerroth die Brust mir deckt!", "tokens": ["Der", "feu\u00b7er\u00b7roth", "die", "Brust", "mir", "deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Wald! Zu Wald! Der Rappe scharrt!", "tokens": ["Zu", "Wald", "!", "Zu", "Wald", "!", "Der", "Rap\u00b7pe", "scharrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bracke sp\u00fcrt! Der R\u00fcde bleckt!\u201c", "tokens": ["Die", "Bra\u00b7cke", "sp\u00fcrt", "!", "Der", "R\u00fc\u00b7de", "bleckt", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr Wilhelm st\u00f6\u00dft ins J\u00e4gerhorn,", "tokens": ["Herr", "Wil\u00b7helm", "st\u00f6\u00dft", "ins", "J\u00e4\u00b7ger\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Geier kr\u00e4chzt in seinem Horst,", "tokens": ["Ein", "Gei\u00b7er", "kr\u00e4chzt", "in", "sei\u00b7nem", "Horst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wipfel peitscht ein dunkler Zorn,", "tokens": ["Die", "Wip\u00b7fel", "peitscht", "ein", "dunk\u00b7ler", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es braust und tost. Dann schweigt der Forst.", "tokens": ["Es", "braust", "und", "tost", ".", "Dann", "schweigt", "der", "Forst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr Wilhelm schl\u00e4gt mit Tyrrel Rath:", "tokens": ["Herr", "Wil\u00b7helm", "schl\u00e4gt", "mit", "Tyr\u00b7rel", "Rath", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich links, du rechts! Fort! Gute Birsch!\u201c", "tokens": ["\u201e", "ich", "links", ",", "du", "rechts", "!", "Fort", "!", "Gu\u00b7te", "Birsch", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "$,", "PPER", "ADV", "$.", "NN", "$.", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es knirscht das Laub darauf er trat.", "tokens": ["Es", "knirscht", "das", "Laub", "da\u00b7rauf", "er", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In heller Lichtung \u00e4tzt ein Hirsch:", "tokens": ["In", "hel\u00b7ler", "Lich\u00b7tung", "\u00e4tzt", "ein", "Hirsch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Rothhirsch der vier Ellen mi\u00dft,", "tokens": ["Ein", "Roth\u00b7hirsch", "der", "vier", "El\u00b7len", "mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich ein J\u00e4gerherze freut,", "tokens": ["Da\u00df", "sich", "ein", "J\u00e4\u00b7ger\u00b7her\u00b7ze", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der dieses Forstes K\u00f6nig ist,", "tokens": ["Der", "die\u00b7ses", "Fors\u00b7tes", "K\u00f6\u00b7nig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit weit ver\u00e4stetem Gest\u00e4ud.", "tokens": ["Mit", "weit", "ver\u00b7\u00e4s\u00b7te\u00b7tem", "Ge\u00b7st\u00e4ud", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Her raunt's aus Waldesfinsterni\u00df", "tokens": ["Her", "raunt's", "aus", "Wal\u00b7des\u00b7fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Tyrrel, der sich duckt ins Moos:", "tokens": ["Zu", "Tyr\u00b7rel", ",", "der", "sich", "duckt", "ins", "Moos", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201everdammt da\u00df mir die Sehne ri\u00df!", "tokens": ["\u201e", "ver\u00b7dammt", "da\u00df", "mir", "die", "Seh\u00b7ne", "ri\u00df", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dr\u00fcck in Teufels Namen los!\u201c", "tokens": ["Du", "dr\u00fcck", "in", "Teu\u00b7fels", "Na\u00b7men", "los", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Herr Tyrrel lauscht. \u201eWer sprach das Wort?\u201c", "tokens": ["Herr", "Tyr\u00b7rel", "lauscht", ".", "\u201e", "Wer", "sprach", "das", "Wort", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "$(", "PWS", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Weilchen schweigt's im Laubesdach.", "tokens": ["Ein", "Weil\u00b7chen", "schweigt's", "im", "Lau\u00b7bes\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eschie\u00df, Tyrrel!\u201c raunt's von anderm Ort.", "tokens": ["\u201e", "schie\u00df", ",", "Tyr\u00b7rel", "!", "\u201c", "raunt's", "von", "an\u00b7derm", "Ort", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NE", "$.", "$(", "VVFIN", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er schie\u00dft. Gen\u00fcber st\u00f6hnt ein Ach.", "tokens": ["Er", "schie\u00dft", ".", "Ge\u00b7n\u00fc\u00b7ber", "st\u00f6hnt", "ein", "Ach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Herr Tyrrel, das war schlimme Birsch!", "tokens": ["Herr", "Tyr\u00b7rel", ",", "das", "war", "schlim\u00b7me", "Birsch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Dickicht rinnt ein B\u00e4chlein rot.", "tokens": ["Im", "Di\u00b7ckicht", "rinnt", "ein", "B\u00e4ch\u00b7lein", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr fehltet Englands gr\u00f6\u00dften Hirsch", "tokens": ["Ihr", "fehl\u00b7tet", "En\u00b7glands", "gr\u00f6\u00df\u00b7ten", "Hirsch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schosset Englands K\u00f6nig todt.", "tokens": ["Und", "schos\u00b7set", "En\u00b7glands", "K\u00f6\u00b7nig", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}