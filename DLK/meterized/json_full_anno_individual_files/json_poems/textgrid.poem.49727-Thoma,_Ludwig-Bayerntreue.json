], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht die Luft von hinten aus,", "tokens": ["Geht", "die", "Luft", "von", "hin\u00b7ten", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ums heilig Tabernakel", "tokens": ["Und", "ums", "hei\u00b7lig", "Ta\u00b7ber\u00b7na\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scharen sich die Zentrumslackel.", "tokens": ["Scha\u00b7ren", "sich", "die", "Zent\u00b7rums\u00b7la\u00b7ckel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wei\u00df und Blau wird \u00fcberkleckt", "tokens": ["Wei\u00df", "und", "Blau", "wird", "\u00fc\u00b7ber\u00b7kleckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Schwarz ganz zugedeckt;", "tokens": ["Und", "mit", "Schwarz", "ganz", "zu\u00b7ge\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz will man den Prinzen lieben,", "tokens": ["Schwarz", "will", "man", "den", "Prin\u00b7zen", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonst wird Haberfeld betrieben.", "tokens": ["Sonst", "wird", "Ha\u00b7ber\u00b7feld", "be\u00b7tri