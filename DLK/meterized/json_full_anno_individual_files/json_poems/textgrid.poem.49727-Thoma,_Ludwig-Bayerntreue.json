{"textgrid.poem.49727": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Bayerntreue", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neulich sprach Prinz Ludewig:", "tokens": ["Neu\u00b7lich", "sprach", "Prinz", "Lu\u00b7de\u00b7wig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbdas geh\u00f6rt sich eigentlich,", "tokens": ["\u00bb", "das", "ge\u00b7h\u00f6rt", "sich", "ei\u00b7gent\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PRF", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer was erbt, ich meine eben,", "tokens": ["Wer", "was", "erbt", ",", "ich", "mei\u00b7ne", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PWS", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll dem Staate auch was geben.\u00ab", "tokens": ["Soll", "dem", "Staa\u00b7te", "auch", "was", "ge\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "PIS", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum war dieses Wort geh\u00f6rt,", "tokens": ["Kaum", "war", "die\u00b7ses", "Wort", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "VVFIN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Hat sich innerlich emp\u00f6rt", "tokens": ["Hat", "sich", "in\u00b7ner\u00b7lich", "em\u00b7p\u00f6rt"],