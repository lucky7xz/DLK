{"textgrid.poem.62422": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Scharnhorst's Tod", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dem wilden Kriegestanze", "tokens": ["In", "dem", "wil\u00b7den", "Krie\u00b7ge\u00b7stan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brach die sch\u00f6nste Heldenlanze,", "tokens": ["Brach", "die", "sch\u00f6ns\u00b7te", "Hel\u00b7den\u00b7lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Preu\u00dfen, euer General.", "tokens": ["Preu\u00b7\u00dfen", ",", "eu\u00b7er", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig auf dem Feld bei L\u00fctzen", "tokens": ["Lus\u00b7tig", "auf", "dem", "Feld", "bei", "L\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah' er Freiheits-Waffen blitzen,", "tokens": ["Sah'", "er", "Frei\u00b7heits\u00b7Waf\u00b7fen", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch ihn traf der Todesstrahl.", "tokens": ["Doch", "ihn", "traf", "der", "To\u00b7des\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbkugel, raffst mich doch nicht nieder,", "tokens": ["\u00bb", "ku\u00b7gel", ",", "raffst", "mich", "doch", "nicht", "nie\u00b7der", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dien' euch blutend, werthe Br\u00fcder,", "tokens": ["Dien'", "euch", "blu\u00b7tend", ",", "wert\u00b7he", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt in Eile mich gen Prag.", "tokens": ["F\u00fchrt", "in", "Ei\u00b7le", "mich", "gen", "Prag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mit Blut um Oestreich werben,", "tokens": ["Will", "mit", "Blut", "um", "O\u00b7e\u00b7streich", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ist's beschlossen, will ich sterben,", "tokens": ["Ist's", "be\u00b7schlos\u00b7sen", ",", "will", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo Schwerin im Blute lag.\u00ab", "tokens": ["Wo", "Schwe\u00b7rin", "im", "Blu\u00b7te", "lag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Arge Stadt, wo Helden kranken,", "tokens": ["Ar\u00b7ge", "Stadt", ",", "wo", "Hel\u00b7den", "kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil'ge von den Br\u00fccken sanken,", "tokens": ["Heil'\u00b7ge", "von", "den", "Br\u00fc\u00b7cken", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00dfest alle Bl\u00fcten ab,", "tokens": ["Rei\u00b7\u00dfest", "al\u00b7le", "Bl\u00fc\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nennen dich mit leisen Schauern, \u2013", "tokens": ["Nen\u00b7nen", "dich", "mit", "lei\u00b7sen", "Schau\u00b7ern", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heil'ge Stadt, nach deinen Mauern", "tokens": ["Heil'\u00b7ge", "Stadt", ",", "nach", "dei\u00b7nen", "Mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht uns manches theure Grab.", "tokens": ["Zieht", "uns", "man\u00b7ches", "theu\u00b7re", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem irdischen Get\u00fcmmel,", "tokens": ["Aus", "dem", "ir\u00b7di\u00b7schen", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Engel in den Himmel", "tokens": ["Ha\u00b7ben", "En\u00b7gel", "in", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Seele sanft gef\u00fchrt.", "tokens": ["Sei\u00b7ne", "See\u00b7le", "sanft", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem alten deutschen Rathe,", "tokens": ["Zu", "dem", "al\u00b7ten", "deut\u00b7schen", "Ra\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den im ritterlichen Staate,", "tokens": ["Den", "im", "rit\u00b7ter\u00b7li\u00b7chen", "Staa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig Kaiser Karl regiert.", "tokens": ["E\u00b7wig", "Kai\u00b7ser", "Karl", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbgr\u00fc\u00df euch Gott, ihr theuren Helden,", "tokens": ["\u00bb", "gr\u00fc\u00df", "euch", "Gott", ",", "ihr", "theu\u00b7ren", "Hel\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann euch frohe Zeitung melden,", "tokens": ["Kann", "euch", "fro\u00b7he", "Zei\u00b7tung", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Volk ist aufgewacht.", "tokens": ["Un\u00b7ser", "Volk", "ist", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutschland hat sein Recht gefunden,", "tokens": ["Deutschland", "hat", "sein", "Recht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schaut, ich trage S\u00fchnungswunden,", "tokens": ["Schaut", ",", "ich", "tra\u00b7ge", "S\u00fch\u00b7nungs\u00b7wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der heil'gen Opferschlacht.\u00ab", "tokens": ["Aus", "der", "heil'\u00b7gen", "Op\u00b7fer\u00b7schlacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Solches hat er dort verk\u00fcndet,", "tokens": ["Sol\u00b7ches", "hat", "er", "dort", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wir alle stehn verb\u00fcndet,", "tokens": ["Und", "wir", "al\u00b7le", "stehn", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df dies Wort nicht L\u00fcge sei.", "tokens": ["Da\u00df", "dies", "Wort", "nicht", "L\u00fc\u00b7ge", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NN", "PTKNEG", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heer, aus seinem Geist geboren,", "tokens": ["Heer", ",", "aus", "sei\u00b7nem", "Geist", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "J\u00e4ger, die sein Muth erkoren,", "tokens": ["J\u00e4\u00b7ger", ",", "die", "sein", "Muth", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hlet ihn zum Feldgeschrei!", "tokens": ["W\u00e4h\u00b7let", "ihn", "zum", "Feld\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu den h\u00f6chsten Bergesforsten,", "tokens": ["Zu", "den", "h\u00f6chs\u00b7ten", "Ber\u00b7ges\u00b7fors\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die freien Adler horsten,", "tokens": ["Wo", "die", "frei\u00b7en", "Ad\u00b7ler", "hors\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat sich fr\u00fch sein Blick gewandt;", "tokens": ["Hat", "sich", "fr\u00fch", "sein", "Blick", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur dem H\u00f6chsten galt sein Streben,", "tokens": ["Nur", "dem", "H\u00f6chs\u00b7ten", "galt", "sein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur in Freiheit konnt' er leben,", "tokens": ["Nur", "in", "Frei\u00b7heit", "konnt'", "er", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scharnhorst ist er drum genannt.", "tokens": ["Scharn\u00b7horst", "ist", "er", "drum", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Keiner war wol treuer, reiner,", "tokens": ["Kei\u00b7ner", "war", "wol", "treu\u00b7er", ",", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4her stand dem K\u00f6nig keiner, \u2013", "tokens": ["N\u00e4\u00b7her", "stand", "dem", "K\u00f6\u00b7nig", "kei\u00b7ner", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIS", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dem Volke schlug sein Herz.", "tokens": ["Doch", "dem", "Vol\u00b7ke", "schlug", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig auf den Lippen schweben", "tokens": ["E\u00b7wig", "auf", "den", "Lip\u00b7pen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird er, wird im Volke leben,", "tokens": ["Wird", "er", ",", "wird", "im", "Vol\u00b7ke", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Besser als in Stein und Erz.", "tokens": ["Bes\u00b7ser", "als", "in", "Stein", "und", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00df uns deine Blicke scheinen,", "tokens": ["La\u00df", "uns", "dei\u00b7ne", "Bli\u00b7cke", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darfst nicht l\u00e4nger mehr beweinen,", "tokens": ["Darfst", "nicht", "l\u00e4n\u00b7ger", "mehr", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Gr\u00e4fin, seinen Fall.", "tokens": ["Sch\u00f6\u00b7ne", "Gr\u00e4\u00b7fin", ",", "sei\u00b7nen", "Fall", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen's alle recht in Treue,", "tokens": ["Mei\u00b7nen's", "al\u00b7le", "recht", "in", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau', dein Vater lebt aufs neue", "tokens": ["Schau'", ",", "dein", "Va\u00b7ter", "lebt", "aufs", "neu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des deutschen Liedes Schall.", "tokens": ["In", "des", "deut\u00b7schen", "Lie\u00b7des", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In dem wilden Kriegestanze", "tokens": ["In", "dem", "wil\u00b7den", "Krie\u00b7ge\u00b7stan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brach die sch\u00f6nste Heldenlanze,", "tokens": ["Brach", "die", "sch\u00f6ns\u00b7te", "Hel\u00b7den\u00b7lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Preu\u00dfen, euer General.", "tokens": ["Preu\u00b7\u00dfen", ",", "eu\u00b7er", "Ge\u00b7ne\u00b7ral", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig auf dem Feld bei L\u00fctzen", "tokens": ["Lus\u00b7tig", "auf", "dem", "Feld", "bei", "L\u00fct\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah' er Freiheits-Waffen blitzen,", "tokens": ["Sah'", "er", "Frei\u00b7heits\u00b7Waf\u00b7fen", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch ihn traf der Todesstrahl.", "tokens": ["Doch", "ihn", "traf", "der", "To\u00b7des\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbkugel, raffst mich doch nicht nieder,", "tokens": ["\u00bb", "ku\u00b7gel", ",", "raffst", "mich", "doch", "nicht", "nie\u00b7der", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dien' euch blutend, werthe Br\u00fcder,", "tokens": ["Dien'", "euch", "blu\u00b7tend", ",", "wert\u00b7he", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt in Eile mich gen Prag.", "tokens": ["F\u00fchrt", "in", "Ei\u00b7le", "mich", "gen", "Prag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mit Blut um Oestreich werben,", "tokens": ["Will", "mit", "Blut", "um", "O\u00b7e\u00b7streich", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "APPR", "NE", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ist's beschlossen, will ich sterben,", "tokens": ["Ist's", "be\u00b7schlos\u00b7sen", ",", "will", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo Schwerin im Blute lag.\u00ab", "tokens": ["Wo", "Schwe\u00b7rin", "im", "Blu\u00b7te", "lag", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Arge Stadt, wo Helden kranken,", "tokens": ["Ar\u00b7ge", "Stadt", ",", "wo", "Hel\u00b7den", "kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil'ge von den Br\u00fccken sanken,", "tokens": ["Heil'\u00b7ge", "von", "den", "Br\u00fc\u00b7cken", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rei\u00dfest alle Bl\u00fcten ab,", "tokens": ["Rei\u00b7\u00dfest", "al\u00b7le", "Bl\u00fc\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nennen dich mit leisen Schauern, \u2