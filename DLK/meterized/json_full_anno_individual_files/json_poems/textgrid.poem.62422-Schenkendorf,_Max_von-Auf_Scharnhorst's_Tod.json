r": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scharnhorst ist er drum genannt.", "tokens": ["Scharn\u00b7horst", "ist", "er", "drum", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Keiner war wol treuer, reiner,", "tokens": ["Kei\u00b7ner", "war", "wol", "treu\u00b7er", ",", "rei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "N\u00e4her stand dem K\u00f6nig keiner, \u2013", "tokens": ["N\u00e4\u00b7her", "stand", "dem", "K\u00f6\u00b7nig", "kei\u00b7ner", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIS", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dem Volke schlug sein Herz.", "tokens": ["Doch", "dem", "Vol\u00b7ke", "schlug", "sein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig auf den Lippen schweben", "tokens": ["E\u00b7wig", "auf", "den", "Lip\u00b7pen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird er, wird im Volke leben,", "tokens": ["Wird", "er", ",", "wird", "im", "Vol\u00b7ke", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Besser als in Stein 