{"dta.poem.8483": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Durchwachte Nacht.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sank die Sonne gl&#252;h und sch\u00f6n,", "tokens": ["Es", "sank", "die", "Son\u00b7ne", "gl", "&#252;", "h", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$(", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aus versengter Welle dann", "tokens": ["Und", "aus", "ver\u00b7seng\u00b7ter", "Wel\u00b7le", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie rauchte nicht das Nebelmeer", "tokens": ["Wie", "rauch\u00b7te", "nicht", "das", "Ne\u00b7bel\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sternenlose Nacht heran! \u2014", "tokens": ["Die", "ster\u00b7nen\u00b7lo\u00b7se", "Nacht", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich h\u00f6re ferne Schritte geh\u2019n, \u2014", "tokens": ["Ich", "h\u00f6\u00b7re", "fer\u00b7ne", "Schrit\u00b7te", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Uhr schl\u00e4gt zehn.", "tokens": ["Die", "Uhr", "schl\u00e4gt", "zehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Noch ist nicht alles Leben eingenickt,", "tokens": ["Noch", "ist", "nicht", "al\u00b7les", "Le\u00b7ben", "ein\u00b7ge\u00b7nickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schlafgem\u00e4cher letzte Angeln knarren;", "tokens": ["Der", "Schlaf\u00b7ge\u00b7m\u00e4\u00b7cher", "letz\u00b7te", "An\u00b7geln", "knar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorsichtig in der Rinne Bauch gedr\u00fcckt,", "tokens": ["Vor\u00b7sich\u00b7tig", "in", "der", "Rin\u00b7ne", "Bauch", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schl\u00fcpft noch der Iltis an des Giebels Sparren;", "tokens": ["Schl\u00fcpft", "noch", "der", "Il\u00b7tis", "an", "des", "Gie\u00b7bels", "Spar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Matt bin ich, m\u00f6chte tr\u00e4umen nur; \u2014", "tokens": ["Matt", "bin", "ich", ",", "m\u00f6ch\u00b7te", "tr\u00e4u\u00b7men", "nur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VMFIN", "VVINF", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Eilf schl\u00e4gt die Uhr.", "tokens": ["Eilf", "schl\u00e4gt", "die", "Uhr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ob mir das Blut so siedend fliegt?", "tokens": ["Ob", "mir", "das", "Blut", "so", "sie\u00b7dend", "fliegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich d\u00fcnkt, ich h\u00f6r\u2019 der Sph\u00e4ren Summen, \u2014", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "h\u00f6r'", "der", "Sph\u00e4\u00b7ren", "Sum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schweigen, dem das Ohr erliegt,", "tokens": ["Ein", "Schwei\u00b7gen", ",", "dem", "das", "Ohr", "er\u00b7liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann wieder fernes, dumpfes Brummen;", "tokens": ["Dann", "wie\u00b7der", "fer\u00b7nes", ",", "dum\u00b7pfes", "Brum\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch horch! des Thurmes Glocke wacht; \u2014", "tokens": ["Doch", "horch", "!", "des", "Thur\u00b7mes", "Glo\u00b7cke", "wacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "$.", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u2019s ist Mitternacht.", "tokens": ["'s", "ist", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und bange, gleich verhaltnem Weinen, steigt", "tokens": ["Und", "ban\u00b7ge", ",", "gleich", "ver\u00b7halt\u00b7nem", "Wei\u00b7nen", ",", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein langer Klageton aus den Syringen;", "tokens": ["Ein", "lan\u00b7ger", "Kla\u00b7ge\u00b7ton", "aus", "den", "Sy\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Nachtigall! ob Thal und H\u00f6he schweigt,", "tokens": ["O", "Nach\u00b7ti\u00b7gall", "!", "ob", "Thal", "und", "H\u00f6\u00b7he", "schweigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Dunkel legt verr\u00e4therische Schlingen;", "tokens": ["Das", "Dun\u00b7kel", "legt", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sche", "Schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein K\u00e4uzlein wacht im Bl\u00e4tterschmuck des Hains; \u2014", "tokens": ["Ein", "K\u00e4uz\u00b7lein", "wacht", "im", "Bl\u00e4t\u00b7ter\u00b7schmuck", "des", "Hains", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Uhr schl\u00e4gt Eins.", "tokens": ["Die", "Uhr", "schl\u00e4gt", "Eins", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Jetzt m\u00f6cht ich schlafen, schlafen gleich,", "tokens": ["Jetzt", "m\u00f6cht", "ich", "schla\u00b7fen", ",", "schla\u00b7fen", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entschlafen unter Mondeshauch,", "tokens": ["Ent\u00b7schla\u00b7fen", "un\u00b7ter", "Mon\u00b7des\u00b7hauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umspielt vom fl\u00fcsternden Gezweig,", "tokens": ["Um\u00b7spielt", "vom", "fl\u00fcs\u00b7tern\u00b7den", "Ge\u00b7zweig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Blute Funken, Funk\u2019 im Strauch,", "tokens": ["Im", "Blu\u00b7te", "Fun\u00b7ken", ",", "Funk'", "im", "Strauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mir im Ohre Melodey; \u2014", "tokens": ["Und", "mir", "im", "Oh\u00b7re", "Me\u00b7lo\u00b7dey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Uhr schl\u00e4gt Zwei.", "tokens": ["Die", "Uhr", "schl\u00e4gt", "Zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wie bin ich aufgeschreckt; o Jugendbild,", "tokens": ["Wie", "bin", "ich", "auf\u00b7ge\u00b7schreckt", ";", "o", "Ju\u00b7gend\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$.", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bist dahin, zerflossen mit dem Dunkel!", "tokens": ["Du", "bist", "da\u00b7hin", ",", "zer\u00b7flos\u00b7sen", "mit", "dem", "Dun\u00b7kel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die unerfreulich graue D\u00e4mmrung quillt,", "tokens": ["Die", "un\u00b7er\u00b7freu\u00b7lich", "grau\u00b7e", "D\u00e4mm\u00b7rung", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Walde irrt ein \u00e4ngstliches Gemunkel;", "tokens": ["Im", "Wal\u00b7de", "irrt", "ein", "\u00e4ngst\u00b7li\u00b7ches", "Ge\u00b7mun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch horch, des Hahnes erster Schrei! \u2014", "tokens": ["Doch", "horch", ",", "des", "Hah\u00b7nes", "ers\u00b7ter", "Schrei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Uhr schl\u00e4gt Drei.", "tokens": ["Die", "Uhr", "schl\u00e4gt", "Drei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Und wieder ruft der Hahn auf\u2019s Neu;", "tokens": ["Und", "wie\u00b7der", "ruft", "der", "Hahn", "auf's", "Neu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sims die Schwalbe gibt sich kund,", "tokens": ["Am", "Sims", "die", "Schwal\u00b7be", "gibt", "sich", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tauben Schw\u00e4rme kreisen scheu", "tokens": ["Der", "Tau\u00b7ben", "Schw\u00e4r\u00b7me", "krei\u00b7sen", "scheu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und taumelnd in des Hofes Rund;", "tokens": ["Und", "tau\u00b7melnd", "in", "des", "Ho\u00b7fes", "Rund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und drunten knarrt des Stalles Th\u00fcr; \u2014", "tokens": ["Und", "drun\u00b7ten", "knarrt", "des", "Stal\u00b7les", "Th\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Uhr schl\u00e4gt Vier.", "tokens": ["Die", "Uhr", "schl\u00e4gt", "Vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "CARD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Da flammt\u2019s in Osten auf, gleich Lavagluth", "tokens": ["Da", "flammt's", "in", "Os\u00b7ten", "auf", ",", "gleich", "La\u00b7vag\u00b7luth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne steigt, und mit den ersten Strahlen", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "und", "mit", "den", "ers\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Wald und Feldern str\u00f6mt Gesanges Fluth,", "tokens": ["In", "Wald", "und", "Fel\u00b7dern", "str\u00f6mt", "Ge\u00b7san\u00b7ges", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Leben quillt aus sch\u00e4umenden Pokalen;", "tokens": ["Das", "Le\u00b7ben", "quillt", "aus", "sch\u00e4u\u00b7men\u00b7den", "Po\u00b7ka\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie ein Gletscher sinkt der Tr\u00e4ume Land", "tokens": ["Und", "wie", "ein", "Glet\u00b7scher", "sinkt", "der", "Tr\u00e4u\u00b7me", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zerrinnend in des Horizontes Brand.", "tokens": ["Zer\u00b7rin\u00b7nend", "in", "des", "Ho\u00b7ri\u00b7zon\u00b7tes", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}