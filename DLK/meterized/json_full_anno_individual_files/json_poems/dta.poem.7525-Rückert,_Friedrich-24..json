{"dta.poem.7525": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Strom, einmal getr\u00fcbt, mu\u00df flie\u00dfen eine Weile,", "tokens": ["Der", "Strom", ",", "ein\u00b7mal", "ge\u00b7tr\u00fcbt", ",", "mu\u00df", "flie\u00b7\u00dfen", "ei\u00b7ne", "Wei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh aus der innern F\u00fcll' er seinen Schaden heile.", "tokens": ["Eh", "aus", "der", "in\u00b7nern", "F\u00fcll'", "er", "sei\u00b7nen", "Scha\u00b7den", "hei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Vom Sturm ersch\u00fcttert, mu\u00df in Wolkendampf die Luft", "tokens": ["Vom", "Sturm", "er\u00b7sch\u00fct\u00b7tert", ",", "mu\u00df", "in", "Wol\u00b7ken\u00b7dampf", "die", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "VMFIN", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ausg\u00e4hren, bis sie sich verkl\u00e4rt in reinen Duft.", "tokens": ["Aus\u00b7g\u00e4h\u00b7ren", ",", "bis", "sie", "sich", "ver\u00b7kl\u00e4rt", "in", "rei\u00b7nen", "Duft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So mu\u00df ein menschliches Gem\u00fcth auch erst ausschwanken,", "tokens": ["So", "mu\u00df", "ein", "menschli\u00b7ches", "Ge\u00b7m\u00fcth", "auch", "erst", "aus\u00b7schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn es ein \u00e4u\u00dfrer Sto\u00df, ein innrer, macht' erkranken.", "tokens": ["Wenn", "es", "ein", "\u00e4u\u00df\u00b7rer", "Sto\u00df", ",", "ein", "inn\u00b7rer", ",", "macht'", "er\u00b7kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Leicht heilt die Wunde, die man deinem Leib geschlagen;", "tokens": ["Leicht", "heilt", "die", "Wun\u00b7de", ",", "die", "man", "dei\u00b7nem", "Leib", "ge\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die selbst dein Herz sich schlug, wird sp\u00e4te Narben tragen.", "tokens": ["Die", "selbst", "dein", "Herz", "sich", "schlug", ",", "wird", "sp\u00e4\u00b7te", "Nar\u00b7ben", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch wenn es grausam hei\u00dft, dem Freund die Wund' aufrei\u00dfen;", "tokens": ["Doch", "wenn", "es", "grau\u00b7sam", "hei\u00dft", ",", "dem", "Freund", "die", "Wund'", "auf\u00b7rei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich selber es zu thun, kann auch nicht menschlich hei\u00dfen.", "tokens": ["Sich", "sel\u00b7ber", "es", "zu", "thun", ",", "kann", "auch", "nicht", "menschlich", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPER", "PTKZU", "VVINF", "$,", "VMFIN", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Viel lieber lindes Oel geu\u00df, das du hast im Haus,", "tokens": ["Viel", "lie\u00b7ber", "lin\u00b7des", "O\u00b7el", "geu\u00df", ",", "das", "du", "hast", "im", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Auf deine Schmerzen und auf alle fremden aus.", "tokens": ["Auf", "dei\u00b7ne", "Schmer\u00b7zen", "und", "auf", "al\u00b7le", "frem\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}