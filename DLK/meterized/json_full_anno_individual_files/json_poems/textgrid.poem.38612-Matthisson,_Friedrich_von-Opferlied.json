{"textgrid.poem.38612": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Opferlied", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flamme lodert, milder Schein", "tokens": ["Die", "Flam\u00b7me", "lo\u00b7dert", ",", "mil\u00b7der", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchgl\u00e4nzt den d\u00fcstern Eichenhain,", "tokens": ["Durch\u00b7gl\u00e4nzt", "den", "d\u00fcs\u00b7tern", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Weihrauchd\u00fcfte wallen.", "tokens": ["Und", "Weih\u00b7rauch\u00b7d\u00fcf\u00b7te", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O neig' ein gn\u00e4dig Ohr zu mir,", "tokens": ["O", "neig'", "ein", "gn\u00e4\u00b7dig", "Ohr", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und la\u00df des J\u00fcnglings Opfer dir,", "tokens": ["Und", "la\u00df", "des", "J\u00fcng\u00b7lings", "Op\u00b7fer", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du H\u00f6chster, wohlgefallen!", "tokens": ["Du", "H\u00f6chs\u00b7ter", ",", "wohl\u00b7ge\u00b7fal\u00b7len", "!"], "token_info"