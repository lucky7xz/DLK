{"textgrid.poem.61113": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel zu wissen geziemt und viel zu lernen dem Dichter,", "tokens": ["Viel", "zu", "wis\u00b7sen", "ge\u00b7ziemt", "und", "viel", "zu", "ler\u00b7nen", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVPP", "KON", "PIS", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach, f\u00fcr seinen Beruf deucht mir das Leben so kurz.", "tokens": ["Ach", ",", "f\u00fcr", "sei\u00b7nen", "Be\u00b7ruf", "deucht", "mir", "das", "Le\u00b7ben", "so", "kurz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Denn er kenne die Welt und ihre Geschichten, er gehe", "tokens": ["Denn", "er", "ken\u00b7ne", "die", "Welt", "und", "ih\u00b7re", "Ge\u00b7schich\u00b7ten", ",", "er", "ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bei den Alten mit Lust wie bei den Neuen zu Gast.", "tokens": ["Bei", "den", "Al\u00b7ten", "mit", "Lust", "wie", "bei", "den", "Neu\u00b7en", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KOKOM", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Fremde L\u00e4nder und Sprachen erforsch' er mit willigem Eifer,", "tokens": ["Frem\u00b7de", "L\u00e4n\u00b7der", "und", "Spra\u00b7chen", "er\u00b7for\u00b7sch'", "er", "mit", "wil\u00b7li\u00b7gem", "Ei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Sei im Norden und sei unter den Palmen zu Haus.", "tokens": ["Sei", "im", "Nor\u00b7den", "und", "sei", "un\u00b7ter", "den", "Pal\u00b7men", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "KON", "VAFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Aber vor allem versteh' er das Herz und die ewige Leiter", "tokens": ["A\u00b7ber", "vor", "al\u00b7lem", "ver\u00b7steh'", "er", "das", "Herz", "und", "die", "e\u00b7wi\u00b7ge", "Lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Seiner Gef\u00fchle: die Lust kenn' er und kenne den Schmerz.", "tokens": ["Sei\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", ":", "die", "Lust", "kenn'", "er", "und", "ken\u00b7ne", "den", "Schmerz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.9": {"text": "Was aus S\u00e4ul' und Gem\u00e4lde dich anspricht, wiss' er zu deuten,", "tokens": ["Was", "aus", "S\u00e4ul'", "und", "Ge\u00b7m\u00e4l\u00b7de", "dich", "an\u00b7spricht", ",", "wiss'", "er", "zu", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Was dir des Waldes Ger\u00e4usch fl\u00fcstert, er fass' es ins Wort.", "tokens": ["Was", "dir", "des", "Wal\u00b7des", "Ge\u00b7r\u00e4usch", "fl\u00fcs\u00b7tert", ",", "er", "fass'", "es", "ins", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Kunst und Natur und Welt und Gem\u00fct, er beherrsche sie alle:", "tokens": ["Kunst", "und", "Na\u00b7tur", "und", "Welt", "und", "Ge\u00b7m\u00fct", ",", "er", "be\u00b7herr\u00b7sche", "sie", "al\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "PIS", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Aber der Tor nur verlangt, da\u00df ein Gelehrter er sei.", "tokens": ["A\u00b7ber", "der", "Tor", "nur", "ver\u00b7langt", ",", "da\u00df", "ein", "Ge\u00b7lehr\u00b7ter", "er", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "+