{"textgrid.poem.52101": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Damit die Motte nicht darff \u00fcber Hunger klagen,", "tokens": ["Da\u00b7mit", "die", "Mot\u00b7te", "nicht", "darff", "\u00fc\u00b7ber", "Hun\u00b7ger", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch sonder Tichter darff der Kramer D\u00fctten drehn,", "tokens": ["Noch", "son\u00b7der", "Tich\u00b7ter", "darff", "der", "Kra\u00b7mer", "D\u00fct\u00b7ten", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird hier das F\u00fcnffte Buch der gute Leser sehn,", "tokens": ["Wird", "hier", "das", "F\u00fcnff\u00b7te", "Buch", "der", "gu\u00b7te", "Le\u00b7ser", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit welchem sich schon wil der harte Lands Knecht tragen.", "tokens": ["Mit", "wel\u00b7chem", "sich", "schon", "wil", "der", "har\u00b7te", "Lands", "Knecht", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "VMFIN", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es sind ja Pappelstiel, und wo von eiteln Sachen", "tokens": ["Es", "sind", "ja", "Pap\u00b7pel\u00b7stiel", ",", "und", "wo", "von", "ei\u00b7teln", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "KON", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was schlechters kan als wol die Pappelstiele seyn;", "tokens": ["Was", "schlech\u00b7ters", "kan", "als", "wol", "die", "Pap\u00b7pel\u00b7stie\u00b7le", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "KOKOM", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich theile Schandfleck aus, und nehme sie auch ein:", "tokens": ["Ich", "thei\u00b7le", "Schand\u00b7fleck", "aus", ",", "und", "neh\u00b7me", "sie", "auch", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn ieder wil mich draus ohn Danck zum Tichter machen.", "tokens": ["Denn", "ie\u00b7der", "wil", "mich", "draus", "ohn", "Danck", "zum", "Tich\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PRF", "PAV", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Damit die Motte nicht darff \u00fcber Hunger klagen,", "tokens": ["Da\u00b7mit", "die", "Mot\u00b7te", "nicht", "darff", "\u00fc\u00b7ber", "Hun\u00b