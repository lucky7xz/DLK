{"dta.poem.7915": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Theelied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Saiten, t\u00f6net sanft und leise,", "tokens": ["Ihr", "Sai\u00b7ten", ",", "t\u00f6\u00b7net", "sanft", "und", "lei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom leichten Finger kaum geregt!", "tokens": ["Vom", "leich\u00b7ten", "Fin\u00b7ger", "kaum", "ge\u00b7regt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr t\u00f6net zu des Z\u00e4rtsten Preise,", "tokens": ["Ihr", "t\u00f6\u00b7net", "zu", "des", "Z\u00e4rts\u00b7ten", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Z\u00e4rtsten, was die Erde hegt.", "tokens": ["Des", "Z\u00e4rts\u00b7ten", ",", "was", "die", "Er\u00b7de", "hegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Indiens mythischem Gebiete,", "tokens": ["In", "In\u00b7di\u00b7ens", "my\u00b7thi\u00b7schem", "Ge\u00b7bie\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo Fr\u00fchling ewig sich erneut,", "tokens": ["Wo", "Fr\u00fch\u00b7ling", "e\u00b7wig", "sich", "er\u00b7neut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Thee, du selber eine Mythe,", "tokens": ["O", "Thee", ",", "du", "sel\u00b7ber", "ei\u00b7ne", "My\u00b7the", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlebst du deine Bl\u00fcthezeit.", "tokens": ["Ver\u00b7lebst", "du", "dei\u00b7ne", "Bl\u00fc\u00b7the\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur zarte Bienenlippen schl\u00fcrfen", "tokens": ["Nur", "zar\u00b7te", "Bie\u00b7nen\u00b7lip\u00b7pen", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus deinen Kelchen Honig ein,", "tokens": ["Aus", "dei\u00b7nen", "Kel\u00b7chen", "Ho\u00b7nig", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur bunte Wunderv\u00f6gel d\u00fcrfen", "tokens": ["Nur", "bun\u00b7te", "Wun\u00b7der\u00b7v\u00f6\u00b7gel", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die S\u00e4nger deines Ruhmes seyn.", "tokens": ["Die", "S\u00e4n\u00b7ger", "dei\u00b7nes", "Ruh\u00b7mes", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann Liebende zum stillen Feste", "tokens": ["Wann", "Lie\u00b7ben\u00b7de", "zum", "stil\u00b7len", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deine duft\u2019gen Schatten fliehn,", "tokens": ["In", "dei\u00b7ne", "duft'\u00b7gen", "Schat\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann r\u00fchrest leise du die Aeste", "tokens": ["Dann", "r\u00fch\u00b7rest", "lei\u00b7se", "du", "die", "A\u00b7es\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und streuest Bl\u00fcthen auf sie hin.", "tokens": ["Und", "streu\u00b7est", "Bl\u00fc\u00b7then", "auf", "sie", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So w\u00e4chsest du am Heimathstrande,", "tokens": ["So", "w\u00e4ch\u00b7sest", "du", "am", "Hei\u00b7math\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom reinsten Sonnenlicht gen\u00e4hrt.", "tokens": ["Vom", "reins\u00b7ten", "Son\u00b7nen\u00b7licht", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch hier in diesem fernen Lande", "tokens": ["Noch", "hier", "in", "die\u00b7sem", "fer\u00b7nen", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist uns dein zarter Sinn bew\u00e4hrt.", "tokens": ["Ist", "uns", "dein", "zar\u00b7ter", "Sinn", "be\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn nur die holden Frauen halten", "tokens": ["Denn", "nur", "die", "hol\u00b7den", "Frau\u00b7en", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich in der m\u00fctterlichen Hut;", "tokens": ["Dich", "in", "der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Hut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Man sieht sie mit dem Kruge walten,", "tokens": ["Man", "sieht", "sie", "mit", "dem", "Kru\u00b7ge", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Nymphen an der heil\u2019gen Flut.", "tokens": ["Wie", "Nym\u00b7phen", "an", "der", "heil'\u00b7gen", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den M\u00e4nnern will es schwer gelingen,", "tokens": ["Den", "M\u00e4n\u00b7nern", "will", "es", "schwer", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu f\u00fchlen deine tiefe Kraft;", "tokens": ["Zu", "f\u00fch\u00b7len", "dei\u00b7ne", "tie\u00b7fe", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur zarte Frauenlippen dringen", "tokens": ["Nur", "zar\u00b7te", "Frau\u00b7en\u00b7lip\u00b7pen", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deines Zaubers Eigenschaft.", "tokens": ["In", "dei\u00b7nes", "Zau\u00b7bers", "Ei\u00b7gen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich selbst, der S\u00e4nger, der dich feiert,", "tokens": ["Ich", "selbst", ",", "der", "S\u00e4n\u00b7ger", ",", "der", "dich", "fei\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erfuhr noch deine Wunder nicht;", "tokens": ["Er\u00b7fuhr", "noch", "dei\u00b7ne", "Wun\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch was der Frauen Mund betheuert,", "tokens": ["Doch", "was", "der", "Frau\u00b7en", "Mund", "be\u00b7theu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mir zu glauben heil\u2019ge Pflicht.", "tokens": ["Ist", "mir", "zu", "glau\u00b7ben", "heil'\u00b7ge", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr aber m\u00f6get sanft verklingen,", "tokens": ["Ihr", "a\u00b7ber", "m\u00f6\u00b7get", "sanft", "ver\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr meine Saiten, kaum geregt!", "tokens": ["Ihr", "mei\u00b7ne", "Sai\u00b7ten", ",", "kaum", "ge\u00b7regt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Frauen k\u00f6nnen w\u00fcrdig singen", "tokens": ["Nur", "Frau\u00b7en", "k\u00f6n\u00b7nen", "w\u00fcr\u00b7dig", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Z\u00e4rtste, was die Erde hegt.", "tokens": ["Das", "Z\u00e4rts\u00b7te", ",", "was", "die", "Er\u00b7de", "hegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}