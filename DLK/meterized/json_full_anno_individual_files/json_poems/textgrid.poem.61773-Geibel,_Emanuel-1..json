{"textgrid.poem.61773": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein sch\u00f6ner Tag im sch\u00f6nen Wien,", "tokens": ["Es", "war", "ein", "sch\u00f6\u00b7ner", "Tag", "im", "sch\u00f6\u00b7nen", "Wi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Linden bl\u00fchten, und die Sonne schien,", "tokens": ["Die", "Lin\u00b7den", "bl\u00fch\u00b7ten", ",", "und", "die", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Arm in Arm, uns selber \u00fcberlassen,", "tokens": ["Und", "Arm", "in", "Arm", ",", "uns", "sel\u00b7ber", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durchschritten wir die morgenfrischen Gassen.", "tokens": ["Durch\u00b7schrit\u00b7ten", "wir", "die", "mor\u00b7gen\u00b7fri\u00b7schen", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Prunkl\u00e4den hier, Pal\u00e4ste stolz und grau,", "tokens": ["Prun\u00b7kl\u00e4\u00b7den", "hier", ",", "Pa\u00b7l\u00e4s\u00b7te", "stolz", "und", "grau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort schwarzget\u00fcrmt Sankt Stephans Riesenbau,", "tokens": ["Dort", "schwarz\u00b7ge\u00b7t\u00fcrmt", "Sankt", "Ste\u00b7phans", "Rie\u00b7sen\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und rings aus laub'gen G\u00e4rten durchs Gedr\u00e4nge", "tokens": ["Und", "rings", "aus", "laub'\u00b7gen", "G\u00e4r\u00b7ten", "durchs", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herflatternd Rosenduft und Geigenkl\u00e4nge.", "tokens": ["Her\u00b7flat\u00b7ternd", "Ro\u00b7sen\u00b7duft", "und", "Gei\u00b7gen\u00b7kl\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein M\u00e4rchen deucht' es uns, ein Traumgeschick:", "tokens": ["Ein", "M\u00e4r\u00b7chen", "deucht'", "es", "uns", ",", "ein", "Traum\u00b7ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sonst ruhlos \u00fcberwacht in Wort und Blick,", "tokens": ["Sonst", "ruh\u00b7los", "\u00fc\u00b7ber\u00b7wacht", "in", "Wort", "und", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und pl\u00f6tzlich nun im bunten Volksgetriebe", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "nun", "im", "bun\u00b7ten", "Volks\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der gro\u00dfen Stadt allein mit unsrer Liebe!", "tokens": ["Der", "gro\u00b7\u00dfen", "Stadt", "al\u00b7lein", "mit", "uns\u00b7rer", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Beschwingt ins Gr\u00fcne lenkten wir den Schritt,", "tokens": ["Be\u00b7schwingt", "ins", "Gr\u00fc\u00b7ne", "lenk\u00b7ten", "wir", "den", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die V\u00f6gel jauchzten, und wir jauchzten mit,", "tokens": ["Die", "V\u00f6\u00b7gel", "jauchz\u00b7ten", ",", "und", "wir", "jauchz\u00b7ten", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis wir zuletzt nach sel'ger Irrfahrt Stunden", "tokens": ["Bis", "wir", "zu\u00b7letzt", "nach", "sel'\u00b7ger", "Irr\u00b7fahrt", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Weg zu Belvederes Schlo\u00df gefunden.", "tokens": ["Den", "Weg", "zu", "Bel\u00b7ve\u00b7de\u00b7res", "Schlo\u00df", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von Panzern drinnen beim ged\u00e4mpften Strahl,", "tokens": ["Von", "Pan\u00b7zern", "drin\u00b7nen", "beim", "ge\u00b7d\u00e4mpf\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von T\u00fcrkenbeute blitzte Saal an Saal,", "tokens": ["Von", "T\u00fcr\u00b7ken\u00b7beu\u00b7te", "blitz\u00b7te", "Saal", "an", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und friedlich neben den ersiegten Waffen", "tokens": ["Und", "fried\u00b7lich", "ne\u00b7ben", "den", "er\u00b7sieg\u00b7ten", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hing, was der Meister Farbenkunst geschaffen.", "tokens": ["Hing", ",", "was", "der", "Meis\u00b7ter", "Far\u00b7ben\u00b7kunst", "ge\u00b7schaf\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Da gr\u00fc\u00dft' uns pl\u00f6tzlich l\u00e4chelnd von der Wand", "tokens": ["Da", "gr\u00fc\u00dft'", "uns", "pl\u00f6tz\u00b7lich", "l\u00e4\u00b7chelnd", "von", "der", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6nste Frauenkopf von Palmas Hand;", "tokens": ["Der", "sch\u00f6ns\u00b7te", "Frau\u00b7en\u00b7kopf", "von", "Pal\u00b7mas", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bezaubert staunt' ich, bis ins Herz erschrocken,", "tokens": ["Be\u00b7zau\u00b7bert", "staunt'", "ich", ",", "bis", "ins", "Herz", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So glich er dir mit deinen goldnen Locken.", "tokens": ["So", "glich", "er", "dir", "mit", "dei\u00b7nen", "gold\u00b7nen", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und k\u00fcssen wollt' ich das holdsel'ge Bild,", "tokens": ["Und", "k\u00fcs\u00b7sen", "wollt'", "ich", "das", "hold\u00b7sel'\u00b7ge", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du aber wehrtest mir und sprachest mild:", "tokens": ["Du", "a\u00b7ber", "wehr\u00b7test", "mir", "und", "spra\u00b7chest", "mild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbwarum nach stummem Reiz den Blick erheben?", "tokens": ["\u00bb", "wa\u00b7rum", "nach", "stum\u00b7mem", "Reiz", "den", "Blick", "er\u00b7he\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast's ja besser, halte dich ans Leben!\u00ab \u2013", "tokens": ["Du", "hast's", "ja", "bes\u00b7ser", ",", "hal\u00b7te", "dich", "ans", "Le\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und wieder durch die G\u00e4rten schw\u00e4rmten wir,", "tokens": ["Und", "wie\u00b7der", "durch", "die", "G\u00e4r\u00b7ten", "schw\u00e4rm\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und von den trunknen Lippen str\u00f6mte mir", "tokens": ["Und", "von", "den", "trunk\u00b7nen", "Lip\u00b7pen", "str\u00f6m\u00b7te", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein \u00fcberm\u00fctig Lied der Liebeswonne,", "tokens": ["Ein", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "Lied", "der", "Lie\u00b7bes\u00b7won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Rosen bl\u00fchten, und es schien die Sonne.", "tokens": ["Die", "Ro\u00b7sen", "bl\u00fch\u00b7ten", ",", "und", "es", "schien", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und denk' ich dran, so weht's durch meinen Sinn", "tokens": ["Und", "denk'", "ich", "dran", ",", "so", "weht's", "durch", "mei\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Rosenduft und Sonnenglanz dahin.", "tokens": ["Wie", "Ro\u00b7sen\u00b7duft", "und", "Son\u00b7nen\u00b7glanz", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Stadt Sankt Stephans, da\u00df dich Gott beh\u00fcte,", "tokens": ["O", "Stadt", "Sankt", "Ste\u00b7phans", ",", "da\u00df", "dich", "Gott", "be\u00b7h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "NE", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo meiner Jugend sch\u00f6nstes M\u00e4rchen bl\u00fchte!", "tokens": ["Wo", "mei\u00b7ner", "Ju\u00b7gend", "sch\u00f6ns\u00b7tes", "M\u00e4r\u00b7chen", "bl\u00fch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {