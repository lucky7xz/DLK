 "iambic.tri"}, "line.7": {"text": "Dr\u00fcben", "tokens": ["Dr\u00fc\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Ist alles stumm und bang.", "tokens": ["Ist", "al\u00b7les", "stumm", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Linde seh ich stehen", "tokens": ["Die", "Lin\u00b7de", "seh", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So traurig dunkelgr\u00fcn.", "tokens": ["So", "trau\u00b7rig", "dun\u00b7kel\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Blumen all vergehen", "tokens": ["Die", "Blu\u00b7men", "all", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wollen nicht mehr bl\u00fchn.", "tokens": ["Und", "wol\u00b7len", "nicht", "mehr", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab' ihn oft gesehen,", "tokens": ["Ich", "hab'", "ihn", "oft", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nun ist er, ach! dahin.", "tokens": ["Nun", "ist", "er", ",", "ach", "!", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["