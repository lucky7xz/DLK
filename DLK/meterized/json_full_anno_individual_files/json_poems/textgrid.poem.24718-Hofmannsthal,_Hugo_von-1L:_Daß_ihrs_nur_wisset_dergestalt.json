{"textgrid.poem.24718": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df ihrs nur wisset dergestalt", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ihrs nur wisset dergestalt", "tokens": ["Da\u00df", "ihrs", "nur", "wis\u00b7set", "der\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hier an diese Wand gemalt.", "tokens": ["Ich", "hier", "an", "die\u00b7se", "Wand", "ge\u00b7malt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir treibt besser keinen Spott.", "tokens": ["Mit", "mir", "treibt", "bes\u00b7ser", "kei\u00b7nen", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "PIAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Bin unser Herr der liebe Gott.", "tokens": ["Bin", "un\u00b7ser", "Herr", "der", "lie\u00b7be", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe mir die Menschen an", "tokens": ["Ich", "se\u00b7he", "mir", "die", "Men\u00b7schen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gut ichs unverrathen kann.", "tokens": ["So", "gut", "ichs", "un\u00b7ver\u00b7ra\u00b7then", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sie verworren stehn und gehen", "tokens": ["Wie", "sie", "ver\u00b7wor\u00b7ren", "stehn", "und", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich selber und Leben nicht verstehen", "tokens": ["Sich", "sel\u00b7ber", "und", "Le\u00b7ben", "nicht", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "KON", "NN", "PTKNEG", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Und bei ihren Geschichten und Sachen", "tokens": ["Und", "bei", "ih\u00b7ren", "Ge\u00b7schich\u00b7ten", "und", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Kommt mir manchmal ein heimliches Lachen.", "tokens": ["Kommt", "mir", "manch\u00b7mal", "ein", "heim\u00b7li\u00b7ches", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Wie sie ihr Herz an Puppen h\u00e4ngen", "tokens": ["Wie", "sie", "ihr", "Herz", "an", "Pup\u00b7pen", "h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und in fertige Kasteln das Leben zw\u00e4ngen.", "tokens": ["Und", "in", "fer\u00b7ti\u00b7ge", "Kas\u00b7teln", "das", "Le\u00b7ben", "zw\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Oft sind's auf ihre Gedanken stolz", "tokens": ["Oft", "sin\u00b7d's", "auf", "ih\u00b7re", "Ge\u00b7dan\u00b7ken", "stolz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Als w\u00e4rens aus Schildkrot und Ebenholz", "tokens": ["Als", "w\u00e4\u00b7rens", "aus", "Schild\u00b7krot", "und", "E\u00b7ben\u00b7holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Sind oft von Trugbildern so besessen", "tokens": ["Sind", "oft", "von", "Trug\u00b7bil\u00b7dern", "so", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADV", "VVINF"], "meter": "-+-++-+-+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Dass sie aufs Wirkliche vergessen.", "tokens": ["Dass", "sie", "aufs", "Wirk\u00b7li\u00b7che", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie lernen nimmer sich bescheiden", "tokens": ["Sie", "ler\u00b7nen", "nim\u00b7mer", "sich", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und wollen immer von Lust und Leiden", "tokens": ["Und", "wol\u00b7len", "im\u00b7mer", "von", "Lust", "und", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die allertiefsten Wurzeln fassen", "tokens": ["Die", "al\u00b7ler\u00b7tiefs\u00b7ten", "Wur\u00b7zeln", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die sich halt nicht betasten lassen", "tokens": ["Die", "sich", "halt", "nicht", "be\u00b7tas\u00b7ten", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "VVFIN", "PTKNEG", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und mit solchem Graben und W\u00fchlen", "tokens": ["Und", "mit", "sol\u00b7chem", "Gra\u00b7ben", "und", "W\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "Springt Glanz und Schmelz von den Gef\u00fchlen.", "tokens": ["Springt", "Glanz", "und", "Schmelz", "von", "den", "Ge\u00b7f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und das Leben ist doch so eingerichtet", "tokens": ["Und", "das", "Le\u00b7ben", "ist", "doch", "so", "ein\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Dass es mit einem spielt und dichtet.", "tokens": ["Dass", "es", "mit", "ei\u00b7nem", "spielt", "und", "dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Man muss sich nur recht f\u00fchren lassen", "tokens": ["Man", "muss", "sich", "nur", "recht", "f\u00fch\u00b7ren", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "PRF", "ADV", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "'s f\u00fchrt einen die wunderlichsten Stra\u00dfen.", "tokens": ["'s", "f\u00fchrt", "ei\u00b7nen", "die", "wun\u00b7der\u00b7lichs\u00b7ten", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Auf eines werden zu ihrem Frommen", "tokens": ["Auf", "ei\u00b7nes", "wer\u00b7den", "zu", "ih\u00b7rem", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Sie doch mit Gr\u00fcbeln nimmer kommen.", "tokens": ["Sie", "doch", "mit", "Gr\u00fc\u00b7beln", "nim\u00b7mer", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Zwar sie zerfaserten's auch recht gern", "tokens": ["Zwar", "sie", "zer\u00b7fa\u00b7ser\u00b7ten's", "auch", "recht", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Doch ist es der tiefste verschlossenste Kern.", "tokens": ["Doch", "ist", "es", "der", "tiefs\u00b7te", "ver\u00b7schlos\u00b7sens\u00b7te", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.31": {"text": "Ich meine den Grund von allen den Sachen", "tokens": ["Ich", "mei\u00b7ne", "den", "Grund", "von", "al\u00b7len", "den", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Die selig und die elend machen.", "tokens": ["Die", "se\u00b7lig", "und", "die", "e\u00b7lend", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Warum kann man aus Menschenaugen", "tokens": ["Wa\u00b7rum", "kann", "man", "aus", "Men\u00b7schen\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Die grenzenlose Seele saugen?", "tokens": ["Die", "gren\u00b7zen\u00b7lo\u00b7se", "See\u00b7le", "sau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Warum liegt Weinen im wandernden Wind?", "tokens": ["Wa\u00b7rum", "liegt", "Wei\u00b7nen", "im", "wan\u00b7dern\u00b7den", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Warum giebts G\u00e4rten die traurig sind?", "tokens": ["Wa\u00b7rum", "giebts", "G\u00e4r\u00b7ten", "die", "trau\u00b7rig", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ART", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Und l\u00e4chelnde Lippen ein rundes Kinn \u2013", "tokens": ["Und", "l\u00e4\u00b7cheln\u00b7de", "Lip\u00b7pen", "ein", "run\u00b7des", "Kinn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "Was haben die f\u00fcr einen Sinn?", "tokens": ["Was", "ha\u00b7ben", "die", "f\u00fcr", "ei\u00b7nen", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Warum ist irgend ein Geigenklang", "tokens": ["Wa\u00b7rum", "ist", "ir\u00b7gend", "ein", "Gei\u00b7gen\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.40": {"text": "Uns\u00e4glich lockend s\u00fcss und bang?", "tokens": ["Un\u00b7s\u00e4g\u00b7lich", "lo\u00b7ckend", "s\u00fcss", "und", "bang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Warum sind Kinder sch\u00f6n? Warum?", "tokens": ["Wa\u00b7rum", "sind", "Kin\u00b7der", "sch\u00f6n", "?", "Wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "ADJD", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Das Leben ist halt einmal stumm.", "tokens": ["Das", "Le\u00b7ben", "ist", "halt", "ein\u00b7mal", "stumm", "."], "token_info": ["word", "wo