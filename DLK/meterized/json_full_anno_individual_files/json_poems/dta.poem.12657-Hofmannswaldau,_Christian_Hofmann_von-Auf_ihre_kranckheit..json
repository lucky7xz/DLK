{"dta.poem.12657": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihre kranckheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr \u00e4rtzte, die ihr sonst dem tode seyd ein tod,", "tokens": ["Ihr", "\u00e4rtz\u00b7te", ",", "die", "ihr", "sonst", "dem", "to\u00b7de", "seyd", "ein", "tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Helfft meiner sch\u00f6nsten doch aus ihrer leibes noth,", "tokens": ["Helfft", "mei\u00b7ner", "sch\u00f6ns\u00b7ten", "doch", "aus", "ih\u00b7rer", "lei\u00b7bes", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Helfft, euer lob soll auch bi\u00df an die wolcken steigen,", "tokens": ["Helfft", ",", "eu\u00b7er", "lob", "soll", "auch", "bi\u00df", "an", "die", "wol\u00b7cken", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VMFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da die sterne selbst zu eurem ruhme neigen.", "tokens": ["Und", "da", "die", "ster\u00b7ne", "selbst", "zu", "eu\u00b7rem", "ruh\u00b7me", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bringt alles, was ihr habt in euren gl\u00e4sern her,", "tokens": ["Bringt", "al\u00b7les", ",", "was", "ihr", "habt", "in", "eu\u00b7ren", "gl\u00e4\u00b7sern", "her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das beste elexier, hertzst\u00e4rckung und was mehr", "tokens": ["Das", "bes\u00b7te", "el\u00b7e\u00b7xier", ",", "hertz\u00b7st\u00e4r\u00b7ckung", "und", "was", "mehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "KON", "PWS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die krancken eilends kan, wie vor, zu rechte bringen,", "tokens": ["Die", "kran\u00b7cken", "ei\u00b7lends", "kan", ",", "wie", "vor", ",", "zu", "rech\u00b7te", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "$,", "PWAV", "PTKVZ", "$,", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich will zehn oden euch zu eurem ruhme singen:", "tokens": ["Ich", "will", "zehn", "o\u00b7den", "euch", "zu", "eu\u00b7rem", "ruh\u00b7me", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "An madrigalen soll gleichfalls kein mangel seyn:", "tokens": ["An", "mad\u00b7ri\u00b7ga\u00b7len", "soll", "gleich\u00b7falls", "kein", "man\u00b7gel", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Sinn-schrifften stellen sich bereits im geiste ein,", "tokens": ["Sinn\u00b7schriff\u00b7ten", "stel\u00b7len", "sich", "be\u00b7reits", "im", "geis\u00b7te", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPRART", "ADJA", "PTKVZ", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.11": {"text": "Und warten nur auf euch, da\u00df sie raus sollen kommen,", "tokens": ["Und", "war\u00b7ten", "nur", "auf", "euch", ",", "da\u00df", "sie", "raus", "sol\u00b7len", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja \u00fcberschrifften hab ich auch schon angenommen.", "tokens": ["Ja", "\u00fc\u00b7bersc\u00b7hriff\u00b7ten", "hab", "ich", "auch", "schon", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVPP", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zwey f\u00e4sser stehn schon da von Hipocrenens fluth,", "tokens": ["Zwey", "f\u00e4s\u00b7ser", "stehn", "schon", "da", "von", "Hi\u00b7po\u00b7cre\u00b7nens", "fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn die ist sonderlich zum verse-machen gut,", "tokens": ["Denn", "die", "ist", "son\u00b7der\u00b7lich", "zum", "ver\u00b7se\u00b7ma\u00b7chen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "APPRART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich habe sie bestellt um euch recht zu bedienen,", "tokens": ["Ich", "ha\u00b7be", "sie", "be\u00b7stellt", "um", "euch", "recht", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "APPR", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn die gesundheit wird bey Doris wieder gr\u00fcnen.", "tokens": ["Wenn", "die", "ge\u00b7sund\u00b7heit", "wird", "bey", "Do\u00b7ris", "wie\u00b7der", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wenn ihr sie curirt, so d\u00e4mpfft ja bald die noth,", "tokens": ["Und", "wenn", "ihr", "sie", "cu\u00b7rirt", ",", "so", "d\u00e4mpfft", "ja", "bald", "die", "noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Macht sie, wie sie vor war, fein appetitlich roth,", "tokens": ["Macht", "sie", ",", "wie", "sie", "vor", "war", ",", "fein", "ap\u00b7pe\u00b7tit\u00b7lich", "roth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "PPER", "PTKVZ", "VAFIN", "$,", "ADJD", "ADJD", "ADJD", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.19": {"text": "Denn bla\u00df seyn will mir noch nicht allerdings behagen,", "tokens": ["Denn", "bla\u00df", "seyn", "will", "mir", "noch", "nicht", "al\u00b7ler\u00b7dings", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Obgleich die m\u00e4dgen sich de\u00dfwegen offt sehr plagen:", "tokens": ["Ob\u00b7gleich", "die", "m\u00e4d\u00b7gen", "sich", "de\u00df\u00b7we\u00b7gen", "offt", "sehr", "pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PRF", "PAV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum sparet keinen flei\u00df, und nehmt sie wohl in acht", "tokens": ["Drum", "spa\u00b7ret", "kei\u00b7nen", "flei\u00df", ",", "und", "nehmt", "sie", "wohl", "in", "acht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ihr das \u00fcbel ihr nicht irgend \u00e4rger macht.", "tokens": ["Da\u00df", "ihr", "das", "\u00fc\u00b7bel", "ihr", "nicht", "ir\u00b7gend", "\u00e4r\u00b7ger", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schlagt den Hippocrates und nebst ihm den Galenen", "tokens": ["Schlagt", "den", "Hip\u00b7po\u00b7cra\u00b7tes", "und", "nebst", "ihm", "den", "Ga\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Bey tag und n\u00e4chten auf, und lernt die noth zu h\u00f6hnen,", "tokens": ["Bey", "tag", "und", "n\u00e4ch\u00b7ten", "auf", ",", "und", "lernt", "die", "noth", "zu", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Die ihre glieder zwingt, ich will erkenntlich seyn,", "tokens": ["Die", "ih\u00b7re", "glie\u00b7der", "zwingt", ",", "ich", "will", "er\u00b7kennt\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Glaubt mir, da\u00df ihr euch solt de\u00dfwegen sehr erfreun.", "tokens": ["Glaubt", "mir", ",", "da\u00df", "ihr", "euch", "solt", "de\u00df\u00b7we\u00b7gen", "sehr", "er\u00b7freun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "VMFIN", "PAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was meynt ihr, ist ihr auch das k\u00fcssen zugelassen,", "tokens": ["Was", "meynt", "ihr", ",", "ist", "ihr", "auch", "das", "k\u00fcs\u00b7sen", "zu\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Darff sie mich auch wohl itzt aus gunst und lieb umfassen,", "tokens": ["Darff", "sie", "mich", "auch", "wohl", "itzt", "aus", "gunst", "und", "lieb", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "APPR", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wofern es nicht ihr schadt, so w\u00e4r es mir sehr lieb,", "tokens": ["Wo\u00b7fern", "es", "nicht", "ihr", "schadt", ",", "so", "w\u00e4r", "es", "mir", "sehr", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn diese kranckheit ist mein rechter freuden-dieb.", "tokens": ["Denn", "die\u00b7se", "kran\u00b7ck\u00b7heit", "ist", "mein", "rech\u00b7ter", "freu\u00b7den\u00b7dieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Jhr oden! schicket euch, ihr solt bald rau\u00df marschieren,", "tokens": ["Ihr", "o\u00b7den", "!", "schi\u00b7cket", "euch", ",", "ihr", "solt", "bald", "rau\u00df", "mar\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jhr madrigal! euch will ich gleichfalls her bald f\u00fchren,", "tokens": ["Ihr", "mad\u00b7ri\u00b7gal", "!", "euch", "will", "ich", "gleich\u00b7falls", "her", "bald", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die kranckheit \u00e4ndert sich, drum komm, o poesie!", "tokens": ["Die", "kran\u00b7ck\u00b7heit", "\u00e4n\u00b7dert", "sich", ",", "drum", "komm", ",", "o", "po\u00b7e\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "PAV", "VVFIN", "$,", "FM", "FM", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Und zahl den \u00e4rtzten hier vor ihre kunst und m\u00fch.", "tokens": ["Und", "zahl", "den", "\u00e4rtz\u00b7ten", "hier", "vor", "ih\u00b7re", "kunst", "und", "m\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich hoffe, da\u00df es wird, ihr Herren! euch vergn\u00fcgen,", "tokens": ["Ich", "hof\u00b7fe", ",", "da\u00df", "es", "wird", ",", "ihr", "Her\u00b7ren", "!", "euch", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn euch zu lobe hier zwey tausend verse fliegen,", "tokens": ["Wenn", "euch", "zu", "lo\u00b7be", "hier", "zwey", "tau\u00b7send", "ver\u00b7se", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVFIN", "ADV", "CARD", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es mangelt mir an geld, poeten sind nicht reich,", "tokens": ["Es", "man\u00b7gelt", "mir", "an", "geld", ",", "po\u00b7e\u00b7ten", "sind", "nicht", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "ADJA", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Darum bezahlt mein geist mit den gedichten euch,", "tokens": ["Da\u00b7rum", "be\u00b7zahlt", "mein", "geist", "mit", "den", "ge\u00b7dich\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und euer ruhm der soll auch nimmermehr ersterben,", "tokens": ["Und", "eu\u00b7er", "ruhm", "der", "soll", "auch", "nim\u00b7mer\u00b7mehr", "ers\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "VMFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Was wolt ihr mehr vor danck vor diese that erwerben?", "tokens": ["Was", "wolt", "ihr", "mehr", "vor", "danck", "vor", "die\u00b7se", "that", "er\u00b7wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "ADV", "APPR", "PDS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}