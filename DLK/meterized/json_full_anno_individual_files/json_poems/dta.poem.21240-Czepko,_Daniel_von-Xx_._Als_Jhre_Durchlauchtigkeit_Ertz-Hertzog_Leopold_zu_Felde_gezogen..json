{"dta.poem.21240": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "Xx .  \n  Als Jhre Durchlauchtigkeit/  \n Ertz-Hertzog Leopold/  \n zu Felde gezogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Da als der kalte Belt/ de\u00df Austers glantz gesehn/", "tokens": ["Da", "als", "der", "kal\u00b7te", "Belt", "/", "de\u00df", "Aus\u00b7ters", "glantz", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wie Er spielt\u2019 vnd warff in Deutschland seine Stralen/", "tokens": ["Vnd", "wie", "Er", "spielt'", "vnd", "warff", "in", "Deutschland", "sei\u00b7ne", "Stra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die voller Frieden bl\u00fchn/ brach Er de\u00df Eisses schalen", "tokens": ["Die", "vol\u00b7ler", "Frie\u00b7den", "bl\u00fchn", "/", "brach", "Er", "de\u00df", "Eis\u00b7ses", "scha\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd hie\u00df den ", "tokens": ["Vnd", "hie\u00df", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Es war schon ", "tokens": ["Es", "war", "schon"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Doch donnert Er sich lo\u00df/ vnd wolt im rauhen krachen/", "tokens": ["Doch", "don\u00b7nert", "Er", "sich", "lo\u00df", "/", "vnd", "wolt", "im", "rau\u00b7hen", "kra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKVZ", "$(", "KON", "VMFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Al\u00df der vor seinem Ei\u00df vnd Eisen starckt geknackt.", "tokens": ["Al\u00df", "der", "vor", "sei\u00b7nem", "Ei\u00df", "vnd", "Ei\u00b7sen", "starckt", "ge\u00b7knackt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN", "KON", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nu ", "tokens": ["Nu"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In dem der Waffen schwall sucht alles einzuschwemmen/", "tokens": ["In", "dem", "der", "Waf\u00b7fen", "schwall", "sucht", "al\u00b7les", "ein\u00b7zu\u00b7schwem\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan eintzig diesen Bruch vnd Sturm in ", "tokens": ["Kan", "eint\u00b7zig", "die\u00b7sen", "Bruch", "vnd", "Sturm", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PDAT", "NN", "KON", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd r\u00fcck et jhnen blo\u00df de\u00df ", "tokens": ["Vnd", "r\u00fcck", "et", "jh\u00b7nen", "blo\u00df", "de\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O HERR/ O Volck! Getrost. Er wil zur Weeser gehn.", "tokens": ["O", "HeRR", "/", "O", "Volck", "!", "Ge\u00b7trost", ".", "Er", "wil", "zur", "Wee\u00b7ser", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "NE", "NN", "$.", "NN", "$.", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie wird nach alter art/ vor seinen Fahnen schlucken.", "tokens": ["Sie", "wird", "nach", "al\u00b7ter", "art", "/", "vor", "sei\u00b7nen", "Fah\u00b7nen", "schlu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dann schawt/ was ", "tokens": ["Dann", "schawt", "/", "was"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$(", "PWS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Vnd dieses/ wie kan es vor ", "tokens": ["Vnd", "die\u00b7ses", "/", "wie", "kan", "es", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "$(", "PWAV", "VMFIN", "PPER", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}