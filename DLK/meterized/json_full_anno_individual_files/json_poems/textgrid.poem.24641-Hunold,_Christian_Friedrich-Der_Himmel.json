{"textgrid.poem.24641": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der erst dem Adler dient/ ist nun zur Sonne kommen.", "tokens": ["Der", "erst", "dem", "Ad\u00b7ler", "dient", "/", "ist", "nun", "zur", "Son\u00b7ne", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$(", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Engell\u00e4ndscher Held/ geht hier ins Engel-land.", "tokens": ["Ein", "En\u00b7gel\u00b7l\u00e4nd\u00b7scher", "Held", "/", "geht", "hier", "ins", "En\u00b7gel\u00b7land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Klagt man/ da\u00df ich zu jung den Printzen weggenommen?", "tokens": ["Klagt", "man", "/", "da\u00df", "ich", "zu", "jung", "den", "Print\u00b7zen", "weg\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$(", "KOUS", "PPER", "PTKA", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach seinen Thaten war Er mir vor alt bekandt.", "tokens": ["Nach", "sei\u00b7nen", "Tha\u00b7ten", "war", "Er", "mir", "vor", "alt", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PPER", "APPR", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Seegen wird hiernechst auf seinem Hause schweben:", "tokens": ["Mein", "See\u00b7gen", "wird", "hier\u00b7nechst", "auf", "sei\u00b7nem", "Hau\u00b7se", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey weisen F\u00fcrsten soll ein Land gl\u00fcckselig seyn.", "tokens": ["Bey", "wei\u00b7sen", "F\u00fcrs\u00b7ten", "soll", "ein", "Land", "gl\u00fcck\u00b7se\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Printz hat gnug gekriegt/ Ihn dessen zu erheben/", "tokens": ["Mein", "Printz", "hat", "gnug", "ge\u00b7kriegt", "/", "Ihn", "des\u00b7sen", "zu", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$(", "PPER", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bring ich Ihn Sieges-voll in meinen Friedenstein.", "tokens": ["Bring", "ich", "Ihn", "Sie\u00b7ges\u00b7voll", "in", "mei\u00b7nen", "Frie\u00b7den\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der erst dem Adler dient/ ist nun zur Sonne kommen.", "tokens": ["Der", "erst", "dem", "Ad\u00b7ler", "dient", "/", "ist", "nun