{"dta.poem.11624": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Das lustige Wirthshaus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Auch seht ihr ja wohl, wie so herrlich das lauft,", "tokens": ["Auch", "seht", "ihr", "ja", "wohl", ",", "wie", "so", "herr\u00b7lich", "das", "lauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "ADV", "ADJD", "PDS", "VVFIN", "$,"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Man denkt, es w\u00e4r Kirme\u00df im Haus;", "tokens": ["Man", "denkt", ",", "es", "w\u00e4r", "Kir\u00b7me\u00df", "im", "Haus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn man uns Betten und St\u00fchle verkauft,", "tokens": ["Und", "wenn", "man", "uns", "Bet\u00b7ten", "und", "St\u00fch\u00b7le", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir lachen die Leute noch aus.", "tokens": ["Wir", "la\u00b7chen", "die", "Leu\u00b7te", "noch", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}