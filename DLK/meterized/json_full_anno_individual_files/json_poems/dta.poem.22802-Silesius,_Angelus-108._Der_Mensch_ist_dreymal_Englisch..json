{"dta.poem.22802": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "108. Der Mensch ist dreymal Englisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Der Thronf\u00fcrst ruht in GOtt: Jhn schaut der Chern-", "tokens": ["Der", "Thron\u00b7f\u00fcrst", "ruht", "in", "Gott", ":", "Jhn", "schaut", "der", "Chern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bin:", "tokens": ["bin", ":"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Ich finde diese Drey in einer Seel allein:", "tokens": ["Ich", "fin\u00b7de", "die\u00b7se", "Drey", "in", "ei\u00b7ner", "Seel", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "CARD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So mu\u00df ein hellger Mensch ja dreyfach Englisch sein.", "tokens": ["So", "mu\u00df", "ein", "hell\u00b7ger", "Mensch", "ja", "drey\u00b7fach", "Eng\u00b7lisch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}