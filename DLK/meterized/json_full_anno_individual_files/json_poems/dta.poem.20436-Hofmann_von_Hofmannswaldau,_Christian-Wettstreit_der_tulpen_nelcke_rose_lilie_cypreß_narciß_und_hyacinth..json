{"dta.poem.20436": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein brunn hat mir den tod durch einen spiegel bracht.", "tokens": ["Ein", "brunn", "hat", "mir", "den", "tod", "durch", "ei\u00b7nen", "spie\u00b7gel", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein/ meine sch\u00f6nheit ist mein todten-gr\u00e4ber worden:", "tokens": ["Nein", "/", "mei\u00b7ne", "sch\u00f6n\u00b7heit", "ist", "mein", "tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "wor\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das auge m\u00fcst das hertz/ das hertz die seel ermorden.", "tokens": ["Das", "au\u00b7ge", "m\u00fcst", "das", "hertz", "/", "das", "hertz", "die", "seel", "er\u00b7mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich sch\u00f6nster von der welt/ bin j\u00e4mmerlich verschmacht/", "tokens": ["Ich", "sch\u00f6ns\u00b7ter", "von", "der", "welt", "/", "bin", "j\u00e4m\u00b7mer\u00b7lich", "ver\u00b7schmacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$(", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als mein holdselig bild ich br\u00fcnstig angelacht/", "tokens": ["Als", "mein", "hold\u00b7se\u00b7lig", "bild", "ich", "br\u00fcns\u00b7tig", "an\u00b7ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verflog mein westen wind in einen schwartzen norden/", "tokens": ["Ver\u00b7flog", "mein", "wes\u00b7ten", "wind", "in", "ei\u00b7nen", "schwart\u00b7zen", "nor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nunmehr ich/ wie man sieht/ verliebter blumen orden", "tokens": ["Nun\u00b7mehr", "ich", "/", "wie", "man", "sieht", "/", "ver\u00b7lieb\u00b7ter", "blu\u00b7men", "or\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$(", "PWAV", "PIS", "VVFIN", "$(", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und meine seuffzer sind zur s\u00fcssen ruh gemacht.", "tokens": ["Und", "mei\u00b7ne", "seuff\u00b7zer", "sind", "zur", "s\u00fcs\u00b7sen", "ruh", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie sch\u00f6n mein rother mund und mein gesicht gewesen/", "tokens": ["Wie", "sch\u00f6n", "mein", "ro\u00b7ther", "mund", "und", "mein", "ge\u00b7sicht", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "VVPP", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bezeugt der purpur-krantz/ der meine stengel ziert:", "tokens": ["Be\u00b7zeugt", "der", "pur\u00b7pur\u00b7krantz", "/", "der", "mei\u00b7ne", "sten\u00b7gel", "ziert", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man kan die jungferschafft auff weissen bl\u00e4ttern lesen/", "tokens": ["Man", "kan", "die", "jung\u00b7fer\u00b7schafft", "auff", "weis\u00b7sen", "bl\u00e4t\u00b7tern", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df mein reiner geist nichts als sich selbst ber\u00fchrt.", "tokens": ["Und", "da\u00df", "mein", "rei\u00b7ner", "geist", "nichts", "als", "sich", "selbst", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PIS", "KOKOM", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die braut war mein gesicht/ der braut-schmuck meine zierde/", "tokens": ["Die", "braut", "war", "mein", "ge\u00b7sicht", "/", "der", "braut\u00b7schmuck", "mei\u00b7ne", "zier\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPOSAT", "VVPP", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das weiche hochzeit-bett ein hertz voller begierde.", "tokens": ["Das", "wei\u00b7che", "hoch\u00b7zeit\u00b7bett", "ein", "hertz", "vol\u00b7ler", "be\u00b7gier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}