{"dta.poem.9199": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "6.  Der Liebsten Zorn.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein schwacher geist/ so schick dich in die trauer/", "tokens": ["Mein", "schwa\u00b7cher", "geist", "/", "so", "schick", "dich", "in", "die", "trau\u00b7er", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein s\u00fcsses kind sieht aus der massen sauer", "tokens": ["Dein", "s\u00fcs\u00b7ses", "kind", "sieht", "aus", "der", "mas\u00b7sen", "sau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder blick/ dardurch sie sich bewegt/", "tokens": ["Ein", "je\u00b7der", "blick", "/", "dar\u00b7durch", "sie", "sich", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$(", "PAV", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist wie ein blitz/ der mich zu boden schl\u00e4gt.", "tokens": ["Ist", "wie", "ein", "blitz", "/", "der", "mich", "zu", "bo\u00b7den", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist di\u00df der lohn vor meine treue liebe/", "tokens": ["Ist", "di\u00df", "der", "lohn", "vor", "mei\u00b7ne", "treu\u00b7e", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich mich nun bi\u00df auf den tod betr\u00fcbe?", "tokens": ["Da\u00df", "ich", "mich", "nun", "bi\u00df", "auf", "den", "tod", "be\u00b7tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu guter nacht du allerliebstes hau\u00df/", "tokens": ["Zu", "gu\u00b7ter", "nacht", "du", "al\u00b7ler\u00b7liebs\u00b7tes", "hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ich seh es wohl/ es ist vor di\u00dfmahl aus.", "tokens": ["Ich", "seh", "es", "wohl", "/", "es", "ist", "vor", "di\u00df\u00b7mahl", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "PPER", "VAFIN", "APPR", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}