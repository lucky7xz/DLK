{"dta.poem.13053": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas er entschlo\u00df, geschah. Ein Strahl des Sonnen Lichts", "tokens": ["\u201e", "was", "er", "ent\u00b7schlo\u00df", ",", "ge\u00b7schah", ".", "Ein", "Strahl", "des", "Son\u00b7nen", "Lichts"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$.", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "470\u201dNahm unterdessen ihm die Kraft des Angesichts,", "tokens": ["\"", "Nahm", "un\u00b7ter\u00b7des\u00b7sen", "ihm", "die", "Kraft", "des", "An\u00b7ge\u00b7sichts", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eso ward in einer Nacht von Ost, West, Nord und S\u00fcden", "tokens": ["\u201e", "so", "ward", "in", "ei\u00b7ner", "Nacht", "von", "Ost", ",", "West", ",", "Nord", "und", "S\u00fc\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "VAFIN", "APPR", "ART", "NN", "APPR", "NE", "$,", "NE", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder gr\u00f6\u00dften Sterne Schaar von ihm zum Rath beschieden.", "tokens": ["\u201e", "der", "gr\u00f6\u00df\u00b7ten", "Ster\u00b7ne", "Schaar", "von", "ihm", "zum", "Rath", "be\u00b7schie\u00b7den", "."], "token_info": ["punct