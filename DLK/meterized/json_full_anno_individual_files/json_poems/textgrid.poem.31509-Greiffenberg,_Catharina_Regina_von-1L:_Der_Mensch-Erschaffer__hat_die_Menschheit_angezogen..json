{"textgrid.poem.31509": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Mensch-Erschaffer / hat die Menschheit angezogen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch-Erschaffer / hat die Menschheit angezogen.", "tokens": ["Der", "Men\u00b7schEr\u00b7schaf\u00b7fer", "/", "hat", "die", "Menschheit", "an\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es sauget an der Brust / der mit den Sternen schafft.", "tokens": ["Es", "sau\u00b7get", "an", "der", "Brust", "/", "der", "mit", "den", "Ster\u00b7nen", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es h\u00fcngert / selbst das Brod: es d\u00fcrst / den Lebens-Safft.", "tokens": ["Es", "h\u00fcn\u00b7gert", "/", "selbst", "das", "Brod", ":", "es", "d\u00fcrst", "/", "den", "Le\u00b7bens\u00b7\u00b7S\u00b7afft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ART", "NN", "$.", "PPER", "ADV", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Das Vrliecht selber / schl\u00e4fft. Der Weg / wird selbst bewogen.", "tokens": ["Das", "Vr\u00b7liecht", "sel\u00b7ber", "/", "schl\u00e4fft", ".", "Der", "Weg", "/", "wird", "selbst", "be\u00b7wo\u00b7gen", "."], "token_inf