{"textgrid.poem.47045": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh um dich, meine sch\u00f6ne scheue Taube,", "tokens": ["Sieh", "um", "dich", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "scheu\u00b7e", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es steht der Wald in seinen blo\u00dfen Haaren,", "tokens": ["Es", "steht", "der", "Wald", "in", "sei\u00b7nen", "blo\u00b7\u00dfen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft mutig Wind und Sonnschein dr\u00fcber fahren,", "tokens": ["L\u00e4\u00dft", "mu\u00b7tig", "Wind", "und", "Sonn\u00b7schein", "dr\u00fc\u00b7ber", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und birgt nicht seinen Schmuck in einer Haube.", "tokens": ["Und", "birgt", "nicht", "sei\u00b7nen", "Schmuck", "in", "ei\u00b7ner", "Hau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was willst du deines Hauptes Bl\u00fctenlaube,", "tokens": ["Was", "willst", "du", "dei\u00b7nes", "Haup\u00b7tes", "Bl\u00fc\u00b7ten\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den jungen Wald im Saft von sechzehn Jahren,", "tokens": ["Den", "jun\u00b7gen", "Wald", "im", "Saft", "von", "sech\u00b7zehn", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch unter einem andern Dach verwahren?", "tokens": ["Noch", "un\u00b7ter", "ei\u00b7nem", "an\u00b7dern", "Dach", "ver\u00b7wah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gib mir sein Dach, das Haar dem Wind zum Raube!", "tokens": ["Gib", "mir", "sein", "Dach", ",", "das", "Haar", "dem", "Wind", "zum", "Rau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich tr\u00e4umte j\u00fcngst, ich s\u00e4he zartgewoben", "tokens": ["Ich", "tr\u00e4um\u00b7te", "j\u00fcngst", ",", "ich", "s\u00e4\u00b7he", "zart\u00b7ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als goldnes Hemde wallen dein Gelocke", "tokens": ["Als", "gold\u00b7nes", "Hem\u00b7de", "wal\u00b7len", "dein", "Ge\u00b7lo\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Haupt zum Fu\u00df dir h\u00fcllend alle Glieder.", "tokens": ["Vom", "Haupt", "zum", "Fu\u00df", "dir", "h\u00fcl\u00b7lend", "al\u00b7le", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PPER", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wird das zur H\u00e4lfte wahr, so will ich's loben,", "tokens": ["Wird", "das", "zur", "H\u00e4lf\u00b7te", "wahr", ",", "so", "will", "ich's", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "APPRART", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du das Haargeweb', wo nicht zum Rocke,", "tokens": ["Wenn", "du", "das", "Haar\u00b7ge\u00b7web'", ",", "wo", "nicht", "zum", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PWAV", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir l\u00e4ssest dienen mindestens zum Mieder.", "tokens": ["Dir", "l\u00e4s\u00b7sest", "die\u00b7nen", "min\u00b7des\u00b7tens", "zum", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh um dich, meine sch\u00f6ne scheue Taube,", "tokens": ["Sieh", "um", "dich", ",", "mei\u00b7ne", "sch\u00f6\u00b7ne", "scheu\u00b7e", "Tau\u00b7be", ","], "token_info