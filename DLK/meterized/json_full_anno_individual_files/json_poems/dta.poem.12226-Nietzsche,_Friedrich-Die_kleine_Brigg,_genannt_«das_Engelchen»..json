{"dta.poem.12226": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die kleine Brigg, genannt \u00abdas Engelchen\u00bb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Jetzt ein Schiff, dereinst ein M\u00e4dchen,", "tokens": ["Jetzt", "ein", "Schiff", ",", "de\u00b7reinst", "ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Ach, noch immer sehr ein M\u00e4dchen!", "tokens": ["Ach", ",", "noch", "im\u00b7mer", "sehr", "ein", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Denn es dreht um Liebe sich", "tokens": ["Denn", "es", "dreht", "um", "Lie\u00b7be", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         St\u00e4ts mein feines Steuerr\u00e4dchen.", "tokens": ["St\u00e4ts", "mein", "fei\u00b7nes", "Steu\u00b7er\u00b7r\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Bin geschm\u00fcckt mit hundert F\u00e4hnchen,", "tokens": ["Bin", "ge\u00b7schm\u00fcckt", "mit", "hun\u00b7dert", "F\u00e4hn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Und das sch\u00f6nste Kapit\u00e4nchen", "tokens": ["Und", "das", "sch\u00f6ns\u00b7te", "Ka\u00b7pi\u00b7t\u00e4n\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Bl\u00e4ht an meinem Steuer sich,", "tokens": ["Bl\u00e4ht", "an", "mei\u00b7nem", "Steu\u00b7er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Als das hundert erste F\u00e4hnchen.", "tokens": ["Als", "das", "hun\u00b7dert", "ers\u00b7te", "F\u00e4hn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "CARD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "             Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Ueberall hin, wo ein Fl\u00e4mmchen", "tokens": ["Ue\u00b7be\u00b7rall", "hin", ",", "wo", "ein", "Fl\u00e4mm\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             F\u00fcr mich gl\u00fcht, lauf ich ein L\u00e4mmchen", "tokens": ["F\u00fcr", "mich", "gl\u00fcht", ",", "lauf", "ich", "ein", "L\u00e4mm\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Meinen Weg sehns\u00fcchtiglich:", "tokens": ["Mei\u00b7nen", "Weg", "sehn\u00b7s\u00fcch\u00b7tig\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Immer war ich solch ein L\u00e4mmchen.", "tokens": ["Im\u00b7mer", "war", "ich", "solch", "ein", "L\u00e4mm\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "             Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Glaubt ihr wohl, dass wie ein H\u00fcndchen", "tokens": ["Glaubt", "ihr", "wohl", ",", "dass", "wie", "ein", "H\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Bell\u2019n ich kann und dass mein M\u00fcndchen", "tokens": ["Bell'n", "ich", "kann", "und", "dass", "mein", "M\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "KON", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Dampf und Feuer wirft um sich?", "tokens": ["Dampf", "und", "Feu\u00b7er", "wirft", "um", "sich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Ach, des Teufels ist mein M\u00fcndchen!", "tokens": ["Ach", ",", "des", "Teu\u00b7fels", "ist", "mein", "M\u00fcnd\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "             Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Sprach ein bitterb\u00f6ses W\u00f6rtchen", "tokens": ["Sprach", "ein", "bit\u00b7ter\u00b7b\u00f6\u00b7ses", "W\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             Einst, dass schnell zum letzten Oertchen", "tokens": ["Einst", ",", "dass", "schnell", "zum", "letz\u00b7ten", "O\u00b7ert\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADJD", "APPRART", "ADJA", "NN"], "meter": "--+-+----", "measure": "anapaest.init"}, "line.4": {"text": "             Mein Geliebtester entwich:", "tokens": ["Mein", "Ge\u00b7lieb\u00b7tes\u00b7ter", "ent\u00b7wich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Ja, er starb an diesem W\u00f6rtchen!", "tokens": ["Ja", ",", "er", "starb", "an", "die\u00b7sem", "W\u00f6rt\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "             Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Kaum geh\u00f6rt, sprang ich vom Klippchen", "tokens": ["Kaum", "ge\u00b7h\u00f6rt", ",", "sprang", "ich", "vom", "Klipp\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Grund und brach ein Rippchen,", "tokens": ["In", "den", "Grund", "und", "brach", "ein", "Ripp\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Dass die liebe Seele wich:", "tokens": ["Dass", "die", "lie\u00b7be", "See\u00b7le", "wich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Ja, sie wich durch dieses Rippchen!", "tokens": ["Ja", ",", "sie", "wich", "durch", "die\u00b7ses", "Ripp\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "             Engelchen: so nennt man mich \u2014", "tokens": ["En\u00b7gel\u00b7chen", ":", "so", "nennt", "man", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "PIS", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "             Meine Seele, wie ein K\u00e4tzchen,", "tokens": ["Mei\u00b7ne", "See\u00b7le", ",", "wie", "ein", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "             That eins, zwei, drei, vier, f\u00fcnf S\u00e4tzchen,", "tokens": ["That", "eins", ",", "zwei", ",", "drei", ",", "vier", ",", "f\u00fcnf", "S\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "             Schwang dann in dies Schiffchen sich \u2014", "tokens": ["Schwang", "dann", "in", "dies", "Schiff\u00b7chen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PDS", "NN", "PRF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "         Ja, sie hat geschwinde T\u00e4tzchen.", "tokens": ["Ja", ",", "sie", "hat", "ge\u00b7schwin\u00b7de", "T\u00e4tz\u00b7chen", "."], "token_info": ["word", "punct", 