{"textgrid.poem.50607": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der saure Thraso schl\u00e4gt durch blo\u00dfes Ansehn wund:", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der saure Thraso schl\u00e4gt durch blo\u00dfes Ansehn wund:", "tokens": ["Der", "sau\u00b7re", "Thra\u00b7so", "schl\u00e4gt", "durch", "blo\u00b7\u00dfes", "An\u00b7sehn", "wund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schwarzer Stutzbart ziert den aufgeschwollnen Mund;", "tokens": ["Ein", "schwar\u00b7zer", "Stutz\u00b7bart", "ziert", "den", "auf\u00b7ge\u00b7schwoll\u00b7nen", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er tr\u00e4gt ein langes Schwert und ein dick spanisch Rohr,", "tokens": ["Er", "tr\u00e4gt", "ein", "lan\u00b7ges", "Schwert", "und", "ein", "dick", "spa\u00b7nisch", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Feder auf dem Hut, den Hut auf einem Ohr;", "tokens": ["Die", "Fe\u00b7der", "auf", "dem", "Hut", ",", "den", "Hut", "auf", "ei\u00b7nem", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rock, Hosen, Wamms und Str\u00fcmpf' und Alles mu\u00df sich gatten;", "tokens": ["Rock", ",", "Ho\u00b7sen", ",", "Wamms", "und", "Str\u00fcmpf'", "und", "Al\u00b7les", "mu\u00df", "sich", "gat\u00b7ten", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "KON", "PIS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er sieht so grausam aus, da\u00df er sich selbst kaum traut,", "tokens": ["Er", "sieht", "so", "grau\u00b7sam", "aus", ",", "da\u00df", "er", "sich", "selbst", "kaum", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df, wenn er ungef\u00e4hr in einen Spiegel schaut,", "tokens": ["Da\u00df", ",", "wenn", "er", "un\u00b7ge\u00b7f\u00e4hr", "in", "ei\u00b7nen", "Spie\u00b7gel", "schaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er selber sich entsetzt vor seinem eignen Schatten.", "tokens": ["Er", "sel\u00b7ber", "sich", "ent\u00b7setzt", "vor", "sei\u00b7nem", "eig\u00b7nen", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PRF", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"