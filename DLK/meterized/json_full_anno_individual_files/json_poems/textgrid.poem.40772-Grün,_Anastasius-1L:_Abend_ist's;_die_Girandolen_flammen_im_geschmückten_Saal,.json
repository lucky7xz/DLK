{"textgrid.poem.40772": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abend ist's; die Girandolen flammen im geschm\u00fcckten Saal,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abend ist's; die Girandolen flammen im geschm\u00fcckten Saal,", "tokens": ["A\u00b7bend", "ist's", ";", "die", "Gi\u00b7ran\u00b7do\u00b7len", "flam\u00b7men", "im", "ge\u00b7schm\u00fcck\u00b7ten", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Im Kristall der hohen Spiegel quillt vertausendfacht ihr Strahl,", "tokens": ["Im", "Kris\u00b7tall", "der", "ho\u00b7hen", "Spie\u00b7gel", "quillt", "ver\u00b7tau\u00b7send\u00b7facht", "ihr", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "In dem Glanzmeer rings bewegen, schwebend fast und feierlich,", "tokens": ["In", "dem", "Glanz\u00b7meer", "rings", "be\u00b7we\u00b7gen", ",", "schwe\u00b7bend", "fast", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Altehrw\u00fcrdige Matronen, junge sch\u00f6ne Damen sich.", "tokens": ["Al\u00b7tehr\u00b7w\u00fcr\u00b7di\u00b7ge", "Mat\u00b7ro\u00b7nen", ",", "jun\u00b7ge", "sch\u00f6\u00b7ne", "Da\u00b7men", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Und dazwischen zieh'n gemessen, schmuck im Glanze des Ornats,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "zieh'n", "ge\u00b7mes\u00b7sen", ",", "schmuck", "im", "Glan\u00b7ze", "des", "Or\u00b7nats", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "VVPP", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hier des Krieges rauhe S\u00f6hne, Friedensdiener dort des Staats;", "tokens": ["Hier", "des", "Krie\u00b7ges", "rau\u00b7he", "S\u00f6h\u00b7ne", ",", "Frie\u00b7dens\u00b7die\u00b7ner", "dort", "des", "Staats", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber Einen seh' ich wandeln, jeder Blick folgt seiner Bahn,", "tokens": ["A\u00b7ber", "Ei\u00b7nen", "seh'", "ich", "wan\u00b7deln", ",", "je\u00b7der", "Blick", "folgt", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch nur wenig der Erkor'nen sind's, die's wagen, ihm zu nah'n.", "tokens": ["Doch", "nur", "we\u00b7nig", "der", "Er\u00b7kor'\u00b7nen", "sin\u00b7d's", ",", "die's", "wa\u00b7gen", ",", "ihm", "zu", "nah'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "PIS", "ART", "NN", "VAFIN", "$,", "PDS", "VVINF", "$,", "PPER", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Er ist's, der das r\u00fcst'ge Prachtschiff Austria am Steuer lenkt,", "tokens": ["Er", "ist's", ",", "der", "das", "r\u00fcst'\u00b7ge", "Pracht\u00b7schiff", "Aust\u00b7ria", "am", "Steu\u00b7er", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "ADJA", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er, der im Congre\u00df der F\u00fcrsten f\u00fcr sie handelt, f\u00fcr sie denkt;", "tokens": ["Er", ",", "der", "im", "Con\u00b7gre\u00df", "der", "F\u00fcrs\u00b7ten", "f\u00fcr", "sie", "han\u00b7delt", ",", "f\u00fcr", "sie", "denkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Doch seht jetzt ihn! wie bescheiden, wie so artig, wie so fein!", "tokens": ["Doch", "seht", "jetzt", "ihn", "!", "wie", "be\u00b7schei\u00b7den", ",", "wie", "so", "ar\u00b7tig", ",", "wie", "so", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "$.", "PWAV", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$,", "PWAV", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Wie manierlich gegen Alle, h\u00f6flich gegen Gro\u00df und Klein!", "tokens": ["Wie", "ma\u00b7nier\u00b7lich", "ge\u00b7gen", "Al\u00b7le", ",", "h\u00f6f\u00b7lich", "ge\u00b7gen", "Gro\u00df", "und", "Klein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PIS", "$,", "ADJD", "APPR", "NE", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.4": {"line.1": {"text": "Seines Kleides Sterne funkeln karg und l\u00e4ssig fast im Licht,", "tokens": ["Sei\u00b7nes", "Klei\u00b7des", "Ster\u00b7ne", "fun\u00b7keln", "karg", "und", "l\u00e4s\u00b7sig", "fast", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber freundlich mildes L\u00e4cheln schwebt ihm stets ums Angesicht,", "tokens": ["A\u00b7ber", "freund\u00b7lich", "mil\u00b7des", "L\u00e4\u00b7cheln", "schwebt", "ihm", "stets", "ums", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn von einem sch\u00f6nen Busen Rosenbl\u00e4tter jetzt er pfl\u00fcckt,", "tokens": ["Wenn", "von", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Bu\u00b7sen", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "jetzt", "er", "pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Oder wenn, wie welke Blumen, K\u00f6nigreiche er zerst\u00fcckt.", "tokens": ["O\u00b7der", "wenn", ",", "wie", "wel\u00b7ke", "Blu\u00b7men", ",", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "er", "zer\u00b7st\u00fcckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "PWAT", "NN", "$,", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Gleich bezaubernd klingt's, wenn zierlich goldne Locken jetzt er preist,", "tokens": ["Gleich", "be\u00b7zau\u00b7bernd", "klingt's", ",", "wenn", "zier\u00b7lich", "gold\u00b7ne", "Lo\u00b7cken", "jetzt", "er", "preist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$,", "KOUS", "ADJD", "ADJA", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Oder wenn er K\u00f6nigskronen von gesalbten H\u00e4uptern rei\u00dft;", "tokens": ["O\u00b7der", "wenn", "er", "K\u00f6\u00b7nigs\u00b7kro\u00b7nen", "von", "ge\u00b7salb\u00b7ten", "H\u00e4up\u00b7tern", "rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ja fast d\u00fcnkt's mich Himmelswonne, die den sel'gen Mann begl\u00fcckt,", "tokens": ["Ja", "fast", "d\u00fcnkt's", "mich", "Him\u00b7mels\u00b7won\u00b7ne", ",", "die", "den", "sel'\u00b7gen", "Mann", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Den sein Wort auf Elba's Felsen, den's in Munkats' Kerker schickt!", "tokens": ["Den", "sein", "Wort", "auf", "El\u00b7ba's", "Fel\u00b7sen", ",", "den's", "in", "Mun\u00b7kats'", "Ker\u00b7ker", "schickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NE", "NN", "$,", "PRELS", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "K\u00f6nnt' Europa jetzt ihn sehen, so verbindlich, so galant,", "tokens": ["K\u00f6nnt'", "Eu\u00b7ro\u00b7pa", "jetzt", "ihn", "se\u00b7hen", ",", "so", "ver\u00b7bind\u00b7lich", ",", "so", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NE", "ADV", "PPER", "VVINF", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie der Kirche frommer Priester, wie der Mann im Kriegsgewand,", "tokens": ["Wie", "der", "Kir\u00b7che", "from\u00b7mer", "Pries\u00b7ter", ",", "wie", "der", "Mann", "im", "Kriegs\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie des Staats besternter Diener ganz von seiner Huld begl\u00fcckt,", "tokens": ["Wie", "des", "Staats", "bes\u00b7tern\u00b7ter", "Die\u00b7ner", "ganz", "von", "sei\u00b7ner", "Huld", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Und die Damen, alt' und junge, erst bezaubert und entz\u00fcckt!", "tokens": ["Und", "die", "Da\u00b7men", ",", "alt'", "und", "jun\u00b7ge", ",", "erst", "be\u00b7zau\u00b7bert", "und", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJA", "KON", "ADJA", "$,", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Mann des Staates, Mann des Rathes! da du just bei Laune bist,", "tokens": ["Mann", "des", "Staa\u00b7tes", ",", "Mann", "des", "Ra\u00b7thes", "!", "da", "du", "just", "bei", "Lau\u00b7ne", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$.", "KOUS", "PPER", "VVFIN", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da du gegen Alle gn\u00e4dig \u00fcberaus zu dieser Frist;", "tokens": ["Da", "du", "ge\u00b7gen", "Al\u00b7le", "gn\u00e4\u00b7dig", "\u00fc\u00b7be\u00b7raus", "zu", "die\u00b7ser", "Frist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "ADJD", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sieh, vor deiner Th\u00fcre drau\u00dfen harrt ein d\u00fcrftiger Client,", "tokens": ["Sieh", ",", "vor", "dei\u00b7ner", "Th\u00fc\u00b7re", "drau\u00b7\u00dfen", "harrt", "ein", "d\u00fcrf\u00b7ti\u00b7ger", "Client", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Der durch Winke deiner Gnade hochbegl\u00fcckt zu werden brennt.", "tokens": ["Der", "durch", "Win\u00b7ke", "dei\u00b7ner", "Gna\u00b7de", "hoch\u00b7be\u00b7gl\u00fcckt", "zu", "wer\u00b7den", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVPP", "PTKZU", "VAINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Brauchst dich nicht vor ihm zu f\u00fcrchten; er ist artig und gescheidt,", "tokens": ["Brauchst", "dich", "nicht", "vor", "ihm", "zu", "f\u00fcrch\u00b7ten", ";", "er", "ist", "ar\u00b7tig", "und", "ge\u00b7scheidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tr\u00e4gt auch keinen Dolch verborgen unter seinem schlichten Kleid;", "tokens": ["Tr\u00e4gt", "auch", "kei\u00b7nen", "Dolch", "ver\u00b7bor\u00b7gen", "un\u00b7ter", "sei\u00b7nem", "schlich\u00b7ten", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Oestreich's Volk ist's, ehrlich, offen, wohlerzogen auch und fein,", "tokens": ["O\u00b7e\u00b7streich's", "Volk", "ist's", ",", "ehr\u00b7lich", ",", "of\u00b7fen", ",", "woh\u00b7ler\u00b7zo\u00b7gen", "auch", "und", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "$,", "ADJD", "$,", "ADJD", "$,", "PWAV", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Sieh, es fleht ganz artig: D\u00fcrft' ich wohl so frei sein, frei zu sein?", "tokens": ["Sieh", ",", "es", "fleht", "ganz", "ar\u00b7tig", ":", "D\u00fcrft'", "ich", "wohl", "so", "frei", "sein", ",", "frei", "zu", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$.", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Abend ist's; die Girandolen flammen im geschm\u00fcckten Saal,", "tokens": ["A\u00b7bend", "ist's", ";", "die", "Gi\u00b7ran\u00b7do\u00b7len", "flam\u00b7men", "im", "ge\u00b7schm\u00fcck\u00b7ten", "Saal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Im Kristall der hohen Spiegel quillt vertausendfacht ihr Strahl,", "tokens": ["Im", "Kris\u00b7tall", "der", "ho\u00b7hen", "Spie\u00b7gel", "quillt", "ver\u00b7tau\u00b7send\u00b7facht", "ihr", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "In dem Glanzmeer rings bewegen, schwebend fast und feierlich,", "tokens": ["In", "dem", "Glanz\u00b7meer", "rings", "be\u00b7we\u00b7gen", ",", "schwe\u00b7bend", "fast", "und", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,", "ADJD", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Altehrw\u00fcrdige Matronen, junge sch\u00f6ne Damen sich.", "tokens": ["Al\u00b7tehr\u00b7w\u00fcr\u00b7di\u00b7ge", "Mat\u00b7ro\u00b7nen", ",", "jun\u00b7ge", "sch\u00f6\u00b7ne", "Da\u00b7men", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Und dazwischen zieh'n gemessen, schmuck im Glanze des Ornats,", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "zieh'n", "ge\u00b7mes\u00b7sen", ",", "schmuck", "im", "Glan\u00b7ze", "des", "Or\u00b7nats", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "VVPP", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Hier des Krieges rauhe S\u00f6hne, Friedensdiener dort des Staats;", "tokens": ["Hier", "des", "Krie\u00b7ges", "rau\u00b7he", "S\u00f6h\u00b7ne", ",", "Frie\u00b7dens\u00b7die\u00b7ner", "dort", "des", "Staats", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber Einen seh' ich wandeln, jeder Blick folgt seiner Bahn,", "tokens": ["A\u00b7ber", "Ei\u00b7nen", "seh'", "ich", "wan\u00b7deln", ",", "je\u00b7der", "Blick", "folgt", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch nur wenig der Erkor'nen sind's, die's wagen, ihm zu nah'n.", "tokens": ["Doch", "nur", "we\u00b7nig", "der", "Er\u00b7kor'\u00b7nen", "sin\u00b7d's", ",", "die's", "wa\u00b7gen", ",", "ihm", "zu", "nah'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "PIS", "ART", "NN", "VAFIN", "$,", "PDS", "VVINF", "$,", "PPER", "APPR", "ADJA", "NN"], "meter": "--+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "Er ist's, der das r\u00fcst'ge Prachtschiff Austria am Steuer lenkt,", "tokens": ["Er", "ist's", ",", "der", "das", "r\u00fcst'\u00b7ge", "Pracht\u00b7schiff", "Aust\u00b7ria", "am", "Steu\u00b7er", "lenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ART", "ADJA", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er, der im Congre\u00df der F\u00fcrsten f\u00fcr sie handelt, f\u00fcr sie denkt;", "tokens": ["Er", ",", "der", "im", "Con\u00b7gre\u00df", "der", "F\u00fcrs\u00b7ten", "f\u00fcr", "sie", "han\u00b7delt", ",", "f\u00fcr", "sie", "denkt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Doch seht jetzt ihn! wie bescheiden, wie so artig, wie so fein!", "tokens": ["Doch", "seht", "jetzt", "ihn", "!", "wie", "be\u00b7schei\u00b7den", ",", "wie", "so", "ar\u00b7tig", ",", "wie", "so", "fe