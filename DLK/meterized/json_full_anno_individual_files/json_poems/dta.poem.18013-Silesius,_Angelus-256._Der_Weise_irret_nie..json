{"dta.poem.18013": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "256.  Der Weise irret nie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weise geht nie irr/ er h\u00e4ngt auf jeder Bahn/", "tokens": ["Der", "Wei\u00b7se", "geht", "nie", "irr", "/", "er", "h\u00e4ngt", "auf", "je\u00b7der", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$(", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Ewgen Wahrheit (GOtt) mit allen kr\u00e4fften an.", "tokens": ["Der", "Ew\u00b7gen", "Wahr\u00b7heit", "(", "Gott", ")", "mit", "al\u00b7len", "kr\u00e4ff\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$(", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}