{"textgrid.poem.25071": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Aurelia", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geflohn hab' ich den gelben Tiber,", "tokens": ["Ge\u00b7flohn", "hab'", "ich", "den", "gel\u00b7ben", "Ti\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dich, o Weib, das mich betrog,", "tokens": ["Und", "dich", ",", "o", "Weib", ",", "das", "mich", "be\u00b7trog", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Liebe mir, ein gl\u00fchend Fieber,", "tokens": ["Als", "Lie\u00b7be", "mir", ",", "ein", "gl\u00fc\u00b7hend", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Mark des Lebens sog.", "tokens": ["Am", "Mark", "des", "Le\u00b7bens", "so\u00b7g."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Doch, ob uns Himmelweiten trennen,", "tokens": ["Doch", ",", "ob", "uns", "Him\u00b7mel\u00b7wei\u00b7ten", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch klopft mein Herz mit wildem Schlag,", "tokens": ["Noch", "klopft", "mein", "Herz", "mit", "wil\u00b7dem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hei\u00df die Wange f\u00fchl' ich brennen,", "tokens": ["Und", "hei\u00df", "die", "Wan\u00b7ge", "f\u00fchl'", "ich", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie an dem Scheidetag.", "tokens": ["Wie", "an", "dem", "Schei\u00b7de\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der schwarzen Augen sengend Feuer \u2013", "tokens": ["Der", "schwar\u00b7zen", "Au\u00b7gen", "sen\u00b7gend", "Feu\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Woll\u00fcstig wallt durch Geist und Sinn", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "wallt", "durch", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mir noch von ihm ein immer neuer", "tokens": ["Mir", "noch", "von", "ihm", "ein", "im\u00b7mer", "neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPER", "ART", "ADV", "ADJA"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Glutstrom entnervend hin.", "tokens": ["Glut\u00b7strom", "ent\u00b7ner\u00