{"dta.poem.2128": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Viola Mariana.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wird man der wirkenden Natur", "tokens": ["Wird", "man", "der", "wir\u00b7ken\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In ihren innern Wirkungen doch nimmer auf die wahre", "tokens": ["In", "ih\u00b7ren", "in\u00b7nern", "Wir\u00b7kun\u00b7gen", "doch", "nim\u00b7mer", "auf", "die", "wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Spur", "tokens": ["Spur"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Mit unserm Geist gerathen k\u00f6nnen.", "tokens": ["Mit", "un\u00b7serm", "Geist", "ge\u00b7ra\u00b7then", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Genug f\u00fcr uns, wenn wir erkennen", "tokens": ["Ge\u00b7nug", "f\u00fcr", "uns", ",", "wenn", "wir", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Daseyn eines weisen Wesens, das stets nach Ord-", "tokens": ["Das", "Da\u00b7seyn", "ei\u00b7nes", "wei\u00b7sen", "We\u00b7sens", ",", "das", "stets", "nach", "Ord"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "nung, Maa\u00df, Gewicht", "tokens": ["nung", ",", "Maa\u00df", ",", "Ge\u00b7wicht"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ADV", "$,", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die viele Millionen Wunder, die es erschaffen, zuge-", "tokens": ["Die", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "Wun\u00b7der", ",", "die", "es", "er\u00b7schaf\u00b7fen", ",", "zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "TRUNC"], "meter": "-+--+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "richt,", "tokens": ["richt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Und dessen Weisheit, Lieb und Macht, im Gro\u00dfen so, wie", "tokens": ["Und", "des\u00b7sen", "Weis\u00b7heit", ",", "Lieb", "und", "Macht", ",", "im", "Gro\u00b7\u00dfen", "so", ",", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PRELAT", "NN", "$,", "NN", "KON", "NN", "$,", "APPRART", "NN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "in dem Kleinen,", "tokens": ["in", "dem", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.12": {"text": "In einer unleugbaren Klarheit, so herrlich \u00fcberall er-", "tokens": ["In", "ei\u00b7ner", "un\u00b7leug\u00b7ba\u00b7ren", "Klar\u00b7heit", ",", "so", "herr\u00b7lich", "\u00fc\u00b7be\u00b7rall", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.13": {"text": "scheinen.", "tokens": ["schei\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}