{"dta.poem.18096": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Als ihm seine Asterie geschrieben.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sollte dieses wol in sein Gem\u00fcthe bringen,", "tokens": ["Wer", "soll\u00b7te", "die\u00b7ses", "wol", "in", "sein", "Ge\u00b7m\u00fc\u00b7the", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unter wei\u00df und schwartz verborgen solche Freud?", "tokens": ["Da\u00df", "un\u00b7ter", "wei\u00df", "und", "schwartz", "ver\u00b7bor\u00b7gen", "sol\u00b7che", "Freud", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "VVFIN", "KON", "ADJD", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df nur ein einig Brieff nem alle Traurigkeit?", "tokens": ["Da\u00df", "nur", "ein", "ei\u00b7nig", "Brieff", "nem", "al\u00b7le", "Trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan auch der Augenlust so weit ins Hertze dringen?", "tokens": ["Kan", "auch", "der", "Au\u00b7gen\u00b7lust", "so", "weit", "ins", "Hert\u00b7ze", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "ADJD", "APPRART", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df die Sinne fast nicht h\u00f6her mehr zu schwingen,", "tokens": ["Ich", "wei\u00df", "die", "Sin\u00b7ne", "fast", "nicht", "h\u00f6\u00b7her", "mehr", "zu", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und habe wol mit Flei\u00df gelesen jederzeit,", "tokens": ["Und", "ha\u00b7be", "wol", "mit", "Flei\u00df", "ge\u00b7le\u00b7sen", "je\u00b7der\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "NN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was von der Liebe nur gefunden weit und breit,", "tokens": ["Was", "von", "der", "Lie\u00b7be", "nur", "ge\u00b7fun\u00b7den", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VVPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es hat mich aber nichts vermocht so sehr zu zwingen;", "tokens": ["Es", "hat", "mich", "a\u00b7ber", "nichts", "ver\u00b7mocht", "so", "sehr", "zu", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIS", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Griech Anacreon, der Sappho sch\u00f6n Gedicht,", "tokens": ["Der", "Griech", "A\u00b7na\u00b7cre\u00b7on", ",", "der", "Sap\u00b7pho", "sch\u00f6n", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und auch Ovidius sind ihm zu gleichen nicht,", "tokens": ["Und", "auch", "O\u00b7vi\u00b7dius", "sind", "ihm", "zu", "glei\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "PPER", "APPR", "ADJA", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Der k\u00fcnstlich Amadis ist nie so hoch gegangen.", "tokens": ["Der", "k\u00fcnst\u00b7lich", "A\u00b7ma\u00b7dis", "ist", "nie", "so", "hoch", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gl\u00fcckselig ist die Hand, die diesen Brief gemacht,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "ist", "die", "Hand", ",", "die", "die\u00b7sen", "Brief", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gl\u00fcckselig ich die Dint und auch die Feder acht,", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "ich", "die", "Dint", "und", "auch", "die", "Fe\u00b7der", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "KON", "ADV", "ART", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und mehr gl\u00fcckselig mich, der ich ihn hab empfangen.", "tokens": ["Und", "mehr", "gl\u00fcck\u00b7se\u00b7lig", "mich", ",", "der", "ich", "ihn", "hab", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "$,", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}