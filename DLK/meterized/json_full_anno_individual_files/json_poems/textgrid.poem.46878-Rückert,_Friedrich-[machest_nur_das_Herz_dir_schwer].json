{"textgrid.poem.46878": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[machest nur das Herz dir schwer]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Machest nur das Herz dir schwer,", "tokens": ["Ma\u00b7chest", "nur", "das", "Herz", "dir", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du sinnest hin und her,", "tokens": ["Wenn", "du", "sin\u00b7nest", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was wir h\u00e4tten m\u00f6gen thun", "tokens": ["Was", "wir", "h\u00e4t\u00b7ten", "m\u00f6\u00b7gen", "thun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "VMFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andres, um am Leben nun", "tokens": ["And\u00b7res", ",", "um", "am", "Le\u00b7ben", "nun"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUI", "APPRART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Leben noch zu haben,", "tokens": ["Un\u00b7ser", "Le\u00b7ben", "noch", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das wir m\u00fcssen jetzt begraben.", "tokens": ["Das", "wir", "m\u00fcs\u00b7sen", "jetzt", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word