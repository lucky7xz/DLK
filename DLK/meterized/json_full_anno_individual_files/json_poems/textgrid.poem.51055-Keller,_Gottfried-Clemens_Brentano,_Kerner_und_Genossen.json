{"textgrid.poem.51055": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Clemens Brentano, Kerner und Genossen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas sind das f\u00fcr possierliche Gesellen", "tokens": ["\u00bb", "was", "sind", "das", "f\u00fcr", "pos\u00b7sier\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In wei\u00dfen Laken und mit R\u00e4ucherpfannen?", "tokens": ["In", "wei\u00b7\u00dfen", "La\u00b7ken", "und", "mit", "R\u00e4u\u00b7ch\u00b7er\u00b7pfan\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ob sie nach Sch\u00e4tzen graben? Geister bannen?", "tokens": ["Ob", "sie", "nach", "Sch\u00e4t\u00b7zen", "gra\u00b7ben", "?", "Geis\u00b7ter", "ban\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "$.", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie lassen sonderbare T\u00f6ne gellen!", "tokens": ["Sie", "las\u00b7sen", "son\u00b7der\u00b7ba\u00b7re", "T\u00f6\u00b7ne", "gel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sahst du dem einen rotes Blut entquellen,", "tokens": ["Sahst", "du", "dem", "ei\u00b7nen", "ro\u00b7tes", "Blut", "ent\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indes dem andern gro\u00dfe Tr\u00e4nen rannen?", "tokens": ["In\u00b7des", "dem", "an\u00b7dern", "gro\u00b7\u00dfe", "Tr\u00e4\u00b7nen", "ran\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie huschen sacht, gespensterhaft von dannen", "tokens": ["Sie", "hu\u00b7schen", "sacht", ",", "ge\u00b7spens\u00b7ter\u00b7haft", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf dieser Zeiten grundemp\u00f6rten Wellen.", "tokens": ["Auf", "die\u00b7ser", "Zei\u00b7ten", "grun\u00b7dem\u00b7p\u00f6r\u00b7ten", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auch scheinen Schild' und Schwerter sie zu tragen", "tokens": ["Auch", "schei\u00b7nen", "Schild'", "und", "Schwer\u00b7ter", "sie", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Holz und um die Stirn ein d\u00fcrr Geflecht", "tokens": ["Von", "Holz", "und", "um", "die", "Stirn", "ein", "d\u00fcrr", "Ge\u00b7flecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Reisig, draus die feinsten Rosen ragen?\u00ab", "tokens": ["Von", "Rei\u00b7sig", ",", "draus", "die", "feins\u00b7ten", "Ro\u00b7sen", "ra\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "$,", "PAV", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie ziehen gen die Sonne ins Gefecht;", "tokens": ["Sie", "zie\u00b7hen", "gen", "die", "Son\u00b7ne", "ins", "Ge\u00b7fecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Poeten sind's, so la\u00df sie ungeschlagen!", "tokens": ["Po\u00b7et\u00b7en", "sin\u00b7d's", ",", "so", "la\u00df", "sie", "un\u00b7ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "VVIMP", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Denn solche, wei\u00dft du, haben immer recht.", "tokens": ["Denn", "sol\u00b7che", ",", "wei\u00dft", "du", ",", "ha\u00b7ben", "im\u00b7mer", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbwas sind das f\u00fcr possierliche Gesellen", "tokens": ["\u00bb", "was", "sind", "das", "f\u00fcr", "pos\u00b7sier\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": 