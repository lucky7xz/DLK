{"dta.poem.3279": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die lyrische Muse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin, wohin rei\u00dft ungewohnte Wuth", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "rei\u00dft", "un\u00b7ge\u00b7wohn\u00b7te", "Wuth"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich auf der Ode k\u00fchnen Fl\u00fcgeln,", "tokens": ["Mich", "auf", "der", "O\u00b7de", "k\u00fch\u00b7nen", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fern von der leisen Fluth", "tokens": ["Fern", "von", "der", "lei\u00b7sen", "Fluth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Am niedern Helikon und jenen Lorbeer-H\u00fcgeln!", "tokens": ["Am", "nie\u00b7dern", "He\u00b7li\u00b7kon", "und", "je\u00b7nen", "Lor\u00b7beer\u00b7H\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich fliehe stolz der Sterblichen Revier;", "tokens": ["Ich", "flie\u00b7he", "stolz", "der", "Sterb\u00b7li\u00b7chen", "Re\u00b7vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich eil in unbeflogne H\u00f6hen:", "tokens": ["Ich", "eil", "in", "un\u00b7be\u00b7flog\u00b7ne", "H\u00f6\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie keichet hinter mir", "tokens": ["Wie", "kei\u00b7chet", "hin\u00b7ter", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Vogel Jupiters, besch\u00e4mt mir nachzusehen!", "tokens": ["Der", "Vo\u00b7gel", "Ju\u00b7pi\u00b7ters", ",", "be\u00b7sch\u00e4mt", "mir", "nach\u00b7zu\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Gegenden, wo mein entz\u00fccktes Ohr", "tokens": ["In", "Ge\u00b7gen\u00b7den", ",", "wo", "mein", "ent\u00b7z\u00fcck\u00b7tes", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Sph\u00e4ren Harmonie verwirret,", "tokens": ["Der", "Sph\u00e4\u00b7ren", "Har\u00b7mo\u00b7nie", "ver\u00b7wir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Muse! fleug mir vor,", "tokens": ["O", "Mu\u00b7se", "!", "fleug", "mir", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du, deren freyer Flug oft irrt, nie sich verirret!", "tokens": ["Du", ",", "de\u00b7ren", "frey\u00b7er", "Flug", "oft", "irrt", ",", "nie", "sich", "ver\u00b7ir\u00b7ret", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "ADV", "VVFIN", "$,", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Ich folge dir bald bis zur Sonne hin,", "tokens": ["Ich", "fol\u00b7ge", "dir", "bald", "bis", "zur", "Son\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald in den ungebahnten Haynen", "tokens": ["Bald", "in", "den", "un\u00b7ge\u00b7bahn\u00b7ten", "Hay\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Libers Priesterinn,", "tokens": ["Mit", "Li\u00b7bers", "Pries\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo keine Muse gieng und andre Sterne scheinen.", "tokens": ["Wo", "kei\u00b7ne", "Mu\u00b7se", "gieng", "und", "and\u00b7re", "Ster\u00b7ne", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "An deiner Hand, wann mich Ly\u00e4us ruft,", "tokens": ["An", "dei\u00b7ner", "Hand", ",", "wann", "mich", "Ly\u00b7\u00e4us", "ruft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was kann den k\u00fchnen Dichter schrecken?", "tokens": ["Was", "kann", "den", "k\u00fch\u00b7nen", "Dich\u00b7ter", "schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In welch entfernter Kluft", "tokens": ["In", "welch", "ent\u00b7fern\u00b7ter", "Kluft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird meiner Leyer Scherz ein schlafend Echo wecken?", "tokens": ["Wird", "mei\u00b7ner", "Le\u00b7yer", "Scherz", "ein", "schla\u00b7fend", "E\u00b7cho", "we\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Denn nur von Lust erklingt mein Saitenspiel,", "tokens": ["Denn", "nur", "von", "Lust", "er\u00b7klingt", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht von leichenvollem Sande,", "tokens": ["Und", "nicht", "von", "lei\u00b7chen\u00b7vol\u00b7lem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von kriegrischem Gew\u00fchl", "tokens": ["Von", "krie\u00b7gri\u00b7schem", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "++---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und vom gekr\u00f6nten Sieg im blutigen Gewande.", "tokens": ["Und", "vom", "ge\u00b7kr\u00f6n\u00b7ten", "Sieg", "im", "blu\u00b7ti\u00b7gen", "Ge\u00b7wan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Zeit ist hin, da unter stolzer Lust,", "tokens": ["Die", "Zeit", "ist", "hin", ",", "da", "un\u00b7ter", "stol\u00b7zer", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Lorbeern, wie ihr Held, bekr\u00e4nzet", "tokens": ["Mit", "Lor\u00b7beern", ",", "wie", "ihr", "Held", ",", "be\u00b7kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PWAV", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und oft an seiner Brust", "tokens": ["Und", "oft", "an", "sei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Muse Necktar trank, durch die er ewig gl\u00e4nzet:", "tokens": ["Die", "Mu\u00b7se", "Neck\u00b7tar", "trank", ",", "durch", "die", "er", "e\u00b7wig", "gl\u00e4n\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie Phosphor gl\u00e4nzt, der um den Morgenthau", "tokens": ["Wie", "Phos\u00b7phor", "gl\u00e4nzt", ",", "der", "um", "den", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Thetis Armen sich entziehet,", "tokens": ["Aus", "The\u00b7tis", "Ar\u00b7men", "sich", "ent\u00b7zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ans gestirnte Blau", "tokens": ["Und", "ans", "ge\u00b7stirn\u00b7te", "Blau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit heitrem L\u00e4cheln tritt, und vom Olympe siehet.", "tokens": ["Mit", "heit\u00b7rem", "L\u00e4\u00b7cheln", "tritt", ",", "und", "vom", "O\u00b7lym\u00b7pe", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ein Sternenheer, das letzte Chor der Nacht,", "tokens": ["Ein", "Ster\u00b7nen\u00b7heer", ",", "das", "letz\u00b7te", "Chor", "der", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Traurt um ihn her in mattem Lichte:", "tokens": ["Traurt", "um", "ihn", "her", "in", "mat\u00b7tem", "Lich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die muntre Welt erwacht,", "tokens": ["Die", "mun\u00b7tre", "Welt", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Schlaf und Schatten fliehn vor seinem Angesichte.", "tokens": ["Und", "Schlaf", "und", "Schat\u00b7ten", "fliehn", "vor", "sei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}