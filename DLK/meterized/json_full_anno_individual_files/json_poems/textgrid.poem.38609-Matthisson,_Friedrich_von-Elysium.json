{"textgrid.poem.38609": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Elysium", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hain! der von der G\u00f6tter Frieden,", "tokens": ["Hain", "!", "der", "von", "der", "G\u00f6t\u00b7ter", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von Thau die Rose, tr\u00e4uft,", "tokens": ["Wie", "von", "Thau", "die", "Ro\u00b7se", ",", "tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Frucht der Hesperiden", "tokens": ["Wo", "die", "Frucht", "der", "Hes\u00b7pe\u00b7ri\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Silberbl\u00fcten reift;", "tokens": ["Zwi\u00b7schen", "Sil\u00b7ber\u00b7bl\u00fc\u00b7ten", "reift", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den ein rosenfarbner Aether", "tokens": ["Den", "ein", "ro\u00b7sen\u00b7farb\u00b7ner", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Ewig unbew\u00f6lkt umfleu\u00dft,", "tokens": ["E\u00b7wig", "un\u00b7be\u00b7w\u00f6lkt", "um\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der den Klageton verschm\u00e4hter", "tokens": ["Der", "den", "Kla\u00b7ge\u00b7ton", "ver\u00b7schm\u00e4h\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Z\u00e4rtlichkeit verstummen hei\u00dft:", "tokens": ["Z\u00e4rt\u00b7lich\u00b7keit", "ver\u00b7stum\u00b7men", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freudigschauernd in der F\u00fclle", "tokens": ["Freu\u00b7digsc\u00b7hau\u00b7ernd", "in", "der", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoher G\u00f6tterseligkeit,", "tokens": ["Ho\u00b7her", "G\u00f6t\u00b7ter\u00b7se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00fc\u00dft, entflohn der Erdenh\u00fclle,", "tokens": ["Gr\u00fc\u00dft", ",", "ent\u00b7flohn", "der", "Er\u00b7den\u00b7h\u00fcl\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Psyche deine Dunkelheit,", "tokens": ["Psy\u00b7che", "dei\u00b7ne", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wonne! wo kein Nebelschleyer", "tokens": ["Won\u00b7ne", "!", "wo", "kein", "Ne\u00b7bel\u00b7schle\u00b7yer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "PIAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ihres Urstoffs Reine tr\u00fcbt,", "tokens": ["Ih\u00b7res", "Ur\u00b7stoffs", "Rei\u00b7ne", "tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo sie geistiger und freyer", "tokens": ["Wo", "sie", "geis\u00b7ti\u00b7ger", "und", "frey\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den entbundnen Fittig \u00fcbt.", "tokens": ["Den", "ent\u00b7bund\u00b7nen", "Fit\u00b7tig", "\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! schon eilt auf Rosenwegen,", "tokens": ["Ha", "!", "schon", "eilt", "auf", "Ro\u00b7sen\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In verkl\u00e4rter Lichtgestalt,", "tokens": ["In", "ver\u00b7kl\u00e4r\u00b7ter", "Licht\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie dem Schattenthal entgegen,", "tokens": ["Sie", "dem", "Schat\u00b7tent\u00b7hal", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die heil'ge Lethe wallt;", "tokens": ["Wo", "die", "heil'\u00b7ge", "Le\u00b7the", "wallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlt sich magisch hingezogen,", "tokens": ["F\u00fchlt", "sich", "ma\u00b7gisch", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie von leiser Geisterhand,", "tokens": ["Wie", "von", "lei\u00b7ser", "Geis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schaut entz\u00fcckt die Silberwogen", "tokens": ["Schaut", "ent\u00b7z\u00fcckt", "die", "Sil\u00b7ber\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des Ufers Blumenrand;", "tokens": ["Und", "des", "U\u00b7fers", "Blu\u00b7men\u00b7rand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kniet voll s\u00fcsser Ahndung nieder,", "tokens": ["Kniet", "voll", "s\u00fcs\u00b7ser", "Ahn\u00b7dung", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pfet, und ihr zitternd Bild", "tokens": ["Sch\u00f6p\u00b7fet", ",", "und", "ihr", "zit\u00b7ternd", "Bild"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leuchtet aus dem Strome wieder,", "tokens": ["Leuch\u00b7tet", "aus", "dem", "Stro\u00b7me", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der der Menschheit Jammer stillt,", "tokens": ["Der", "der", "Menschheit", "Jam\u00b7mer", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie auf sanfter Meeresfl\u00e4che", "tokens": ["Wie", "auf", "sanf\u00b7ter", "Mee\u00b7res\u00b7fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die entw\u00f6lkte Luna schwimmt,", "tokens": ["Die", "ent\u00b7w\u00f6lk\u00b7te", "Lu\u00b7na", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder im Kristall der B\u00e4che", "tokens": ["O\u00b7der", "im", "Kris\u00b7tall", "der", "B\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Hespers goldne Fackel glimmt.", "tokens": ["Hes\u00b7pers", "gold\u00b7ne", "Fa\u00b7ckel", "glimmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Psyche trinkt, und nicht vergebens!", "tokens": ["Psy\u00b7che", "trinkt", ",", "und", "nicht", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pl\u00f6tzlich in der Fluthen Grab", "tokens": ["Pl\u00f6tz\u00b7lich", "in", "der", "Flut\u00b7hen", "Grab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sinkt das Nachtst\u00fcck ihres Lebens", "tokens": ["Sinkt", "das", "Nacht\u00b7st\u00fcck", "ih\u00b7res", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Traumgesicht hinab.", "tokens": ["Wie", "ein", "Traum\u00b7ge\u00b7sicht", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4nzender, auf k\u00fchnern Fl\u00fcgeln,", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", ",", "auf", "k\u00fch\u00b7nern", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwebt sie aus des Thales Nacht", "tokens": ["Schwebt", "sie", "aus", "des", "Tha\u00b7les", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu den goldbebl\u00fcmten H\u00fcgeln,", "tokens": ["Zu", "den", "gold\u00b7be\u00b7bl\u00fcm\u00b7ten", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo ein ew'ger Fr\u00fchling lacht.", "tokens": ["Wo", "ein", "ew'\u00b7ger", "Fr\u00fch\u00b7ling", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Welch ein feyerliches Schweigen!", "tokens": ["Welch", "ein", "fey\u00b7er\u00b7li\u00b7ches", "Schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise nur, wie Zephyrs Hauch,", "tokens": ["Lei\u00b7se", "nur", ",", "wie", "Ze\u00b7phyrs", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00e4uselt's in den Lorbeerzweigen,", "tokens": ["S\u00e4u\u00b7selt's", "in", "den", "Lor\u00b7beer\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bebt's im Amaranthenstrauch!", "tokens": ["Bebt's", "im", "A\u00b7ma\u00b7ran\u00b7then\u00b7strauch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So in heil'ger Stille ruhten", "tokens": ["So", "in", "heil'\u00b7ger", "Stil\u00b7le", "ruh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Luft und Wogen, also schwieg", "tokens": ["Luft", "und", "Wo\u00b7gen", ",", "al\u00b7so", "schwieg"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Natur, da aus den Fluthen", "tokens": ["Die", "Na\u00b7tur", ",", "da", "aus", "den", "Flut\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Anadyomene stieg.", "tokens": ["A\u00b7nad\u00b7yo\u00b7me\u00b7ne", "stieg", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Welch ein ungewohnter Schimmer!", "tokens": ["Welch", "ein", "un\u00b7ge\u00b7wohn\u00b7ter", "Schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erde! dieses Zauberlicht", "tokens": ["Er\u00b7de", "!", "die\u00b7ses", "Zau\u00b7ber\u00b7licht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flammte selbst im Lenze nimmer", "tokens": ["Flamm\u00b7te", "selbst", "im", "Len\u00b7ze", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Aurorens Angesicht!", "tokens": ["Von", "Au\u00b7ro\u00b7rens", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh! des glatten Epheus Ranken", "tokens": ["Sieh", "!", "des", "glat\u00b7ten", "E\u00b7pheus", "Ran\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tauchen sich in Purpurglanz!", "tokens": ["Tau\u00b7chen", "sich", "in", "Pur\u00b7pur\u00b7glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blumen, die den Quell umwanken,", "tokens": ["Blu\u00b7men", ",", "die", "den", "Quell", "um\u00b7wan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Funkeln wie ein Sternenkranz!", "tokens": ["Fun\u00b7keln", "wie", "ein", "Ster\u00b7nen\u00b7kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So begann's im Hain zu tagen,", "tokens": ["So", "be\u00b7gann's", "im", "Hain", "zu", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die keusche Cynthia,", "tokens": ["Als", "die", "keu\u00b7sche", "Cyn\u00b7thia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hoch vom stolzen Drachenwagen,", "tokens": ["Hoch", "vom", "stol\u00b7zen", "Dra\u00b7chen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den geliebten Schl\u00e4fer sah;", "tokens": ["Den", "ge\u00b7lieb\u00b7ten", "Schl\u00e4\u00b7fer", "sah", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als die Fluren sich versch\u00f6nten,", "tokens": ["Als", "die", "Flu\u00b7ren", "sich", "ver\u00b7sch\u00f6n\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN"