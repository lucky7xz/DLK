{"dta.poem.20289": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff die abbildung des Cupido/  \n wie er die pfeile wetzte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido/ der dich mehr als seinen k\u00f6cher liebt/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "der", "dich", "mehr", "als", "sei\u00b7nen", "k\u00f6\u00b7cher", "liebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELS", "PPER", "PIAT", "KOKOM", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will seinen schleiffer-zeug in deine kammer tragen/", "tokens": ["Will", "sei\u00b7nen", "schleif\u00b7fer\u00b7zeug", "in", "dei\u00b7ne", "kam\u00b7mer", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df/ da\u00df er dir schon im geiste k\u00fcsse giebt/", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "er", "dir", "schon", "im", "geis\u00b7te", "k\u00fcs\u00b7se", "giebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dir manch schl\u00fcpffrich wort wird in das ohre sagen.", "tokens": ["Und", "dir", "manch", "schl\u00fcpf\u00b7frich", "wort", "wird", "in", "das", "oh\u00b7re", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "ADJD", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er stellt sich nackt und blo\u00df f\u00fcr deinen augen ein/", "tokens": ["Er", "stellt", "sich", "nackt", "und", "blo\u00df", "f\u00fcr", "dei\u00b7nen", "au\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADV", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein weisser attla\u00df ist der schnee von brust und lenden;", "tokens": ["Sein", "weis\u00b7ser", "att\u00b7la\u00df", "ist", "der", "schnee", "von", "brust", "und", "len\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn wahre liebe soll recht offenhertzig seyn/", "tokens": ["Denn", "wah\u00b7re", "lie\u00b7be", "soll", "recht", "of\u00b7fen\u00b7hert\u00b7zig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "VMFIN", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und unsre augen nicht durch schminck und schmuck verblenden.", "tokens": ["Und", "uns\u00b7re", "au\u00b7gen", "nicht", "durch", "schminck", "und", "schmuck", "ver\u00b7blen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er lehrt dich/ wie man recht vertraulich lieben kan/", "tokens": ["Er", "lehrt", "dich", "/", "wie", "man", "recht", "ver\u00b7trau\u00b7lich", "lie\u00b7ben", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "PIS", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie heisse liebe sich nicht leichtlich l\u00e4st verdecken/", "tokens": ["Wie", "heis\u00b7se", "lie\u00b7be", "sich", "nicht", "leicht\u00b7lich", "l\u00e4st", "ver\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "PTKNEG", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die paradie\u00df-tracht steht ihm mehr als zierlich an/", "tokens": ["Die", "pa\u00b7ra\u00b7die\u00df\u00b7tracht", "steht", "ihm", "mehr", "als", "zier\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "KOKOM", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn reiner liebe zeug ist rein von staub und flecken.", "tokens": ["Denn", "rei\u00b7ner", "lie\u00b7be", "zeug", "ist", "rein", "von", "staub", "und", "fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Beyneben tr\u00f6stet mich die gute zuversicht/", "tokens": ["Bey\u00b7ne\u00b7ben", "tr\u00f6s\u00b7tet", "mich", "die", "gu\u00b7te", "zu\u00b7ver\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Er werde gegen dich auch meiner treu gedencken/", "tokens": ["Er", "wer\u00b7de", "ge\u00b7gen", "dich", "auch", "mei\u00b7ner", "treu", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sagen/ wie mir offt mein mattes hertze bricht/", "tokens": ["Und", "sa\u00b7gen", "/", "wie", "mir", "offt", "mein", "mat\u00b7tes", "hert\u00b7ze", "bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWAV", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein hertz/ das sich allein will nach Ambretten lencken.", "tokens": ["Mein", "hertz", "/", "das", "sich", "al\u00b7lein", "will", "nach", "Am\u00b7bret\u00b7ten", "len\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PRF", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wei\u00df/ er wird nicht weit von deiner lager-statt/", "tokens": ["Ich", "wei\u00df", "/", "er", "wird", "nicht", "weit", "von", "dei\u00b7ner", "la\u00b7ger\u00b7statt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "(sein schleiffen wird dir ja nicht deine ruh verst\u00f6ren/)", "tokens": ["(", "sein", "schleif\u00b7fen", "wird", "dir", "ja", "nicht", "dei\u00b7ne", "ruh", "ver\u00b7st\u00f6\u00b7ren", "/", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "VVINF", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach seiner k\u00fchnen art/ bewegen stahl und rad/", "tokens": ["Nach", "sei\u00b7ner", "k\u00fch\u00b7nen", "art", "/", "be\u00b7we\u00b7gen", "stahl", "und", "rad", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "VVINF", "VVFIN", "KON", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und was hier ferner folgt dich deutlich lassen h\u00f6ren.", "tokens": ["Und", "was", "hier", "fer\u00b7ner", "folgt", "dich", "deut\u00b7lich", "las\u00b7sen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ambrette/ wo dein geist nicht meinen rath veracht/", "tokens": ["Am\u00b7bret\u00b7te", "/", "wo", "dein", "geist", "nicht", "mei\u00b7nen", "rath", "ver\u00b7acht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So la\u00df best\u00e4ndigkeit und liebe sich verm\u00e4hlen.", "tokens": ["So", "la\u00df", "be\u00b7st\u00e4n\u00b7dig\u00b7keit", "und", "lie\u00b7be", "sich", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "NN", "KON", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer auff ver\u00e4nderung und neue funcken tracht/", "tokens": ["Wer", "auff", "ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "neu\u00b7e", "fun\u00b7cken", "tracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem wird es nimmermehr an wermuths-k\u00f6rnern fehlen.", "tokens": ["Dem", "wird", "es", "nim\u00b7mer\u00b7mehr", "an", "wer\u00b7muths\u00b7k\u00f6r\u00b7nern", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich lobe/ was bi\u00dfher dein treuer geist ver\u00fcbt/", "tokens": ["Ich", "lo\u00b7be", "/", "was", "bi\u00df\u00b7her", "dein", "treu\u00b7er", "geist", "ver\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich will ein kostbar oel in deine flammen giessen/", "tokens": ["Ich", "will", "ein", "kost\u00b7bar", "o\u00b7el", "in", "dei\u00b7ne", "flam\u00b7men", "gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Wo deine seele nicht die neben-z\u00fcge liebt/", "tokens": ["Wo", "dei\u00b7ne", "see\u00b7le", "nicht", "die", "ne\u00b7ben\u00b7z\u00fc\u00b7ge", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "So will ich deine gunst/ so gut ich kan/ vers\u00fcssen.", "tokens": ["So", "will", "ich", "dei\u00b7ne", "gunst", "/", "so", "gut", "ich", "kan", "/", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "ADJD", "PPER", "VMFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df/ wie redlich dir Cretin sein hertze schenckt/", "tokens": ["Ich", "wei\u00df", "/", "wie", "red\u00b7lich", "dir", "Cre\u00b7tin", "sein", "hert\u00b7ze", "schenckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "ADJD", "PPER", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Wie willig sich sein geist nach deinem willem beuget/", "tokens": ["Wie", "wil\u00b7lig", "sich", "sein", "geist", "nach", "dei\u00b7nem", "wil\u00b7lem", "beu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und wie er mehr auff dein als seine wohlfahrt denckt/", "tokens": ["Und", "wie", "er", "mehr", "auff", "dein", "als", "sei\u00b7ne", "wohl\u00b7fahrt", "denckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "PPOSAT", "KOUS", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ja wo es dir gef\u00e4llt/ zu seinem grabe steiget.", "tokens": ["Ja", "wo", "es", "dir", "ge\u00b7f\u00e4llt", "/", "zu", "sei\u00b7nem", "gra\u00b7be", "stei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "PPER", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich hab ihn neulich noch mit wehmuth ange schaut/", "tokens": ["Ich", "hab", "ihn", "neu\u00b7lich", "noch", "mit", "weh\u00b7muth", "an\u00b7ge", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Als du aus zeitvertreib von ihm dich weggerissen/", "tokens": ["Als", "du", "aus", "zeit\u00b7ver\u00b7treib", "von", "ihm", "dich", "weg\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie er aus trauer-sucht ihm hatt ein haus gebaut/", "tokens": ["Wie", "er", "aus", "trau\u00b7e\u00b7rsucht", "ihm", "hatt", "ein", "haus", "ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und er in einsamkeit sich dachte zu verschliessen.", "tokens": ["Und", "er", "in", "ein\u00b7sam\u00b7keit", "sich", "dach\u00b7te", "zu", "ver\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "PRF", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die armen macht er ihm zum spiegel seiner noth/", "tokens": ["Die", "ar\u00b7men", "macht", "er", "ihm", "zum", "spie\u00b7gel", "sei\u00b7ner", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ambrett\u2019/ Ambrette/ rieff er aus dem bleichen munde/", "tokens": ["Am\u00b7brett'", "/", "Am\u00b7bret\u00b7te", "/", "rieff", "er", "aus", "dem", "blei\u00b7chen", "mun\u00b7de", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Dein aussenbleiben w\u00e4r ihm herber als der tod/", "tokens": ["Dein", "aus\u00b7sen\u00b7blei\u00b7ben", "w\u00e4r", "ihm", "her\u00b7ber", "als", "der", "tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "VAFIN", "PPER", "ADV", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und deine wiederkunfft bezuckert ihm die stunde.", "tokens": ["Und", "dei\u00b7ne", "wie\u00b7der\u00b7kunfft", "be\u00b7zu\u00b7ckert", "ihm", "die", "stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Es hat Cupido mir auch ferner zugesagt", "tokens": ["Es", "hat", "Cu\u00b7pi\u00b7do", "mir", "auch", "fer\u00b7ner", "zu\u00b7ge\u00b7sagt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NE", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein mehres wegen mein bey ihr noch anzubringen/", "tokens": ["Ein", "meh\u00b7res", "we\u00b7gen", "mein", "bey", "ihr", "noch", "an\u00b7zu\u00b7brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "APPR", "PPER", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Eh\u2019 als er mich verlie\u00df/ so hat er mich gefragt", "tokens": ["Eh'", "als", "er", "mich", "ver\u00b7lie\u00df", "/", "so", "hat", "er", "mich", "ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "PPER", "PRF", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wie tieff mein Auge d\u00f6rfft in deine gegend dringen.", "tokens": ["Wie", "tieff", "mein", "Au\u00b7ge", "d\u00f6rfft", "in", "dei\u00b7ne", "ge\u00b7gend", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich hoffe/ weil er so beweglich bitten kan/", "tokens": ["Ich", "hof\u00b7fe", "/", "weil", "er", "so", "be\u00b7weg\u00b7lich", "bit\u00b7ten", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Er werd\u2019/ Ambrette/ dich noch endlich wohl erweichen.", "tokens": ["Er", "werd'", "/", "Am\u00b7bret\u00b7te", "/", "dich", "noch", "end\u00b7lich", "wohl", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "NE", "$(", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ach freundin! h\u00f6r ihn doch mit offnen hertzen an/", "tokens": ["Ach", "freun\u00b7din", "!", "h\u00f6r", "ihn", "doch", "mit", "off\u00b7nen", "hert\u00b7zen", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und la\u00df mein freyes aug in berg und th\u00e4ler streichen/", "tokens": ["Und", "la\u00df", "mein", "frey\u00b7es", "aug", "in", "berg", "und", "th\u00e4\u00b7ler", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "NE", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Cupido mag itzund verschleiffen tag und nacht/", "tokens": ["Cu\u00b7pi\u00b7do", "mag", "it\u00b7zund", "ver\u00b7schleif\u00b7fen", "tag", "und", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.50": {"text": "Du wirst ihm ja bey dir den engen raum verg\u00f6nnen/", "tokens": ["Du", "wirst", "ihm", "ja", "bey", "dir", "den", "en\u00b7gen", "raum", "ver\u00b7g\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Es hat der kleinste gott die allergr\u00f6ste macht/", "tokens": ["Es", "hat", "der", "kleins\u00b7te", "gott", "die", "al\u00b7ler\u00b7gr\u00f6s\u00b7te", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wer wird/ was ihm beliebt/ doch wohl verwehren k\u00f6nnen?", "tokens": ["Wer", "wird", "/", "was", "ihm", "be\u00b7liebt", "/", "doch", "wohl", "ver\u00b7weh\u00b7ren", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "PWS", "PPER", "ADJD", "$(", "ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Doch wenn er einmahl dich entbl\u00f6st im bette schaut/", "tokens": ["Doch", "wenn", "er", "ein\u00b7mahl", "dich", "ent\u00b7bl\u00f6st", "im", "bet\u00b7te", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "APPRART", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Und an der zarten haut sein ange kan ergetzen/", "tokens": ["Und", "an", "der", "zar\u00b7ten", "haut", "sein", "an\u00b7ge", "kan", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "VVFIN", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "So schleifft er ferner nicht/ und ruffet \u00fcberlaut:", "tokens": ["So", "schleifft", "er", "fer\u00b7ner", "nicht", "/", "und", "ruf\u00b7fet", "\u00fc\u00b7berl\u00b7aut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ambrette soll allein hier stumpffe pfeile wetzen.", "tokens": ["Am\u00b7bret\u00b7te", "soll", "al\u00b7lein", "hier", "stumpf\u00b7fe", "pfei\u00b7le", "wet\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}