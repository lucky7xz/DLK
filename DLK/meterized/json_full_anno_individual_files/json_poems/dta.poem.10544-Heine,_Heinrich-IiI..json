{"dta.poem.10544": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Still versteckt der Mond sich drau\u00dfen", "tokens": ["Still", "ver\u00b7steckt", "der", "Mond", "sich", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter'm gr\u00fcnen Tannenbaum,", "tokens": ["Hin\u00b7ter'm", "gr\u00fc\u00b7nen", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im Zimmer unsre Lampe", "tokens": ["Und", "im", "Zim\u00b7mer", "uns\u00b7re", "Lam\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flackert matt und leuchtet kaum.", "tokens": ["Fla\u00b7ckert", "matt", "und", "leuch\u00b7tet", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber meine blauen Sterne", "tokens": ["A\u00b7ber", "mei\u00b7ne", "blau\u00b7en", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlen auf in heller'm Licht,", "tokens": ["Strah\u00b7len", "auf", "in", "hel\u00b7ler'm", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gl\u00fchn die Purpurr\u00f6slein,", "tokens": ["Und", "es", "gl\u00fchn", "die", "Pur\u00b7pur\u00b7r\u00f6s\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und das liebe M\u00e4dchen spricht:", "tokens": ["Und", "das", "lie\u00b7be", "M\u00e4d\u00b7chen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ekleines V\u00f6lkchen, Wichtelm\u00e4nnchen,", "tokens": ["\u201e", "klei\u00b7nes", "V\u00f6lk\u00b7chen", ",", "Wich\u00b7tel\u00b7m\u00e4nn\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehlen unser Brod und Speck,", "tokens": ["Steh\u00b7len", "un\u00b7ser", "Brod", "und", "Speck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abends liegt es noch im Kasten,", "tokens": ["A\u00b7bends", "liegt", "es", "noch", "im", "Kas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Morgens ist es weg.", "tokens": ["Und", "des", "Mor\u00b7gens", "ist", "es", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}