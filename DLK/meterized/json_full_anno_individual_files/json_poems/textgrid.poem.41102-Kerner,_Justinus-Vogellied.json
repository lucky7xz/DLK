{"textgrid.poem.41102": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Vogellied", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel pflegt wohl auf dem Blatt", "tokens": ["Der", "Vo\u00b7gel", "pflegt", "wohl", "auf", "dem", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein kleines Lied zu singen,", "tokens": ["Sein", "klei\u00b7nes", "Lied", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch nie er's aufgeschrieben hat", "tokens": ["Doch", "nie", "er's", "auf\u00b7ge\u00b7schrie\u00b7ben", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIS", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Federn aus den Schwingen;", "tokens": ["Mit", "Fe\u00b7dern", "aus", "den", "Schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie's aus der Kehle kommt, verhallt's,", "tokens": ["Wie's", "aus", "der", "Keh\u00b7le", "kommt", ",", "ver\u00b7hallt's", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sagt nicht: Lies es und behalt's.", "tokens": ["Er", "sagt", "nicht", ":", "Lies", "es", "und", "be\u00b7halt'", "s."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "VVFIN", "PPER", "KON", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So hab' auch ich manch kleines Lied", "tokens": ["So", "hab'", "auch", "ich", "manch", "klei\u00b7nes", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Busch und Wald gesungen,", "tokens": ["Durch", "Busch", "und", "Wald", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das, weil ich war zu schreibem\u00fcd,", "tokens": ["Das", ",", "weil", "ich", "war", "zu", "schrei\u00b7be\u00b7m\u00fcd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Busch und Wald verklungen,", "tokens": ["In", "Busch", "und", "Wald", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Echo nur bemerkt es, das,", "tokens": ["Das", "E\u00b7cho", "nur", "be\u00b7merkt", "es", ",", "das", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Rezensent, im Berge sa\u00df.", "tokens": ["Ein", "Re\u00b7zen\u00b7sent", ",", "im", "Ber\u00b7ge", "sa\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$