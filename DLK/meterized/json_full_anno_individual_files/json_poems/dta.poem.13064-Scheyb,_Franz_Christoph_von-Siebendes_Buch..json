{"dta.poem.13064": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fremde trat zur\u00fcck. Inzwischen war der Grei\u00df", "tokens": ["Die", "Frem\u00b7de", "trat", "zu\u00b7r\u00fcck", ".", "I\u00b7nzwi\u00b7schen", "war", "der", "Grei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jm Sinn besch\u00e4ftiget, als d\u00e4cht\u2019 er, was das hei\u00df;", "tokens": ["Jm", "Sinn", "be\u00b7sch\u00e4f\u00b7ti\u00b7get", ",", "als", "d\u00e4cht'", "er", ",", "was", "das", "hei\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KOUS", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wies er Freundlichkeit. Weil viele vieles redten,", "tokens": ["Doch", "wies", "er", "Freund\u00b7lich\u00b7keit", ".", "Weil", "vie\u00b7le", "vie\u00b7les", "red\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$.", "KOUS", "PIS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So ward ich die\u00df gewahr: \u201eWir k\u00f6nnten etwas wetten,", "tokens": ["So", "ward", "ich", "die\u00df", "ge\u00b7wahr", ":", "\u201e", "Wir", "k\u00f6nn\u00b7ten", "et\u00b7was", "wet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDS", "ADJD", "$.", "$(", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eda\u00df unser guter Rath die Nachricht vor gewu\u00dft,", "tokens": ["\u201e", "da\u00df", "un\u00b7ser", "gu\u00b7ter", "Rath", "die", "Nach\u00b7richt", "vor", "ge\u00b7wu\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "590\u201dDie von Theresia man uns erinnern mu\u00dft.", "tokens": ["\"", "Die", "von", "The\u00b7re\u00b7sia", "man", "uns", "e\u00b7rin\u00b7nern", "mu\u00dft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "APPR", "NE", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}