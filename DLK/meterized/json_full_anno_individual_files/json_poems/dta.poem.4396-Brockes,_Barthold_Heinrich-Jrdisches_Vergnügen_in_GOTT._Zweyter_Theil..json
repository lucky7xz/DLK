{"dta.poem.4396": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jrdisches  \n  Vergn\u00fcgen  \n in  \n GOTT.  \n Zweyter Theil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern du, lieber Mensch,", "tokens": ["Wo\u00b7fern", "du", ",", "lie\u00b7ber", "Mensch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ein Atheist,", "tokens": ["ein", "A\u00b7theist", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wie oder blo\u00df ein Thier", "tokens": ["Wie", "o\u00b7der", "blo\u00df", "ein", "Thier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "mit andern Thieren bist;", "tokens": ["mit", "an\u00b7dern", "Thie\u00b7ren", "bist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So steht dir frey, da\u00df du die Welt", "tokens": ["So", "steht", "dir", "frey", ",", "da\u00df", "du", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was uns in die Sinne f\u00e4llt,", "tokens": ["Und", "was", "uns", "in", "die", "Sin\u00b7ne", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ver\u00e4chtlich h\u00e4lt\u2019st, und nicht be-", "tokens": ["Ver\u00b7\u00e4cht\u00b7lich", "h\u00e4lt'st", ",", "und", "nicht", "be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "PTKNEG", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "trachtest;", "tokens": ["trach\u00b7test", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "So steht dir frey, da\u00df du die Zeit,", "tokens": ["So", "steht", "dir", "frey", ",", "da\u00df", "du", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Darin man die Beschaffenheit", "tokens": ["Da\u00b7rin", "man", "die", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Creatur und ihre Herrlich-", "tokens": ["Der", "Crea\u00b7tur", "und", "ih\u00b7re", "Herr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "keit", "tokens": ["keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Bewundert, f\u00fcr verloren achtest.", "tokens": ["Be\u00b7wun\u00b7dert", ",", "f\u00fcr", "ver\u00b7lo\u00b7ren", "ach\u00b7test", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Allein", "tokens": ["Al\u00b7lein"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Wofern du dir nicht selbst die", "tokens": ["Wo\u00b7fern", "du", "dir", "nicht", "selbst", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "ART"], "meter": "+---+--", "measure": "dactylic.init"}, "line.16": {"text": "Sele raubest,", "tokens": ["Se\u00b7le", "rau\u00b7best", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Wofern du eine Gottheit glau-", "tokens": ["Wo\u00b7fern", "du", "ei\u00b7ne", "Got\u00b7theit", "glau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "best,", "tokens": ["best", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Die alles, Stern- und Sonnen-", "tokens": ["Die", "al\u00b7les", ",", "Stern", "und", "Son\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Schein,", "tokens": ["Schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Die Himmel, Erd\u2019 und Meer ge-", "tokens": ["Die", "Him\u00b7mel", ",", "Erd'", "und", "Meer", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "macht,", "tokens": ["macht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Die dich und alle Ding\u2019 hervor", "tokens": ["Die", "dich", "und", "al\u00b7le", "Ding'", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KON", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "gebracht;", "tokens": ["ge\u00b7bracht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "So kannst du ja nicht anders", "tokens": ["So", "kannst", "du", "ja", "nicht", "an\u00b7ders"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "denken,", "tokens": ["den\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Als da\u00df der Sch\u00f6pfer wei\u00df, da\u00df", "tokens": ["Als", "da\u00df", "der", "Sch\u00f6p\u00b7fer", "wei\u00df", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "dich Sein Werk nicht r\u00fchrt,", "tokens": ["dich", "Sein", "Werk", "nicht", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "Da\u00df du\u2019s nicht w\u00fcrdigest, Jhm", "tokens": ["Da\u00df", "du's", "nicht", "w\u00fcr\u00b7di\u00b7gest", ",", "Jhm"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "PTKNEG", "VAFIN", "$,", "PPER"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.30": {"text": "einen Blick zu schenken,", "tokens": ["ei\u00b7nen", "Blick", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Da\u00df folglich GOTT, so viel an", "tokens": ["Da\u00df", "folg\u00b7lich", "GoTT", ",", "so", "viel", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "$,", "ADV", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "dir, verliert", "tokens": ["dir", ",", "ver\u00b7liert"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Macht, Weis heit, Lieb\u2019 und Ehr\u2019.", "tokens": ["Macht", ",", "Weis", "heit", ",", "Lieb'", "und", "Ehr'", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "FM.la", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Armsel'ge Creatur,", "tokens": ["Arm\u00b7sel'\u00b7ge", "Crea\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.35": {"text": "Wie elend ist dein Stand? da du", "tokens": ["Wie", "e\u00b7lend", "ist", "dein", "Stand", "?", "da", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "noch nicht empfunden,", "tokens": ["noch", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.37": {"text": "Da\u00df GOTT hier auf der Welt", "tokens": ["Da\u00df", "GoTT", "hier", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "mit deiner Lust nicht nur", "tokens": ["mit", "dei\u00b7ner", "Lust", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Sein Lob so wunderbar, so Gna-", "tokens": ["Sein", "Lob", "so", "wun\u00b7der\u00b7bar", ",", "so", "Gna"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "den- reich verbunden,", "tokens": ["den", "reich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["TRUNC", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Nein, da\u00df so gar dein\u2019 Anmut", "tokens": ["Nein", ",", "da\u00df", "so", "gar", "dein'", "An\u00b7mut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "auf der Welt,", "tokens": ["auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Die sich auf GOttes Ehre gr\u00fcn-", "tokens": ["Die", "sich", "auf", "Got\u00b7tes", "Eh\u00b7re", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "det,", "tokens": ["det", ","], "token_info": ["word", "punct"], "pos": ["PDS", "$,"], "meter": "-", "measure": "single.down"}, "line.45": {"text": "Aus Gnaden Jhm so wol gef\u00e4llt,", "tokens": ["Aus", "Gna\u00b7den", "Jhm", "so", "wol", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df sie auch dort gewi\u00df unend-", "tokens": ["Da\u00df", "sie", "auch", "dort", "ge\u00b7wi\u00df", "un\u00b7en\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.47": {"text": "lich' Anmut findet.", "tokens": ["lich'", "An\u00b7mut", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "Und die\u00df vers\u00e4umest du, und", "tokens": ["Und", "die\u00df", "ver\u00b7s\u00e4u\u00b7mest", "du", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$,", "KON"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "willt mit Flei\u00df nicht sehn,", "tokens": ["willt", "mit", "Flei\u00df", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.50": {"text": "Was durch des H\u00f6chsten Lieb\u2019", "tokens": ["Was", "durch", "des", "H\u00f6chs\u00b7ten", "Lieb'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "und weise Macht geschehn.", "tokens": ["und", "wei\u00b7se", "Macht", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.52": {"text": "Bedenke, was du thust! so wei\u00df", "tokens": ["Be\u00b7den\u00b7ke", ",", "was", "du", "thust", "!", "so", "wei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "ich, du versp\u00fcrest,", "tokens": ["ich", ",", "du", "ver\u00b7sp\u00fc\u00b7rest", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.54": {"text": "Da\u00df du nicht hier allein, auch", "tokens": ["Da\u00df", "du", "nicht", "hier", "al\u00b7lein", ",", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADV", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "dort, dein Heyl verlierest.", "tokens": ["dort", ",", "dein", "Heyl", "ver\u00b7lie\u00b7rest", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.56": {"text": "Die Strafe f\u00e4ngt bereits in die-", "tokens": ["Die", "Stra\u00b7fe", "f\u00e4ngt", "be\u00b7reits", "in", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.57": {"text": "sem Leben an.", "tokens": ["sem", "Le\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.58": {"text": "Denn \u00fcberkommst du gleich das", "tokens": ["Denn", "\u00fc\u00b7ber\u00b7kommst", "du", "gleich", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "gr\u00f6ste Gl\u00fcck auf Erden;", "tokens": ["gr\u00f6s\u00b7te", "Gl\u00fcck", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.60": {"text": "So kannst du doch unm\u00f6glich", "tokens": ["So", "kannst", "du", "doch", "un\u00b7m\u00f6g\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.61": {"text": "gl\u00fccklich werden.", "tokens": ["gl\u00fcck\u00b7lich", "wer\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.62": {"text": "Sprich selbst: ob etwas dich wol", "tokens": ["Sprich", "selbst", ":", "ob", "et\u00b7was", "dich", "wol"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "KOUS", "PIS", "PRF", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.63": {"text": "recht vergn\u00fcgen kann", "tokens": ["recht", "ver\u00b7gn\u00fc\u00b7gen", "kann"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVINF", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.64": {"text": "Von allem, was du such\u2019st, von", "tokens": ["Von", "al\u00b7lem", ",", "was", "du", "such'st", ",", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "APPR"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.65": {"text": "allem, was du treibest!", "tokens": ["al\u00b7lem", ",", "was", "du", "trei\u00b7best", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.66": {"text": "Sprich, ob dasjenige, worauf", "tokens": ["Sprich", ",", "ob", "das\u00b7je\u00b7ni\u00b7ge", ",", "wo\u00b7rauf"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "KOUS", "PDS", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.67": 