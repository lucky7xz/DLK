{"dta.poem.9682": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eyfersucht.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eyfersucht die dient zu nichts/ als zu schimpff und eigner", "tokens": ["Ey\u00b7fer\u00b7sucht", "die", "dient", "zu", "nichts", "/", "als", "zu", "schimpff", "und", "eig\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "PIS", "$(", "KOUS", "PTKA", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "schande/", "tokens": ["schan\u00b7de", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wenn sich selbst ein weib nicht h\u00e4lt/ halten keine feste bande:", "tokens": ["Wenn", "sich", "selbst", "ein", "weib", "nicht", "h\u00e4lt", "/", "hal\u00b7ten", "kei\u00b7ne", "fes\u00b7te", "ban\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$(", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-++-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Es hilfft keine starcke th\u00fcr. Die ist keusch und fehler rein/", "tokens": ["Es", "hilfft", "kei\u00b7ne", "star\u00b7cke", "th\u00fcr.", "Die", "ist", "keusch", "und", "feh\u00b7ler", "rein", "/"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "ART", "VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die sich h\u00fctet/ da\u00df der mann nicht erst darff der h\u00fctter seyn.", "tokens": ["Die", "sich", "h\u00fc\u00b7tet", "/", "da\u00df", "der", "mann", "nicht", "erst", "darff", "der", "h\u00fct\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "$(", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VMFIN", "ART", "ADJD", "VAINF", "$."], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}