{"textgrid.poem.48460": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Im Namen dreier Schwestern auf ihres Vatern (Heinrich Niehusen) Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar es geb\u00fchrt die Zier der angenehmen Tage,", "tokens": ["Zwar", "es", "ge\u00b7b\u00fchrt", "die", "Zier", "der", "an\u00b7ge\u00b7neh\u00b7men", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df man von lauter Lust und sch\u00f6nem Wesen sage,", "tokens": ["da\u00df", "man", "von", "lau\u00b7ter", "Lust", "und", "sch\u00f6\u00b7nem", "We\u00b7sen", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "itzt, da das reiche Feld in voller Rose steht,", "tokens": ["itzt", ",", "da", "das", "rei\u00b7che", "Feld", "in", "vol\u00b7ler", "Ro\u00b7se", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und mit so mancher Frucht und G\u00fcte schwanger geht.", "tokens": ["und", "mit", "so", "man\u00b7cher", "Frucht", "und", "G\u00fc\u00b7te", "schwan\u00b7ger", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Feldman hat schon itzt die Scheuren ausger\u00e4umet,", "tokens": ["Der", "Feld\u00b7man", "hat", "schon", "itzt", "die", "Scheu\u00b7ren", "aus\u00b7ge\u00b7r\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "davon ihm Nacht und Tag und alle Stunden tr\u00e4umet,", "tokens": ["da\u00b7von", "ihm", "Nacht", "und", "Tag", "und", "al\u00b7le", "Stun\u00b7den", "tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "wie er die Sense schon hab' an das Korn gelegt,", "tokens": ["wie", "er", "die", "Sen\u00b7se", "schon", "hab'", "an", "das", "Korn", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wie ers schon eingef\u00fchrt. Der treue G\u00e4rtner hegt,", "tokens": ["wie", "ers", "schon", "ein\u00b7ge\u00b7f\u00fchrt", ".", "Der", "treu\u00b7e", "G\u00e4rt\u00b7ner", "hegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VVPP", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "was Baum und Erde giebt. Der Meier geht zur Auen,", "tokens": ["was", "Baum", "und", "Er\u00b7de", "giebt", ".", "Der", "Mei\u00b7er", "geht", "zur", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "KON", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der Fischer in die See, die klugen J\u00e4ger schauen", "tokens": ["der", "Fi\u00b7scher", "in", "die", "See", ",", "die", "klu\u00b7gen", "J\u00e4\u00b7ger", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "sich nach dem Wildwerk um. Das Dorf, das ist erfreut,", "tokens": ["sich", "nach", "dem", "Wild\u00b7werk", "um", ".", "Das", "Dorf", ",", "das", "ist", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$.", "ART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die Stadt vertreibt mit Lust die angenehme Zeit.", "tokens": ["die", "Stadt", "ver\u00b7treibt", "mit", "Lust", "die", "an\u00b7ge\u00b7neh\u00b7me", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df alles gibt uns Fug und Anla\u00df gnung zu freuen:", "tokens": ["Di\u00df", "al\u00b7les", "gibt", "uns", "Fug", "und", "An\u00b7la\u00df", "gnung", "zu", "freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "mehr aber eines noch, da\u00df uns geliebten Dreien", "tokens": ["mehr", "a\u00b7ber", "ei\u00b7nes", "noch", ",", "da\u00df", "uns", "ge\u00b7lieb\u00b7ten", "Drei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADV", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "der fromme Himmel di\u00df zur Gunst hat angetan,", "tokens": ["der", "from\u00b7me", "Him\u00b7mel", "di\u00df", "zur", "Gunst", "hat", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df er in Fr\u00f6ligkeit uns l\u00e4sset schauen an", "tokens": ["da\u00df", "er", "in", "Fr\u00f6\u00b7lig\u00b7keit", "uns", "l\u00e4s\u00b7set", "schau\u00b7en", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "des lieben Vatern Tag. Was kan uns doch geschehen", "tokens": ["des", "lie\u00b7ben", "Va\u00b7tern", "Tag", ".", "Was", "kan", "uns", "doch", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "mehr Liebers als di\u00df eins, da\u00df wir den k\u00f6nnen sehen", "tokens": ["mehr", "Lie\u00b7bers", "als", "di\u00df", "eins", ",", "da\u00df", "wir", "den", "k\u00f6n\u00b7nen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KOKOM", "PDS", "PIS", "$,", "KOUS", "PPER", "ART", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "itzt seinen Tag begehn, der unsers Lebens Quell", "tokens": ["itzt", "sei\u00b7nen", "Tag", "be\u00b7gehn", ",", "der", "un\u00b7sers", "Le\u00b7bens", "Quell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und Gl\u00fcckes Ursprung ist? Der Himmel ist ihm hell'", "tokens": ["und", "Gl\u00fc\u00b7ckes", "Ur\u00b7sprung", "ist", "?", "Der", "Him\u00b7mel", "ist", "ihm", "hell'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VAFIN", "$.", "ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "und legt die Wolken hin. Kein Sturm, kein Blitz, kein Regen", "tokens": ["und", "legt", "die", "Wol\u00b7ken", "hin", ".", "Kein", "Sturm", ",", "kein", "Blitz", ",", "kein", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "und was uns mehr betr\u00fcbt, der ist ihm heut' entgegen.", "tokens": ["und", "was", "uns", "mehr", "be\u00b7tr\u00fcbt", ",", "der", "ist", "ihm", "heut'", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,", "PRELS", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der H\u00f6chste giebet selbst auch seinen Willen drein", "tokens": ["Der", "H\u00f6chs\u00b7te", "gie\u00b7bet", "selbst", "auch", "sei\u00b7nen", "Wil\u00b7len", "drein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und hei\u00dfet unsern Wundsch an ihm erf\u00fcllet sein.", "tokens": ["und", "hei\u00b7\u00dfet", "un\u00b7sern", "Wund\u00b7sch", "an", "ihm", "er\u00b7f\u00fcl\u00b7let", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Seht, Vater, diesen Tag zu zehnmal zehen Malen", "tokens": ["Seht", ",", "Va\u00b7ter", ",", "die\u00b7sen", "Tag", "zu", "zehn\u00b7mal", "ze\u00b7hen", "Ma\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "PDAT", "NN", "APPR", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "so fr\u00f6lich, als wir itzt! Gott gie\u00dfe seine Strahlen", "tokens": ["so", "fr\u00f6\u00b7lich", ",", "als", "wir", "itzt", "!", "Gott", "gie\u00b7\u00dfe", "sei\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "$.", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "des Gl\u00fcckes auf euch aus! Weil Wasser in der See,", "tokens": ["des", "Gl\u00fc\u00b7ckes", "auf", "euch", "aus", "!", "Weil", "Was\u00b7ser", "in", "der", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$.", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "weil Sternen in der Luft, in Wiesen s\u00fc\u00dfer Klee,", "tokens": ["weil", "Ster\u00b7nen", "in", "der", "Luft", ",", "in", "Wie\u00b7sen", "s\u00fc\u00b7\u00dfer", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "in W\u00e4ldern B\u00e4ume sind, so lange lebt vergn\u00fcget", "tokens": ["in", "W\u00e4l\u00b7dern", "B\u00e4u\u00b7me", "sind", ",", "so", "lan\u00b7ge", "lebt", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "VAFIN", "$,", "ADV", "ADV", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "und seht, wie alles sich zu unserm Besten f\u00fcget!", "tokens": ["und", "seht", ",", "wie", "al\u00b7les", "sich", "zu", "un\u00b7serm", "Bes\u00b7ten", "f\u00fc\u00b7get", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So viel zieh' f\u00f6rderhin des Gl\u00fcckes bei euch ein,", "tokens": ["So", "viel", "zieh'", "f\u00f6r\u00b7der\u00b7hin", "des", "Gl\u00fc\u00b7ckes", "bei", "euch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "so viel der Schlingen hier an unserm Bande sein!", "tokens": ["so", "viel", "der", "Schlin\u00b7gen", "hier", "an", "un\u00b7serm", "Ban\u00b7de", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zwar es geb\u00fchrt die Zier der angenehmen Tage,", "tokens": ["Zwar", "es", "ge\u00b7b\u00fchrt", "die", "Zier", "der", "an\u00b7ge\u00b7neh\u00b7men", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df man von lauter Lust und sch\u00f6nem Wesen sage,", "tokens": ["da\u00df", "man", "von", "lau\u00b7ter", "Lust", "und", "sch\u00f6\u00b7nem", "We\u00b7sen", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "itzt, da das reiche Feld in voller Rose steht,", "tokens": ["itzt", ",", "da", "das", "rei\u00b7che", "Feld", "in", "vol\u00b7ler", "Ro\u00b7se", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und mit so mancher Frucht und G\u0