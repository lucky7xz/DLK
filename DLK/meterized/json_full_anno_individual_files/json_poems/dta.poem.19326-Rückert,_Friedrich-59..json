{"dta.poem.19326": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Aehnlichkeiten Spur zu folgen hast du Freiheit,", "tokens": ["Der", "A\u00b7ehn\u00b7lich\u00b7kei\u00b7ten", "Spur", "zu", "fol\u00b7gen", "hast", "du", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VAFIN", "PPER", "NN", "$,"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Verwechseln darfst du nur sie nicht mit Einerleiheit.", "tokens": ["Ver\u00b7wech\u00b7seln", "darfst", "du", "nur", "sie", "nicht", "mit", "Ei\u00b7ner\u00b7lei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Ding, das du begreifst, ist freilich im Begriff,", "tokens": ["Das", "Ding", ",", "das", "du", "be\u00b7greifst", ",", "ist", "frei\u00b7lich", "im", "Be\u00b7griff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der Begriff ist nicht des Dinges Inbegriff.", "tokens": ["Doch", "der", "Be\u00b7griff", "ist", "nicht", "des", "Din\u00b7ges", "In\u00b7be\u00b7griff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wer sieht nicht, da\u00df sein Bild im Spiegel \u00e4hnlich sei", "tokens": ["Wer", "sieht", "nicht", ",", "da\u00df", "sein", "Bild", "im", "Spie\u00b7gel", "\u00e4hn\u00b7lich", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "APPRART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm selber? doch ist es mit ihm drum einerlei?", "tokens": ["Ihm", "sel\u00b7ber", "?", "doch", "ist", "es", "mit", "ihm", "drum", "ei\u00b7ner\u00b7lei", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$.", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PAV", "PTKVZ", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ob ich der Spiegel sei der Welt, ob sie der meine,", "tokens": ["Ob", "ich", "der", "Spie\u00b7gel", "sei", "der", "Welt", ",", "ob", "sie", "der", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir bleiben immer Zwei, worin sich zeigt das Eine.", "tokens": ["Wir", "blei\u00b7ben", "im\u00b7mer", "Zwei", ",", "wo\u00b7rin", "sich", "zeigt", "das", "Ei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "$,", "PWAV", "PRF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}