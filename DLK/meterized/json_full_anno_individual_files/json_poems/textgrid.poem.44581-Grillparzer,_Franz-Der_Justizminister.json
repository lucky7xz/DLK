{"textgrid.poem.44581": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Justizminister", "genre": "verse", "period": "N.A.", "pub_year": 1851, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sagen sich, da\u00df ein Minister schied,", "tokens": ["Sie", "sa\u00b7gen", "sich", ",", "da\u00df", "ein", "Mi\u00b7nis\u00b7ter", "schied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sagen sichs gleichgiltig leerer Mienen,", "tokens": ["Und", "sa\u00b7gen", "sichs", "gleich\u00b7gil\u00b7tig", "lee\u00b7rer", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Als w\u00e4rs ein neuer Ton zum alten Lied,", "tokens": ["Als", "w\u00e4rs", "ein", "neu\u00b7er", "Ton", "zum", "al\u00b7ten", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und die Justiz verk\u00f6rpert schon in ihnen.", "tokens": ["Und", "die", "Jus\u00b7tiz", "ver\u00b7k\u00f6r\u00b7pert", "schon", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "--++-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und wahrlich! erst gekn\u00fcpft des Rechtes Netz,", "tokens": ["Und", "wahr\u00b7lich", "!", "erst", "ge\u00b7kn\u00fcpft", "des", "Rech\u00b7tes", "Netz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Liegt Richter und Gericht schon im Gesetz.", "tokens": ["Liegt", "Rich\u00b7ter", "und", "Ge\u00b7richt", "schon", "im", "Ge\u00b7setz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch w\u00e4hrend mich kaum k\u00fcmmert der Verlauf,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "mich", "kaum", "k\u00fcm\u00b7mert", "der", "Ver\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Macht ein Gef\u00fchl sich frei und immer freier,", "tokens": ["Macht", "ein", "Ge\u00b7f\u00fchl", "sich", "frei", "und", "im\u00b7mer", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und pl\u00f6tzlich gehts vor meinen Augen auf,", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "gehts", "vor", "mei\u00b7nen", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich hebend wie ein ferner Wolkenschleier.", "tokens": ["Sich", "he\u00b7bend", "wie", "ein", "fer\u00b7ner", "Wol\u00b7ken\u00b7schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ist das nicht Frankfurt, die berufne Stadt?", "tokens": ["Ist", "das", "nicht", "Frank\u00b7furt", ",", "die", "be\u00b7ruf\u00b7ne", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum Rat berufen, aber jetzt zur Tat.", "tokens": ["Zum", "Rat", "be\u00b7ru\u00b7fen", ",", "a\u00b7ber", "jetzt", "zur", "Tat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Durch alle Stra\u00dfen wogt des Frevels Wut,", "tokens": ["Durch", "al\u00b7le", "Stra\u00b7\u00dfen", "wogt", "des", "Fre\u00b7vels", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Waffen schwingen mordbegierge H\u00e4nde,", "tokens": ["Die", "Waf\u00b7fen", "schwin\u00b7gen", "mord\u00b7be\u00b7gier\u00b7ge", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lichnowsky, Auerswald in ihrem Blut,", "tokens": ["Lich\u00b7nows\u00b7ky", ",", "Au\u00b7ers\u00b7wald", "in", "ih\u00b7rem", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und \u00fcbrall starre Barrikadenw\u00e4nde.", "tokens": ["Und", "\u00fcb\u00b7rall", "star\u00b7re", "Bar\u00b7ri\u00b7ka\u00b7den\u00b7w\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Freiheit, ihres eignen Wesens frei,", "tokens": ["Die", "Frei\u00b7heit", ",", "ih\u00b7res", "eig\u00b7nen", "We\u00b7sens", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Lehrt durch Gewalttat, redet durch Geschrei.", "tokens": ["Lehrt", "durch", "Ge\u00b7walt\u00b7tat", ",", "re\u00b7det", "durch", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "An Deutschlands Wurzel ist das Beil gelegt,", "tokens": ["An", "Deutschlands", "Wur\u00b7zel", "ist", "das", "Beil", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nur noch ein Streich, so sinkt die Eiche nieder,", "tokens": ["Nur", "noch", "ein", "Streich", ",", "so", "sinkt", "die", "Ei\u00b7che", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vergebens, was sich sp\u00e4ter hilfreich regt,", "tokens": ["Ver\u00b7ge\u00b7bens", ",", "was", "sich", "sp\u00e4\u00b7ter", "hilf\u00b7reich", "regt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PRF", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Stammes Last erdr\u00fcckt im Fall die Glieder.", "tokens": ["Des", "Stam\u00b7mes", "Last", "er\u00b7dr\u00fcckt", "im", "Fall", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer hilft? wer rettet? Wo ein Hort und Haupt?", "tokens": ["Wer", "hilft", "?", "wer", "ret\u00b7tet", "?", "Wo", "ein", "Hort", "und", "Haupt", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PWS", "VVFIN", "$.", "PWAV", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo, der an sich und an die Rettung glaubt?", "tokens": ["Wo", ",", "der", "an", "sich", "und", "an", "die", "Ret\u00b7tung", "glaubt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "APPR", "PRF", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch halt! ein Mann der Rede und der Schrift,", "tokens": ["Doch", "halt", "!", "ein", "Mann", "der", "Re\u00b7de", "und", "der", "Schrift", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bleibt seiner Herr im Gr\u00e4uel der Verw\u00fcstung,", "tokens": ["Bleibt", "sei\u00b7ner", "Herr", "im", "Gr\u00e4u\u00b7el", "der", "Ver\u00b7w\u00fcs\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tritt auf die Bresche, die verwaist er trifft,", "tokens": ["Tritt", "auf", "die", "Bre\u00b7sche", ",", "die", "ver\u00b7waist", "er", "trifft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und macht sein Friedenskleid zur Waffenr\u00fcstung.", "tokens": ["Und", "macht", "sein", "Frie\u00b7dens\u00b7kleid", "zur", "Waf\u00b7fen\u00b7r\u00fcs\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie sonst den Rat, so ordnet er den Krieg.", "tokens": ["Wie", "sonst", "den", "Rat", ",", "so", "ord\u00b7net", "er", "den", "Krieg", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Rechtstreit wars: dem Rechte blieb der Sieg.", "tokens": ["Ein", "Rechts\u00b7treit", "wars", ":", "dem", "Rech\u00b7te", "blieb", "der", "Sieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Mann warst du! \u2013 Was frag ich um dein Jetzt.", "tokens": ["Der", "Mann", "warst", "du", "!", "\u2013", "Was", "frag", "ich", "um", "dein", "Jetzt", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$.", "$(", "PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Heute ist ein Erbe doch des Gestern,", "tokens": ["Das", "Heu\u00b7te", "ist", "ein", "Er\u00b7be", "doch", "des", "Ge\u00b7stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Da\u00df etwa leicht man heute dich ersetzt,", "tokens": ["Da\u00df", "et\u00b7wa", "leicht", "man", "heu\u00b7te", "dich", "er\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "PIS", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll dich verkleinern nicht und nicht verl\u00e4stern.", "tokens": ["Soll", "dich", "ver\u00b7klei\u00b7nern", "nicht", "und", "nicht", "ver\u00b7l\u00e4s\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch jedem andern Schmach, der schnell vergi\u00dft", "tokens": ["Doch", "je\u00b7dem", "an\u00b7dern", "Schmach", ",", "der", "schnell", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des, was er war, in dem, was er nun ist.", "tokens": ["Des", ",", "was", "er", "war", ",", "in", "dem", ",", "was", "er", "nun", "ist", "."], "token