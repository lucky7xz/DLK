{"dta.poem.6680": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von dem Vergn\u00fcgen und Ergetzen,", "tokens": ["Von", "dem", "Ver\u00b7gn\u00fc\u00b7gen", "und", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So die Music in uns erweckt,", "tokens": ["So", "die", "Mu\u00b7sic", "in", "uns", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie wohl componirt, mu\u00df man zum Grunde setzen", "tokens": ["Wenn", "sie", "wohl", "com\u00b7po\u00b7nirt", ",", "mu\u00df", "man", "zum", "Grun\u00b7de", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "VMFIN", "PIS", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geheimer Z\u00fcge Krafft, so in der Seele steckt.", "tokens": ["Ge\u00b7hei\u00b7mer", "Z\u00fc\u00b7ge", "Krafft", ",", "so", "in", "der", "See\u00b7le", "steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sind in unsern Geist nat\u00fcrlich eingegossen,", "tokens": ["Die", "sind", "in", "un\u00b7sern", "Geist", "na\u00b7t\u00fcr\u00b7lich", "ein\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch den Gebrauch sind sie vermehret und vergr\u00f6ssert,", "tokens": ["Durch", "den", "Ge\u00b7brauch", "sind", "sie", "ver\u00b7meh\u00b7ret", "und", "ver\u00b7gr\u00f6s\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch M\u00fch und Flei\u00df verbessert:", "tokens": ["Durch", "M\u00fch", "und", "Flei\u00df", "ver\u00b7bes\u00b7sert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er wird dadurch, da\u00df er den Wohl-Laut offt genossen,", "tokens": ["Er", "wird", "da\u00b7durch", ",", "da\u00df", "er", "den", "Wohl\u00b7Laut", "offt", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Noch immer fertiger und mehr Erkenntni\u00df kriegen.", "tokens": ["Noch", "im\u00b7mer", "fer\u00b7ti\u00b7ger", "und", "mehr", "Er\u00b7kennt\u00b7ni\u00df", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist nicht c\u00f6rperlich so z\u00e4rtliches Vergn\u00fcgen.", "tokens": ["Es", "ist", "nicht", "c\u00f6r\u00b7per\u00b7lich", "so", "z\u00e4rt\u00b7li\u00b7ches", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}