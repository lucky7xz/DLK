{"dta.poem.20089": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Um Dich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Du wirst es nie zu T\u00fcchtgem bringen", "tokens": ["Du", "wirst", "es", "nie", "zu", "T\u00fccht\u00b7gem", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Deines Grames Tr\u00e4umerein,", "tokens": ["Bei", "Dei\u00b7nes", "Gra\u00b7mes", "Tr\u00e4u\u00b7me\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Thr\u00e4nen lassen nichts gelingen,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "las\u00b7sen", "nichts", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer schaffen will, mu\u00df fr\u00f6hlich sein.", "tokens": ["Wer", "schaf\u00b7fen", "will", ",", "mu\u00df", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl ", "tokens": ["Wohl"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Der in die Scholle niederbricht,", "tokens": ["Der", "in", "die", "Schol\u00b7le", "nie\u00b7der\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Reift nur heran bei Sonnenlicht.", "tokens": ["Reift", "nur", "he\u00b7ran", "bei", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}