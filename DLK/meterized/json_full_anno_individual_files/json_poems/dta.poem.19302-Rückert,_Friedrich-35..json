{"dta.poem.19302": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "35.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch kann nie so ganz ins Sinnliche versinken,", "tokens": ["Der", "Mensch", "kann", "nie", "so", "ganz", "ins", "Sinn\u00b7li\u00b7che", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist treibt ihn empor stets Geistesluft zu trinken.", "tokens": ["Der", "Geist", "treibt", "ihn", "em\u00b7por", "stets", "Geis\u00b7tes\u00b7luft", "zu", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch hat er seine Lung' erfrischt an Himmelshauchen,", "tokens": ["Doch", "hat", "er", "sei\u00b7ne", "Lung'", "er\u00b7frischt", "an", "Him\u00b7mels\u00b7hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Treibt es ihn bald genung zur\u00fcck in Schlamm zu tauchen.", "tokens": ["Treibt", "es", "ihn", "bald", "ge\u00b7nung", "zu\u00b7r\u00fcck", "in", "Schlamm", "zu", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So in sein Leben theilt sich der getheilte Trieb;", "tokens": ["So", "in", "sein", "Le\u00b7ben", "theilt", "sich", "der", "ge\u00b7theil\u00b7te", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht Vogel und nicht Fisch, was ist er? ein Amfib;", "tokens": ["Nicht", "Vo\u00b7gel", "und", "nicht", "Fisch", ",", "was", "ist", "er", "?", "ein", "Am\u00b7fib", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "KON", "PTKNEG", "NN", "$,", "PWS", "VAFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das nicht ganz Fisch mehr ist, dem stummen Abgrund eigen,", "tokens": ["Das", "nicht", "ganz", "Fisch", "mehr", "ist", ",", "dem", "stum\u00b7men", "Ab\u00b7grund", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ADJD", "ADV", "VAFIN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch noch nicht ganz Thier, ans feste Land zu steigen;", "tokens": ["Doch", "auch", "noch", "nicht", "ganz", "Thier", ",", "ans", "fes\u00b7te", "Land", "zu", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "ADV", "NN", "$,", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das jetzo sich erhebt, und sch\u00f6pft zu leben Luft,", "tokens": ["Das", "jet\u00b7zo", "sich", "er\u00b7hebt", ",", "und", "sch\u00f6pft", "zu", "le\u00b7ben", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PRF", "VVFIN", "$,", "KON", "VVFIN", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wieder sich begr\u00e4bt in feuchten Moderduft.", "tokens": ["Dann", "wie\u00b7der", "sich", "be\u00b7gr\u00e4bt", "in", "feuch\u00b7ten", "Mo\u00b7der\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Im innerlichen Streit mit sich befangen ewig,", "tokens": ["Im", "in\u00b7ner\u00b7li\u00b7chen", "Streit", "mit", "sich", "be\u00b7fan\u00b7gen", "e\u00b7wig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PRF", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ganze Lebenszeit bleibt es hindurch beidlebig.", "tokens": ["Die", "gan\u00b7ze", "Le\u00b7bens\u00b7zeit", "bleibt", "es", "hin\u00b7durch", "beid\u00b7le\u00b7big", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PAV", "ADJD", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.7": {"line.1": {"text": "Wird auch die Menschheit so in alle Ewigkeit", "tokens": ["Wird", "auch", "die", "Menschheit", "so", "in", "al\u00b7le", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier bleiben unerl\u00f6st von der Beidlebigkeit?", "tokens": ["Hier", "blei\u00b7ben", "un\u00b7er\u00b7l\u00f6st", "von", "der", "Beid\u00b7le\u00b7big\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Wird nie ihr bessrer Geist sie ihren niedren Wiegen", "tokens": ["Wird", "nie", "ihr", "bess\u00b7rer", "Geist", "sie", "ih\u00b7ren", "nied\u00b7ren", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entraffen, um mit ihr von H\u00f6h zu H\u00f6h zu fliegen?", "tokens": ["E\u00b7ntraf\u00b7fen", ",", "um", "mit", "ihr", "von", "H\u00f6h", "zu", "H\u00f6h", "zu", "flie\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUI", "APPR", "PPOSAT", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Soll immer nur der Geist allein, als wie der Schwan,", "tokens": ["Soll", "im\u00b7mer", "nur", "der", "Geist", "al\u00b7lein", ",", "als", "wie", "der", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschieden von dem Leib, sich schwingen himmelan?", "tokens": ["Ge\u00b7schie\u00b7den", "von", "dem", "Leib", ",", "sich", "schwin\u00b7gen", "him\u00b7me\u00b7lan", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRF", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}