{"dta.poem.3019": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling gr\u00fc\u00dft die Erd' und macht die Hoffnung gr\u00fcn,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "gr\u00fc\u00dft", "die", "Erd'", "und", "macht", "die", "Hoff\u00b7nung", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebe R\u00fchrung thaut, und meine Gr\u00e4ber bl\u00fchn.", "tokens": ["Der", "Lie\u00b7be", "R\u00fch\u00b7rung", "thaut", ",", "und", "mei\u00b7ne", "Gr\u00e4\u00b7ber", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das liebste was ich hab', ist Gottes Liebesgabe,", "tokens": ["Das", "liebs\u00b7te", "was", "ich", "hab'", ",", "ist", "Got\u00b7tes", "Lie\u00b7bes\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ich es nun im Grab', ob ichs im Herzen habe.", "tokens": ["Ob", "ich", "es", "nun", "im", "Grab'", ",", "ob", "ichs", "im", "Her\u00b7zen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPRART", "NN", "$,", "KOUS", "PIS", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das beste was ich bin, wird immer Gottes bleiben,", "tokens": ["Das", "bes\u00b7te", "was", "ich", "bin", ",", "wird", "im\u00b7mer", "Got\u00b7tes", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur mein B\u00f6ses mu\u00df ich ganz mir selbst zuschreiben.", "tokens": ["Und", "nur", "mein", "B\u00f6\u00b7ses", "mu\u00df", "ich", "ganz", "mir", "selbst", "zu\u00b7schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Versuch es nur und schreib es einem andern an,", "tokens": ["Ver\u00b7such", "es", "nur", "und", "schreib", "es", "ei\u00b7nem", "an\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du f\u00fchlst in dir, dadurch ist dirs nicht ausgethan.", "tokens": ["Du", "f\u00fchlst", "in", "dir", ",", "da\u00b7durch", "ist", "dirs", "nicht", "aus\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PAV", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wer nicht das Rechte wei\u00df, gut ists wenn ers nur thut;", "tokens": ["Wer", "nicht", "das", "Rech\u00b7te", "wei\u00df", ",", "gut", "ists", "wenn", "ers", "nur", "thut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$,", "ADJD", "VAFIN", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn er recht es wei\u00df, so ist es doppelt gut.", "tokens": ["Doch", "wenn", "er", "recht", "es", "wei\u00df", ",", "so", "ist", "es", "dop\u00b7pelt", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wer B\u00f6ses wei\u00df und thuts, der thut viel B\u00f6sres noch;", "tokens": ["Wer", "B\u00f6\u00b7ses", "wei\u00df", "und", "thuts", ",", "der", "thut", "viel", "B\u00f6s\u00b7res", "noch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "KON", "VVFIN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wer unwissend auch es thut, thut B\u00f6ses doch.", "tokens": ["Doch", "wer", "un\u00b7wis\u00b7send", "auch", "es", "thut", ",", "thut", "B\u00f6\u00b7ses", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "NE", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Gott ist was Gutes ist an jedem guten Triebe,", "tokens": ["Gott", "ist", "was", "Gu\u00b7tes", "ist", "an", "je\u00b7dem", "gu\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "NN", "VAFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Glanz am Mond, die Bl\u00fct' am Baum, in dir die Liebe.", "tokens": ["Der", "Glanz", "am", "Mond", ",", "die", "Bl\u00fct'", "am", "Baum", ",", "in", "dir", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "In jedem Geiste, der nicht zagt f\u00fcrs Licht zu kriegen,", "tokens": ["In", "je\u00b7dem", "Geis\u00b7te", ",", "der", "nicht", "zagt", "f\u00fcrs", "Licht", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist sichtbar Gottes Geist zur Welt herabgestiegen.", "tokens": ["Ist", "sicht\u00b7bar", "Got\u00b7tes", "Geist", "zur", "Welt", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wenn er im Kampf erliegt, kehrt er als Sieger heim,", "tokens": ["Wenn", "er", "im", "Kampf", "er\u00b7liegt", ",", "kehrt", "er", "als", "Sie\u00b7ger", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier lassend den mit Blut gepflanzten Friedenskeim.", "tokens": ["Hier", "las\u00b7send", "den", "mit", "Blut", "ge\u00b7pflanz\u00b7ten", "Frie\u00b7dens\u00b7keim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Den Geist mit der Natur sollst du zusammendichten,", "tokens": ["Den", "Geist", "mit", "der", "Na\u00b7tur", "sollst", "du", "zu\u00b7sam\u00b7men\u00b7dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Erd' in Himmelsglanz verkl\u00e4ren, nicht vernichten.", "tokens": ["Die", "Erd'", "in", "Him\u00b7mels\u00b7glanz", "ver\u00b7kl\u00e4\u00b7ren", ",", "nicht", "ver\u00b7nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Kehr auf die Sinnenwelt so deine Th\u00e4tigkeit,", "tokens": ["Kehr", "auf", "die", "Sin\u00b7nen\u00b7welt", "so", "dei\u00b7ne", "Th\u00e4\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht die Lust an ihr dich mit dir selbst entzweit.", "tokens": ["Da\u00df", "nicht", "die", "Lust", "an", "ihr", "dich", "mit", "dir", "selbst", "ent\u00b7zweit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "PPER", "PRF", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "An keinem niedern Stoff la\u00df die Gedanken haften;", "tokens": ["An", "kei\u00b7nem", "nie\u00b7dern", "Stoff", "la\u00df", "die", "Ge\u00b7dan\u00b7ken", "haf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sinn vom Gegenstand nimmt an die Eigenschaften.", "tokens": ["Der", "Sinn", "vom", "Ge\u00b7gen\u00b7stand", "nimmt", "an", "die", "Ei\u00b7gen\u00b7schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Betrachte liebend Gott, willst du gott\u00e4hnlich werden;", "tokens": ["Be\u00b7trach\u00b7te", "lie\u00b7bend", "Gott", ",", "willst", "du", "got\u00b7t\u00e4hn\u00b7lich", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn das Gem\u00fct nimmt an vom Liebsten die Geberden.", "tokens": ["Denn", "das", "Ge\u00b7m\u00fct", "nimmt", "an", "vom", "Liebs\u00b7ten", "die", "Ge\u00b7ber\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Doch willst du an der Welt unschuldig dich erbaun,", "tokens": ["Doch", "willst", "du", "an", "der", "Welt", "un\u00b7schul\u00b7dig", "dich", "er\u00b7baun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00dft alles du in Gott und Gott in Allem schaun.", "tokens": ["Mu\u00dft", "al\u00b7les", "du", "in", "Gott", "und", "Gott", "in", "Al\u00b7lem", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Und das ist gar nicht schwer; der h\u00f6chsten Liebe Spur", "tokens": ["Und", "das", "ist", "gar", "nicht", "schwer", ";", "der", "h\u00f6chs\u00b7ten", "Lie\u00b7be", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PTKNEG", "ADJD", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Niedersten zu schaun, hab' Liebesaugen nur!", "tokens": ["Im", "Nie\u00b7ders\u00b7ten", "zu", "schaun", ",", "hab'", "Lie\u00b7be\u00b7sau\u00b7gen", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die Liebe siehst du dann, wie dort im Reigen gehn", "tokens": ["Die", "Lie\u00b7be", "siehst", "du", "dann", ",", "wie", "dort", "im", "Rei\u00b7gen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Stern', in Blumen so hier auf den Gr\u00fcften stehn.", "tokens": ["Der", "Stern'", ",", "in", "Blu\u00b7men", "so", "hier", "auf", "den", "Gr\u00fcf\u00b7ten", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}