{"textgrid.poem.39824": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwaben im Winkel", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt her, die ihr mit feinen Witzen,", "tokens": ["Kommt", "her", ",", "die", "ihr", "mit", "fei\u00b7nen", "Wit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Nadelspitzen euch bewehrt!", "tokens": ["Mit", "Na\u00b7del\u00b7spit\u00b7zen", "euch", "be\u00b7wehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schwaben, \u00bbdie im Winkel sitzen,\u00ab", "tokens": ["Die", "Schwa\u00b7ben", ",", "\u00bb", "die", "im", "Win\u00b7kel", "sit\u00b7zen", ",", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "$(", "ART", "APPRART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwarten euch am frommen Herd!", "tokens": ["Er\u00b7war\u00b7ten", "euch", "am", "from\u00b7men", "Herd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kennt ihr auch wohl den schn\u00f6den Winkel,", "tokens": ["Kennt", "ihr", "auch", "wohl", "den", "schn\u00f6\u00b7den", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das d\u00fcstre Haus, das uns umzirkt? \u2013", "tokens": ["Das", "d\u00fcst\u00b7re", "Haus", ",", "das", "uns", "um\u00b7zirkt", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Smaragdnes Weinlaub, goldner Dinkel", "tokens": ["Sma\u00b7ragd\u00b7nes", "Wein\u00b7laub", ",", "gold\u00b7ner", "Din\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hat Wand und Estrich ihm durchwirkt.", "tokens": ["Hat", "Wand", "und", "Est\u00b7rich", "ihm", "durch\u00b7wirkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn der Flocken tr\u00fcb Gewimmel", "tokens": ["Und", "wenn", "der", "Flo\u00b7cken", "tr\u00fcb", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch lang verfinstert eure Luft,", "tokens": ["Noch", "lang", "ver\u00b7fins\u00b7tert", "eu\u00b7re", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spannt schon ein fr\u00fchlingsblauer Himmel", "tokens": ["Spannt", "schon", "ein", "fr\u00fch\u00b7lings\u00b7blau\u00b7er", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Dach aus \u00fcber Bl\u00fctenduft.", "tokens": ["Sein", "Dach", "aus", "\u00fc\u00b7ber", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Donau spielt auf unsrer Schwelle,", "tokens": ["Die", "Do\u00b7nau", "spielt", "auf", "uns\u00b7rer", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der J\u00fcngling Rhein tr\u00e4umt schon vom Strand;", "tokens": ["Der", "J\u00fcng\u00b7ling", "Rhein", "tr\u00e4umt", "schon", "vom", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Dichtung volle Wunderquelle", "tokens": ["Der", "Dich\u00b7tung", "vol\u00b7le", "Wun\u00b7der\u00b7quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schie\u00dft auf und eilt in's deutsche Land.", "tokens": ["Schie\u00dft", "auf", "und", "eilt", "in's", "deut\u00b7sche", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Stromg\u00f6tter tauchen aus den Fluten,", "tokens": ["Strom\u00b7g\u00f6t\u00b7ter", "tau\u00b7chen", "aus", "den", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der S\u00e4nger und der Seher naht,", "tokens": ["Der", "S\u00e4n\u00b7ger", "und", "der", "Se\u00b7her", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Munde Klang, im Auge Gluten;", "tokens": ["Im", "Mun\u00b7de", "Klang", ",", "im", "Au\u00b7ge", "Glu\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$,", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Winkelz\u00fcge sind sein Pfad:", "tokens": ["Nicht", "Win\u00b7kel\u00b7z\u00fc\u00b7ge", "sind", "sein", "Pfad", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier ", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Durch dessen Wort das Schicksal braust \u2013", "tokens": ["Durch", "des\u00b7sen", "Wort", "das", "Schick\u00b7sal", "braust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er stie\u00df das Puppenvolk am Zwirne", "tokens": ["Er", "stie\u00df", "das", "Pup\u00b7pen\u00b7volk", "am", "Zwir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von eurer Scene mit der Faust;", "tokens": ["Von", "eu\u00b7rer", "Sce\u00b7ne", "mit", "der", "Faust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort ", "tokens": ["Dort"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Er, Pindars Bruderstrom, entwallt;", "tokens": ["Er", ",", "Pin\u00b7dars", "Bru\u00b7der\u00b7strom", ",", "ent\u00b7wallt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "NE", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dem dunklen Ungrund gab Gestalt;", "tokens": ["Dem", "dunk\u00b7len", "Un\u00b7grund", "gab", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dort ", "tokens": ["Dort"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "In tiefer Zeiten heil'ges Lied \u2013", "tokens": ["In", "tie\u00b7fer", "Zei\u00b7ten", "heil'\u00b7ges", "Lied", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schwan, de\u00df Ton, geh\u00f6hnt von Unken,", "tokens": ["Ein", "Schwan", ",", "de\u00df", "Ton", ",", "ge\u00b7h\u00f6hnt", "von", "Un\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hoch \u00fcber Land und Meere zieht.", "tokens": ["Hoch", "\u00fc\u00b7ber", "Land", "und", "Mee\u00b7re", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Aus diesem Winkel schritt auch ", "tokens": ["Aus", "die\u00b7sem", "Win\u00b7kel", "schritt", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdeckter Bl\u00f6\u00dfe stolzer Schild,", "tokens": ["Ver\u00b7deck\u00b7ter", "Bl\u00f6\u00b7\u00dfe", "stol\u00b7zer", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dessen Blut manch d\u00fcrft'ger Egel,", "tokens": ["Von", "des\u00b7sen", "Blut", "manch", "d\u00fcrft'\u00b7ger", "E\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Augenblicke trunken, schwillt.", "tokens": ["F\u00fcr", "Au\u00b7gen\u00b7bli\u00b7cke", "trun\u00b7ken", ",", "schwillt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nicht spotten sollt ihr unsres Strebens:", "tokens": ["Nicht", "spot\u00b7ten", "sollt", "ihr", "uns\u00b7res", "Stre\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch unser Stral entsprang dem Quell,", "tokens": ["Auch", "un\u00b7ser", "Stral", "ent\u00b7sprang", "dem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem keuschen Born des Dichterlebens,", "tokens": ["Dem", "keu\u00b7schen", "Born", "des", "Dich\u00b7ter\u00b7le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lauter will er bleiben, hell.", "tokens": ["Und", "lau\u00b7ter", "will", "er", "blei\u00b7ben", ",", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Den weiten Erdkreis f\u00fcllt Gemeinheit,", "tokens": ["Den", "wei\u00b7ten", "Erd\u00b7kreis", "f\u00fcllt", "Ge\u00b7mein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gro\u00df war sie, frech, zu jeder Zeit;", "tokens": ["Gro\u00df", "war", "sie", ",", "frech", ",", "zu", "je\u00b7der", "Zeit", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJD", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Gute bleibt an Zahl die Kleinheit,", "tokens": ["Das", "Gu\u00b7te", "bleibt", "an", "Zahl", "die", "Klein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihr, ihr scheltet's \u00bbKleinlichkeit?\u00ab", "tokens": ["Und", "ihr", ",", "ihr", "schel\u00b7tet's", "\u00bb", "Klein\u00b7lich\u00b7keit", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Hei\u00dft kleinlich der euch, der die Gruben", "tokens": ["Hei\u00dft", "klein\u00b7lich", "der", "euch", ",", "der", "die", "Gru\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Unsaubern L\u00fcgentr\u00f6dels scheut,", "tokens": ["Un\u00b7sau\u00b7bern", "L\u00fc\u00b7gen\u00b7tr\u00f6\u00b7dels", "scheut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht seine Hand dem Lotterbuben,", "tokens": ["Nicht", "sei\u00b7ne", "Hand", "dem", "Lot\u00b7ter\u00b7bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem feilen Museheuchler beut?", "tokens": ["Dem", "fei\u00b7len", "Mu\u00b7se\u00b7heuch\u00b7ler", "beut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So mag, wer will, im Sumpfe spritzen", "tokens": ["So", "mag", ",", "wer", "will", ",", "im", "Sump\u00b7fe", "sprit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "$,", "PWS", "VMFIN", "$,", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei aller Fr\u00f6sche gr\u00fcner Brut;", "tokens": ["Bei", "al\u00b7ler", "Fr\u00f6\u00b7sche", "gr\u00fc\u00b7ner", "Brut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir Schwaben, \u00bbdie im Winkel sitzen,\u00ab", "tokens": ["Wir", "Schwa\u00b7ben", ",", "\u00bb", "die", "im", "Win\u00b7kel", "sit\u00b7zen", ",", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "$(", "ART", "APPRART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir tauchen uns in reine Flut;", "tokens": ["Wir", "tau\u00b7chen", "uns", "in", "rei\u00b7ne", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Wir schwingen uns, wie unsre Seher,", "tokens": ["Wir", "schwin\u00b7gen", "uns", ",", "wie", "uns\u00b7re", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Adler, auf zu Sonn' und Blitz;", "tokens": ["Die", "Ad\u00b7ler", ",", "auf", "zu", "Sonn'", "und", "Blitz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Werft uns in's Blaue nach, ihr Schm\u00e4her,", "tokens": ["Werft", "uns", "in's", "Blau\u00b7e", "nach", ",", "ihr", "Schm\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "