{"dta.poem.8110": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott sey und dem Lamm sey, das erw\u00fcrgt ward, Anbetung!", "tokens": ["Gott", "sey", "und", "dem", "Lamm", "sey", ",", "das", "er\u00b7w\u00fcrgt", "ward", ",", "An\u00b7be\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ART", "NN", "VAFIN", "$,", "PRELS", "VVPP", "VAFIN", "$,", "NN", "$."], "meter": "++--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Jubelpreis dem erhabnen Sohn! Du entriefst der Nacht", "tokens": ["Ju\u00b7bel\u00b7preis", "dem", "er\u00b7hab\u00b7nen", "Sohn", "!", "Du", "en\u00b7triefst", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der Verwerfung, die der Tod traf! o wir sind", "tokens": ["Der", "Ver\u00b7wer\u00b7fung", ",", "die", "der", "Tod", "traf", "!", "o", "wir", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "FM", "PPER", "VAFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Entflohn dem Abgrund des Verderbens!", "tokens": ["Ent\u00b7flohn", "dem", "Ab\u00b7grund", "des", "Ver\u00b7der\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}