{"dta.poem.15858": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr, da du jedem Ding hast aufgedr\u00fcckt dein Zeichen,", "tokens": ["Herr", ",", "da", "du", "je\u00b7dem", "Ding", "hast", "auf\u00b7ge\u00b7dr\u00fcckt", "dein", "Zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VAFIN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch einem K\u00f6nige darf ich dich wol vergleichen.", "tokens": ["Auch", "ei\u00b7nem", "K\u00f6\u00b7ni\u00b7ge", "darf", "ich", "dich", "wol", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein K\u00f6nig w\u00e4re das von unbescholtnem Preise,", "tokens": ["Ein", "K\u00f6\u00b7nig", "w\u00e4\u00b7re", "das", "von", "un\u00b7be\u00b7scholt\u00b7nem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der w\u00e4r' in seinem Reich allm\u00e4chtig und allweise,", "tokens": ["Der", "w\u00e4r'", "in", "sei\u00b7nem", "Reich", "all\u00b7m\u00e4ch\u00b7tig", "und", "all\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie du in deinem bist, und h\u00e4tte so sein Land", "tokens": ["Wie", "du", "in", "dei\u00b7nem", "bist", ",", "und", "h\u00e4t\u00b7te", "so", "sein", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "VAFIN", "$,", "KON", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner, wie du hast die Welt in deiner Hand.", "tokens": ["In", "sei\u00b7ner", ",", "wie", "du", "hast", "die", "Welt", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "PWAV", "PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gen\u00fcgen w\u00fcrd' ihm nicht, die Z\u00fcgel nur zu fassen", "tokens": ["Ge\u00b7n\u00fc\u00b7gen", "w\u00fcrd'", "ihm", "nicht", ",", "die", "Z\u00fc\u00b7gel", "nur", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Ganzen, Einzelnes dem Gl\u00fcck zu \u00fcberlassen.", "tokens": ["Des", "Gan\u00b7zen", ",", "Ein\u00b7zel\u00b7nes", "dem", "Gl\u00fcck", "zu", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Er griff' ins kleinste Glied vom gro\u00dfen Radgetriebe", "tokens": ["Er", "griff'", "ins", "kleins\u00b7te", "Glied", "vom", "gro\u00b7\u00dfen", "Rad\u00b7ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit seiner Weisheit ein, mit seiner Macht und Liebe.", "tokens": ["Mit", "sei\u00b7ner", "Weis\u00b7heit", "ein", ",", "mit", "sei\u00b7ner", "Macht", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Diener dienten ihm, die sich nur wollen dienen,", "tokens": ["Die", "Die\u00b7ner", "dien\u00b7ten", "ihm", ",", "die", "sich", "nur", "wol\u00b7len", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PRF", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fortwirkt' ungeschw\u00e4cht sein erster Sto\u00df in ihnen.", "tokens": ["Und", "fort\u00b7wirkt'", "un\u00b7ge\u00b7schw\u00e4cht", "sein", "ers\u00b7ter", "Sto\u00df", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Als Mitte f\u00fchlt' er sich, aus der die Stralen flammen,", "tokens": ["Als", "Mit\u00b7te", "f\u00fchlt'", "er", "sich", ",", "aus", "der", "die", "Stra\u00b7len", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "PRF", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fa\u00dfte in sein Herz die tausende zusammen.", "tokens": ["Und", "fa\u00df\u00b7te", "in", "sein", "Herz", "die", "tau\u00b7sen\u00b7de", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Du, der allm\u00e4chtig lenkt, was er allweise denkt,", "tokens": ["Du", ",", "der", "all\u00b7m\u00e4ch\u00b7tig", "lenkt", ",", "was", "er", "all\u00b7wei\u00b7se", "denkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur du, mein K\u00f6nig, bist ein K\u00f6nig unbeschr\u00e4nkt.", "tokens": ["Nur", "du", ",", "mein", "K\u00f6\u00b7nig", ",", "bist", "ein", "K\u00f6\u00b7nig", "un\u00b7be\u00b7schr\u00e4nkt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du bist der K\u00f6nig, der die K\u00f6nigskronen schenkt", "tokens": ["Du", "bist", "der", "K\u00f6\u00b7nig", ",", "der", "die", "K\u00f6\u00b7nigs\u00b7kro\u00b7nen", "schenkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den K\u00f6n'gen, deren Haupt vor dir in Staub sich senkt.", "tokens": ["Den", "K\u00f6n'\u00b7gen", ",", "de\u00b7ren", "Haupt", "vor", "dir", "in", "Staub", "sich", "senkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPR", "PPER", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}