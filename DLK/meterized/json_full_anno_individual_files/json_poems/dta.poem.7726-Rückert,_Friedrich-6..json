{"dta.poem.7726": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum K\u00f6nig sendet ein Erobrer die Gesandten,", "tokens": ["Zum", "K\u00f6\u00b7nig", "sen\u00b7det", "ein", "E\u00b7rob\u00b7rer", "die", "Ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die fordern zum Tribut ihn auf als Schutzverwandten.", "tokens": ["Die", "for\u00b7dern", "zum", "Tri\u00b7but", "ihn", "auf", "als", "Schutz\u00b7ver\u00b7wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PPER", "APPR", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da wollt' er seine Pracht recht ihnen lassen scheinen,", "tokens": ["Da", "wollt'", "er", "sei\u00b7ne", "Pracht", "recht", "ih\u00b7nen", "las\u00b7sen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "ADJD", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeigte sich geschm\u00fcckt mit Perl' und Edelsteinen.", "tokens": ["Und", "zeig\u00b7te", "sich", "ge\u00b7schm\u00fcckt", "mit", "Perl'", "und", "E\u00b7del\u00b7stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entlassen wollt' er sie von seinem Glanz geblendet:", "tokens": ["Ent\u00b7las\u00b7sen", "wollt'", "er", "sie", "von", "sei\u00b7nem", "Glanz", "ge\u00b7blen\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Tr\u00e4gt solchen Schmuck der Mann, der euch verwegen sendet?", "tokens": ["Tr\u00e4gt", "sol\u00b7chen", "Schmuck", "der", "Mann", ",", "der", "euch", "ver\u00b7we\u00b7gen", "sen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie sprachen: Solchen nicht, doch andern, auch wohl theuer;", "tokens": ["Sie", "spra\u00b7chen", ":", "Sol\u00b7chen", "nicht", ",", "doch", "an\u00b7dern", ",", "auch", "wohl", "theu\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VMFIN", "PTKNEG", "$,", "ADV", "PIS", "$,", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Augen spr\u00fchen ihm, wie Edelsteine, Feuer;", "tokens": ["Die", "Au\u00b7gen", "spr\u00fc\u00b7hen", "ihm", ",", "wie", "E\u00b7del\u00b7stei\u00b7ne", ",", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und wenn am Tag der Schlacht ihm wird die Stirne hei\u00df,", "tokens": ["Und", "wenn", "am", "Tag", "der", "Schlacht", "ihm", "wird", "die", "Stir\u00b7ne", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umdiademet ihn mit Perlentropfen Schwei\u00df.", "tokens": ["Um\u00b7dia\u00b7de\u00b7met", "ihn", "mit", "Per\u00b7len\u00b7trop\u00b7fen", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wer solchen Schmuck tr\u00e4gt, ihm f\u00e4llt leicht der andre zu,", "tokens": ["Wer", "sol\u00b7chen", "Schmuck", "tr\u00e4gt", ",", "ihm", "f\u00e4llt", "leicht", "der", "and\u00b7re", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Abfallend einem, der zur Schau ihn tr\u00e4gt wie du.", "tokens": ["Ab\u00b7fal\u00b7lend", "ei\u00b7nem", ",", "der", "zur", "Schau", "ihn", "tr\u00e4gt", "wie", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}