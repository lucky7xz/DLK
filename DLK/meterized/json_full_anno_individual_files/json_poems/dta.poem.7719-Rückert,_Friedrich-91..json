{"dta.poem.7719": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was geschehn ist in der Welt derweile?", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ge\u00b7schehn", "ist", "in", "der", "Welt", "der\u00b7wei\u00b7le", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "VVPP", "VAFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewi\u00df viel Wichtiges in dem und jenem Theile.", "tokens": ["Ge\u00b7wi\u00df", "viel", "Wich\u00b7ti\u00b7ges", "in", "dem", "und", "je\u00b7nem", "Thei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein es hat mein Ohr, mein Auge nicht ber\u00fchrt,", "tokens": ["Al\u00b7lein", "es", "hat", "mein", "Ohr", ",", "mein", "Au\u00b7ge", "nicht", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keine Ahnung auch hab' ich davon gesp\u00fcrt.", "tokens": ["Und", "kei\u00b7ne", "Ah\u00b7nung", "auch", "hab'", "ich", "da\u00b7von", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und gleichwol ist es da, nur ohne da\u00df ichs wei\u00df,", "tokens": ["Und", "gleich\u00b7wol", "ist", "es", "da", ",", "nur", "oh\u00b7ne", "da\u00df", "ichs", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "APPR", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und macht, auch unbemerkt, schon kalt mir oder hei\u00df;", "tokens": ["Und", "macht", ",", "auch", "un\u00b7be\u00b7merkt", ",", "schon", "kalt", "mir", "o\u00b7der", "hei\u00df", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Weil nichts den gro\u00dfen Leib der Menschheit kann ber\u00fchren,", "tokens": ["Weil", "nichts", "den", "gro\u00b7\u00dfen", "Leib", "der", "Menschheit", "kann", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Davon nicht Mitgef\u00fchl die Glieder m\u00fc\u00dften sp\u00fcren.", "tokens": ["Da\u00b7von", "nicht", "Mit\u00b7ge\u00b7f\u00fchl", "die", "Glie\u00b7der", "m\u00fc\u00df\u00b7ten", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und k\u00f6nnt' ich klar nur in des Herzen Spiegel sehn,", "tokens": ["Und", "k\u00f6nnt'", "ich", "klar", "nur", "in", "des", "Her\u00b7zen", "Spie\u00b7gel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So f\u00e4nd' ich schon darin, was in der Welt geschehn.", "tokens": ["So", "f\u00e4nd'", "ich", "schon", "da\u00b7rin", ",", "was", "in", "der", "Welt", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nun mu\u00df ich warten, bis zuletzt Ger\u00fccht und Zeitung", "tokens": ["Nun", "mu\u00df", "ich", "war\u00b7ten", ",", "bis", "zu\u00b7letzt", "Ge\u00b7r\u00fccht", "und", "Zei\u00b7tung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu mir gelangt mit des Geschehenen Verbreitung;", "tokens": ["Zu", "mir", "ge\u00b7langt", "mit", "des", "Ge\u00b7sche\u00b7he\u00b7nen", "Ver\u00b7brei\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Da\u00df mich mitfreue, mitbetr\u00fcbe, was betraf", "tokens": ["Da\u00df", "mich", "mit\u00b7freu\u00b7e", ",", "mit\u00b7be\u00b7tr\u00fc\u00b7be", ",", "was", "be\u00b7traf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Wohl und Weh die Welt, derweil ich lag im Schlaf.", "tokens": ["Von", "Wohl", "und", "Weh", "die", "Welt", ",", "der\u00b7weil", "ich", "lag", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}