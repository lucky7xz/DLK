{"dta.poem.145": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er sie entbl\u00f6sset sahe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Schaut/ wie die Rosen doch/ die milchern Brust bekr\u00f6hnen/", "tokens": ["Schaut", "/", "wie", "die", "Ro\u00b7sen", "doch", "/", "die", "mil\u00b7chern", "Brust", "be\u00b7kr\u00f6h\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "ART", "NN", "ADV", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie die Anmuth da mit theuren Perlen spielt/", "tokens": ["Und", "wie", "die", "An\u00b7muth", "da", "mit", "theu\u00b7ren", "Per\u00b7len", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie unvergleichlich sind die Glieder dieser Sch\u00f6nen/", "tokens": ["Wie", "un\u00b7ver\u00b7gleich\u00b7lich", "sind", "die", "Glie\u00b7der", "die\u00b7ser", "Sch\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Sch\u00f6nheit hat daran ein Meister-St\u00fcck erziehlt.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "da\u00b7ran", "ein", "Meis\u00b7ter\u00b7St\u00fcck", "er\u00b7ziehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Sie hat da ihren Thron der Liebe auffgestelt/", "tokens": ["Sie", "hat", "da", "ih\u00b7ren", "Thron", "der", "Lie\u00b7be", "auff\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Schwaanen reine Pracht reicht nicht an diese Br\u00fcste/", "tokens": ["Der", "Schwaa\u00b7nen", "rei\u00b7ne", "Pracht", "reicht", "nicht", "an", "die\u00b7se", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der feinste ", "tokens": ["Der", "feins\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Es mag ", "tokens": ["Es", "mag"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Hier zeigt sich eine Frucht die viel mahl sch\u00f6ner ist/", "tokens": ["Hier", "zeigt", "sich", "ei\u00b7ne", "Frucht", "die", "viel", "mahl", "sch\u00f6\u00b7ner", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ART", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der bunte Sommer lacht auf den beliebten Wangen", "tokens": ["Der", "bun\u00b7te", "Som\u00b7mer", "lacht", "auf", "den", "be\u00b7lieb\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Es zeigt sich Milch und Blut auf deinen ", "tokens": ["Es", "zeigt", "sich", "Milch", "und", "Blut", "auf", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit diesen sind sie auch zur Gn\u00fcge angef\u00fcllt/", "tokens": ["Mit", "die\u00b7sen", "sind", "sie", "auch", "zur", "Gn\u00fc\u00b7ge", "an\u00b7ge\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und stecket eine Krafft in denen Meer-Corallen?", "tokens": ["Und", "ste\u00b7cket", "ei\u00b7ne", "Krafft", "in", "de\u00b7nen", "Meer\u00b7Coral\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PRELS", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.16": {"text": "So ist dieselbige hier gleichfals eingeh\u00fcllt.", "tokens": ["So", "ist", "die\u00b7sel\u00b7bi\u00b7ge", "hier", "gleich\u00b7fals", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gantz Morgenland zinst nicht die Perlen/ die den gleichen/", "tokens": ["Gantz", "Mor\u00b7gen\u00b7land", "zinst", "nicht", "die", "Per\u00b7len", "/", "die", "den", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$(", "ART", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die dieser zarte Leib so h\u00e4uffig sehen l\u00e4st/", "tokens": ["Die", "die\u00b7ser", "zar\u00b7te", "Leib", "so", "h\u00e4uf\u00b7fig", "se\u00b7hen", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "ADV", "ADJD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der weisse Atla\u00df mu\u00df vor diesen Gliedern weichen/", "tokens": ["Der", "weis\u00b7se", "At\u00b7la\u00df", "mu\u00df", "vor", "die\u00b7sen", "Glie\u00b7dern", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auroren sind umsonst die Thr\u00e4nen ausgepre\u00dft.", "tokens": ["Au\u00b7ro\u00b7ren", "sind", "um\u00b7sonst", "die", "Thr\u00e4\u00b7nen", "aus\u00b7ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die rund-gew\u00f6lbte Schoo\u00df ist artig ausgezieret", "tokens": ["Die", "run\u00b7dge\u00b7w\u00f6lb\u00b7te", "Schoo\u00df", "ist", "ar\u00b7tig", "aus\u00b7ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Anmuth hat dahin den besten Schatz gelegt;", "tokens": ["Die", "An\u00b7muth", "hat", "da\u00b7hin", "den", "bes\u00b7ten", "Schatz", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Der wird aufs lieblichste mit aller Lust gepflegt.", "tokens": ["Der", "wird", "aufs", "lieb\u00b7lichs\u00b7te", "mit", "al\u00b7ler", "Lust", "ge\u00b7pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da suchet man den Grund der Wollust zu ergr\u00fcnden/", "tokens": ["Da", "su\u00b7chet", "man", "den", "Grund", "der", "Wol\u00b7lust", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Erg\u00f6tzen kan man da bey Lust und Anmuth finden/", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", "kan", "man", "da", "bey", "Lust", "und", "An\u00b7muth", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein s\u00fcsses Etwas mehrt die stille Lieblichkeit.", "tokens": ["Ein", "s\u00fcs\u00b7ses", "Et\u00b7was", "mehrt", "die", "stil\u00b7le", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Lysette z\u00fcrne nicht/ ich sehe diese Sch\u00e4tze", "tokens": ["Ly\u00b7set\u00b7te", "z\u00fcr\u00b7ne", "nicht", "/", "ich", "se\u00b7he", "die\u00b7se", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$(", "PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "In der Vollkommenheit/ die mich err\u00f6hten macht/", "tokens": ["In", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "/", "die", "mich", "er\u00b7r\u00f6h\u00b7ten", "macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PRELS", "PPER", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein Geist ist schon bestrickt in dem so sch\u00f6nen Netze/", "tokens": ["Mein", "Geist", "ist", "schon", "be\u00b7strickt", "in", "dem", "so", "sch\u00f6\u00b7nen", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "APPR", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Freyheit gebe ich die letzte gute Nacht.", "tokens": ["Der", "Frey\u00b7heit", "ge\u00b7be", "ich", "die", "letz\u00b7te", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Actaeon starb als er Dianen nur erblickte", "tokens": ["Ac\u00b7tae\u00b7on", "starb", "als", "er", "Di\u00b7a\u00b7nen", "nur", "er\u00b7blick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KOUS", "PPER", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich sterbe ebenfals der edlen Freyheit ab/", "tokens": ["Ich", "ster\u00b7be", "e\u00b7ben\u00b7fals", "der", "ed\u00b7len", "Frey\u00b7heit", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie legte sich/ als mich dein Augen-Glantz ber\u00fcckte/", "tokens": ["Sie", "leg\u00b7te", "sich", "/", "als", "mich", "dein", "Au\u00b7gen\u00b7Glantz", "be\u00b7r\u00fcck\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Nun findet sie vor sich in deiner Brust das Grab.", "tokens": ["Nun", "fin\u00b7det", "sie", "vor", "sich", "in", "dei\u00b7ner", "Brust", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}