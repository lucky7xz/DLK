{"textgrid.poem.37247": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[sehr tadelnswert ist unser Tun]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehr tadelnswert ist unser Tun,", "tokens": ["Sehr", "ta\u00b7delns\u00b7wert", "ist", "un\u00b7ser", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind nicht brav und bieder. \u2013", "tokens": ["Wir", "sind", "nicht", "brav", "und", "bie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesetzt den Fall, es k\u00e4me nun", "tokens": ["Ge\u00b7setzt", "den", "Fall", ",", "es", "k\u00e4\u00b7me", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die S\u00fcndflut noch mal wieder.", "tokens": ["Die", "S\u00fcnd\u00b7flut", "noch", "mal", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das w\u00e4r ein Zappeln und Geschreck!", "tokens": ["Das", "w\u00e4r", "ein", "Zap\u00b7peln", "und", "Ge\u00b7schreck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir tauchten alle unter;", "tokens": ["Wir", "tauch\u00b7ten", "al\u00b7le", "un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann kr\u00f6chen wir wieder aus dem Dreck", "tokens": ["Dann", "kr\u00f6\u00b7chen", "wir", "wie\u00b7der", "aus", "dem", "Dreck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und w\u00e4ren, wie sonst, recht munter.", "tokens": ["Und", "w\u00e4\u00b7ren", ",", "wie", "sonst", ",", "recht", "mun\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWAV", "ADV", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sehr tadelnswert ist unser Tun,", "tokens": ["Sehr", "ta\u00b7delns\u00b7wert", "ist", "un\u00b7ser", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sind nicht brav und bieder. \u2013", "tokens": ["Wir", "sind", "nicht", "brav", "und", "bie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesetzt den Fall, es k\u00e4me nun", "tokens": ["Ge\u00b7setzt", "den", "Fall", ",", "es", "k\u00e4\u00b7me", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die S\u00fcndflut noch mal wieder.", "tokens": ["Die", "S\u00fcnd\u00b7flut", "noch", "mal", "wie\u00b7der