{"dta.poem.1939": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Thut ein gesezter Geist/", "tokens": ["Thut", "ein", "ge\u00b7sez\u00b7ter", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was Recht und Tugend heist/", "tokens": ["Was", "Recht", "und", "Tu\u00b7gend", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kan noch P\u00f6fels-Wutt", "tokens": ["So", "kan", "noch", "P\u00f6\u00b7fels\u00b7Wutt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die B\u00f6ses heist und thut/", "tokens": ["Die", "B\u00f6\u00b7ses", "heist", "und", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch saures F\u00fcrsten-Blicken", "tokens": ["Noch", "sau\u00b7res", "F\u00fcrs\u00b7ten\u00b7Bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den steiffen Sinn verr\u00fccken/", "tokens": ["Den", "steif\u00b7fen", "Sinn", "ver\u00b7r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ob Wind und Welle braust/", "tokens": ["Ob", "Wind", "und", "Wel\u00b7le", "braust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ob Sturm und Wetter saust/", "tokens": ["Ob", "Sturm", "und", "Wet\u00b7ter", "saust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ob schon der Krey\u00df der Welt", "tokens": ["Ob", "schon", "der", "Krey\u00df", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "In einen Hauffen f\u00e4llt/", "tokens": ["In", "ei\u00b7nen", "Hauf\u00b7fen", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Sieht er sich ohn Erschrecken", "tokens": ["Sieht", "er", "sich", "ohn", "Er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Flutt/ Staub und Schutt bedecken.", "tokens": ["Flutt", "/", "Staub", "und", "Schutt", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}