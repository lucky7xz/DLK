{"dta.poem.23432": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kinderzucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Feinde kannst du t\u00f6dten,", "tokens": ["Die", "Fein\u00b7de", "kannst", "du", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie meine Mutter spricht;", "tokens": ["Wie", "mei\u00b7ne", "Mut\u00b7ter", "spricht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00f6r' ich sie auch sagen", "tokens": ["Doch", "h\u00f6r'", "ich", "sie", "auch", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar ernst an manchen Tagen:", "tokens": ["Gar", "ernst", "an", "man\u00b7chen", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Maik\u00e4fer t\u00f6dte nicht!", "tokens": ["Mai\u00b7k\u00e4\u00b7fer", "t\u00f6d\u00b7te", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Dann frag' ich meine Mutter:", "tokens": ["Dann", "frag'", "ich", "mei\u00b7ne", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer denn die Feinde sind?", "tokens": ["Wer", "denn", "die", "Fein\u00b7de", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sagt sie mir geschwind sie,", "tokens": ["Dann", "sagt", "sie", "mir", "ge\u00b7schwind", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die deutschen Feinde sind die \u2014", "tokens": ["Die", "deut\u00b7schen", "Fein\u00b7de", "sind", "die"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Franzosen, liebes Kind.", "tokens": ["Fran\u00b7zo\u00b7sen", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hast Recht wohl, liebe Mutter,", "tokens": ["Hast", "Recht", "wohl", ",", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gut da\u00df ich's endlich wei\u00df:", "tokens": ["Gut", "da\u00df", "ich's", "end\u00b7lich", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Franz\u00f6sische Vocabeln", "tokens": ["Fran\u00b7z\u00f6\u00b7si\u00b7sche", "Vo\u00b7ca\u00b7beln"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Lafontaine's Fabeln,", "tokens": ["Und", "La\u00b7fon\u00b7tai\u00b7ne's", "Fa\u00b7beln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die kosten mir viel Schwei\u00df.", "tokens": ["Die", "kos\u00b7ten", "mir", "viel", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}