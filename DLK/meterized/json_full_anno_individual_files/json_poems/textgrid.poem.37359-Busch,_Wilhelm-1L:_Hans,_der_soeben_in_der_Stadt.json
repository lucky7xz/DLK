{"textgrid.poem.37359": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hans, der soeben in der Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hans, der soeben in der Stadt", "tokens": ["Hans", ",", "der", "soe\u00b7ben", "in", "der", "Stadt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein fettes Schwein verwertet hat,", "tokens": ["Sein", "fet\u00b7tes", "Schwein", "ver\u00b7wer\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ging sp\u00e4t nach Haus bei Mondenschein.", "tokens": ["Ging", "sp\u00e4t", "nach", "Haus", "bei", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Fremder folgt und holt ihn ein.", "tokens": ["Ein", "Frem\u00b7der", "folgt", "und", "holt", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gr\u00fc\u00df Gott, rief Hans, das trifft sich gut,", "tokens": ["Gr\u00fc\u00df", "Gott", ",", "rief", "Hans", ",", "das", "trifft", "sich", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "VVFIN", "NE", "$,", "PDS", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "i