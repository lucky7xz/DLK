{"dta.poem.7136": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir im Schlaf offt gute Reime", "tokens": ["Wenn", "wir", "im", "Schlaf", "offt", "gu\u00b7te", "Rei\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn alle Schw\u00fcrigkeit erfinden:", "tokens": ["Ohn", "al\u00b7le", "Schw\u00fc\u00b7rig\u00b7keit", "er\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn wir im Traum recht gute Lehren", "tokens": ["Wenn", "wir", "im", "Traum", "recht", "gu\u00b7te", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Fremden etwan h\u00f6ren,", "tokens": ["Von", "ei\u00b7nem", "Frem\u00b7den", "et\u00b7wan", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie geht dies zu? geschicht es ohne mich,", "tokens": ["Wie", "geht", "dies", "zu", "?", "ge\u00b7schicht", "es", "oh\u00b7ne", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "PTKVZ", "$.", "VVPP", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df jemand in mir mit mir spricht?", "tokens": ["Da\u00df", "je\u00b7mand", "in", "mir", "mit", "mir", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bin ich derselbige, der mir wovon Bericht,", "tokens": ["Bin", "ich", "der\u00b7sel\u00b7bi\u00b7ge", ",", "der", "mir", "wo\u00b7von", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PDS", "$,", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Durch eines andern Mund, ertheilt, und innerlich,", "tokens": ["Durch", "ei\u00b7nes", "an\u00b7dern", "Mund", ",", "er\u00b7theilt", ",", "und", "in\u00b7ner\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$,", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So da\u00df es niemand sonst, als ich, verstehe, saget,", "tokens": ["So", "da\u00df", "es", "nie\u00b7mand", "sonst", ",", "als", "ich", ",", "ver\u00b7ste\u00b7he", ",", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PIS", "ADV", "$,", "KOUS", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das, wornach ich kein eintzigmal gefraget?", "tokens": ["Das", ",", "wor\u00b7nach", "ich", "kein", "eint\u00b7zig\u00b7mal", "ge\u00b7fra\u00b7get", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da wir nun das, was in uns selbst geschicht,", "tokens": ["Da", "wir", "nun", "das", ",", "was", "in", "uns", "selbst", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDS", "$,", "PRELS", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nicht kennen und nicht fassen;", "tokens": ["Nicht", "ken\u00b7nen", "und", "nicht", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wie wird sich denn von uns doch die verborgne Spur,", "tokens": ["Wie", "wird", "sich", "denn", "von", "uns", "doch", "die", "ver\u00b7borg\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "ADV", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der m\u00e4chtigen Natur,", "tokens": ["Der", "m\u00e4ch\u00b7ti\u00b7gen", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ergr\u00fcblen und begreiffen lassen?", "tokens": ["Er\u00b7gr\u00fcb\u00b7len", "und", "be\u00b7greif\u00b7fen", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}