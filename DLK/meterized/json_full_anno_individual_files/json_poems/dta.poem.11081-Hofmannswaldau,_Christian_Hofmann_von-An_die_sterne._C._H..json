{"dta.poem.11081": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die sterne.  \n  C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sterne m\u00f6gt euch in die decken", "tokens": ["Ihr", "ster\u00b7ne", "m\u00f6gt", "euch", "in", "die", "de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nacht und des gew\u00f6lcks verstecken.", "tokens": ["Der", "nacht", "und", "des", "ge\u00b7w\u00f6l\u00b7cks", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich sag\u2019 es euch, h\u00fcllt ja bey zeiten euren schein", "tokens": ["Ich", "sag'", "es", "euch", ",", "h\u00fcllt", "ja", "bey", "zei\u00b7ten", "eu\u00b7ren", "schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "ADV", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In dunckle schatten ein;", "tokens": ["In", "dunck\u00b7le", "schat\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn Silvia wird itzund durch die gassen", "tokens": ["Denn", "Sil\u00b7via", "wird", "it\u00b7zund", "durch", "die", "gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Von mir sich f\u00fchren lassen.", "tokens": ["Von", "mir", "sich", "f\u00fch\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein bitten und die nacht", "tokens": ["Mein", "bit\u00b7ten", "und", "die", "nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hat sie zu dieser lust gebracht.", "tokens": ["Hat", "sie", "zu", "die\u00b7ser", "lust", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nun wi\u00dft ihr ja, da\u00df ihr nicht k\u00f6nnt bestehen,", "tokens": ["Nun", "wi\u00dft", "ihr", "ja", ",", "da\u00df", "ihr", "nicht", "k\u00f6nnt", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So bald die sonn\u2019 ist willens aufzugehen.", "tokens": ["So", "bald", "die", "sonn'", "ist", "wil\u00b7lens", "auf\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VAFIN", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}