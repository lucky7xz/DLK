{"dta.poem.15060": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Abentheuer, das sich mit der Aeneis  \n Hrn. Joh. Christ. Schwartzen in Conrector  \n Erlebachs Schule zugetragen hat.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mach einen Sturm und st\u00fcrtz ihr Schiff ins Meer hinein,", "tokens": ["Mach", "ei\u00b7nen", "Sturm", "und", "st\u00fcrtz", "ihr", "Schiff", "ins", "Meer", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJD", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df sie ein Gaukelspiel der tollen Wellen seyn.", "tokens": ["La\u00df", "sie", "ein", "Gau\u00b7kel\u00b7spiel", "der", "tol\u00b7len", "Wel\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil mir vierzehn sch\u00f6n gewachsne Nymphen dienen,", "tokens": ["Und", "weil", "mir", "vier\u00b7zehn", "sch\u00f6n", "ge\u00b7wachs\u00b7ne", "Nym\u00b7phen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "CARD", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So soll f\u00fcr diesen Dienst die pr\u00e4chtigste von ihnen,", "tokens": ["So", "soll", "f\u00fcr", "die\u00b7sen", "Dienst", "die", "pr\u00e4ch\u00b7tigs\u00b7te", "von", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PDAT", "NN", "ART", "ADJA", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Deiopeia, dir auf ewig eigen seyn;", "tokens": ["Dei\u00b7o\u00b7pei\u00b7a", ",", "dir", "auf", "e\u00b7wig", "ei\u00b7gen", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Und diese m\u00fcsse dich mit manchem Sohn erfreun:", "tokens": ["Und", "die\u00b7se", "m\u00fcs\u00b7se", "dich", "mit", "man\u00b7chem", "Sohn", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn sie soll sich mit dir als Ehefrau verm\u00e4hlen.", "tokens": ["Denn", "sie", "soll", "sich", "mit", "dir", "als", "E\u00b7he\u00b7frau", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "APPR", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}}}}