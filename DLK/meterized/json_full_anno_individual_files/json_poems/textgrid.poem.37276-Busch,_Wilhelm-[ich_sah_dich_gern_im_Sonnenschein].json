{"textgrid.poem.37276": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich sah dich gern im Sonnenschein]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich gern im Sonnenschein,", "tokens": ["Ich", "sah", "dich", "gern", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn laut die V\u00f6glein sangen,", "tokens": ["Wenn", "laut", "die", "V\u00f6\u00b7glein", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn durch die Wangen und Lippen dein", "tokens": ["Wenn", "durch", "die", "Wan\u00b7gen", "und", "Lip\u00b7pen", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rosig die Strahlen drangen.", "tokens": ["Ro\u00b7sig", "die", "Strah\u00b7len", "dran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ich sah dich auch gern im Mondenlicht", "tokens": ["Ich", "sah", "dich", "auch", "gern", "im", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beim Dufte der Jasminen,", "tokens": ["Beim", "Duf\u00b7te", "der", "Jas\u00b7mi\u00