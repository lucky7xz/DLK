{"dta.poem.12271": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "David .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war der Kleinste meiner Br\u00fcder,               ", "tokens": ["Ich", "war", "der", "Kleins\u00b7te", "mei\u00b7ner", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meines Vaters j\u00fcngster Sohn;", "tokens": ["Und", "mei\u00b7nes", "Va\u00b7ters", "j\u00fcngs\u00b7ter", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich stellte k\u00fchn mich dem zuwider,", "tokens": ["Ich", "stell\u00b7te", "k\u00fchn", "mich", "dem", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem ein Sch\u00e4flein l\u00e4uft davon:", "tokens": ["Vor", "dem", "ein", "Sch\u00e4f\u00b7lein", "l\u00e4uft", "da\u00b7von", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00dfte meinem Vater seyn", "tokens": ["Ich", "mu\u00df\u00b7te", "mei\u00b7nem", "Va\u00b7ter", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein H\u00fcter seiner L\u00e4mmerlein.", "tokens": ["Ein", "H\u00fc\u00b7ter", "sei\u00b7ner", "L\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hierbey hab ich mir eingerichtet", "tokens": ["Hier\u00b7bey", "hab", "ich", "mir", "ein\u00b7ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Harfenspiel mit meiner Hand,", "tokens": ["Ein", "Har\u00b7fen\u00b7spiel", "mit", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meinem Gott ein Buch gedichtet;", "tokens": ["Und", "mei\u00b7nem", "Gott", "ein", "Buch", "ge\u00b7dich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer aber macht es ihm bekannt?", "tokens": ["Wer", "a\u00b7ber", "macht", "es", "ihm", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer saget meinem Herrn es an,", "tokens": ["Wer", "sa\u00b7get", "mei\u00b7nem", "Herrn", "es", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich die Psalter harfen kann?", "tokens": ["Da\u00df", "ich", "die", "Psal\u00b7ter", "har\u00b7fen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du selber, Herr! hast mich geh\u00f6ret,", "tokens": ["Du", "sel\u00b7ber", ",", "Herr", "!", "hast", "mich", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$.", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was meiner Saiten Spiel vermag,", "tokens": ["Was", "mei\u00b7ner", "Sai\u00b7ten", "Spiel", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was mich deine Furcht gelehret,", "tokens": ["Und", "was", "mich", "dei\u00b7ne", "Furcht", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich bey deinen Schafen lag:", "tokens": ["Da", "ich", "bey", "dei\u00b7nen", "Scha\u00b7fen", "lag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um dieses hast du mich gebracht,", "tokens": ["Um", "die\u00b7ses", "hast", "du", "mich", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich zum K\u00f6nig dann gemacht.", "tokens": ["Und", "mich", "zum", "K\u00f6\u00b7nig", "dann", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ob ich von meinen Br\u00fcdern allen", "tokens": ["Ob", "ich", "von", "mei\u00b7nen", "Br\u00fc\u00b7dern", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kleinste gleich gewesen bin,", "tokens": ["Der", "Kleins\u00b7te", "gleich", "ge\u00b7we\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So hat doch keiner dir gefallen,", "tokens": ["So", "hat", "doch", "kei\u00b7ner", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich nur, David war dir fein,", "tokens": ["Als", "ich", "nur", ",", "Da\u00b7vid", "war", "dir", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "NE", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mu\u00dfte von den Schafen gehen,", "tokens": ["Ich", "mu\u00df\u00b7te", "von", "den", "Scha\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und unter einer Krone stehen.", "tokens": ["Und", "un\u00b7ter", "ei\u00b7ner", "Kro\u00b7ne", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich der Geringste mu\u00dft es wagen", "tokens": ["Ich", "der", "Ge\u00b7rings\u00b7te", "mu\u00dft", "es", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem geharnischten Goliath,", "tokens": ["Mit", "dem", "ge\u00b7har\u00b7nischten", "Go\u00b7li\u00b7ath", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihm das b\u00f6se Haupt abschlagen,", "tokens": ["Und", "ihm", "das", "b\u00f6\u00b7se", "Haupt", "ab\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das dich und mich geh\u00f6hnet hat:", "tokens": ["Das", "dich", "und", "mich", "ge\u00b7h\u00f6h\u00b7net", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KON", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er schwur bey seinem G\u00f6tzen mir", "tokens": ["Er", "schwur", "bey", "sei\u00b7nem", "G\u00f6t\u00b7zen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Tod, und selbst starb er daf\u00fcr.", "tokens": ["Den", "Tod", ",", "und", "selbst", "starb", "er", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Sein Schwerdt hab ich ihm ausgezogen,", "tokens": ["Sein", "Schwerdt", "hab", "ich", "ihm", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihm vom Leib den Hals entzweyt,", "tokens": ["Und", "ihm", "vom", "Leib", "den", "Hals", "ent\u00b7zweyt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ihm der Geist ist ausgeflogen,", "tokens": ["Da\u00df", "ihm", "der", "Geist", "ist", "aus\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ungest\u00fcmmer Grausamkeit:", "tokens": ["Mit", "un\u00b7ge\u00b7st\u00fcm\u00b7mer", "Grau\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hiemit hat meine Siegeshand", "tokens": ["Hie\u00b7mit", "hat", "mei\u00b7ne", "Sie\u00b7ges\u00b7hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schmach von Israel gewandt.", "tokens": ["Die", "Schmach", "von", "Is\u00b7rael", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}