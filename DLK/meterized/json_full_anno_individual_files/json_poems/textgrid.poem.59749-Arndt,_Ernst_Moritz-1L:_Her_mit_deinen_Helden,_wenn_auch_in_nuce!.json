{"textgrid.poem.59749": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Her mit deinen Helden, wenn auch in nuce!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Her mit deinen Helden, wenn auch in nuce!", "tokens": ["Her", "mit", "dei\u00b7nen", "Hel\u00b7den", ",", "wenn", "auch", "in", "nu\u00b7ce", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Heut sollst du mir deine Sehrm\u00e4nner nennen,", "tokens": ["Heut", "sollst", "du", "mir", "dei\u00b7ne", "Sehr\u00b7m\u00e4n\u00b7ner", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die vor dir ", "tokens": ["Die", "vor", "dir"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Im Ruhmessonnenschein leuchten und brennen.", "tokens": ["Im", "Ruh\u00b7mes\u00b7son\u00b7nen\u00b7schein", "leuch\u00b7ten", "und", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Frostwetter ist es, da\u00df Gott erbarm'!", "tokens": ["Frost\u00b7wet\u00b7ter", "ist", "es", ",", "da\u00df", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wir sind an Taten und Ehren arm.", "tokens": ["Wir", "sind", "an", "Ta\u00b7ten", "und", "Eh\u00b7ren", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Verpufft mich billig, doch wie dem sei,", "tokens": ["Ver\u00b7pufft", "mich", "bil\u00b7lig", ",", "doch", "wie", "dem", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADV", "KOKOM", "PDS", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Deutschen bleibt der Beste der Gr\u00f6\u00dfte,", "tokens": ["Dem", "Deut\u00b7schen", "bleibt", "der", "Bes\u00b7te", "der", "Gr\u00f6\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Treueste Beste \u2013 das bleibt dabei.", "tokens": ["Der", "Treu\u00b7es\u00b7te", "Bes\u00b7te", "\u2013", "das", "bleibt", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "VVFIN", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Solchen Ehrenspruch begreint mir kein Hohn:", "tokens": ["Sol\u00b7chen", "Eh\u00b7ren\u00b7spruch", "be\u00b7greint", "mir", "kein", "Hohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Der Beste war ", "tokens": ["Der", "Bes\u00b7te", "war"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wort, das von g\u00f6ttlichen Flammen spr\u00fcht!", "tokens": ["Wort", ",", "das", "von", "g\u00f6tt\u00b7li\u00b7chen", "Flam\u00b7men", "spr\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Vernimm, nie hat's unter menschlichem Sch\u00e4del,", "tokens": ["Ver\u00b7nimm", ",", "nie", "hat's", "un\u00b7ter", "menschli\u00b7chem", "Sch\u00e4\u00b7del", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In menschlichem Herzen nie stolzer gegl\u00fcht,", "tokens": ["In", "menschli\u00b7chem", "Her\u00b7zen", "nie", "stol\u00b7zer", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gegl\u00fchet, gebl\u00fchet auf deutscher Au", "tokens": ["Ge\u00b7gl\u00fc\u00b7het", ",", "ge\u00b7bl\u00fc\u00b7het", "auf", "deut\u00b7scher", "Au"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Als im Ritterglanze, im ", "tokens": ["Als", "im", "Rit\u00b7ter\u00b7glan\u00b7ze", ",", "im"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KOUS", "APPRART", "NN", "$,", "APPRART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Licht suchend unter so strahlenden Lichtern.", "tokens": ["Licht", "su\u00b7chend", "un\u00b7ter", "so", "strah\u00b7len\u00b7den", "Lich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Du meinst der Schlachten Treffer und Finder,", "tokens": ["Du", "meinst", "der", "Schlach\u00b7ten", "Tref\u00b7fer", "und", "Fin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das hellste Aug' von den hellen Gesichtern.", "tokens": ["Das", "hells\u00b7te", "Aug'", "von", "den", "hel\u00b7len", "Ge\u00b7sich\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da schaute vor vielen mit Adlerblick", "tokens": ["Da", "schau\u00b7te", "vor", "vie\u00b7len", "mit", "Ad\u00b7ler\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Wie sind da die Tausende betend gezogen!", "tokens": ["Wie", "sind", "da", "die", "Tau\u00b7sen\u00b7de", "be\u00b7tend", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie sind da die Fahnen und Herzen zum Himmel", "tokens": ["Wie", "sind", "da", "die", "Fah\u00b7nen", "und", "Her\u00b7zen", "zum", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In Gottes Hoffnung und Wonne geflogen!", "tokens": ["In", "Got\u00b7tes", "Hoff\u00b7nung", "und", "Won\u00b7ne", "ge\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der L\u00f6we ", "tokens": ["Der", "L\u00f6\u00b7we"], "token_info": ["word", "word"], "pos": ["ART", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wer solchem gleich fechten und beten kann!", "tokens": ["Wer", "sol\u00b7chem", "gleich", "fech\u00b7ten", "und", "be\u00b7ten", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Eine Frage fast hoch \u00fcber meinem Erreich.", "tokens": ["Ei\u00b7ne", "Fra\u00b7ge", "fast", "hoch", "\u00fc\u00b7ber", "mei\u00b7nem", "Er\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ich meine, du meinest den tapfersten Willen:", "tokens": ["Ich", "mei\u00b7ne", ",", "du", "mei\u00b7nest", "den", "tap\u00b7fers\u00b7ten", "Wil\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Solcher Stillen ist Erdreich und Himmelreich \u2013", "tokens": ["Sol\u00b7cher", "Stil\u00b7len", "ist", "Er\u00b7dreich", "und", "Him\u00b7mel\u00b7reich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "KON", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So merke die W\u00f6rter ", "tokens": ["So", "mer\u00b7ke", "die", "W\u00f6r\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Darin sitzt ", "tokens": ["Da\u00b7rin", "sitzt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "In Deutschland zu fragen nach mutigstem Mut.", "tokens": ["In", "Deutschland", "zu", "fra\u00b7gen", "nach", "mu\u00b7tigs\u00b7tem", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mut war ja von aller\u00e4ltesten Tagen", "tokens": ["Mut", "war", "ja", "von", "al\u00b7le\u00b7r\u00e4l\u00b7tes\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ein eigenstes, allerdeutschestes Gut \u2013", "tokens": ["Ein", "ei\u00b7gens\u00b7tes", ",", "al\u00b7ler\u00b7deut\u00b7sches\u00b7tes", "Gut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch der nimmer und vor nichts sich gef\u00fcrchtet, voran", "tokens": ["Doch", "der", "nim\u00b7mer", "und", "vor", "nichts", "sich", "ge\u00b7f\u00fcrch\u00b7tet", ",", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "ADV", "KON", "APPR", "PIS", "PRF", "VVPP", "$,", "PTKVZ"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Stehe hier ", "tokens": ["Ste\u00b7he", "hier"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Der herrlich schlie\u00dfet den Heldenreihn,", "tokens": ["Der", "herr\u00b7lich", "schlie\u00b7\u00dfet", "den", "Hel\u00b7den\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Gewaltigste war in des Vaterlands Marken,", "tokens": ["Der", "Ge\u00b7wal\u00b7tigs\u00b7te", "war", "in", "des", "Va\u00b7ter\u00b7lands", "Mar\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Der St\u00e4rkste der unzerbrechliche ", "tokens": ["Der", "St\u00e4rks\u00b7te", "der", "un\u00b7zer\u00b7brech\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Solange klinget von deutschen Lippen Gesang,", "tokens": ["So\u00b7lan\u00b7ge", "klin\u00b7get", "von", "deut\u00b7schen", "Lip\u00b7pen", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wird klingen des m\u00e4chtigen Namens Klang.", "tokens": ["Wird", "klin\u00b7gen", "des", "m\u00e4ch\u00b7ti\u00b7gen", "Na\u00b7mens", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "ADJA", "NN", "NN