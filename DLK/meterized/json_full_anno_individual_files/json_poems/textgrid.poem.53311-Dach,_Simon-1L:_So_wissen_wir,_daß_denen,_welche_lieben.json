{"textgrid.poem.53311": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wissen wir, da\u00df denen, welche lieben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wissen wir, da\u00df denen, welche lieben", "tokens": ["So", "wis\u00b7sen", "wir", ",", "da\u00df", "de\u00b7nen", ",", "wel\u00b7che", "lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PDS", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich, lieber Gott, jhr Vnmuth vnd verdru\u00df,", "tokens": ["Dich", ",", "lie\u00b7ber", "Gott", ",", "jhr", "Vn\u00b7muth", "vnd", "ver\u00b7dru\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vnd was sie nur in Furcht vnd Glauben \u00fcben,", "tokens": ["Vnd", "was", "sie", "nur", "in", "Furcht", "vnd", "Glau\u00b7ben", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voraus jhr Todt zum besten dienen mu\u00df.", "tokens": ["Vo\u00b7raus", "jhr", "Todt", "zum", "bes\u00b7ten", "die\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wol, allen wol die einig dich erkiesen", "tokens": ["Wol", ",", "al\u00b7len", "wol", "die", "ei\u00b7nig", "dich", "er\u00b7kie\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PIAT", "ADV", "ART", "ADJD", "PPER", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "F\u00fcr jhren Freundt, du heltst best\u00e4ndig Fu\u00df.", "tokens": ["F\u00fcr", "jhren", "Freundt", ",", "du", "heltst", "be\u00b7st\u00e4n\u00b7dig", "Fu\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn sich das Gl\u00fcck nun zornig hat erwiesen,", "tokens": ["Wenn", "sich", "das", "Gl\u00fcck", "nun", "zor\u00b7nig", "hat", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd an vns setzt mit st\u00fcrmender Gewalt,", "tokens": ["Vnd", "an", "vns", "setzt", "mit", "st\u00fcr\u00b7men\u00b7der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einer Fluth vnd einem starken Riesen,", "tokens": ["Gleich", "ei\u00b7ner", "Fluth", "vnd", "ei\u00b7nem", "star\u00b7ken", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir arme sind zu beyden F\u00fcssen kalt", "tokens": ["Wir", "ar\u00b7me", "sind", "zu", "bey\u00b7den", "F\u00fcs\u00b7sen", "kalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "VAFIN", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vnd wissen nicht wohin wir vns fast lencken,", "tokens": ["Vnd", "wis\u00b7sen", "nicht", "wo\u00b7hin", "wir", "vns", "fast", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWAV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So leistest du vns Schutz vnd Auffenthalt.", "tokens": ["So", "leis\u00b7test", "du", "vns", "Schutz", "vnd", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das liebe Creutz wird keinem doch was schencken,", "tokens": ["Das", "lie\u00b7be", "Creutz", "wird", "kei\u00b7nem", "doch", "was", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIS", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer jrgends nur ein guter Christ wil seyn,", "tokens": ["Wer", "jr\u00b7gends", "nur", "ein", "gu\u00b7ter", "Christ", "wil", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es stellt vns nach, vnd wil sich an vns hencken,", "tokens": ["Es", "stellt", "vns", "nach", ",", "vnd", "wil", "sich", "an", "vns", "hen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd setzet offt den frommen auch ein Bein,", "tokens": ["Vnd", "set\u00b7zet", "offt", "den", "from\u00b7men", "auch", "ein", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wahre Rhue wird doch nicht vmb mich schweben,", "tokens": ["Die", "wah\u00b7re", "Rhue", "wird", "doch", "nicht", "vmb", "mich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schenckt mir das Gl\u00fcck gleich vberfl\u00fcssig ein.", "tokens": ["Schenckt", "mir", "das", "Gl\u00fcck", "gleich", "vberf\u00b7l\u00fcs\u00b7sig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+--++-+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wenn nichts gebricht, wenn Wolstandt, Ehr vnd Leben", "tokens": ["Wenn", "nichts", "ge\u00b7bricht", ",", "wenn", "Wol\u00b7standt", ",", "Ehr", "vnd", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "KOUS", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd starker Leib bey mir in bl\u00fchte sindt,", "tokens": ["Vnd", "star\u00b7ker", "Leib", "bey", "mir", "in", "bl\u00fch\u00b7te", "sindt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wird mir doch die Erb-Schuldt wiederstreben", "tokens": ["So", "wird", "mir", "doch", "die", "Er\u00b7bSchuldt", "wie\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vnd meiner Lust verkehren allen Windt.", "tokens": ["Vnd", "mei\u00b7ner", "Lust", "ver\u00b7keh\u00b7ren", "al\u00b7len", "Windt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der altte Mensch mus immer mich bekriegen,", "tokens": ["Der", "alt\u00b7te", "Mensch", "mus", "im\u00b7mer", "mich", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der manchen Kampff mir offtmals abgewinnt.", "tokens": ["Der", "man\u00b7chen", "Kampff", "mir", "offt\u00b7mals", "ab\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auch Sathanas pflegt nicht gern still zu liegen,", "tokens": ["Auch", "Sa\u00b7tha\u00b7nas", "pflegt", "nicht", "gern", "still", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er br\u00fcllt vmb mich gleich einem L\u00f6wen her,", "tokens": ["Er", "br\u00fcllt", "vmb", "mich", "gleich", "ei\u00b7nem", "L\u00f6\u00b7wen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr seinem Mord vnd grossen kr\u00e4fften siegen", "tokens": ["F\u00fcr", "sei\u00b7nem", "Mord", "vnd", "gros\u00b7sen", "kr\u00e4ff\u00b7ten", "sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Fleisch vnd Blut zu sorglich vnd zu schwer,", "tokens": ["Ist", "Fleisch", "vnd", "Blut", "zu", "sorg\u00b7lich", "vnd", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Helle sperrt-nach-mir-auff jhren Rachen,", "tokens": ["Die", "Hel\u00b7le", "sperr\u00b7tnach\u00b7mir\u00b7auff", "jhren", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mir drewet auch des Todes scharffes Speer.", "tokens": ["Mir", "dre\u00b7wet", "auch", "des", "To\u00b7des", "scharf\u00b7fes", "Speer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was soll ich nun vor solchen Feinden machen,", "tokens": ["Was", "soll", "ich", "nun", "vor", "sol\u00b7chen", "Fein\u00b7den", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann Gott nicht ist mein Fel\u00df vnd Zuversicht?", "tokens": ["Wann", "Gott", "nicht", "ist", "mein", "Fel\u00df", "vnd", "Zu\u00b7ver\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKNEG", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist er mit mir, so will ich aller lachen,", "tokens": ["Ist", "er", "mit", "mir", ",", "so", "will", "ich", "al\u00b7ler", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VMFIN", "PPER", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil seine Handt leicht jhre Macht zerbricht,", "tokens": ["Weil", "sei\u00b7ne", "Handt", "leicht", "jhre", "Macht", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Je mehr sie sich viel wesens vnterwinden,", "tokens": ["Je", "mehr", "sie", "sich", "viel", "we\u00b7sens", "vn\u00b7ter\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Je mehr ist Er mein Hoffnung vnd mein Liecht.", "tokens": ["Je", "mehr", "ist", "Er", "mein", "Hoff\u00b7nung", "vnd", "mein", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich weis, da\u00df nichts verdammlichs sey zu finden", "tokens": ["Ich", "weis", ",", "da\u00df", "nichts", "ver\u00b7damm\u00b7lichs", "sey", "zu", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PIS", "ADJA", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An denen, so in Jesu Christo sindt.", "tokens": ["An", "de\u00b7nen", ",", "so", "in", "Je\u00b7su", "Chris\u00b7to", "sindt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "ADV", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Glaube kan mich jhm so fest verbinden,", "tokens": ["Der", "Glau\u00b7be", "kan", "mich", "jhm", "so", "fest", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das seine Trew nie einen Ri\u00df gewinnt,", "tokens": ["Das", "sei\u00b7ne", "Trew", "nie", "ei\u00b7nen", "Ri\u00df", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er lesst mich nicht, was je mich zu vmbgeben", "tokens": ["Er", "lesst", "mich", "nicht", ",", "was", "je", "mich", "zu", "vmb\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Hellen Reich durch wilden Zorn beginnt.", "tokens": ["Der", "Hel\u00b7len", "Reich", "durch", "wil\u00b7den", "Zorn", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das ist gewis, da\u00df weder Todt noch Leben,", "tokens": ["Das", "ist", "ge\u00b7wis", ",", "da\u00df", "we\u00b7der", "Todt", "noch", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "KOUS", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht F\u00fcrstenthum nicht Engel noch Gewalt,", "tokens": ["Nicht", "F\u00fcrs\u00b7ten\u00b7thum", "nicht", "En\u00b7gel", "noch", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PTKNEG", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts tieffes noch was pfleget hoch zu schweben,", "tokens": ["Nichts", "tief\u00b7fes", "noch", "was", "pfle\u00b7get", "hoch", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "KON", "PWS", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch etwas sonst von Sachen mannigfalt", "tokens": ["Noch", "et\u00b7was", "sonst", "von", "Sa\u00b7chen", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.