, "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Auch Sathanas pflegt nicht gern still zu liegen,", "tokens": ["Auch", "Sa\u00b7tha\u00b7nas", "pflegt", "nicht", "gern", "still", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er br\u00fcllt vmb mich gleich einem L\u00f6wen her,", "tokens": ["Er", "br\u00fcllt", "vmb", "mich", "gleich", "ei\u00b7nem", "L\u00f6\u00b7wen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr seinem Mord vnd grossen kr\u00e4fften siegen", "tokens": ["F\u00fcr", "sei\u00b7nem", "Mord", "vnd", "gros\u00b7sen", "kr\u00e4ff\u00b7ten", "sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist Fleisch vnd Blut zu sorglich vnd zu schwer,", "tokens": ["Ist", "Fleisch", "vnd", "Blut", "zu", "sorg\u00b7lich", "vnd", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Helle sperrt-nach-mir-auff jhren Rachen,", "tokens": ["Die", "Hel\u00b7le", "sperr\u00b7tnach\u00b7mir\u00b7auff", "jhren", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mir drewet auch des Tod