, "sich", "viel", "we\u00b7sens", "vn\u00b7ter\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Je mehr ist Er mein Hoffnung vnd mein Liecht.", "tokens": ["Je", "mehr", "ist", "Er", "mein", "Hoff\u00b7nung", "vnd", "mein", "Liecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ich weis, da\u00df nichts verdammlichs sey zu finden", "tokens": ["Ich", "weis", ",", "da\u00df", "nichts", "ver\u00b7damm\u00b7lichs", "sey", "zu", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "PIS", "ADJA", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An denen, so in Jesu Christo sindt.", "tokens": ["An", "de\u00b7nen", ",", "so", "in", "Je\u00b7su", "Chris\u00b7to", "sindt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "ADV", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Glaube kan mich jhm so fest verbinden,", "tokens": ["Der", "Glau\u00b7be", "kan", "mich", "jhm", "so", "fest", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das seine Trew nie einen Ri\u00df gewinnt,", "tokens": ["Das", "sei\u00b7ne", "Trew", "nie", "ei\u00b7nen", "Ri\u00df", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er lesst mich nicht, was je mich zu vmbgeben", "tokens": ["Er", "lesst", "mich", "nicht", ",", "was", "je", "mich", "zu", "vmb\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Hellen Reich durch wilden Zorn beginnt.", "tokens": ["Der", "Hel\u00b7len", "Reich", "durch", "wil\u00b7den", "Zorn", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Das ist gewis, da\u00df weder Todt noch Leben,", "tokens": ["Das", "ist", "ge\u00b7wis", ",", "da\u00df", "we\u00b7der", "Todt", "noch", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "KOUS", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht F\u00fcrstenthum nicht Engel noch Gewalt,", "tokens": ["Nicht", "F\u00fcrs\u00b7ten\u00b7thum", "nicht", "En\u00b7gel", "noch", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PTKNEG", "NE", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts tieffes noch was pfleget hoch zu schweben,", "tokens": ["Nichts", "tief\u00b7fes", "noch", "was", "pfle\u00b7get", "hoch", "zu", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "KON", "PWS", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch etwas sonst von Sachen mannigfalt", "tokens": ["Noch", "et\u00b7was", "sonst", "von", "Sa\u00b7chen", "man\u00b7nig\u00b7falt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Aus Christi Huld vnd Liebe mich mag heben,", "tokens": ["Aus", "Chris\u00b7ti", "Huld", "vnd", "Lie\u00b7be", "mich", "mag", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "KON", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ist vnd bleibt mein s\u00fcsser Auffenthalt.", "tokens": ["Er", "ist", "vnd", "bleibt", "mein", "s\u00fcs\u00b7ser", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}