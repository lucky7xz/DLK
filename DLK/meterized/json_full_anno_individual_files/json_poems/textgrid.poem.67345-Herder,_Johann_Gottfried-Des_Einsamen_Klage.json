t der Hoffnung Morgenroth;", "tokens": ["Hell", "macht", "der", "Hoff\u00b7nung", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Deine Fackel, holder Tod,", "tokens": ["Nur", "Dei\u00b7ne", "Fa\u00b7ckel", ",", "hol\u00b7der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir strahlt mit mildem Schein.", "tokens": ["Mir", "strahlt", "mit", "mil\u00b7dem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wo weilest Du?", "tokens": ["Wo", "wei\u00b7lest", "Du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bring mich zur Ruh!", "tokens": ["Bring", "mich", "zur", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Komm, f\u00fchr mich in Dein stilles Land", "tokens": ["Komm", ",", "f\u00fchr", "mich", "in", "Dein", "stil\u00b7les", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlie\u00dfe mir mit sanfter Hand", "tokens": ["Und", "schlie\u00b7\u00dfe", "mir", "mit", "sanf\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die tr\u00fcben Augen zu!", "tokens": ["Die", "tr\u00fc\u00b7ben", "Au\u00b7gen", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}