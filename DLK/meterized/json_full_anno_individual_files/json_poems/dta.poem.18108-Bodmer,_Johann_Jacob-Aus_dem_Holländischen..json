{"dta.poem.18108": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Holl\u00e4ndischen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieweil man mu\u00df, zu kriegen himmlisch Gut,", "tokens": ["Die\u00b7weil", "man", "mu\u00df", ",", "zu", "krie\u00b7gen", "himm\u00b7lisch", "Gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "$,", "PTKZU", "VVINF", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die S\u00fcnde beichten und beklagen,", "tokens": ["Die", "S\u00fcn\u00b7de", "beich\u00b7ten", "und", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und zu entfliehn der H\u00f6llen heisse Glut", "tokens": ["Und", "zu", "ent\u00b7fliehn", "der", "H\u00f6l\u00b7len", "heis\u00b7se", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In seinem Hertzen Reue tragen,", "tokens": ["In", "sei\u00b7nem", "Hert\u00b7zen", "Reu\u00b7e", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So bitt ich doch, o meines Lebens Schein,", "tokens": ["So", "bitt", "ich", "doch", ",", "o", "mei\u00b7nes", "Le\u00b7bens", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "FM", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gebt mir mein Hertz, das ich verlie\u00df bey euch,", "tokens": ["Gebt", "mir", "mein", "Hertz", ",", "das", "ich", "ver\u00b7lie\u00df", "bey", "euch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wollt aber ihrs behalten ja allein,", "tokens": ["Wollt", "a\u00b7ber", "ihrs", "be\u00b7hal\u00b7ten", "ja", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So beichtet mein und eure S\u00fcnd zugleich.", "tokens": ["So", "beich\u00b7tet", "mein", "und", "eu\u00b7re", "S\u00fcnd", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}