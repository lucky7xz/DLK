{"dta.poem.23707": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Rangstreit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wilst du? sprach mein M&#228;dchen.", "tokens": ["Was", "wilst", "du", "?", "sprach", "mein", "M", "&#228;", "dchen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "VVFIN", "PPOSAT", "NE", "$(", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich sprach: Ich will dich k\u00fcssen.", "tokens": ["Ich", "sprach", ":", "Ich", "will", "dich", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da that das M\u00e4dchen b\u00f6se,", "tokens": ["Da", "that", "das", "M\u00e4d\u00b7chen", "b\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: La\u00df mich erst k\u00fcssen!", "tokens": ["Und", "sprach", ":", "La\u00df", "mich", "erst", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da zankten wir uns beide.", "tokens": ["Da", "zank\u00b7ten", "wir", "uns", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da nannt es mich: Du Falscher!", "tokens": ["Da", "nannt", "es", "mich", ":", "Du", "Fal\u00b7scher", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sprach: Ich liebe st\u00e4rker,", "tokens": ["Und", "sprach", ":", "Ich", "lie\u00b7be", "st\u00e4r\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drum mu\u00df ich dich erst k\u00fcssen.", "tokens": ["Drum", "mu\u00df", "ich", "dich", "erst", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da h\u00f6rt ich auf zu zanken.", "tokens": ["Da", "h\u00f6rt", "ich", "auf", "zu", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da k\u00fc\u00dfte mich mein M\u00e4dchen.", "tokens": ["Da", "k\u00fc\u00df\u00b7te", "mich", "mein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da merkt ich, als es k\u00fc\u00dfte,", "tokens": ["Da", "merkt", "ich", ",", "als", "es", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Es sei kein Rang im K\u00fcssen;", "tokens": ["Es", "sei", "kein", "Rang", "im", "K\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Denn, wenn sich zweene k\u00fcssen,", "tokens": ["Denn", ",", "wenn", "sich", "zwee\u00b7ne", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ist ieglicher der Erste.", "tokens": ["Ist", "ieg\u00b7li\u00b7cher", "der", "Ers\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}