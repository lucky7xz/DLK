{"textgrid.poem.63711": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckkehr zur Natur", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als h\u00e4tt' uns lang ein Zwist geschieden,", "tokens": ["Als", "h\u00e4tt'", "uns", "lang", "ein", "Zwist", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nun geschlichtet wunderbar,", "tokens": ["Der", "nun", "ge\u00b7schlich\u00b7tet", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trat ich ein in deinen Frieden", "tokens": ["So", "trat", "ich", "ein", "in", "dei\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ward im Tiefsten still und klar.", "tokens": ["Und", "ward", "im", "Tiefs\u00b7ten", "still", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah das Meer sich leuchtend dehnen,", "tokens": ["Ich", "sah", "das", "Meer", "sich", "leuch\u00b7tend", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Fr\u00fchlingswonnen stand die Flur,", "tokens": ["In", "Fr\u00fch\u00b7lings\u00b7won\u00b7nen", "stand", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da warf ich wieder mich in Tr\u00e4nen", "tokens": ["Da", "warf", "ich", "wie\u00b7der", "mich", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An deine Mutterbrust, Natur.", "tokens": ["An", "dei\u00b7ne", "Mut\u00b7ter\u00b7brust", ",", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich kannte dich, und doch im stillen", "tokens": ["Ich", "kann\u00b7te", "dich", ",", "und", "doch", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trotzt' ich der Liebe, die mich zwang,", "tokens": ["Trotzt'", "ich", "der", "Lie\u00b7be", ",", "die", "mich", "zwang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die um den spr\u00f6den Eigenwillen", "tokens": ["Die", "um", "den", "spr\u00f6\u00b7den", "Ei\u00b7gen\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So zarte Fesseln freundlich schlang.", "tokens": ["So", "zar\u00b7te", "Fes\u00b7seln", "freund\u00b7lich", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Geiste sucht' ich mein Gen\u00fcgen,", "tokens": ["Am", "Geis\u00b7te", "sucht'", "ich", "mein", "Ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zahme Schw\u00e4che schien mir's nur,", "tokens": ["Und", "zah\u00b7me", "Schw\u00e4\u00b7che", "schien", "mir's", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich unter deine Zucht zu f\u00fcgen", "tokens": ["Mich", "un\u00b7ter", "dei\u00b7ne", "Zucht", "zu", "f\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und still zu wandeln deine Spur.", "tokens": ["Und", "still", "zu", "wan\u00b7deln", "dei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du schwiegst, und fort und fort in Treuen", "tokens": ["Du", "schwiegst", ",", "und", "fort", "und", "fort", "in", "Treu\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PTKVZ", "KON", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geselltest du dich nah zu mir,", "tokens": ["Ge\u00b7sell\u00b7test", "du", "dich", "nah", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den nicht'gen Unmut zu zerstreuen,", "tokens": ["Den", "nicht'\u00b7gen", "Un\u00b7mut", "zu", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und riefst so sanft: Ich bin bei dir!", "tokens": ["Und", "riefst", "so", "sanft", ":", "Ich", "bin", "bei", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sahst mich an aus Himmelsreine,", "tokens": ["Du", "sahst", "mich", "an", "aus", "Him\u00b7mels\u00b7rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus Wald und Blumen m\u00fctterlich \u2013", "tokens": ["Aus", "Wald", "und", "Blu\u00b7men", "m\u00fct\u00b7ter\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Umsonst! Nicht war ich mehr der Deine,", "tokens": ["Um\u00b7sonst", "!", "Nicht", "war", "ich", "mehr", "der", "Dei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKNEG", "VAFIN", "PPER", "ADV", "ART", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und so verscherzt' ich dich und mich.", "tokens": ["Und", "so", "ver\u00b7scherzt'", "ich", "dich", "und", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Empfinden sollt' ich's. Wie die Schw\u00fcle", "tokens": ["Emp\u00b7fin\u00b7den", "sollt'", "ich'", "s.", "Wie", "die", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "VVIMP", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des engen Tagwerks mich umfing,", "tokens": ["Des", "en\u00b7gen", "Tag\u00b7werks", "mich", "um\u00b7fing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie mir im hastigen Gew\u00fchle", "tokens": ["Wie", "mir", "im", "has\u00b7ti\u00b7gen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der gleiche Mut verloren ging \u2013", "tokens": ["Der", "glei\u00b7che", "Mut", "ver\u00b7lo\u00b7ren", "ging", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Leib verfiel dem langen Kranken,", "tokens": ["Der", "Leib", "ver\u00b7fiel", "dem", "lan\u00b7gen", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele zittert' in der Pein,", "tokens": ["Die", "See\u00b7le", "zit\u00b7tert'", "in", "der", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da zogen sehnliche Gedanken", "tokens": ["Da", "zo\u00b7gen", "sehn\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "An deine Heilkraft in mich ein.", "tokens": ["An", "dei\u00b7ne", "Heil\u00b7kraft", "in", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nun! \u2013 O, magst du schon dem Knaben", "tokens": ["Und", "nun", "!", "\u2013", "O", ",", "magst", "du", "schon", "dem", "Kna\u00b7ben"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "$(", "NE", "$,", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die noch verh\u00fcllte Seele weihn,", "tokens": ["Die", "noch", "ver\u00b7h\u00fcll\u00b7te", "See\u00b7le", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Mann aus hundert Quellen laben,", "tokens": ["Den", "Mann", "aus", "hun\u00b7dert", "Quel\u00b7len", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Greisen eine Freistatt sein:", "tokens": ["Dem", "Grei\u00b7sen", "ei\u00b7ne", "Freis\u00b7tatt", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur wer genest, f\u00fchlt ganz tief innen", "tokens": ["Nur", "wer", "ge\u00b7nest", ",", "f\u00fchlt", "ganz", "tief", "in\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VVFIN", "$,", "VVFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die F\u00fclle deiner Liebeskraft,", "tokens": ["Die", "F\u00fcl\u00b7le", "dei\u00b7ner", "Lie\u00b7bes\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und rein und reizbar noch an Sinnen,", "tokens": ["Und", "rein", "und", "reiz\u00b7bar", "noch", "an", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Umf\u00e4ngt er dich mit Leidenschaft.", "tokens": ["Um\u00b7f\u00e4ngt", "er", "dich", "mit", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So nimm mich wieder, hehres Leben,", "tokens": ["So", "nimm", "mich", "wie\u00b7der", ",", "heh\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem Scho\u00dfe birg den Sohn!", "tokens": ["In", "dei\u00b7nem", "Scho\u00b7\u00dfe", "birg", "den", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du l\u00e4chelst mir, du hast vergeben", "tokens": ["Du", "l\u00e4\u00b7chelst", "mir", ",", "du", "hast", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und segnest den Verirrten schon.", "tokens": ["Und", "seg\u00b7nest", "den", "Ver\u00b7irr\u00b7ten", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du \u00fcbert\u00f6nst mit Vogelstimmen", "tokens": ["Du", "\u00fc\u00b7ber\u00b7t\u00f6nst", "mit", "Vo\u00b7gel\u00b7stim\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Beichte, die dein Ohr vernahm,", "tokens": ["Die", "Beich\u00b7te", ",", "die", "dein", "Ohr", "ver\u00b7nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und in des Morgens Gl\u00fchn und Glimmen", "tokens": ["Und", "in", "des", "Mor\u00b7gens", "Gl\u00fchn", "und", "Glim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begr\u00e4bst du dieses Rot der Scham.", "tokens": ["Be\u00b7gr\u00e4bst", "du", "die\u00b7ses", "Rot", "der", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Als h\u00e4tt' uns lang ein Zwist geschieden,", "tokens": ["Als", "h\u00e4tt'", "uns", "lang", "ein", "Zwist", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nun geschlichtet wunderbar,", "tokens": ["Der", "nun", "ge\u00b7schlich\u00b7tet", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trat ich ein in deinen Frieden", "tokens": ["So", "trat", "ich", "ein", "in", "dei\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ward im Tiefsten still und klar.", "tokens": ["Und", "ward", "im", "Tiefs\u00b7ten", "still", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sah das Meer sich leuchtend dehnen,", "tokens": ["Ich", "sah", "das", "Meer", "sich", "leuch\u00b7tend", "deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Fr\u00fchlingswonnen stand die Flur,", "tokens": ["In", "Fr\u00fch\u00b7lings\u00b7won\u00b7nen", "stand", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos"