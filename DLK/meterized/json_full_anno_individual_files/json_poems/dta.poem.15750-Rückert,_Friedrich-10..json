{"dta.poem.15750": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fcckte Zeiten, wo ein einzig Angesicht", "tokens": ["Be\u00b7gl\u00fcck\u00b7te", "Zei\u00b7ten", ",", "wo", "ein", "ein\u00b7zig", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt dem Dichter zeigt, und ihm wird Ein Gedicht.", "tokens": ["Die", "Welt", "dem", "Dich\u00b7ter", "zeigt", ",", "und", "ihm", "wird", "Ein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In unsern Zeiten zeigt sie gar viel Angesichter,", "tokens": ["In", "un\u00b7sern", "Zei\u00b7ten", "zeigt", "sie", "gar", "viel", "An\u00b7ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jedem anzuthun sein Recht vermag kein Dichter.", "tokens": ["Und", "je\u00b7dem", "an\u00b7zu\u00b7thun", "sein", "Recht", "ver\u00b7mag", "kein", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PIS", "VVIZU", "PPOSAT", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er wird, wenn er sich h\u00e4lt an eine Seit', einseitig,", "tokens": ["Er", "wird", ",", "wenn", "er", "sich", "h\u00e4lt", "an", "ei\u00b7ne", "Seit'", ",", "ein\u00b7sei\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schwindlig, wenn er will auf alles sehn gleichzeitig.", "tokens": ["Und", "schwind\u00b7lig", ",", "wenn", "er", "will", "auf", "al\u00b7les", "sehn", "gleich\u00b7zei\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}