{"dta.poem.962": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "\u00dc BERSCHRIFTEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Des sehers wort ist wenigen gemeinsam               ", "tokens": ["Des", "se\u00b7hers", "wort", "ist", "we\u00b7ni\u00b7gen", "ge\u00b7mein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schon als die ersten k&#252;hnen w&#252;nsche kamen               ", "tokens": ["Schon", "als", "die", "ers\u00b7ten", "k", "&#252;", "hnen", "w", "&#252;", "nsche", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$(", "VVFIN", "NE", "$(", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In einem seltnen reiche ernst und einsam               ", "tokens": ["In", "ei\u00b7nem", "selt\u00b7nen", "rei\u00b7che", "ernst", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erfand er f&#252;r die dinge eigne namen             ", "tokens": ["Er\u00b7fand", "er", "f", "&#252;", "r", "die", "din\u00b7ge", "eig\u00b7ne", "na\u00b7men"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "$(", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Die hier erdonnerten von ungeheuern               ", "tokens": ["Die", "hier", "er\u00b7don\u00b7ner\u00b7ten", "von", "un\u00b7ge\u00b7heu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Befehlen oder lispelten wie bitten               ", "tokens": ["Be\u00b7feh\u00b7len", "o\u00b7der", "lis\u00b7pel\u00b7ten", "wie", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "KOKOM", "VVINF"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Die wie Paktolen in rubinenfeuern               ", "tokens": ["Die", "wie", "Pak\u00b7to\u00b7len", "in", "ru\u00b7bi\u00b7nen\u00b7feu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bald wie linde fr&#252;hlingsb&#228;che glitten             ", "tokens": ["Und", "bald", "wie", "lin\u00b7de", "fr", "&#252;", "hlingsb", "&#228;", "che", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADJA", "FM", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "An deren kraft und klang er sich ergezte               ", "tokens": ["An", "de\u00b7ren", "kraft", "und", "klang", "er", "sich", "er\u00b7gez\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "KON", "VVFIN", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie waren wenn er sich im h\u00f6chsten schwunge               ", "tokens": ["Sie", "wa\u00b7ren", "wenn", "er", "sich", "im", "h\u00f6chs\u00b7ten", "schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der welt entfliehend unter tr&#228;ume sezte               ", "tokens": ["Der", "welt", "ent\u00b7flie\u00b7hend", "un\u00b7ter", "tr", "&#228;", "u\u00b7me", "sez\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPR", "NE", "$(", "FM.la", "FM.la"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des tempels saitenspiel und heilge zunge             ", "tokens": ["Des", "tem\u00b7pels", "sai\u00b7ten\u00b7spiel", "und", "heil\u00b7ge", "zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Nur sie &#8212; und nicht der sanften lehre lallen               ", "tokens": ["Nur", "sie", "&#8212;", "und", "nicht", "der", "sanf\u00b7ten", "leh\u00b7re", "lal\u00b7len"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$(", "KON", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das m&#252;tterliche &#8212; hat er sich erlesen               ", "tokens": ["Das", "m", "&#252;", "tter\u00b7li\u00b7che", "&#8212;", "hat", "er", "sich", "er\u00b7le\u00b7sen"], "token_info": ["word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "$(", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als er im rausch von mai und nachtigallen               ", "tokens": ["Als", "er", "im", "rausch", "von", "mai", "und", "nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJD", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sann &#252;ber erster sehnsucht fabelwesen             ", "tokens": ["Sann", "&#252;", "ber", "ers\u00b7ter", "sehn\u00b7sucht", "fa\u00b7bel\u00b7we\u00b7sen"], "token_info": ["word", "XML_entity", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als er zum lenker seiner lebensfr&#252;he               ", "tokens": ["Als", "er", "zum", "len\u00b7ker", "sei\u00b7ner", "le\u00b7bens\u00b7fr", "&#252;", "he"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "FM", "FM", "FM"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im beten rief ob die verheissung l\u00f6ge &#183; &#183;               ", "tokens": ["Im", "be\u00b7ten", "rief", "ob", "die", "ver\u00b7heis\u00b7sung", "l\u00f6\u00b7ge", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "XML_entity"], "pos": ["APPRART", "ADJA", "VVFIN", "KOUS", "ART", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erflehend dass aus zagen busens m&#252;he               ", "tokens": ["Er\u00b7fle\u00b7hend", "dass", "aus", "za\u00b7gen", "bu\u00b7sens", "m", "&#252;", "he"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["VVPP", "KOUS", "APPR", "ADJA", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das denkbild sich zur sonne heben m\u00f6ge.             ", "tokens": ["Das", "denk\u00b7bild", "sich", "zur", "son\u00b7ne", "he\u00b7ben", "m\u00f6\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}