{"textgrid.poem.67731": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist! Gesalbter, den uns Gott versprach!", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist! Gesalbter, den uns Gott versprach!", "tokens": ["Du", "bist", "!", "Ge\u00b7salb\u00b7ter", ",", "den", "uns", "Gott", "ver\u00b7sprach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es gl\u00e4nzt Dein neues Reich", "tokens": ["Es", "gl\u00e4nzt", "Dein", "neu\u00b7es", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Himmel auf. Die V\u00f6lker feiern nach", "tokens": ["Den", "Him\u00b7mel", "auf", ".", "Die", "V\u00f6l\u00b7ker", "fei\u00b7ern", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und knien. Der Mond erhebt es bleich.", "tokens": ["Und", "kni\u00b7en", ".", "Der", "Mond", "er\u00b7hebt", "es", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Jehovah, der zu Meeren spricht: \u00bbVerseigt!", "tokens": ["Je\u00b7ho\u00b7vah", ",", "der", "zu", "Mee\u00b7ren", "spricht", ":", "\u00bb", "Ver\u00b7seigt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$.", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es wachse Land hervor,", "tokens": ["Es", "wach\u00b7se", "Land", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus ihm Gold!\u00ab sprach zu den ... \u00bbWeicht!\u00ab", "tokens": ["Und", "aus", "ihm", "Gold", "!", "\u00ab", "sprach", "zu", "den", "...", "\u00bb", "Weicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "NN", "$.", "$(", "VVFIN", "APPR", "ART", "$(", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sprach's: es wuchs ein Baum empor.", "tokens": ["Er", "sprach's", ":", "es", "wuchs", "ein", "Baum", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus der Mandane dunklem Schoo\u00df kam ", "tokens": ["Aus", "der", "Man\u00b7da\u00b7ne", "dunk\u00b7lem", "Schoo\u00df", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Asien ist kaum", "tokens": ["Und", "A\u00b7sien", "ist", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "VAFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zum weiten Schatten g'nug, vom schwarzen Meer", "tokens": ["Zum", "wei\u00b7ten", "Schat\u00b7ten", "g'\u00b7nug", ",", "vom", "schwar\u00b7zen", "Meer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bis zu des rothen Purpursaum.", "tokens": ["Bis", "zu", "des", "ro\u00b7then", "Pur\u00b7pur\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Herr rief ", "tokens": ["Der", "Herr", "rief"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Man sah ", "tokens": ["Man", "sah"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die H\u00fcgel flohn; es b\u00fcckte sich das Land;", "tokens": ["Die", "H\u00fc\u00b7gel", "flohn", ";", "es", "b\u00fcck\u00b7te", "sich", "das", "Land", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Und Riegel, Thore, Schlo\u00df zerbrach der Herr;", "tokens": ["Und", "Rie\u00b7gel", ",", "Tho\u00b7re", ",", "Schlo\u00df", "zer\u00b7brach", "der", "Herr", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da gab er ", "tokens": ["Da", "gab", "er"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der Tiefe Sch\u00e4tze. Wer ist m\u00e4chtiger", "tokens": ["Der", "Tie\u00b7fe", "Sch\u00e4t\u00b7ze", ".", "Wer", "ist", "m\u00e4ch\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "VAFIN", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sch\u00f6ner als Jehovens Thron!", "tokens": ["Und", "sch\u00f6\u00b7ner", "als", "Je\u00b7ho\u00b7vens", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bber schaffet Frieden,\u00ab spricht des Lichtes Aufgang;", "tokens": ["\u00bb", "er", "schaf\u00b7fet", "Frie\u00b7den", ",", "\u00ab", "spricht", "des", "Lich\u00b7tes", "Auf\u00b7gang", ";"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$,", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bber ist, er ist der Herr!\u00ab", "tokens": ["\u00bb", "er", "ist", ",", "er", "ist", "der", "Herr", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bber schaffet Uebel,\u00ab spricht der Niedergang,", "tokens": ["\u00bb", "er", "schaf\u00b7fet", "Ue\u00b7bel", ",", "\u00ab", "spricht", "der", "Nie\u00b7der\u00b7gang", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Cyrus spricht: \u00bbEr ist der ", "tokens": ["Und", "Cy\u00b7rus", "spricht", ":", "\u00bb", "Er", "ist", "der"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "PPER", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Ceder bebe! durch des Tempels Schutt", "tokens": ["Die", "Ce\u00b7der", "be\u00b7be", "!", "durch", "des", "Tem\u00b7pels", "Schutt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rausch' heil'ge Sympathie!", "tokens": ["Rausch'", "heil'\u00b7ge", "Sym\u00b7pa\u00b7thie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er spricht zur Stadt und Tempel: \u00bbSeid gebaut!\u00ab \u2013 es thut", "tokens": ["Er", "spricht", "zur", "Stadt", "und", "Tem\u00b7pel", ":", "\u00bb", "Seid", "ge\u00b7baut", "!", "\u00ab", "\u2013", "es", "thut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$.", "$(", "VAIMP", "VVPP", "$.", "$(", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es Cyrus, und \u2013 da stehen sie!", "tokens": ["Es", "Cy\u00b7rus", ",", "und", "\u2013", "da", "ste\u00b7hen", "sie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "KON", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der g\u00fcrtet K\u00f6nigen das Blutschwert ab", "tokens": ["Der", "g\u00fcr\u00b7tet", "K\u00f6\u00b7ni\u00b7gen", "das", "Blut\u00b7schwert", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und regnet Ruh und Gl\u00fcck", "tokens": ["Und", "reg\u00b7net", "Ruh", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf seine Heerden; Fremde giebt sein Stab", "tokens": ["Auf", "sei\u00b7ne", "Heer\u00b7den", ";", "Frem\u00b7de", "giebt", "sein", "Stab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem ersten Hirten gern zur\u00fcck.", "tokens": ["Dem", "ers\u00b7ten", "Hir\u00b7ten", "gern", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O h\u00fcpfe, Volk! wie sich die Taube freut,", "tokens": ["O", "h\u00fcp\u00b7fe", ",", "Volk", "!", "wie", "sich", "die", "Tau\u00b7be", "freut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$.", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich ihr Retter naht.", "tokens": ["Wenn", "sich", "ihr", "Ret\u00b7ter", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lechzt das Kind zur Mutter hin; sie beut", "tokens": ["So", "lechzt", "das", "Kind", "zur", "Mut\u00b7ter", "hin", ";", "sie", "beut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihm ihre Brust, um die es bat,", "tokens": ["Ihm", "ih\u00b7re", "Brust", ",", "um", "die", "es", "bat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und weint und sieht herab. Es trinkt und blickt", "tokens": ["Und", "weint", "und", "sieht", "her\u00b7ab", ".", "Es", "trinkt", "und", "blickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr l\u00e4chelnd Dank herauf.", "tokens": ["Ihr", "l\u00e4\u00b7chelnd", "Dank", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O K\u00f6nig, schau vom Throne! Juda schickt", "tokens": ["O", "K\u00f6\u00b7nig", ",", "schau", "vom", "Thro\u00b7ne", "!", "Ju\u00b7da", "schickt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJD", "APPRART", "NN", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir mindstens eine Thr\u00e4ne auf.", "tokens": ["Dir", "minds\u00b7tens", "ei\u00b7ne", "Thr\u00e4\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"lin