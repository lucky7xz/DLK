{"dta.poem.14663": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewache auf, wach' auf, Du Sch\u00f6ne!", "tokens": ["\u201e", "wa\u00b7che", "auf", ",", "wach'", "auf", ",", "Du", "Sch\u00f6\u00b7ne", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebesheimath ringsum lacht,", "tokens": ["Lie\u00b7bes\u00b7hei\u00b7math", "ring\u00b7sum", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zaubrisch ringen Duft und T\u00f6ne,", "tokens": ["Zaub\u00b7risch", "rin\u00b7gen", "Duft", "und", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderbarer Blumen Pracht", "tokens": ["Wun\u00b7der\u00b7ba\u00b7rer", "Blu\u00b7men", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Funkelt rings im Morgengolde \u2014", "tokens": ["Fun\u00b7kelt", "rings", "im", "Mor\u00b7gen\u00b7gol\u00b7de"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schau um Dich! wach auf, Du Holde!\u201c", "tokens": ["Schau", "um", "Dich", "!", "wach", "auf", ",", "Du", "Hol\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "ADJD", "PTKVZ", "$,", "PPER", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}