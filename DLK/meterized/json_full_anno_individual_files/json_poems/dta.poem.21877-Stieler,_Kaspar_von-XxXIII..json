{"dta.poem.21877": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Du sichst mir zu/ Vulkan/", "tokens": ["Du", "sichst", "mir", "zu", "/", "Vul\u00b7kan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "$(", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "wie ich die Venus herze/", "tokens": ["wie", "ich", "die", "Ve\u00b7nus", "her\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und/ weil du meinst/ ich scherze/", "tokens": ["und", "/", "weil", "du", "meinst", "/", "ich", "scher\u00b7ze", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "ADV", "$(", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wie wirstu heissen? Pan.", "tokens": ["wie", "wirs\u00b7tu", "heis\u00b7sen", "?", "Pan", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$.", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}