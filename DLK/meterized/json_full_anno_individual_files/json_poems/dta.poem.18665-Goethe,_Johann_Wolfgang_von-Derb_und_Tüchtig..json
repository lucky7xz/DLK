{"dta.poem.18665": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Derb und T\u00fcchtig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dichten ist ein Uebermuth,", "tokens": ["Dich\u00b7ten", "ist", "ein", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand schelte mich!", "tokens": ["Nie\u00b7mand", "schel\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Habt getrost ein warmes Blut", "tokens": ["Habt", "ge\u00b7trost", "ein", "war\u00b7mes", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh und frey wie ich.", "tokens": ["Froh", "und", "frey", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Sollte jeder Stunde Pein", "tokens": ["Soll\u00b7te", "je\u00b7der", "Stun\u00b7de", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bitter schmecken mir;", "tokens": ["Bit\u00b7ter", "schme\u00b7cken", "mir", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00fcrd\u2019 ich auch bescheiden seyn", "tokens": ["W\u00fcrd'", "ich", "auch", "be\u00b7schei\u00b7den", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch mehr als ihr.", "tokens": ["Und", "noch", "mehr", "als", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Denn Bescheidenheit ist fein", "tokens": ["Denn", "Be\u00b7schei\u00b7den\u00b7heit", "ist", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das M\u00e4dchen bl\u00fcht,", "tokens": ["Wenn", "das", "M\u00e4d\u00b7chen", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sie will zart geworben seyn", "tokens": ["Sie", "will", "zart", "ge\u00b7wor\u00b7ben", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVPP", "VAINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Rohen flieht.", "tokens": ["Die", "den", "Ro\u00b7hen", "flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Auch ist gut Bescheidenheit", "tokens": ["Auch", "ist", "gut", "Be\u00b7schei\u00b7den\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht ein weiser Mann,", "tokens": ["Spricht", "ein", "wei\u00b7ser", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der von Zeit und Ewigkeit", "tokens": ["Der", "von", "Zeit", "und", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich belehren kann!", "tokens": ["Mich", "be\u00b7leh\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Dichten ist ein Uebermuth!", "tokens": ["Dich\u00b7ten", "ist", "ein", "Ue\u00b7ber\u00b7muth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treib\u2019 es gern allein.", "tokens": ["Treib'", "es", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Freund und Frauen, frisch von Blut,", "tokens": ["Freund", "und", "Frau\u00b7en", ",", "frisch", "von", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt nur auch herein.", "tokens": ["Kommt", "nur", "auch", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "M\u00f6nchlein ohne Kapp\u2019 und Kutt\u2019", "tokens": ["M\u00f6nch\u00b7lein", "oh\u00b7ne", "Kapp'", "und", "Kutt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwatze nicht auf mich ein,", "tokens": ["Schwat\u00b7ze", "nicht", "auf", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Zwar du machest mich caput,", "tokens": ["Zwar", "du", "ma\u00b7chest", "mich", "ca\u00b7put", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht bescheiden! Nein.", "tokens": ["Nicht", "be\u00b7schei\u00b7den", "!", "Nein", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Deiner Phrasen leeres Was", "tokens": ["Dei\u00b7ner", "Phra\u00b7sen", "lee\u00b7res", "Was"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibet mich davon,", "tokens": ["Trei\u00b7bet", "mich", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Abgeschliffen hab\u2019 ich das", "tokens": ["Ab\u00b7ge\u00b7schlif\u00b7fen", "hab'", "ich", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An den Solen schon.", "tokens": ["An", "den", "So\u00b7len", "schon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn des Dichters M\u00fchle geht", "tokens": ["Wenn", "des", "Dich\u00b7ters", "M\u00fch\u00b7le", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halte sie nicht ein:", "tokens": ["Hal\u00b7te", "sie", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn wer einmal uns versteht", "tokens": ["Denn", "wer", "ein\u00b7mal", "uns", "ver\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "PPER", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Wird uns auch verzeihn.", "tokens": ["Wird", "uns", "auch", "ver\u00b7zeihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}