{"textgrid.poem.48792": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "17. An Dorinnen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Amarellen hier, die Oepfel, diese Pfirschen,", "tokens": ["Die", "A\u00b7ma\u00b7rel\u00b7len", "hier", ",", "die", "O\u00b7ep\u00b7fel", ",", "die\u00b7se", "Pfir\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+----+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "las Amor f\u00fcr sich selbst aus seiner Mutter Scho\u00df", "tokens": ["las", "A\u00b7mor", "f\u00fcr", "sich", "selbst", "aus", "sei\u00b7ner", "Mut\u00b7ter", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPR", "PRF", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und legte sie bei sich in den betauten Mo\u00df.", "tokens": ["und", "leg\u00b7te", "sie", "bei", "sich", "in", "den", "be\u00b7tau\u00b7ten", "Mo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sein Gartenman, Priap, brach ihm auch diese Kirschen,", "tokens": ["Sein", "Gar\u00b7ten\u00b7man", ",", "Priap", ",", "brach", "ihm", "auch", "die\u00b7se", "Kir\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+--+---+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "die aussehn wie dein Mund und ohne Kernen knirschen.", "tokens": ["die", "aus\u00b7sehn", "wie", "dein", "Mund", "und", "oh\u00b7ne", "Ker\u00b7nen", "knir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "KOKOM", "PPOSAT", "NN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df alles trug das Kind auf einer Schalen blo\u00df.", "tokens": ["Di\u00df", "al\u00b7les", "trug", "das", "Kind", "auf", "ei\u00b7ner", "Scha\u00b7len", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich kame gleich darzu, da man den Lustwald schlo\u00df,", "tokens": ["Ich", "ka\u00b7me", "gleich", "dar\u00b7zu", ",", "da", "man", "den", "Lust\u00b7wald", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der ringsum wird verwacht mit Gemsen und mit Hirschen.", "tokens": ["der", "ring\u00b7sum", "wird", "ver\u00b7wacht", "mit", "Gem\u00b7sen", "und", "mit", "Hir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er lachte mich halb an und bote mir die Sch\u00fcssel.", "tokens": ["Er", "lach\u00b7te", "mich", "halb", "an", "und", "bo\u00b7te", "mir", "die", "Sch\u00fcs\u00b7sel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da sprach er: Nim es ganz und hier ist auch der Schl\u00fcssel,", "tokens": ["Da", "sprach", "er", ":", "Nim", "es", "ganz", "und", "hier", "ist", "auch", "der", "Schl\u00fcs\u00b7sel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "PPER", "ADV", "KON", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "besuch' uns, wenn du wilt. Lieb, dieses send' ich dir.", "tokens": ["be\u00b7such'", "uns", ",", "wenn", "du", "wilt", ".", "Lieb", ",", "die\u00b7ses", "send'", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$.", "NN", "$,", "PDS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gef\u00e4llt es dir wie mir, so la\u00df uns ohne S\u00e4umen", "tokens": ["Ge\u00b7f\u00e4llt", "es", "dir", "wie", "mir", ",", "so", "la\u00df", "uns", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PWAV", "PPER", "$,", "ADV", "VVIMP", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "noch heute gehen hin zu solchen s\u00fc\u00dfen B\u00e4umen.", "tokens": ["noch", "heu\u00b7te", "ge\u00b7hen", "hin", "zu", "sol\u00b7chen", "s\u00fc\u00b7\u0