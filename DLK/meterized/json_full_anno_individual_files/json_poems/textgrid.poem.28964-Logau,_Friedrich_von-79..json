{"textgrid.poem.28964": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den, der sich nicht wehren wil, heist man, wie man heist das Theil,", "tokens": ["Den", ",", "der", "sich", "nicht", "weh\u00b7ren", "wil", ",", "heist", "man", ",", "wie", "man", "heist", "das", "Theil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "PIS", "$,", "PWAV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das de\u00df Hundes Weib so frey pflegt zu brauchen und so geil.", "tokens": ["Das", "de\u00df", "Hun\u00b7des", "Weib", "so", "frey", "pflegt", "zu", "brau\u00b7chen", "und", "so", "geil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "KON", "ADV", "ADJD", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie so di\u00df? Weil ieder Hund dran sich macht, dran reibt, dran reucht,", "tokens": ["Wie", "so", "di\u00df", "?", "Weil", "ie\u00b7der", "Hund", "dran", "sich", "macht", ",", "dran", "reibt", ",", "dran", "reucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDS", "$.", "KOUS", "PIAT", "NN", "PAV", "PRF", "VVFIN", "$,", "PAV", "VVFIN", "$,", "PAV", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und also den feigen Mann ieder braucht, wie ihn nur deucht,", "tokens": ["Und", "al\u00b7so", "den", "fei\u00b7gen", "Mann", "ie\u00b7der", "braucht", ",", "wie", "ihn", "nur", "deucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Oder weil die deutsche Welt weiland einen Hund band auff", "tokens": ["O\u00b7der", "weil", "die", "deut\u00b7sche", "Welt", "wei\u00b7land", "ei\u00b7nen", "Hund", "band", "auff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVFIN", "APPR"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Dem, der au\u00df der Schlacht entgieng, nicht durch Gegenwehr, durch Lauff.", "tokens": ["Dem", ",", "der", "au\u00df", "der", "Schlacht", "ent\u00b7gieng", ",", "nicht", "durch", "Ge\u00b7gen\u00b7wehr", ",", "durch", "Lauff", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,", "PTKNEG", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Den, der sich nicht wehren wil, heist man, wie man heist das Theil,", "tokens": ["Den", ",", "der", "sich", "nicht", "weh\u00b7ren", "wil", ",", "heist", "man", ",", "wie", "man", "heist", "das", "Theil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "PIS", "$,", "PWAV", "PIS", 