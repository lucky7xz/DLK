"iambic.tetra"}, "line.3": {"text": "So sehr, als ander Unkraut, schadet,", "tokens": ["So", "sehr", ",", "als", "an\u00b7der", "Un\u00b7kraut", ",", "scha\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADJD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und man es leicht aus selbem radet,", "tokens": ["Und", "man", "es", "leicht", "aus", "sel\u00b7bem", "ra\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPER", "ADJD", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Auch, wenns Getrayde h\u00f6her steiget,", "tokens": ["Auch", ",", "wenns", "Ge\u00b7tray\u00b7de", "h\u00f6\u00b7her", "stei\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es sich zum Untergange neiget;", "tokens": ["Es", "sich", "zum", "Un\u00b7ter\u00b7gan\u00b7ge", "nei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kann ich mich darum nicht kr\u00e4nken,", "tokens": ["So", "kann", "ich", "mich", "da\u00b7rum", "nicht", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vielmehr zwingt ihrer Farben Pracht,", "tokens": ["Viel\u00b7mehr", "zwingt", "ih\u00b7rer", "Far\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Durch eine angenehme Macht,", "tokens": ["Durch", "ei\u00b7ne", "an\u00b7ge\u00b7neh\u00b7me", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von ihnen dieses zu gedenken:", "tokens": ["Von", "ih\u00b7nen", "die\u00b7ses", "zu", "ge\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch diese Bluhmen, da sie sind, so m\u00fcssen sie auch", "tokens": ["Auch", "die\u00b7se", "Bluh\u00b7men", ",", "da", "sie", "sind", ",", "so", "m\u00fcs\u00b7sen", "sie", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$,", "KOUS", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "n\u00fctzlich seyn,", "tokens": ["n\u00fctz\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ob wir den Nutzen gleich nicht kennen:", "tokens": ["Ob", "wir", "den", "Nut\u00b7zen", "gleich", "nicht", "ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie sich der Nutzen denn erweiset,", "tokens": ["Wie", "sich", "der", "Nut\u00b7zen", "denn", "er\u00b7wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Da, statt des Kohls, die\u00df Kraut oft speiset;", "tokens": ["Da", ",", "statt", "des", "Kohls", ",", "die\u00df", "Kraut", "oft", "spei\u00b7set", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUI", "ART", "NN", "$,", "PDS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ja, noch dazu, da\u00df, da es d\u00fcngt,", "tokens": ["Ja", ",", "noch", "da\u00b7zu", ",", "da\u00df", ",", "da", "es", "d\u00fcngt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "PAV", "$,", "KOUS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es einem Acker Nutzen bringt.", "tokens": ["Es", "ei\u00b7nem", "A\u00b7cker", "Nut\u00b7zen", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Nun h\u00e4tte Gott so holden Schein,", "tokens": ["Nun", "h\u00e4t\u00b7te", "Gott", "so", "hol\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihnen, uns versagen k\u00f6nnen,", "tokens": ["In", "ih\u00b7nen", ",", "uns", "ver\u00b7sa\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und statt, vor aller Menschen Blicken,", "tokens": ["Und", "statt", ",", "vor", "al\u00b7ler", "Men\u00b7schen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n und lieblich sie zu schm\u00fccken,", "tokens": ["So", "sch\u00f6n", "und", "lieb\u00b7lich", "sie", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Trauer-Farben sie bedecken,", "tokens": ["Mit", "Trau\u00b7e\u00b7rFa\u00b7rben", "sie", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In schmutz\u2019gem Kleide sie verstecken,", "tokens": ["In", "schmutz'\u00b7gem", "Klei\u00b7de", "sie", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und, durch den Anblick, uns betr\u00fcben", "tokens": ["Und", ",", "durch", "den", "An\u00b7blick", ",", "uns", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schrecken k\u00f6nnen; aber so", "tokens": ["Und", "schre\u00b7cken", "k\u00f6n\u00b7nen", ";", "a\u00b7ber", "so"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "VMINF", "$.", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wird man fast, wider Willen, froh,", "tokens": ["Wird", "man", "fast", ",", "wi\u00b7der", "Wil\u00b7len", ",", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Man mu\u00df fast ihren Schimmer lieben,", "tokens": ["Man", "mu\u00df", "fast", "ih\u00b7ren", "Schim\u00b7mer", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn wir, wie sie so gl\u00e4nzend, sch\u00f6n", "tokens": ["Wenn", "wir", ",", "wie", "sie", "so", "gl\u00e4n\u00b7zend", ",", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Erde schm\u00fccken, wie sie bl\u00fch\u2019n,", "tokens": ["Die", "Er\u00b7de", "schm\u00fc\u00b7cken", ",", "wie", "sie", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ja fast im gelben Feuer gl\u00fch\u2019n,", "tokens": ["Ja", "fast", "im", "gel\u00b7ben", "Feu\u00b7er", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit einiger Betrachtung, seh\u2019n.", "tokens": ["Mit", "ei\u00b7ni\u00b7ger", "Be\u00b7trach\u00b7tung", ",", "seh'", "n."], "token_info": ["word", "word", "word", "punct", "word", "abbreviation"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Auch wenn die Sonne selbst nicht scheinet,", "tokens": ["Auch", "wenn", "die", "Son\u00b7ne", "selbst", "nicht", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00e4nzen sie so hell, so rein,", "tokens": ["So", "gl\u00e4n\u00b7zen", "sie", "so", "hell", ",", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unser Auge \u00f6fters meynet,", "tokens": ["Da\u00df", "un\u00b7ser", "Au\u00b7ge", "\u00f6f\u00b7ters", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00fcrden sie vom Sonnen-Schein,", "tokens": ["Als", "w\u00fcr\u00b7den", "sie", "vom", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Auch selbst in tr\u00fcber Luft, bestrahlet,", "tokens": ["Auch", "selbst", "in", "tr\u00fc\u00b7ber", "Luft", ",", "be\u00b7strah\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00f6n sind sie, so ungemein", "tokens": ["So", "sch\u00f6n", "sind", "sie", ",", "so", "un\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Finger der Natur gemahlet.", "tokens": ["Vom", "Fin\u00b7ger", "der", "Na\u00b7tur", "ge\u00b7mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Prangt sonst bey uns das gelbe Gold", "tokens": ["Prangt", "sonst", "bey", "uns", "das", "gel\u00b7be", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Jm Glanz, dem alle Menschen hold;", "tokens": ["Jm", "Glanz", ",", "dem", "al\u00b7le", "Men\u00b7schen", "hold", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So scheinet dieses gelbe Feld", "tokens": ["So", "schei\u00b7net", "die\u00b7ses", "gel\u00b7be", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast eine wirklich g\u00fcldne Welt,", "tokens": ["Fast", "ei\u00b7ne", "wirk\u00b7lich", "g\u00fcld\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wird dahero meistens allen,", "tokens": ["Und", "wird", "da\u00b7he\u00b7ro", "meis\u00b7tens", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch den so lieben Glanz, gefallen.", "tokens": ["Durch", "den", "so", "lie\u00b7ben", "Glanz", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Wenn man nun diese helle Zier", "tokens": ["Wenn", "man", "nun", "die\u00b7se", "hel\u00b7le", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm dunklen Walde, wie ich hier,", "tokens": ["Jm", "dunk\u00b7len", "Wal\u00b7de", ",", "wie", "ich", "hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch dunklen Gegensatz, annoch versch\u00f6nert sieht;", "tokens": ["Durch", "dunk\u00b7len", "Ge\u00b7gen\u00b7satz", ",", "an\u00b7noch", "ver\u00b7sch\u00f6\u00b7nert", "sieht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird auch das schl\u00e4frigste Gem\u00fcht", "tokens": ["Wird", "auch", "das", "schl\u00e4f\u00b7rigs\u00b7te", "Ge\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Recht, als aus einem Schlaf, erwecket,", "tokens": ["Recht", ",", "als", "aus", "ei\u00b7nem", "Schlaf", ",", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df es was liebliches entdecket.", "tokens": ["Da\u00df", "es", "was", "lieb\u00b7li\u00b7ches", "ent\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es wird auch oft, fast wider Willen,", "tokens": ["Es", "wird", "auch", "oft", ",", "fast", "wi\u00b7der", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein\u2019 Art von Anmuht ihn erf\u00fcllen,", "tokens": ["Ein'", "Art", "von", "An\u00b7muht", "ihn", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die, durch das Aug\u2019, ans Herze dringt.", "tokens": ["Die", ",", "durch", "das", "Aug'", ",", "ans", "Her\u00b7ze", "dringt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ach, la\u00dft uns denn auch unser Denken", "tokens": ["Ach", ",", "la\u00dft", "uns", "denn", "auch", "un\u00b7ser", "Den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auf das, was wirklich sch\u00f6n ist, lenken,", "tokens": ["Auf", "das", ",", "was", "wirk\u00b7lich", "sch\u00f6n", "ist", ",", "len\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "ADJD", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und, aus der recht ger\u00fchrten Brust,", "tokens": ["Und", ",", "aus", "der", "recht", "ge\u00b7r\u00fchr\u00b7ten", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr die darinn geschenkte Lust,", "tokens": ["F\u00fcr", "die", "da\u00b7rinn", "ge\u00b7schenk\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dem, Der sie schenkt, ein Dank-Lied schenken!", "tokens": ["Dem", ",", "Der", "sie", "schenkt", ",", "ein", "Dank\u00b7Lied", "schen\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-++--", "measure": "unknown.measure.tetra"}}}}}