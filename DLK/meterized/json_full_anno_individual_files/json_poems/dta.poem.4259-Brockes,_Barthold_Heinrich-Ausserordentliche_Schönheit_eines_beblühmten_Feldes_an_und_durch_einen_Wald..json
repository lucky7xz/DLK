{"dta.poem.4259": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ausserordentliche  \n  Sch\u00f6nheit eines bebl\u00fchmten Feldes  \n an und durch einen Wald.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bey entw\u00f6lktem heitern Wetter,", "tokens": ["Wer", "bey", "ent\u00b7w\u00f6lk\u00b7tem", "hei\u00b7tern", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In W\u00e4ldern, die bestrahlten Bl\u00e4tter,", "tokens": ["In", "W\u00e4l\u00b7dern", ",", "die", "be\u00b7strahl\u00b7ten", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem menschlichen Gem\u00fcht,", "tokens": ["Mit", "ei\u00b7nem", "menschli\u00b7chen", "Ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das hei\u00dft, mit Ueberlegung, sieht;", "tokens": ["Das", "hei\u00dft", ",", "mit", "Ue\u00b7ber\u00b7le\u00b7gung", ",", "sieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird, durch den Schmuck der Luft und Erden,", "tokens": ["Wird", ",", "durch", "den", "Schmuck", "der", "Luft", "und", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der fast durchl\u00e4ucht\u2019gen Bl\u00e4tter Pracht,", "tokens": ["Der", "fast", "durch\u00b7l\u00e4ucht'\u00b7gen", "Bl\u00e4t\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Preise De\u00df, Der sie gemacht,", "tokens": ["Zum", "Prei\u00b7se", "De\u00df", ",", "Der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "$,", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Recht innerlich ger\u00fchret werden.", "tokens": ["Recht", "in\u00b7ner\u00b7lich", "ge\u00b7r\u00fch\u00b7ret", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel\u2019 an- und durchgestrahlte Stellen,", "tokens": ["Viel'", "an", "und", "durch\u00b7ges\u00b7trahl\u00b7te", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, durch der Schatten gr\u00fcne Nacht,", "tokens": ["Die", ",", "durch", "der", "Schat\u00b7ten", "gr\u00fc\u00b7ne", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich noch um desto mehr erhellen,", "tokens": ["Sich", "noch", "um", "des\u00b7to", "mehr", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ergetzen, durch der holden B\u00fcsche,", "tokens": ["Er\u00b7get\u00b7zen", ",", "durch", "der", "hol\u00b7den", "B\u00fc\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von D\u00e4mmrung bald, und bald vom Licht,", "tokens": ["Von", "D\u00e4mm\u00b7rung", "bald", ",", "und", "bald", "vom", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Formiertem lieblichen Gemische,", "tokens": ["For\u00b7mier\u00b7tem", "lieb\u00b7li\u00b7chen", "Ge\u00b7mi\u00b7sche", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein sie betrachtendes Gesicht.", "tokens": ["Ein", "sie", "be\u00b7trach\u00b7ten\u00b7des", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Das Auge schwimmt, von Lust entz\u00fccket,", "tokens": ["Das", "Au\u00b7ge", "schwimmt", ",", "von", "Lust", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In einem gr\u00fcnen Anmuhts-Meer,", "tokens": ["In", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Anm\u00b7uhts\u00b7Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das, was es \u00fcberall erblicket,", "tokens": ["Das", ",", "was", "es", "\u00fc\u00b7be\u00b7rall", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Zieht seine Blicke hin und her,", "tokens": ["Zieht", "sei\u00b7ne", "Bli\u00b7cke", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie wallen, schweben, schiessen, wandern,", "tokens": ["Sie", "wal\u00b7len", ",", "schwe\u00b7ben", ",", "schies\u00b7sen", ",", "wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Von einer Sch\u00f6nheit zu der andern,", "tokens": ["Von", "ei\u00b7ner", "Sch\u00f6n\u00b7heit", "zu", "der", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Menge l\u00e4\u00dft sie nirgend steh\u2019n,", "tokens": ["Die", "Men\u00b7ge", "l\u00e4\u00dft", "sie", "nir\u00b7gend", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So da\u00df, weil alles gar zu sch\u00f6n,", "tokens": ["So", "da\u00df", ",", "weil", "al\u00b7les", "gar", "zu", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "KOUS", "PIS", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sie oft, um gar zu viel zu seh\u2019n,", "tokens": ["Sie", "oft", ",", "um", "gar", "zu", "viel", "zu", "seh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUI", "ADV", "PTKA", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nichts eigentlichs zu seh\u2019n, im Stande.", "tokens": ["Nichts", "ei\u00b7gent\u00b7lichs", "zu", "seh'n", ",", "im", "Stan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "PIS", "PTKZU", "VVINF", "$,", "APPRART", "NN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "In welchem Zustand ich mich dann,", "tokens": ["In", "wel\u00b7chem", "Zu\u00b7stand", "ich", "mich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor wenig Tagen, auch befande,", "tokens": ["Vor", "we\u00b7nig", "Ta\u00b7gen", ",", "auch", "be\u00b7fan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den ich doch kaum beschreiben kann;", "tokens": ["Den", "ich", "doch", "kaum", "be\u00b7schrei\u00b7ben", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sahe Licht und Dunkelheit,", "tokens": ["Ich", "sa\u00b7he", "Licht", "und", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In gleicher gr\u00fcnen Lieblichkeit,", "tokens": ["In", "glei\u00b7cher", "gr\u00fc\u00b7nen", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Schmuck sich, Wechsels- weise, dienen,", "tokens": ["Zum", "Schmuck", "sich", ",", "Wech\u00b7sels", "wei\u00b7se", ",", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "$,", "TRUNC", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df sie alle, durch die Kraft", "tokens": ["Und", "da\u00df", "sie", "al\u00b7le", ",", "durch", "die", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der angenehmen Nachbarschaft,", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7men", "Nach\u00b7bar\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Erhabener und sch\u00f6ner schienen.", "tokens": ["Er\u00b7ha\u00b7be\u00b7ner", "und", "sch\u00f6\u00b7ner", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn bald die Blick\u2019 auf g\u00fcldnen H\u00f6h\u2019n", "tokens": ["Wenn", "bald", "die", "Blick'", "auf", "g\u00fcld\u00b7nen", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der angestrahlten Zweige liefen,", "tokens": ["Der", "an\u00b7ge\u00b7strahl\u00b7ten", "Zwei\u00b7ge", "lie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sunken sie in ja so sch\u00f6n-", "tokens": ["So", "sun\u00b7ken", "sie", "in", "ja", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Licht- scheue dunkel- gr\u00fcne Tiefen,", "tokens": ["Licht", "scheu\u00b7e", "dun\u00b7kel", "gr\u00fc\u00b7ne", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "ADJA", "TRUNC", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In welchen sie denn \u00f6ftermahl,", "tokens": ["In", "wel\u00b7chen", "sie", "denn", "\u00f6f\u00b7ter\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von neuem, von dem Sonnen-Strahl,", "tokens": ["Von", "neu\u00b7em", ",", "von", "dem", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Getroffne lichte Stellen funden,", "tokens": ["Ge\u00b7troff\u00b7ne", "lich\u00b7te", "Stel\u00b7len", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die bey und durch einander stunden.", "tokens": ["Die", "bey", "und", "durch", "ein\u00b7an\u00b7der", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "KON", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zuletzt ri\u00df eine sch\u00f6ne Stelle,", "tokens": ["Zu\u00b7letzt", "ri\u00df", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So noch vor allen andern helle,", "tokens": ["So", "noch", "vor", "al\u00b7len", "an\u00b7dern", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die fast verwirrten Blicke dort,", "tokens": ["Die", "fast", "ver\u00b7wirr\u00b7ten", "Bli\u00b7cke", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach einem ganz bestrahlten Ort,", "tokens": ["Nach", "ei\u00b7nem", "ganz", "be\u00b7strahl\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch eine dunkle Oeffnung, fort,", "tokens": ["Durch", "ei\u00b7ne", "dunk\u00b7le", "Oeff\u00b7nung", ",", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wurden, unter dunkle Bogen,", "tokens": ["Und", "wur\u00b7den", ",", "un\u00b7ter", "dunk\u00b7le", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Weit in die Fern\u2019 hinausgezogen.", "tokens": ["Weit", "in", "die", "Fern'", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die\u00df war ein ebenes Gefilde,", "tokens": ["Die\u00df", "war", "ein", "e\u00b7be\u00b7nes", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gelben Bluhmen ganz bedeckt,", "tokens": ["Mit", "gel\u00b7ben", "Bluh\u00b7men", "ganz", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df, so weit es sich erstreckt,", "tokens": ["Und", "lie\u00df", ",", "so", "weit", "es", "sich", "er\u00b7streckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob es wirkiich sich verg\u00fclde.", "tokens": ["Als", "ob", "es", "wir\u00b7kiich", "sich", "ver\u00b7g\u00fcl\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Kein gelbes Feur kann heller gl\u00fch\u2019n,", "token