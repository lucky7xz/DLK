{"dta.poem.5620": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bevor man nun ein Wesen kan beschreiben,", "tokens": ["Be\u00b7vor", "man", "nun", "ein", "We\u00b7sen", "kan", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Soll man bey unserm erst und bey uns selber bleiben:", "tokens": ["Soll", "man", "bey", "un\u00b7serm", "erst", "und", "bey", "uns", "sel\u00b7ber", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "ADV", "KON", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df wir den herrlichen Zusammenklang verstehen,", "tokens": ["Da\u00df", "wir", "den", "herr\u00b7li\u00b7chen", "Zu\u00b7sam\u00b7men\u00b7klang", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des, was sie bey uns thun, des, was sie in sich sind,", "tokens": ["Des", ",", "was", "sie", "bey", "uns", "thun", ",", "des", ",", "was", "sie", "in", "sich", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,", "ART", "$,", "PRELS", "PPER", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Derhalben mu\u00df man sie von neuen \u00fcbersehen.", "tokens": ["Der\u00b7hal\u00b7ben", "mu\u00df", "man", "sie", "von", "neu\u00b7en", "\u00fc\u00b7ber\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PPER", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man wird in seinen reiffen Jahren", "tokens": ["Man", "wird", "in", "sei\u00b7nen", "reif\u00b7fen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Dinge, die man als ein Kind", "tokens": ["Die", "Din\u00b7ge", ",", "die", "man", "als", "ein", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am allerfestesten geglaubt zu wissen,", "tokens": ["Am", "al\u00b7ler\u00b7fes\u00b7tes\u00b7ten", "ge\u00b7glaubt", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Noch einmal gr\u00fcndlicher erfahren,", "tokens": ["Noch", "ein\u00b7mal", "gr\u00fcnd\u00b7li\u00b7cher", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Noch einmal untersuchen m\u00fcssen.", "tokens": ["Noch", "ein\u00b7mal", "un\u00b7ter\u00b7su\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dahin zu kommen nun, mu\u00df man an allen Dingen,", "tokens": ["Da\u00b7hin", "zu", "kom\u00b7men", "nun", ",", "mu\u00df", "man", "an", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "ADV", "$,", "VMFIN", "PIS", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die man durch Sinnen kennt, sich selbst, zu zweifeln, zwingen.", "tokens": ["Die", "man", "durch", "Sin\u00b7nen", "kennt", ",", "sich", "selbst", ",", "zu", "zwei\u00b7feln", ",", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "VVFIN", "$,", "PRF", "ADV", "$,", "PTKZU", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}