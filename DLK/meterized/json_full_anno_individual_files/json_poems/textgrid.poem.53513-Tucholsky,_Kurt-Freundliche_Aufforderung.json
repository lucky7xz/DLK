{"textgrid.poem.53513": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Freundliche Aufforderung", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein dicker, aber reiner Knabe,", "tokens": ["Ich", "bin", "ein", "di\u00b7cker", ",", "a\u00b7ber", "rei\u00b7ner", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "von treuer, braver, biederer Ehrlichkeit;", "tokens": ["von", "treu\u00b7er", ",", "bra\u00b7ver", ",", "bie\u00b7de\u00b7rer", "Ehr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ich freu mich an dem bi\u00dfchen, das ich habe,", "tokens": ["ich", "freu", "mich", "an", "dem", "bi\u00df\u00b7chen", ",", "das", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und geh in schmucklos grauem B\u00fcrgerkleid.", "tokens": ["und", "geh", "in", "schmuck\u00b7los", "grau\u00b7em", "B\u00fcr\u00b7ger\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch w\u00fcrd auch ich das goldne Kalb umhuppen,", "tokens": ["Doch", "w\u00fcrd", "auch", "ich", "das", "gold\u00b7ne", "Kalb", "um\u00b7hup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "nennt mir ein Schieber eine gro\u00dfe Zahl", "tokens": ["nennt", "mir", "ein", "Schie\u00b7ber", "ei\u00b7ne", "gro\u00b7\u00dfe", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "und deutet auf den Kaffee tief im Schuppen:", "tokens": ["und", "deu\u00b7tet", "auf", "den", "Kaf\u00b7fee", "tief", "im", "Schup\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbna, wollen wir mal?\u00ab", "tokens": ["\u00bb", "na", ",", "wol\u00b7len", "wir", "mal", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Michel steht ausw\u00e4rts. Sei es in Rum\u00e4nien,", "tokens": ["Mi\u00b7chel", "steht", "aus\u00b7w\u00e4rts", ".", "Sei", "es", "in", "Ru\u00b7m\u00e4\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "VAFIN", "PPER", "APPR", "NE", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "sei es in Belgien, seis in der T\u00fcrkei \u2013", "tokens": ["sei", "es", "in", "Bel\u00b7gi\u00b7en", ",", "seis", "in", "der", "T\u00fcr\u00b7kei", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NE", "$,", "ADV", "APPR", "ART", "NN", "$("], "meter": "+--+--+--++", "measure": "dactylic.tri.plus"}, "line.3": {"text": "und in der Heimat sitzen nur die wenigen,", "tokens": ["und", "in", "der", "Hei\u00b7mat", "sit\u00b7zen", "nur", "die", "we\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "PIAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die gr\u00fcnden eine Vaterlandspartei.", "tokens": ["die", "gr\u00fcn\u00b7den", "ei\u00b7ne", "Va\u00b7ter\u00b7land\u00b7spar\u00b7tei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Kammerherr reicht zierlich wie zum Tanze", "tokens": ["Der", "Kam\u00b7mer\u00b7herr", "reicht", "zier\u00b7lich", "wie", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die Fingerspitzen einem General;", "tokens": ["die", "Fin\u00b7ger\u00b7spit\u00b7zen", "ei\u00b7nem", "Ge\u00b7ne\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "stehn sie parat, dann fragt der Chef vons Ganze:", "tokens": ["stehn", "sie", "pa\u00b7rat", ",", "dann", "fragt", "der", "Chef", "vons", "Gan\u00b7ze", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbna, wollen wir mal?\u00ab", "tokens": ["\u00bb", "na", ",", "wol\u00b7len", "wir", "mal", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VMFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Friede ist ein junger, eleganter", "tokens": ["Der", "Frie\u00b7de", "ist", "ein", "jun\u00b7ger", ",", "e\u00b7leg\u00b7an\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Flaneur auf jenem Boulevard der Welt.", "tokens": ["Fla\u00b7neur", "auf", "je\u00b7nem", "Bou\u00b7le\u00b7vard", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Tag zu Tag wird er nur noch charmanter,", "tokens": ["Von", "Tag", "zu", "Tag", "wird", "er", "nur", "noch", "char\u00b7man\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "doch scheints, da\u00df er den Damen nicht gef\u00e4llt.", "tokens": ["doch", "scheints", ",", "da\u00df", "er", "den", "Da\u00b7men", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da gehn nun so viel, mit und ohne Schleier,", "tokens": ["Da", "gehn", "nun", "so", "viel", ",", "mit", "und", "oh\u00b7ne", "Schlei\u00b7er", ","], "token_info": ["w