{"dta.poem.9750": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte weder reich noch thron/ und war gekr\u00f6net/", "tokens": ["Ich", "hat\u00b7te", "we\u00b7der", "reich", "noch", "thron", "/", "und", "war", "ge\u00b7kr\u00f6\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "ADJD", "ADV", "NE", "$(", "KON", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey meiner frau macht sich fast ieder stand belehnet.", "tokens": ["Bey", "mei\u00b7ner", "frau", "macht", "sich", "fast", "ie\u00b7der", "stand", "be\u00b7leh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denckt nicht/ da\u00df mein geschlecht mit mir gegangen ein/", "tokens": ["Denckt", "nicht", "/", "da\u00df", "mein", "ge\u00b7schlecht", "mit", "mir", "ge\u00b7gan\u00b7gen", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$(", "KOUS", "PPOSAT", "VVPP", "APPR", "PPER", "VVPP", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil solcher k\u00f6nige noch viel auf erden seyn.", "tokens": ["Weil", "sol\u00b7cher", "k\u00f6\u00b7ni\u00b7ge", "noch", "viel", "auf", "er\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "ADV", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}