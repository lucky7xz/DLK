{"textgrid.poem.26069": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ein altes Herbstblatt fliegt im Acker auf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein altes Herbstblatt fliegt im Acker auf,", "tokens": ["Ein", "al\u00b7tes", "Herbst\u00b7blatt", "fliegt", "im", "A\u00b7cker", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es raschelt laut, als ob es Leben hat,", "tokens": ["Es", "ra\u00b7schelt", "laut", ",", "als", "ob", "es", "Le\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als will es wieder zu dem Baum hinauf.", "tokens": ["Als", "will", "es", "wie\u00b7der", "zu", "dem", "Baum", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hell glei\u00dft ein Schmetterling her \u00fcbern Flu\u00df,", "tokens": ["Hell", "glei\u00dft", "ein", "Schmet\u00b7ter\u00b7ling", "her", "\u00fc\u00b7bern", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APZR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist wie ein auferstandener Fr\u00fchlingsgeist,", "tokens": ["Ist", "wie", "ein", "auf\u00b7er\u00b7stan\u00b7de\u00b7ner", "Fr\u00fch\u00b7lings\u00b7ge\u00b7ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Blumenseelen schnell beschw\u00f6ren mu\u00df.", "tokens": ["Der", "Blu\u00b7men\u00b7see\u00b7len", "schnell", "be\u00b7schw\u00f6\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bei meinem Fu\u00df geht scheu mein Schatten mit,", "tokens": ["Bei", "mei\u00b7nem", "Fu\u00df", "geht", "scheu", "mein", "Schat\u00b7ten", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ist mir wie die Sehnsucht treu;", "tokens": ["Der", "ist", "mir", "wie", "die", "Sehn\u00b7sucht", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie und mein Herz, die halten ewig Schritt.", "tokens": ["Sie", "und", "mein", "Herz", ",", "die", "hal\u00b7ten", "e\u00b7wig", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein altes Herbstblatt fliegt im Acker auf,", "tokens": ["Ein", "al\u00b7tes", "Herbst\u00b7blatt", "fliegt", "im", "A\u00b7cker", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJ