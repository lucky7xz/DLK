{"textgrid.poem.49546": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Petrarch", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine allererste Reime,", "tokens": ["Mei\u00b7ne", "al\u00b7le\u00b7rers\u00b7te", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jene bilderreiche Tr\u00e4ume,", "tokens": ["Je\u00b7ne", "bil\u00b7der\u00b7rei\u00b7che", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich, o ihr Myrtenb\u00e4ume,", "tokens": ["Wenn", "ich", ",", "o", "ihr", "Myr\u00b7ten\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Sch\u00f6nheit Busen sang:", "tokens": ["An", "der", "Sch\u00f6n\u00b7heit", "Bu\u00b7sen", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden leicht den Preis gewinnen;", "tokens": ["Wer\u00b7den", "leicht", "den", "Preis", "ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dann ein Heer von Charitinnen", "tokens": ["Dann", "ein", "Heer", "von", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN"], "meter": "+