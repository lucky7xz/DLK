{"dta.poem.19125": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CiII.  Auf der Frey-Frau von Meu\u00dfbach  \n 82tes Geburts-Fest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Den zeitigen, und jenen spahren,", "tokens": ["Den", "zei\u00b7ti\u00b7gen", ",", "und", "je\u00b7nen", "spah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist GOttes Weise, wer ist klug?", "tokens": ["Ist", "Got\u00b7tes", "Wei\u00b7se", ",", "wer", "ist", "klug", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "$,", "PWS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Weib bey vier und achtzig Jahren,", "tokens": ["Das", "Weib", "bey", "vier", "und", "acht\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die JEsum auf den Armen trug!", "tokens": ["Die", "Je\u00b7sum", "auf", "den", "Ar\u00b7men", "trug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Alte, der zur Ruhe eilte,", "tokens": ["Der", "Al\u00b7te", ",", "der", "zur", "Ru\u00b7he", "eil\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als er das Heyl der Welt umfieng!", "tokens": ["Als", "er", "das", "Heyl", "der", "Welt", "um\u00b7fi\u00b7eng", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wer einem doch Bericht ertheilte!", "tokens": ["Wer", "ei\u00b7nem", "doch", "Be\u00b7richt", "er\u00b7theil\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mir ists ein unbegreiflich Ding.", "tokens": ["Mir", "ists", "ein", "un\u00b7be\u00b7greif\u00b7lich", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df, wer seinen Heyland liebet,", "tokens": ["Ge\u00b7wi\u00df", ",", "wer", "sei\u00b7nen", "Hey\u00b7land", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und liebt zugleich sein Eigenthum;", "tokens": ["Und", "liebt", "zu\u00b7gleich", "sein", "Ei\u00b7gen\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wird erfreuet und betr\u00fcbet,", "tokens": ["Der", "wird", "er\u00b7freu\u00b7et", "und", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Christi Schande oder Ruhm.", "tokens": ["Durch", "Chris\u00b7ti", "Schan\u00b7de", "o\u00b7der", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bitte meinen Seelen-Werber:", "tokens": ["Ich", "bit\u00b7te", "mei\u00b7nen", "See\u00b7len\u00b7Wer\u00b7ber", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er wende nur die Schmach von mir,", "tokens": ["Er", "wen\u00b7de", "nur", "die", "Schmach", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dar\u00fcber ich kein Leiden h\u00e4rber,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "ich", "kein", "Lei\u00b7den", "h\u00e4r\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und keinen gr\u00f6ssern Schmertzen sp\u00fcr.", "tokens": ["Und", "kei\u00b7nen", "gr\u00f6s\u00b7sern", "Schmert\u00b7zen", "sp\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich meyne, JEsum Christum nennen,", "tokens": ["Ich", "mey\u00b7ne", ",", "Je\u00b7sum", "Chris\u00b7tum", "nen\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NE", "NE", "VVINF", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und seinem Hertzen ferne seyn;", "tokens": ["Und", "sei\u00b7nem", "Hert\u00b7zen", "fer\u00b7ne", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich selber nicht im Grunde kennen,", "tokens": ["Sich", "sel\u00b7ber", "nicht", "im", "Grun\u00b7de", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und also nicht um Gnade schreyn:", "tokens": ["Und", "al\u00b7so", "nicht", "um", "Gna\u00b7de", "schreyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil aber Fleisch und Blut commode,", "tokens": ["Weil", "a\u00b7ber", "Fleisch", "und", "Blut", "com\u00b7mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sichs nicht gerne sauer macht,", "tokens": ["Und", "sichs", "nicht", "ger\u00b7ne", "sau\u00b7er", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Christenthum auf seine Mode", "tokens": ["Ein", "Chris\u00b7ten\u00b7thum", "auf", "sei\u00b7ne", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ersehen, das die Welt erdacht.", "tokens": ["Er\u00b7se\u00b7hen", ",", "das", "die", "Welt", "er\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwey Dinge sind, die unsre Seele", "tokens": ["Zwey", "Din\u00b7ge", "sind", ",", "die", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Seeligkeit entgegen f\u00fchrn:", "tokens": ["Der", "See\u00b7lig\u00b7keit", "ent\u00b7ge\u00b7gen", "f\u00fchrn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das erste ist die Wunden-H\u00f6le", "tokens": ["Das", "ers\u00b7te", "ist", "die", "Wun\u00b7den\u00b7H\u00f6le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir uns dahinein verliern;", "tokens": ["Wenn", "wir", "uns", "da\u00b7hi\u00b7nein", "ver\u00b7li\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das andre, Christi Joch, das linde.", "tokens": ["Das", "and\u00b7re", ",", "Chris\u00b7ti", "Joch", ",", "das", "lin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NE", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das erste bringet uns zur Ruh:", "tokens": ["Das", "ers\u00b7te", "brin\u00b7get", "uns", "zur", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das andre lencket uns geschwinde", "tokens": ["Das", "and\u00b7re", "len\u00b7cket", "uns", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sicher auf die Schrancken zu.", "tokens": ["Und", "si\u00b7cher", "auf", "die", "Schran\u00b7cken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hat jemand ein verkl\u00e4rtes Auge,", "tokens": ["Hat", "je\u00b7mand", "ein", "ver\u00b7kl\u00e4r\u00b7tes", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Heyland in sein Hertz zu sehn;", "tokens": ["Dem", "Hey\u00b7land", "in", "sein", "Hertz", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wisse, da\u00df er gar nichts tauge,", "tokens": ["Der", "wis\u00b7se", ",", "da\u00df", "er", "gar", "nichts", "tau\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, da\u00df es um sein Heyl geschehn.", "tokens": ["Und", ",", "da\u00df", "es", "um", "sein", "Heyl", "ge\u00b7schehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat aber jemand Gnade funden,", "tokens": ["Hat", "a\u00b7ber", "je\u00b7mand", "Gna\u00b7de", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und will nicht in die Streiter-Bahn,", "tokens": ["Und", "will", "nicht", "in", "die", "Strei\u00b7ter\u00b7Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darinn die Zeugen \u00fcberwunden;", "tokens": ["Da\u00b7rinn", "die", "Zeu\u00b7gen", "\u00fc\u00b7berw\u00b7un\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der giebt die Gnade wieder an.", "tokens": ["Der", "giebt", "die", "Gna\u00b7de", "wie\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr! der du unsre arme Seele", "tokens": ["Herr", "!", "der", "du", "uns\u00b7re", "ar\u00b7me", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auf deinem Mutter Hertzen tr\u00e4gst,", "tokens": ["Auf", "dei\u00b7nem", "Mut\u00b7ter", "Hert\u00b7zen", "tr\u00e4gst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und an der Werckstatt ihrer H\u00f6le", "tokens": ["Und", "an", "der", "Wercks\u00b7tatt", "ih\u00b7rer", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets neue Treu vor Augen legst;", "tokens": ["Stets", "neu\u00b7e", "Treu", "vor", "Au\u00b7gen", "legst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erhalte uns nach deinem Willen,", "tokens": ["Er\u00b7hal\u00b7te", "uns", "nach", "dei\u00b7nem", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df jedes sich, du Seelen-Mann,", "tokens": ["Bi\u00df", "je\u00b7des", "sich", ",", "du", "See\u00b7len\u00b7Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PRF", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In deinen blutgen Wunden stillen,", "tokens": ["In", "dei\u00b7nen", "blut\u00b7gen", "Wun\u00b7den", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und deines Joches r\u00fchmen kan.", "tokens": ["Und", "dei\u00b7nes", "Jo\u00b7ches", "r\u00fch\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}