{"dta.poem.18661": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Liebliches .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.71", "nl:0.28"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Was doch buntes dort verbindet", "tokens": ["Was", "doch", "bun\u00b7tes", "dort", "ver\u00b7bin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJA", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir den Himmel mit der H\u00f6he?", "tokens": ["Mir", "den", "Him\u00b7mel", "mit", "der", "H\u00f6\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgennebelung verblindet", "tokens": ["Mor\u00b7gen\u00b7ne\u00b7be\u00b7lung", "ver\u00b7blin\u00b7det"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mir des Blickes scharfe Sehe.", "tokens": ["Mir", "des", "Bli\u00b7ckes", "schar\u00b7fe", "Se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind es Zelten des Vessires", "tokens": ["Sind", "es", "Zel\u00b7ten", "des", "Ves\u00b7si\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Die er lieben Frauen baute?", "tokens": ["Die", "er", "lie\u00b7ben", "Frau\u00b7en", "bau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind es Teppiche des Festes", "tokens": ["Sind", "es", "Tep\u00b7pi\u00b7che", "des", "Fes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Weil er sich der Liebsten traute?", "tokens": ["Weil", "er", "sich", "der", "Liebs\u00b7ten", "trau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Roth und weiss, gemischt, gesprenkelt", "tokens": ["Roth", "und", "weiss", ",", "ge\u00b7mischt", ",", "ge\u00b7spren\u00b7kelt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "VVFIN", "$,", "VVPP", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcsst\u2019 ich sch\u00f6nres nicht zu schauen;", "tokens": ["W\u00fcsst'", "ich", "sch\u00f6n\u00b7res", "nicht", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wie Hafis kommt dein Schiras", "tokens": ["Doch", "wie", "Ha\u00b7fis", "kommt", "dein", "Schi\u00b7ras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Nordens tr\u00fcbe Gauen?", "tokens": ["Auf", "des", "Nor\u00b7dens", "tr\u00fc\u00b7be", "Gau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja es sind die bunten Mohne,", "tokens": ["Ja", "es", "sind", "die", "bun\u00b7ten", "Moh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich nachbarlich erstrecken,", "tokens": ["Die", "sich", "nach\u00b7bar\u00b7lich", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und, dem Kriegesgott zum Hohne,", "tokens": ["Und", ",", "dem", "Krie\u00b7ges\u00b7gott", "zum", "Hoh\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felder streifweis freundlich decken.", "tokens": ["Fel\u00b7der", "streif\u00b7weis", "freund\u00b7lich", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "M\u00f6ge stets so der Gescheute", "tokens": ["M\u00f6\u00b7ge", "stets", "so", "der", "Ge\u00b7scheu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nutzend Blumenzierde pflegen,", "tokens": ["Nut\u00b7zend", "Blu\u00b7men\u00b7zier\u00b7de", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Sonnenschein, wie heute,", "tokens": ["Und", "ein", "Son\u00b7nen\u00b7schein", ",", "wie", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Kl\u00e4ren sie auf meinen Wegen!", "tokens": ["Kl\u00e4\u00b7ren", "sie", "auf", "mei\u00b7nen", "We\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}