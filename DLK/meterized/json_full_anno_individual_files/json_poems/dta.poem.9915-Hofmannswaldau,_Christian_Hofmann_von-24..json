{"dta.poem.9915": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Klugen bin ich \u00fcber alles/ narren dien\u2019 ich nicht gar viel/", "tokens": ["Klu\u00b7gen", "bin", "ich", "\u00fc\u00b7ber", "al\u00b7les", "/", "nar\u00b7ren", "dien'", "ich", "nicht", "gar", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PIS", "$(", "VVINF", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Keiner der kan mich entbehren/ der viel lern und wissen will.", "tokens": ["Kei\u00b7ner", "der", "kan", "mich", "ent\u00b7beh\u00b7ren", "/", "der", "viel", "lern", "und", "wis\u00b7sen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "VMFIN", "PRF", "VVINF", "$(", "ART", "ADV", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mich deckt eines thieres haut. Hat mein vater was verbrochen/", "tokens": ["Mich", "deckt", "ei\u00b7nes", "thie\u00b7res", "haut", ".", "Hat", "mein", "va\u00b7ter", "was", "ver\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "VAFIN", "PPOSAT", "NN", "PIS", "VVINF", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wird mit feuer es an mir/ aber nicht an ihm gerochen.", "tokens": ["Wird", "mit", "feu\u00b7er", "es", "an", "mir", "/", "a\u00b7ber", "nicht", "an", "ihm", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPER", "APPR", "PPER", "$(", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+-+-++--+-", "measure": "anapaest.di.plus"}}}}}