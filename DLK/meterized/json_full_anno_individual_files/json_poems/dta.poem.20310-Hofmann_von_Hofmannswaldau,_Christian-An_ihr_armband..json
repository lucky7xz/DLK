{"dta.poem.20310": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An ihr armband.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So offt ich dich erseh/ du h\u00f6chst begl\u00fccktes band/", "tokens": ["So", "offt", "ich", "dich", "er\u00b7seh", "/", "du", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7tes", "band", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PRF", "VVFIN", "$(", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werd ich gegen dich mit eyfer angebrannt.", "tokens": ["So", "werd", "ich", "ge\u00b7gen", "dich", "mit", "ey\u00b7fer", "an\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich ha\u00df\u2019 und neide dich nicht deines schmuckes wegen/", "tokens": ["Ich", "ha\u00df'", "und", "nei\u00b7de", "dich", "nicht", "dei\u00b7nes", "schmu\u00b7ckes", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wormit du tausenden an sch\u00f6nheit \u00fcberlegen.", "tokens": ["Wor\u00b7mit", "du", "tau\u00b7sen\u00b7den", "an", "sch\u00f6n\u00b7heit", "\u00fc\u00b7berl\u00b7e\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch nicht/ weil eine hand/ die kunst und flei\u00df erhebt/", "tokens": ["Auch", "nicht", "/", "weil", "ei\u00b7ne", "hand", "/", "die", "kunst", "und", "flei\u00df", "er\u00b7hebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$(", "KOUS", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus feinem golde dich gesticket und gewebt/", "tokens": ["Aus", "fei\u00b7nem", "gol\u00b7de", "dich", "ge\u00b7sti\u00b7cket", "und", "ge\u00b7webt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "KON", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Viel wen\u2019ger/ weil du bist mit sammet unterschlagen/", "tokens": ["Viel", "wen'\u00b7ger", "/", "weil", "du", "bist", "mit", "sam\u00b7met", "un\u00b7ter\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "$(", "KOUS", "PPER", "VAFIN", "APPR", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und t\u00e4glich/ als ein bild der freyheit/ wirst getragen.", "tokens": ["Und", "t\u00e4g\u00b7lich", "/", "als", "ein", "bild", "der", "frey\u00b7heit", "/", "wirst", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "KOUS", "ART", "NN", "ART", "NN", "$(", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein. Sondern weil du h\u00e4ltst den sch\u00f6nen arm verdeckt/", "tokens": ["Nein", ".", "Son\u00b7dern", "weil", "du", "h\u00e4ltst", "den", "sch\u00f6\u00b7nen", "arm", "ver\u00b7deckt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "An welchem Solime die liljen auffgesteckt.", "tokens": ["An", "wel\u00b7chem", "So\u00b7li\u00b7me", "die", "lil\u00b7jen", "auff\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und weil mir blo\u00df durch dich das gl\u00fccke wird entrissen/", "tokens": ["Und", "weil", "mir", "blo\u00df", "durch", "dich", "das", "gl\u00fc\u00b7cke", "wird", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "PDS", "VVFIN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den ort/ den du ber\u00fchrst/ nicht auch/ wie du/ zu k\u00fcssen.", "tokens": ["Den", "ort", "/", "den", "du", "be\u00b7r\u00fchrst", "/", "nicht", "auch", "/", "wie", "du", "/", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPER", "VVFIN", "$(", "PTKNEG", "ADV", "$(", "PWAV", "PPER", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach dieses st\u00fcrtzet mich in tieffsten kummer-stand!", "tokens": ["Ach", "die\u00b7ses", "st\u00fcrt\u00b7zet", "mich", "in", "tieffs\u00b7ten", "kum\u00b7mer\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch nein. Ich liebe dich/ du angenehmes band/", "tokens": ["Doch", "nein", ".", "Ich", "lie\u00b7be", "dich", "/", "du", "an\u00b7ge\u00b7neh\u00b7mes", "band", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "$(", "PPER", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die anmuth hat ihr bild dir selber eingegossen/", "tokens": ["Die", "an\u00b7muth", "hat", "ihr", "bild", "dir", "sel\u00b7ber", "ein\u00b7ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du tr\u00e4gst was himmlisches in deinem krey\u00df verschlossen/", "tokens": ["Du", "tr\u00e4gst", "was", "himm\u00b7li\u00b7sches", "in", "dei\u00b7nem", "krey\u00df", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJA", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du tr\u00e4gst den namen/ der schon in den sternen schwebt/", "tokens": ["Du", "tr\u00e4gst", "den", "na\u00b7men", "/", "der", "schon", "in", "den", "ster\u00b7nen", "schwebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Solime besitzt/ ich aber werde lieben/", "tokens": ["Den", "So\u00b7li\u00b7me", "be\u00b7sitzt", "/", "ich", "a\u00b7ber", "wer\u00b7de", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "PPER", "ADV", "VAFIN", "VVINF", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Und der mit haaren zwar hier in dein gold gewebt/", "tokens": ["Und", "der", "mit", "haa\u00b7ren", "zwar", "hier", "in", "dein", "gold", "ge\u00b7webt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit flammen aber ist in meine brust geschrieben.", "tokens": ["Mit", "flam\u00b7men", "a\u00b7ber", "ist", "in", "mei\u00b7ne", "brust", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}