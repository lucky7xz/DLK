{"dta.poem.6739": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er\u00f6ffnet sich der Apffel nun dem Licht;", "tokens": ["Er\u00b7\u00f6ff\u00b7net", "sich", "der", "Apf\u00b7fel", "nun", "dem", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wird von den Beschaffenheiten", "tokens": ["So", "wird", "von", "den", "Be\u00b7schaf\u00b7fen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der dreyen Feuchtigkeiten,", "tokens": ["Der", "drey\u00b7en", "Feuch\u00b7tig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die W\u00e4ssrichte, nebst der, so in der Mitten,", "tokens": ["Die", "W\u00e4ss\u00b7rich\u00b7te", ",", "nebst", "der", ",", "so", "in", "der", "Mit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und, Linsen gleich, scheint aus Crystall geschnitten,", "tokens": ["Und", ",", "Lin\u00b7sen", "gleich", ",", "scheint", "aus", "Crys\u00b7tall", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "ADV", "$,", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Auch die von Glas: zur Handlung vom Gesicht", "tokens": ["Auch", "die", "von", "Glas", ":", "zur", "Hand\u00b7lung", "vom", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "NN", "$.", "APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das mehreste verricht.", "tokens": ["Das", "meh\u00b7res\u00b7te", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da es von innen gantz durchsichtig und umringt", "tokens": ["Da", "es", "von", "in\u00b7nen", "gantz", "durch\u00b7sich\u00b7tig", "und", "um\u00b7ringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von seiner H\u00e4utgen Meng; so dringt", "tokens": ["Von", "sei\u00b7ner", "H\u00e4ut\u00b7gen", "Meng", ";", "so", "dringt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der zarten Strahlen Heer,", "tokens": ["Der", "zar\u00b7ten", "Strah\u00b7len", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das von den C\u00f6rpern r\u00fcckwerts springt,", "tokens": ["Das", "von", "den", "C\u00f6r\u00b7pern", "r\u00fcck\u00b7werts", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sich so gerad, ins Aug\u2019, da\u00df, wo das Nervgen steht,", "tokens": ["Sich", "so", "ge\u00b7rad", ",", "ins", "Aug'", ",", "da\u00df", ",", "wo", "das", "Nerv\u00b7gen", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "$,", "APPRART", "NN", "$,", "KOUS", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es alles auf ein P\u00fcnctgen geht.", "tokens": ["Es", "al\u00b7les", "auf", "ein", "P\u00fcnct\u00b7gen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Jm Grund ist jedes Rund mit einem Netz bedeckt,", "tokens": ["Jm", "Grund", "ist", "je\u00b7des", "Rund", "mit", "ei\u00b7nem", "Netz", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dahin sich jede Nerv\u2019 in kleine F\u00e4den streckt,", "tokens": ["Da\u00b7hin", "sich", "je\u00b7de", "Ner\u00b7v'", "in", "klei\u00b7ne", "F\u00e4\u00b7den", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Daselbst formiren sie, was man Retina heisst,", "tokens": ["Da\u00b7selbst", "for\u00b7mi\u00b7ren", "sie", ",", "was", "man", "Re\u00b7ti\u00b7na", "heisst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PRELS", "PIS", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "An welcher sich der Anfang weist", "tokens": ["An", "wel\u00b7cher", "sich", "der", "An\u00b7fang", "weist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von Gegenw\u00fcrffen, die vom Licht", "tokens": ["Von", "Ge\u00b7gen\u00b7w\u00fcrf\u00b7fen", ",", "die", "vom", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Erleuchtet sind, und nahe beym Gesicht.", "tokens": ["Er\u00b7leuch\u00b7tet", "sind", ",", "und", "na\u00b7he", "beym", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da w\u00fcrckt der zarte Sinn sein k\u00fcnstlich Zauber-St\u00fcck,", "tokens": ["Da", "w\u00fcrckt", "der", "zar\u00b7te", "Sinn", "sein", "k\u00fcnst\u00b7lich", "Zau\u00b7ber\u00b7St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als wie auf einen Tuch, das leer: daselbst entstehen", "tokens": ["Als", "wie", "auf", "ei\u00b7nen", "Tuch", ",", "das", "leer", ":", "da\u00b7selbst", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "$.", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und kan man jeden Augenblick", "tokens": ["Und", "kan", "man", "je\u00b7den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Viel tansend Schildereyen sehen.", "tokens": ["Viel", "tan\u00b7send", "Schil\u00b7de\u00b7re\u00b7yen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}