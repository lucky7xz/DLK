{"dta.poem.8886": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Venedig .  \n  XIX .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.85", "ca:0.14"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Als aus der Flut Palladio's Tempel stiegen,", "tokens": ["Als", "aus", "der", "Flut", "Pal\u00b7la\u00b7dio's", "Tem\u00b7pel", "stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An deren Staffeln sich die Wellen schmiegen,", "tokens": ["An", "de\u00b7ren", "Staf\u00b7feln", "sich", "die", "Wel\u00b7len", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die uns getragen ohne Falsch und T\u00fccke.", "tokens": ["Die", "uns", "ge\u00b7tra\u00b7gen", "oh\u00b7ne", "Falsch", "und", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir landen an, wir danken es dem Gl\u00fccke,", "tokens": ["Wir", "lan\u00b7den", "an", ",", "wir", "dan\u00b7ken", "es", "dem", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Lagune scheint zur\u00fcck zu fliegen,", "tokens": ["Und", "die", "La\u00b7gu\u00b7ne", "scheint", "zu\u00b7r\u00fcck", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Dogen alte S\u00e4uleng\u00e4nge liegen", "tokens": ["Der", "Do\u00b7gen", "al\u00b7te", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7ge", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor uns gigantisch mit der Seufzerbr\u00fccke.", "tokens": ["Vor", "uns", "gi\u00b7gan\u00b7tisch", "mit", "der", "Seuf\u00b7zer\u00b7br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Venedigs L\u00f6wen, sonst Venedigs Wonne,", "tokens": ["Ve\u00b7ne\u00b7digs", "L\u00f6\u00b7wen", ",", "sonst", "Ve\u00b7ne\u00b7digs", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Mit ehrnen Fl\u00fcgeln sehen wir ihn ragen", "tokens": ["Mit", "ehr\u00b7nen", "Fl\u00fc\u00b7geln", "se\u00b7hen", "wir", "ihn", "ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf seiner kolossalischen Colonne.", "tokens": ["Auf", "sei\u00b7ner", "ko\u00b7los\u00b7sa\u00b7li\u00b7schen", "Co\u00b7lon\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich steig' an's Land, nicht ohne Furcht und Zagen,", "tokens": ["Ich", "steig'", "an's", "Land", ",", "nicht", "oh\u00b7ne", "Furcht", "und", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da gl\u00e4nzt der Markusplatz im Licht der Sonne:", "tokens": ["Da", "gl\u00e4nzt", "der", "Mar\u00b7kus\u00b7platz", "im", "Licht", "der", "Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich ihn wirklich zu betreten wagen?", "tokens": ["Soll", "ich", "ihn", "wirk\u00b7lich", "zu", "be\u00b7tre\u00b7ten", "wa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}