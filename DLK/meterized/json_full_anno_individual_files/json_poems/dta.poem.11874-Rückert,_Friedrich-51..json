{"dta.poem.11874": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe doch genug des Sch\u00f6nen aller Art", "tokens": ["Ich", "ha\u00b7be", "doch", "ge\u00b7nug", "des", "Sch\u00f6\u00b7nen", "al\u00b7ler", "Art"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf dieser eiligen Vor\u00fcberfahrt gewahrt,", "tokens": ["Auf", "die\u00b7ser", "ei\u00b7li\u00b7gen", "Vor\u00b7\u00fc\u00b7berf\u00b7ahrt", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auf dieser eiligen Vor\u00fcberfahrt durchs Leben,", "tokens": ["Auf", "die\u00b7ser", "ei\u00b7li\u00b7gen", "Vor\u00b7\u00fc\u00b7berf\u00b7ahrt", "durchs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug, den Menschengeist \u00fcber die Welt zu heben;", "tokens": ["Ge\u00b7nug", ",", "den", "Men\u00b7schen\u00b7geist", "\u00fc\u00b7ber", "die", "Welt", "zu", "he\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Genug des G\u00f6ttlichen im Menschenangesicht,", "tokens": ["Ge\u00b7nug", "des", "G\u00f6tt\u00b7li\u00b7chen", "im", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Spiegel der Natur und Dichtung Zauberlicht.", "tokens": ["Im", "Spie\u00b7gel", "der", "Na\u00b7tur", "und", "Dich\u00b7tung", "Zau\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn es mehr nicht war, so war es meine Schuld;", "tokens": ["Und", "wenn", "es", "mehr", "nicht", "war", ",", "so", "war", "es", "mei\u00b7ne", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df es soviel war, ist Gottes gro\u00dfe Huld;", "tokens": ["Und", "da\u00df", "es", "so\u00b7viel", "war", ",", "ist", "Got\u00b7tes", "gro\u00b7\u00dfe", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VAFIN", "$,", "VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Stralen jener Huld, die selbst das Aug' erschlie\u00dfen,", "tokens": ["Die", "Stra\u00b7len", "je\u00b7ner", "Huld", ",", "die", "selbst", "das", "Aug'", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eigensinnig sich dem Lichte will verschlie\u00dfen;", "tokens": ["Das", "ei\u00b7gen\u00b7sin\u00b7nig", "sich", "dem", "Lich\u00b7te", "will", "ver\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Den Augendeckel r\u00fchrt der Himmelsku\u00df gelind:", "tokens": ["Den", "Au\u00b7gen\u00b7de\u00b7ckel", "r\u00fchrt", "der", "Him\u00b7mels\u00b7ku\u00df", "ge\u00b7lind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh, das ist Gottes Welt, und du bist Gottes Kind.", "tokens": ["Sieh", ",", "das", "ist", "Got\u00b7tes", "Welt", ",", "und", "du", "bist", "Got\u00b7tes", "Kind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "NN", "NN", "$,", "KON", "PPER", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}