{"textgrid.poem.63786": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Mantua", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du nach Mantua, wirst du dir vor allen", "tokens": ["Kommst", "du", "nach", "Man\u00b7tua", ",", "wirst", "du", "dir", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,", "VAFIN", "PPER", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Giulios ber\u00fchmte Freskenwelt betrachten,", "tokens": ["Giu\u00b7li\u00b7os", "be\u00b7r\u00fchm\u00b7te", "Fres\u00b7ken\u00b7welt", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Sternbilder, Bacchanal, Gigantenschlachten,", "tokens": ["Stern\u00b7bil\u00b7der", ",", "Bac\u00b7cha\u00b7nal", ",", "Gi\u00b7gan\u00b7ten\u00b7schlach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Und den Palast del T\u00e8 erstaunt durchwallen.", "tokens": ["Und", "den", "Pa\u00b7last", "del", "T\u00e8", "er\u00b7staunt", "durch\u00b7wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast du an dreister Sinnenkraft Gefallen,", "tokens": ["Hast", "du", "an", "dreis\u00b7ter", "Sin\u00b7nen\u00b7kraft", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word",