{"textgrid.poem.63944": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger und die Wachtel", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00e4ger, der mit s\u00fc\u00dfen Griffen", "tokens": ["Ein", "J\u00e4\u00b7ger", ",", "der", "mit", "s\u00fc\u00b7\u00dfen", "Grif\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wachteln lange Zeit gepfiffen,", "tokens": ["Den", "Wach\u00b7teln", "lan\u00b7ge", "Zeit", "ge\u00b7pfif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fing endlich eine. Guter Mann,", "tokens": ["Fing", "end\u00b7lich", "ei\u00b7ne", ".", "Gu\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "(hub sie vertraut zum J\u00e4ger an)", "tokens": ["(", "hub", "sie", "ver\u00b7traut", "zum", "J\u00e4\u00b7ger", "an", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df es wohl, an mir allein", "tokens": ["Ich", "wei\u00df", "es", "wohl", ",", "an", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann dir nicht viel gelegen seyn.", "tokens": ["Kann", "dir", "nicht", "viel", "ge\u00b7le\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch willst du mir das Leben schenken,", "tokens": ["Doch", "willst", "du", "mir", "das", "Le\u00b7ben", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So wirst du noch an mich gedenken!", "tokens": ["So", "wirst", "du", "noch", "an", "mich", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Du sollst durch meine seltnen Gaben", "tokens": ["Du", "sollst", "durch", "mei\u00b7ne", "selt\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Traun! Wachteln g'nug zu fangen haben!", "tokens": ["Traun", "!", "Wach\u00b7teln", "g'\u00b7nug", "zu", "fan\u00b7gen", "ha\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Ich will sie selbst in's Netz dir f\u00fchren,", "tokens": ["Ich", "will", "sie", "selbst", "in's", "Netz", "dir", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV",