{"textgrid.poem.32268": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Die Haushaltung", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zankst du schon wieder? sprach Hans Lau", "tokens": ["Zankst", "du", "schon", "wie\u00b7der", "?", "sprach", "Hans", "Lau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seiner lieben Ehefrau.", "tokens": ["Zu", "sei\u00b7ner", "lie\u00b7ben", "E\u00b7he\u00b7frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbversoffner, unversch\u00e4mter Mann\u00ab \u2013 \u2013 \u2013", "tokens": ["\u00bb", "ver\u00b7soff\u00b7ner", ",", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Mann", "\u00ab", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJA", "NN", "$(", "$(", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geduld, mein Kind, ich zieh' mich an \u2013 \u2013", "tokens": ["Ge\u00b7duld", ",", "mein", "Kind", ",", "ich", "zieh'", "mich", "an", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbwo nun schon wieder hin?\u00ab", "tokens": ["\u00bb", "wo", "nun", "schon", "wie\u00b7der", "hin", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct",