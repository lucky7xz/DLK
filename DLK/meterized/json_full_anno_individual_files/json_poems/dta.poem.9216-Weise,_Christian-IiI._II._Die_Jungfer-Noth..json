{"dta.poem.9216": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI. II.  \n Die Jungfer-Noth.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sind ich einen trost", "tokens": ["Wo", "sind", "ich", "ei\u00b7nen", "trost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In meinen hertzenleide?", "tokens": ["In", "mei\u00b7nen", "hert\u00b7zen\u00b7lei\u00b7de", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein gl\u00fcck ist doch erbost.", "tokens": ["Mein", "gl\u00fcck", "ist", "doch", "er\u00b7bost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und g\u00f6nnt mir keine freude", "tokens": ["Und", "g\u00f6nnt", "mir", "kei\u00b7ne", "freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach gebt mir einen mann/", "tokens": ["Ach", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der mich verla\u00dfnes kind ein bi\u00dfgen tr\u00f6sten kan.", "tokens": ["Der", "mich", "ver\u00b7la\u00df\u00b7nes", "kind", "ein", "bi\u00df\u00b7gen", "tr\u00f6s\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "2. Ich bin vor warten kranck/", "tokens": ["Ich", "bin", "vor", "war\u00b7ten", "kranck", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die sch\u00f6nen sommer-tage", "tokens": ["Die", "sch\u00f6\u00b7nen", "som\u00b7mer\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind mir nur gar zu lang/", "tokens": ["Sind", "mir", "nur", "gar", "zu", "lang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mehren meine plage:", "tokens": ["Und", "meh\u00b7ren", "mei\u00b7ne", "pla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gebt mir einen mann/", "tokens": ["Drum", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der mir die liebe zeit mit lust vertreiben kan.", "tokens": ["Der", "mir", "die", "lie\u00b7be", "zeit", "mit", "lust", "ver\u00b7trei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "3. Ich wei\u00df kein eintzig spiel/", "tokens": ["Ich", "wei\u00df", "kein", "eint\u00b7zig", "spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das mir belieblich w\u00e4re/", "tokens": ["Das", "mir", "be\u00b7lieb\u00b7lich", "w\u00e4\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zwar/ wann ich spielen wil/", "tokens": ["Zwar", "/", "wann", "ich", "spie\u00b7len", "wil", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So find ich kein geh\u00f6re/", "tokens": ["So", "find", "ich", "kein", "ge\u00b7h\u00f6\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gebt mir einen mann/", "tokens": ["Drum", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der mit mir aus-und ein im bette spielen kan.", "tokens": ["Der", "mit", "mir", "aus\u00b7\u00b7und", "ein", "im", "bet\u00b7te", "spie\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJD", "ART", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "4. Zur hochzeit m\u00f6cht ichs wohl", "tokens": ["Zur", "hoch\u00b7zeit", "m\u00f6cht", "ichs", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von junggesellen leiden/", "tokens": ["Von", "jung\u00b7ge\u00b7sel\u00b7len", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch weil ich immer soll", "tokens": ["Doch", "weil", "ich", "im\u00b7mer", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nur welcke r\u00fcben schneiden/", "tokens": ["Nur", "wel\u00b7cke", "r\u00fc\u00b7ben", "schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So gebt mir einen mann/", "tokens": ["So", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der auf der hochzeit mich zum tantze f\u00fchren kan.", "tokens": ["Der", "auf", "der", "hoch\u00b7zeit", "mich", "zum", "tant\u00b7ze", "f\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "5. Es ist mir nicht bewust/", "tokens": ["Es", "ist", "mir", "nicht", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich in vielen jahren", "tokens": ["Da\u00df", "ich", "in", "vie\u00b7len", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auff eine sommer-lust", "tokens": ["Auff", "ei\u00b7ne", "som\u00b7mer\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Spatzieren w\u00e4r gefahren/", "tokens": ["Spat\u00b7zie\u00b7ren", "w\u00e4r", "ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach gebt mir einen mann/", "tokens": ["Ach", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der mich zur vogel stang auf pfingsten f\u00fchren kan.", "tokens": ["Der", "mich", "zur", "vo\u00b7gel", "stang", "auf", "pfings\u00b7ten", "f\u00fch\u00b7ren", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "6. Hab ich in dieser welt", "tokens": ["Hab", "ich", "in", "die\u00b7ser", "welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nicht lauter ungel\u00fccke/", "tokens": ["Nicht", "lau\u00b7ter", "un\u00b7ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das liederliche geld/", "tokens": ["Das", "lie\u00b7der\u00b7li\u00b7che", "geld", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das w\u00e4chst mir auch nicht dicke:", "tokens": ["Das", "w\u00e4chst", "mir", "auch", "nicht", "di\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gebt mir einen mann/", "tokens": ["Drum", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem ich die pfenge fein im hosen steubern kan.", "tokens": ["Dem", "ich", "die", "pfen\u00b7ge", "fein", "im", "ho\u00b7sen", "steu\u00b7bern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "APPRART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "7. Ach/ ist er noch nicht da/", "tokens": ["Ach", "/", "ist", "er", "noch", "nicht", "da", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es liegt mir im gekr\u00f6se", "tokens": ["Es", "liegt", "mir", "im", "ge\u00b7kr\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcrwahr ein bi\u00dfgen nah/", "tokens": ["F\u00fcr\u00b7wahr", "ein", "bi\u00df\u00b7gen", "nah", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich werde gerne b\u00f6se/", "tokens": ["Ich", "wer\u00b7de", "ger\u00b7ne", "b\u00f6\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gebt mir einen mann", "tokens": ["Drum", "gebt", "mir", "ei\u00b7nen", "mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der meinen b\u00f6sen sinn mit sanfftmuth leiden kan.", "tokens": ["Der", "mei\u00b7nen", "b\u00f6\u00b7sen", "sinn", "mit", "sanfft\u00b7muth", "lei\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "8. Ich bin der jungferschafft", "tokens": ["Ich", "bin", "der", "jung\u00b7fer\u00b7schafft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von gantzem hertzen m\u00fcde/", "tokens": ["Von", "gant\u00b7zem", "hert\u00b7zen", "m\u00fc\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und meine schlechte krafft", "tokens": ["Und", "mei\u00b7ne", "schlech\u00b7te", "krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hilfft mir zu keinem friede/", "tokens": ["Hilfft", "mir", "zu", "kei\u00b7nem", "frie\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum gebt mir einen mann/", "tokens": ["Drum", "gebt", "mir", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der mich zu einer frau mit ehren machen kan.", "tokens": ["Der", "mich", "zu", "ei\u00b7ner", "frau", "mit", "eh\u00b7ren", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "APPR", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}