{"dta.poem.20016": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Die traurige Hochzeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Augsburg in dem hohen Saal", "tokens": ["Zu", "Augs\u00b7burg", "in", "dem", "ho\u00b7hen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Fugger hielt sein Hochzeitmahl.", "tokens": ["Herr", "Fug\u00b7ger", "hielt", "sein", "Hoch\u00b7zeit\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kunigunde hie\u00df die junge Braut,", "tokens": ["Ku\u00b7ni\u00b7gun\u00b7de", "hie\u00df", "die", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sa\u00df krank und bleich, gab keinen Laut.", "tokens": ["Sa\u00df", "krank", "und", "bleich", ",", "gab", "kei\u00b7nen", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zw\u00f6lf goldene Becher giengen herum,", "tokens": ["Zw\u00f6lf", "gol\u00b7de\u00b7ne", "Be\u00b7cher", "gien\u00b7gen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nichts trank Herr Fugger, so bleich und stumm.", "tokens": ["Nichts", "trank", "Herr", "Fug\u00b7ger", ",", "so", "bleich", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Zw\u00f6lf Blumenk\u00f6rbe bot man umher,", "tokens": ["Zw\u00f6lf", "Blu\u00b7men\u00b7k\u00f6r\u00b7be", "bot", "man", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Braut verlangte kein Bl\u00fcmlein mehr.", "tokens": ["Die", "Braut", "ver\u00b7lang\u00b7te", "kein", "Bl\u00fcm\u00b7lein", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Zw\u00f6lf Harfner lockten zum Fackeltanz.", "tokens": ["Zw\u00f6lf", "Harf\u00b7ner", "lock\u00b7ten", "zum", "Fa\u00b7ckel\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Fackeln gaben so matten Glanz.", "tokens": ["Die", "Fa\u00b7ckeln", "ga\u00b7ben", "so", "mat\u00b7ten", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Die G\u00e4ste tanzten in langen Reihn,", "tokens": ["Die", "G\u00e4s\u00b7te", "tanz\u00b7ten", "in", "lan\u00b7gen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwo wei\u00dfe Gestalten hintendrein.", "tokens": ["Zwo", "wei\u00b7\u00dfe", "Ge\u00b7stal\u00b7ten", "hin\u00b7ten\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die G\u00e4ste tanzten zum Saal hinaus,", "tokens": ["Die", "G\u00e4s\u00b7te", "tanz\u00b7ten", "zum", "Saal", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie tanzten und tanzten wohl aus dem Haus.", "tokens": ["Sie", "tanz\u00b7ten", "und", "tanz\u00b7ten", "wohl", "aus", "dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Saiten der Harfen sprangen zumal,", "tokens": ["Die", "Sai\u00b7ten", "der", "Har\u00b7fen", "spran\u00b7gen", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stumm schlichen die Harfner sich aus dem Saal.", "tokens": ["Stumm", "schli\u00b7chen", "die", "Harf\u00b7ner", "sich", "aus", "dem", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Im Saale vernahm man keinen Laut,", "tokens": ["Im", "Saa\u00b7le", "ver\u00b7nahm", "man", "kei\u00b7nen", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PIAT", "APPR", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Todt sa\u00dfen im Dunkel Br\u00e4ut'gam und Braut.", "tokens": ["Todt", "sa\u00b7\u00dfen", "im", "Dun\u00b7kel", "Br\u00e4ut'\u00b7gam", "und", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "NE", "KON", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}}}}