{"textgrid.poem.41891": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: So willst du denn so schnell das Werk vollenden,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So willst du denn so schnell das Werk vollenden,", "tokens": ["So", "willst", "du", "denn", "so", "schnell", "das", "Werk", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wozu die Kraft der treuen Brust dich trieb,", "tokens": ["Wo\u00b7zu", "die", "Kraft", "der", "treu\u00b7en", "Brust", "dich", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und pfl\u00fcckst so bald mit ungest\u00fcmen H\u00e4nden", "tokens": ["Und", "pfl\u00fcckst", "so", "bald", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7men", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten Schmuck, der deinem Leben blieb?", "tokens": ["Den", "letz\u00b7ten", "Schmuck", ",", "der", "dei\u00b7nem", "Le\u00b7ben", "blieb", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir bl\u00fcht das Gl\u00fcck nur noch in s\u00fc\u00dfen Tr\u00e4umen,", "tokens": ["Dir", "bl\u00fcht", "das", "Gl\u00fcck", "nur", "noch", "in", "s\u00fc\u00b7\u00dfen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und feindlich ist dir drau\u00dfen Lieb' und Welt;", "tokens": ["Und", "feind\u00b7lich", "ist", "dir", "drau\u00b7\u00dfen", "Lieb'", "und", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kein andrer Lenz wird deinem Herzen keimen,", "tokens": ["Kein", "an\u00b7drer", "Lenz", "wird", "dei\u00b7nem", "Her\u00b7zen", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn auch der T\u00e4uschung holde Bl\u00fcthe f\u00e4llt.", "tokens": ["Wenn", "auch", "der", "T\u00e4u\u00b7schung", "hol\u00b7de", "Bl\u00fc\u00b7the", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, noch einmal will ich herniedersinken,", "tokens": ["Ja", ",", "noch", "ein\u00b7mal", "will", "ich", "her\u00b7nie\u00b7der\u00b7sin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du heil'ges Meer, in deine tiefe Fluth,", "tokens": ["Du", "heil'\u00b7ges", "Meer", ",", "in", "dei\u00b7ne", "tie\u00b7fe", "Fluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will unverzagt bis auf die Neig' ihn trinken,", "tokens": ["Will", "un\u00b7ver\u00b7zagt", "bis", "auf", "die", "Neig'", "ihn", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den vollen Kelch der g\u00f6ttlich reinen Gluth;", "tokens": ["Den", "vol\u00b7len", "Kelch", "der", "g\u00f6tt\u00b7lich", "rei\u00b7nen", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Will seinen Rand mit allen Blumen kr\u00f6nen,", "tokens": ["Will", "sei\u00b7nen", "Rand", "mit", "al\u00b7len", "Blu\u00b7men", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die tausendfach mein letzter Lenz mir beut,", "tokens": ["Die", "tau\u00b7send\u00b7fach", "mein", "letz\u00b7ter", "Lenz", "mir", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mich geliebt und reich und gl\u00fccklich w\u00e4hnen", "tokens": ["Und", "mich", "ge\u00b7liebt", "und", "reich", "und", "gl\u00fcck\u00b7lich", "w\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVPP", "KON", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im raschen Traum der sel'gen Trunkenheit.", "tokens": ["Im", "ra\u00b7schen", "Traum", "der", "sel'\u00b7gen", "Trun\u00b7ken\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn dann einst, was ich geliebt im Leben,", "tokens": ["Doch", "wenn", "dann", "einst", ",", "was", "ich", "ge\u00b7liebt", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "$,", "PWS", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch meine Kraft verherrlicht und erh\u00f6ht,", "tokens": ["Durch", "mei\u00b7ne", "Kraft", "ver\u00b7herr\u00b7licht", "und", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Engeln rings und Glorien umgeben,", "tokens": ["Von", "En\u00b7geln", "rings", "und", "Glo\u00b7ri\u00b7en", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor aller Welt, ein leuchtend Vorbild, steht;", "tokens": ["Vor", "al\u00b7ler", "Welt", ",", "ein", "leuch\u00b7tend", "Vor\u00b7bild", ",", "steht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ART", "ADJD", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich getilgt des Lebens alte Schulden,", "tokens": ["Wenn", "ich", "ge\u00b7tilgt", "des", "Le\u00b7bens", "al\u00b7te", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ich der Welt auch ihre Schuld verziehn,", "tokens": ["Wenn", "ich", "der", "Welt", "auch", "ih\u00b7re", "Schuld", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bittern Ha\u00df ger\u00e4cht durch stolzes Dulden,", "tokens": ["Und", "bit\u00b7tern", "Ha\u00df", "ge\u00b7r\u00e4cht", "durch", "stol\u00b7zes", "Dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und gro\u00dfen Lohn f\u00fcr gro\u00dfen Schmerz verliehn:", "tokens": ["Und", "gro\u00b7\u00dfen", "Lohn", "f\u00fcr", "gro\u00b7\u00dfen", "Schmerz", "ver\u00b7liehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann la\u00df, o Gott, wohl darf ich k\u00fchn es fodern,", "tokens": ["Dann", "la\u00df", ",", "o", "Gott", ",", "wohl", "darf", "ich", "k\u00fchn", "es", "fo\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "$,", "FM", "NN", "$,", "ADV", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht hast du Lust an deines Kindes Schmerz,", "tokens": ["Nicht", "hast", "du", "Lust", "an", "dei\u00b7nes", "Kin\u00b7des", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Einen Blitz auf mich hernieder lodern;", "tokens": ["Nur", "Ei\u00b7nen", "Blitz", "auf", "mich", "her\u00b7nie\u00b7der", "lo\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann nimm es hin, dies tiefgekr\u00e4nkte Herz!", "tokens": ["Dann", "nimm", "es", "hin", ",", "dies", "tief\u00b7ge\u00b7kr\u00e4nk\u00b7te", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$,", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df hell und leicht auf deiner Flammenschwinge,", "tokens": ["Da\u00df", "hell", "und", "leicht", "auf", "dei\u00b7ner", "Flam\u00b7men\u00b7schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von welcher stets ein Strahl in mir gebrannt,", "tokens": ["Von", "wel\u00b7cher", "stets", "ein", "Strahl", "in", "mir", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Tod zur\u00fcck den reinen Geist dir bringe,", "tokens": ["Der", "Tod", "zu\u00b7r\u00fcck", "den", "rei\u00b7nen", "Geist", "dir", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den hier die Welt versto\u00dfen und verkannt.", "tokens": ["Den", "hier", "die", "Welt", "ver\u00b7sto\u00b7\u00dfen", "und", "ver\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So willst du denn so schnell das Werk vollenden,", "tokens": ["So", "willst", "du", "denn", "so", "schnell", "das", "Werk", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wozu die Kraft der treuen Brust dich trieb,", "tokens": ["Wo\u00b7zu", "die", "Kraft", "der", "treu\u00b7en", "Brust", "dich", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und pfl\u00fcckst so bald mit ungest\u00fcmen H\u00e4nden", "tokens": ["Und", "pfl\u00fcckst", "so", "bald", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7men", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten Schmuck, der deine