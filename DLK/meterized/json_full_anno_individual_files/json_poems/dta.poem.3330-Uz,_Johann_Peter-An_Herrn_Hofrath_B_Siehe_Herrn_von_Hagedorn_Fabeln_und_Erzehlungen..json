{"dta.poem.3330": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Zum andernmal, o Freund! gr\u00fcnt R\u00f6mhilds", "tokens": ["Zum", "an\u00b7dern\u00b7mal", ",", "o", "Freund", "!", "gr\u00fcnt", "R\u00f6m\u00b7hilds"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "PIS", "$,", "FM", "NN", "$.", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aue wieder,", "tokens": ["Au\u00b7e", "wie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Zum andernmal f\u00fcr mich! Mit rauschen-", "tokens": ["Zum", "an\u00b7dern\u00b7mal", "f\u00fcr", "mich", "!", "Mit", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADV", "APPR", "PPER", "$.", "APPR", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dem Gefieder", "tokens": ["dem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Scherzt \u00fcberall der sanfte West!", "tokens": ["Scherzt", "\u00fc\u00b7be\u00b7rall", "der", "sanf\u00b7te", "West", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Nachtigall singt ihre Lieder:", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "singt", "ih\u00b7re", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die fromme Schwalbe baut ihr Nest.", "tokens": ["Die", "from\u00b7me", "Schwal\u00b7be", "baut", "ihr", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Noch diesen Fr\u00fchling wird mein Aufenthalt hier dauern:", "tokens": ["Noch", "die\u00b7sen", "Fr\u00fch\u00b7ling", "wird", "mein", "Auf\u00b7ent\u00b7halt", "hier", "dau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich w\u00fcrde nicht untr\u00f6stlich trauern,", "tokens": ["Ich", "w\u00fcr\u00b7de", "nicht", "un\u00b7tr\u00f6st\u00b7lich", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn unter den bejahrten Mauern", "tokens": ["Wenn", "un\u00b7ter", "den", "be\u00b7jahr\u00b7ten", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mein k\u00fcnftig Nestchen aufbewahrt,", "tokens": ["Mein", "k\u00fcnf\u00b7tig", "Nest\u00b7chen", "auf\u00b7be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mir angewiesen werden sollte,", "tokens": ["Mir", "an\u00b7ge\u00b7wie\u00b7sen", "wer\u00b7den", "soll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wofern ein Vogel guter Art,", "tokens": ["Wo\u00b7fern", "ein", "Vo\u00b7gel", "gu\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Siehe Herrn von Hagedorn Fabeln und Erzehlungen. Nett, schalkhaft, h\u00fcpfend, zart,", "tokens": ["Sie\u00b7he", "Herrn", "von", "Ha\u00b7ge\u00b7dorn", "Fa\u00b7beln", "und", "Er\u00b7zeh\u00b7lun\u00b7gen", ".", "Nett", ",", "schalk\u00b7haft", ",", "h\u00fcp\u00b7fend", ",", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NE", "NN", "KON", "NN", "$.", "NN", "$,", "ADJD", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "--+-+-+-+-+-+-++-+-+", "measure": "anapaest.init"}, "line.15": {"text": "Mit mir zu Neste tragen wollte.", "tokens": ["Mit", "mir", "zu", "Nes\u00b7te", "tra\u00b7gen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}