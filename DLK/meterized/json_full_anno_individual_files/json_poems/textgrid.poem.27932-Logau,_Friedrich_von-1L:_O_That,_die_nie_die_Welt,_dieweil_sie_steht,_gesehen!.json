{"textgrid.poem.27932": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O That, die nie die Welt, dieweil sie steht, gesehen!", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O That, die nie die Welt, dieweil sie steht, gesehen!", "tokens": ["O", "That", ",", "die", "nie", "die", "Welt", ",", "die\u00b7weil", "sie", "steht", ",", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O That, dieweil die Welt wird stehn, wird nie geschehen!", "tokens": ["O", "That", ",", "die\u00b7weil", "die", "Welt", "wird", "stehn", ",", "wird", "nie", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "ART", "NN", "VAFIN", "VVINF", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O That, die Welt in Ertzt und Cedern billich schreibt,", "tokens": ["O", "That", ",", "die", "Welt", "in", "Ertzt", "und", "Ce\u00b7dern", "bil\u00b7lich", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie sie immer kan, dem Alter einverleibt!", "tokens": ["Und", "wie", "sie", "im\u00b7mer", "kan", ",", "dem", "Al\u00b7ter", "ein\u00b7ver\u00b7leibt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VMFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O That, f\u00fcr der hinfort die allerk\u00fchnsten Helden,", "tokens": ["O", "That", ",", "f\u00fcr", "der", "hin\u00b7fort", "die", "al\u00b7ler\u00b7k\u00fchns\u00b7ten", "Hel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was iemals sie gethan, sich sch\u00e4men mehr zu melden!", "tokens": ["Was", "ie\u00b7mals", "sie", "ge\u00b7than", ",", "sich", "sch\u00e4\u00b7men", "mehr", "zu", "mel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVPP", "$,", "PRF", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr der Achilles starrt, f\u00fcr der auch Hector stutzt,", "tokens": ["F\u00fcr", "der", "A\u00b7chil\u00b7les", "starrt", ",", "f\u00fcr", "der", "auch", "Hec\u00b7tor", "stutzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "$,", "APPR", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Hercules nicht mehr auff seine Keule trotzt!", "tokens": ["Und", "Her\u00b7cu\u00b7les", "nicht", "mehr", "auff", "sei\u00b7ne", "Keu\u00b7le", "trotzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "H\u00f6rt! seht! und steigt empor! macht alle L\u00f6cher weiter:", "tokens": ["H\u00f6rt", "!", "seht", "!", "und", "steigt", "em\u00b7por", "!", "macht", "al\u00b7le", "L\u00f6\u00b7cher", "wei\u00b7ter", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "$.", "KON", "VVFIN", "PTKVZ", "$.", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dort fliehen Helden her, dort lauffen dreissig Reuter,", "tokens": ["Dort", "flie\u00b7hen", "Hel\u00b7den", "her", ",", "dort", "lauf\u00b7fen", "dreis\u00b7sig", "Reu\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "CARD", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die greiffen k\u00fchnlich an ein w\u00fcstes G\u00e4rtner-Haus", "tokens": ["Die", "greif\u00b7fen", "k\u00fchn\u00b7lich", "an", "ein", "w\u00fcs\u00b7tes", "G\u00e4rt\u00b7ner\u00b7Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und schmeissen Ofen ein und schlagen Fenster au\u00df.", "tokens": ["Und", "schmeis\u00b7sen", "O\u00b7fen", "ein", "und", "schla\u00b7gen", "Fens\u00b7ter", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O That, die nie die Welt, dieweil sie steht, gesehen!", "tokens": ["O", "That", ",", "die", "nie", "die", "W