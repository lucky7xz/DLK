{"textgrid.poem.66670": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Partei, Partei \u2013 ich habe sie genommen,", "tokens": ["Par\u00b7tei", ",", "Par\u00b7tei", "\u2013", "ich", "ha\u00b7be", "sie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als grausam ", "tokens": ["Als", "grau\u00b7sam"], "token_info": ["word", "word"], "pos": ["KOUS", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "F\u00fcr die mein Glaube gl\u00fchend schon erglommen", "tokens": ["F\u00fcr", "die", "mein", "Glau\u00b7be", "gl\u00fc\u00b7hend", "schon", "er\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Tagen der Verfolgung und Gefahr.", "tokens": ["In", "Ta\u00b7gen", "der", "Ver\u00b7fol\u00b7gung", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit meines Liedes Macht bin ich gekommen", "tokens": ["Mit", "mei\u00b7nes", "Lie\u00b7des", "Macht", "bin", "ich", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Kampfe f\u00fcr der Unterdr\u00fcckten Schar", "tokens": ["Zum", "Kamp\u00b7fe", "f\u00fcr", "der", "Un\u00b7ter\u00b7dr\u00fcck\u00b7ten", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und freue mich der Funken, die gez\u00fcndet", "tokens": ["Und", "freu\u00b7e", "mich", "der", "Fun\u00b7ken", ",", "die", "ge\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Im Herzen derer, die die Not verb\u00fcndet.", "tokens": ["Im", "Her\u00b7zen", "de\u00b7rer", ",", "die", "die", "Not", "ver\u00b7b\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Partei, Partei \u2013 wer leugnet seine Seele?", "tokens": ["Par\u00b7tei", ",", "Par\u00b7tei", "\u2013", "wer", "leug\u00b7net", "sei\u00b7ne", "See\u00b7le", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, meine ganze Seele gab ich hin:", "tokens": ["Ja", ",", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "gab", "ich", "hin", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ton drang ohne Falsch mir aus der Kehle,", "tokens": ["Der", "Ton", "drang", "oh\u00b7ne", "Falsch", "mir", "aus", "der", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts ist, was ich verschleire und verhehle,", "tokens": ["Nichts", "ist", ",", "was", "ich", "ver\u00b7schlei\u00b7re", "und", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Muse ward zur Proletarierin,", "tokens": ["Die", "Mu\u00b7se", "ward", "zur", "Pro\u00b7le\u00b7ta\u00b7ri\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und rot an ihr war nicht nur die Kokarde,", "tokens": ["Und", "rot", "an", "ihr", "war", "nicht", "nur", "die", "Ko\u00b7kar\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit Haut und Haaren ward ich Freiheitsbarde.", "tokens": ["Mit", "Haut", "und", "Haa\u00b7ren", "ward", "ich", "Frei\u00b7heits\u00b7bar\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Partei, Partei \u2013 ich geh nun ganz alleine,", "tokens": ["Par\u00b7tei", ",", "Par\u00b7tei", "\u2013", "ich", "geh", "nun", "ganz", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein \u00dcberl\u00e4ufer mit Verr\u00e4terlist.", "tokens": ["Kein", "\u00dc\u00b7berl\u00b7\u00e4u\u00b7fer", "mit", "Ver\u00b7r\u00e4\u00b7ter\u00b7list", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Wicht, wer mir nachwirft Verleumdersteine", "tokens": ["Ein", "Wicht", ",", "wer", "mir", "nach\u00b7wirft", "Ver\u00b7leum\u00b7der\u00b7stei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich mit seiner Bosheit Ma\u00dfe mi\u00dft!", "tokens": ["Und", "mich", "mit", "sei\u00b7ner", "Bos\u00b7heit", "Ma\u00b7\u00dfe", "mi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin mir selbst Partei und hab die reine,", "tokens": ["Ich", "bin", "mir", "selbst", "Par\u00b7tei", "und", "hab", "die", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "NN", "KON", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Rotgoldne Flagge Poesie gehi\u00dft", "tokens": ["Rot\u00b7gold\u00b7ne", "Flag\u00b7ge", "Poe\u00b7sie", "ge\u00b7hi\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mit Silberstern \u2013 kein falsches Gut zu paschen,", "tokens": ["Mit", "Sil\u00b7bers\u00b7tern", "\u2013", "kein", "fal\u00b7sches", "Gut", "zu", "pa\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht rechts noch links, doch gottlob nicht verwaschen ...", "tokens": ["Nicht", "rechts", "noch", "links", ",", "doch", "gott\u00b7lob", "nicht", "ver\u00b7wa\u00b7schen", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADV", "$,", "ADV", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie dort der Weih, der \u00fcberm See die Kreise", "tokens": ["Wie", "dort", "der", "Weih", ",", "der", "\u00fc\u00b7berm", "See", "die", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald nah der Flut, bald hoch in L\u00fcften zieht", "tokens": ["Bald", "nah", "der", "Flut", ",", "bald", "hoch", "in", "L\u00fcf\u00b7ten", "zieht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seinen Schwingen immer neue Gleise", "tokens": ["Und", "sei\u00b7nen", "Schwin\u00b7gen", "im\u00b7mer", "neu\u00b7e", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fortbahnend jetzt in blaue Fernen flieht", "tokens": ["Fort\u00b7bah\u00b7nend", "jetzt", "in", "blau\u00b7e", "Fer\u00b7nen", "flieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jetzt in unberechenbarer Weise", "tokens": ["Und", "jetzt", "in", "un\u00b7be\u00b7re\u00b7chen\u00b7ba\u00b7rer", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weitbogig auftaucht, pl\u00f6tzlich niedersieht", "tokens": ["Weit\u00b7bo\u00b7gig", "auf\u00b7taucht", ",", "pl\u00f6tz\u00b7lich", "nie\u00b7der\u00b7sieht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und abw\u00e4rts pfeilt nach Wasser oder Beute \u2013", "tokens": ["Und", "ab\u00b7w\u00e4rts", "pfeilt", "nach", "Was\u00b7ser", "o\u00b7der", "Beu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fchl' ich mich frei vom Standpunkt kleiner Leute.", "tokens": ["F\u00fchl'", "ich", "mich", "frei", "vom", "Stand\u00b7punkt", "klei\u00b7ner", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Partei, Partei \u2013 ich habe sie genommen,", "tokens": ["Par\u00b7tei", ",", "Par\u00b7tei", "\u2013", "ich", "ha\u00b7be", "sie", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$(", "PPER", "VAFIN", "PPER",