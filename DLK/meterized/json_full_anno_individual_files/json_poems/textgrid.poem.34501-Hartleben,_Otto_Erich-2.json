"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gehe nicht auf einen andern Acker!", "tokens": ["ge\u00b7he", "nicht", "auf", "ei\u00b7nen", "an\u00b7dern", "A\u00b7cker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Siehe, wo sie schneiden auf dem Felde,", "tokens": ["Sie\u00b7he", ",", "wo", "sie", "schnei\u00b7den", "auf", "dem", "Fel\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "gehe ihnen nach, und wenn dich d\u00fcrstet,", "tokens": ["ge\u00b7he", "ih\u00b7nen", "nach", ",", "und", "wenn", "dich", "d\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "gehe hin, wo meine Knaben sch\u00f6pfen:", "tokens": ["ge\u00b7he", "hin", ",", "wo", "mei\u00b7ne", "Kna\u00b7ben", "sch\u00f6p\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "niemand soll dich kr\u00e4nken, wo ich Herr bin.", "tokens": ["nie\u00b7mand", "soll", "dich", "kr\u00e4n\u00b7ken", ",", "wo", "ich", "Herr", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Keine Fremde bist du mir: ich h\u00f6rte,", "tokens": ["Kei\u00b7ne", "Frem\u00b7de", "bist", "du", "mir", ":", "ich", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "was du Gutes thatest an der Mutter", "tokens": ["was", "du", "Gu\u00b7tes", "tha\u00b7test", "an", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "deines Mannes, die du treu begleitet", "tokens": ["dei\u00b7nes", "Man\u00b7nes", ",", "die", "du", "treu", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "in ein Land, das du zuvor nicht kanntest.", "tokens": ["in", "ein", "Land", ",", "das", "du", "zu\u00b7vor", "nicht", "kann\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "M\u00f6ge dir der Herr die That gedenken!", "tokens": ["M\u00f6\u00b7ge", "dir", "der", "Herr", "die", "That", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Von dem Gotte Israels, zu dem du", "tokens": ["Von", "dem", "Got\u00b7te", "Is\u00b7raels", ",", "zu", "dem", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "fremd gewandert, dass du Zuflucht f\u00e4ndest", "tokens": ["fremd", "ge\u00b7wan\u00b7dert", ",", "dass", "du", "Zu\u00b7flucht", "f\u00e4n\u00b7dest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "unter seinen Fl\u00fcgeln \u2013 m\u00f6ge Segen,", "tokens": ["un\u00b7ter", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "\u2013", "m\u00f6\u00b7ge", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VMFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "reicher Segen auf dich niedersinken!", "tokens": ["rei\u00b7cher", "Se\u00b7gen", "auf", "dich", "nie\u00b7der\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.23": {"line.1": {"text": "Und es sprach das Weib zu seinen F\u00fcssen:", "tokens": ["Und", "es", "sprach", "das", "Weib", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.24": {"line.1": {"text": "Lass mich f\u00fcrder Gnade bei dir finden,", "tokens": ["Lass", "mich", "f\u00fcr\u00b7der", "Gna\u00b7de", "bei", "dir", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Herr! Nun hast du deine Magd getr\u00f6stet,", "tokens": ["Herr", "!", "Nun", "hast", "du", "dei\u00b7ne", "Magd", "ge\u00b7tr\u00f6s\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "denn du hast sie freundlich angesprochen.", "tokens": ["denn", "du", "hast", "sie", "freund\u00b7lich", "an\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}