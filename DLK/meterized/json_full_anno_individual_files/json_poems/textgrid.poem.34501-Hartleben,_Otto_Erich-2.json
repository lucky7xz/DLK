{"textgrid.poem.34501": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings um Bethlehem erklang die Sichel,", "tokens": ["Rings", "um", "Beth\u00b7le\u00b7hem", "er\u00b7klang", "die", "Si\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.2": {"text": "denn es war die Zeit der Gerstenernte,", "tokens": ["denn", "es", "war", "die", "Zeit", "der", "Gers\u00b7te\u00b7nern\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "da Naemi und die Moabitin", "tokens": ["da", "Nae\u00b7mi", "und", "die", "Mo\u00b7a\u00b7bi\u00b7tin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wiederkehrten in das Land der Juden.", "tokens": ["wie\u00b7der\u00b7kehr\u00b7ten", "in", "das", "Land", "der", "Ju\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bittre Noth bedr\u00e4ngte beide Frauen,", "tokens": ["Bitt\u00b7re", "Noth", "be\u00b7dr\u00e4ng\u00b7te", "bei\u00b7de", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und Naemi weinte laut und klagte:", "tokens": ["und", "Nae\u00b7mi", "wein\u00b7te", "laut", "und", "klag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Voll und reich bin ich hinausgezogen", "tokens": ["Voll", "und", "reich", "bin", "ich", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "ehmals in die Fremde \u2013 leer und ledig", "tokens": ["eh\u00b7mals", "in", "die", "Frem\u00b7de", "\u2013", "leer", "und", "le\u00b7dig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "hat der Herr mich wieder heimgeleitet!", "tokens": ["hat", "der", "Herr", "mich", "wie\u00b7der", "heim\u00b7ge\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Da sprach Ruth, die Moabitin, tr\u00f6stend:", "tokens": ["Da", "sprach", "Ruth", ",", "die", "Mo\u00b7a\u00b7bi\u00b7tin", ",", "tr\u00f6s\u00b7tend", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "NN", "$,", "ADJD", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Lass aufs Feld mich gehn, geliebte Mutter,", "tokens": ["Lass", "aufs", "Feld", "mich", "gehn", ",", "ge\u00b7lieb\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "lass aufs Feld mich gehn und \u00c4hren lesen,", "tokens": ["lass", "aufs", "Feld", "mich", "gehn", "und", "\u00c4h\u00b7ren", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wo ich Gnade finde bei den Schnittern,", "tokens": ["wo", "ich", "Gna\u00b7de", "fin\u00b7de", "bei", "den", "Schnit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "dass ich lindre deine Noth und meine.", "tokens": ["dass", "ich", "lind\u00b7re", "dei\u00b7ne", "Noth", "und", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Und es war ein Mann mit Namen Boas,", "tokens": ["Und", "es", "war", "ein", "Mann", "mit", "Na\u00b7men", "Boas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPR", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "angesehn im Volk und wohlbeg\u00fctert;", "tokens": ["an\u00b7ge\u00b7sehn", "im", "Volk", "und", "wohl\u00b7be\u00b7g\u00fc\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "viele Knaben, viele M\u00e4gde schnitten", "tokens": ["vie\u00b7le", "Kna\u00b7ben", ",", "vie\u00b7le", "M\u00e4g\u00b7de", "schnit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "auf den reichen Feldern seine Garben,", "tokens": ["auf", "den", "rei\u00b7chen", "Fel\u00b7dern", "sei\u00b7ne", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und auf seinem Acker las die \u00c4hren", "tokens": ["und", "auf", "sei\u00b7nem", "A\u00b7cker", "las", "die", "\u00c4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ruth und beugte sich nach jedem Halme.", "tokens": ["Ruth", "und", "beug\u00b7te", "sich", "nach", "je\u00b7dem", "Hal\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch aus Betlehem, der Stadt, am Abend", "tokens": ["Doch", "aus", "Bet\u00b7le\u00b7hem", ",", "der", "Stadt", ",", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "kam hinaus auf seine Felder Boas.", "tokens": ["kam", "hin\u00b7aus", "auf", "sei\u00b7ne", "Fel\u00b7der", "Boas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und er sprach:", "tokens": ["Und", "er", "sprach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wes ist die fremde Dirne,", "tokens": ["Wes", "ist", "die", "frem\u00b7de", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "die da hingeht hinter meinen Knechten?", "tokens": ["die", "da", "hin\u00b7geht", "hin\u00b7ter", "mei\u00b7nen", "Knech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Und der Knabe, der die Schnitter f\u00fchrte,", "tokens": ["Und", "der", "Kna\u00b7be", ",", "der", "die", "Schnit\u00b7ter", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "winkte sie herbei und sprach:", "tokens": ["wink\u00b7te", "sie", "her\u00b7bei", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Dirne", "tokens": ["Die", "Dir\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "hat Naemi aus dem Land der Heiden", "tokens": ["hat", "Nae\u00b7mi", "aus", "dem", "Land", "der", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "mitgebracht: sie sammelt auf dem Felde", "tokens": ["mit\u00b7ge\u00b7bracht", ":", "sie", "sam\u00b7melt", "auf", "dem", "Fel\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "\u00c4hren, die verloren liegen bleiben,", "tokens": ["\u00c4h\u00b7ren", ",", "die", "ver\u00b7lo\u00b7ren", "lie\u00b7gen", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und sie m\u00fcht sich seit dem fr\u00fchen Morgen.", "tokens": ["und", "sie", "m\u00fcht", "sich", "seit", "dem", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Auf die Kniee sank die Moabitin.", "tokens": ["Auf", "die", "Kni\u00b7ee", "sank", "die", "Mo\u00b7a\u00b7bi\u00b7tin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Boas wandte seine milden Augen", "tokens": ["Boas", "wand\u00b7te", "sei\u00b7ne", "mil\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "auf des jungen Weibes tiefe Demuth", "tokens": ["auf", "des", "jun\u00b7gen", "Wei\u00b7bes", "tie\u00b7fe", "De\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "und erbarmte sich und sprach voll G\u00fcte:", "tokens": ["und", "er\u00b7barm\u00b7te", "sich", "und", "sprach", "voll", "G\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Gehe nicht von hinnen, meine Tochter,", "tokens": ["Ge\u00b7he", "nicht", "von", "hin\u00b7nen", ",", "mei\u00b7ne", "Toch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "gehe nicht auf einen andern Acker!", "tokens": ["ge\u00b7he", "nicht", "auf", "ei\u00b7nen", "an\u00b7dern", "A\u00b7cker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Siehe, wo sie schneiden auf dem Felde,", "tokens": ["Sie\u00b7he", ",", "wo", "sie", "schnei\u00b7den", "auf", "dem", "Fel\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "gehe ihnen nach, und wenn dich d\u00fcrstet,", "tokens": ["ge\u00b7he", "ih\u00b7nen", "nach", ",", "und", "wenn", "dich", "d\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "gehe hin, wo meine Knaben sch\u00f6pfen:", "tokens": ["ge\u00b7he", "hin", ",", "wo", "mei\u00b7ne", "Kna\u00b7ben", "sch\u00f6p\u00b7fen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "niemand soll dich kr\u00e4nken, wo ich Herr bin.", "tokens": ["nie\u00b7mand", "soll", "dich", "kr\u00e4n\u00b7ken", ",", "wo", "ich", "Herr", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Keine Fremde bist du mir: ich h\u00f6rte,", "tokens": ["Kei\u00b7ne", "Frem\u00b7de", "bist", "du", "mir", ":", "ich", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "was du Gutes thatest an der Mutter", "tokens": ["was", "du", "Gu\u00b7tes", "tha\u00b7test", "an", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "deines Mannes, die du treu begleitet", "tokens": ["dei\u00b7nes", "Man\u00b7nes", ",", "die", "du", "treu", "be\u00b7glei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "in ein Land, das du zuvor nicht kanntest.", "tokens": ["in", "ein", "Land", ",", "das", "du", "zu\u00b7vor", "nicht", "kann\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "M\u00f6ge dir der Herr die That gedenken!", "tokens": ["M\u00f6\u00b7ge", "dir", "der", "Herr", "die", "That", "ge\u00b7den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Von dem Gotte Israels, zu dem du", "tokens": ["Von", "dem", "Got\u00b7te", "Is\u00b7raels", ",", "zu", "dem", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "fremd gewandert, dass du Zuflucht f\u00e4ndest", "tokens": ["fremd", "ge\u00b7wan\u00b7dert", ",", "dass", "du", "Zu\u00b7flucht", "f\u00e4n\u00b7dest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "unter seinen Fl\u00fcgeln \u2013 m\u00f6ge Segen,", "tokens": ["un\u00b7ter", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "\u2013", "m\u00f6\u00b7ge", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "VMFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "reicher Segen auf dich niedersinken!", "tokens": ["rei\u00b7cher", "Se\u00b7gen", "auf", "dich", "nie\u00b7der\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Und es