{"dta.poem.5393": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf diesen sieht man weissen Buch-auf jenen gelben Weitzen", "tokens": ["Auf", "die\u00b7sen", "sieht", "man", "weis\u00b7sen", "Buch\u00b7auf", "je\u00b7nen", "gel\u00b7ben", "Weit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "ADJA", "NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "prangen,", "tokens": ["pran\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und \u00fcber ihnen sch\u00f6nes Laub von langen Eichen-Aesten", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7nen", "sch\u00f6\u00b7nes", "Laub", "von", "lan\u00b7gen", "Ei\u00b7chen\u00b7Aes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "hangen,", "tokens": ["han\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wodurch, noch herrlicher erhaben durch ihrer Bl\u00e4tter Dun-", "tokens": ["Wo\u00b7durch", ",", "noch", "herr\u00b7li\u00b7cher", "er\u00b7ha\u00b7ben", "durch", "ih\u00b7rer", "Bl\u00e4t\u00b7ter", "Dun"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "ADJA", "ADJD", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "ckel-Gr\u00fcn,", "tokens": ["ckel\u00b7Gr\u00fcn", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die\u00df Feld, als w\u00e4r es \u00fcbersilbert, das dort, als \u00fcberg\u00fcl-", "tokens": ["Die\u00df", "Feld", ",", "als", "w\u00e4r", "es", "\u00fc\u00b7ber\u00b7sil\u00b7bert", ",", "das", "dort", ",", "als", "\u00fc\u00b7ber\u00b7g\u00fcl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "KOKOM", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "ADV", "$,", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "det, schien.", "tokens": ["det", ",", "schien", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Ach w\u00fcrde, was auf diesem Schau-Platz f\u00fcr ein sch\u00f6n", "tokens": ["Ach", "w\u00fcr\u00b7de", ",", "was", "auf", "die\u00b7sem", "Schau\u00b7Platz", "f\u00fcr", "ein", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "$,", "PRELS", "APPR", "PDAT", "NN", "APPR", "ART", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schau-Spiel wird gespielet,", "tokens": ["Schau\u00b7Spiel", "wird", "ge\u00b7spie\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.11": {"text": "Das blos allein in unsrer Lust auf unsers Sch\u00f6pfers Ehre", "tokens": ["Das", "blos", "al\u00b7lein", "in", "uns\u00b7rer", "Lust", "auf", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "zielet,", "tokens": ["zie\u00b7let", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Und durch die wirckende Natur ohn Unterla\u00df wird aufge-", "tokens": ["Und", "durch", "die", "wir\u00b7cken\u00b7de", "Na\u00b7tur", "ohn", "Un\u00b7ter\u00b7la\u00df", "wird", "auf\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "f\u00fchrt,", "tokens": ["f\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Von uns, als Schauern, die vern\u00fcnftig, mit Lust gesehn,", "tokens": ["Von", "uns", ",", "als", "Schau\u00b7ern", ",", "die", "ver\u00b7n\u00fcnf\u00b7tig", ",", "mit", "Lust", "ge\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "NN", "$,", "PRELS", "ADJD", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "mit Danck versp\u00fchrt!", "tokens": ["mit", "Danck", "ver\u00b7sp\u00fchrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}