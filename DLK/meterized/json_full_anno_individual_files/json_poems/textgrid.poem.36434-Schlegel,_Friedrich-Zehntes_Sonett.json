{"textgrid.poem.36434": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zehntes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re fern das Pl\u00e4tschern deiner Wasser.", "tokens": ["Ich", "h\u00f6\u00b7re", "fern", "das", "Pl\u00e4t\u00b7schern", "dei\u00b7ner", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchl' mein Herz in meine Hoden sinken.", "tokens": ["Ich", "f\u00fchl'", "mein", "Herz", "in", "mei\u00b7ne", "Ho\u00b7den", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es dr\u00e4ngt mich wieder, dein Pipi zu trinken,", "tokens": ["Es", "dr\u00e4ngt", "mich", "wie\u00b7der", ",", "dein", "Pi\u00b7pi", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil ich ein ruchlos raffinierter Prasser.", "tokens": ["Weil", "ich", "ein", "ruch\u00b7los", "raf\u00b7fi\u00b7nier\u00b7ter", "Pras\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man l\u00fcgt, da\u00df deine gelben Quellen stinken.", "tokens": ["Man", "l\u00fcgt", ",", "da\u00df", "dei\u00b7ne", "gel\u00b7ben", "Quel\u00b7len", "stin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich macht ihr Duft, wenn ich sie trinke, blasser.", "tokens": ["Mich", "macht", "ihr", "Duft", ",", "wenn", "ich", "sie", "trin\u00b7ke", ",", "blas\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6cht', ein Kieselstein, ein ewig nasser,", "tokens": ["Ich", "m\u00f6cht'", ",", "ein", "Kie\u00b7sel\u00b7stein", ",", "ein", "e\u00b7wig", "nas\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "NN", "$,", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In deinen Fluten selig schimmernd blinken.", "tokens": ["In", "dei\u00b7nen", "Flu\u00b7ten", "se\u00b7lig", "schim\u00b7mernd", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wirst du mir, Geliebte, ganz zu eigen,", "tokens": ["So", "wirst", "du", "mir", ",", "Ge\u00b7lieb\u00b7te", ",", "ganz", "zu", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "NN", "$,", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie mehr als in