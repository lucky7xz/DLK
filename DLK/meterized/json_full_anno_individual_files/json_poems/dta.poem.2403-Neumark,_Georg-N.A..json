{"dta.poem.2403": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach kommet Her\u00dfliebster mein\u2019 einige ", "tokens": ["Ach", "kom\u00b7met", "Her\u00df\u00b7liebs\u00b7ter", "mein'", "ei\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "NN", "PPOSAT", "PIAT"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ach kommet ach kommet mein liebestes ", "tokens": ["Ach", "kom\u00b7met", "ach", "kom\u00b7met", "mein", "lie\u00b7bes\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Jhr meines Begehrens annehmliche Weide/", "tokens": ["Ihr", "mei\u00b7nes", "Be\u00b7geh\u00b7rens", "an\u00b7nehm\u00b7li\u00b7che", "Wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ach eylet doch zu mir und seumet euch nicht", "tokens": ["Ach", "ey\u00b7let", "doch", "zu", "mir", "und", "seu\u00b7met", "euch", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Vernehmet was euch meine Seele gewehret", "tokens": ["Ver\u00b7neh\u00b7met", "was", "euch", "mei\u00b7ne", "See\u00b7le", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Verstehet und sehet mein williges Hertz/", "tokens": ["Ver\u00b7ste\u00b7het", "und", "se\u00b7het", "mein", "wil\u00b7li\u00b7ges", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Als welches auf dieser Welt nichtes begehret", "tokens": ["Als", "wel\u00b7ches", "auf", "die\u00b7ser", "Welt", "nich\u00b7tes", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "APPR", "PDAT", "NN", "PIS", "VVFIN"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Als euch nur alleine zu lieben mit Schertz.", "tokens": ["Als", "euch", "nur", "al\u00b7lei\u00b7ne", "zu", "lie\u00b7ben", "mit", "Schertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "So lang\u2019 ich in dieser Welt habe das ", "tokens": ["So", "lang'", "ich", "in", "die\u00b7ser", "Welt", "ha\u00b7be", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "PDAT", "NN", "VAFIN", "ART"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "So lange wil ich in best\u00e4ndiger ", "tokens": ["So", "lan\u00b7ge", "wil", "ich", "in", "be\u00b7st\u00e4n\u00b7di\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Euch/ Euch mein Geliebter seyn einig ergeben", "tokens": ["Euch", "/", "Euch", "mein", "Ge\u00b7lieb\u00b7ter", "seyn", "ei\u00b7nig", "er\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PPER", "PPOSAT", "NN", "PPOSAT", "ADJD", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Und solches soll werden tag-t\u00e4glichen neu.", "tokens": ["Und", "sol\u00b7ches", "soll", "wer\u00b7den", "tag\u00b7t\u00e4g\u00b7li\u00b7chen", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Kein Trauren/ kein Schauren/ kein Neyden/", "tokens": ["Kein", "Trau\u00b7ren", "/", "kein", "Schau\u00b7ren", "/", "kein", "Ney\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "kein Leyden", "tokens": ["kein", "Ley\u00b7den"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Betr\u00fcben sol unseren ehelichen Stand/", "tokens": ["Be\u00b7tr\u00fc\u00b7ben", "sol", "un\u00b7se\u00b7ren", "e\u00b7he\u00b7li\u00b7chen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Von eurer Gunst soll mich kein Ungl\u00fck nicht", "tokens": ["Von", "eu\u00b7rer", "Gunst", "soll", "mich", "kein", "Un\u00b7gl\u00fck", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "scheiden/", "tokens": ["schei\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Seht nehmet zum ", "tokens": ["Seht", "neh\u00b7met", "zum"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "VVFIN", "APPRART"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Mund und Hand.", "tokens": ["Mund", "und", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}