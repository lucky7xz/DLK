{"dta.poem.20679": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die Prinzessinn Heinrich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcn wie der Fr\u00fchling war Dein Kleid;", "tokens": ["Gr\u00fcn", "wie", "der", "Fr\u00fch\u00b7ling", "war", "Dein", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wei\u00df, wie der Schnee zur Winterzeit,", "tokens": ["Wei\u00df", ",", "wie", "der", "Schnee", "zur", "Win\u00b7ter\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stirne, die voll Lieblichkeit", "tokens": ["Die", "Stir\u00b7ne", ",", "die", "voll", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab zu allem Volke redte.", "tokens": ["Her\u00b7ab", "zu", "al\u00b7lem", "Vol\u00b7ke", "red\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Grazien sahn neidisch nach,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "sahn", "nei\u00b7disch", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Dichtergott Apollo sprach:", "tokens": ["Der", "Dich\u00b7ter\u00b7gott", "A\u00b7pol\u00b7lo", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer sich in Dich verkleidet h\u00e4tte.", "tokens": ["Wer", "sich", "in", "Dich", "ver\u00b7klei\u00b7det", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Diana, sprach er, stieg herab,", "tokens": ["Di\u00b7a\u00b7na", ",", "sprach", "er", ",", "stieg", "her\u00b7ab", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und weil der Wald ihr keine Freuden", "tokens": ["Und", "weil", "der", "Wald", "ihr", "kei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr ihre G\u00f6tterseele gab,", "tokens": ["F\u00fcr", "ih\u00b7re", "G\u00f6t\u00b7ter\u00b7see\u00b7le", "gab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gefiel es ihr sich menschlich einzukleiden;", "tokens": ["Ge\u00b7fiel", "es", "ihr", "sich", "menschlich", "ein\u00b7zu\u00b7klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Prinzessinn! also sprach Apoll", "tokens": ["Prin\u00b7zes\u00b7sinn", "!", "al\u00b7so", "sprach", "A\u00b7poll"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Zu seinem Sohn den Kriegesdichter:", "tokens": ["Zu", "sei\u00b7nem", "Sohn", "den", "Krie\u00b7ges\u00b7dich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Indem erblicktest Du hoch auf dem Dohm zween", "tokens": ["In\u00b7dem", "er\u00b7blick\u00b7test", "Du", "hoch", "auf", "dem", "Dohm", "zween"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Lichter,", "tokens": ["Lich\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Und forschtest nach, warum? Der Dichter sprach:", "tokens": ["Und", "forschtest", "nach", ",", "wa\u00b7rum", "?", "Der", "Dich\u00b7ter", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "es soll", "tokens": ["es", "soll"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Vor Alters sich verirrt ein frommer Bischof", "tokens": ["Vor", "Al\u00b7ters", "sich", "ver\u00b7irrt", "ein", "from\u00b7mer", "Bi\u00b7schof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PRF", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "haben,", "tokens": ["ha\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Der ordnete alsdann vor k\u00fcnftige Gefahr", "tokens": ["Der", "ord\u00b7ne\u00b7te", "als\u00b7dann", "vor", "k\u00fcnf\u00b7ti\u00b7ge", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Lichter an, die nun f\u00fcnf hundert volle Jahr", "tokens": ["Die", "Lich\u00b7ter", "an", ",", "die", "nun", "f\u00fcnf", "hun\u00b7dert", "vol\u00b7le", "Jahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "CARD", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dem Wandrer in der Nacht getreu bezeichnet", "tokens": ["Dem", "Wand\u00b7rer", "in", "der", "Nacht", "ge\u00b7treu", "be\u00b7zeich\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "haben", "tokens": ["ha\u00b7ben"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Die rechte Stra\u00dfe nach der Stadt.", "tokens": ["Die", "rech\u00b7te", "Stra\u00b7\u00dfe", "nach", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Du l\u00e4cheltest und sprachst:", "tokens": ["Du", "l\u00e4\u00b7chel\u00b7test", "und", "sprachst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eder gute Bischof hat", "tokens": ["\u201e", "der", "gu\u00b7te", "Bi\u00b7schof", "hat"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "\u201edas beste Werk gethan, weil vor f\u00fcnf", "tokens": ["\u201e", "das", "bes\u00b7te", "Werk", "ge\u00b7than", ",", "weil", "vor", "f\u00fcnf"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVPP", "$,", "KOUS", "APPR", "CARD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "hundert Jahren", "tokens": ["hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u201edie Menschen, wie die Zeit, so ganz ver-", "tokens": ["\u201e", "die", "Men\u00b7schen", ",", "wie", "die", "Zeit", ",", "so", "ganz", "ver"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "finstert waren.\u201e", "tokens": ["fins\u00b7tert", "wa\u00b7ren", ".", "\u201e"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVFIN", "VAFIN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Weich feiner Witz sprach dazumal", "tokens": ["Weich", "fei\u00b7ner", "Witz", "sprach", "da\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Engelton aus Deinem Munde.", "tokens": ["Im", "En\u00b7gel\u00b7ton", "aus", "Dei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und stolzer als mein Freund bin ich auf eine", "tokens": ["Und", "stol\u00b7zer", "als", "mein", "Freund", "bin", "ich", "auf", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stunde,", "tokens": ["Stun\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "In der mich Deiner Reime Wahl ", "tokens": ["In", "der", "mich", "Dei\u00b7ner", "Rei\u00b7me", "Wahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Aufmunterte an Deiner Seite.", "tokens": ["Auf\u00b7mun\u00b7ter\u00b7te", "an", "Dei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gl\u00fccklich war die Griechinn nicht,", "tokens": ["So", "gl\u00fcck\u00b7lich", "war", "die", "Grie\u00b7chinn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sapho hie\u00df, sie sang vor keinem Angesicht,", "tokens": ["Die", "Sa\u00b7pho", "hie\u00df", ",", "sie", "sang", "vor", "kei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Deiner Hoheit Strahlen um sich streute.", "tokens": ["Das", "Dei\u00b7ner", "Ho\u00b7heit", "Strah\u00b7len", "um", "sich", "streu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}