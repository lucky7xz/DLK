{"textgrid.poem.62057": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es scheint der Mond ins Zimmer]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es scheint der Mond ins Zimmer,", "tokens": ["Es", "scheint", "der", "Mond", "ins", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sternlein strahlt in's Haus,", "tokens": ["Ein", "Stern\u00b7lein", "strahlt", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich denke nach, wie immer,", "tokens": ["Ich", "den\u00b7ke", "nach", ",", "wie", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach nicht an Saus und Braus.", "tokens": ["Ach", "nicht", "an", "Saus", "und", "Braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich denk' an all das Sch\u00f6ne,", "tokens": ["Ich", "denk'", "an", "all", "das", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gro\u00dfe Illusion,", "tokens": ["Die", "gro\u00b7\u00dfe", "Il\u00b7lu\u00b7si\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "p