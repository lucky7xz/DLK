{"dta.poem.12071": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Herz das Unruh f\u00fchlt, ist noch in sich nicht heil;", "tokens": ["Ein", "Herz", "das", "Un\u00b7ruh", "f\u00fchlt", ",", "ist", "noch", "in", "sich", "nicht", "heil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "PRF", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem bessern beigemischt ist noch ein schlechtres Theil.", "tokens": ["Dem", "bes\u00b7sern", "bei\u00b7ge\u00b7mischt", "ist", "noch", "ein", "schlecht\u00b7res", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch nicht unheilbar ist ein Herz das Unruh f\u00fchlt,", "tokens": ["Doch", "nicht", "un\u00b7heil\u00b7bar", "ist", "ein", "Herz", "das", "Un\u00b7ruh", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom schlechtern ist noch nicht das Bessre weggesp\u00fclt.", "tokens": ["Vom", "schlech\u00b7tern", "ist", "noch", "nicht", "das", "Bess\u00b7re", "weg\u00b7ge\u00b7sp\u00fclt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur wo die Unruh schweigt, da ist der Kampf entschieden,", "tokens": ["Nur", "wo", "die", "Un\u00b7ruh", "schweigt", ",", "da", "ist", "der", "Kampf", "ent\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sei es zu ew'gem Tod, sei es zu ew'gem Frieden.", "tokens": ["Sei", "es", "zu", "ew'\u00b7gem", "Tod", ",", "sei", "es", "zu", "ew'\u00b7gem", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kann ew'ger Tod auch seyn vor Gott, dem ew'gen Leben?", "tokens": ["Kann", "ew'\u00b7ger", "Tod", "auch", "seyn", "vor", "Gott", ",", "dem", "ew'\u00b7gen", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ADV", "VAINF", "APPR", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welche Verstockung kann der Gnade widerstreben?", "tokens": ["Wel\u00b7che", "Ver\u00b7sto\u00b7ckung", "kann", "der", "Gna\u00b7de", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Das kranke Herz, das ganz erstorben w\u00e4hnt zu seyn,", "tokens": ["Das", "kran\u00b7ke", "Herz", ",", "das", "ganz", "ers\u00b7tor\u00b7ben", "w\u00e4hnt", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "VVFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genesen mu\u00df auch es, durch scharfe Liebespein.", "tokens": ["Ge\u00b7ne\u00b7sen", "mu\u00df", "auch", "es", ",", "durch", "schar\u00b7fe", "Lie\u00b7be\u00b7spein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPER", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}