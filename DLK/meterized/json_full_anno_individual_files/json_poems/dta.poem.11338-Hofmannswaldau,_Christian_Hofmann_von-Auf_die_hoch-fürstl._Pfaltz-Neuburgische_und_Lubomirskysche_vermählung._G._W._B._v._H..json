{"dta.poem.11338": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die hoch-f\u00fcrstl. Pfaltz-Neuburgische  \n und Lubomirskysche verm\u00e4hlung.  \n  G. W. B. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was vor ein s\u00fcsser trieb erfrischt die matte brust,", "tokens": ["Was", "vor", "ein", "s\u00fcs\u00b7ser", "trieb", "er\u00b7frischt", "die", "mat\u00b7te", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VVFIN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der das verh\u00e4ngni\u00df stets entrissen alle lust,", "tokens": ["Der", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "stets", "ent\u00b7ris\u00b7sen", "al\u00b7le", "lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und keinen neuen tag bi\u00dfhers zugezehlet,", "tokens": ["Und", "kei\u00b7nen", "neu\u00b7en", "tag", "bi\u00df\u00b7hers", "zu\u00b7ge\u00b7zeh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der nicht mit neuer art die seele hat gequ\u00e4let?", "tokens": ["Der", "nicht", "mit", "neu\u00b7er", "art", "die", "see\u00b7le", "hat", "ge\u00b7qu\u00e4\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was stimmt ein muntres lied in meinem hertzen an,", "tokens": ["Was", "stimmt", "ein", "mun\u00b7tres", "lied", "in", "mei\u00b7nem", "hert\u00b7zen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da der verschmachte geist vor angst kaum seufftzen kan?", "tokens": ["Da", "der", "ver\u00b7schmach\u00b7te", "geist", "vor", "angst", "kaum", "seufft\u00b7zen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O fragen ohne grund! Wenn man mit freuden siehet,", "tokens": ["O", "fra\u00b7gen", "oh\u00b7ne", "grund", "!", "Wenn", "man", "mit", "freu\u00b7den", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$.", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df unser eigen gl\u00fcck in fremder wohlfarth bl\u00fchet,", "tokens": ["Da\u00df", "un\u00b7ser", "ei\u00b7gen", "gl\u00fcck", "in", "frem\u00b7der", "wohl\u00b7farth", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Kan man wohl seyn betr\u00fcbt? Nein, grosser Carolus!", "tokens": ["Kan", "man", "wohl", "seyn", "be\u00b7tr\u00fcbt", "?", "Nein", ",", "gros\u00b7ser", "Ca\u00b7ro\u00b7lus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VAINF", "VVFIN", "$.", "PTKANT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der anfang deiner lust ist meiner schmertzen schlu\u00df;", "tokens": ["Der", "an\u00b7fang", "dei\u00b7ner", "lust", "ist", "mei\u00b7ner", "schmert\u00b7zen", "schlu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja, dein vergn\u00fcgungs-glantz verg\u00fcldet meine z\u00e4hren,", "tokens": ["Ja", ",", "dein", "ver\u00b7gn\u00fc\u00b7gungs\u00b7glantz", "ver\u00b7g\u00fcl\u00b7det", "mei\u00b7ne", "z\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und machet, da\u00df sie heut in perlen sich verkehren.", "tokens": ["Und", "ma\u00b7chet", ",", "da\u00df", "sie", "heut", "in", "per\u00b7len", "sich", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "ADJA", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Durchlauchter br\u00e4utigam! Di\u00df ist der andre tag,", "tokens": ["Durch\u00b7lauch\u00b7ter", "br\u00e4u\u00b7ti\u00b7gam", "!", "Di\u00df", "ist", "der", "and\u00b7re", "tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den ich in Schlesien vor gut erkennen mag,", "tokens": ["Den", "ich", "in", "Schle\u00b7si\u00b7en", "vor", "gut", "er\u00b7ken\u00b7nen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "APPR", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit mir die gantze welt, dieweil in seinen stunden", "tokens": ["Mit", "mir", "die", "gant\u00b7ze", "welt", ",", "die\u00b7weil", "in", "sei\u00b7nen", "stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Des himmels weiser schlu\u00df zwey hertzen hat verbunden,", "tokens": ["Des", "him\u00b7mels", "wei\u00b7ser", "schlu\u00df", "zwey", "hert\u00b7zen", "hat", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So die vollkommenheit mit sinnlichem bedacht", "tokens": ["So", "die", "voll\u00b7kom\u00b7men\u00b7heit", "mit", "sinn\u00b7li\u00b7chem", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zum meister-st\u00fccke hat als meisterin gemacht,", "tokens": ["Zum", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "hat", "als", "meis\u00b7te\u00b7rin", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "KOKOM", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und stets zum muster wehlt, nachdem sie alles reisset,", "tokens": ["Und", "stets", "zum", "mus\u00b7ter", "wehlt", ",", "nach\u00b7dem", "sie", "al\u00b7les", "reis\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Was sch\u00f6n, was tugendhafft, und was vollkommen heisset,", "tokens": ["Was", "sch\u00f6n", ",", "was", "tu\u00b7gend\u00b7hafft", ",", "und", "was", "voll\u00b7kom\u00b7men", "heis\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PWS", "VVFIN", "$,", "KON", "PWS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Offt aber sehen mu\u00df, da\u00df in der mahlerey", "tokens": ["Offt", "a\u00b7ber", "se\u00b7hen", "mu\u00df", ",", "da\u00df", "in", "der", "mah\u00b7le\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gar selten farb und kunst dem muster \u00e4hnlich sey.", "tokens": ["Gar", "sel\u00b7ten", "farb", "und", "kunst", "dem", "mus\u00b7ter", "\u00e4hn\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "ADV", "ART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das lob, so vielen nur im wahn der schmeichler gr\u00fcnet,", "tokens": ["Das", "lob", ",", "so", "vie\u00b7len", "nur", "im", "wahn", "der", "schmeich\u00b7ler", "gr\u00fc\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "ADV", "APPRART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hast du, o grosser held! eh, als erlangt, verdienet:", "tokens": ["Hast", "du", ",", "o", "gros\u00b7ser", "held", "!", "eh", ",", "als", "er\u00b7langt", ",", "ver\u00b7die\u00b7net", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "ADJD", "VVFIN", "$.", "KOUS", "$,", "KOUS", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was deinen nahmen cr\u00f6nt, ist kein geborgtes gut,", "tokens": ["Was", "dei\u00b7nen", "nah\u00b7men", "cr\u00f6nt", ",", "ist", "kein", "ge\u00b7borg\u00b7tes", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "PIAT", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn seine wurtzel sprie\u00dft aus deinem helden-muth.", "tokens": ["Denn", "sei\u00b7ne", "wurt\u00b7zel", "sprie\u00dft", "aus", "dei\u00b7nem", "hel\u00b7den\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was? aus dem grossen geist, der, wo er sich hinlencket,", "tokens": ["Was", "?", "aus", "dem", "gros\u00b7sen", "geist", ",", "der", ",", "wo", "er", "sich", "hin\u00b7len\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dir ohne reich ein reich in allen hertzen schencket.", "tokens": ["Dir", "oh\u00b7ne", "reich", "ein", "reich", "in", "al\u00b7len", "hert\u00b7zen", "schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "ART", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Theresia ist sch\u00f6n; doch was von aussen gl\u00e4ntzt,", "tokens": ["The\u00b7re\u00b7sia", "ist", "sch\u00f6n", ";", "doch", "was", "von", "aus\u00b7sen", "gl\u00e4ntzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$.", "KON", "PWS", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "Hat unbegrentzter ruhm durch sittsamkeit begrentzt,", "tokens": ["Hat", "un\u00b7be\u00b7grentz\u00b7ter", "ruhm", "durch", "sitt\u00b7sam\u00b7keit", "be\u00b7grentzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und finsterni\u00df gemacht durch allzugrosse strahlen;", "tokens": ["Und", "fins\u00b7ter\u00b7ni\u00df", "ge\u00b7macht", "durch", "all\u00b7zu\u00b7gros\u00b7se", "strah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die sonne l\u00e4st sich sehn, und nicht mit farben mahlen.", "tokens": ["Die", "son\u00b7ne", "l\u00e4st", "sich", "sehn", ",", "und", "nicht", "mit", "far\u00b7ben", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,", "KON", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sch\u00f6n ist Theresia: Nichts geht an sch\u00f6nheit ihr,", "tokens": ["Sch\u00f6n", "ist", "The\u00b7re\u00b7sia", ":", "Nichts", "geht", "an", "sch\u00f6n\u00b7heit", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "$.", "PIS", "VVFIN", "APPR", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.34": {"text": "Doch ihre tugend noch weit ihrer sch\u00f6nheit f\u00fcr.", "tokens": ["Doch", "ih\u00b7re", "tu\u00b7gend", "noch", "weit", "ih\u00b7rer", "sch\u00f6n\u00b7heit", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Was in das auge f\u00e4llt, kan auch das auge tr\u00fcgen,", "tokens": ["Was", "in", "das", "au\u00b7ge", "f\u00e4llt", ",", "kan", "auch", "das", "au\u00b7ge", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und, wie ein fauler kern in frischen schaalen, liegen;", "tokens": ["Und", ",", "wie", "ein", "fau\u00b7ler", "kern", "in", "fri\u00b7schen", "schaa\u00b7len", ",", "lie\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Doch reine tugend bleibt befreyt von diesem spruch.", "tokens": ["Doch", "rei\u00b7ne", "tu\u00b7gend", "bleibt", "be\u00b7freyt", "von", "die\u00b7sem", "spruch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Denn, wie der rosen pracht mit lieblichem geruch", "tokens": ["Denn", ",", "wie", "der", "ro\u00b7sen", "pracht", "mit", "lieb\u00b7li\u00b7chem", "ge\u00b7ruch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der gantzen gegend zwar beliebte lust erwecket,", "tokens": ["Der", "gant\u00b7zen", "ge\u00b7gend", "zwar", "be\u00b7lieb\u00b7te", "lust", "er\u00b7we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Aus schamr\u00f6th\u2019 aber sich mit ihren knospen decket,", "tokens": ["Aus", "scham\u00b7r\u00f6\u00b7th'", "a\u00b7ber", "sich", "mit", "ih\u00b7ren", "knos\u00b7pen", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.41": {"text": "Und bey der demuth nur noch gr\u00f6\u00dfre zierde weist;", "tokens": ["Und", "bey", "der", "de\u00b7muth", "nur", "noch", "gr\u00f6\u00df\u00b7re", "zier\u00b7de", "weist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "So pfleget in der welt ein gro\u00df-gesinnter geist", "tokens": ["So", "pfle\u00b7get", "in", "der", "welt", "ein", "gro\u00df\u00b7ge\u00b7sinn\u00b7ter", "geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In stiller sittsamkeit sich selber zu ergetzen,", "tokens": ["In", "stil\u00b7ler", "sitt\u00b7sam\u00b7keit", "sich", "sel\u00b7ber", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn jeder sieht sein lob, nicht aber wei\u00df zu sch\u00e4tzen.", "tokens": ["Wenn", "je\u00b7der", "sieht", "sein", "lob", ",", "nicht", "a\u00b7ber", "wei\u00df", "zu", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPOSAT", "NN", "$,", "PTKNEG", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich sehe wohl, da\u00df sie voll s\u00fcsser anmuth ist:", "tokens": ["Ich", "se\u00b7he", "wohl", ",", "da\u00df", "sie", "voll", "s\u00fcs\u00b7ser", "an\u00b7muth", "ist", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ich sehe wohl, da\u00df du bewunderns-w\u00fcrdig bist;", "tokens": ["Ich", "se\u00b7he", "wohl", ",", "da\u00df", "du", "be\u00b7wun\u00b7derns\u00b7w\u00fcr\u00b7dig", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Doch ihr vollkommner geist in zarter jugend-bl\u00fcthe,", "tokens": ["Doch", "ihr", "voll\u00b7komm\u00b7ner", "geist", "in", "zar\u00b7ter", "ju\u00b7gend\u00b7bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Bey f\u00fcrstlicher geburt, dein f\u00fcrstliches gem\u00fcthe,", "tokens": ["Bey", "f\u00fcrst\u00b7li\u00b7cher", "ge\u00b7burt", ",", "dein", "f\u00fcrst\u00b7li\u00b7ches", "ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Die tugend, welche sich auf ihre sch\u00f6nheit gr\u00fcndt,", "tokens": ["Die", "tu\u00b7gend", ",", "wel\u00b7che", "sich", "auf", "ih\u00b7re", "sch\u00f6n\u00b7heit", "gr\u00fcndt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die sch\u00f6nheit, welche sich bey deiner tugend findt,", "tokens": ["Die", "sch\u00f6n\u00b7heit", ",", "wel\u00b7che", "sich", "bey", "dei\u00b7ner", "tu\u00b7gend", "findt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Und euch im nahmen nur bey andre menschen setzet,", "tokens": ["Und", "euch", "im", "nah\u00b7men", "nur", "bey", "and\u00b7re", "men\u00b7schen", "set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Jm wesen aber schon vor mehr als menschen sch\u00e4tzet,", "tokens": ["Jm", "we\u00b7sen", "a\u00b7ber", "schon", "vor", "mehr", "als", "men\u00b7schen", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "APPR", "PIAT", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.53": {"text": "Durchlauchtes liebes-paar! sind gaben, welche man", "tokens": ["Durch\u00b7lauch\u00b7tes", "lie\u00b7bes\u00b7paar", "!", "sind", "ga\u00b7ben", ",", "wel\u00b7che", "man"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "VAFIN", "VVFIN", "$,", "PRELS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Zwar sehen, aber nicht genugsam r\u00fchmen kan:", "tokens": ["Zwar", "se\u00b7hen", ",", "a\u00b7ber", "nicht", "ge\u00b7nug\u00b7sam", "r\u00fch\u00b7men", "kan", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Sind wunder, die so schwer nach w\u00fcrden zu besingen,", "tokens": ["Sind", "wun\u00b7der", ",", "die", "so", "schwer", "nach", "w\u00fcr\u00b7den", "zu", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Als in ein enges fa\u00df den Ocean, zu bringen.", "tokens": ["Als", "in", "ein", "en\u00b7ges", "fa\u00df", "den", "O\u00b7cean", ",", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "VVFIN", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.57": {"text": "Kein mensch hat solchen geist, kein geist hat solche krafft,", "tokens": ["Kein", "mensch", "hat", "sol\u00b7chen", "geist", ",", "kein", "geist", "hat", "sol\u00b7che", "krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Und keine krafft gebiehrt uns solche wissenschafft,", "tokens": ["Und", "kei\u00b7ne", "krafft", "ge\u00b7biehrt", "uns", "sol\u00b7che", "wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "So bey dem hohen glantz, in dem du stehst gezieret,", "tokens": ["So", "bey", "dem", "ho\u00b7hen", "glantz", ",", "in", "dem", "du", "stehst", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Nicht menschheit, geist und krafft, und wissenschafft verliehret;", "tokens": ["Nicht", "menschheit", ",", "geist", "und", "krafft", ",", "und", "wis\u00b7sen\u00b7schafft", "ver\u00b7lieh\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "VVPP", "KON", "NN", "$,", "KON", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.61": {"text": "Doch schliesset schon dein ruhm den allerkl\u00fcgsten mund,", "tokens": ["Doch", "schlies\u00b7set", "schon", "dein", "ruhm", "den", "al\u00b7ler\u00b7kl\u00fcgs\u00b7ten", "mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "So macht ihn doch der welt ein sittsam schweigen kund,", "tokens": ["So", "macht", "ihn", "doch", "der", "welt", "ein", "sitt\u00b7sam", "schwei\u00b7gen", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Wenn itzt das gantze land auf dich die blicke wendet,", "tokens": ["Wenn", "itzt", "das", "gant\u00b7ze", "land", "auf", "dich", "die", "bli\u00b7cke", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und in den blicken dir das gantze hertze sendet.", "tokens": ["Und", "in", "den", "bli\u00b7cken", "dir", "das", "gant\u00b7ze", "hert\u00b7ze", "sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Eh in dem w\u00fcsten nichts, das in der finstern schos", "tokens": ["Eh", "in", "dem", "w\u00fcs\u00b7ten", "nichts", ",", "das", "in", "der", "fins\u00b7tern", "schos"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "VVFIN", "PIS", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.66": {"text": "Ein wesen, das nichts war, ein nichts und alles schlos,", "tokens": ["Ein", "we\u00b7sen", ",", "das", "nichts", "war", ",", "ein", "nichts", "und", "al\u00b7les", "schlos", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "$,", "ART", "PIS", "KON", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Feur, wasser, erde, lufft, zu feur, lufft, wasser, erden,", "tokens": ["Feur", ",", "was\u00b7ser", ",", "er\u00b7de", ",", "lufft", ",", "zu", "feur", ",", "lufft", ",", "was\u00b7ser", ",", "er\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "$,", "NN", "$,", "VVFIN", "$,", "PTKA", "ADJD", "$,", "VVFIN", "$,", "PRELS", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Durch erde, wasser, feur und lufft noch muste werden,", "tokens": ["Durch", "er\u00b7de", ",", "was\u00b7ser", ",", "feur", "und", "lufft", "noch", "mus\u00b7te", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "$,", "ADJD", "KON", "NN", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ja, eh als die natur verm\u00e4hlte sich der welt,", "tokens": ["Ja", ",", "eh", "als", "die", "na\u00b7tur", "ver\u00b7m\u00e4hl\u00b7te", "sich", "der", "welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "War deine heyrath schon im himmel fest gestellt,", "tokens": ["War", "dei\u00b7ne", "hey\u00b7rath", "schon", "im", "him\u00b7mel", "fest", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Und dieser tag bestimmt, der uns so lieblich gl\u00e4ntzet,", "tokens": ["Und", "die\u00b7ser", "tag", "be\u00b7stimmt", ",", "der", "uns", "so", "lieb\u00b7lich", "gl\u00e4nt\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und dein verg\u00f6ttert hau\u00df, mit ewgen myrthen kr\u00e4ntzet.", "tokens": ["Und", "dein", "ver\u00b7g\u00f6t\u00b7tert", "hau\u00df", ",", "mit", "ew\u00b7gen", "myr\u00b7then", "kr\u00e4nt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Nun was vom himmel kommt, das wird auch himmlisch seyn;", "tokens": ["Nun", "was", "vom", "him\u00b7mel", "kommt", ",", "das", "wird", "auch", "himm\u00b7lisch", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPRART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Drum freue dich mit mir! du hoch-betr\u00fcbter Rhein!", "tokens": ["Drum", "freu\u00b7e", "dich", "mit", "mir", "!", "du", "hoch\u00b7be\u00b7tr\u00fcb\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Begl\u00fccktes Schlesien! hilff diesen tag besingen,", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Schle\u00b7si\u00b7en", "!", "hilff", "die\u00b7sen", "tag", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Der uns bald s\u00fc\u00dfigkeit wird von dem starcken bringen.", "tokens": ["Der", "uns", "bald", "s\u00fc\u00b7\u00dfig\u00b7keit", "wird", "von", "dem", "star\u00b7cken", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Willkommen! sch\u00f6ner tag! sey tausendmahl gegr\u00fc\u00dft!", "tokens": ["Will\u00b7kom\u00b7men", "!", "sch\u00f6\u00b7ner", "tag", "!", "sey", "tau\u00b7send\u00b7mahl", "ge\u00b7gr\u00fc\u00dft", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Weil du viel werther mir als tausend tage bist,", "tokens": ["Weil", "du", "viel", "wert\u00b7her", "mir", "als", "tau\u00b7send", "ta\u00b7ge", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "PPER", "KOKOM", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Die nur das zeiten-buch auf schwachen bl\u00e4ttern tr\u00e4get,", "tokens": ["Die", "nur", "das", "zei\u00b7ten\u00b7buch", "auf", "schwa\u00b7chen", "bl\u00e4t\u00b7tern", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Wenn treue liebe dich tieff in die seelen pr\u00e4get.", "tokens": ["Wenn", "treu\u00b7e", "lie\u00b7be", "dich", "tieff", "in", "die", "see\u00b7len", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Du solt das gegen-gifft hinfort von meiner pein,", "tokens": ["Du", "solt", "das", "ge\u00b7gen\u00b7gifft", "hin\u00b7fort", "von", "mei\u00b7ner", "pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Du solt der jahre schlu\u00df, und auch ihr anfang seyn!", "tokens": ["Du", "solt", "der", "jah\u00b7re", "schlu\u00df", ",", "und", "auch", "ihr", "an\u00b7fang", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKVZ", "$,", "KON", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Und welckt mein lorbeer-zweig schon bey den creutz-cypressen;", "tokens": ["Und", "welckt", "mein", "lor\u00b7beer\u00b7zweig", "schon", "bey", "den", "creutz\u00b7cy\u00b7pres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "So schw\u00f6r ich, da\u00df ich doch dich nimmer will vergessen:", "tokens": ["So", "schw\u00f6r", "ich", ",", "da\u00df", "ich", "doch", "dich", "nim\u00b7mer", "will", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "Ja diese meine brust, die keinen seufftzer kennt,", "tokens": ["Ja", "die\u00b7se", "mei\u00b7ne", "brust", ",", "die", "kei\u00b7nen", "seufft\u00b7zer", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.86": {"text": "Der nicht in danckbarkeit vor meinem hertzog brennt,", "tokens": ["Der", "nicht", "in", "dan\u00b7ck\u00b7bar\u00b7keit", "vor", "mei\u00b7nem", "hert\u00b7zog", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADV", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.87": {"text": "Soll ihre w\u00fcnsche stets mit diesem wunsch anheben:", "tokens": ["Soll", "ih\u00b7re", "w\u00fcn\u00b7sche", "stets", "mit", "die\u00b7sem", "wunsch", "an\u00b7he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "ADV", "APPR", "PDAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Es lebe Carolus! Theresia soll leben!", "tokens": ["Es", "le\u00b7be", "Ca\u00b7ro\u00b7lus", "!", "The\u00b7re\u00b7sia", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$.", "NE", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}