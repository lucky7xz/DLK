{"textgrid.poem.40802": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbans Meer, gleich diesem, baut die Kerker alle!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbans Meer, gleich diesem, baut die Kerker alle!", "tokens": ["\u00bb", "ans", "Meer", ",", "gleich", "die\u00b7sem", ",", "baut", "die", "Ker\u00b7ker", "al\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "$,", "ADV", "PDAT", "$,", "VVFIN", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ringsum nur Meer, endloser Himmel dr\u00fcber!", "tokens": ["Ring\u00b7sum", "nur", "Meer", ",", "end\u00b7lo\u00b7ser", "Him\u00b7mel", "dr\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$,", "ADJA", "NN", "PAV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Setzt eures Sklaven enge, dunkle Halle", "tokens": ["Setzt", "eu\u00b7res", "Skla\u00b7ven", "en\u00b7ge", ",", "dunk\u00b7le", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit und Unendlichkeit gen\u00fcber!", "tokens": ["Der", "Frei\u00b7heit", "und", "Un\u00b7end\u00b7lich\u00b7keit", "ge\u00b7n\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df, wenn er schuldig, selbst der Wellen Kosen", "tokens": ["Da\u00df", ",", "wenn", "er", "schul\u00b7dig", ",", "selbst", "der", "Wel\u00b7len", "Ko\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm Nachts und Tags von seiner Schuld erz\u00e4hle,", "tokens": ["Ihm", "Nachts", "und", "Tags", "von", "sei\u00b7ner", "Schuld", "er\u00b7z\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fort und fort ihm laut der Brandung Tosen", "tokens": ["Und", "fort", "und", "fort", "ihm", "laut", "der", "Bran\u00b7dung", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Herrn Gerichte donnre in die Seele!", "tokens": ["Des", "Herrn", "Ge\u00b7rich\u00b7te", "donn\u00b7re", "in", "die", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df, wenn er schuldlos, nicht ans Ohr euch dringe,", "tokens": ["Da\u00df", ",", "wenn", "er", "schuld\u00b7los", ",", "nicht", "ans", "Ohr", "euch", "drin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADJD", "$,", "PTKNEG", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch nicht den Schlummer st\u00f6re seine Kla