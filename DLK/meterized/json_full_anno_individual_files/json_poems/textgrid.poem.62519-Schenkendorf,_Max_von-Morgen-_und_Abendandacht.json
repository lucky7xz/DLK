{"textgrid.poem.62519": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen- und Abendandacht", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgenstern und Abendstern \u2013", "tokens": ["Mor\u00b7gens\u00b7tern", "und", "A\u00b7bends\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+---+--", "measure": "dactylic.init"}, "line.2": {"text": "Die der Herr uns angez\u00fcndet,", "tokens": ["Die", "der", "Herr", "uns", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der als Helfer nah und fern", "tokens": ["Der", "als", "Hel\u00b7fer", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich verhei\u00dfen und verk\u00fcndet;", "tokens": ["Sich", "ver\u00b7hei\u00b7\u00dfen", "und", "ver\u00b7k\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer und an jedem Ort", "tokens": ["Im\u00b7mer", "und", "an", "je\u00b7dem", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh' ich ihn sich niederneigen,", "tokens": ["Seh'", "ich", "ihn", "sich", "nie\u00b7der\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PRF", "VVINF", "$,"], 