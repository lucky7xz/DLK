{"textgrid.poem.63429": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Wenn n\u00fcmms dat nich s\u00fcht", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Din Moder ehr Dirn un min Vader sin S\u0153hn,", "tokens": ["Din", "Mo\u00b7der", "ehr", "Dirn", "un", "min", "Va\u00b7der", "sin", "S\u0153hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "wu lang heww' sick de nich all kennt!", "tokens": ["wu", "lang", "hew\u00b7w'", "sick", "de", "nich", "all", "kennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "FM", "FM", "FM", "PTKNEG", "PIAT", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Se lat' nich vunanner, se lat' nich vuneen,", "tokens": ["Se", "lat'", "nich", "vu\u00b7nan\u00b7ner", ",", "se", "lat'", "nich", "vu\u00b7ne\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ADJA", "$,", "FM", "FM", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "so heww' se tosamen sick wennt.", "tokens": ["so", "hew\u00b7w'", "se", "to\u00b7sa\u00b7men", "sick", "wennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "De G\u00f6ren de platschten tohop in de P\u00fctt,", "tokens": ["De", "G\u00f6\u00b7ren", "de", "platschten", "to\u00b7hop", "in", "de", "P\u0