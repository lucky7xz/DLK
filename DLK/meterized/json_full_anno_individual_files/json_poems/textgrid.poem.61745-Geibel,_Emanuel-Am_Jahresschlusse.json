nfo": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da des jungen Reichs Geb\u00e4ude", "tokens": ["Da", "des", "jun\u00b7gen", "Reichs", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelan vollendet steigt,", "tokens": ["Him\u00b7me\u00b7lan", "voll\u00b7en\u00b7det", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ein Geist der Eintracht drinnen", "tokens": ["Da", "ein", "Geist", "der", "Ein\u00b7tracht", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie am Pfingstfest niederz\u00fcckt", "tokens": ["Wie", "am", "Pfingst\u00b7fest", "nie\u00b7der\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Kaisers Hand die Zinnen", "tokens": ["Und", "des", "Kai\u00b7sers", "Hand", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Kranz der Freiheit schm\u00fcckt!", "tokens": ["Mit", "dem", "Kranz", "der", "Frei\u00b7heit", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}