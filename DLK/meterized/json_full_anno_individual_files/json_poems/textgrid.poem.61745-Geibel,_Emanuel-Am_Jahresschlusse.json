{"textgrid.poem.61745": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Am Jahresschlusse", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du endlich allverst\u00e4ndlich,", "tokens": ["Hast", "du", "end\u00b7lich", "all\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schicksal, deinen Spruch getan,", "tokens": ["Schick\u00b7sal", ",", "dei\u00b7nen", "Spruch", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Fr\u00fchlingsbrausen endlich", "tokens": ["Und", "wie", "Fr\u00fch\u00b7lings\u00b7brau\u00b7sen", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weht's das deutsche Leben an?", "tokens": ["Weht's", "das", "deut\u00b7sche", "Le\u00b7ben", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, der Bannfluch ist gebrochen,", "tokens": ["Ja", ",", "der", "Bann\u00b7fluch", "ist", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der beklemmend auf uns lag,", "tokens": ["Der", "be\u00b7klem\u00b7mend", "auf", "uns", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und befreit mit Herzenspochen", "tokens": ["Und", "be\u00b7freit", "mit", "Her\u00b7zen\u00b7spo\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gr\u00fc\u00dfen wir den jungen Tag.", "tokens": ["Gr\u00fc\u00b7\u00dfen", "wir", "den", "jun\u00b7gen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo an B\u00f6hmens wald'gen Borden", "tokens": ["Wo", "an", "B\u00f6h\u00b7mens", "wald'\u00b7gen", "Bor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siebenmal die Schlacht getobt,", "tokens": ["Sie\u00b7ben\u00b7mal", "die", "Schlacht", "ge\u00b7tobt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Hat der schwarze Aar vom Norden", "tokens": ["Hat", "der", "schwar\u00b7ze", "Aar", "vom", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Schwingen Kraft erprobt;", "tokens": ["Sei\u00b7ner", "Schwin\u00b7gen", "Kraft", "er\u00b7probt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Staub von ihr getr\u00fcmmert", "tokens": ["In", "den", "Staub", "von", "ihr", "ge\u00b7tr\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sank die Fessel, die so lang", "tokens": ["Sank", "die", "Fes\u00b7sel", ",", "die", "so", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jeden Hoffnungstraum verk\u00fcmmert,", "tokens": ["Je\u00b7den", "Hoff\u00b7nungs\u00b7traum", "ver\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der aus deutscher Seele sprang.", "tokens": ["Der", "aus", "deut\u00b7scher", "See\u00b7le", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch, wie stolz im Feld der Waffen", "tokens": ["Doch", ",", "wie", "stolz", "im", "Feld", "der", "Waf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Euer Wurf, ihr Sieger, fiel,", "tokens": ["Eu\u00b7er", "Wurf", ",", "ihr", "Sie\u00b7ger", ",", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Halb erst steht das Werk geschaffen,", "tokens": ["Halb", "erst", "steht", "das", "Werk", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsrer Sehnsucht hohes Ziel.", "tokens": ["Uns\u00b7rer", "Sehn\u00b7sucht", "ho\u00b7hes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andern Grund noch gilt's zu legen", "tokens": ["An\u00b7dern", "Grund", "noch", "gilt's", "zu", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als des Schwertes freudlos Recht;", "tokens": ["Als", "des", "Schwer\u00b7tes", "freud\u00b7los", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur in freier Liebe Segen", "tokens": ["Nur", "in", "frei\u00b7er", "Lie\u00b7be", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kn\u00fcpft Geschlecht sich an Geschlecht.", "tokens": ["Kn\u00fcpft", "Ge\u00b7schlecht", "sich", "an", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wallt denn, eurer Lorbeerzweige", "tokens": ["Wallt", "denn", ",", "eu\u00b7rer", "Lor\u00b7beer\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrdig, unsrem Volk voran!", "tokens": ["W\u00fcr\u00b7dig", ",", "uns\u00b7rem", "Volk", "vo\u00b7ran", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder eitle Hader schweige,", "tokens": ["Je\u00b7der", "eit\u00b7le", "Ha\u00b7der", "schwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Hohn sei abgetan!", "tokens": ["Je\u00b7der", "Hohn", "sei", "ab\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeigt, wie sch\u00f6n dem Heldenmute", "tokens": ["Zeigt", ",", "wie", "sch\u00f6n", "dem", "Hel\u00b7den\u00b7mu\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weisheit sich und G\u00fcte paart,", "tokens": ["Weis\u00b7heit", "sich", "und", "G\u00fc\u00b7te", "paart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und am stammverwandten Blute", "tokens": ["Und", "am", "stamm\u00b7ver\u00b7wand\u00b7ten", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ehrt des Geistes Eigenart!", "tokens": ["Ehrt", "des", "Geis\u00b7tes", "Ei\u00b7gen\u00b7art", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber ihr, die dieser Zeiten", "tokens": ["A\u00b7ber", "ihr", ",", "die", "die\u00b7ser", "Zei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sturm gebeugt, erhebt das Herz!", "tokens": ["Sturm", "ge\u00b7beugt", ",", "er\u00b7hebt", "das", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcnftig Heil will sich bereiten,", "tokens": ["K\u00fcnf\u00b7tig", "Heil", "will", "sich", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Wandlung nur ist Schmerz.", "tokens": ["Und", "die", "Wand\u00b7lung", "nur", "ist", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Brach auch Teures euch zusammen,", "tokens": ["Brach", "auch", "Teu\u00b7res", "euch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lernt aufs Ganze gl\u00e4ubig sehn!", "tokens": ["Lernt", "aufs", "Gan\u00b7ze", "gl\u00e4u\u00b7big", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lodernd mu\u00df der Holzsto\u00df flammen,", "tokens": ["Lo\u00b7dernd", "mu\u00df", "der", "Holz\u00b7sto\u00df", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll der Ph\u00f6nix auferstehn.", "tokens": ["Soll", "der", "Ph\u00f6\u00b7nix", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum getrost! Und schw\u00f6rt in treuer", "tokens": ["Drum", "ge\u00b7trost", "!", "Und", "schw\u00f6rt", "in", "treu\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVPP", "$.", "KON", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kraft zum gro\u00dfen Vaterland,", "tokens": ["Kraft", "zum", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des heil'gen Opfers Feuer", "tokens": ["Und", "des", "heil'\u00b7gen", "Op\u00b7fers", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Sch\u00fcrt es selbst mit frommer Hand!", "tokens": ["Sch\u00fcrt", "es", "selbst", "mit", "from\u00b7mer", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werft der Eifersucht Gedanken,", "tokens": ["Werft", "der", "Ei\u00b7fer\u00b7sucht", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werft den alten Groll hinein!", "tokens": ["Werft", "den", "al\u00b7ten", "Groll", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Brausend auch die letzten Schranken", "tokens": ["Brau\u00b7send", "auch", "die", "letz\u00b7ten", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sp\u00fclt hinunter dann der Main.", "tokens": ["Sp\u00fclt", "hin\u00b7un\u00b7ter", "dann", "der", "Main", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wann kommst du, Tag der Freude", "tokens": ["O", "wann", "kommst", "du", ",", "Tag", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PWAV", "VVFIN", "PPER", "$,", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den mein ahnend Herz mir zeigt,", "tokens": ["Den", "mein", "ah\u00b7nend", "Herz", "mir", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da des jungen Reichs Geb\u00e4ude", "tokens": ["Da", "des", "jun\u00b7gen", "Reichs", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmelan vollendet steigt,", "tokens": ["Him\u00b7me\u00b7lan", "voll\u00b7en\u00b7det", "steigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ein Geist der Eintracht drinnen", "tokens": ["Da", "ein", "Geist", "der", "Ein\u00b7tracht", "drin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie am Pfingstfest niederz\u00fcckt", "tokens": ["Wie", "am", "Pfingst\u00b7fest", "nie\u00b7der\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des Kaisers Hand die Zinnen", "tokens": ["Und", "des", "Kai\u00b7sers", "Hand", "die", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Kranz der Freiheit schm\u00fcckt!", "tokens": ["Mit", "dem", "Kranz", "der", "Frei\u00b7heit", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hast du endlich allverst\u00e4ndlich,", "tokens": ["Hast", "du", "end\u00b7lich", "all\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schicksal, deinen Spruch getan,", "tokens": ["Schick\u00b7sal", ",", "dei\u00b7nen", "Spruch", "ge\u00b7tan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie Fr\u00fchlingsbrausen endlich", "tokens": ["Und", "wie", "Fr\u00fch\u00b7lings\u00b7brau\u00b7sen", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"},