{"dta.poem.8632": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Vor K\u00e4lte ist die Luft erstarrt&#8218;", "tokens": ["Vor", "K\u00e4l\u00b7te", "ist", "die", "Luft", "er\u00b7starrt", "&#8218;"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kracht der Schnee von meinen Tritten,", "tokens": ["Es", "kracht", "der", "Schnee", "von", "mei\u00b7nen", "Trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es dampft mein Hauch, es klirrt mein Bart;", "tokens": ["Es", "dampft", "mein", "Hauch", ",", "es", "klirrt", "mein", "Bart", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur fort, nur immer fort geschritten!", "tokens": ["Nur", "fort", ",", "nur", "im\u00b7mer", "fort", "ge\u00b7schrit\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie feierlich die Gegend schweigt!", "tokens": ["Wie", "fei\u00b7er\u00b7lich", "die", "Ge\u00b7gend", "schweigt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond bescheint die alten Fichten,", "tokens": ["Der", "Mond", "be\u00b7scheint", "die", "al\u00b7ten", "Fich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die, sehnsuchtsvoll zum Tod geneigt\u201a", "tokens": ["Die", ",", "sehn\u00b7suchts\u00b7voll", "zum", "Tod", "ge\u00b7neigt", "\u201a"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Zweig zur\u00fcck zur Erde richten.", "tokens": ["Den", "Zweig", "zu\u00b7r\u00fcck", "zur", "Er\u00b7de", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Frost! friere mir in's Herz hinein!", "tokens": ["Frost", "!", "frie\u00b7re", "mir", "in's", "Herz", "hin\u00b7ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in das hei\u00dfbewegte, wilde!", "tokens": ["Tief", "in", "das", "hei\u00df\u00b7be\u00b7weg\u00b7te", ",", "wil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df einmal Ruh mag drinnen seyn\u201a", "tokens": ["Da\u00df", "ein\u00b7mal", "Ruh", "mag", "drin\u00b7nen", "seyn", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VMFIN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie hier im n\u00e4chtlichen Gefilde!", "tokens": ["Wie", "hier", "im", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7fil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}