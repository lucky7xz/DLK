 {"text": "Man gibt das Beste, das man kann,", "tokens": ["Man", "gibt", "das", "Bes\u00b7te", ",", "das", "man", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "man gibt sein gl\u00fchendstes Herzblut dran,", "tokens": ["man", "gibt", "sein", "gl\u00fc\u00b7hends\u00b7tes", "Herz\u00b7blut", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "mit Leben bezahltes Leben ...", "tokens": ["mit", "Le\u00b7ben", "be\u00b7zahl\u00b7tes", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und hat man etwas fertig gebracht ...", "tokens": ["und", "hat", "man", "et\u00b7was", "fer\u00b7tig", "ge\u00b7bracht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "dann ist der ganze Dank daf\u00fcr,", "tokens": ["dann", "ist", "der", "gan\u00b7ze", "Dank", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "da\u00df ein paar Freunde freundlich sagen:", "tokens": ["da\u00df", "ein", "paar", "Freun\u00b7de", "freund\u00b7lich", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "lin