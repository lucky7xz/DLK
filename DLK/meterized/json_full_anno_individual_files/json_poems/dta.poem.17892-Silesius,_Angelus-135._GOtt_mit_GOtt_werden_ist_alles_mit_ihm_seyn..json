{"dta.poem.17892": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "135.  GOtt mit GOtt werden ist alles mit  \n ihm seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOtt mit GOtt gewird/ ist mit ihm eine Freud", "tokens": ["Wer", "Gott", "mit", "Gott", "ge\u00b7wird", "/", "ist", "mit", "ihm", "ei\u00b7ne", "Freud"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "APPR", "NN", "VAPP", "$(", "VAFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Ewge Majest\u00e4t/ ein Reich und Herrligkeit.", "tokens": ["Ein", "Ew\u00b7ge", "Ma\u00b7jes\u00b7t\u00e4t", "/", "ein", "Reich", "und", "Herr\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}