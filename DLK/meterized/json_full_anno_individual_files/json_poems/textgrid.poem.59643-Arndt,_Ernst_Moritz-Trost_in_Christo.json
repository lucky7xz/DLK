{"textgrid.poem.59643": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Trost in Christo", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin des Lebens m\u00fcde,", "tokens": ["Ich", "bin", "des", "Le\u00b7bens", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der eitlen Eitelkeit,", "tokens": ["Der", "eit\u00b7len", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O komm, du Gottesfriede,", "tokens": ["O", "komm", ",", "du", "Got\u00b7tes\u00b7frie\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm mich aus dem Streit,", "tokens": ["Und", "nimm", "mich", "aus", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm mich in deine Ruh',", "tokens": ["Nimm", "mich", "in", "dei\u00b7ne", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In deine stillen Freuden,", "tokens": ["In", "dei\u00b7ne", "stil\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schlie\u00dfe bittern Leiden", "tokens": ["Und", "schlie\u00b7\u00dfe", "bit\u00b7tern", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Wahns Erinnrung zu.", "tokens": ["Des", "Wahns", "E\u00b7rinn\u00b7rung", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zuviel hab' ich geduldet,", "tokens": ["Zu\u00b7viel", "hab'", "ich", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gek\u00e4mpfet \u00fcberlang,", "tokens": ["Ge\u00b7k\u00e4mp\u00b7fet", "\u00fc\u00b7berl\u00b7ang", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ges\u00fcndigt und verschuldet,", "tokens": ["Ge\u00b7s\u00fcn\u00b7digt", "und", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum ist mir weh und bang;", "tokens": ["Drum", "ist", "mir", "weh", "und", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df nicht aus noch ein", "tokens": ["Ich", "wei\u00df", "nicht", "aus", "noch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf diesen biestern Stra\u00dfen,", "tokens": ["Auf", "die\u00b7sen", "bies\u00b7tern", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich w\u00e4re gar verlassen,", "tokens": ["Ich", "w\u00e4\u00b7re", "gar", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00e4r' Jesus Christ nicht mein.", "tokens": ["W\u00e4r'", "Je\u00b7sus", "Christ", "nicht", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich w\u00e4re l\u00e4ngst vergangen,", "tokens": ["Ich", "w\u00e4\u00b7re", "l\u00e4ngst", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r' Jesus Christ nicht mein,", "tokens": ["W\u00e4r'", "Je\u00b7sus", "Christ", "nicht", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "PTKNEG", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Zittern und in Bangen,", "tokens": ["In", "Zit\u00b7tern", "und", "in", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In S\u00fcndenangst und Pein,", "tokens": ["In", "S\u00fcn\u00b7de\u00b7nangst", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In tiefer Seelennot,", "tokens": ["In", "tie\u00b7fer", "See\u00b7len\u00b7not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4r' er, das Licht der Frommen,", "tokens": ["W\u00e4r'", "er", ",", "das", "Licht", "der", "From\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vom Himmel nicht gekommen,", "tokens": ["Vom", "Him\u00b7mel", "nicht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Zukunft Morgenrot.", "tokens": ["Der", "Zu\u00b7kunft", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du s\u00fc\u00dfer Jesu Christe,", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Je\u00b7su", "Chris\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So freundlich und so hold!", "tokens": ["So", "freund\u00b7lich", "und", "so", "hold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! Wenn doch jeder w\u00fc\u00dfte,", "tokens": ["Ach", "!", "Wenn", "doch", "je\u00b7der", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was deine Huld gewollt,", "tokens": ["Was", "dei\u00b7ne", "Huld", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", 