{"dta.poem.13082": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enur einer H\u00fctte Bau verschafft mir gr\u00f6ssern Ruhm,", "tokens": ["\u201e", "nur", "ei\u00b7ner", "H\u00fct\u00b7te", "Bau", "ver\u00b7schafft", "mir", "gr\u00f6s\u00b7sern", "Ruhm", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eals aller K\u00fcnste Macht, Werth, Recht und Eigenthum.", "tokens": ["\u201e", "als", "al\u00b7ler", "K\u00fcns\u00b7te", "Macht", ",", "Werth", ",", "Recht", "und", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Saal ist mein Beweis. Man hat vor tausend Jahren,", "tokens": ["\u201e", "der", "Saal", "ist", "mein", "Be\u00b7weis", ".", "Man", "hat", "vor", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PIS", "VAFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eauf diesem Plaz, wo nichts als W\u00fcsteneyen waren,", "tokens": ["\u201e", "auf", "die\u00b7sem", "Plaz", ",", "wo", "nichts", "als", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", "wa\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "$,", "PWAV", "PIS", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eein schlechtes Jagt-Geb\u00e4u von Erd\u2019 und Holz gebaut,", "tokens": ["\u201e", "ein", "schlech\u00b7tes", "Jag\u00b7tGe\u00b7b\u00e4u", "von", "Erd'", "und", "Holz", "ge\u00b7baut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "130\u201dBey dem es selber fast der Einsamkeit gegraut.", "tokens": ["\"", "Bey", "dem", "es", "sel\u00b7ber", "fast", "der", "Ein\u00b7sam\u00b7keit", "ge\u00b7graut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eman dachte damahls nicht auf Zepter und auf Kronen;", "tokens": ["\u201e", "man", "dach\u00b7te", "da\u00b7mahls", "nicht", "auf", "Zep\u00b7ter", "und", "auf", "Kro\u00b7nen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "ADV", "PTKNEG", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201enichts als ein J\u00e4ger mu\u00dft in dieser H\u00fctte wohnen.", "tokens": ["\u201e", "nichts", "als", "ein", "J\u00e4\u00b7ger", "mu\u00dft", "in", "die\u00b7ser", "H\u00fct\u00b7te", "woh\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "ART", "NN", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edie\u00df ist der erste Stein, den ich zum Grund gelegt,", "tokens": ["\u201e", "die\u00df", "ist", "der", "ers\u00b7te", "Stein", ",", "den", "ich", "zum", "Grund", "ge\u00b7legt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eder jezt der ganzen Welt vornehmste Wohnung tr\u00e4gt.", "tokens": ["\u201e", "der", "jezt", "der", "gan\u00b7zen", "Welt", "vor\u00b7nehms\u00b7te", "Woh\u00b7nung", "tr\u00e4gt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "135\u201dIch sage, diesen Saal, in dem sich mancher neiget,", "tokens": ["\"", "Ich", "sa\u00b7ge", ",", "die\u00b7sen", "Saal", ",", "in", "dem", "sich", "man\u00b7cher", "nei\u00b7get", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PDAT", "NN", "$,", "APPR", "PRELS", "PRF", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eder sonst mit seinem Stolz bis an die Wolcken steiget.", "tokens": ["\u201e", "der", "sonst", "mit", "sei\u00b7nem", "Stolz", "bis", "an", "die", "Wol\u00b7cken", "stei\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "APPR", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201esagt wer vermag so viel als das, was ich beginn?", "tokens": ["\u201e", "sagt", "wer", "ver\u00b7mag", "so", "viel", "als", "das", ",", "was", "ich", "be\u00b7ginn", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PWS", "VVFIN", "ADV", "PIAT", "KOKOM", "PDS", "$,", "PWS", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eso bleibet mir gewi\u00df das Vorrecht zum Gewinn.", "tokens": ["\u201e", "so", "blei\u00b7bet", "mir", "ge\u00b7wi\u00df", "das", "Vor\u00b7recht", "zum", "Ge\u00b7winn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ekann ich ein \u00f6des Dach zu Kron-Pal\u00e4sten machen,", "tokens": ["\u201e", "kann", "ich", "ein", "\u00f6\u00b7des", "Dach", "zu", "Kron\u00b7Pa\u00b7l\u00e4s\u00b7ten", "ma\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "140\u201dWas kann ich nicht, wo man von K\u00f6niglichen Sachen,", "tokens": ["\"", "Was", "kann", "ich", "nicht", ",", "wo", "man", "von", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Sa\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "PTKNEG", "$,", "PWAV", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201evon eines F\u00fcrstens Ruhm, von Helden-Thaten spricht,", "tokens": ["\u201e", "von", "ei\u00b7nes", "F\u00fcrs\u00b7tens", "Ruhm", ",", "von", "Hel\u00b7den\u00b7Tha\u00b7ten", "spricht", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "NN", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201ef\u00fcr die man ein Geb\u00e4u zu f\u00fchren sich verpflicht.", "tokens": ["\u201e", "f\u00fcr", "die", "man", "ein", "Ge\u00b7b\u00e4u", "zu", "f\u00fch\u00b7ren", "sich", "ver\u00b7pflicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PIS", "ART", "NN", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}