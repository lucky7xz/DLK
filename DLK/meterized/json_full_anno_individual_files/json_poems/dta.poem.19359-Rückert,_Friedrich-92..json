{"dta.poem.19359": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer keinen Willen hat, kann \u00fcberhaupt nichts wollen,", "tokens": ["Wer", "kei\u00b7nen", "Wil\u00b7len", "hat", ",", "kann", "\u00fc\u00b7ber\u00b7haupt", "nichts", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VAFIN", "$,", "VMFIN", "ADV", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch also dieses nicht, da\u00df wir ihn achten sollen.", "tokens": ["Auch", "al\u00b7so", "die\u00b7ses", "nicht", ",", "da\u00df", "wir", "ihn", "ach\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du achtest in dem Kind, das keinen Willen hat,", "tokens": ["Du", "ach\u00b7test", "in", "dem", "Kind", ",", "das", "kei\u00b7nen", "Wil\u00b7len", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den k\u00fcnftigen, den du erziehst mit Zucht und Rath.", "tokens": ["Den", "k\u00fcnf\u00b7ti\u00b7gen", ",", "den", "du", "er\u00b7ziehst", "mit", "Zucht", "und", "Rath", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Wahnsinn achtest du und im Verbrechen was?", "tokens": ["Im", "Wahn\u00b7sinn", "ach\u00b7test", "du", "und", "im", "Ver\u00b7bre\u00b7chen", "was", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "APPRART", "NN", "PWS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Willen, der sich dort verga\u00df, hier sich verma\u00df.", "tokens": ["Den", "Wil\u00b7len", ",", "der", "sich", "dort", "ver\u00b7ga\u00df", ",", "hier", "sich", "ver\u00b7ma\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "F\u00fcr den, der sich verga\u00df, hast du die Pflicht zu denken,", "tokens": ["F\u00fcr", "den", ",", "der", "sich", "ver\u00b7ga\u00df", ",", "hast", "du", "die", "Pflicht", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den, der sich verma\u00df, rechtm\u00e4\u00dfig zu beschr\u00e4nken.", "tokens": ["Und", "den", ",", "der", "sich", "ver\u00b7ma\u00df", ",", "recht\u00b7m\u00e4\u00b7\u00dfig", "zu", "be\u00b7schr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PRF", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dem Kranken unterlegst du deine Willensweise,", "tokens": ["Dem", "Kran\u00b7ken", "un\u00b7ter\u00b7legst", "du", "dei\u00b7ne", "Wil\u00b7lens\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wehrest, die er will, ihm die verbotne Speise.", "tokens": ["Und", "weh\u00b7rest", ",", "die", "er", "will", ",", "ihm", "die", "ver\u00b7bot\u00b7ne", "Spei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Schwachen sind mit Recht dem Starken unterthan,", "tokens": ["Die", "Schwa\u00b7chen", "sind", "mit", "Recht", "dem", "Star\u00b7ken", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der das f\u00fcr sie, was sie nicht k\u00f6nnen, wollen kan.", "tokens": ["Der", "das", "f\u00fcr", "sie", ",", "was", "sie", "nicht", "k\u00f6n\u00b7nen", ",", "wol\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$,", "VMFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}