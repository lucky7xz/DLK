{"dta.poem.17559": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "181.  Das allergeitzigste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Geitzig ist ein Hertz! wenn tausend Welten w\u00e4ren/", "tokens": ["Wie", "Geit\u00b7zig", "ist", "ein", "Hertz", "!", "wenn", "tau\u00b7send", "Wel\u00b7ten", "w\u00e4\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "KOUS", "CARD", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es w\u00fcrde sie gesambt/ und mehr darzu begehren.", "tokens": ["Es", "w\u00fcr\u00b7de", "sie", "ge\u00b7sambt", "/", "und", "mehr", "dar\u00b7zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$(", "KON", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}