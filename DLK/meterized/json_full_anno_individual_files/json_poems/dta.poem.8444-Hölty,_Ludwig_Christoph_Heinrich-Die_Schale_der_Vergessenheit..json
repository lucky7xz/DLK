{"dta.poem.8444": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Schale der Vergessenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Eine Schale des Stroms, welcher Vergessenheit", "tokens": ["Ei\u00b7ne", "Scha\u00b7le", "des", "Stroms", ",", "wel\u00b7cher", "Ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAT", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Durch Elisiums Blumen rollt,", "tokens": ["Durch", "E\u00b7lis\u00b7i\u00b7ums", "Blu\u00b7men", "rollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bring, o Genius, bring deinem Verschmachtenden!", "tokens": ["Bring", ",", "o", "Ge\u00b7nius", ",", "bring", "dei\u00b7nem", "Ver\u00b7schmach\u00b7ten\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dort, wo Faons die S\u00e4ngerin,", "tokens": ["Dort", ",", "wo", "Faons", "die", "S\u00e4n\u00b7ge\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort, wo Orfeus vergass seiner Euridice,", "tokens": ["Dort", ",", "wo", "Or\u00b7feus", "ver\u00b7gass", "sei\u00b7ner", "Eu\u00b7ri\u00b7di\u00b7ce", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Sch\u00f6pf den silbernen Schlummerquell!", "tokens": ["Sch\u00f6pf", "den", "sil\u00b7ber\u00b7nen", "Schlum\u00b7mer\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Ha! dann tauch' ich dein Bild, spr\u00f6de Gebieterin,", "tokens": ["Ha", "!", "dann", "tauch'", "ich", "dein", "Bild", ",", "spr\u00f6\u00b7de", "Ge\u00b7bie\u00b7te\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.8": {"text": "Und die l\u00e4chelnden Lippen voll", "tokens": ["Und", "die", "l\u00e4\u00b7cheln\u00b7den", "Lip\u00b7pen", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Lautenklanges, des Haars schattige Wallungen,", "tokens": ["Lau\u00b7ten\u00b7klan\u00b7ges", ",", "des", "Haars", "schat\u00b7ti\u00b7ge", "Wal\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Und das Beben der weissen Brust,", "tokens": ["Und", "das", "Be\u00b7ben", "der", "weis\u00b7sen", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "Und den siegenden Blick, der mir im Marke ", "tokens": ["Und", "den", "sie\u00b7gen\u00b7den", "Blick", ",", "der", "mir", "im", "Mar\u00b7ke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Tauch' ich tief in den Schlummerquell!", "tokens": ["Tauch'", "ich", "tief", "in", "den", "Schlum\u00b7mer\u00b7quell", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}