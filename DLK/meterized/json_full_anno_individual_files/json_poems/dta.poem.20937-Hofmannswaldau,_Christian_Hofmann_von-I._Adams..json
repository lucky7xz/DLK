{"dta.poem.20937": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Adams.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "An statt der Mutterscho\u00df war mir des H\u00f6chsten", "tokens": ["An", "statt", "der", "Mut\u00b7ter\u00b7scho\u00df", "war", "mir", "des", "H\u00f6chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hand/", "tokens": ["Hand", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das Paradis mein Hau\u00df/ die weite Welt mein Land.", "tokens": ["Das", "Pa\u00b7ra\u00b7dis", "mein", "Hau\u00df", "/", "die", "wei\u00b7te", "Welt", "mein", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Straffe weil mir di\u00df zu enge scheinte seyn/", "tokens": ["Zur", "Straf\u00b7fe", "weil", "mir", "di\u00df", "zu", "en\u00b7ge", "schein\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "PPER", "PDS", "APPR", "ADJA", "VVFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schleust dieser schlechte Raum den stoltzen C\u00f6rper", "tokens": ["Schleust", "die\u00b7ser", "schlech\u00b7te", "Raum", "den", "stolt\u00b7zen", "C\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ein.", "tokens": ["ein", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}}}}}