{"textgrid.poem.36251": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der schwarze Mantel will sich dichter falten,", "tokens": ["Der", "schwar\u00b7ze", "Man\u00b7tel", "will", "sich", "dich\u00b7ter", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die freundlichen Gespr\u00e4che sind verschollen;", "tokens": ["Die", "freund\u00b7li\u00b7chen", "Ge\u00b7spr\u00e4\u00b7che", "sind", "ver\u00b7schol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo allen Wesen tief Gesang entquollen,", "tokens": ["Wo", "al\u00b7len", "We\u00b7sen", "tief", "Ge\u00b7sang", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da mu\u00df die stumme Einsamkeit nun walten.", "tokens": ["Da", "mu\u00df", "die", "stum\u00b7me", "Ein\u00b7sam\u00b7keit", "nun", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es darf den gro\u00dfen Flug das Herz entfalten,", "tokens": ["Es", "darf", "den", "gro\u00b7\u00dfen", "Flug", "das", "Herz", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Fantasie nicht mehr der T\u00e4uschung zollen;", "tokens": ["Und", "Fan\u00b7ta\u00b7sie", "nicht", "mehr", "der", "T\u00e4u\u00b7schung", "zol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was farbig prangt, mu\u00df bald ins Dunkel rollen,", "tokens": ["Was", "far\u00b7big", "prangt", ",", "mu\u00df", "bald", "ins", "Dun\u00b7kel", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur unsichtbares Licht kann nie veralten.", "tokens": ["Nur", "un\u00b7sicht\u00b7ba\u00b7res", "Licht", "kann", "nie", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Willkommen, heil'ge Nacht, in deinen Schauern!", "tokens": ["Will\u00b7kom\u00b7men", ",", "heil'\u00b7ge", "Nacht", ",", "in", "dei\u00b7nen", "Schau\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es strahlt in dir des Lichtes Licht dem Frommen,", "tokens": ["Es", "strahlt", "in", "dir", "des", "Lich\u00b7tes", "Licht", "dem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "w