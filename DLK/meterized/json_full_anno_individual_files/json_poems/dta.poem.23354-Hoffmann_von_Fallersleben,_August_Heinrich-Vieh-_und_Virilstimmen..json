{"dta.poem.23354": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vieh- und Virilstimmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ochse br\u00fcllet, es grunzt das Schwein,", "tokens": ["Der", "O\u00b7chse", "br\u00fcl\u00b7let", ",", "es", "grunzt", "das", "Schwein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Schafe bl\u00e4ken, die Fr\u00f6sche schrei'n \u2014", "tokens": ["Die", "Scha\u00b7fe", "bl\u00e4\u00b7ken", ",", "die", "Fr\u00f6\u00b7sche", "schrei'n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob sch\u00f6n das lautet? wird wohl keiner fragen;", "tokens": ["Ob", "sch\u00f6n", "das", "lau\u00b7tet", "?", "wird", "wohl", "kei\u00b7ner", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PDS", "VVFIN", "$.", "VAFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was l\u00e4\u00dft sich auch von Bestiensprache sagen?", "tokens": ["Was", "l\u00e4\u00dft", "sich", "auch", "von", "Be\u00b7sti\u00b7ens\u00b7pra\u00b7che", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch br\u00fcllt kein Ochs' und es grunzt kein Schwein,", "tokens": ["Doch", "br\u00fcllt", "kein", "Ochs'", "und", "es", "grunzt", "kein", "Schwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch Schafe bl\u00e4ken und Fr\u00f6sche schrei'n", "tokens": ["Noch", "Scha\u00b7fe", "bl\u00e4\u00b7ken", "und", "Fr\u00f6\u00b7sche", "schrei'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVINF", "KON", "NN", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So unterth\u00e4nigst, j\u00e4mmerlichst wehm\u00fcthigst", "tokens": ["So", "un\u00b7ter\u00b7th\u00e4\u00b7nigst", ",", "j\u00e4m\u00b7mer\u00b7lichst", "weh\u00b7m\u00fct\u00b7higst"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als deutsche Unterthanen tiefst dem\u00fcthigst.", "tokens": ["Als", "deut\u00b7sche", "Un\u00b7ter\u00b7tha\u00b7nen", "tiefst", "de\u00b7m\u00fct\u00b7higst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}