{"textgrid.poem.63659": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "In dunkler Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In dunkler Nacht zuweilen fahr' ich j\u00e4h empor", "tokens": ["In", "dunk\u00b7ler", "Nacht", "zu\u00b7wei\u00b7len", "fahr'", "ich", "j\u00e4h", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus tiefem Schlaf, und auf den leisen Atemzug", "tokens": ["Aus", "tie\u00b7fem", "Schlaf", ",", "und", "auf", "den", "lei\u00b7sen", "A\u00b7tem\u00b7zug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Liebsten horchend, lieg' ich eine Weile still", "tokens": ["Der", "Liebs\u00b7ten", "hor\u00b7chend", ",", "lieg'", "ich", "ei\u00b7ne", "Wei\u00b7le", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und starre bangend in die leere Finsternis,", "tokens": ["Und", "star\u00b7re", "ban\u00b7gend", "in", "die", "lee\u00b7re", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo hinterm Schirme d\u00e4mmernd nur ein Lichtschein glimmt.", "tokens": ["Wo", "hin\u00b7term", "Schir\u00b7me", "d\u00e4m\u00b7mernd", "nur", "ein", "Licht\u00b7schein", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Laut des Lebens dringt herauf vom Garten her,", "tokens": ["Kein", "Laut", "des", "Le\u00b7bens", "dringt", "her\u00b7auf", "vom", "Gar\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Herz nur pocht mir in der Brust. Und stiller noch", "tokens": ["Das", "Herz", "nur", "pocht", "mir", "in", "der", "Brust", ".", "Und", "stil\u00b7ler", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und stummer wird's, als s\u00e4nke rings in Todesschlaf", "tokens": ["Und", "stum\u00b7mer", "wird's", ",", "als", "s\u00e4n\u00b7ke", "rings", "in", "To\u00b7des\u00b7schlaf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOUS", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die weite Welt. Und pl\u00f6tzlich ist's, als st\u00e4nde still", "tokens": ["Die", "wei\u00b7te", "Welt", ".", "Und", "pl\u00f6tz\u00b7lich", "ist's", ",", "als", "st\u00e4n\u00b7de", "still"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "ADJD", "VAFIN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der sanfte Atem neben mir \u2013 mit Knistern lischt", "tokens": ["Der", "sanf\u00b7te", "A\u00b7tem", "ne\u00b7ben", "mir", "\u2013", "mit", "Knis\u00b7tern", "lischt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$(", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Fl\u00e4mmchen aus, und l\u00e4hmend \u00fcberschauert mich", "tokens": ["Das", "Fl\u00e4mm\u00b7chen", "aus", ",", "und", "l\u00e4h\u00b7mend", "\u00fc\u00b7bersc\u00b7hau\u00b7ert", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein eisig Grauen.", "tokens": ["Ein", "ei\u00b7sig", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ew'ge M\u00e4chte, k\u00e4m' es je,", "tokens": ["Ew'\u00b7ge", "M\u00e4ch\u00b7te", ",", "k\u00e4m'", "es", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df dieses Lebens Flamme, dran das meine sich", "tokens": ["Da\u00df", "die\u00b7ses", "Le\u00b7bens", "Flam\u00b7me", ",", "dran", "das", "mei\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "$,", "PAV", "ART", "PPOSAT", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Belebt und w\u00e4rmt, des besten Weibes Liebeshauch", "tokens": ["Be\u00b7lebt", "und", "w\u00e4rmt", ",", "des", "bes\u00b7ten", "Wei\u00b7bes", "Lie\u00b7bes\u00b7hauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verlodert' und verweht' im rauhen Todessturm", "tokens": ["Ver\u00b7lo\u00b7dert'", "und", "ver\u00b7weht'", "im", "rau\u00b7hen", "To\u00b7des\u00b7sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ich \u2013 o Gott! \u2013 in ungeheurer Einsamkeit", "tokens": ["Und", "ich", "\u2013", "o", "Gott", "!", "\u2013", "in", "un\u00b7ge\u00b7heu\u00b7rer", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "FM", "NN", "$.", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zur\u00fcckgeblieben \u2013 wie \u2013 wie sollt' ich das bestehn", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "\u2013", "wie", "\u2013", "wie", "sollt'", "ich", "das", "be\u00b7stehn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$(", "PWAV", "$(", "PWAV", "VMFIN", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und nicht ihr nach hintaumeln in die ew'ge Nacht!", "tokens": ["Und", "nicht", "ihr", "nach", "hin\u00b7tau\u00b7meln", "in", "die", "ew'\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "APPR", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist nicht, was Holdes je auf meinem Pfad gebl\u00fcht,", "tokens": ["Ist", "nicht", ",", "was", "Hol\u00b7des", "je", "auf", "mei\u00b7nem", "Pfad", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "PWS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nur an der Sonne dieses Auges aufgespro\u00dft,", "tokens": ["Nur", "an", "der", "Son\u00b7ne", "die\u00b7ses", "Au\u00b7ges", "auf\u00b7ge\u00b7spro\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und wenn sie auslischt, f\u00e4nd' ich meine Stra\u00dfe noch", "tokens": ["Und", "wenn", "sie", "aus\u00b7lischt", ",", "f\u00e4nd'", "ich", "mei\u00b7ne", "Stra\u00b7\u00dfe", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "In winterlicher W\u00fcste? Ew'ge Machte, nein,", "tokens": ["In", "win\u00b7ter\u00b7li\u00b7cher", "W\u00fcs\u00b7te", "?", "Ew'\u00b7ge", "Mach\u00b7te", ",", "nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADJA", "NN", "$,", "PTKANT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Nur das, nur das nicht! Tut mir euer \u00c4rgstes an,", "tokens": ["Nur", "das", ",", "nur", "das", "nicht", "!", "Tut", "mir", "eu\u00b7er", "\u00c4rgs\u00b7tes", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "$,", "ADV", "PDS", "PTKNEG", "$.", "NN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "La\u00dft mich erblinden, la\u00dft an Kraft und Lebensmut", "tokens": ["La\u00dft", "mich", "er\u00b7blin\u00b7den", ",", "la\u00dft", "an", "Kraft", "und", "Le\u00b7bens\u00b7mut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mich ganz verarmen, keiner Freude m\u00e4chtig mehr \u2013", "tokens": ["Mich", "ganz", "ver\u00b7ar\u00b7men", ",", "kei\u00b7ner", "Freu\u00b7de", "m\u00e4ch\u00b7tig", "mehr", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "PIAT", "NN", "ADJD", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nur das, nur das nicht! Schon bei dem Gedanken tritt", "tokens": ["Nur", "das", ",", "nur", "das", "nicht", "!", "Schon", "bei", "dem", "Ge\u00b7dan\u00b7ken", "tritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "$,", "ADV", "PDS", "PTKNEG", "$.", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Der kalte Schwei\u00df mir auf die Stirn, im Busen klopft", "tokens": ["Der", "kal\u00b7te", "Schwei\u00df", "mir", "auf", "die", "Stirn", ",", "im", "Bu\u00b7sen", "klopft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Herz, wie ihn zu sprengen, mir am Gaumen klebt", "tokens": ["Das", "Herz", ",", "wie", "ihn", "zu", "spren\u00b7gen", ",", "mir", "am", "Gau\u00b7men", "klebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "$,", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Fiebernd die Zunge \u2013 auf vom Lager heb' ich mich,", "tokens": ["Fie\u00b7bernd", "die", "Zun\u00b7ge", "\u2013", "auf", "vom", "La\u00b7ger", "heb'", "ich", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$(", "APPR", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ein Licht zu z\u00fcnden \u2013 da an meiner Seite t\u00f6nt", "tokens": ["Ein", "Licht", "zu", "z\u00fcn\u00b7den", "\u2013", "da", "an", "mei\u00b7ner", "Sei\u00b7te", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$(", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die liebe Stimme: Schl\u00e4fst du nicht? Ist dir nicht wohl", "tokens": ["Die", "lie\u00b7be", "Stim\u00b7me", ":", "Schl\u00e4fst", "du", "nicht", "?", "Ist", "dir", "nicht", "wohl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "VAFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gib deine Hand mir \u2013 so! Nun wird's vor\u00fcbergehn.", "tokens": ["Gib", "dei\u00b7ne", "Hand", "mir", "\u2013", "so", "!", "Nun", "wird's", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$(", "ADV", "$.", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du hast zu lang geschrieben.", "tokens": ["Du", "hast", "zu", "lang", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Und die zarte Hand", "tokens": ["Und", "die", "zar\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.36": {"text": "In meine legend, schlummert sie von neuem ein.", "tokens": ["In", "mei\u00b7ne", "le\u00b7gend", ",", "schlum\u00b7mert", "sie", "von", "neu\u00b7em", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "$,", "VVFIN", "PPER", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mir aber ist, als w\u00e4r' an meinem Haupt vorbei", "tokens": ["Mir", "a\u00b7ber", "ist", ",", "als", "w\u00e4r'", "an", "mei\u00b7nem", "Haupt", "vor\u00b7bei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "KOKOM", "VAFIN", "APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein Blitz gestreift, und gn\u00e4dig h\u00e4tte noch einmal", "tokens": ["Ein", "Blitz", "ge\u00b7streift", ",", "und", "gn\u00e4\u00b7dig", "h\u00e4t\u00b7te", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ADJD", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das Schicksal sich erbarmt des armen Sterblichen.", "tokens": ["Das", "Schick\u00b7sal", "sich", "er\u00b7barmt", "des", "ar\u00b7men", "Sterb\u00b7li\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.40": {"text": "Und sieh, schon k\u00fcndet tr\u00f6stlich sich das Fr\u00fchrot an.", "tokens": ["Und", "sieh", ",", "schon", "k\u00fcn\u00b7det", "tr\u00f6st\u00b7lich", "sich", "das", "Fr\u00fch\u00b7rot", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "ADJD", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vom Garten leise klingt herauf ein Vogelruf", "tokens": ["Vom", "Gar\u00b7ten", "lei\u00b7se", "klingt", "her\u00b7auf", "ein", "Vo\u00b7gel\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und von dem nahen Turme der Basilika", "tokens": ["Und", "von", "dem", "na\u00b7hen", "Tur\u00b7me", "der", "Ba\u00b7si\u00b7li\u00b7ka"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das Gl\u00f6cklein, das zur Messe ruft.", "tokens": ["Das", "Gl\u00f6c\u00b7klein", ",", "das", "zur", "Mes\u00b7se", "ruft", "."], "token_in