, "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der sanfte Atem neben mir \u2013 mit Knistern lischt", "tokens": ["Der", "sanf\u00b7te", "A\u00b7tem", "ne\u00b7ben", "mir", "\u2013", "mit", "Knis\u00b7tern", "lischt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$(", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das Fl\u00e4mmchen aus, und l\u00e4hmend \u00fcberschauert mich", "tokens": ["Das", "Fl\u00e4mm\u00b7chen", "aus", ",", "und", "l\u00e4h\u00b7mend", "\u00fc\u00b7bersc\u00b7hau\u00b7ert", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein eisig Grauen.", "tokens": ["Ein", "ei\u00b7sig", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Ew'ge M\u00e4chte, k\u00e4m' es je,", "tokens": ["Ew'\u00b7ge", "M\u00e4ch\u00b7te", ",", "k\u00e4m'", "es", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df dieses Lebens Flamme, dran das meine sich", "tokens": ["Da\u00df", "die\u00b7ses", "Le\u00b7bens", "Flam\u00b7me", ",", "dran", "das", "mei\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "$,", "PAV", "ART", "PPOSAT", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Belebt und w\u00e4rmt, des besten Weibes Liebeshauch", "tokens": ["Be\u00b7lebt", "und", "w\u00e4rmt", ",", "des", "bes\u