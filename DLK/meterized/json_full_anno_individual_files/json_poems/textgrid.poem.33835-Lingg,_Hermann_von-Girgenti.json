{"textgrid.poem.33835": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Girgenti", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fremd ist mir Alles hier, aber auch du", "tokens": ["Fremd", "ist", "mir", "Al\u00b7les", "hier", ",", "a\u00b7ber", "auch", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PIS", "ADV", "$,", "ADV", "ADV", "PPER"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Bist mir ja fremd geworden; die dich umgeben,", "tokens": ["Bist", "mir", "ja", "fremd", "ge\u00b7wor\u00b7den", ";", "die", "dich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VAPP", "$.", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wer sind sie? Wem neigst du dich zu?", "tokens": ["Wer", "sind", "sie", "?", "Wem", "neigst", "du", "dich", "zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wer sch\u00fctzt dich, wer versch\u00f6nt dir das Leben?", "tokens": ["Wer", "sch\u00fctzt", "dich", ",", "wer", "ver\u00b7sch\u00f6nt", "dir", "das", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich wei\u00df es nimmer! Was uns gemeinsam", "tokens": ["Ich", "wei\u00df", "es", "nim\u00b7mer", "!", "Was", "uns", "ge\u00b7mein\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PWS", "PPER", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und traut war, zerrann wie der wehende Sand;", "tokens": ["Und", "traut", "war", ",", "zer\u00b7rann", "wie", "der", "we\u00b7hen\u00b7de", "Sand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "$,", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Verlassen durchwandr' ich und einsam", "tokens": ["Ver\u00b7las\u00b7sen", "durch\u00b7wandr'", "ich", "und", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Das fremde Land,", "tokens": ["Das", "frem\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die St\u00e4tten, von welchen Alles,", "tokens": ["Die", "St\u00e4t\u00b7ten", ",", "von", "wel\u00b7chen", "Al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Was einst so m\u00e4chtig bestand,", "tokens": ["Was", "einst", "so", "m\u00e4ch\u00b7tig", "be\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Bis auf die Zeugen des Verfalles,", "tokens": ["Bis", "auf", "die", "Zeu\u00b7gen", "des", "Ver\u00b7fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die stolzen Ruinen, verschwand.", "tokens": ["Die", "stol\u00b7zen", "Ru\u00b7i\u00b7nen", ",", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Fremd ist mir der Berge Gestalt,", "tokens": ["Fremd", "ist", "mir", "der", "Ber\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Von der gl\u00fchenden Mittagsluft umwoben,", "tokens": ["Von", "der", "gl\u00fc\u00b7hen\u00b7den", "Mit\u00b7tags\u00b7luft", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Und fremd erschallt", "tokens": ["Und", "fremd", "er\u00b7schallt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Der Hirten Ruf vom Felspfad oben.", "tokens": ["Der", "Hir\u00b7ten", "Ruf", "vom", "Fel\u00b7spfad", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Von den Menschen, die mir begegnen, keinen,", "tokens": ["Von", "den", "Men\u00b7schen", ",", "die", "mir", "be\u00b7geg\u00b7nen", ",", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "PIAT", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Der heimkehrt zu den Seinen,", "tokens": ["Der", "heim\u00b7kehrt", "zu", "den", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "PPOSS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Geleitet in sein Haus", "tokens": ["Ge\u00b7lei\u00b7tet", "in", "sein", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Mein fl\u00fcchtiger Gru\u00df. Sie selbst auch erscheinen", "tokens": ["Mein", "fl\u00fcch\u00b7ti\u00b7ger", "Gru\u00df", ".", "Sie", "selbst", "auch", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sich fremd hier, und wie sie hinaus", "tokens": ["Sich", "fremd", "hier", ",", "und", "wie", "sie", "hin\u00b7aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "ADV", "$,", "KON", "PWAV", "PPER", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Aufs Meer, aufs wogende, schauen,", "tokens": ["Aufs", "Meer", ",", "aufs", "wo\u00b7gen\u00b7de", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Ob nicht wiederkehre der Stadt", "tokens": ["Ob", "nicht", "wie\u00b7der\u00b7keh\u00b7re", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "VVFIN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.24": {"text": "Uralter Gebieter, um fahrtensatt", "tokens": ["Ur\u00b7al\u00b7ter", "Ge\u00b7bie\u00b7ter", ",", "um", "fahr\u00b7ten\u00b7satt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUI", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.25": {"text": "Nun wieder zu herrschen und aufzubauen", "tokens": ["Nun", "wie\u00b7der", "zu", "herr\u00b7schen", "und", "auf\u00b7zu\u00b7bau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Die Gr\u00f6\u00dfe der einstigen Zeit,", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "der", "eins\u00b7ti\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.27": {"text": "Die untergegangene Herrlichkeit,", "tokens": ["Die", "un\u00b7ter\u00b7ge\u00b7gan\u00b7ge\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Da m\u00f6gen sie wohl \u00fcber den Schauern", "tokens": ["Da", "m\u00f6\u00b7gen", "sie", "wohl", "\u00fc\u00b7ber", "den", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Auf den Tr\u00fcmmern der Pracht,", "tokens": ["Auf", "den", "Tr\u00fcm\u00b7mern", "der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.30": {"text": "Wie Fremde sich f\u00fchlen und trauern", "tokens": ["Wie", "Frem\u00b7de", "sich", "f\u00fch\u00b7len", "und", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.31": {"text": "Vor der Vorzeit gigantischer Macht.", "tokens": ["Vor", "der", "Vor\u00b7zeit", "gi\u00b7gan\u00b7ti\u00b7scher", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nur wenn vom Meer dort herauf", "tokens": ["Nur", "wenn", "vom", "Meer", "dort", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPRART", "NN", "ADV", "PTKVZ"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Sonne steigt und \u00fcberstr\u00f6mt mit Feuer", "tokens": ["Die", "Son\u00b7ne", "steigt", "und", "\u00fc\u00b7ber\u00b7str\u00f6mt", "mit", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die geborstene W\u00f6lbung, den S\u00e4ulenknauf", "tokens": ["Die", "ge\u00b7bors\u00b7te\u00b7ne", "W\u00f6l\u00b7bung", ",", "den", "S\u00e4u\u00b7len\u00b7knauf"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und das riesige Stufengem\u00e4uer,", "tokens": ["Und", "das", "rie\u00b7si\u00b7ge", "Stu\u00b7fen\u00b7ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Dann leuchtet's wie seliger Hauch,", "tokens": ["Dann", "leuch\u00b7tet's", "wie", "se\u00b7li\u00b7ger", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie Ahnung jener Tage", "tokens": ["Wie", "Ah\u00b7nung", "je\u00b7ner", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Voll Sch\u00f6nheit und Liebe, dann lebt mir auch", "tokens": ["Voll", "Sch\u00f6n\u00b7heit", "und", "Lie\u00b7be", ",", "dann", "lebt", "mir", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Dein Angedenken wieder. O sage,", "tokens": ["Dein", "An\u00b7ge\u00b7den\u00b7ken", "wie\u00b7der", ".", "O", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$.", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ist's wahr, du trugst hier am Feste", "tokens": ["Ist's", "wahr", ",", "du", "trugst", "hier", "am", "Fes\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Der Himmlischen den Erntekranz,", "tokens": ["Der", "Himm\u00b7li\u00b7schen", "den", "Ern\u00b7te\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du f\u00fchrtest, wenn man die Trauben pre\u00dfte,", "tokens": ["Du", "f\u00fchr\u00b7test", ",", "wenn", "man", "die", "Trau\u00b7ben", "pre\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Als Erste den Reigentanz?", "tokens": ["Als", "Ers\u00b7te", "den", "Rei\u00b7gen\u00b7tanz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und hast du nicht schon einmal mit mir", "tokens": ["Und", "hast", "du", "nicht", "schon", "ein\u00b7mal", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von Liebe gesprochen,", "tokens": ["Von", "Lie\u00b7be", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Hat nicht vor diesen Stufen hier", "tokens": ["Hat", "nicht", "vor", "die\u00b7sen", "Stu\u00b7fen", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Einst deine Hand in meiner geruht?", "tokens": ["Einst", "dei\u00b7ne", "Hand", "in", "mei\u00b7ner", "ge\u00b7ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "F\u00fchlt' ich dein Herz nicht an meinem poche?", "tokens": ["F\u00fchlt'", "ich", "dein", "Herz", "nicht", "an", "mei\u00b7nem", "po\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Ach, die Zeit, die nagende Flut", "tokens": ["Ach", ",", "die", "Zeit", ",", "die", "na\u00b7gen\u00b7de", "Flut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Hat die Steinkolosse zerbrochen,", "tokens": ["Hat", "die", "Stein\u00b7ko\u00b7los\u00b7se", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Was gro\u00df und sch\u00f6n war, ist ausgetan.", "tokens": ["Was", "gro\u00df", "und", "sch\u00f6n", "war", ",", "ist", "aus\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Ja, w\u00fcrden auch wir uns wiedersehen,", "tokens": ["Ja", ",", "w\u00fcr\u00b7den", "auch", "wir", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Fremd schauten wir uns an,", "tokens": ["Fremd", "schau\u00b7ten", "wir", "uns", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Und k\u00f6nnten uns nicht mehr verstehen,", "tokens": ["Und", "k\u00f6nn\u00b7ten", "uns", "nicht", "mehr", "ver\u00b7ste\u00b7hen", ","], "toke