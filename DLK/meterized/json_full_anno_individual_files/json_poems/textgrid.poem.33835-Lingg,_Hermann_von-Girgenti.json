, "measure": "iambic.tetra"}, "line.29": {"text": "Wie Meeresgrund. Es hat Gewalt,", "tokens": ["Wie", "Mee\u00b7res\u00b7grund", ".", "Es", "hat", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$.", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Ward uns das herrlichste Gut entrissen,", "tokens": ["Ward", "uns", "das", "herr\u00b7lichs\u00b7te", "Gut", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Da\u00df es f\u00fcr uns in Schattengestalt", "tokens": ["Da\u00df", "es", "f\u00fcr", "uns", "in", "Schat\u00b7ten\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.32": {"text": "Her\u00fcber wallt,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "wallt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Sanft leuchtend aus Finsternissen.", "tokens": ["Sanft", "leuch\u00b7tend", "aus", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Und Allem verleiht es, Allem um uns her", "tokens": ["Und", "Al\u00b7lem", "ver\u00b7leiht", "es", ",", "Al\u00b7lem", "um", "uns", "her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PIS", "APPR", "PRF", "APZR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Ein tieferes Leben, es gibt", "tokens": ["