{"textgrid.poem.49082": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "40. An den Kronprinzen von Baiern", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Kr\u00f6sus reicher Schatz noch stolzer Pharaonen", "tokens": ["Nicht", "Kr\u00f6\u00b7sus", "rei\u00b7cher", "Schatz", "noch", "stol\u00b7zer", "Pha\u00b7ra\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "ADJD", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewalt und Vorrecht darf den Genius umfangen,", "tokens": ["Ge\u00b7walt", "und", "Vor\u00b7recht", "darf", "den", "Ge\u00b7nius", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df niedrig fessle er an sie fest sein Verlangen;", "tokens": ["Da\u00df", "nied\u00b7rig", "fess\u00b7le", "er", "an", "sie", "fest", "sein", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gewohnt zu schwingen sich in h\u00f6h're Regionen,", "tokens": ["Ge\u00b7wohnt", "zu", "schwin\u00b7gen", "sich", "in", "h\u00f6h'\u00b7re", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "W\u00e4hlt er zum Ziel nur dort die lichteste der Kronen,", "tokens": ["W\u00e4hlt", "er", "zum", "Ziel", "nur", "dort", "die", "lich\u00b7tes\u00b7te", "der", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die einst Astr\u00e4a (wie Zeus hehre T\u00f6chter sangen,", "tokens": ["Die", "einst", "A\u00b7str\u00e4a", "(", "wie", "Zeus", "heh\u00b7re", "T\u00f6ch\u00b7ter", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NE", "$(", "KOKOM", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Als sie im Wettkampf mit den Pieriden rangen)", "tokens": ["Als", "sie", "im", "Wett\u00b7kampf", "mit", "den", "Pie\u00b7ri\u00b7den", "ran\u00b7gen", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Geflochten, gl\u00e4nzende Verdienste zu belohnen.", "tokens": ["Ge\u00b7floch\u00b7ten", ",", "gl\u00e4n\u00b7zen\u00b7de", "Ver\u00b7diens\u00b7te", "zu", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich hat, erhab'ner Prinz, ein selig Loos getroffen,", "tokens": ["Dich", "hat", ",", "er\u00b7hab'\u00b7ner", "Prinz", ",", "ein", "se\u00b7lig", "Loos", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die G\u00f6tter liehen dir des Gl\u00fcckes reichste Habe,", "tokens": ["Die", "G\u00f6t\u00b7ter", "lie\u00b7hen", "dir", "des", "Gl\u00fc\u00b7ckes", "reichs\u00b7te", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dir steht, wie einst der Thron, der Helikon jetzt offen;", "tokens": ["Dir", "steht", ",", "wie", "einst", "der", "Thron", ",", "der", "He\u00b7li\u00b7kon", "jetzt", "of\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Herrsch' froh indessen hier, mit hoher Musen Gabe,", "tokens": ["Herr\u00b7sch'", "froh", "in\u00b7des\u00b7sen", "hier", ",", "mit", "ho\u00b7her", "Mu\u00b7sen", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Und sch\u00fctz', darf Sch\u00f6neres der Sterbliche wol hoffen?", "tokens": ["Und", "sch\u00fctz'", ",", "darf", "Sch\u00f6\u00b7ne\u00b7res", "der", "Sterb\u00b7li\u00b7che", "wol", "hof\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VMFIN", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambi