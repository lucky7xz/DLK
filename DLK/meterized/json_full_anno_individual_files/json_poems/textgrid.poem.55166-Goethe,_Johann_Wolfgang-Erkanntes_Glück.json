{"textgrid.poem.55166": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Erkanntes Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bed\u00e4chtlich Natur sonst unter viele verteilet,", "tokens": ["Was", "be\u00b7d\u00e4cht\u00b7lich", "Na\u00b7tur", "sonst", "un\u00b7ter", "vie\u00b7le", "ver\u00b7tei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Gab sie mit reichlicher Hand alles der Einzigen, ihr.", "tokens": ["Gab", "sie", "mit", "reich\u00b7li\u00b7cher", "Hand", "al\u00b7les", "der", "Ein\u00b7zi\u00b7gen", ",", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PIS", "ART", "NN", "$,", "PPER", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Und die so herrlich Begabte, von vielen so innig Verehrte,", "tokens": ["Und", "die", "so", "herr\u00b7lich", "Be\u00b7gab\u00b7te", ",", "von", "vie\u00b7len", "so", "in\u00b7nig", "Ver\u00b7ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJD", "NN", "$,", "APPR", "PIAT", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gab ein liebend Geschick freundlich dem Gl\u00fccklichen, mir.", "tokens": ["Gab", "ein", "lie\u00b7bend", "Ge\u00b7schick", "freund\u00b7lich", "dem", "Gl\u00fcck\u00b7li\u00b7chen", ",", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"]