{"textgrid.poem.38234": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck der Schlemmer", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Baum in Oesterreich,", "tokens": ["Es", "steht", "ein", "Baum", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der tr\u00e4gt Muskaten Blumen;", "tokens": ["Der", "tr\u00e4gt", "Mus\u00b7ka\u00b7ten", "Blu\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die erste Blume, die er trug,", "tokens": ["Die", "ers\u00b7te", "Blu\u00b7me", ",", "die", "er", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die brach ein's K\u00f6nigs Tochter.", "tokens": ["Die", "brach", "ein's", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darzu so kam ein Reuter gegangen,", "tokens": ["Dar\u00b7zu", "so", "kam", "ein", "Reu\u00b7ter", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der freit des K\u00f6nigs Tochter;", "tokens": ["Der", "freit", "des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er freit sie l\u00e4nger denn sieben Jahr,", "tokens": ["Er", "freit", "sie", "l\u00e4n\u00b7ger", "denn", "sie\u00b7ben", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er konnt sie nicht erfreien.", "tokens": ["Er", "konnt", "sie", "nicht", "er\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df ab, la\u00df ab du junger Knab,", "tokens": ["La\u00df", "ab", ",", "la\u00df", "ab", "du", "jun\u00b7ger", "Knab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kannst mich nicht erfreien;", "tokens": ["Du", "kannst", "mich", "nicht", "er\u00b7frei\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin viel besser geborn denn du,", "tokens": ["Ich", "bin", "viel", "bes\u00b7ser", "ge\u00b7born", "denn", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "KON", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Vater und auch von Mutter.", "tokens": ["Von", "Va\u00b7ter", "und", "auch", "von", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Bist du viel besser geborn,denn ich,", "tokens": ["Bist", "du", "viel", "bes\u00b7ser", "ge\u00b7born", ",", "denn", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,", "KON", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Von Vater und auch von Mutter,", "tokens": ["Von", "Va\u00b7ter", "und", "auch", "von", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So bin ich deines Vaters gedingter Knecht,", "tokens": ["So", "bin", "ich", "dei\u00b7nes", "Va\u00b7ters", "ge\u00b7ding\u00b7ter", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und schwing dem R\u00f6\u00dflein sein Futter.", "tokens": ["Und", "schwing", "dem", "R\u00f6\u00df\u00b7lein", "sein", "Fut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Bist du mein's Vaters gedingter Knecht,", "tokens": ["Bist", "du", "mein's", "Va\u00b7ters", "ge\u00b7ding\u00b7ter", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und schwingst dem R\u00f6\u00dflein sein Futter;", "tokens": ["Und", "schwingst", "dem", "R\u00f6\u00df\u00b7lein", "sein", "Fut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So giebt dir mein Vater auch gro\u00dfen Lohn,", "tokens": ["So", "giebt", "dir", "mein", "Va\u00b7ter", "auch", "gro\u00b7\u00dfen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Damit la\u00df dir gen\u00fcgen.", "tokens": ["Da\u00b7mit", "la\u00df", "dir", "ge\u00b7n\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Den gro\u00dfen Lohn, den er mir giebt,", "tokens": ["Den", "gro\u00b7\u00dfen", "Lohn", ",", "den", "er", "mir", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der wird mir viel zu sauer;", "tokens": ["Der", "wird", "mir", "viel", "zu", "sau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn andre zum Schlafk\u00e4mmerlein gehn,", "tokens": ["Wenn", "and\u00b7re", "zum", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So mu\u00df ich zu der Scheuer.", "tokens": ["So", "mu\u00df", "ich", "zu", "der", "Scheu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des Nachts wohl um die halbe Nacht,", "tokens": ["Des", "Nachts", "wohl", "um", "die", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4gdlein begunnt zu trauren;", "tokens": ["Das", "M\u00e4gd\u00b7lein", "be\u00b7gunnt", "zu", "trau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie nahm ihre Kleider untern Arm,", "tokens": ["Sie", "nahm", "ih\u00b7re", "Klei\u00b7der", "un\u00b7tern", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und gieng wohl zu der Scheuer.", "tokens": ["Und", "gieng", "wohl", "zu", "der", "Scheu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des Morgens da der Tag anbrach,", "tokens": ["Des", "Mor\u00b7gens", "da", "der", "Tag", "an\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter begunnt zu rufen;", "tokens": ["Die", "Mut\u00b7ter", "be\u00b7gunnt", "zu", "ru\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Steh auf, steh auf, du gedingter Knecht,", "tokens": ["Steh", "auf", ",", "steh", "auf", ",", "du", "ge\u00b7ding\u00b7ter", "Knecht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und gieb dem Ro\u00df das Futter.", "tokens": ["Und", "gieb", "dem", "Ro\u00df", "das", "Fut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Futter, das ich ihm geben will,", "tokens": ["Das", "Fut\u00b7ter", ",", "das", "ich", "ihm", "ge\u00b7ben", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das liegt in meinen Armen,", "tokens": ["Das", "liegt", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "N\u00e4chten Abends war ich euer gedingter Knecht,", "tokens": ["N\u00e4ch\u00b7ten", "A\u00b7bends", "war", "ich", "eu\u00b7er", "ge\u00b7ding\u00b7ter", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Euer Eidam bin ich worden.", "tokens": ["Eu\u00b7er", "Ei\u00b7dam", "bin", "ich", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da\u00df du mein Eidam worden bist,", "tokens": ["Da\u00df", "du", "mein", "Ei\u00b7dam", "wor\u00b7den", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De\u00df mu\u00df sich Gott erbarmen!", "tokens": ["De\u00df", "mu\u00df", "sich", "Gott", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' sie Rittern und Grafen versagt,", "tokens": ["Ich", "hab'", "sie", "Rit\u00b7tern", "und", "Gra\u00b7fen", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Schlemmer ist sie worden!", "tokens": ["Dem", "Schlem\u00b7mer", "ist", "sie", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Dem Schlemmer, dem sie worden ist,", "tokens": ["Dem", "Schlem\u00b7mer", ",", "dem", "sie", "wor\u00b7den", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der kann sie wohl ern\u00e4hren;", "tokens": ["Der", "kann", "sie", "wohl", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er trinkt viel lieber den k\u00fchlen Wein", "tokens": ["Er", "trinkt", "viel", "lie\u00b7ber", "den", "k\u00fch\u00b7len", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn Wasser aus dem Brunnen.", "tokens": ["Denn", "Was\u00b7ser", "aus", "dem", "Brun\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der uns dies neue Liedlein sang,", "tokens": ["Der", "uns", "dies", "neu\u00b7e", "Lied\u00b7lein", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat's gar wohl gesungen;", "tokens": ["Er", "hat's", "gar", "wohl", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ist dreimal in Paris gewesen,", "tokens": ["Er", "ist", "drei\u00b7mal", "in", "Pa\u00b7ris", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NE", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und immer wieder kommen.", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Es steht ein Baum in Oesterreich,", "tokens": ["Es", "steht", "ein", "Baum", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der tr\u00e4gt Muskaten Blumen;", "tokens": ["Der", "tr\u00e4gt", "Mus\u00b7ka\u00b7ten", "Blu\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die erste Blume, die er trug,", "tokens": ["Die", "ers\u00b7te", "Blu\u00b7me", ",", "die", "er", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die brach ein's K\u00f6nigs Tochter.", "tokens": ["Die", "brach", "ein's", "K\u00f6\u00b7nigs", "Toch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Darzu so kam ein Reuter gegangen,", "tokens": ["Dar\u00b7zu", "so", "kam", "ein", "Reu\u00b7ter", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der freit des K\u00f6nigs Tochter;", "tokens": ["Der", "freit", "des", "K\u00f6\u00b7nigs", "Toch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "N