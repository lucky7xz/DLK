{"textgrid.poem.25337": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Gethsemane", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Stunde war's,", "tokens": ["Um", "die", "Stun\u00b7de", "wa\u00b7r's", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da die heilige Stille der Mitternacht", "tokens": ["Da", "die", "hei\u00b7li\u00b7ge", "Stil\u00b7le", "der", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Auftaucht vom Meer und segnend \u00fcber Welten f\u00e4hrt.", "tokens": ["Auf\u00b7taucht", "vom", "Meer", "und", "seg\u00b7nend", "\u00fc\u00b7ber", "Wel\u00b7ten", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "J\u00e4h durch die Palmen schritt das Todesgrauen,", "tokens": ["J\u00e4h", "durch", "die", "Pal\u00b7men", "schritt", "das", "To\u00b7des\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Urweltenweh", "tokens": ["Ur\u00b7wel\u00b7ten\u00b7weh"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Rang auf zum Firmament.", "tokens": ["Rang", "auf", "zum", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schwer hing der Himmel \u2013", "tokens": ["Schwer", "hing", "der", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Nacht ... Tod ... In tiefem Schlaf die J\u00fcnger ...", "tokens": ["Nacht", "...", "Tod", "...", "In", "tie\u00b7fem", "Schlaf", "die", "J\u00fcn\u00b7ger", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "APPR", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wilde, brennendwilde Einsamkeit ...", "tokens": ["Und", "wil\u00b7de", ",", "bren\u00b7nend\u00b7wil\u00b7de", "Ein\u00b7sam\u00b7keit", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aufschluchzend schl\u00e4gt er auf die Wurzelknorren,", "tokens": ["Auf\u00b7schluch\u00b7zend", "schl\u00e4gt", "er", "auf", "die", "Wur\u00b7zel\u00b7knor\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Weint in die Nacht,", "tokens": ["Weint", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.12": {"text": "Die l\u00e4chelnd \u00fcber's Haupt die Schleier hebt.", "tokens": ["Die", "l\u00e4\u00b7chelnd", "\u00fc\u00b7ber's", "Haupt", "die", "Schlei\u00b7er", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein sengend Leuchten durch die D\u00e4mmernebel:", "tokens": ["Ein", "sen\u00b7gend", "Leuch\u00b7ten", "durch", "die", "D\u00e4m\u00b7mer\u00b7ne\u00b7bel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Die Sonne.", "tokens": ["Die", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Von Glockenst\u00fchlen sprang sie rot in graue T\u00fcrme,", "tokens": ["Von", "Glo\u00b7cken\u00b7st\u00fch\u00b7len", "sprang", "sie", "rot", "in", "grau\u00b7e", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fiel st\u00e4ubend in die Kuppeln, flutete", "tokens": ["Fiel", "st\u00e4u\u00b7bend", "in", "die", "Kup\u00b7peln", ",", "flu\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In wildem Quellen durch die schlanken St\u00e4mme,", "tokens": ["In", "wil\u00b7dem", "Quel\u00b7len", "durch", "die", "schlan\u00b7ken", "St\u00e4m\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wegspuren zeichnend roten Flammengoldes.", "tokens": ["Weg\u00b7spu\u00b7ren", "zeich\u00b7nend", "ro\u00b7ten", "Flam\u00b7men\u00b7gol\u00b7des", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vom Boden weg", "tokens": ["Vom", "Bo\u00b7den", "weg"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Sah Christus \u2013 blickte", "tokens": ["Sah", "Chris\u00b7tus", "\u2013", "blick\u00b7te"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "NE", "$(", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mit fremden Augen in die sch\u00e4umende Morgenglut,", "tokens": ["Mit", "frem\u00b7den", "Au\u00b7gen", "in", "die", "sch\u00e4u\u00b7men\u00b7de", "Mor\u00b7gen\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und wie ein Wecken klang's ihm durch die Brust,", "tokens": ["Und", "wie", "ein", "We\u00b7cken", "klang's", "ihm", "durch", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Das uralt junge Sch\u00f6pferlied des Lichts:", "tokens": ["Das", "ur\u00b7alt", "jun\u00b7ge", "Sch\u00f6p\u00b7fer\u00b7lied", "des", "Lichts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Posaunen trugen ehern es empor", "tokens": ["Po\u00b7sau\u00b7nen", "tru\u00b7gen", "e\u00b7hern", "es", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOUS", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und alle Geigen fielen flimmernd ein", "tokens": ["Und", "al\u00b7le", "Gei\u00b7gen", "fie\u00b7len", "flim\u00b7mernd", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In brausenden Bogenstrichen,", "tokens": ["In", "brau\u00b7sen\u00b7den", "Bo\u00b7gen\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "V\u00f6gel jauchzten,", "tokens": ["V\u00f6\u00b7gel", "jauchz\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Und Morgenglocken wehten von den T\u00fcrmen", "tokens": ["Und", "Mor\u00b7gen\u00b7glo\u00b7cken", "weh\u00b7ten", "von", "den", "T\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Jerusalems herauf, einrauschend in", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lems", "her\u00b7auf", ",", "ein\u00b7rau\u00b7schend", "in"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "VVPP", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die breiten Takte, die", "tokens": ["Die", "brei\u00b7ten", "Tak\u00b7te", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Im Werdelied des Tags die Welt durchfurchten.", "tokens": ["Im", "Wer\u00b7de\u00b7lied", "des", "Tags", "die", "Welt", "durch\u00b7furch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Nieder fiel Christus, starrte", "tokens": ["Nie\u00b7der", "fiel", "Chris\u00b7tus", ",", "starr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "Hinunter auf die rote Stadt, die", "tokens": ["Hin\u00b7un\u00b7ter", "auf", "die", "ro\u00b7te", "Stadt", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "In tausend T\u00fcrmen tausend Fackeln fachte,", "tokens": ["In", "tau\u00b7send", "T\u00fcr\u00b7men", "tau\u00b7send", "Fa\u00b7ckeln", "fach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und zur Sonne auf,", "tokens": ["Und", "zur", "Son\u00b7ne", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Zur ewig g\u00f6ttlichen jauchzte sein Mund:", "tokens": ["Zur", "e\u00b7wig", "g\u00f6tt\u00b7li\u00b7chen", "jauchz\u00b7te", "sein", "Mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "\u00bbo sterben, sterben, Gott! ... In Meere will", "tokens": ["\u00bb", "o", "ster\u00b7ben", ",", "ster\u00b7ben", ",", "Gott", "!", "...", "In", "Mee\u00b7re", "will"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "FM", "VVINF", "$,", "VVFIN", "$,", "NN", "$.", "$(", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ich tauchen purpur\u00fcberrauscht,", "tokens": ["Ich", "tau\u00b7chen", "pur\u00b7pu\u00b7r\u00fc\u00b7berr\u00b7auscht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "In Licht zerflie\u00dfen, ganz in Duft mich l\u00f6sen,", "tokens": ["In", "Licht", "zer\u00b7flie\u00b7\u00dfen", ",", "ganz", "in", "Duft", "mich", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,", "ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Als Welle wehen in des Weltalls Strom.", "tokens": ["Als", "Wel\u00b7le", "we\u00b7hen", "in", "des", "Welt\u00b7alls", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Denn nun", "tokens": ["Denn", "nun"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Ward mir der Welten letzter, tiefster Sinn.", "tokens": ["Ward", "mir", "der", "Wel\u00b7ten", "letz\u00b7ter", ",", "tiefs\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Aus deiner Sonne Morgenaugen las ich ihn. \u2013", "tokens": ["Aus", "dei\u00b7ner", "Son\u00b7ne", "Mor\u00b7ge\u00b7nau\u00b7gen", "las", "ich", "ihn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O sterben, sterben, Gott! ... Doch wie", "tokens": ["O", "ster\u00b7ben", ",", "ster\u00b7ben", ",", "Gott", "!", "...", "Doch", "wie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVINF", "$,", "VVFIN", "$,", "NN", "$.", "$(", "KON", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schiffer, dem", "tokens": ["Der", "Schif\u00b7fer", ",", "dem"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Brandung in des Nachens Rippen brach,", "tokens": ["Die", "Bran\u00b7dung", "in", "des", "Na\u00b7chens", "Rip\u00b7pen", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Flutenumdr\u00f6hnt", "tokens": ["Flu\u00b7ten\u00b7um\u00b7dr\u00f6hnt"], "token_info": ["word"], "pos": ["NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Zukunft goldverbr\u00e4mtes Eiland gr\u00fc\u00dft:", "tokens": ["Der", "Zu\u00b7kunft", "gold\u00b7ver\u00b7br\u00e4m\u00b7tes", "Ei\u00b7land", "gr\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So gr\u00fc\u00df ich euch, Schlummernde, Ungeborne \u2013", "tokens": ["So", "gr\u00fc\u00df", "ich", "euch", ",", "Schlum\u00b7mern\u00b7de", ",", "Un\u00b7ge\u00b7bor\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Aus harter Nacht ein junges Sonnenvolk.", "tokens": ["Aus", "har\u00b7ter", "Nacht", "ein", "jun\u00b7ges", "Son\u00b7nen\u00b7volk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn also lehrte mich dein Sch\u00f6pfertag:", "tokens": ["Denn", "al\u00b7so", "lehr\u00b7te", "mich", "dein", "Sch\u00f6p\u00b7fer\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Glut quillt aus Asche, Leben spr\u00fcht aus Tod,", "tokens": ["Glut", "quillt", "aus", "A\u00b7sche", ",", "Le\u00b7ben", "spr\u00fcht", "aus", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus tiefsten N\u00e4chten d\u00e4mmern neue Morgenr\u00f6ten.\u00ab", "tokens": ["Aus", "tiefs\u00b7ten", "N\u00e4ch\u00b7ten", "d\u00e4m\u00b7mern", "neu\u00b7e", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und gehobnen Blicks", "tokens": ["Und", "ge\u00b7hob\u00b7nen", "Blicks"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tr