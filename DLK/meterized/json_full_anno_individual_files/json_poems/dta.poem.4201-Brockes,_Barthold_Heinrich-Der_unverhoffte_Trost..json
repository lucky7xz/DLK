{"dta.poem.4201": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der unverhoffte Trost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von vielen Sorgen \u00fcberh\u00e4uft, von tr\u00fcber Schwermuht", "tokens": ["Von", "vie\u00b7len", "Sor\u00b7gen", "\u00fc\u00b7berh\u00b7\u00e4uft", ",", "von", "tr\u00fc\u00b7ber", "Schwer\u00b7muht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "recht gedr\u00fccket,", "tokens": ["recht", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Trat ich, bey dunkler Abendzeit, vor meines Hauses offne", "tokens": ["Trat", "ich", ",", "bey", "dunk\u00b7ler", "A\u00b7bend\u00b7zeit", ",", "vor", "mei\u00b7nes", "Hau\u00b7ses", "off\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Th\u00fcr,", "tokens": ["Th\u00fcr", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sah eine Zeitlang vor mir nieder, mich nagt\u2019, fast \u00fcber die", "tokens": ["Sah", "ei\u00b7ne", "Zeit\u00b7lang", "vor", "mir", "nie\u00b7der", ",", "mich", "nagt'", ",", "fast", "\u00fc\u00b7ber", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "ADV", "APPR", "ART"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Geb\u00fchr,", "tokens": ["Ge\u00b7b\u00fchr", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Ein schwarzer Schmerz, ein dunkler Gram, der meinen", "tokens": ["Ein", "schwar\u00b7zer", "Schmerz", ",", "ein", "dunk\u00b7ler", "Gram", ",", "der", "mei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ganzen Geist bestricket;", "tokens": ["gan\u00b7zen", "Geist", "be\u00b7stri\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Kaum aber schlug ich ungefehr", "tokens": ["Kaum", "a\u00b7ber", "schlug", "ich", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die tr\u00fcben Augen in die H\u00f6he, so fiel das helle Sternen-", "tokens": ["Die", "tr\u00fc\u00b7ben", "Au\u00b7gen", "in", "die", "H\u00f6\u00b7he", ",", "so", "fiel", "das", "hel\u00b7le", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Heer,", "tokens": ["Heer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Und, vom entw\u00f6lkten Firmament, voll Dunkelheit, vermischt", "tokens": ["Und", ",", "vom", "ent\u00b7w\u00f6lk\u00b7ten", "Fir\u00b7ma\u00b7ment", ",", "voll", "Dun\u00b7kel\u00b7heit", ",", "ver\u00b7mischt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "mit Licht,", "tokens": ["mit", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Das ehrenw\u00fcrdige Spectakel, auf einmahl schnell mir ins", "tokens": ["Das", "eh\u00b7ren\u00b7w\u00fcr\u00b7di\u00b7ge", "Spec\u00b7ta\u00b7kel", ",", "auf", "ein\u00b7mahl", "schnell", "mir", "ins"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADV", "ADJD", "PPER", "APPRART"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Gesicht.", "tokens": ["Ge\u00b7sicht", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Ich fuhr, als wie von einem Strahl und Anblick unverhoffter", "tokens": ["Ich", "fuhr", ",", "als", "wie", "von", "ei\u00b7nem", "Strahl", "und", "An\u00b7blick", "un\u00b7ver\u00b7hoff\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "KOKOM", "APPR", "ART", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Flammen", "tokens": ["Flam\u00b7men"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ger\u00fchrt und gleichsam ganz durchdrungen, auf einmahl", "tokens": ["Ge\u00b7r\u00fchrt", "und", "gleich\u00b7sam", "ganz", "durch\u00b7drun\u00b7gen", ",", "auf", "ein\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "ADV", "VVPP", "$,", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "schnell in mir zusammen.", "tokens": ["schnell", "in", "mir", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Sogleich verschwand, als wie ein Blitz, des tr\u00fcben Kummers", "tokens": ["Sog\u00b7leich", "ver\u00b7schwand", ",", "als", "wie", "ein", "Blitz", ",", "des", "tr\u00fc\u00b7ben", "Kum\u00b7mers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "finstre Nacht;", "tokens": ["finst\u00b7re", "Nacht", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Es ward in meiner Seelen helle, des dunklen Himmels lichte", "tokens": ["Es", "ward", "in", "mei\u00b7ner", "See\u00b7len", "hel\u00b7le", ",", "des", "dunk\u00b7len", "Him\u00b7mels", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "$,", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Pracht", "tokens": ["Pracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Zertrennte schnell des Unmuhts Duft, als wie ein Licht die", "tokens": ["Zer\u00b7trenn\u00b7te", "schnell", "des", "Un\u00b7muhts", "Duft", ",", "als", "wie", "ein", "Licht", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Finsternissen.", "tokens": ["Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Ich ward aus meiner Schwermuht Tiefe recht mit Gewalt", "tokens": ["Ich", "ward", "aus", "mei\u00b7ner", "Schwer\u00b7muht", "Tie\u00b7fe", "recht", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.27": {"text": "herausgerissen,", "tokens": ["her\u00b7aus\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.28": {"text": "Und in der Ruhe Port gef\u00fchrt. Noch eh ich mich worauf", "tokens": ["Und", "in", "der", "Ru\u00b7he", "Port", "ge\u00b7f\u00fchrt", ".", "Noch", "eh", "ich", "mich", "wo\u00b7rauf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE", "VVPP", "$.", "ADV", "KOUS", "PPER", "PRF", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.29": {"text": "besann,", "tokens": ["be\u00b7sann", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.30": {"text": "Fing sich, in meinem finstern Wesen, ein gleichsam neues", "tokens": ["Fing", "sich", ",", "in", "mei\u00b7nem", "fins\u00b7tern", "We\u00b7sen", ",", "ein", "gleich\u00b7sam", "neu\u00b7es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Wesen an.", "tokens": ["We\u00b7sen", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wie konnt\u2019 auch dieses anders seyn? Des Sch\u00f6pfers", "tokens": ["Wie", "konnt'", "auch", "die\u00b7ses", "an\u00b7ders", "seyn", "?", "Des", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "PDAT", "ADV", "VAINF", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "allergr\u00f6\u00dften Werke,", "tokens": ["al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten", "Wer\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die hellen Zeugen Seiner Liebe, die Proben Seiner weisen", "tokens": ["Die", "hel\u00b7len", "Zeu\u00b7gen", "Sei\u00b7ner", "Lie\u00b7be", ",", "die", "Pro\u00b7ben", "Sei\u00b7ner", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "St\u00e4rke,", "tokens": ["St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Entdeckten GOttes Gegenwart. Bey solchem wunderbaren", "tokens": ["Ent\u00b7deck\u00b7ten", "Got\u00b7tes", "Ge\u00b7gen\u00b7wart", ".", "Bey", "sol\u00b7chem", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Licht", "tokens": ["Licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Bestund das Heer des dunklen Grams, die Menge schwarzer", "tokens": ["Be\u00b7stund", "das", "Heer", "des", "dunk\u00b7len", "Grams", ",", "die", "Men\u00b7ge", "schwar\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sorgen nicht;", "tokens": ["Sor\u00b7gen", "nicht", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Sie schwunden wie die leichten Schatten, durch die der", "tokens": ["Sie", "schwun\u00b7den", "wie", "die", "leich\u00b7ten", "Schat\u00b7ten", ",", "durch", "die", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Strahl der Sonne bricht.", "tokens": ["Strahl", "der", "Son\u00b7ne", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mich tr\u00f6stete, belebte, st\u00e4rkte auf Seine ", "tokens": ["Mich", "tr\u00f6s\u00b7te\u00b7te", ",", "be\u00b7leb\u00b7te", ",", "st\u00e4rk\u00b7te", "auf", "Sei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Vertrauen,", "tokens": ["Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Auf Seine ", "tokens": ["Auf", "Sei\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "bald zu schauen,", "tokens": ["bald", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Und auf die g\u00f6ttliche ", "tokens": ["Und", "auf", "die", "g\u00f6tt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.16": {"text": "sicht.", "tokens": ["sicht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}