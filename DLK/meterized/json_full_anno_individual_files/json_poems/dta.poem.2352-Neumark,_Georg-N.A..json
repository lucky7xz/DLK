{"dta.poem.2352": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fck zu Astreen Sohn! Du Au\u00dfbund kluger Tichter/", "tokens": ["Gl\u00fck", "zu", "A\u00b7streen", "Sohn", "!", "Du", "Au\u00df\u00b7bund", "klu\u00b7ger", "Tich\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$.", "PPER", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du schwingst dich von der Nacht hin an die grossen Lich-", "tokens": ["Du", "schwingst", "dich", "von", "der", "Nacht", "hin", "an", "die", "gros\u00b7sen", "Lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die voller Flam\u0303en sind: Der \u00e4dlen Rechte Flei\u00df/ (ter/", "tokens": ["Die", "vol\u00b7ler", "Flam\u0303en", "sind", ":", "Der", "\u00e4d\u00b7len", "Rech\u00b7te", "Flei\u00df", "/", "(", "ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "NN", "$(", "$(", "NE", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und was dein reiner Sinn von reinen Versen wei\u00df/", "tokens": ["Und", "was", "dein", "rei\u00b7ner", "Sinn", "von", "rei\u00b7nen", "Ver\u00b7sen", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du reiner ", "tokens": ["Du", "rei\u00b7ner"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Man kan zwar die ", "tokens": ["Man", "kan", "zwar", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In welchen beyden du ein guter Meister bist/", "tokens": ["In", "wel\u00b7chen", "bey\u00b7den", "du", "ein", "gu\u00b7ter", "Meis\u00b7ter", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "PPER", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann man das n\u00f6htigste darbey nur nicht vergist/", "tokens": ["Wann", "man", "das", "n\u00f6h\u00b7tigs\u00b7te", "dar\u00b7bey", "nur", "nicht", "ver\u00b7gist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "PAV", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als du mit Ruhm gethan. Jtzt schaffen deine W\u00e4lder/", "tokens": ["Als", "du", "mit", "Ruhm", "ge\u00b7than", ".", "Jtzt", "schaf\u00b7fen", "dei\u00b7ne", "W\u00e4l\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da schohn der Herbst beraubt die schammerirten Felder/", "tokens": ["Da", "schohn", "der", "Herbst", "be\u00b7raubt", "die", "scham\u00b7mer\u00b7ir\u00b7ten", "Fel\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "uns einen Sommer her; wann du sie selber singst/", "tokens": ["uns", "ei\u00b7nen", "Som\u00b7mer", "her", ";", "wann", "du", "sie", "sel\u00b7ber", "singst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "$.", "PWAV", "PPER", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die Violdegam in sanffter Stimmung zwingst", "tokens": ["Und", "die", "Vi\u00b7ol\u00b7de\u00b7gam", "in", "sanff\u00b7ter", "Stim\u00b7mung", "zwingst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu reiner Harmonie/ so fallen unsre Lieder", "tokens": ["Zu", "rei\u00b7ner", "Har\u00b7mo\u00b7nie", "/", "so", "fal\u00b7len", "uns\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als w\u00fcrden sie gescheucht/ in ihrem Nebel nieder/", "tokens": ["Als", "w\u00fcr\u00b7den", "sie", "ge\u00b7scheucht", "/", "in", "ih\u00b7rem", "Ne\u00b7bel", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bald sind wir lebendig/ todt/ freudig/ bald betr\u00fcbt/", "tokens": ["Bald", "sind", "wir", "le\u00b7ben\u00b7dig", "/", "todt", "/", "freu\u00b7dig", "/", "bald", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo deiner Seitenklang den Liedern Seele giebt.", "tokens": ["Wo", "dei\u00b7ner", "Sei\u00b7ten\u00b7klang", "den", "Lie\u00b7dern", "See\u00b7le", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sind/ wie meine/ nicht gesetzt vor Esets Ohren.", "tokens": ["Sie", "sind", "/", "wie", "mei\u00b7ne", "/", "nicht", "ge\u00b7setzt", "vor", "E\u00b7sets", "Oh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOKOM", "PPOSAT", "$(", "PTKNEG", "VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bald wird ein geistliches bald weltliches gebohren;", "tokens": ["Bald", "wird", "ein", "geist\u00b7li\u00b7ches", "bald", "welt\u00b7li\u00b7ches", "ge\u00b7boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Kl\u00e4ffer der sie uns vor grob und baurisch helt/", "tokens": ["Ein", "Kl\u00e4f\u00b7fer", "der", "sie", "uns", "vor", "grob", "und", "bau\u00b7risch", "helt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "PRF", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der wei\u00df nicht da\u00df er hat ein Urtheil selbst gef\u00e4llt", "tokens": ["Der", "wei\u00df", "nicht", "da\u00df", "er", "hat", "ein", "Ur\u00b7theil", "selbst", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "KOUS", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Von seinem Unverstand\u2019. Es brauchen die ", "tokens": ["Von", "sei\u00b7nem", "Un\u00b7ver\u00b7stand'", ".", "Es", "brau\u00b7chen", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "An statt der Orgel wol zuweilen Sch\u00e4ferfl\u00f6ten/", "tokens": ["An", "statt", "der", "Or\u00b7gel", "wol", "zu\u00b7wei\u00b7len", "Sch\u00e4\u00b7fer\u00b7fl\u00f6\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Geist ist iedesmahl zu iedem nicht geschikt.", "tokens": ["Der", "Geist", "ist", "ie\u00b7des\u00b7mahl", "zu", "ie\u00b7dem", "nicht", "ge\u00b7schikt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dir hats in geistlichen und weltlichen gegl\u00fckt/", "tokens": ["Dir", "hats", "in", "geist\u00b7li\u00b7chen", "und", "welt\u00b7li\u00b7chen", "ge\u00b7gl\u00fckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "KON", "PWAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Herr Neumark/ fahre fort! Du giebst dir selbst die Sporen/", "tokens": ["Herr", "Neu\u00b7mark", "/", "fah\u00b7re", "fort", "!", "Du", "giebst", "dir", "selbst", "die", "Spo\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dr\u00fcm hat Apollo dich und Orpheus au\u00dferkohren;", "tokens": ["Dr\u00fcm", "hat", "A\u00b7pol\u00b7lo", "dich", "und", "Or\u00b7pheus", "au\u00b7\u00dfer\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PPER", "KON", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Sternenvolle Burg weist dir ein gr\u00f6\u00dfres Pfand/", "tokens": ["Die", "Ster\u00b7nen\u00b7vol\u00b7le", "Burg", "weist", "dir", "ein", "gr\u00f6\u00df\u00b7res", "Pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gl\u00fck zu! Sie schl\u00e4gt dir noch den Doctor in die Hand.", "tokens": ["Gl\u00fck", "zu", "!", "Sie", "schl\u00e4gt", "dir", "noch", "den", "Doc\u00b7tor", "in", "die", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}