{"textgrid.poem.57313": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du, der des Herschers Weg zur Unsterblichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, der des Herschers Weg zur Unsterblichkeit", "tokens": ["Du", ",", "der", "des", "Her\u00b7schers", "Weg", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mit scharfem Blick sah; aber der Weg' auch viel", "tokens": ["Mit", "schar\u00b7fem", "Blick", "sah", ";", "a\u00b7ber", "der", "Weg'", "auch", "viel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht sah, die f\u00fchren durch die grosse,", "tokens": ["Nicht", "sah", ",", "die", "f\u00fch\u00b7ren", "durch", "die", "gros\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oft von Get\u00e4uschten verw\u00fcnschte Irre:", "tokens": ["Oft", "von", "Ge\u00b7t\u00e4uschten", "ver\u00b7w\u00fcnschte", "Ir\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Nicht sahst, dass Deutschlands Dichtkunst sich schnell erhob,", "tokens": ["Nicht", "sahst", ",", "dass", "Deutschlands", "Dicht\u00b7kunst", "sich", "schnell", "er\u00b7hob", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "NE", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus fester Wurzel daurender Stamm, und weit", "tokens": ["Aus", "fes\u00b7ter", "Wur\u00b7zel", "dau\u00b7ren\u00b7der", "Stamm", ",", "und", "weit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der \u00c4ste Schatten warf! doch jetzo", "tokens": ["Der", "\u00c4s\u00b7te", "Schat\u00b7ten", "warf", "!", "doch", "jet\u00b7zo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch es entbehrtest, zum Wuchs den Hainbaum", "tokens": ["Auch", "es", "ent\u00b7behr\u00b7test", ",", "zum", "Wuchs", "den", "Hain\u00b7baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Mit Thau zu frischen: Friedrich, dein Adlerblick", "tokens": ["Mit", "Thau", "zu", "fri\u00b7schen", ":", "Fried\u00b7rich", ",", "dein", "Ad\u00b7ler\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$.", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo war er, da sich regte des Geistes Kraft,", "tokens": ["Wo", "war", "er", ",", "da", "sich", "reg\u00b7te", "des", "Geis\u00b7tes", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "PRF", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Muth, Flamme, alles, dem Belohner", "tokens": ["Muth", ",", "Flam\u00b7me", ",", "al\u00b7les", ",", "dem", "Be\u00b7loh\u00b7ner"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PIS", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6nige seyn, es nicht schaffen k\u00f6nnen?", "tokens": ["K\u00f6\u00b7ni\u00b7ge", "seyn", ",", "es", "nicht", "schaf\u00b7fen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Seyd stolz, auch ihr saht, Dichter, wo durch die Irr'", "tokens": ["Seyd", "stolz", ",", "auch", "ihr", "saht", ",", "Dich\u00b7ter", ",", "wo", "durch", "die", "Irr'"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "PPER", "VVFIN", "$,", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein steiler Pfad ging. Ohne die Frischling, wuchs", "tokens": ["Ein", "stei\u00b7ler", "Pfad", "ging", ".", "Oh\u00b7ne", "die", "Frischling", ",", "wuchs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Hain' es fort, und neue Sprosse", "tokens": ["Im", "Hain'", "es", "fort", ",", "und", "neu\u00b7e", "Spros\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKVZ", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00e4uselten, tauschten von Fr\u00fchlingsl\u00fcften.", "tokens": ["S\u00e4u\u00b7sel\u00b7ten", ",", "tauschten", "von", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Doch kont' auch H\u00f6rer deutsches Gesanges seyn,", "tokens": ["Doch", "kont'", "auch", "H\u00f6\u00b7rer", "deut\u00b7sches", "Ge\u00b7san\u00b7ges", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "NN", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Defs Ohre Zauber war der t\u00fcdeske Reim,", "tokens": ["Defs", "Oh\u00b7re", "Zau\u00b7ber", "war", "der", "t\u00fc\u00b7des\u00b7ke", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Durch den er jetzt des Thrones Launen", "tokens": ["Durch", "den", "er", "jetzt", "des", "Thro\u00b7nes", "Lau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheuchte, und jetzo der Schlacht Gespenster?", "tokens": ["Scheuch\u00b7te", ",", "und", "jet\u00b7zo", "der", "Schlacht", "Ge\u00b7spens\u00b7ter", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Dein Lied nicht sch\u00fctzt dich vor der Vergessenheit;", "tokens": ["Dein", "Lied", "nicht", "sch\u00fctzt", "dich", "vor", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Schirm sind Thaten! Aber des Meisters Werk,", "tokens": ["Dein", "Schirm", "sind", "Tha\u00b7ten", "!", "A\u00b7ber", "des", "Meis\u00b7ters", "Werk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur das bleibt da, wie's ist: in Nebel", "tokens": ["Nur", "das", "bleibt", "da", ",", "wie's", "ist", ":", "in", "Ne\u00b7bel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "ADV", "$,", "PWAV", "VAFIN", "$.", "APPR", "NN"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00fcllt die Geschichte die That des Meisters.", "tokens": ["H\u00fcllt", "die", "Ge\u00b7schich\u00b7te", "die", "That", "des", "Meis\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Mehr tr\u00fcbt der Nebel, wenn, was du thatest, du", "tokens": ["Mehr", "tr\u00fcbt", "der", "Ne\u00b7bel", ",", "wenn", ",", "was", "du", "tha\u00b7test", ",", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["