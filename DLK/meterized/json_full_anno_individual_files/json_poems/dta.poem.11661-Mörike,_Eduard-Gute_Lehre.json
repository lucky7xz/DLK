{"dta.poem.11661": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Gute Lehre", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt ein warmes Regelein,", "tokens": ["Es", "f\u00e4llt", "ein", "war\u00b7mes", "Re\u00b7gel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie duften da die Blumen,", "tokens": ["Wie", "duf\u00b7ten", "da", "die", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Apfelbl\u00fcth' so fein!", "tokens": ["Die", "Ap\u00b7fel\u00b7bl\u00fcth'", "so", "fein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im H\u00e4uselein da dr\u00fcben", "tokens": ["Im", "H\u00e4u\u00b7sel\u00b7ein", "da", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Bauer vespert wohlgemuth,", "tokens": ["Ein", "Bau\u00b7er", "ves\u00b7pert", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat's Fensterlein halb offen,", "tokens": ["Hat's", "Fens\u00b7ter\u00b7lein", "halb", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das L\u00fcftlein th\u00e4t ihm gut.", "tokens": ["Das", "L\u00fcft\u00b7lein", "th\u00e4t", "ihm", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ei, spricht er bei sich selbsten,", "tokens": ["Ei", ",", "spricht", "er", "bei", "sich", "selbs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sonntagsstr\u00e4uschen h\u00e4tt' ich gern,", "tokens": ["Ein", "Sonn\u00b7tags\u00b7str\u00e4u\u00b7schen", "h\u00e4tt'", "ich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf morgen in die Predigt,", "tokens": ["Auf", "mor\u00b7gen", "in", "die", "Pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tulipanen oder Stern.", "tokens": ["Tu\u00b7li\u00b7pa\u00b7nen", "o\u00b7der", "Stern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein V\u00f6glein hat's vernommen,", "tokens": ["Ein", "V\u00f6\u00b7glein", "hat's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das denkt: dir soll geholfen seyn;", "tokens": ["Das", "denkt", ":", "dir", "soll", "ge\u00b7hol\u00b7fen", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t schnell ein Bl\u00fcmlein holen,", "tokens": ["Th\u00e4t", "schnell", "ein", "Bl\u00fcm\u00b7lein", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bringt's im Schn\u00e4belein.", "tokens": ["Und", "bringt's", "im", "Schn\u00e4\u00b7be\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ei, lachte da mein Peter!", "tokens": ["Ei", ",", "lach\u00b7te", "da", "mein", "Pe\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "PPOSAT", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat flugs sein Fenster zugemacht,", "tokens": ["Hat", "flugs", "sein", "Fens\u00b7ter", "zu\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat's V\u00f6gelein gefangen", "tokens": ["Hat's", "V\u00f6\u00b7ge\u00b7lein", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und in den K\u00e4fig bracht.", "tokens": ["Und", "in", "den", "K\u00e4\u00b7fig", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, mu\u00df das V\u00f6glein trauern!", "tokens": ["Ach", ",", "mu\u00df", "das", "V\u00f6\u00b7glein", "trau\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und war auch von der Stunde krank;", "tokens": ["Und", "war", "auch", "von", "der", "Stun\u00b7de", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind feine Kerl die Bauern,", "tokens": ["Sind", "fei\u00b7ne", "Kerl", "die", "Bau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die geben Stank f\u00fcr Dank!", "tokens": ["Die", "ge\u00b7ben", "Stank", "f\u00fcr", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}