{"textgrid.poem.48327": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Einzug", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und siehe da, zum dritten Mal", "tokens": ["Und", "sie\u00b7he", "da", ",", "zum", "drit\u00b7ten", "Mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ziehen sie ein durch das gro\u00dfe Portal;", "tokens": ["Zie\u00b7hen", "sie", "ein", "durch", "das", "gro\u00b7\u00dfe", "Por\u00b7tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Der Kaiser vorauf, die Sonne scheint,", "tokens": ["Der", "Kai\u00b7ser", "vor\u00b7auf", ",", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Alles lacht und alles weint,", "tokens": ["Al\u00b7les", "lacht", "und", "al\u00b7les", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Erst die Garde. Brigaden vier,", "tokens": ["Erst", "die", "Gar\u00b7de", ".", "Bri\u00b7ga\u00b7den", "vier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "NN", "CARD", "$,"], "meter": "+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Garde und Garde-Grenadier':", "tokens": ["Gar\u00b7de", "und", "Gar\u00b7de\u00b7Gre\u00b7na\u00b7dier'", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Elisabether, Alexandriner,", "tokens": ["E\u00b7lis\u00b7a\u00b7be\u00b7ther", ",", "A\u00b7le\u00b7xan\u00b7dri\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Franziskaner, Augustiner,", "tokens": ["Fran\u00b7zis\u00b7ka\u00b7ner", ",", "Au\u00b7gus\u00b7ti\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie nahmen, noch nicht zufrieden mit Chlum,", "tokens": ["Sie", "nah\u00b7men", ",", "noch", "nicht", "zu\u00b7frie\u00b7den", "mit", "Chlum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKNEG", "ADJD", "APPR", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bei Privat ein Privatissimum. \u2013", "tokens": ["Bei", "Pri\u00b7vat", "ein", "Pri\u00b7va\u00b7tis\u00b7si\u00b7mum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit ihnen kommen, geschlossen, gekoppelt,", "tokens": ["Mit", "ih\u00b7nen", "kom\u00b7men", ",", "ge\u00b7schlos\u00b7sen", ",", "ge\u00b7kop\u00b7pelt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die S\u00e4bel in H\u00e4nden, den ", "tokens": ["Die", "S\u00e4\u00b7bel", "in", "H\u00e4n\u00b7den", ",", "den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Die hellblauen Reiter von Mars la Tour,", "tokens": ["Die", "hell\u00b7blau\u00b7en", "Rei\u00b7ter", "von", "Mars", "la", "Tour", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NE", "NE", "$,"], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Aber an Zahl die H\u00e4lfte nur.", "tokens": ["A\u00b7ber", "an", "Zahl", "die", "H\u00e4lf\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Garde vor\u00fcber. \u2013 Garde tritt an:", "tokens": ["Gar\u00b7de", "vor\u00b7\u00fc\u00b7ber", ".", "\u2013", "Gar\u00b7de", "tritt", "an", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "$(", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Regiment des Kaisers, Mann an Mann,", "tokens": ["Re\u00b7gi\u00b7ment", "des", "Kai\u00b7sers", ",", "Mann", "an", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbkein Schu\u00df; Gewehr zur Attacke rechts.\u00ab", "tokens": ["\u00bb", "kein", "Schu\u00df", ";", "Ge\u00b7wehr", "zur", "At\u00b7ta\u00b7cke", "rechts", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "$.", "NN", "APPRART", "NN", "ADV", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Dem einen zur Lust, dem andern zur Qual;", "tokens": ["Dem", "ei\u00b7nen", "zur", "Lust", ",", "dem", "an\u00b7dern", "zur", "Qual", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPRART", "NN", "$,", "PRELS", "PIS", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was von den Turkos noch \u00fcbrig geblieben,", "tokens": ["Was", "von", "den", "Tur\u00b7kos", "noch", "\u00fcb\u00b7rig", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Spricht wohl von einer b\u00f6sen Sieben.", "tokens": ["Spricht", "wohl", "von", "ei\u00b7ner", "b\u00f6\u00b7sen", "Sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Blumen fliegen aus jedem Haus,", "tokens": ["Blu\u00b7men", "flie\u00b7gen", "aus", "je\u00b7dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der Himmel str\u00f6mt lachende Lichter aus,", "tokens": ["Der", "Him\u00b7mel", "str\u00f6mt", "la\u00b7chen\u00b7de", "Lich\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und der Lichtball selber l\u00e4chelt in Wonne:", "tokens": ["Und", "der", "Licht\u00b7ball", "sel\u00b7ber", "l\u00e4\u00b7chelt", "in", "Won\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbes gibt doch noch Neues unter der Sonne.\u00ab", "tokens": ["\u00bb", "es", "gibt", "doch", "noch", "Neu\u00b7es", "un\u00b7ter", "der", "Son\u00b7ne", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADV", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Gewi\u00df. Eben jetzt einschwenkt in das Tor,", "tokens": ["Ge\u00b7wi\u00df", ".", "E\u00b7ben", "jetzt", "ein\u00b7schwenkt", "in", "das", "Tor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Keine Linie zur\u00fcck, keine Linie vor,", "tokens": ["Kei\u00b7ne", "Li\u00b7nie", "zu\u00b7r\u00fcck", ",", "kei\u00b7ne", "Li\u00b7nie", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-++-++-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "En bataillon, frisch wie der Lenz,", "tokens": ["En", "ba\u00b7tail\u00b7lon", ",", "frisch", "wie", "der", "Lenz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die ganze Armee in Double-Essenz.", "tokens": ["Die", "gan\u00b7ze", "Ar\u00b7mee", "in", "Dou\u00b7ble\u00b7Es\u00b7senz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Korps bedeutet jeder Zug,", "tokens": ["Ein", "Korps", "be\u00b7deu\u00b7tet", "je\u00b7der", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das ist kein Schreiten, das ist wie Flug,", "tokens": ["Das", "ist", "kein", "Schrei\u00b7ten", ",", "das", "ist", "wie", "Flug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PDS", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das macht, weil ihnen ungesehn", "tokens": ["Das", "macht", ",", "weil", "ih\u00b7nen", "un\u00b7ge\u00b7sehn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dreihundert Fahnen zu H\u00e4upten wehn.", "tokens": ["Drei\u00b7hun\u00b7dert", "Fah\u00b7nen", "zu", "H\u00e4up\u00b7ten", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Bunt gew\u00fcrfelt Preu\u00dfen, Hessen,", "tokens": ["Bunt", "ge\u00b7w\u00fcr\u00b7felt", "Preu\u00b7\u00dfen", ",", "Hes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bayern und Baden nicht zu vergessen,", "tokens": ["Bay\u00b7ern", "und", "Ba\u00b7den", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sachsen, Schwaben, J\u00e4ger, Sch\u00fctzen,", "tokens": ["Sach\u00b7sen", ",", "Schwa\u00b7ben", ",", "J\u00e4\u00b7ger", ",", "Sch\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pickelhauben und Helme und M\u00fctzen,", "tokens": ["Pi\u00b7ckel\u00b7hau\u00b7ben", "und", "Hel\u00b7me", "und", "M\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Das Eiserne Kreuz ihre einzige Zier;", "tokens": ["Das", "Ei\u00b7ser\u00b7ne", "Kreuz", "ih\u00b7re", "ein\u00b7zi\u00b7ge", "Zier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Alles zerschossen; ihr ganzes Prahlen", "tokens": ["Al\u00b7les", "zer\u00b7schos\u00b7sen", ";", "ihr", "gan\u00b7zes", "Prah\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVPP", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Nur ein Wettstreit in den Zahlen,", "tokens": ["Nur", "ein", "Wett\u00b7streit", "in", "den", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den Zahlen derer, die ", "tokens": ["In", "den", "Zah\u00b7len", "de\u00b7rer", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "PDS", "$,", "PRELS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Zum dritten Mal", "tokens": ["Zum", "drit\u00b7ten", "Mal"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ziehen sie ein durch das gro\u00dfe Portal;", "tokens": ["Zie\u00b7hen", "sie", "ein", "durch", "das", "gro\u00b7\u00dfe", "Por\u00b7tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Die Linden hinauf erdr\u00f6hnt ihr Schritt,", "tokens": ["Die", "Lin\u00b7den", "hin\u00b7auf", "er\u00b7dr\u00f6hnt", "ihr", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Preu\u00dfen-Deutschland f\u00fchlt ihn mit.", "tokens": ["Preu\u00b7\u00dfen\u00b7Deut\u00b7schland", "f\u00fchlt", "ihn", "mit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hunderttausende auf den Zehenspitzen!", "tokens": ["Hun\u00b7dert\u00b7tau\u00b7sen\u00b7de", "auf", "den", "Ze\u00b7hen\u00b7spit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vor\u00fcber, wo Einarm und Stelzfu\u00df sitzen,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "wo", "Ei\u00b7narm", "und", "Stelz\u00b7fu\u00df", "sit\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Jedem Stelzfu\u00df bis in sein Bein von Holz", "tokens": ["Je\u00b7dem", "Stelz\u00b7fu\u00df", "bis", "in", "sein", "Bein", "von", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00e4hrt der alte Schlachtenstolz.", "tokens": ["F\u00e4hrt", "der", "al\u00b7te", "Schlach\u00b7ten\u00b7stolz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Halt,", "tokens": ["Halt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Vor des Gro\u00dfen K\u00f6nigs ernster Gestalt.", "tokens": ["Vor", "des", "Gro\u00b7\u00dfen", "K\u00f6\u00b7nigs", "erns\u00b7ter", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.9": {"line.1": {"text": "Bei dem Fritzen-Denkmal stehen sie wieder,", "tokens": ["Bei", "dem", "Frit\u00b7zen\u00b7Denk\u00b7mal", "ste\u00b7hen", "sie", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie blicken hinauf, der Alte blickt nieder;", "tokens": ["Sie", "bli\u0