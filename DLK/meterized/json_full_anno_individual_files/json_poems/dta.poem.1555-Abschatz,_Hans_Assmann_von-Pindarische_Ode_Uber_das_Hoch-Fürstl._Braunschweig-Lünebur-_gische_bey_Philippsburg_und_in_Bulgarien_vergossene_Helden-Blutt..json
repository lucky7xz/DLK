{"dta.poem.1555": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Pindarische Ode  \n Uber das Hoch-F\u00fcrstl. Braunschweig-L\u00fcnebur-  \n gische bey Philippsburg und in Bulgarien  \n vergossene Helden-Blutt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Theurer Stamm von alten Helden/ der f\u00fcr Deutschlands\nFreyheit wacht/ ", "tokens": ["Theu\u00b7rer", "Stamm", "von", "al\u00b7ten", "Hel\u00b7den", "/", "der", "f\u00fcr", "Deutschlands", "Frey\u00b7heit", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ADJA", "NN", "$(", "ART", "APPR", "NE", "NN", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Der vermeinten Welt-Bezwinger Macht und List zu nichte", "tokens": ["Der", "ver\u00b7mein\u00b7ten", "Welt\u00b7Be\u00b7zwin\u00b7ger", "Macht", "und", "List", "zu", "nich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "APPR", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Wann dein k\u00fchnes F\u00fcrsten-Blutt f\u00fcr das Heyl der Christen-", "tokens": ["Wann", "dein", "k\u00fch\u00b7nes", "F\u00fcrs\u00b7ten\u00b7Blutt", "f\u00fcr", "das", "Heyl", "der", "Chris\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "F\u00fcr des Rhein-Stroms Sicherheit k\u00e4mpffend in den Sand", "tokens": ["F\u00fcr", "des", "Rhein\u00b7Stroms", "Si\u00b7cher\u00b7heit", "k\u00e4mpf\u00b7fend", "in", "den", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Soll man dir mehr hei\u00dfe Z\u00e4hren", "tokens": ["Soll", "man", "dir", "mehr", "hei\u00b7\u00dfe", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder Ruhm und Danck gew\u00e4hren?", "tokens": ["O\u00b7der", "Ruhm", "und", "Danck", "ge\u00b7w\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Gro\u00dfmutt ist zu gro\u00df unsre Klagen zu verlangen:", "tokens": ["Dei\u00b7ne", "Gro\u00df\u00b7mutt", "ist", "zu", "gro\u00df", "uns\u00b7re", "Kla\u00b7gen", "zu", "ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht Cypressen/ Lorbern nur/ solln bey deiner Trauer prangen.", "tokens": ["Nicht", "Cyp\u00b7res\u00b7sen", "/", "Lor\u00b7bern", "nur", "/", "solln", "bey", "dei\u00b7ner", "Trau\u00b7er", "pran\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$(", "NN", "ADV", "$(", "VMFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Unsre Wehmutt ist zu schwach/ unsre Thr\u00e4nen zu geringe/", "tokens": ["Uns\u00b7re", "Weh\u00b7mutt", "ist", "zu", "schwach", "/", "uns\u00b7re", "Thr\u00e4\u00b7nen", "zu", "ge\u00b7rin\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$(", "PPOSAT", "NN", "APPR", "ADJA", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df man f\u00fcr so theures Blutt sie zum treuen Opffer bringe:", "tokens": ["Da\u00df", "man", "f\u00fcr", "so", "theu\u00b7res", "Blutt", "sie", "zum", "treu\u00b7en", "Opf\u00b7fer", "brin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADV", "ADJA", "ADJD", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Drum wir mit erstarrtem Schweigen", "tokens": ["Drum", "wir", "mit", "er\u00b7starr\u00b7tem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "APPR", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Unsre Traurigkeit bezeigen.", "tokens": ["Uns\u00b7re", "Trau\u00b7rig\u00b7keit", "be\u00b7zei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ertz und Jaspis kan den Helden f\u00fcr ein Grab-Mahl nicht be-", "tokens": ["Ertz", "und", "Jas\u00b7pis", "kan", "den", "Hel\u00b7den", "f\u00fcr", "ein", "Grab\u00b7Mahl", "nicht", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Purpur-Dint\u2019 und goldne Farben sind zu bla\u00df sie zu erh\u00f6hn/", "tokens": ["Pur\u00b7pur\u00b7Dint'", "und", "gold\u00b7ne", "Far\u00b7ben", "sind", "zu", "bla\u00df", "sie", "zu", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "VAFIN", "PTKA", "ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schwanen-Federn sind zu hart/ und zu heischer unser Mund/", "tokens": ["Schwa\u00b7nen\u00b7Fe\u00b7dern", "sind", "zu", "hart", "/", "und", "zu", "hei\u00b7scher", "un\u00b7ser", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKA", "ADJD", "$(", "KON", "APPR", "ADJA", "PPOSAT", "NN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ihr Verdienst und unser Leyd nach Geb\u00fchr zu machen kund/", "tokens": ["Ihr", "Ver\u00b7dienst", "und", "un\u00b7ser", "Leyd", "nach", "Ge\u00b7b\u00fchr", "zu", "ma\u00b7chen", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "PTKVZ", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Beydes bleibt ins Hertz gepr\u00e4get", "tokens": ["Bey\u00b7des", "bleibt", "ins", "Hertz", "ge\u00b7pr\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Nachwelt beygeleget.", "tokens": ["Und", "der", "Nach\u00b7welt", "bey\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}