{"textgrid.poem.60527": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Rezensent", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich las in seinem Buche viel Frivoles,", "tokens": ["Ich", "las", "in", "sei\u00b7nem", "Bu\u00b7che", "viel", "Fri\u00b7vo\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheinbar Verst\u00e4ndiges und witzig Hohles,", "tokens": ["Schein\u00b7bar", "Ver\u00b7st\u00e4n\u00b7di\u00b7ges", "und", "wit\u00b7zig", "Hoh\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich sah ihn seine Richtermiene schneiden,", "tokens": ["Ich", "sah", "ihn", "sei\u00b7ne", "Rich\u00b7ter\u00b7mie\u00b7ne", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah ihn f\u00fchren spitzige Lanzetten,", "tokens": ["Ich", "sah", "ihn", "f\u00fch\u00b7ren", "spit\u00b7zi\u00b7ge", "Lan\u00b7zet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit ekler Lust Skandale auszuweiden,", "tokens": ["Mit", "ek\u00b7ler", "Lust", "Skan\u00b7da\u00b7le", "aus\u00b7zu\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Heliogabal\u00e4isch Formen kneten.", "tokens": ["He\u00b7lio\u00b7ga\u00b7ba\u00b7l\u00e4\u00b7isch", "For\u00b7men", "kne\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ich sah ihn Unrat sammeln in Retorten,", "tokens": ["Ich", "sah", "ihn", "Un\u00b7rat", "sam\u00b7meln", "in", "Re\u00b7tor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er sublimierte ihn zu scharfen Witzen,", "tokens": ["Er", "sub\u00b7li\u00b7mier\u00b7te", "ihn", "zu", "schar\u00b7fen", "Wit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Am Boden blieb nach schnellverdampften Worten", "tokens": ["Am", "Bo\u00b7den", "blieb", "nach", "schnell\u00b7ver\u00b7dampf\u00b7ten", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als \u203acaput mortuum\u2039 die Ehre sitzen", "tokens": ["Als", "\u203a", "ca\u00b7put", "mor\u00b7tu\u00b7um", "\u2039", "die", "Eh\u00b7re", "sit\u00b7zen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "FM", "FM", "FM", "$(", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich las in seinem Buche viel Frivoles,", "tokens": ["Ich", "las", "in", "sei\u00b7nem", "Bu\u00b7che", "viel", "Fri\u00b7vo\u00b7les", ","], "toke