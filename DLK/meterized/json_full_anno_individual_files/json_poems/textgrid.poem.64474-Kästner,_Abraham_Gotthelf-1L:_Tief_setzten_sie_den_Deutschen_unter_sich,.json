{"textgrid.poem.64474": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief setzten sie den Deutschen unter sich,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief setzten sie den Deutschen unter sich,", "tokens": ["Tief", "setz\u00b7ten", "sie", "den", "Deut\u00b7schen", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der eitle Gallier, der ungez\u00e4hmte Brite;", "tokens": ["Der", "eit\u00b7le", "Gal\u00b7lier", ",", "der", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "Bri\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Germanien, das l\u00e4ngst den Spott zu schl\u00e4frig litte,", "tokens": ["Ger\u00b7ma\u00b7ni\u00b7en", ",", "das", "l\u00e4ngst", "den", "Spott", "zu", "schl\u00e4f\u00b7rig", "lit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ART", "NN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zeigt beyden seinen ", "tokens": ["Zeigt", "bey\u00b7den", "sei\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Feind verschm\u00e4ht ihn erst, bis er, zu stark belehrt,", "tokens": ["Der", "Feind", "ver\u00b7schm\u00e4ht", "ihn", "erst", ",", "bis", "er", ",", "zu", "stark", "be\u00b7lehrt", ","], "token_info": [