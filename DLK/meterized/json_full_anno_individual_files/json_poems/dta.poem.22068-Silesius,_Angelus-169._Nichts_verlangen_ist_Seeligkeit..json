{"dta.poem.22068": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "169. Nichts verlangen ist Seeligkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Die Heilgen sind darumb mit Gottes ruh umbfangen/", "tokens": ["Die", "Heil\u00b7gen", "sind", "da\u00b7rumb", "mit", "Got\u00b7tes", "ruh", "umb\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und haben Seeligkeit/ weil sie nach nichts verlangen.", "tokens": ["Und", "ha\u00b7ben", "See\u00b7lig\u00b7keit", "/", "weil", "sie", "nach", "nichts", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$(", "KOUS", "PPER", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}