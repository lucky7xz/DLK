i"}, "line.2": {"text": "Der Strom Gebirge trennt,", "tokens": ["Der", "Strom", "Ge\u00b7bir\u00b7ge", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weht oft sein Laut her\u00fcber,", "tokens": ["Weht", "oft", "sein", "Laut", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den meine Seele kennt;", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "kennt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich den Fels erklimme,", "tokens": ["Wenn", "ich", "den", "Fels", "er\u00b7klim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den noch kein Fu\u00df erreicht,", "tokens": ["Den", "noch", "kein", "Fu\u00df", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lausch' ich nach jener Stimme;", "tokens": ["Lausch'", "ich", "nach", "je\u00b7ner", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch Kluft und Echo schweigt.", "tokens": ["Doch", "Kluft", "und", "E\u00b7cho", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo durch die Nacht der Fichten", "tokens": ["Wo", "durch", "die", "Nacht", "der", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein D\u00e4mm'rungsflimmer wallt,", "tokens": ["Ein", "D\u00e4m\u00b7m'\u00b7rungs\u00b7flim\u00b7mer", "wallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seh' ich dich z\u00f6gernd fl\u00fcchten,", "tokens": ["Seh'", "ich", "dich", "z\u00f6\u00b7gernd", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geliebte Luftgestalt!", "tokens": ["Ge\u00b7lieb\u00b7te", "Luft\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn, sanft dir nachzulangen,", "tokens": ["Wenn", ",", "sanft", "dir", "nach\u00b7zu\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sehnsucht Arm sich hebt,", "tokens": ["Der", "Sehn\u00b7sucht", "Arm", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist dein Phantom zergangen,", "tokens": ["Ist", "dein", "Phan\u00b7tom", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Tauged\u00fcft verschwebt.", "tokens": ["