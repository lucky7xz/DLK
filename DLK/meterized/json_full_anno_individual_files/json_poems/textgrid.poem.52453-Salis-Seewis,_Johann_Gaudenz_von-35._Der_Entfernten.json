{"textgrid.poem.52453": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "35. Der Entfernten", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl denk' ich allenthalben,", "tokens": ["Wohl", "denk'", "ich", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O du Entfernte, dein!", "tokens": ["O", "du", "Ent\u00b7fern\u00b7te", ",", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fr\u00fch, wenn die Wolken falben,", "tokens": ["Fr\u00fch", ",", "wenn", "die", "Wol\u00b7ken", "fal\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und sp\u00e4t im Sternenschein.", "tokens": ["Und", "sp\u00e4t", "im", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Grund des Morgengoldes,", "tokens": ["Im", "Grund", "des", "Mor\u00b7gen\u00b7gol\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im roten Abendlicht,", "tokens": ["Im", "ro\u00b7ten", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umschwebst du mich, o holdes,", "tokens": ["Um\u00b7schwebst", "du", "mich", ",", "o", "hol\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Geliebtes Traumgesicht!", "tokens": ["Ge\u00b7lieb\u00b7tes", "Traum\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es folgt in alle Weite", "tokens": ["Es", "folgt", "in", "al\u00b7le", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein trautes Bild mir nach,", "tokens": ["Dein", "trau\u00b7tes", "Bild", "mir", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wallt mir stets zur Seite,", "tokens": ["Es", "wallt", "mir", "stets", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Tr\u00e4umen oder wach;", "tokens": ["In", "Tr\u00e4u\u00b7men", "o\u00b7der", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn L\u00fcfte sanft bestreifen", "tokens": ["Wenn", "L\u00fcf\u00b7te", "sanft", "be\u00b7strei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der See beschilften Strand,", "tokens": ["Der", "See", "be\u00b7schilf\u00b7ten", "Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Umfl\u00fcstern mich die Schleifen", "tokens": ["Um\u00b7fl\u00fcs\u00b7tern", "mich", "die", "Schlei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von seinem Busenband.", "tokens": ["Von", "sei\u00b7nem", "Bu\u00b7sen\u00b7band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Abglanz seines Schleiers", "tokens": ["Ein", "Ab\u00b7glanz", "sei\u00b7nes", "Schlei\u00b7ers"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Scheint auf die Saat gewebt;", "tokens": ["Scheint", "auf", "die", "Saat", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Hauch, was des Gem\u00e4uers", "tokens": ["Sein", "Hauch", ",", "was", "des", "Ge\u00b7m\u00e4u\u00b7ers"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bewegten Eppich hebt;", "tokens": ["Be\u00b7weg\u00b7ten", "Ep\u00b7pich", "hebt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Kleidung weiche Falten,", "tokens": ["Der", "Klei\u00b7dung", "wei\u00b7che", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geformt aus Glanz und Duft,", "tokens": ["Ge\u00b7formt", "aus", "Glanz", "und", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entschwinden in den Spalten", "tokens": ["Ent\u00b7schwin\u00b7den", "in", "den", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der \u00f6den Felsenkluft.", "tokens": ["Der", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7kluft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wo rauschender und tr\u00fcber", "tokens": ["Wo", "rau\u00b7schen\u00b7der", "und", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Strom Gebirge trennt,", "tokens": ["Der", "Strom", "Ge\u00b7bir\u00b7ge", "trennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weht oft sein Laut her\u00fcber,", "tokens": ["Weht", "oft", "sein", "Laut", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den meine Seele kennt;", "tokens": ["Den", "mei\u00b7ne", "See\u00b7le", "kennt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich den Fels erklimme,", "tokens": ["Wenn", "ich", "den", "Fels", "er\u00b7klim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den noch kein Fu\u00df erreicht,", "tokens": ["Den", "noch", "kein", "Fu\u00df", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lausch' ich nach jener Stimme;", "tokens": ["Lausch'", "ich", "nach", "je\u00b7ner", "Stim\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch Kluft und Echo schweigt.", "tokens": ["Doch", "Kluft", "und", "E\u00b7cho", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wo durch die Nacht der Fichten", "tokens": ["Wo", "durch", "die", "Nacht", "der", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein D\u00e4mm'rungsflimmer wallt,", "tokens": ["Ein", "D\u00e4m\u00b7m'\u00b7rungs\u00b7flim\u00b7mer", "wallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seh' ich dich z\u00f6gernd fl\u00fcchten,", "tokens": ["Seh'", "ich", "dich", "z\u00f6\u00b7gernd", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geliebte Luftgestalt!", "tokens": ["Ge\u00b7lieb\u00b7te", "Luft\u00b7ge\u00b7stalt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn, sanft dir nachzulangen,", "tokens": ["Wenn", ",", "sanft", "dir", "nach\u00b7zu\u00b7lan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Sehnsucht Arm sich hebt,", "tokens": ["Der", "Sehn\u00b7sucht", "Arm", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist dein Phantom zergangen,", "tokens": ["Ist", "dein", "Phan\u00b7tom", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie Tauged\u00fcft verschwebt.", "tokens": ["Wie", "Tau\u00b7ge\u00b7d\u00fcft", "ver\u00b7schwebt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl denk' ich allenthalben,", "tokens": ["Wohl", "denk'", "ich", "al\u00b7len\u00b7thal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O du Entfernte, dein!", "tokens": ["O", "du", "Ent\u00b7fern\u00b7te", ",", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fr\u00fch, wenn die Wolken falben,", "tokens": ["Fr\u00fch", ",", "wenn", "die", "Wol\u00b7ken", "fal\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "