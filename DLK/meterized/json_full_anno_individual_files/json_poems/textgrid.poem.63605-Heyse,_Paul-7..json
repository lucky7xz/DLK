ord", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Seh' ich den Knaben aus den D\u00e4mmernissen", "tokens": ["Seh'", "ich", "den", "Kna\u00b7ben", "aus", "den", "D\u00e4m\u00b7mer\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Der Nacht mir winken mit der kleinen Hand,", "tokens": ["Der", "Nacht", "mir", "win\u00b7ken", "mit", "der", "klei\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und lautaufweinend sink' ich in die Kissen.", "tokens": ["Und", "laut\u00b7auf\u00b7wei\u00b7nend", "sink'", "ich", "in", "die", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}