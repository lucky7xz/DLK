, "line.11": {"text": "Hin ging er, wie der holde Fr\u00fchling scheidet,", "tokens": ["Hin", "ging", "er", ",", "wie", "der", "hol\u00b7de", "Fr\u00fch\u00b7ling", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Von sch\u00fcchtern zartem Jugendglanz verkl\u00e4rt.", "tokens": ["Von", "sch\u00fcch\u00b7tern", "zar\u00b7tem", "Ju\u00b7gend\u00b7glanz", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ist das ein Ende nicht, das jeder neidet?", "tokens": ["Ist", "das", "ein", "En\u00b7de", "nicht", ",", "das", "je\u00b7der", "nei\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "PTKNEG", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ist's nicht ein frevelnd eigens\u00fccht'ger Gram", "tokens": ["Ist's", "nicht", "ein", "fre\u00b7velnd", "ei\u00b7gen\u00b7s\u00fccht'\u00b7ger", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Um den, der ewig keinen Kummer leidet? \u2013", "tokens": ["Um", "den", ",", "der", "e\u00b7wig", "kei\u00b7nen", "Kum\u00b7mer", "lei\u00b7det", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "$,", "PRELS", "ADJD", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Doch wenn die Nacht mit ihren S