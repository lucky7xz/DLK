{"textgrid.poem.36486": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Kr\u00e4he und die Bachstelze", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bachstelze, geh doch nicht so wacklich, sprach die Kr\u00e4he,", "tokens": ["Bach\u00b7stel\u00b7ze", ",", "geh", "doch", "nicht", "so", "wack\u00b7lich", ",", "sprach", "die", "Kr\u00e4\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich, liebe Kr\u00e4he, sprach das V\u00f6gelchen, ich gehe", "tokens": ["Ich", ",", "lie\u00b7be", "Kr\u00e4\u00b7he", ",", "sprach", "das", "V\u00f6\u00b7gel\u00b7chen", ",", "ich", "ge\u00b7he"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach Weisheit nicht, ich w\u00fcrde krank;", "tokens": ["Nach", "Weis\u00b7heit", "nicht", ",", "ich", "w\u00fcr\u00b7de", "krank", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich gehe meinem Hang", "tokens": ["Ich", "ge\u00b7he", "mei\u00b7nem", "Hang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu kleinen Freuden nach, und sehe", "tokens": ["Zu", "klei\u00b7nen", 