{"textgrid.poem.36377": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6. Christus im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei finstrer Nacht, zur ersten Wacht,", "tokens": ["Bei", "finst\u00b7rer", "Nacht", ",", "zur", "ers\u00b7ten", "Wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein' Stimm' begunt' zu klagen;", "tokens": ["Ein'", "Stimm'", "beg\u00b7unt'", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich nahm in Acht, was sie da sagt,", "tokens": ["Ich", "nahm", "in", "Acht", ",", "was", "sie", "da", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welch Leid sie mu\u00dfte tragen.", "tokens": ["Welch", "Leid", "sie", "mu\u00df\u00b7te", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein g\u00f6ttlich Blut, gebeugt im Mut,", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Blut", ",", "ge\u00b7beugt", "im", "Mut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlassen von Gef\u00e4hrten;", "tokens": ["Ver\u00b7las\u00b7sen", "von", "Ge\u00b7f\u00e4hr\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In gro\u00dfer Not, rang mit dem Tod,", "tokens": ["In", "gro\u00b7\u00dfer", "Not", ",", "rang", "mit", "dem", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Garten lag zur Erden.", "tokens": ["Im", "Gar\u00b7ten", "lag", "zur", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es war der liebe Gottes Sohn,", "tokens": ["Es", "war", "der", "lie\u00b7be", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Haupt gesenkt in Armen;", "tokens": ["Sein", "Haupt", "ge\u00b7senkt", "in", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel wei\u00df' und bleicher als der Mond,", "tokens": ["Viel", "wei\u00df'", "und", "blei\u00b7cher", "als", "der", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wen m\u00f6cht' es nicht erbarmen!", "tokens": ["Wen", "m\u00f6cht'", "es", "nicht", "er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach Vater, liebster Vater mein,", "tokens": ["Ach", "Va\u00b7ter", ",", "liebs\u00b7ter", "Va\u00b7ter", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df ich den Kelch dann trinken?", "tokens": ["Mu\u00df", "ich", "den", "Kelch", "dann", "trin\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mag es dann nicht anders sein,", "tokens": ["Und", "mag", "es", "dann", "nicht", "an\u00b7ders", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df meine Seel' nicht sinken!", "tokens": ["La\u00df", "mei\u00b7ne", "Seel'", "nicht", "sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Geliebtes Kind, leer ihn geschwind,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "leer", "ihn", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt er den Vater sagen;", "tokens": ["H\u00f6rt", "er", "den", "Va\u00b7ter", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei stark gesinnt, bald \u00fcberwindt,", "tokens": ["Sei", "stark", "ge\u00b7sinnt", ",", "bald", "\u00fc\u00b7berw\u00b7indt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst das Leiden tragen.", "tokens": ["Du", "wirst", "das", "Lei\u00b7den", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach Vater mein, mu\u00df es so sein,", "tokens": ["Ach", "Va\u00b7ter", "mein", ",", "mu\u00df", "es", "so", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPOSAT", "$,", "VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "So will ich's mutvoll wagen,", "tokens": ["So", "will", "ich's", "mut\u00b7voll", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will trinken rein, den Kelch allein,", "tokens": ["Will", "trin\u00b7ken", "rein", ",", "den", "Kelch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADJD", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir kann ich's nicht versagen.", "tokens": ["Dir", "kann", "ich's", "nicht", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch Sinn und Mut, erschrecken tut.", "tokens": ["Doch", "Sinn", "und", "Mut", ",", "er\u00b7schre\u00b7cken", "tut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich mein Leben lassen?", "tokens": ["Soll", "ich", "mein", "Le\u00b7ben", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O bittrer Tod! dein' Angst und Not", "tokens": ["O", "bit\u00b7trer", "Tod", "!", "dein'", "Angst", "und", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist gro\u00df ohn' alle Ma\u00dfen.", "tokens": ["Ist", "gro\u00df", "ohn'", "al\u00b7le", "Ma\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Jungfrau zart, die mich gebar,", "tokens": ["O", "Jung\u00b7frau", "zart", ",", "die", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,", "PRELS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sollst du mein' Leiden wissen?", "tokens": ["Sollst", "du", "mein'", "Lei\u00b7den", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ach so hart, f\u00fcrwahr, f\u00fcrwahr,", "tokens": ["Die", "ach", "so", "hart", ",", "f\u00fcr\u00b7wahr", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "ADV", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz dir w\u00e4r' zerrissen.", "tokens": ["Das", "Herz", "dir", "w\u00e4r'", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ach Mutter mein, siehst du die Pein,", "tokens": ["Ach", "Mut\u00b7ter", "mein", ",", "siehst", "du", "die", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPOSAT", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wird dir das Herz zerspringen;", "tokens": ["Wird", "dir", "das", "Herz", "zer\u00b7sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die herbe Pein, mich nehmen ein,", "tokens": ["Die", "her\u00b7be", "Pein", ",", "mich", "neh\u00b7men", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tod und Marter ringen.", "tokens": ["Mit", "Tod", "und", "Mar\u00b7ter", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ade, Ade, zu guter Nacht,", "tokens": ["A\u00b7de", ",", "A\u00b7de", ",", "zu", "gu\u00b7ter", "Nacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Maria, Mutter milde;", "tokens": ["Ma\u00b7ria", ",", "Mut\u00b7ter", "mil\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist niemand da, der mit mir wacht,", "tokens": ["Ist", "nie\u00b7mand", "da", ",", "der", "mit", "mir", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser W\u00fcsten wilde?", "tokens": ["In", "die\u00b7ser", "W\u00fcs\u00b7ten", "wil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Kreuz mir vor den Augen schwebt,", "tokens": ["Ein", "Kreuz", "mir", "vor", "den", "Au\u00b7gen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O weh der bittern Schmerzen!", "tokens": ["O", "weh", "der", "bit\u00b7tern", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dran soll ich morgen sein erhebt,", "tokens": ["Dran", "soll", "ich", "mor\u00b7gen", "sein", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VAINF", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Tief schmerzt es mich im Herzen.", "tokens": ["Tief", "schmerzt", "es", "mich", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der sch\u00f6ne Mond will untergehn,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mond", "will", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Leid kann er nicht scheinen;", "tokens": ["F\u00fcr", "Leid", "kann", "er", "nicht", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sternlein stehn, ohn' Flimmern sehn,", "tokens": ["Die", "Stern\u00b7lein", "stehn", ",", "ohn'", "Flim\u00b7mern", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir sie wollen weinen.", "tokens": ["Mit", "mir", "sie", "wol\u00b7len", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kein Vogelsang, kein Freudenklang,", "tokens": ["Kein", "Vo\u00b7gel\u00b7sang", ",", "kein", "Freu\u00b7den\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man h\u00f6ret in den L\u00fcften;", "tokens": ["Man", "h\u00f6\u00b7ret", "in", "den", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die wilden Tier', trauren mit mir,", "tokens": ["Die", "wil\u00b7den", "Tier'", ",", "trau\u00b7ren", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In Steinen und in Kl\u00fcften.", "tokens": ["In", "Stei\u00b7nen", "und", "in", "Kl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Bei finstrer Nacht, zur ersten Wacht,", "tokens": ["Bei", "finst\u00b7rer", "Nacht", ",", "zur", "ers\u00b7ten", "Wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein' Stimm' begunt' zu klagen;", "tokens": ["Ein'", "Stimm'", "beg\u00b7unt'", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich nahm in Acht, was sie da sagt,", "tokens": ["Ich", "nahm", "in", "Acht", ",", "was", "sie", "da", "sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welch Leid sie mu\u00dfte tragen.", "tokens": ["We