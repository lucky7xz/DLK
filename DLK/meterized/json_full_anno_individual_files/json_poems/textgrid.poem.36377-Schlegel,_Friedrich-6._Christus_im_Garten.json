, "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Tod und Marter ringen.", "tokens": ["Mit", "Tod", "und", "Mar\u00b7ter", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ade, Ade, zu guter Nacht,", "tokens": ["A\u00b7de", ",", "A\u00b7de", ",", "zu", "gu\u00b7ter", "Nacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Maria, Mutter milde;", "tokens": ["Ma\u00b7ria", ",", "Mut\u00b7ter", "mil\u00b7de", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist niemand da, der mit mir wacht,", "tokens": ["Ist", "nie\u00b7mand", "da", ",", "der", "mit", "mir", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser W\u00fcsten wilde?", "tokens": ["In", "die\u00b7ser", "W\u00fcs\u00b7ten", "wil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Ein Kreuz mir vor den Augen schwebt,", "tokens": ["Ein", "Kreuz", "mir", "vor", "den", "Au\u00b7gen", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O weh der bittern Schmerzen!", "tokens": ["O", "weh", "der", "bit\u00b7tern", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dran soll ich morgen sein erhebt,", "tokens": ["Dran", "soll", "ich", "mor\u00b7gen", "sein", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VAINF", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Tief schmerzt es mich im Herzen.", "tokens": ["Tief", "schmerzt", "es", "mich", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Der sch\u00f6ne Mond will untergehn,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Mond", "will", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Leid kann er nicht scheinen;", "tokens": ["F\u00fcr", "Leid", "kann", "er", "nicht", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sternlein stehn, ohn' Flimmern sehn,", "tokens": ["Die", "Stern\u00b7lein", "stehn", ",", "ohn'", "Flim\u00b7mern", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir sie wollen weinen.", "tokens": ["Mit", "mir", "sie", "wol\u00b7len", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Kein Vogelsang, kein Freudenklang,", "tokens": ["Kein", "Vo\u00b7gel\u00b7sang", ",", "kein", "Freu\u00b7den\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man h\u00f6ret in den L\u00fcften;", "tokens": ["Man", "h\u00f6\u00b7ret", "in", "den", "L\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die wilden Tier', trauren mit mir,", "tokens": ["Die", "wil\u00b7den", "Tier'", ",", "trau\u00b7ren", "mit", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In Steinen und in Kl\u00fcften.", "tokens": ["In", "Stei\u00b7nen", "und", "in", "Kl\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}