{"textgrid.poem.37634": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Drei-K\u00f6nigs-Fest", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so lang' gesprochen,", "tokens": ["Es", "ist", "so", "lang'", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bald ein Aufruhr sei,", "tokens": ["Da\u00df", "bald", "ein", "Auf\u00b7ruhr", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum ist er ausgebrochen,", "tokens": ["Drum", "ist", "er", "aus\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dem erstickten Geschrei,", "tokens": ["Von", "dem", "er\u00b7stick\u00b7ten", "Ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Pulverthurm sprang auf,", "tokens": ["Der", "Pul\u00b7ver\u00b7thurm", "sprang", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Fenster klirrten drauf.", "tokens": ["Die", "Fens\u00b7ter", "klirr\u00b7ten", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer kann den Herzog hindern,", "tokens": ["Wer", "kann", "den", "Her\u00b7zog", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er z\u00fcndet wie ein Blitz,", "tokens": ["Er", "z\u00fcn\u00b7det", "wie", "ein", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig mit den Kindern", "tokens": ["Der", "K\u00f6\u00b7nig", "mit", "den", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zieht in des Reiches Spitz,", "tokens": ["Zieht", "in", "des", "Rei\u00b7ches", "Spitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Herzog nah dem Thor", "tokens": ["Der", "Her\u00b7zog", "nah", "dem", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Steht einsam schon davor.", "tokens": ["Steht", "ein\u00b7sam", "schon", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des K\u00f6nigs Garden singen,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Gar\u00b7den", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er horchet immer zu,", "tokens": ["Er", "hor\u00b7chet", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drei-K\u00f6nigs-Lied sie bringen,", "tokens": ["Drei\u00b7K\u00f6\u00b7nigs\u00b7Lied", "sie", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchziehn die Stadt in Ruh,", "tokens": ["Durch\u00b7ziehn", "die", "Stadt", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Herzog denkt sich schon,", "tokens": ["Der", "Her\u00b7zog", "denkt", "sich", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie er da zieht zum Thron.", "tokens": ["Wie", "er", "da", "zieht", "zum", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die heiligen drei K\u00f6nig,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "drei", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ziehen auch vor's Thor,", "tokens": ["Die", "zie\u00b7hen", "auch", "vor's", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wundern sich nicht wenig,", "tokens": ["Und", "wun\u00b7dern", "sich", "nicht", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wen sie erblicken davor;", "tokens": ["Wen", "sie", "er\u00b7bli\u00b7cken", "da\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Der Herzog sie nicht sieht,", "tokens": ["Der", "Her\u00b7zog", "sie", "nicht", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.6": {"text": "Bis ihm Gewalt geschieht.", "tokens": ["Bis", "ihm", "Ge\u00b7walt", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit seinem h\u00f6lzern Schwerte", "tokens": ["Mit", "sei\u00b7nem", "h\u00f6l\u00b7zern", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mohr giebt einen Sto\u00df,", "tokens": ["Der", "Mohr", "giebt", "ei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da f\u00e4llt er gleich zur Erde", "tokens": ["Da", "f\u00e4llt", "er", "gleich", "zur", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von seinem hohen Ro\u00df,", "tokens": ["Von", "sei\u00b7nem", "ho\u00b7hen", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schleppen ihn herein,", "tokens": ["Sie", "schlep\u00b7pen", "ihn", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wo er sollt' ziehen ein.", "tokens": ["Wo", "er", "sollt'", "zie\u00b7hen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie anders sind die Blicke,", "tokens": ["Wie", "an\u00b7ders", "sind", "die", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die er auf Stra\u00dfen kriegt,", "tokens": ["Die", "er", "auf", "Stra\u00b7\u00dfen", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kluge lacht mit T\u00fccke,", "tokens": ["Der", "Klu\u00b7ge", "lacht", "mit", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ihn das Gl\u00fcck betr\u00fcgt,", "tokens": ["Da\u00df", "ihn", "das", "Gl\u00fcck", "be\u00b7tr\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Arm, der willig ihn zog,", "tokens": ["Der", "Arm", ",", "der", "wil\u00b7lig", "ihn", "zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Jetzt eine Feige bog.", "tokens": ["Jetzt", "ei\u00b7ne", "Fei\u00b7ge", "bog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Backe will ihm springen", "tokens": ["Die", "Ba\u00b7cke", "will", "ihm", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor R\u00f6the und vor Scham,", "tokens": ["Vor", "R\u00f6\u00b7the", "und", "vor", "Scham", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als sie zum K\u00f6nig bringen", "tokens": ["Als", "sie", "zum", "K\u00f6\u00b7nig", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Herzog wie ein Lamm,", "tokens": ["Den", "Her\u00b7zog", "wie", "ein", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er findet all im Spiel", "tokens": ["Er", "fin\u00b7det", "all", "im", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der muntern Kinder viel.", "tokens": ["Der", "mun\u00b7tern", "Kin\u00b7der", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Kuchen ist gebacken,", "tokens": ["Der", "Ku\u00b7chen", "ist", "ge\u00b7ba\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er mu\u00df da gleich zum Spiel,", "tokens": ["Er", "mu\u00df", "da", "gleich", "zum", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Krone ist voll Zacken,", "tokens": ["Die", "Kro\u00b7ne", "ist", "voll", "Za\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm gar nicht mehr gefiel,", "tokens": ["Ihm", "gar", "nicht", "mehr", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Gl\u00fcck giebt ihm den Lohn,", "tokens": ["Das", "Gl\u00fcck", "giebt", "ihm", "den", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Papierne Kron zum Hohn.", "tokens": ["Pa\u00b7pier\u00b7ne", "Kron", "zum", "Hohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Hofnarr macht viel Sp\u00e4\u00dfe,", "tokens": ["Der", "Hof\u00b7narr", "macht", "viel", "Sp\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als w\u00e4r es wie zuvor,", "tokens": ["Als", "w\u00e4r", "es", "wie", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "KOKOM", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wenn man ganz verg\u00e4\u00dfe,", "tokens": ["Als", "wenn", "man", "ganz", "ver\u00b7g\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er emp\u00f6rt zuvor,", "tokens": ["Da\u00df", "er", "em\u00b7p\u00f6rt", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Muth bedienet nun,", "tokens": ["Sein", "Muth", "be\u00b7die\u00b7net", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die er wollt absetzen thun.", "tokens": ["Die", "er", "wollt", "ab\u00b7set\u00b7zen", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ist Feldherr \u00fcber alle,", "tokens": ["Ist", "Feld\u00b7herr", "\u00fc\u00b7ber", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieht her mit gro\u00dfer Pracht,", "tokens": ["Zieht", "her", "mit", "gro\u00b7\u00dfer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In seiner Trommeln Schalle", "tokens": ["In", "sei\u00b7ner", "Trom\u00b7meln", "Schal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voran in jede Schlacht,", "tokens": ["Vo\u00b7ran", "in", "je\u00b7de", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die drei K\u00f6n'ge stehn ihn bei,", "tokens": ["Die", "drei", "K\u00f6n'\u00b7ge", "stehn", "ihn", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er verbleibe treu.", "tokens": ["Da\u00df", "er", "ver\u00b7blei\u00b7be", "treu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es ist so lang' gesprochen,", "tokens": ["Es", "ist", "so", "lang'", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bald ein Aufruhr sei,", "tokens": ["Da\u00df", "bald", "ein", "Auf\u00b7ruhr", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum ist er ausgebrochen,", "tokens": ["Drum", "ist", "er", "aus\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von dem erstickten Geschrei,", "tokens": ["Von",