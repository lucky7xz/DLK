{"textgrid.poem.39468": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hermes und Aphroditens spr\u00f6den Knaben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hermes und Aphroditens spr\u00f6den Knaben", "tokens": ["Her\u00b7mes", "und", "A\u00b7phro\u00b7di\u00b7tens", "spr\u00f6\u00b7den", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4lt Salmacis in Liebeswuth umschlungen,", "tokens": ["H\u00e4lt", "Sal\u00b7ma\u00b7cis", "in", "Lie\u00b7bes\u00b7wuth", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis sie den Leib, eh als den Sinn, durchdrungen,", "tokens": ["Bis", "sie", "den", "Leib", ",", "eh", "als", "den", "Sinn", ",", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in dem Gegenstand den Wunsch begraben.", "tokens": ["Und", "in", "dem", "Ge\u00b7gen\u00b7stand", "den", "Wunsch", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn die Natur, die sie beleidigt haben,", "tokens": ["Denn", "die", "Na\u00b7tur", ",", "die", "sie", "be\u00b7lei\u00b7digt", "ha\u00b7ben", ","], "tok