{"dta.poem.3368": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchpsalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hallelujah! Wie schimmert die Morgenwelt!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!", "Wie", "schim\u00b7mert", "die", "Mor\u00b7gen\u00b7welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hallelujah! Wem jubelt ihr Fr\u00fchgesang?", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!", "Wem", "ju\u00b7belt", "ihr", "Fr\u00fch\u00b7ge\u00b7sang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wem dampfen, Wiese, deine Nebel?", "tokens": ["Wem", "damp\u00b7fen", ",", "Wie\u00b7se", ",", "dei\u00b7ne", "Ne\u00b7bel", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fluren, wem athmet ihr euren Weihrauch?", "tokens": ["Flu\u00b7ren", ",", "wem", "ath\u00b7met", "ihr", "eu\u00b7ren", "Weih\u00b7rauch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wem saust der Ostwind Hymnen im Tannenwald?", "tokens": ["Wem", "saust", "der", "Ost\u00b7wind", "Hym\u00b7nen", "im", "Tan\u00b7nen\u00b7wald", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wem lallst du, fromme Lerche, dein Morgenlied?", "tokens": ["Wem", "lallst", "du", ",", "from\u00b7me", "Ler\u00b7che", ",", "dein", "Mor\u00b7gen\u00b7lied", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wem schwirrst du, gr\u00fcnlichgoldner K\u00e4fer?", "tokens": ["Wem", "schwirrst", "du", ",", "gr\u00fcn\u00b7lich\u00b7gold\u00b7ner", "K\u00e4\u00b7fer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sonne, wem tanzest du deinen Reigen?", "tokens": ["Son\u00b7ne", ",", "wem", "tan\u00b7zest", "du", "dei\u00b7nen", "Rei\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dem Unsichtbaren, welchen kein Aug' ersp\u00e4ht!", "tokens": ["Dem", "Un\u00b7sicht\u00b7ba\u00b7ren", ",", "wel\u00b7chen", "kein", "Aug'", "er\u00b7sp\u00e4ht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Unh\u00f6rbaren, welchen kein Ohr erhorcht,", "tokens": ["Dem", "Un\u00b7h\u00f6r\u00b7ba\u00b7ren", ",", "wel\u00b7chen", "kein", "Ohr", "er\u00b7horcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAT", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Allgef\u00fchlten, Allgeliebten", "tokens": ["Dem", "All\u00b7ge\u00b7f\u00fchl\u00b7ten", ",", "All\u00b7ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jubeln die Preise der jungen Sch\u00f6pfung!", "tokens": ["Ju\u00b7beln", "die", "Prei\u00b7se", "der", "jun\u00b7gen", "Sch\u00f6p\u00b7fung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Hallelujah! Erhabner, ich sah dich nie!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!", "Er\u00b7hab\u00b7ner", ",", "ich", "sah", "dich", "nie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hallelujah! Verborgner, du riefst mir nie!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7jah", "!", "Ver\u00b7borg\u00b7ner", ",", "du", "riefst", "mir", "nie", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Doch klopft dir jeder meiner Pulse,", "tokens": ["Doch", "klopft", "dir", "je\u00b7der", "mei\u00b7ner", "Pul\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00fcttert dir jegliche meiner Nerven!", "tokens": ["Sch\u00fct\u00b7tert", "dir", "jeg\u00b7li\u00b7che", "mei\u00b7ner", "Ner\u00b7ven", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Einst geht ein andrer Morgen, ein sch\u00f6nerer,", "tokens": ["Einst", "geht", "ein", "an\u00b7drer", "Mor\u00b7gen", ",", "ein", "sch\u00f6\u00b7ne\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir auf, ein ew'ger Morgen. \u2014 Ach, werd' ich dann,", "tokens": ["Mir", "auf", ",", "ein", "ew'\u00b7ger", "Mor\u00b7gen", ".", "Ach", ",", "werd'", "ich", "dann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$.", "$(", "ITJ", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sprich, Vater, werd' ich dann dich schauen,", "tokens": ["Sprich", ",", "Va\u00b7ter", ",", "werd'", "ich", "dann", "dich", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchen zu schauen mein Geist verschmachtet?", "tokens": ["Wel\u00b7chen", "zu", "schau\u00b7en", "mein", "Geist", "ver\u00b7schmach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}