{"textgrid.poem.35727": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "51. An Stolberg", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier unterm Baume weht's so k\u00fchl,", "tokens": ["Hier", "un\u00b7term", "Bau\u00b7me", "weht's", "so", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frisch ist dieser Wein.", "tokens": ["Und", "frisch", "ist", "die\u00b7ser", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier schau' ich weit des Sees Gew\u00fchl,", "tokens": ["Hier", "schau'", "ich", "weit", "des", "Sees", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meiner kleinen Fische Spiel,", "tokens": ["Und", "mei\u00b7ner", "klei\u00b7nen", "Fi\u00b7sche", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sinne ganz allein.", "tokens": ["Und", "sin\u00b7ne", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich nipp' aus meinem Feierglas,", "tokens": ["Ich", "nipp'", "aus", "mei\u00b7nem", "Fei\u00b7er\u00b7glas", ","], "token_info": ["word", "word", "word