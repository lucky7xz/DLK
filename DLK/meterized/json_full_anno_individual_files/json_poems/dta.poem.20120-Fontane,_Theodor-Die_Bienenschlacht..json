{"dta.poem.20120": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Bienenschlacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wespen und die Bienen", "tokens": ["Die", "Wes\u00b7pen", "und", "die", "Bie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie haben sich entzweit,", "tokens": ["Sie", "ha\u00b7ben", "sich", "ent\u00b7zweit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie Guelphen und Ghibellinen", "tokens": ["Wie", "Guel\u00b7phen", "und", "Ghi\u00b7bel\u00b7li\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Stehen sie im Streit,", "tokens": ["Ste\u00b7hen", "sie", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Parthei nimmt Hummel und K\u00e4fer,", "tokens": ["Part\u00b7hei", "nimmt", "Hum\u00b7mel", "und", "K\u00e4\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und selbst der Blumen-Elf,", "tokens": ["Und", "selbst", "der", "Blu\u00b7men\u00b7Elf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es fl\u00fcstern die Lilienschl\u00e4fer:", "tokens": ["Es", "fl\u00fcs\u00b7tern", "die", "Li\u00b7li\u00b7en\u00b7schl\u00e4\u00b7fer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u201ehie Waibling und hie Welf!\u201c", "tokens": ["\u201e", "hie", "Waib\u00b7ling", "und", "hie", "Welf", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "KON", "ADV", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Bienen halten sich wacker,", "tokens": ["Die", "Bie\u00b7nen", "hal\u00b7ten", "sich", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch ach, trotz Wall und Thurm,", "tokens": ["Doch", "ach", ",", "trotz", "Wall", "und", "Thurm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Schoten- und Bohnen-Acker", "tokens": ["Den", "Scho\u00b7ten", "und", "Boh\u00b7nen\u00b7A\u00b7cker"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN"], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nahm der Feind im Sturm;", "tokens": ["Nahm", "der", "Feind", "im", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schon um die heimische Linde,", "tokens": ["Schon", "um", "die", "hei\u00b7mi\u00b7sche", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie um Herd und Haus,", "tokens": ["Wie", "um", "Herd", "und", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Sammelt das Bienen-Gesinde", "tokens": ["Sam\u00b7melt", "das", "Bie\u00b7nen\u00b7Ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Sich zum letzten Strau\u00df.", "tokens": ["Sich", "zum", "letz\u00b7ten", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Eine (sie stund auf Wache,", "tokens": ["Ei\u00b7ne", "(", "sie", "stund", "auf", "Wa\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und das Weinen war ihr nah)", "tokens": ["Und", "das", "Wei\u00b7nen", "war", "ihr", "nah", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwur: \u201eeine herrliche Sache", "tokens": ["Schwur", ":", "\u201e", "ei\u00b7ne", "herr\u00b7li\u00b7che", "Sa\u00b7che"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "$(", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Sei dies ", "tokens": ["Sei", "dies"], "token_info": ["word", "word"], "pos": ["VAFIN", "PDS"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Da\u00df ihr Stand so ein harter", "tokens": ["Da\u00df", "ihr", "Stand", "so", "ein", "har\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Freue sie nur zu sehn,", "tokens": ["Freu\u00b7e", "sie", "nur", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie die dreihundert Sparter", "tokens": ["Wie", "die", "drei\u00b7hun\u00b7dert", "Spar\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "CARD", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "W\u00fcrden sie untergehn.\u201c", "tokens": ["W\u00fcr\u00b7den", "sie", "un\u00b7ter\u00b7gehn", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sprach da eine Zweite:", "tokens": ["Sprach", "da", "ei\u00b7ne", "Zwei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u201ewohl, sie stimme dem bei,", "tokens": ["\u201e", "wohl", ",", "sie", "stim\u00b7me", "dem", "bei", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VVFIN", "ART", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df zu fallen im Streite", "tokens": ["Da\u00df", "zu", "fal\u00b7len", "im", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKZU", "VVINF", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ein Vergn\u00fcgen sei;", "tokens": ["Ein", "Ver\u00b7gn\u00fc\u00b7gen", "sei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nur sie w\u00e4re verwundert,", "tokens": ["Nur", "sie", "w\u00e4\u00b7re", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Da\u00df man auf Sparta s\u00e4h\u2019,", "tokens": ["Da\u00df", "man", "auf", "Spar\u00b7ta", "s\u00e4h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Pforzheim und seine Vierhundert", "tokens": ["Pforz\u00b7heim", "und", "sei\u00b7ne", "Vier\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "H\u00e4tte man ja in der N\u00e4h\u2019\u201c.", "tokens": ["H\u00e4t\u00b7te", "man", "ja", "in", "der", "N\u00e4h'", "\u201c", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$(", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Sprach es. Die Anderen alle,", "tokens": ["Sprach", "es", ".", "Die", "An\u00b7de\u00b7ren", "al\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ART", "ADJA", "PIS", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Immer gesinnungsvoll,", "tokens": ["Im\u00b7mer", "ge\u00b7sin\u00b7nungs\u00b7voll", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Klatschten in diesem Falle", "tokens": ["Klatschten", "in", "die\u00b7sem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Geradezu wie toll; \u2014", "tokens": ["Ge\u00b7ra\u00b7de\u00b7zu", "wie", "toll", ";"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Siehe! da schwarz am Himmel,", "tokens": ["Sie\u00b7he", "!", "da", "schwarz", "am", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Heuschreckenzug,", "tokens": ["Wie", "Heu\u00b7schre\u00b7cken\u00b7zug", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nahet das Wespengewimmel", "tokens": ["Na\u00b7het", "das", "Wes\u00b7pen\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Sich im Siegesflug.", "tokens": ["Sich", "im", "Sie\u00b7ges\u00b7flug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Solche Schw\u00e4rme und Fl\u00fcge", "tokens": ["Sol\u00b7che", "Schw\u00e4r\u00b7me", "und", "Fl\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Nimmer der Garten sah,", "tokens": ["Nim\u00b7mer", "der", "Gar\u00b7ten", "sah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wahre Hunnenz\u00fcge", "tokens": ["Wah\u00b7re", "Hun\u00b7nen\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sind es des Attila.", "tokens": ["Sind", "es", "des", "At\u00b7ti\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gierig nach Blut und Morden", "tokens": ["Gie\u00b7rig", "nach", "Blut", "und", "Mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "St\u00fcrmen sie heran,", "tokens": ["St\u00fcr\u00b7men", "sie", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wie die Mongolenhorden", "tokens": ["Wie", "die", "Mon\u00b7go\u00b7len\u00b7hor\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Unter Dschingiskhan.", "tokens": ["Un\u00b7ter", "Dschin\u00b7gis\u00b7khan", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Bald in gebogenem Horne,", "tokens": ["Bald", "in", "ge\u00b7bo\u00b7ge\u00b7nem", "Hor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bald in gespitztem Keil,", "tokens": ["Bald", "in", "ge\u00b7spitz\u00b7tem", "Keil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aber immer nach vorne", "tokens": ["A\u00b7ber", "im\u00b7mer", "nach", "vor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stachel und Hintertheil:", "tokens": ["Sta\u00b7chel", "und", "Hin\u00b7ter\u00b7theil", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "So, nach reifer Betrachtung,", "tokens": ["So", ",", "nach", "rei\u00b7fer", "Be\u00b7trach\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "St\u00fcrmen sie herbei,", "tokens": ["St\u00fcr\u00b7men", "sie", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Weil es der Verachtung", "tokens": ["Weil", "es", "der", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Sprechendster Ausdruck sei.", "tokens": ["Spre\u00b7chends\u00b7ter", "Aus\u00b7druck", "sei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Auch die Bienen, in Demuth", "tokens": ["Auch", "die", "Bie\u00b7nen", ",", "in", "De\u00b7muth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden sich de\u00df bewu\u00dft,", "tokens": ["Wer\u00b7den", "sich", "de\u00df", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schier unendliche Wehmuth", "tokens": ["Schier", "un\u00b7end\u00b7li\u00b7che", "Weh\u00b7muth"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schleicht in ihre Brust,", "tokens": ["Schleicht", "in", "ih\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Stimmen statt Schlachtgesanges,", "tokens": ["Stim\u00b7men", "statt", "Schlacht\u00b7ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Klagelieder an,", "tokens": ["Kla\u00b7ge\u00b7lie\u00b7der", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und vor allem ein banges:", "tokens": ["Und", "vor", "al\u00b7lem", "ein", "ban\u00b7ges", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ART", "ADJA", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201ezeige dich braver Mann!\u201c", "tokens": ["\u201e", "zei\u00b7ge", "dich", "bra\u00b7ver", "Mann", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Siehe, da schnell ein Sasse", "tokens": ["Sie\u00b7he", ",", "da", "schnell", "ein", "Sas\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tritt hervor aus den Rei