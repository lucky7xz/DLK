{"dta.poem.23760": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Schwester.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Mein lieber Bruder, vor dem Jahre", "tokens": ["Mein", "lie\u00b7ber", "Bru\u00b7der", ",", "vor", "dem", "Jah\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War ich, wie du, so sch\u00f6n!", "tokens": ["War", "ich", ",", "wie", "du", ",", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hatt ich da f\u00fcr schwarze Haare?", "tokens": ["Was", "hatt", "ich", "da", "f\u00fcr", "schwar\u00b7ze", "Haa\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast sie noch gesehn.", "tokens": ["Du", "hast", "sie", "noch", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da lobte jedes die\u00df Gesicht", "tokens": ["Da", "lob\u00b7te", "je\u00b7des", "die\u00df", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "PDS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bewundernd um die Wette,", "tokens": ["Be\u00b7wun\u00b7dernd", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und schwur, es sey kein M\u00e4dchen nicht", "tokens": ["Und", "schwur", ",", "es", "sey", "kein", "M\u00e4d\u00b7chen", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sch\u00f6n, als Henriette.", "tokens": ["So", "sch\u00f6n", ",", "als", "Hen\u00b7riet\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Allein die Sch\u00f6nheit ist vergangen.", "tokens": ["Al\u00b7lein", "die", "Sch\u00f6n\u00b7heit", "ist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da kam der Blattern Wuth,", "tokens": ["Da", "kam", "der", "Blat\u00b7tern", "Wuth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerri\u00df mir diese glatten Wangen,", "tokens": ["Zer\u00b7ri\u00df", "mir", "die\u00b7se", "glat\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00f6scht aus der Augen Gluth:", "tokens": ["L\u00f6scht", "aus", "der", "Au\u00b7gen", "Gluth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch glaube nicht, da\u00df michs verdr\u00fc\u00dft.", "tokens": ["Doch", "glau\u00b7be", "nicht", ",", "da\u00df", "michs", "ver\u00b7dr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nein, es hat mich gelehret,", "tokens": ["Nein", ",", "es", "hat", "mich", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Da\u00df das nur wahre Sch\u00f6nheit ist,", "tokens": ["Da\u00df", "das", "nur", "wah\u00b7re", "Sch\u00f6n\u00b7heit", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was keine Zeit zerst\u00f6ret.", "tokens": ["Was", "kei\u00b7ne", "Zeit", "zer\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}