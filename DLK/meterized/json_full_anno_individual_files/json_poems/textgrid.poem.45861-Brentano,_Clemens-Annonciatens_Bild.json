{"textgrid.poem.45861": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Annonciatens Bild", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am H\u00fcgel sitzt sie, wo von k\u00fchlen Reben", "tokens": ["Am", "H\u00fc\u00b7gel", "sitzt", "sie", ",", "wo", "von", "k\u00fch\u00b7len", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Dach sich w\u00f6lbt durchrankt von bunter Wicke,", "tokens": ["Ein", "Dach", "sich", "w\u00f6lbt", "durc\u00b7hrankt", "von", "bun\u00b7ter", "Wi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Abendhimmel ruhen ihre Blicke,", "tokens": ["Im", "A\u00b7bend\u00b7him\u00b7mel", "ru\u00b7hen", "ih\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo goldne Pfeile durch die D\u00e4mmrung schweben.", "tokens": ["Wo", "gold\u00b7ne", "Pfei\u00b7le", "durch", "die", "D\u00e4mm\u00b7rung", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Orangen sind ihr in den Scho\u00df gegeben", "tokens": ["O\u00b7ran\u00b7gen", "sind", "ihr", "in", "den", "Scho\u00df", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu zeigen, wie die Glut sie nur entz\u00fccke,", "tokens": ["Zu", "zei\u00b7gen", ",", "wie", "die", "Glut", "sie", "nur", "ent\u00b7z\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4nger weilt die Sonne, sieht zur\u00fccke", "tokens": ["Und", "l\u00e4n\u00b7ger", "weilt", "die", "Son\u00b7ne", ",", "sieht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum stillen Kinde in das dunkle Leben.", "tokens": ["Zum", "stil\u00b7len", "Kin\u00b7de", "in", "das", "dunk\u00b7le", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der freien Stirne schwarze Locken kr\u00e4nzet", "tokens": ["Der", "frei\u00b7en", "Stir\u00b7ne", "schwar\u00b7ze", "Lo\u00b7cken", "kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr goldner Pomeranzen s\u00fc\u00dfe Bl\u00fcte,", "tokens": ["Ihr", "gold\u00b7ner", "Po\u00b7me\u00b7ran\u00b7zen", "s\u00fc\u00b7\u00dfe", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN"