{"textgrid.poem.43416": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Droschkenfuhrmann weinet,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Droschkenfuhrmann weinet,", "tokens": ["Der", "Droschken\u00b7fuhr\u00b7mann", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Er kam in gro\u00dfe Noth:", "tokens": ["Er", "kam", "in", "gro\u00b7\u00dfe", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist sein Pferd gefallen", "tokens": ["Es", "ist", "sein", "Pferd", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und liegt als w\u00e4r' es todt.", "tokens": ["Und", "liegt", "als", "w\u00e4r'", "es", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kommt herbeigesprungen", "tokens": ["Da", "kommt", "her\u00b7bei\u00b7ge\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Mann in vollem Lauf:", "tokens": ["Ein", "Mann", "in", "vol\u00b7lem", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df ein gutes Mittel,", "tokens": ["Ich", "wei\u00df", "ein", "gu\u00b7tes", "Mit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Euer Pferd steht auf.", "tokens": ["Und", "Eu\u00b7er", "Pferd", "steht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Legt eine Staatszeitung unter!", "tokens": ["Legt", "ei\u00b7ne", "Staats\u00b7zei\u00b7tung", "un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Mittel ist bew\u00e4hrt;", "tokens": ["Das", "Mit\u00b7tel", "ist", "be\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Politik, ich schw\u00f6r' es,", "tokens": ["Die", "Po\u00b7li\u00b7tik", ",", "ich", "schw\u00f6r'", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vertr\u00e4gt, bei Gott! kein Pferd.", "tokens": ["Ver\u00b7tr\u00e4gt", ",", "bei", "Gott", "!", "kein", "Pferd", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Droschkenfuhrmann weinet,", "tokens": ["Der", "Droschken\u00b7fuhr\u00b7mann", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter":