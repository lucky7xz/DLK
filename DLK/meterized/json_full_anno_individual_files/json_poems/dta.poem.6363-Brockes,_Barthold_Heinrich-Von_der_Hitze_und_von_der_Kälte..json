{"dta.poem.6363": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Oeffnungen der Dinge, welche brennen,", "tokens": ["In", "Oeff\u00b7nun\u00b7gen", "der", "Din\u00b7ge", ",", "wel\u00b7che", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchrt seine glatten Aal\u2019 ein kaltes Wasser ein,", "tokens": ["F\u00fchrt", "sei\u00b7ne", "glat\u00b7ten", "Aal'", "ein", "kal\u00b7tes", "Was\u00b7ser", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und machet, da\u00df sie sich nicht ferner regen k\u00f6nnen;", "tokens": ["Und", "ma\u00b7chet", ",", "da\u00df", "sie", "sich", "nicht", "fer\u00b7ner", "re\u00b7gen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADV", "ADJA", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und eben dadurch l\u00f6scht es seinen Brand und Schein.", "tokens": ["Und", "e\u00b7ben", "da\u00b7durch", "l\u00f6scht", "es", "sei\u00b7nen", "Brand", "und", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}