{"textgrid.poem.47496": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zu der Rose, zu dem Weine komm!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu der Rose, zu dem Weine komm!", "tokens": ["Zu", "der", "Ro\u00b7se", ",", "zu", "dem", "Wei\u00b7ne", "komm", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Her zu diesem stillen Haine komm!", "tokens": ["Her", "zu", "die\u00b7sem", "stil\u00b7len", "Hai\u00b7ne", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mild zu stillen meiner Sehnsucht Ach,", "tokens": ["Mild", "zu", "stil\u00b7len", "mei\u00b7ner", "Sehn\u00b7sucht", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "PPOSAT", "NN", "ITJ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denn es r\u00fchrt ja selbst die Steine, komm!", "tokens": ["Denn", "es", "r\u00fchrt", "ja", "selbst", "die", "Stei\u00b7ne", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hold zu hemmen meiner Z\u00e4hre Bach,", "tokens": ["Hold", "zu", "hem\u00b7men", "mei\u00b7ner", "Z\u00e4h\u00b7re", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die ich schon so lange weine, komm!", "tokens": ["Die", "ich", "schon", "so", "lan\u00b7ge", "wei\u00b7ne", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mir zu spenden hier im Laubgemach", "tokens": ["Mir", "zu", "spen\u00b7den", "hier", "im", "Laub\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Edens Heil in aller Reine, komm!", "tokens": ["E\u00b7dens", "Heil", "in", "al\u00b7ler", "Rei\u00b7ne", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Bald, o bald, da\u00df nicht in Asche jach", "tokens": ["Bald", ",", "o", "bald", ",", "da\u00df", "nicht", "in", "A\u00b7sche", "jach"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "ADV", "$,", "KOUS", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Falle mein verkohlt Gebeine, komm!", "tokens": ["Fal\u00b7le", "mein", "ver\u00b7kohlt", "Ge\u00b7bei\u00b7ne", ",", "komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber erst, wenn Tag und Sonne schwach,", "tokens": ["A\u00b7ber", "erst", ",", "wenn", "Tag", "