"ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und auch bereit zu ehren euren Schein/", "tokens": ["Und", "auch", "be\u00b7reit", "zu", "eh\u00b7ren", "eu\u00b7ren", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wollt ihr mir gleich nicht g\u00fcnstig seyn.", "tokens": ["Wollt", "ihr", "mir", "gleich", "nicht", "g\u00fcns\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}