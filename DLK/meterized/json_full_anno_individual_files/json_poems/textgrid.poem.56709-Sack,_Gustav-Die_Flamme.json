{"textgrid.poem.56709": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Flamme", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich wei\u00df, woher ich stamme;", "tokens": ["Auch", "ich", "wei\u00df", ",", "wo\u00b7her", "ich", "stam\u00b7me", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schw\u00e4lend tr\u00fcb gleich einer Flamme,", "tokens": ["schw\u00e4\u00b7lend", "tr\u00fcb", "gleich", "ei\u00b7ner", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "die das Moor zum Schw\u00e4len brachte \u2013", "tokens": ["die", "das", "Moor", "zum", "Schw\u00e4\u00b7len", "brach\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses Moor, das ich verachte,", "tokens": ["die\u00b7ses", "Moor", ",", "das", "ich", "ver\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Not und Plage hei\u00dft dies Moor \u2013", "tokens": ["Not", "und", "Pla\u00b7ge", "hei\u00dft", "dies", "Moor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text