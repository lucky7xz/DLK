{"dta.poem.2404": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wer solte nun wolseyn/ dem nicht das Hertze lachte/", "tokens": ["Wer", "sol\u00b7te", "nun", "wol\u00b7seyn", "/", "dem", "nicht", "das", "Hert\u00b7ze", "lach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "$(", "ART", "PTKNEG", "PDS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihn die Braut so gr\u00fcsst? Ja wen\u0303 ichs recht betrachte/", "tokens": ["Wenn", "ihn", "die", "Braut", "so", "gr\u00fcs\u00b7st", "?", "Ja", "we\u00f1", "ichs", "recht", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$.", "PTKANT", "KON", "PIS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vermein\u2019 ich/ da\u00df ", "tokens": ["Ver\u00b7mein'", "ich", "/", "da\u00df"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "$(", "KOUS"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "froh/", "tokens": ["froh", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und da\u00df vor treuer Lieb\u2019 er brenne lichterloh.", "tokens": ["Und", "da\u00df", "vor", "treu\u00b7er", "Lieb'", "er", "bren\u00b7ne", "lich\u00b7ter\u00b7loh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er wils zwar nicht gestehn/ er kans auch nicht verneinen/", "tokens": ["Er", "wils", "zwar", "nicht", "ge\u00b7stehn", "/", "er", "kans", "auch", "nicht", "ver\u00b7nei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVPP", "$(", "PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Glut verbirgt er wol/ doch kan er nicht das scheinen", "tokens": ["Die", "Glut", "ver\u00b7birgt", "er", "wol", "/", "doch", "kan", "er", "nicht", "das", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Flammen nehmen ab. Denn keuscher Liebe Licht", "tokens": ["Den", "Flam\u00b7men", "neh\u00b7men", "ab", ".", "Denn", "keu\u00b7scher", "Lie\u00b7be", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hat stets die Eigenschafft/ da\u00df es herausser bricht/", "tokens": ["Hat", "stets", "die", "Ei\u00b7gen\u00b7schafft", "/", "da\u00df", "es", "her\u00b7aus\u00b7ser", "bricht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sehr man seinen Strahl zu bergen sich bem\u00fchet/", "tokens": ["Wie", "sehr", "man", "sei\u00b7nen", "Strahl", "zu", "ber\u00b7gen", "sich", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PPOSAT", "NN", "PTKZU", "VVINF", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie man mit Wunderung\u2019 an manchen Menschen siehet.", "tokens": ["Wie", "man", "mit", "Wun\u00b7de\u00b7rung'", "an", "man\u00b7chen", "Men\u00b7schen", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein Beyspiel ist verhast. Wo gehstu Feder hin/", "tokens": ["Ein", "Bey\u00b7spiel", "ist", "ver\u00b7hast", ".", "Wo", "gehs\u00b7tu", "Fe\u00b7der", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "PWAV", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie lange helstu noch so manchen sch\u00f6nen Sinn", "tokens": ["Wie", "lan\u00b7ge", "hels\u00b7tu", "noch", "so", "man\u00b7chen", "sch\u00f6\u00b7nen", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mit schlechten Worten auf? Es sey genug geschrieben.", "tokens": ["Mit", "schlech\u00b7ten", "Wor\u00b7ten", "auf", "?", "Es", "sey", "ge\u00b7nug", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Wunsch leg\u2019 ich noch ab/ worzu mich hat getrieben", "tokens": ["Den", "Wunsch", "leg'", "ich", "noch", "ab", "/", "wor\u00b7zu", "mich", "hat", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "PWAV", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Her Breutgam eure Gunst: Gott helffe/ da\u00df der Bund/", "tokens": ["Her", "Breut\u00b7gam", "eu\u00b7re", "Gunst", ":", "Gott", "helf\u00b7fe", "/", "da\u00df", "der", "Bund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "$.", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Den ihr mit eurer Braut/ mit Hertz/ mit Hand/ mit", "tokens": ["Den", "ihr", "mit", "eu\u00b7rer", "Braut", "/", "mit", "Hertz", "/", "mit", "Hand", "/", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "$(", "APPR", "NN", "$(", "APPR", "NN", "$(", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Mund", "tokens": ["Mund"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Habt felsenfest gemacht/ Euch ewig m\u00f6ge n\u00fctzen/", "tokens": ["Habt", "fel\u00b7sen\u00b7fest", "ge\u00b7macht", "/", "Euch", "e\u00b7wig", "m\u00f6\u00b7ge", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$(", "PPER", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und auch f\u00fcr allem Fall Euch m\u00f6ge kr\u00e4fftig sch\u00fctzen/", "tokens": ["Und", "auch", "f\u00fcr", "al\u00b7lem", "Fall", "Euch", "m\u00f6\u00b7ge", "kr\u00e4ff\u00b7tig", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "NN", "PPER", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Lebt wol/ frisch und gesund/ lebt selig wie ihr lebt/", "tokens": ["Lebt", "wol", "/", "frisch", "und", "ge\u00b7sund", "/", "lebt", "se\u00b7lig", "wie", "ihr", "lebt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "ADJD", "KON", "ADJD", "$(", "VVFIN", "ADJD", "KOKOM", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Hier in besteht mein Wunsch/ den ihr euch selber gebt.", "tokens": ["Hier", "in", "be\u00b7steht", "mein", "Wunsch", "/", "den", "ihr", "euch", "sel\u00b7ber", "gebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "PPOSAT", "NN", "$(", "ART", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Noch bitt\u2019 ich auch zuletzt/ beklaget doch den armen", "tokens": ["Noch", "bitt'", "ich", "auch", "zu\u00b7letzt", "/", "be\u00b7kla\u00b7get", "doch", "den", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Betr\u00fcbten Kloriman und tragt mit ihm Erbarmen.", "tokens": ["Be\u00b7tr\u00fcb\u00b7ten", "Klo\u00b7ri\u00b7man", "und", "tragt", "mit", "ihm", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wer aber wissen wil/ wers ist/ der sey bericht;", "tokens": ["Wer", "a\u00b7ber", "wis\u00b7sen", "wil", "/", "wers", "ist", "/", "der", "sey", "be\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVINF", "VMFIN", "$(", "PWS", "VAFIN", "$(", "ART", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wei\u00df nicht wie er heist/ ich kenn. ihn warlich nicht.", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "er", "heist", "/", "ich", "kenn", ".", "ihn", "war\u00b7lich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "$.", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}