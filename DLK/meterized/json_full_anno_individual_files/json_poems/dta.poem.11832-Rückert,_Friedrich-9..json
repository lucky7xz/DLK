{"dta.poem.11832": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen m\u00fcssen dir von Zeit zu Zeit es sagen,", "tokens": ["Die", "Men\u00b7schen", "m\u00fcs\u00b7sen", "dir", "von", "Zeit", "zu", "Zeit", "es", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df was f\u00fcr sie du thust, m\u00f6g' ihren Beifall tragen.", "tokens": ["Da\u00df", "was", "f\u00fcr", "sie", "du", "thust", ",", "m\u00f6g'", "ih\u00b7ren", "Bei\u00b7fall", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PPER", "VVFIN", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und sagen sie es nicht, so mu\u00df in deiner Brust", "tokens": ["Und", "sa\u00b7gen", "sie", "es", "nicht", ",", "so", "mu\u00df", "in", "dei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Stimm' es sagen, da\u00df du nicht Unn\u00fctzes thust.", "tokens": ["Die", "Stimm'", "es", "sa\u00b7gen", ",", "da\u00df", "du", "nicht", "Un\u00b7n\u00fct\u00b7zes", "thust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ohn' einen Zuruf so von au\u00dfen oder innen,", "tokens": ["Ohn'", "ei\u00b7nen", "Zu\u00b7ruf", "so", "von", "au\u00b7\u00dfen", "o\u00b7der", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "ADV", "KON", "ADV", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Bleibt ohne Lust, und ohn' Erfolg auch, dein Beginnen.", "tokens": ["Bleibt", "oh\u00b7ne", "Lust", ",", "und", "ohn'", "Er\u00b7folg", "auch", ",", "dein", "Be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "APPR", "NN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}