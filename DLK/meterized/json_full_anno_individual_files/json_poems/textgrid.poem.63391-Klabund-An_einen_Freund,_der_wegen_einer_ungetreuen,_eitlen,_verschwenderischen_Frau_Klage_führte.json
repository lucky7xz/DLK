{"textgrid.poem.63391": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund, der wegen einer ungetreuen, eitlen, verschwenderischen Frau Klage f\u00fchrte", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kannst dem Fr\u00fchling nicht Halt gebieten", "tokens": ["Du", "kannst", "dem", "Fr\u00fch\u00b7ling", "nicht", "Halt", "ge\u00b7bie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nicht der ungetreuen Frau.", "tokens": ["Und", "nicht", "der", "un\u00b7ge\u00b7treu\u00b7en", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Nordwind saust um deine Stirn.", "tokens": ["Der", "Nord\u00b7wind", "saust", "um", "dei\u00b7ne", "Stirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh, geh von dannen.", "tokens": ["Geh", ",", "geh", "von", "dan\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Hast du Geld, so stiehlt es deine Frau.", "tokens": ["Hast", "du", "Geld", ",", "so", "stiehlt", "es", "dei\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.