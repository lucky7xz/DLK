{"textgrid.poem.59278": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weltlauf", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was du gestern frisch gesungen,", "tokens": ["Was", "du", "ge\u00b7stern", "frisch", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist doch heute schon verklungen,", "tokens": ["Ist", "doch", "heu\u00b7te", "schon", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und beim letzten Klange schreit", "tokens": ["Und", "beim", "letz\u00b7ten", "Klan\u00b7ge", "schreit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Alle Welt nach Neuigkeit.", "tokens": ["Al\u00b7le", "Welt", "nach", "Neu\u00b7ig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "War ein Held, der legt' verwegen", "tokens": ["War", "ein", "Held", ",", "der", "legt'", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einstmals seinen blut'gen Degen", "tokens": ["Einst\u00b7mals", "sei\u00b7nen", "blut'\u00b7gen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wie Gottes schwere Hand", "tokens": ["Als", "wie", "Got\u00b7tes", "schwe\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber das erschrockne Land.", "tokens": ["\u00dc\u00b7ber", "das", "er\u00b7schrock\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mu\u00dft's doch bl\u00fchn und rauschen lassen,", "tokens": ["Mu\u00dft's", "doch", "bl\u00fchn", "und", "rau\u00b7schen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "KON", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den toten L\u00f6wen fassen", "tokens": ["Und", "den", "to\u00b7ten", "L\u00f6\u00b7wen", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Knaben nun nach Jungenart", "tokens": ["Kna\u00b7ben", "nun", "nach", "Jun\u00b7ge\u00b7nart"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungestraft an M\u00e4hn und Bart.", "tokens": ["Un\u00b7ge\u00b7straft", "an", "M\u00e4hn", "und", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So viel Gipfel als da funkeln,", "tokens": ["So", "viel", "Gip\u00b7fel", "als", "da", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word",