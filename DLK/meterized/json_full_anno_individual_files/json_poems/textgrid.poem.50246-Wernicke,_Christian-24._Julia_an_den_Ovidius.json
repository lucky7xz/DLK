{"textgrid.poem.50246": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "24. Julia an den Ovidius", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Verse, die Du schreibst vom Leffeln,", "tokens": ["Die", "Ver\u00b7se", ",", "die", "Du", "schreibst", "vom", "Lef\u00b7feln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die misst man zwar, mein Freund, mit Scheffeln;", "tokens": ["Die", "misst", "man", "zwar", ",", "mein", "Freund", ",", "mit", "Schef\u00b7feln", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch giebts hier, wenn ichs sagen soll,", "tokens": ["Doch", "giebts", "hier", ",", "wenn", "ichs", "sa\u00b7gen", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sonst w\u00e4rst du l\u00e4ngst zu mir gekommen,", "tokens": ["Sonst", "w\u00e4rst", "du", "l\u00e4ngst", "zu", "mir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00e4ttst mit mir vorlieb genommen;", "tokens": ["Und", "h\u00e4ttst", "mit", "mir", "vor\u00b7lieb", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn, dass ich ziemlich ", "tokens": ["Denn", ",", "dass", "ich", "ziem\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Davor bin ich in Rom bekant.", "tokens": ["Da\u00b7vor", "bin", "ich", "in", "Rom", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zudem, so steckt' ich manchen Heller", "tokens": ["Zu\u00b7dem", ",", "so", "steckt'", "ich", "man\u00b7chen", "Hel\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im Essen unter deinen Teller,", "tokens": ["Im", "Es\u00b7sen", "un\u00b7ter", "dei\u00b7nen", "Tel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auff dass es nicht der Vater merk':", "tokens": ["Auff", "dass", "es", "nicht", "der", "Va\u00b7ter", "merk'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Verse, die Du schreibst vom Leffeln,", "tokens": ["Die", "Ver\u00b7se", ",", "die", "Du", "schreibst", "vom", "Lef\u00b7fe