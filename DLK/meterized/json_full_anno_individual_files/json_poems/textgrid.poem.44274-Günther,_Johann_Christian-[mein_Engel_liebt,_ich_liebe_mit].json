{"textgrid.poem.44274": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[mein Engel liebt, ich liebe mit]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel liebt, ich liebe mit,", "tokens": ["Mein", "En\u00b7gel", "liebt", ",", "ich", "lie\u00b7be", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lieben wir uns selbst zusammen,", "tokens": ["So", "lie\u00b7ben", "wir", "uns", "selbst", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und der zum Altar frohe Schritt", "tokens": ["Und", "der", "zum", "Al\u00b7tar", "fro\u00b7he", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdoppelt die vermischten Flammen.", "tokens": ["Ver\u00b7dop\u00b7pelt", "die", "ver\u00b7mischten", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Misgunst sey auch noch so blind,", "tokens": ["Die", "Mis\u00b7gunst", "sey", "auch", "noch", "so", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin dein Schaz, du bist mein Kind.", "tokens": ["Ich", "bin", "dein", "Schaz", ",", "du", "bist", "mein", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, die der Himmel selbst gesand,", "tokens": ["Du", ",", "die", "der", "Him\u00b7mel", "selbst", "ge\u00b7sand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir meines Lebens Lust zu machen,", "tokens": ["Mir", "mei\u00b7nes", "Le\u00b7bens", "Lust", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Solst sonder Creuz und Unbestand", "tokens": ["Solst", "son\u00b7der", "Creuz", "und", "Un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meinen treuen Armen lachen.", "tokens": ["In", "mei\u00b7nen", "treu\u00b7en", "Ar\u00b7men", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O was vor Z\u00e4rtligkeit und Lust", "tokens": ["O", "was", "vor", "Z\u00e4rt\u00b7lig\u00b7keit", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Versprech ich mir auf deiner Brust!", "tokens": ["Ver\u00b7sprech", "ich", "mir", "auf", "dei\u00b7ner", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach welch Vergn\u00fcgen wird uns nicht", "tokens": ["Ach", "welch", "Ver\u00b7gn\u00fc\u00b7gen", "wird", "uns", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAT", "NN", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens Elend recht vers\u00fc\u00dfen!", "tokens": ["Des", "Le\u00b7bens", "E\u00b7lend", "recht", "ver\u00b7s\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Leitstern ist dein Angesicht,", "tokens": ["Mein", "Leits\u00b7tern", "ist", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Gl\u00fccke wird mein feurig K\u00fc\u00dfen,", "tokens": ["Dein", "Gl\u00fc\u00b7cke", "wird", "mein", "feu\u00b7rig", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem mir auf der ganzen Welt", "tokens": ["Nach\u00b7dem", "mir", "auf", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts Be\u00dfres als dein Bild gef\u00e4llt.", "tokens": ["Nichts", "Be\u00df\u00b7res", "als", "dein", "Bild", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "KOKOM", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Unruh plagt mich zum Voraus,", "tokens": ["Die", "Un\u00b7ruh", "plagt", "mich", "zum", "Vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dir die erste Nacht zu spielen.", "tokens": ["Mit", "dir", "die", "ers\u00b7te", "Nacht", "zu", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein Kleinod vor mein Haus;", "tokens": ["Du", "bist", "ein", "Klei\u00b7nod", "vor", "mein", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was werd ich vor Erg\u00f6zung f\u00fchlen,", "tokens": ["Was", "werd", "ich", "vor", "Er\u00b7g\u00f6\u00b7zung", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn deine keusche Liebesfrucht", "tokens": ["Wenn", "dei\u00b7ne", "keu\u00b7sche", "Lie\u00b7bes\u00b7frucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Neigung zu erhalten sucht.", "tokens": ["Die", "Nei\u00b7gung", "zu", "er\u00b7hal\u00b7ten", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In Tr\u00e4umen scherz ich schon mit dir,", "tokens": ["In", "Tr\u00e4u\u00b7men", "scherz", "ich", "schon", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mein so redliches Verlangen", "tokens": ["Und", "mein", "so", "red\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhizt die sehnliche Begier,", "tokens": ["Er\u00b7hizt", "die", "sehn\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich gar und g\u00e4nzlich zu umfangen.", "tokens": ["Dich", "gar", "und", "g\u00e4nz\u00b7lich", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Stunden eilt und bringt den Tag,", "tokens": ["Ihr", "Stun\u00b7den", "eilt", "und", "bringt", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An dem sie mich erquicken mag.", "tokens": ["An", "dem", "sie", "mich", "er\u00b7qui\u00b7cken", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVINF", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Viel Freyer haben dich begehrt,", "tokens": ["Viel", "Frey\u00b7er", "ha\u00b7ben", "dich", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du allerliebste Christiane;", "tokens": ["Du", "al\u00b7ler\u00b7liebs\u00b7te", "Chris\u00b7ti\u00b7a\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin kaum deiner Neigung werth,", "tokens": ["Ich", "bin", "kaum", "dei\u00b7ner", "Nei\u00b7gung", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du f\u00fchrst mich auf die Ehrenbahne,", "tokens": ["Du", "f\u00fchrst", "mich", "auf", "die", "Eh\u00b7ren\u00b7bah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Indem ein Weib von deiner Art", "tokens": ["In\u00b7dem", "ein", "Weib", "von", "dei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts zu des Mannes Ehre spart.", "tokens": ["Nichts", "zu", "des", "Man\u00b7nes", "Eh\u00b7re", "spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Komm bald und f\u00fclle mir den Arm,", "tokens": ["Komm", "bald", "und", "f\u00fcl\u00b7le", "mir", "den", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist die Seele meines Lebens,", "tokens": ["Du", "bist", "die", "See\u00b7le", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch dich wird selbst mein Feuer warm", "tokens": ["Durch", "dich", "wird", "selbst", "mein", "Feu\u00b7er", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und darum sprech ich nicht vergebens:", "tokens": ["Und", "da\u00b7rum", "sprech", "ich", "nicht", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein solcher ", "tokens": ["Ein", "sol\u00b7cher"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Da\u00df man sich vor der Zeit verzehrt.", "tokens": ["Da\u00df", "man", "sich", "vor", "der", "Zeit", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das, was ich selbst nicht schreiben kan,", "tokens": ["Das", ",", "was", "ich", "selbst", "nicht", "schrei\u00b7ben", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wird mein Engel be\u00dfer f\u00fchlen.", "tokens": ["Das", "wird", "mein", "En\u00b7gel", "be\u00b7\u00dfer", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag bricht nach und nach heran,", "tokens": ["Der", "Tag", "bricht", "nach", "und", "nach", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "KON", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Reizung zeigt mir schon das Spielen,", "tokens": ["Die", "Rei\u00b7zung", "zeigt", "mir", "schon", "das", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier geb ich mich in deiner Schoos", "tokens": ["Hier", "geb", "ich", "mich", "in", "dei\u00b7ner", "Schoos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit aller Lieb und Eintracht blos.", "tokens": ["Mit", "al\u00b7ler", "Lieb", "und", "Ein\u00b7tracht", "blos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Himmel hat es so gef\u00fcgt,", "tokens": ["Der", "Him\u00b7mel", "hat", "es", "so", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sollen miteinander scherzen.", "tokens": ["Wir", "sol\u00b7len", "mi\u00b7tein\u00b7an\u00b7der", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was dir und mir im Sinne liegt,", "tokens": ["Was", "dir", "und", "mir", "im", "Sin\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sind in Warheit gute Schmerzen,", "tokens": ["Das", "sind", "in", "War\u00b7heit", "gu\u00b7te", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ein- und andre Friedensnacht", "tokens": ["Die", "ein", "und", "and\u00b7re", "Frie\u00b7dens\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In kurzer Zeit zur Wollust macht.", "tokens": ["In", "kur\u00b7zer", "Zeit", "zur", "Wol\u00b7lust", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Mein Engel liebt, ich liebe mit,", "tokens": ["Mein", "En\u00b7gel", "liebt", ",", "ich", "lie\u00b7be", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": 