, "tokens": ["Durch", "dich", "wird", "selbst", "mein", "Feu\u00b7er", "warm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und darum sprech ich nicht vergebens:", "tokens": ["Und", "da\u00b7rum", "sprech", "ich", "nicht", "ver\u00b7ge\u00b7bens", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein solcher ", "tokens": ["Ein", "sol\u00b7cher"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Da\u00df man sich vor der Zeit verzehrt.", "tokens": ["Da\u00df", "man", "sich", "vor", "der", "Zeit", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Das, was ich selbst nicht schreiben kan,", "tokens": ["Das", ",", "was", "ich", "selbst", "nicht", "schrei\u00b7ben", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wird mein Engel be\u00dfer f\u00fchlen.", "tokens": ["Das", "wird", "mein", "En\u00b7gel", "be\u00b7\u00dfer", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag bricht nach und nach heran,", "tokens": ["Der", "Tag", "bricht", "nach", "und", "nach", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "KON", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Reizung zeigt mir schon das Spielen,", "tokens": ["Die", "Rei\u00b7zung", "zeigt", "mir", "schon", "das", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier geb ich mich in deiner Schoos", "tokens": ["Hier", "geb", "ich", "mich", "in", "dei\u00b7ner", "Schoos"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit aller Lieb und Eintracht blos.", "tokens": ["Mit", "al\u00b7ler", "Lieb", "und", "Ein\u00b7tracht", "blos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Der Himmel hat es so gef\u00fcgt,", "tokens": ["Der", "Him\u00b7mel", "hat", "es", "so", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sollen miteinander scherzen.", "tokens": ["Wir", "sol\u00b7len", "mi\u00b7tein\u00b7an\u00b7der", "scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was dir und mir im Sinne liegt,", "tokens": ["Was", "dir", "und", "mir", "im", "Sin\u00b7ne", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KON", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sind in Warheit gute Schmerzen,", "tokens": ["Das", "sind", "in", "War\u00b7heit", "gu\u00b7te", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ein- und andre Friedensnacht", "tokens": ["Die", "ein", "und", "and\u00b7re", "Frie\u00b7dens\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In kurzer Zeit zur Wollust macht.", "tokens": ["In", "kur\u00b7zer", "Zeit", "zur", "Wol\u00b7lust", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}