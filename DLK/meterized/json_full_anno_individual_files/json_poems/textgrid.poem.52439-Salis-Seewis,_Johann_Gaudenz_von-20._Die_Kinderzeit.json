{"textgrid.poem.52439": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "20. Die Kinderzeit", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfe Zeit herzinniger Gef\u00fchle", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Zeit", "her\u00b7zin\u00b7ni\u00b7ger", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kindlichkeit!", "tokens": ["Der", "Kind\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie denk' ich dein so gern im Weltgew\u00fchle,", "tokens": ["Wie", "denk'", "ich", "dein", "so", "gern", "im", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du s\u00fc\u00dfe Zeit!", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Schon ist in Nacht des Lebens viel geschwunden;", "tokens": ["Schon", "ist", "in", "Nacht", "des", "Le\u00b7bens", "viel", "ge\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du strahlst von fern", "tokens": ["Du", "strahlst", "von", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir heller stets, wie durch der D\u00e4mm'rung Stunden", "tokens": ["Mir", "hel\u00b7ler", "stets", ",", "wie", "durch", "der", "D\u00e4m\u00b7m'\u00b7rung", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Abendstern.", "tokens": ["Der", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Noch seh' ich sie als Kind in holdem Sinnen", "tokens": ["Noch", "seh'", "ich", "sie", "als", "Kind", "in", "hol\u00b7dem", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Veilchen sp\u00e4hn,", "tokens": ["Nach", "Veil\u00b7chen", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr blondes Haar, ihr Lenzgewand von Linnen", "tokens": ["Ihr", "blon\u00b7des", "Haar", ",", "ihr", "Lenz\u00b7ge\u00b7wand", "von", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Winde wehn.", "tokens": ["Im", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Noch schwebt vor mir die gr\u00fcne Seidenschleife,", "tokens": ["Noch", "schwebt", "vor", "mir", "die", "gr\u00fc\u00b7ne", "Sei\u00b7den\u00b7schlei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dort sie trug;", "tokens": ["Die", "dort", "sie", "trug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich w\u00fc\u00dfte noch die Farbe jeder Streife", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "noch", "die", "Far\u00b7be", "je\u00b7der", "Strei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Busentuch.", "tokens": ["Am", "Bu\u00b7sen\u00b7tuch", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Vom Wiesenplan, wohin wir Knaben kamen", "tokens": ["Vom", "Wie\u00b7sen\u00b7plan", ",", "wo\u00b7hin", "wir", "Kna\u00b7ben", "ka\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum M\u00e4dchenkreis,", "tokens": ["Zum", "M\u00e4d\u00b7chen\u00b7kreis", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Behielt ich mehr, als ich vom Kreis der Damen", "tokens": ["Be\u00b7hielt", "ich", "mehr", ",", "als", "ich", "vom", "Kreis", "der", "Da\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach Tagen wei\u00df.", "tokens": ["Nach", "Ta\u00b7gen", "wei\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "O s\u00fc\u00dfe Zeit! als ich von Haselhecken", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Zeit", "!", "als", "ich", "von", "Ha\u00b7sel\u00b7he\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Pferd mir schnitt,", "tokens": ["Mein", "Pferd", "mir", "schnitt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und rasch einher auf dem gestreiften Stecken", "tokens": ["Und", "rasch", "ein\u00b7her", "auf", "dem", "ge\u00b7streif\u00b7ten", "Ste\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Feld durchritt.", "tokens": ["Das", "Feld", "durc\u00b7hritt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Da reizten mich, statt eitler Lorbeerkr\u00e4nze,", "tokens": ["Da", "reiz\u00b7ten", "mich", ",", "statt", "eit\u00b7ler", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Violen nur;", "tokens": ["Vi\u00b7o\u00b7len", "nur", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Landguts Hag war meiner W\u00fcnsche Grenze,", "tokens": ["Des", "Land\u00b7guts", "Hag", "war", "mei\u00b7ner", "W\u00fcn\u00b7sche", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Hof die Flur;", "tokens": ["Mein", "Hof", "die", "Flur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Vergn\u00fcgt, wenn ich Soldatenheer' aus Bleie", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "wenn", "ich", "Sol\u00b7da\u00b7ten\u00b7heer'", "aus", "Blei\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Schau gestellt,", "tokens": ["Zur", "Schau", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und stolzer, als vor meiner Krieger Reihe,", "tokens": ["Und", "stol\u00b7zer", ",", "als", "vor", "mei\u00b7ner", "Krie\u00b7ger", "Rei\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "KOUS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Waffenfeld.", "tokens": ["Im", "Waf\u00b7fen\u00b7feld", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Ganz unbekannt war, was mein Herz begehrte,", "tokens": ["Ganz", "un\u00b7be\u00b7kannt", "war", ",", "was", "mein", "Herz", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu klein dem Neid.", "tokens": ["Zu", "klein", "dem", "Neid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mich k\u00fcmmerten nicht F\u00fcrsten, nicht Gelehrte,", "tokens": ["Mich", "k\u00fcm\u00b7mer\u00b7ten", "nicht", "F\u00fcrs\u00b7ten", ",", "nicht", "Ge\u00b7lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht beider Streit.", "tokens": ["Nicht", "bei\u00b7der", "Streit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "O s\u00fc\u00dfe Zeit! Durchbebt von Wehmutsschauer,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Zeit", "!", "Durch\u00b7bebt", "von", "Weh\u00b7muts\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedenk' ich dein;", "tokens": ["Ge\u00b7denk'", "ich", "dein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Blick nach dir, getr\u00fcbt von sp\u00e4t'rer Trauer,", "tokens": ["Den", "Blick", "nach", "dir", ",", "ge\u00b7tr\u00fcbt", "von", "sp\u00e4t'\u00b7rer", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hellt Abendschein.", "tokens": ["Hellt", "A\u00b7bend\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Gespielen, wir sind nun ver\u00e4ndert, \u00e4lter", "tokens": ["Ge\u00b7spie\u00b7len", ",", "wir", "sind", "nun", "ver\u00b7\u00e4n\u00b7dert", ",", "\u00e4l\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weit zerstreut;", "tokens": ["Und", "weit", "zer\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch mancher, ach! zu weltklug, h\u00f6hnt nun k\u00e4lter", "tokens": ["Auch", "man\u00b7cher", ",", "ach", "!", "zu", "welt\u00b7klug", ",", "h\u00f6hnt", "nun", "k\u00e4l\u00b7ter"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "ITJ", "$.", "PTKZU", "VVFIN", "$,", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Herzlichkeit.", "tokens": ["Die", "Herz\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Weg ist die Bank, wo wir uns abends setzten,", "tokens": ["Weg", "ist", "die", "Bank", ",", "wo", "wir", "uns", "a\u00b7bends", "setz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00f6d' ihr Raum;", "tokens": ["Und", "\u00f6d'", "ihr", "Raum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der niedre Strauch, an dem wir uns erg\u00f6tzten,", "tokens": ["Der", "nied\u00b7re", "Strauch", ",", "an", "dem", "wir", "uns", "er\u00b7g\u00f6tz\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwuchs zum Baum.", "tokens": ["Er\u00b7wuchs", "zum", "Baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Der Zwang zerri\u00df, am fremden Brautaltare,", "tokens": ["Der", "Zwang", "zer\u00b7ri\u00df", ",", "am", "frem\u00b7den", "Brau\u00b7tal\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens Plan,", "tokens": ["Des", "Her\u00b7zens", "Plan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und manchen trug die schwarze Totenbahre", "tokens": ["Und", "man\u00b7chen", "trug", "die", "schwar\u00b7ze", "To\u00b7ten\u00b7bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Ziel der Bahn.", "tokens": ["Zum", "Ziel", "der", "Bahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Klein ward der Kreis! die Abendwolken senken", "tokens": ["Klein", "ward", "der", "Kreis", "!", "die", "Ab\u00b7end\u00b7wol\u00b7ken", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich tief herein;", "tokens": ["Sich", "tief", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wer \u00fcbrig blieb, mu\u00df manchem Angedenken", "tokens": ["Wer", "\u00fcb\u00b7rig", "blieb", ",", "mu\u00df", "man\u00b7chem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon Seufzer weih'n.", "tokens": ["Schon", "Seuf\u00b7zer", "weih'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["ADV", "NN", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "O s\u00fc\u00dfe Zeit herzinniger Gef\u00fchle", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Zeit", "her\u00b7zin\u00b7ni\u00b7ger", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kindlichkeit!", "tokens": ["Der", "Kind\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie denk' ich dein so gern im Weltgew\u00fchle,", "tokens": ["Wie", "denk'", "ich", "dein", "so", "gern", "im", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du s\u00fc\u00dfe Zeit!", "tokens": ["Du", "s\u00fc\u00b7\u00dfe", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Schon ist in Nacht des Lebens viel geschwunden;", "tokens": ["Schon", "ist", "in", "Nacht", "des", "Le\u00b7bens", "viel", "ge\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du strahlst von fern", "tokens": ["Du", "strahlst", "von", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mir heller stets, wie durch der D\u00e4mm'rung Stunden", "tokens": ["Mir", "hel\u00b7ler", "stets", ",", "wie", "durch", "der", "D\u00e4m\u00b7m'\u00b7rung", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Abendstern.", "tokens": ["Der", "A\u00b7bends\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.17": {"line.1": {"text": "Noch seh' ich sie als Kind in holdem Sinnen", "tokens": ["Noch", "seh'", "ich", "sie", "als", "Kind", "in", "hol\u00b7dem", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Veilchen sp\u00e4hn,", "tokens": ["Nach", "Veil\u00b7chen", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr blondes Haar, ihr Lenzgewand von Linnen", "tokens": ["Ihr", "blon\u00b7des", "Haar", ",", "ihr", "Lenz\u00b7ge\u00b7wand", "von", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Winde wehn.", "tokens": ["Im", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Noch schwebt vor mir die gr\u00fcne Seidenschleife,", "tokens": ["Noch", "schwebt", "vor", "mir", "die", "gr\u00fc\u00b7ne", "Sei\u00b7den\u00b7schlei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dort sie trug;", "tokens": ["Die", "dort", "sie", "trug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich w\u00fc\u00dfte noch die Farbe jeder Streife", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "noch", "die", "Far\u00b7be", "je\u00b7der", "Strei\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "wo