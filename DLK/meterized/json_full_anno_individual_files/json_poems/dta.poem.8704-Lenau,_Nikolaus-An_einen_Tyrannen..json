{"dta.poem.8704": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einen Tyrannen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Tyrann! des Blutes, welches in Schlachten du", "tokens": ["Ty\u00b7rann", "!", "des", "Blu\u00b7tes", ",", "wel\u00b7ches", "in", "Schlach\u00b7ten", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "$,", "PRELS", "APPR", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vergossen kalt, das rauchte vom Henkerbeil,", "tokens": ["Ver\u00b7gos\u00b7sen", "kalt", ",", "das", "rauch\u00b7te", "vom", "Hen\u00b7ker\u00b7beil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das, deinen Qualen zu entrinnen,", "tokens": ["Das", ",", "dei\u00b7nen", "Qua\u00b7len", "zu", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6mte dein Sklave mit eigner Hand hin:", "tokens": ["Str\u00f6m\u00b7te", "dein", "Skla\u00b7ve", "mit", "eig\u00b7ner", "Hand", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Vor deinem Aug' in strafender Ewigkeit", "tokens": ["Vor", "dei\u00b7nem", "Aug'", "in", "stra\u00b7fen\u00b7der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aufsch\u00e4umen, schwellen zum Vulkane,", "tokens": ["Auf\u00b7sch\u00e4u\u00b7men", ",", "schwel\u00b7len", "zum", "Vul\u00b7ka\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der von den Seligen streng dich scheidet!", "tokens": ["Der", "von", "den", "Se\u00b7li\u00b7gen", "streng", "dich", "schei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Erwacht dann Sehnsucht hei\u00df in der Seele dir", "tokens": ["Er\u00b7wacht", "dann", "Sehn\u00b7sucht", "hei\u00df", "in", "der", "See\u00b7le", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "NN", "ADJD", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hin\u00fcber in die Th\u00e4ler Elysiums,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "in", "die", "Th\u00e4\u00b7ler", "E\u00b7ly\u00b7si\u00b7ums", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Willst \u00fcberklimmen du die H\u00f6h'n, dann", "tokens": ["Willst", "\u00fc\u00b7ber\u00b7klim\u00b7men", "du", "die", "H\u00f6h'n", ",", "dann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schleudren sie dich in die Tiefe donnernd!", "tokens": ["Schleu\u00b7dren", "sie", "dich", "in", "die", "Tie\u00b7fe", "don\u00b7nernd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Entgegen glei\u00dfe deinem entsezten Blick", "tokens": ["Ent\u00b7ge\u00b7gen", "glei\u00b7\u00dfe", "dei\u00b7nem", "ent\u00b7sez\u00b7ten", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein Schneegebirg von Menschengebeinen, hoch;", "tokens": ["Ein", "Schnee\u00b7ge\u00b7birg", "von", "Men\u00b7schen\u00b7ge\u00b7bei\u00b7nen", ",", "hoch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dar\u00fcber bleich und unbeweglich", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "bleich", "und", "un\u00b7be\u00b7weg\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Starre des Mondes bek\u00fcmmert Antlitz.", "tokens": ["Star\u00b7re", "des", "Mon\u00b7des", "be\u00b7k\u00fcm\u00b7mert", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dann stocke, schweige jenes Gebirg des Bluts,", "tokens": ["Dann", "sto\u00b7cke", ",", "schwei\u00b7ge", "je\u00b7nes", "Ge\u00b7birg", "des", "Bluts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Her\u00fcberklinge deinem verlass'nen Ohr", "tokens": ["Her\u00b7\u00fc\u00b7berk\u00b7lin\u00b7ge", "dei\u00b7nem", "ver\u00b7lass'\u00b7nen", "Ohr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das Wonnelied der Auserw\u00e4hlten,", "tokens": ["Das", "Won\u00b7ne\u00b7lied", "der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00e4uselnd, unendliche Sehnsucht weckend.", "tokens": ["S\u00e4u\u00b7selnd", ",", "un\u00b7end\u00b7li\u00b7che", "Sehn\u00b7sucht", "we\u00b7ckend", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch pl\u00f6tzlich st\u00f6re Kettengerassel dich,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "st\u00f6\u00b7re", "Ket\u00b7ten\u00b7ge\u00b7ras\u00b7sel", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Sterbgewinsel, das durch die L\u00fcfte klagt,", "tokens": ["Und", "Sterb\u00b7ge\u00b7win\u00b7sel", ",", "das", "durch", "die", "L\u00fcf\u00b7te", "klagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und heulend rolle dir die Windsbraut", "tokens": ["Und", "heu\u00b7lend", "rol\u00b7le", "dir", "die", "Winds\u00b7braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sch\u00e4dellawinen vor deine F\u00fc\u00dfe!", "tokens": ["Sch\u00e4\u00b7del\u00b7la\u00b7wi\u00b7nen", "vor", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}