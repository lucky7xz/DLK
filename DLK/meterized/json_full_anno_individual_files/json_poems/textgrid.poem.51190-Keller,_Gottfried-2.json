], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wir auf Lenzesschwingen,", "tokens": ["Doch", "wir", "auf", "Len\u00b7zes\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Spielen und mit Singen,", "tokens": ["Mit", "Spie\u00b7len", "und", "mit", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir m\u00fcssen in die Furchen dann", "tokens": ["Wir", "m\u00fcs\u00b7sen", "in", "die", "Fur\u00b7chen", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den neuen Samen bringen.", "tokens": ["Den", "neu\u00b7en", "Sa\u00b7men", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr brecht die Bahn durch finstre Nacht,", "tokens": ["Ihr", "brecht", "die", "Bahn", "durch", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fackel in der sichern Hand;", "tokens": ["Die", "Fa\u00b7ckel", "in", "der", "si\u00b7chern", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr seid die Vorhut und die Wacht,", "tokens": ["Ihr", "seid", "die", "Vor\u00b7hut", "und", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sengt und brennt in Feindesland;", "tokens": ["Ihr", "sengt", "und", "brennt", "in", "Fein\u00b7des\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor der Posaune Schallen", "tokens": ["Vor", "der", "Po\u00b7sau\u00b7ne", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Jericho gefallen:", "tokens": ["Ist", "Je\u00b7ri\u00b7cho", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor eurer Tuba st\u00fcrzen selbst", "tokens": ["Vor", "eu\u00b7rer", "Tu\u00b7ba", "st\u00fcr\u00b7zen", "selbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Himmels h\u00f6chste Hallen!", "tokens": ["Des", "Him\u00b7mels", "h\u00f6chs\u00b7te", "Hal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dann aber folgt der S\u00e4nger Schar,", "tokens": ["Dann", "a\u00b7ber", "folgt", "der", "S\u00e4n\u00b7ger", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen neuen Himmel baut,", "tokens": ["Die", "ei\u00b7nen", "neu\u00b7en", "Him\u00b7mel", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darinnen man im Lichttalar", "tokens": ["Da\u00b7rin\u00b7nen", "man", "im", "Licht\u00b7ta\u00b7lar"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den alten Gott der Liebe schaut!", "tokens": ["Den", "al\u00b7ten", "Gott", "der", "Lie\u00b7be", "schaut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Voran, voran, ihr Bittern,", "tokens": ["Vo\u00b7ran", ",", "vo\u00b7ran", ",", "ihr", "Bit\u00b7tern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In fegenden Gewittern!", "tokens": ["In", "fe\u00b7gen\u00b7den", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir ziehen heilend, segnend nach", "tokens": ["Wir", "zie\u00b7hen", "hei\u00b7lend", ",", "seg\u00b7nend", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit hell gestimmten Zithern!", "tokens": ["Mit", "hell", "ge\u00b7stimm\u00b7ten", "Zi\u00b7thern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}