{"textgrid.poem.58651": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, als der verlorne Sohn, hatt' mich fern hinweg begeben", "tokens": ["Ich", ",", "als", "der", "ver\u00b7lor\u00b7ne", "Sohn", ",", "hatt'", "mich", "fern", "hin\u00b7weg", "be\u00b7ge\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADJD", "ADV", "VVPP"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In die Welt und Eigenheit, fern von Gott und seinem Leben;", "tokens": ["In", "die", "Welt", "und", "Ei\u00b7gen\u00b7heit", ",", "fern", "von", "Gott", "und", "sei\u00b7nem", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "ADJD", "APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mir verirrter Kreatur ist der Herr erschienen da,", "tokens": ["Mir", "ver\u00b7irr\u00b7ter", "Kre\u00b7a\u00b7tur", "ist", "der", "Herr", "er\u00b7schie\u00b7nen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Seine Liebe dacht' an mich, seine G\u00fcte zog mich nah.", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "dacht'", "an", "mich", ",", "sei\u00b7ne", "G\u00fc\u00b7te", "zog", "mich", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", 