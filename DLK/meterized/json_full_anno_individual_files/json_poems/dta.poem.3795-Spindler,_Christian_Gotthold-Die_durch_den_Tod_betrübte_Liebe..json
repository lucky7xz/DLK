{"dta.poem.3795": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die durch den Tod betr\u00fcbte Liebe.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Die Elendsvolle Sterblichkeit", "tokens": ["Die", "E\u00b7lends\u00b7vol\u00b7le", "Sterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat mir mein bestes hingenommen,", "tokens": ["Hat", "mir", "mein", "bes\u00b7tes", "hin\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch den Hinfall dieser Zeit", "tokens": ["Und", "durch", "den", "Hin\u00b7fall", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich um meinen Schatz gekommen!", "tokens": ["Bin", "ich", "um", "mei\u00b7nen", "Schatz", "ge\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O! Freund! hilff mir mein Liebstes klagen,", "tokens": ["O", "!", "Freund", "!", "hilff", "mir", "mein", "Liebs\u00b7tes", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bemercke nur mein banges Zagen,", "tokens": ["Be\u00b7mer\u00b7cke", "nur", "mein", "ban\u00b7ges", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der herbe Schmerz ist gar zu arg,", "tokens": ["Der", "her\u00b7be", "Schmerz", "ist", "gar", "zu", "arg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es droht Charlotten nun ein Sarg.", "tokens": ["Es", "droht", "Char\u00b7lot\u00b7ten", "nun", "ein", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie starb; Charlotte ist nun todt!", "tokens": ["Sie", "starb", ";", "Char\u00b7lot\u00b7te", "ist", "nun", "todt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Anmuths voll und holden Wangen,", "tokens": ["Die", "An\u00b7muths", "voll", "und", "hol\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr angenehmes Purpur-Roth,", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7mes", "Pur\u00b7pur\u00b7Roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mir durch diesen Fall entgangen!", "tokens": ["Ist", "mir", "durch", "die\u00b7sen", "Fall", "ent\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach herbe Trennung s\u00fcsser Liebe!", "tokens": ["Ach", "her\u00b7be", "Tren\u00b7nung", "s\u00fcs\u00b7ser", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ach kurtze Dauer edler Triebe,", "tokens": ["Ach", "kurt\u00b7ze", "Dau\u00b7er", "ed\u00b7ler", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach, meinen frommen Gegenstand", "tokens": ["Ach", ",", "mei\u00b7nen", "from\u00b7men", "Ge\u00b7gen\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Deckt Erde, Moder, Asch und Sand.", "tokens": ["Deckt", "Er\u00b7de", ",", "Mo\u00b7der", ",", "Asch", "und", "Sand", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm Freund! geh mit zu ihrem Grabe,", "tokens": ["Komm", "Freund", "!", "geh", "mit", "zu", "ih\u00b7rem", "Gra\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, hilff mir doch Cypressen streun;", "tokens": ["Komm", ",", "hilff", "mir", "doch", "Cyp\u00b7res\u00b7sen", "streun", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die ich bi\u00dfher geliebet habe,", "tokens": ["Die", "ich", "bi\u00df\u00b7her", "ge\u00b7lie\u00b7bet", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Soll nun ein Raub der W\u00fcrmer seyn.", "tokens": ["Soll", "nun", "ein", "Raub", "der", "W\u00fcr\u00b7mer", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich die redlichen Gebeine", "tokens": ["Da", "ich", "die", "red\u00b7li\u00b7chen", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In ihrer Grufft mit ach! beweine,", "tokens": ["In", "ih\u00b7rer", "Grufft", "mit", "ach", "!", "be\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ITJ", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O! treuster Freund! so dichte mit,", "tokens": ["O", "!", "treus\u00b7ter", "Freund", "!", "so", "dich\u00b7te", "mit", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$.", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein fromm und banges Trauerlied.", "tokens": ["Ein", "fromm", "und", "ban\u00b7ges", "Trau\u00b7er\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie stirbt im Morgen ihrer Jahre", "tokens": ["Sie", "stirbt", "im", "Mor\u00b7gen", "ih\u00b7rer", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegt nun meine Lust und Treu!", "tokens": ["Da", "liegt", "nun", "mei\u00b7ne", "Lust", "und", "Treu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist denn von der Todten-Bahre", "tokens": ["So", "ist", "denn", "von", "der", "Tod\u00b7ten\u00b7Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die \u00e4chte Tugend selbst nicht frey?", "tokens": ["Die", "\u00e4ch\u00b7te", "Tu\u00b7gend", "selbst", "nicht", "frey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch diese kan es nicht verwehren?", "tokens": ["Auch", "die\u00b7se", "kan", "es", "nicht", "ver\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df denn den Leib die Grufft zerst\u00f6hren?", "tokens": ["Mu\u00df", "denn", "den", "Leib", "die", "Grufft", "zer\u00b7st\u00f6h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie? wird denn, was so tugendhafft,", "tokens": ["Wie", "?", "wird", "denn", ",", "was", "so", "tu\u00b7gend\u00b7hafft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch von dem Tode hingerafft?", "tokens": ["Auch", "von", "dem", "To\u00b7de", "hin\u00b7ge\u00b7rafft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der herbe Ri\u00df, so mich ger\u00fchret,", "tokens": ["Der", "her\u00b7be", "Ri\u00df", ",", "so", "mich", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entgeistert meinen schwachen Geist.", "tokens": ["Ent\u00b7geis\u00b7tert", "mei\u00b7nen", "schwa\u00b7chen", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer schon sein ander Hertz verliehret,", "tokens": ["Wer", "schon", "sein", "an\u00b7der", "Hertz", "ver\u00b7lieh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es der Tod von hinnen rei\u00dft,", "tokens": ["Wenn", "es", "der", "Tod", "von", "hin\u00b7nen", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem mu\u00df auf diesem Rund der Erden", "tokens": ["Dem", "mu\u00df", "auf", "die\u00b7sem", "Rund", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gantze Welt zu enge werden.", "tokens": ["Die", "gant\u00b7ze", "Welt", "zu", "en\u00b7ge", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du kennst schon, Freund! dergleichen Quaal,", "tokens": ["Du", "kennst", "schon", ",", "Freund", "!", "derg\u00b7lei\u00b7chen", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "$.", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Man st\u00fcrbe lieber tausendmahl.", "tokens": ["Man", "st\u00fcr\u00b7be", "lie\u00b7ber", "tau\u00b7send\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So bist du mir nunmehr entrissen,", "tokens": ["So", "bist", "du", "mir", "nun\u00b7mehr", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Charlotte! meine gr\u00f6ste Lust!", "tokens": ["Char\u00b7lot\u00b7te", "!", "mei\u00b7ne", "gr\u00f6s\u00b7te", "Lust", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll ich dich auf ewig missen?", "tokens": ["So", "soll", "ich", "dich", "auf", "e\u00b7wig", "mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier zittert Hertze Geist und Brust.", "tokens": ["Hier", "zit\u00b7tert", "Hert\u00b7ze", "Geist", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du must mir auch im Grabe g\u00f6nnen,", "tokens": ["Du", "must", "mir", "auch", "im", "Gra\u00b7be", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mich darff den deinen nennen.", "tokens": ["Da\u00df", "ich", "mich", "darff", "den", "dei\u00b7nen", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "ART", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So hefftig bebt kein sch\u00fcchtern Wild,", "tokens": ["So", "heff\u00b7tig", "bebt", "kein", "sch\u00fcch\u00b7tern", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als wie mein Hertz mit Angst erf\u00fcllt.", "tokens": ["Als", "wie", "mein", "Hertz", "mit", "Angst", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Schlaff sanfft in deiner Ruhekammer!", "tokens": ["Schlaff", "sanfft", "in", "dei\u00b7ner", "Ru\u00b7he\u00b7kam\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schmuck von deinem Leichenstein", "tokens": ["Der", "Schmuck", "von", "dei\u00b7nem", "Lei\u00b7chen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Soll bey mir thr\u00e4nend mit viel Jammer", "tokens": ["Soll", "bey", "mir", "thr\u00e4\u00b7nend", "mit", "viel", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Hertzen aufgerichtet seyn.", "tokens": ["Jm", "Hert\u00b7zen", "auf\u00b7ge\u00b7rich\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Hertz sey deine Leichen-H\u00f6le,", "tokens": ["Mein", "Hertz", "sey", "dei\u00b7ne", "Lei\u00b7chen\u00b7H\u00f6le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du andre Helffte meiner Seele!", "tokens": ["Du", "and\u00b7re", "Helff\u00b7te", "mei\u00b7ner", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da mich dein Tod so sehr betr\u00fcbt,", "tokens": ["Da", "mich", "dein", "Tod", "so", "sehr", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So werd ich auch nicht mehr verliebt.", "tokens": ["So", "werd", "ich", "auch", "nicht", "mehr", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}