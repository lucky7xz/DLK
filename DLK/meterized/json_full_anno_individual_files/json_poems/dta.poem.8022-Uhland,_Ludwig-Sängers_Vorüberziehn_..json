{"dta.poem.8022": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "S\u00e4ngers Vor\u00fcberziehn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schlief am Bl\u00fcthenh\u00fcgel,", "tokens": ["Ich", "schlief", "am", "Bl\u00fc\u00b7then\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hart an des Pfades Rand.", "tokens": ["Hart", "an", "des", "Pfa\u00b7des", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da lieh der Traum mir Fl\u00fcgel", "tokens": ["Da", "lieh", "der", "Traum", "mir", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In\u2019s goldne Fabelland.", "tokens": ["In's", "gold\u00b7ne", "Fa\u00b7bel\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Erwacht, mit trunknen Blicken,", "tokens": ["Er\u00b7wacht", ",", "mit", "trunk\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wer aus Wolken fiel,", "tokens": ["Wie", "wer", "aus", "Wol\u00b7ken", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gewahr\u2019 ich noch im R\u00fccken", "tokens": ["Ge\u00b7wahr'", "ich", "noch", "im", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den S\u00e4nger mit dem Spiel.", "tokens": ["Den", "S\u00e4n\u00b7ger", "mit", "dem", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er schwindet um die B\u00e4ume,", "tokens": ["Er", "schwin\u00b7det", "um", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch h\u00f6r\u2019 ich fernen Klang.", "tokens": ["Noch", "h\u00f6r'", "ich", "fer\u00b7nen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob der die Wundertr\u00e4ume", "tokens": ["Ob", "der", "die", "Wun\u00b7der\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir in die Seele sang?", "tokens": ["Mir", "in", "die", "See\u00b7le", "sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}