{"textgrid.poem.47767": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[trage nicht so grelle T\u00f6ne vor]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trage nicht so grelle T\u00f6ne vor,", "tokens": ["Tra\u00b7ge", "nicht", "so", "grel\u00b7le", "T\u00f6\u00b7ne", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Prediger! Zu weichlich ist mein Ohr,", "tokens": ["Pre\u00b7di\u00b7ger", "!", "Zu", "weich\u00b7lich", "ist", "mein", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ist zu musikalischer Natur,", "tokens": ["Ist", "zu", "mu\u00b7si\u00b7ka\u00b7li\u00b7scher", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die s\u00fc\u00dfe S\u00e4ngerin der Flur,", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfe", "S\u00e4n\u00b7ge\u00b7rin", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der es horcht, hat es so ganz verw\u00f6hnt,", "tokens": ["Der", "es", "horcht", ",", "hat", "es", "so", "ganz", "ver\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df es schmerzt, wenn deine Stimme t\u00f6nt.", "tokens": ["Da\u00df", "es", "schmerzt", ",", "wenn", "dei\u00b7ne", "Stim\u00b7me", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Willst du nicht, da\u00df ich in's Weite flieh',", "tokens": ["Willst", "du", "nicht", ",", "da\u00df", "ich", "in's", "Wei\u00b7te", "flieh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "So versuch' es und verwandle sie", "tokens": ["So", "ver\u00b7such'", "es", "und", "ver\u00b7wand\u00b7le", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "In gelinde, zarte Melodie!", "tokens": ["In", "ge\u00b7lin\u00b7de", ",", "zar\u00b7te", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Trage nicht so grelle T\u00f6ne vor,", "tokens": ["Tra\u00b7ge", "nicht", "so", "grel\u00b7le", "T\u00f6\u00b7ne", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Prediger! Zu weichlich ist mein Ohr,", "tokens": ["Pre\u00b7di\u00b7ger", "!", "Zu", "weich\u00b7lich", "ist", "mein", "Ohr", ","], "token_info": ["word", "punct",