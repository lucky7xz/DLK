{"dta.poem.2583": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mercurius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind auch die G\u00f6tter selbst von deinen Pfeilen wund/", "tokens": ["Sind", "auch", "die", "G\u00f6t\u00b7ter", "selbst", "von", "dei\u00b7nen", "Pfei\u00b7len", "wund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie solte Floramor nicht die Hyell\u2019 umarmen;", "tokens": ["Wie", "sol\u00b7te", "Flo\u00b7ra\u00b7mor", "nicht", "die", "Hyell'", "um\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und der Hyellen Geist in gleicher Glut erwarmen?", "tokens": ["Und", "der", "Hyel\u00b7len", "Geist", "in", "glei\u00b7cher", "Glut", "er\u00b7war\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Jhr sch\u00f6nes Auge winckt/ und schweiget gleich der Mund/", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "Au\u00b7ge", "winckt", "/", "und", "schwei\u00b7get", "gleich", "der", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$(", "KON", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem liebsten Br\u00e4ut\u2019gam zu/ du magst die Sache handeln/", "tokens": ["Dem", "liebs\u00b7ten", "Br\u00e4ut'\u00b7gam", "zu", "/", "du", "magst", "die", "Sa\u00b7che", "han\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich als Bottschaffter mu\u00df noch ferne Wege wandeln.", "tokens": ["Ich", "als", "Bott\u00b7schaff\u00b7ter", "mu\u00df", "noch", "fer\u00b7ne", "We\u00b7ge", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "VMFIN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}