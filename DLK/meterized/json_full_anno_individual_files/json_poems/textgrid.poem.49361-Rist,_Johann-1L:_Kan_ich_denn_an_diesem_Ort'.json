"pos": ["KON", "APPR", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Kan die Flut nicht stille stehn?", "tokens": ["Kan", "die", "Flut", "nicht", "stil\u00b7le", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Mu\u00df sie hin und wider schweben?", "tokens": ["Mu\u00df", "sie", "hin", "und", "wi\u00b7der", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "KON", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ach, was wird denn wol geschehn", "tokens": ["Ach", ",", "was", "wird", "denn", "wol", "ge\u00b7schehn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Unserm schwachen Leben!", "tokens": ["Un\u00b7serm", "schwa\u00b7chen", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Seht, die fl\u00fcgelschnelle Zeit", "tokens": ["Seht", ",", "die", "fl\u00fc\u00b7gel\u00b7schnel\u00b7le", "Zeit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wil bereit", "tokens": ["Wil", "be\u00b7reit"], "token_info": ["word", "word"], "pos": ["VMFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Uns ein Grabmal geben.", "tokens": ["Uns", "ein", "Grab\u00b7mal", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measur