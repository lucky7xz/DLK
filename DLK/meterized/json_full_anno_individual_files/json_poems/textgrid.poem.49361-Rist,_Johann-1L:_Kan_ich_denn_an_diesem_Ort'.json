{"textgrid.poem.49361": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kan ich denn an diesem Ort'", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kan ich denn an diesem Ort'", "tokens": ["Kan", "ich", "denn", "an", "die\u00b7sem", "Ort'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des sch\u00f6nen H\u00fcgels Spitzen,", "tokens": ["Auf", "des", "sch\u00f6\u00b7nen", "H\u00fc\u00b7gels", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der ungest\u00fcme Nord", "tokens": ["Wo", "der", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Nord"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum mich l\u00e4sset sitzen,", "tokens": ["Kaum", "mich", "l\u00e4s\u00b7set", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lauter nichts best\u00e4ndigs sehn?", "tokens": ["Lau\u00b7ter", "nichts", "be\u00b7st\u00e4n\u00b7digs", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df es gehn", "tokens": ["Mu\u00df", "es", "gehn"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schneller als die Blitzen?", "tokens": ["Schnel\u00b7ler