{"textgrid.poem.65333": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der kleine Schreiber", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleiner Schreiber, kleiner Schreiber, h\u00f6r' und la\u00df dein Werfen sein!", "tokens": ["Klei\u00b7ner", "Schrei\u00b7ber", ",", "klei\u00b7ner", "Schrei\u00b7ber", ",", "h\u00f6r'", "und", "la\u00df", "dein", "Wer\u00b7fen", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN", "KON", "VVIMP", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Warfst mir heut' ein St\u00fcckchen Zucker in den Busen grad' hinein.", "tokens": ["Warfst", "mir", "heut'", "ein", "St\u00fcck\u00b7chen", "Zu\u00b7cker", "in", "den", "Bu\u00b7sen", "grad'", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn du wirst noch einmal werfen, zeig' ich es dem Bischof an,", "tokens": ["Wenn", "du", "wirst", "noch", "ein\u00b7mal", "wer\u00b7fen", ",", "zeig'", "ich", "es", "dem", "Bi\u00b7schof", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und er l\u00e4\u00dft das Haar dir scheeren, und er thut dich in den Bann.", "tokens": ["U