{"textgrid.poem.40569": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Kinder, la\u00dft mir verschont", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Kinder, la\u00dft mir verschont", "tokens": ["Ihr", "Kin\u00b7der", ",", "la\u00dft", "mir", "ver\u00b7schont"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zaunk\u00f6nigs Nest und Zelle,", "tokens": ["Zaun\u00b7k\u00f6\u00b7nigs", "Nest", "und", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Denn wo ein Edler wohnt,", "tokens": ["Denn", "wo", "ein", "Ed\u00b7ler", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist eine heilige Stelle.", "tokens": ["Ist", "ei\u00b7ne", "hei\u00b7li\u00b7ge", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn traulich der flammende Herd", "tokens": ["Wenn", "trau\u00b7lich", "der", "flam\u00b7men\u00b7de", "Herd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Euch Z\u00fcnglein belebt und Gedanken", "tokens": ["Euch", "Z\u00fcn\u00b7glein", "be\u00b7lebt", "und", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVPP", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Euch w\u00e4rmt im Frost und euch n\u00e4hrt,", "tokens": ["Euch", "w\u00e4rmt", "im", "Frost", "und", "euch", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Dem V\u00f6glein nur sollt ihr's danken.", "tokens": ["Dem", "V\u00f6\u00b7glein", "nur", "sollt", "ih\u00b7r's", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "In dunkler kalter Zeit,", "tokens": ["In", "dunk\u00b7ler", "kal\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als uns des Feuers Gabe", "tokens": ["Als", "uns", "des", "Feu\u00b7ers", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die G\u00f6tter noch bargen mit Neid,", "tokens": ["Die", "G\u00f6t\u00b7ter", "noch", "bar\u00b7gen", "mit", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wie Ueberreiche ihr Habe;", "tokens": ["Wie", "Ue\u00b7ber\u00b7rei\u00b7che", "ihr", "Ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da in dem V\u00f6glein klein", "tokens": ["Da", "in", "dem", "V\u00f6\u00b7glein", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erwuchs ein gro\u00dfer Gedanke,", "tokens": ["Er\u00b7wuchs", "ein", "gro\u00b7\u00dfer", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es flog in den Himmel hinein,", "tokens": ["Es", "flog", "in", "den", "Him\u00b7mel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Durchbrechend die Wolkenschranke.", "tokens": ["Durch\u00b7bre\u00b7chend", "die", "Wol\u00b7ken\u00b7schran\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Dem Jovisadler, der schlief,", "tokens": ["Dem", "Jo\u00b7vi\u00b7sad\u00b7ler", ",", "der", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ri\u00df es den Brand aus den Krallen;", "tokens": ["Ri\u00df", "es", "den", "Brand", "aus", "den", "Kral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und ob er's auch sengte tief,", "tokens": ["Und", "ob", "er's", "auch", "seng\u00b7te", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Beute lie\u00df es nicht fallen.", "tokens": ["Die", "Beu\u00b7te", "lie\u00df", "es", "nicht", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wie ein st\u00fcrzender Stern", "tokens": ["Und", "wie", "ein", "st\u00fcr\u00b7zen\u00b7der", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Fiel's erdenw\u00e4rts mit den Sch\u00e4tzen;", "tokens": ["Fiel's", "er\u00b7den\u00b7w\u00e4rts", "mit", "den", "Sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da eilten von nah und fern", "tokens": ["Da", "eil\u00b7ten", "von", "nah", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "KON", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Br\u00fcder, den Wunden zu letzen.", "tokens": ["Die", "Br\u00fc\u00b7der", ",", "den", "Wun\u00b7den", "zu", "let\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Die eigenen Federn leiht", "tokens": ["Die", "ei\u00b7ge\u00b7nen", "Fe\u00b7dern", "leiht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihm jeder, die Bl\u00f6\u00dfen zu decken;", "tokens": ["Ihm", "je\u00b7der", ",", "die", "Bl\u00f6\u00b7\u00dfen", "zu", "de\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Drum ist auch sein braunes Kleid", "tokens": ["Drum", "ist", "auch", "sein", "brau\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein Bettlermantel voll Flecken.", "tokens": ["Ein", "Bett\u00b7ler\u00b7man\u00b7tel", "voll", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Rothkehlchen voran! Doch vom Brand", "tokens": ["Roth\u00b7kehl\u00b7chen", "vo\u00b7ran", "!", "Doch", "vom", "Brand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "KON", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ist selbst versengt es worden;", "tokens": ["Ist", "selbst", "ver\u00b7sengt", "es", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "PPER", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00e4gt's noch das rothe Band", "tokens": ["So", "tr\u00e4gt's", "noch", "das", "ro\u00b7the", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Am Busen als Ehrenorden.", "tokens": ["Am", "Bu\u00b7sen", "als", "Eh\u00b7ren\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOUS", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Nur Kukuk, der Gauch, gab nichts", "tokens": ["Nur", "Ku\u00b7kuk", ",", "der", "Gauch", ",", "gab", "nichts"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PIS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als eine gute Lehre:", "tokens": ["Als", "ei\u00b7ne", "gu\u00b7te", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhast du nur die Gr\u00f6\u00dfe des Wichts,", "tokens": ["\u00bb", "hast", "du", "nur", "die", "Gr\u00f6\u00b7\u00dfe", "des", "Wichts", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit G\u00f6ttergluth nicht verkehre!\u00ab", "tokens": ["Mit", "G\u00f6t\u00b7ter\u00b7gluth", "nicht", "ver\u00b7keh\u00b7re", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Zaunk\u00f6nig r\u00e4chte sich auch,", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig", "r\u00e4ch\u00b7te", "sich", "auch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wie nur es Edlen gelungen:", "tokens": ["Wie", "nur", "es", "Ed\u00b7len", "ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er br\u00fctet die Jungen dem Gauch", "tokens": ["Er", "br\u00fc\u00b7tet", "die", "Jun\u00b7gen", "dem", "Gauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Zugleich mit den eigenen Jungen.", "tokens": ["Zu\u00b7gleich", "mit", "den", "ei\u00b7ge\u00b7nen", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Es wurde die ganze Schaar", "tokens": ["Es", "wur\u00b7de", "die", "gan\u00b7ze", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu Aerzten im Heilungsdrange", "tokens": ["Zu", "A\u00b7erz\u00b7ten", "im", "Hei\u00b7lungs\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Grasm\u00fccke mit dem Trokar,", "tokens": ["Gras\u00b7m\u00fc\u00b7cke", "mit", "dem", "Tro\u00b7kar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "++-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Krummschnabel kam mit der Zange.", "tokens": ["Krumm\u00b7schna\u00b7bel", "kam", "mit", "der", "Zan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Die Meise wetzt und weist", "tokens": ["Die", "Mei\u00b7se", "wetzt", "und", "weist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Blutd\u00fcrstig ihr Lanzettchen,", "tokens": ["Blut\u00b7d\u00fcrs\u00b7tig", "ihr", "Lan\u00b7zett\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Wunderpflaster preist", "tokens": ["Als", "Wun\u00b7der\u00b7pflas\u00b7ter", "preist"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Specht ein w\u00fcrzig Bl\u00e4ttchen;", "tokens": ["Der", "Specht", "ein", "w\u00fcr\u00b7zig", "Bl\u00e4tt\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Es f\u00fcllt in der Quelle klar", "tokens": ["Es", "f\u00fcllt", "in", "der", "Quel\u00b7le", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Spritzlein die Bekassine,", "tokens": ["Das", "Spritz\u00b7lein", "die", "Be\u00b7kas\u00b7si\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Kernbei\u00dfer macht sogar", "tokens": ["Kern\u00b7bei\u00b7\u00dfer", "macht", "so\u00b7gar"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Amputiren schon Miene.", "tokens": ["Zum", "Am\u00b7pu\u00b7ti\u00b7ren", "schon", "Mie\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Elster aber entbrennt,", "tokens": ["Die", "Els\u00b7ter", "a\u00b7ber", "ent\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Grauschwesteramt zu verrichten,", "tokens": ["Grauschwes\u00b7ter\u00b7amt", "zu", "ver\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie zupft Charpie und kennt", "tokens": ["Sie", "zupft", "Char\u00b7pie", "und", "kennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hausmittel und Stadtgeschichten.", "tokens": ["Haus\u00b7mit\u00b7tel", "und", "Stadt\u00b7ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Zaunk\u00f6nig mild abwehrt", "tokens": ["Zaun\u00b7k\u00f6\u00b7nig", "mild", "ab\u00b7wehrt"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Sorgen, die sie ihm weihen:", "tokens": ["Die", "Sor\u00b7gen", ",", "die", "sie", "ihm", "wei\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "\u00bbwen himmlisch Feuer versehrt,", "tokens": ["\u00bb", "wen", "himm\u00b7lisch", "Feu\u00b7er", "ver\u00b7sehrt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den heilen nicht ird'sche Arz'n