 "line.5": {"text": "Es zieht dich an, es rei\u00dft dich heiter fort,", "tokens": ["Es", "zieht", "dich", "an", ",", "es", "rei\u00dft", "dich", "hei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wo du wandelst, schm\u00fcckt sich Weg und Ort;", "tokens": ["Und", "wo", "du", "wan\u00b7delst", ",", "schm\u00fcckt", "sich", "Weg", "und", "Ort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du z\u00e4hlst nicht mehr, berechnest keine Zeit,", "tokens": ["Du", "z\u00e4hlst", "nicht", "mehr", ",", "be\u00b7rech\u00b7nest", "kei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und jeder Schritt ist Unerme\u00dflichkeit.", "tokens": ["Und", "je\u00b7der", "Schritt", "ist", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was w\u00e4r ein Gott, der nur von au\u00dfen stie\u00dfe,", "tokens": ["Was", "w\u00e4r", "ein", "Gott", ",", "der", "nur", "von", "au\u00b7\u00dfen", "stie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADV", "VVFIN", "$,"],