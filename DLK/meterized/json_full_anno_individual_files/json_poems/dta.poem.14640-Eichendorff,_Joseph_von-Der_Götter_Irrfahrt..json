{"dta.poem.14640": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00f6tter Irrfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch der Vater hebt auf's neue,", "tokens": ["Doch", "der", "Va\u00b7ter", "hebt", "auf's", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Gebirge, Thal und Strand", "tokens": ["Und", "Ge\u00b7bir\u00b7ge", ",", "Thal", "und", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Taucht allm\u00e4lig auf in's Freie;", "tokens": ["Taucht", "all\u00b7m\u00e4\u00b7lig", "auf", "in's", "Frei\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es gr\u00fcnt das junge Land,", "tokens": ["Und", "es", "gr\u00fcnt", "das", "jun\u00b7ge", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Irrend farb'ge Lichter schweifen", "tokens": ["Ir\u00b7rend", "fa\u00b7rb'\u00b7ge", "Lich\u00b7ter", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und von Blumen gl\u00e4nzt die Flur,", "tokens": ["Und", "von", "Blu\u00b7men", "gl\u00e4nzt", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wo des Vaters Blick' sie streifen \u2014", "tokens": ["Wo", "des", "Va\u00b7ters", "Blick'", "sie", "strei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da zerrei\u00dft die Angelschnur.", "tokens": ["Da", "zer\u00b7rei\u00dft", "die", "An\u00b7gel\u00b7schnur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}