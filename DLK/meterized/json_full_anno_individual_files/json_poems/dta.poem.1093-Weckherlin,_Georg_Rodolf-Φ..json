{"dta.poem.1093": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "\u03a6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "All mein gl\u00fcck war/ das ich sah/", "tokens": ["All", "mein", "gl\u00fcck", "war", "/", "das", "ich", "sah", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie dir nichts bessers geschah/", "tokens": ["Wie", "dir", "nichts", "bes\u00b7sers", "ge\u00b7schah", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADV", "VVFIN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dan dich hertzlich vmb zufangen;", "tokens": ["Dan", "dich", "hertz\u00b7lich", "vmb", "zu\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "APPR", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd das kein liebers hal\u00dfband", "tokens": ["Vnd", "das", "kein", "lie\u00b7bers", "hal\u00df\u00b7band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dan meine arm mit wolstand", "tokens": ["Dan", "mei\u00b7ne", "arm", "mit", "wol\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "APPR", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vmb deinen hal\u00df konte hangen.", "tokens": ["Vmb", "dei\u00b7nen", "hal\u00df", "kon\u00b7te", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}