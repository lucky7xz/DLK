{"dta.poem.7254": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Prediger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Langsam und schwer vom Thurme stieg die Klage,", "tokens": ["Lang\u00b7sam", "und", "schwer", "vom", "Thur\u00b7me", "stieg", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein dumpf Gewimmer zwischen jedem Schlage,", "tokens": ["Ein", "dumpf", "Ge\u00b7wim\u00b7mer", "zwi\u00b7schen", "je\u00b7dem", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Memnons S\u00e4ule weint im Morgenflor.", "tokens": ["Wie", "Mem\u00b7nons", "S\u00e4u\u00b7le", "weint", "im", "Mor\u00b7gen\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Glockenstuhle zitterte der Balke,", "tokens": ["Am", "Glo\u00b7cken\u00b7stuh\u00b7le", "zit\u00b7ter\u00b7te", "der", "Bal\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Dohlen flatterten vom Nest, ein Falke", "tokens": ["Die", "Doh\u00b7len", "flat\u00b7ter\u00b7ten", "vom", "Nest", ",", "ein", "Fal\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Stieg pfeifend an der Fahne Schaft empor.", "tokens": ["Stieg", "pfei\u00b7fend", "an", "der", "Fah\u00b7ne", "Schaft", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem dr\u00f6hnt die Glocke? \u2014 Einem der entkettet,", "tokens": ["Wem", "dr\u00f6hnt", "die", "Glo\u00b7cke", "?", "Ei\u00b7nem", "der", "ent\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "$(", "PIS", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De\u00df m\u00fcden Leib ein Fackelzug gebettet", "tokens": ["De\u00df", "m\u00fc\u00b7den", "Leib", "ein", "Fa\u00b7ckel\u00b7zug", "ge\u00b7bet\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In letzter Nacht bei seinem einzgen Kind.", "tokens": ["In", "letz\u00b7ter", "Nacht", "bei", "sei\u00b7nem", "einz\u00b7gen", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer war der Mann? \u2014 Ein Geist im \u00e4chten Gleise,", "tokens": ["Wer", "war", "der", "Mann", "?", "Ein", "Geist", "im", "\u00e4ch\u00b7ten", "Glei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "$(", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kein Wucherer, kein Ehrendieb, und weise", "tokens": ["Kein", "Wu\u00b7che\u00b7rer", ",", "kein", "Eh\u00b7ren\u00b7dieb", ",", "und", "wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie reiche Leute selten weise sind.", "tokens": ["Wie", "rei\u00b7che", "Leu\u00b7te", "sel\u00b7ten", "wei\u00b7se", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Darum so mancher Greis mit Stock und Brille,", "tokens": ["Da\u00b7rum", "so", "man\u00b7cher", "Greis", "mit", "Stock", "und", "Bril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So manches Regentuch und Handpostille,", "tokens": ["So", "man\u00b7ches", "Re\u00b7gen\u00b7tuch", "und", "Hand\u00b7pos\u00b7til\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich m\u00fchsam schiebend durch der Menge Drang.", "tokens": ["Sich", "m\u00fch\u00b7sam", "schie\u00b7bend", "durch", "der", "Men\u00b7ge", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er war ein heitrer Wirth in seinem Schlosse, \u2014", "tokens": ["Er", "war", "ein", "hei\u00b7trer", "Wirth", "in", "sei\u00b7nem", "Schlos\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Darum am Thor so manche Staatskarosse,", "tokens": ["Da\u00b7rum", "am", "Thor", "so", "man\u00b7che", "Staats\u00b7ka\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So mancher Flor das Kirchenschiff entlang.", "tokens": ["So", "man\u00b7cher", "Flor", "das", "Kir\u00b7chen\u00b7schiff", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Glocken schwiegen, alle Kniee sanken,", "tokens": ["Die", "Glo\u00b7cken", "schwie\u00b7gen", ",", "al\u00b7le", "Kni\u00b7ee", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Posaunensto\u00df! \u2014 Die W\u00f6lbung schien zu wanken.", "tokens": ["Po\u00b7sau\u00b7nen\u00b7sto\u00df", "!", "Die", "W\u00f6l\u00b7bung", "schien", "zu", "wan\u00b7ken", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Auf S\u00fcnderschwielen, Thau in B\u00fc\u00dfermalen!", "tokens": ["Auf", "S\u00fcn\u00b7der\u00b7schwie\u00b7len", ",", "Thau", "in", "B\u00fc\u00b7\u00dfer\u00b7ma\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mir war als s\u00e4h ich des Gerichtes Schalen,", "tokens": ["Mir", "war", "als", "s\u00e4h", "ich", "des", "Ge\u00b7rich\u00b7tes", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als h\u00f6rt ich tr\u00f6pfeln meines Heilands Blut.", "tokens": ["Als", "h\u00f6rt", "ich", "tr\u00f6p\u00b7feln", "mei\u00b7nes", "Hei\u00b7lands", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das Amen war verhallt. Ein zitternd Schweigen", "tokens": ["Das", "A\u00b7men", "war", "ver\u00b7hallt", ".", "Ein", "zit\u00b7ternd", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lag auf der Menge, nur des Odems Steigen", "tokens": ["Lag", "auf", "der", "Men\u00b7ge", ",", "nur", "des", "O\u00b7dems", "Stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durchs\u00e4uselte den weiten Hallenbau.", "tokens": ["Durch\u00b7s\u00e4u\u00b7sel\u00b7te", "den", "wei\u00b7ten", "Hal\u00b7len\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur an der Tumba schwarzer Fl\u00e4mmchen Knistern", "tokens": ["Nur", "an", "der", "Tum\u00b7ba", "schwar\u00b7zer", "Fl\u00e4mm\u00b7chen", "Knis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schien leise mit dem Grabe noch zu fl\u00fcstern,", "tokens": ["Schien", "lei\u00b7se", "mit", "dem", "Gra\u00b7be", "noch", "zu", "fl\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Weihrauchwirbel streute Aschengrau.", "tokens": ["Der", "Weih\u00b7rauch\u00b7wir\u00b7bel", "streu\u00b7te", "A\u00b7schen\u00b7grau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u201egeliebte!\u201c scholl es von der W\u00f6lbung nieder,", "tokens": ["\u201e", "ge\u00b7lieb\u00b7te", "!", "\u201c", "scholl", "es", "von", "der", "W\u00f6l\u00b7bung", "nie\u00b7der", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wolke sank, und m\u00e4hlich stiegen Glieder,", "tokens": ["Die", "Wol\u00b7ke", "sank", ",", "und", "m\u00e4h\u00b7lich", "stie\u00b7gen", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Kanzelbord ein junger Priester stand.", "tokens": ["Am", "Kan\u00b7zel\u00b7bord", "ein", "jun\u00b7ger", "Pries\u00b7ter", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Schattenbild dem alle Lust verronnen,", "tokens": ["Kein", "Schat\u00b7ten\u00b7bild", "dem", "al\u00b7le", "Lust", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein frischer saftger Stamm am Lebensbronnen,", "tokens": ["Ein", "fri\u00b7scher", "saft\u00b7ger", "Stamm", "am", "Le\u00b7bens\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Adler ruhend auf Jehovah's Hand!", "tokens": ["Ein", "Ad\u00b7ler", "ru\u00b7hend", "auf", "Je\u00b7ho\u00b7vah's", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u201egeliebte\u201c, sprach er, \u201eselig sind die Todten", "tokens": ["\u201e", "ge\u00b7lieb\u00b7te", "\u201c", ",", "sprach", "er", ",", "\u201e", "se\u00b7lig", "sind", "die", "Tod\u00b7ten"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So in dem Herrn entschliefen, treue Boten,", "tokens": ["So", "in", "dem", "Herrn", "ent\u00b7schlie\u00b7fen", ",", "treu\u00b7e", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von ihrer Sendung rastend.\u201c Dann entstieg", "tokens": ["Von", "ih\u00b7rer", "Sen\u00b7dung", "ras\u00b7tend", ".", "\u201c", "Dann", "ent\u00b7stieg"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "$(", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Wort, gewaltig wie des Jordans Wallen,", "tokens": ["Das", "Wort", ",", "ge\u00b7wal\u00b7tig", "wie", "des", "Jor\u00b7dans", "Wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mild wie die Luft in Horebs Cederhallen,", "tokens": ["Mild", "wie", "die", "Luft", "in", "Ho\u00b7rebs", "Ce\u00b7der\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.6": {"text": "Als er bezeugte des Gerechten Sieg.", "tokens": ["Als", "er", "be\u00b7zeug\u00b7te", "des", "Ge\u00b7rech\u00b7ten", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}