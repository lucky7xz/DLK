{"dta.poem.18647": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E r ist ungl\u00fccklich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrmen denn die Ungl\u00fccks-Wetter", "tokens": ["St\u00fcr\u00b7men", "denn", "die", "Un\u00b7gl\u00fccks\u00b7Wet\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uffig auff mich Armen zu?", "tokens": ["H\u00e4uf\u00b7fig", "auff", "mich", "Ar\u00b7men", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmel werde mein Erretter/", "tokens": ["Him\u00b7mel", "wer\u00b7de", "mein", "Er\u00b7ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fctze meiner Seelen Ruh!", "tokens": ["Sch\u00fct\u00b7ze", "mei\u00b7ner", "See\u00b7len", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch nein/ der Himmel hat mir Hertz und Ohr verschlossen/", "tokens": ["Doch", "nein", "/", "der", "Him\u00b7mel", "hat", "mir", "Hertz", "und", "Ohr", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "ART", "NN", "VAFIN", "PPER", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo bin ich endlich hin zu meiner Qvaal verstossen?", "tokens": ["Wo", "bin", "ich", "end\u00b7lich", "hin", "zu", "mei\u00b7ner", "Qvaal", "ver\u00b7stos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Meine Sonn' ist untergangen", "tokens": ["Mei\u00b7ne", "Sonn'", "ist", "un\u00b7ter\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die d\u00fcstre Schmertzens-See.", "tokens": ["In", "die", "d\u00fcst\u00b7re", "Schmert\u00b7zens\u00b7See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten die den Geist umfangen/", "tokens": ["Schat\u00b7ten", "die", "den", "Geist", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind nur Jammer/ Ach und Weh.", "tokens": ["Sind", "nur", "Jam\u00b7mer", "/", "Ach", "und", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Augen weinet Blut/ weil keine Thr\u00e4nen reichen/", "tokens": ["Ihr", "Au\u00b7gen", "wei\u00b7net", "Blut", "/", "weil", "kei\u00b7ne", "Thr\u00e4\u00b7nen", "rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$(", "KOUS", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Demant-festen Sinn des Schicksals zu erweichen.", "tokens": ["Den", "De\u00b7mant\u00b7fes\u00b7ten", "Sinn", "des", "Schick\u00b7sals", "zu", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Lisimene/ werthste Seele!", "tokens": ["Li\u00b7si\u00b7me\u00b7ne", "/", "werths\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo sind deine Strahlen hin?", "tokens": ["Wo", "sind", "dei\u00b7ne", "Strah\u00b7len", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich lieg' in der Marter-H\u00f6le", "tokens": ["Ich", "lieg'", "in", "der", "Mar\u00b7ter\u00b7H\u00f6le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil ich nun entfernet bin.", "tokens": ["Weil", "ich", "nun", "ent\u00b7fer\u00b7net", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach da\u00df mein Auge doch die Sonne nie erblicket/", "tokens": ["Ach", "da\u00df", "mein", "Au\u00b7ge", "doch", "die", "Son\u00b7ne", "nie", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mir nun Finsterni\u00df/ und andern Klarheit schicket.", "tokens": ["Die", "mir", "nun", "Fins\u00b7ter\u00b7ni\u00df", "/", "und", "an\u00b7dern", "Klar\u00b7heit", "schi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn ich doch nur nicht gebunden", "tokens": ["Wenn", "ich", "doch", "nur", "nicht", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nste solte von dir gehn/", "tokens": ["Sch\u00f6ns\u00b7te", "sol\u00b7te", "von", "dir", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber so mu\u00df meinen Wunden", "tokens": ["A\u00b7ber", "so", "mu\u00df", "mei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Linderung und H\u00fclff' entstehn.", "tokens": ["Lin\u00b7de\u00b7rung", "und", "H\u00fclff'", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach Amors G\u00fcte hat mich nur so hoch gestellet/", "tokens": ["Ach", "A\u00b7mors", "G\u00fc\u00b7te", "hat", "mich", "nur", "so", "hoch", "ge\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df mich sein harter Grimm nun desto tieffer f\u00e4llet.", "tokens": ["Da\u00df", "mich", "sein", "har\u00b7ter", "Grimm", "nun", "des\u00b7to", "tief\u00b7fer", "f\u00e4l\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NE", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Lust und Freude mu\u00df ersterben/", "tokens": ["Lust", "und", "Freu\u00b7de", "mu\u00df", "ers\u00b7ter\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Mein Vergn\u00fcgen k\u00fcst das Grab.", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "k\u00fcst", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kont' ich vormahls Rosen erben/", "tokens": ["Kont'", "ich", "vor\u00b7mahls", "Ro\u00b7sen", "er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so brech ich Dornen ab.", "tokens": ["Nun", "so", "brech", "ich", "Dor\u00b7nen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch darff dein sch\u00f6ner Arm mich nun nicht mehr vergn\u00fcgen/", "tokens": ["Doch", "darff", "dein", "sch\u00f6\u00b7ner", "Arm", "mich", "nun", "nicht", "mehr", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So la\u00df mich nur zum Trost in deinen Hertzen liegen.", "tokens": ["So", "la\u00df", "mich", "nur", "zum", "Trost", "in", "dei\u00b7nen", "Hert\u00b7zen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}