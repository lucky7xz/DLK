{"textgrid.poem.24066": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tonnen stehen im dunklen Keller,", "tokens": ["Ton\u00b7nen", "ste\u00b7hen", "im", "dunk\u00b7len", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Breite, braune, bauchige Tonnen,", "tokens": ["Brei\u00b7te", ",", "brau\u00b7ne", ",", "bau\u00b7chi\u00b7ge", "Ton\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und zwischen ihnen taumelt meine Jagd", "tokens": ["Und", "zwi\u00b7schen", "ih\u00b7nen", "tau\u00b7melt", "mei\u00b7ne", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach einem Sonnenstrahl,", "tokens": ["Nach", "ei\u00b7nem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den ich im vorigen Sommer sah.", "tokens": ["Den", "ich", "im", "vo\u00b7ri\u00b7gen", "Som\u00b7mer", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sicher, in einer dieser Tonnen steckt er:", "tokens": ["Si\u00b7cher", ",", "in", "ei\u00b7ner", "die\u00b7ser", "T