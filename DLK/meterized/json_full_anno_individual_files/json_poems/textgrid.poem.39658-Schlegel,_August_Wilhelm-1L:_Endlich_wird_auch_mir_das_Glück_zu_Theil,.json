{"textgrid.poem.39658": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich wird auch mir das Gl\u00fcck zu Theil,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich wird auch mir das Gl\u00fcck zu Theil,", "tokens": ["End\u00b7lich", "wird", "auch", "mir", "das", "Gl\u00fcck", "zu", "Theil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Holde Dichterin, dich zu befragen,", "tokens": ["Hol\u00b7de", "Dich\u00b7te\u00b7rin", ",", "dich", "zu", "be\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach, mein Herz hat dir so viel zu sagen.", "tokens": ["Ach", ",", "mein", "Herz", "hat", "dir", "so", "viel", "zu", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es ist nicht f\u00fcr die Langeweil.", "tokens": ["Und", "es", "ist", "nicht", "f\u00fcr", "die", "Lan\u00b7ge\u00b7weil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aengstlich sah ich deine gro\u00dfe Eil,", "tokens": ["A\u00b7engst\u00b7lich", "sah", "ich", "dei\u00b7ne", "gro\u00b7\u00dfe", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich nach Hause von uns wegzutragen:", "tokens": ["Dich", "nach", "Hau\u00b7se", "von", "uns", "weg\u00b7zu\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich verzehrte mich in stillen Klagen", "tokens": ["Ich", "ver\u00b7zehr\u00b7te", "mich", "in", "stil\u00b7len", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und verzagte fast an meinem Heil.", "tokens": ["Und", "ver\u00b7zag\u00b7te", "fast", "an", "mei\u00b7nem", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "H\u00f6re denn! doch thu zuvor den Schwur", "tokens": ["H\u00f6\u00b7re", "denn", "!", "doch", "thu", "zu\u00b7vor", "den", "Schwur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "$.", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bei den Grazien und Musen zw\u00f6lfen,", "tokens": ["Bei", "den", "Gra\u00b7zi\u00b7en", "und", "Mu\u00b7sen", "zw\u00f6l\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bei der Tugend, Sch\u00f6nheit und Natur:", "tokens": ["Bei", "der", "Tu\u00b7gend", ",", "Sc