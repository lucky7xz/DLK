{"dta.poem.10156": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4dliche Unachtsamkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat etwan ein geschickter K\u00fcnstler ein sch\u00f6nes Bild durch", "tokens": ["Hat", "et\u00b7wan", "ein", "ge\u00b7schick\u00b7ter", "K\u00fcnst\u00b7ler", "ein", "sch\u00f6\u00b7nes", "Bild", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kunst gemacht;", "tokens": ["Kunst", "ge\u00b7macht", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Beschaut man solches ja, bewunderts, und, im betrachten,", "tokens": ["Be\u00b7schaut", "man", "sol\u00b7ches", "ja", ",", "be\u00b7wun\u00b7derts", ",", "und", ",", "im", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "PIS", "ADV", "$,", "ADV", "$,", "KON", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "lobt man ihn.", "tokens": ["lobt", "man", "ihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mit den so herrlichen Gesch\u00f6pfen, die selber GOTT her-", "tokens": ["Mit", "den", "so", "herr\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6p\u00b7fen", ",", "die", "sel\u00b7ber", "GoTT", "her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "NE", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "vor gebracht,", "tokens": ["vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ob sie gleich tausend mahl so sch\u00f6n, will niemand sich so", "tokens": ["Ob", "sie", "gleich", "tau\u00b7send", "mahl", "so", "sch\u00f6n", ",", "will", "nie\u00b7mand", "sich", "so"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "CARD", "ADV", "ADV", "ADJD", "$,", "VMFIN", "PIS", "PRF", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "viel bem\u00fchn.", "tokens": ["viel", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Mich deucht, ich find\u2019 in der Verachtung von GOttes Wer-", "tokens": ["Mich", "deucht", ",", "ich", "find'", "in", "der", "Ver\u00b7ach\u00b7tung", "von", "Got\u00b7tes", "Wer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "cken, eine Spur", "tokens": ["cken", ",", "ei\u00b7ne", "Spur"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVINF", "$,", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der durch den Stoltz in unserm Fall verdorbnen menschli-", "tokens": ["Der", "durch", "den", "Stoltz", "in", "un\u00b7serm", "Fall", "ver\u00b7dorb\u00b7nen", "menschli"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "chen Natur.", "tokens": ["chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.13": {"text": "Wir raubten, st\u00fcnd es nur bey uns, der ewgen Gottheit", "tokens": ["Wir", "raub\u00b7ten", ",", "st\u00fcnd", "es", "nur", "bey", "uns", ",", "der", "ew\u00b7gen", "Got\u00b7theit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "gern die Cron',", "tokens": ["gern", "die", "Cron'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.15": {"text": "Und setzten, voller Stoltz und Frevels, uns selber gern auf", "tokens": ["Und", "setz\u00b7ten", ",", "vol\u00b7ler", "Stoltz", "und", "Fre\u00b7vels", ",", "uns", "sel\u00b7ber", "gern", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "KON", "NN", "$,", "PPER", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Seinen Thron.", "tokens": ["Sei\u00b7nen", "Thron", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Es zeiget die\u00df verfahren klar, wie ungern wir der Seelen", "tokens": ["Es", "zei\u00b7get", "die\u00df", "ver\u00b7fah\u00b7ren", "klar", ",", "wie", "un\u00b7gern", "wir", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "VVINF", "ADJD", "$,", "PWAV", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "Krafft,", "tokens": ["Krafft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.19": {"text": "Dem Sch\u00f6pfer aller Welt zu Ehren, gebrauchen, und wie", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Welt", "zu", "Eh\u00b7ren", ",", "ge\u00b7brau\u00b7chen", ",", "und", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "APPR", "NN", "$,", "VVINF", "$,", "KON", "PWAV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "unschmackhaft", "tokens": ["un\u00b7schmack\u00b7haft"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+--", "measure": "dactylic.init"}, "line.21": {"text": "Uns Seine Wunder-s\u00fcsse Gaben, womit Er uns beschen-", "tokens": ["Uns", "Sei\u00b7ne", "Wun\u00b7der\u00b7s\u00fcs\u00b7se", "Ga\u00b7ben", ",", "wo\u00b7mit", "Er", "uns", "be\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PWAV", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "cket, sind.", "tokens": ["cket", ",", "sind", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Er macht uns Freude, wir uns f\u00fchllos; Er macht uns se-", "tokens": ["Er", "macht", "uns", "Freu\u00b7de", ",", "wir", "uns", "f\u00fchl\u00b7los", ";", "Er", "macht", "uns", "se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "PRF", "ADJD", "$.", "PPER", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "hend, wir uns blind.", "tokens": ["hend", ",", "wir", "uns", "blind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Dadurch nun, da\u00df die, blo\u00df von Stoltz und Eitelkeit er-", "tokens": ["Da\u00b7durch", "nun", ",", "da\u00df", "die", ",", "blo\u00df", "von", "Stoltz", "und", "Ei\u00b7tel\u00b7keit", "er"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "f\u00fcllte, Brust,", "tokens": ["f\u00fcll\u00b7te", ",", "Brust", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Durch aller ihrer Sinnen Th\u00fcren, von GOTTES Allmacht", "tokens": ["Durch", "al\u00b7ler", "ih\u00b7rer", "Sin\u00b7nen", "Th\u00fc\u00b7ren", ",", "von", "GoT\u00b7TES", "All\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "NN", "$,", "APPR", "NE", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "nichts empfindet;", "tokens": ["nichts", "emp\u00b7fin\u00b7det", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Raubt man so gar, so viel an uns, des Sch\u00f6pfers Lust, der", "tokens": ["Raubt", "man", "so", "gar", ",", "so", "viel", "an", "uns", ",", "des", "Sch\u00f6p\u00b7fers", "Lust", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "PPER", "$,", "ART", "NN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Seine Lust,", "tokens": ["Sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "O Liebe! bey den durch Sein Werck ger\u00fchrten Menschen", "tokens": ["O", "Lie\u00b7be", "!", "bey", "den", "durch", "Sein", "Werck", "ge\u00b7r\u00fchr\u00b7ten", "Men\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Kindern, findet.", "tokens": ["Kin\u00b7dern", ",", "fin\u00b7det", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Ist denn hiedurch, da wir iedoch zu diesem edlen Zweck er-", "tokens": ["Ist", "denn", "hie\u00b7durch", ",", "da", "wir", "ie\u00b7doch", "zu", "die\u00b7sem", "ed\u00b7len", "Zweck", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PAV", "$,", "KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.34": {"text": "kohren,", "tokens": ["koh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Des Sch\u00f6pfers Weisheit, Lieb\u2019 und Macht f\u00fcr uns nicht", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Lieb'", "und", "Macht", "f\u00fcr", "uns", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "APPR", "PPER", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "leider gantz verlohren?", "tokens": ["lei\u00b7der", "gantz", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}