{"dta.poem.20800": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   IX .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00e4nder auf und ab zu Tod und Leben", "tokens": ["Die", "L\u00e4n\u00b7der", "auf", "und", "ab", "zu", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gesellt das Bild sich des geliebten Mannes,", "tokens": ["Ge\u00b7sellt", "das", "Bild", "sich", "des", "ge\u00b7lieb\u00b7ten", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der durch sein Lieben H\u00f6chstes offenbarte,", "tokens": ["Der", "durch", "sein", "Lie\u00b7ben", "H\u00f6chs\u00b7tes", "of\u00b7fen\u00b7bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Retter uns, ein Tilger jedes Bannes.", "tokens": ["Ein", "Ret\u00b7ter", "uns", ",", "ein", "Til\u00b7ger", "je\u00b7des", "Ban\u00b7nes", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es ragt das Kreuz wo Menschentritte schweben", "tokens": ["Es", "ragt", "das", "Kreuz", "wo", "Men\u00b7schen\u00b7trit\u00b7te", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PWAV", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bedeutungsvoll als ewige Standarte.", "tokens": ["Be\u00b7deu\u00b7tungs\u00b7voll", "als", "e\u00b7wi\u00b7ge", "Stand\u00b7ar\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach, wo sich ja die zarte", "tokens": ["Ach", ",", "wo", "sich", "ja", "die", "zar\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PRF", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Lichtbl\u00fcte h\u00f6hern Lebens mag entfalten,", "tokens": ["Licht\u00b7bl\u00fc\u00b7te", "h\u00f6\u00b7hern", "Le\u00b7bens", "mag", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.9": {"text": "Allstets mu\u00df ringen sie mit wilden Wettern", "tokens": ["Alls\u00b7tets", "mu\u00df", "rin\u00b7gen", "sie", "mit", "wil\u00b7den", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und traurig sich entbl\u00e4ttern,", "tokens": ["Und", "trau\u00b7rig", "sich", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Soll gold'ne Frucht die Folgezeit erhalten.", "tokens": ["Soll", "gold'\u00b7ne", "Frucht", "die", "Fol\u00b7ge\u00b7zeit", "er\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nur sterbend wirst du jedes Ziel erreichen,", "tokens": ["Nur", "ster\u00b7bend", "wirst", "du", "je\u00b7des", "Ziel", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Drum ist das Kreuz der Weltgeschichte Zeichen.", "tokens": ["Drum", "ist", "das", "Kreuz", "der", "Welt\u00b7ge\u00b7schich\u00b7te", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Frei ist der Geist, doch ist bestimmt sein Wirken,", "tokens": ["Frei", "ist", "der", "Geist", ",", "doch", "ist", "be\u00b7stimmt", "sein", "Wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Ist \u2014 also will er's \u2014 strengem Ma\u00df verfallen\u201a", "tokens": ["Ist", "al\u00b7so", "will", "er's", "stren\u00b7gem", "Ma\u00df", "ver\u00b7fal\u00b7len", "\u201a"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "ADV", "VMFIN", "PIS", "$(", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und weil er Liebe sein will, himmelsglutig", "tokens": ["Und", "weil", "er", "Lie\u00b7be", "sein", "will", ",", "him\u00b7mels\u00b7glu\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VAINF", "VMFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erw\u00e4hlend demutvolles Erdenwallen,", "tokens": ["Er\u00b7w\u00e4h\u00b7lend", "de\u00b7mut\u00b7vol\u00b7les", "Er\u00b7den\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Mu\u00df er in ihm entfremdeten Bezirken", "tokens": ["Mu\u00df", "er", "in", "ihm", "ent\u00b7frem\u00b7de\u00b7ten", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gesammte Schuld der Erde s\u00fchnen blutig.", "tokens": ["Ge\u00b7samm\u00b7te", "Schuld", "der", "Er\u00b7de", "s\u00fch\u00b7nen", "blu\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So nahmen auf sich mutig", "tokens": ["So", "nah\u00b7men", "auf", "sich", "mu\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Kreuz der Menschheit Helden und Befreier,", "tokens": ["Ihr", "Kreuz", "der", "Menschheit", "Hel\u00b7den", "und", "Be\u00b7frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die mit dem Griffel, die mit frommen Thaten,", "tokens": ["Die", "mit", "dem", "Grif\u00b7fel", ",", "die", "mit", "from\u00b7men", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die mit dem Pflug und Spaten,", "tokens": ["Die", "mit", "dem", "Pflug", "und", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die mit des Schwertes Wucht, die mit der Leier,", "tokens": ["Die", "mit", "des", "Schwer\u00b7tes", "Wucht", ",", "die", "mit", "der", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Denn alle sind des Mittlers, wie sie kamen,", "tokens": ["Denn", "al\u00b7le", "sind", "des", "Mitt\u00b7lers", ",", "wie", "sie", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die, ihn verkl\u00e4rend, von dem Seinen nahmen.", "tokens": ["Die", ",", "ihn", "ver\u00b7kl\u00e4\u00b7rend", ",", "von", "dem", "Sei\u00b7nen", "nah\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PPER", "VVPP", "$,", "APPR", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und sein sind, durch sein Lieben, alle Schmerzen", "tokens": ["Und", "sein", "sind", ",", "durch", "sein", "Lie\u00b7ben", ",", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAINF", "VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Welt, von Blute Abels des Gerechten", "tokens": ["Der", "Welt", ",", "von", "Blu\u00b7te", "A\u00b7bels", "des", "Ge\u00b7rech\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis zu der Weltgeschichte letzten Plagen\u201a", "tokens": ["Bis", "zu", "der", "Welt\u00b7ge\u00b7schich\u00b7te", "letz\u00b7ten", "Pla\u00b7gen", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df eine Dornenkrone mochte flechten", "tokens": ["Da\u00df", "ei\u00b7ne", "Dor\u00b7nen\u00b7kro\u00b7ne", "moch\u00b7te", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Heilige daraus in seinem Herzen,", "tokens": ["Der", "Hei\u00b7li\u00b7ge", "da\u00b7raus", "in", "sei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weit blutiger als die sein Haubt getragen.", "tokens": ["Weit", "blu\u00b7ti\u00b7ger", "als", "die", "sein", "Haubt", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er klagte unsre Klagen", "tokens": ["Er", "klag\u00b7te", "uns\u00b7re", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und weinte unsre Thr\u00e4nen eh' wir waren\u201a", "tokens": ["Und", "wein\u00b7te", "uns\u00b7re", "Thr\u00e4\u00b7nen", "eh'", "wir", "wa\u00b7ren", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOUS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Damit hinfort wir ", "tokens": ["Da\u00b7mit", "hin\u00b7fort", "wir"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Erhebend so zur reinen", "tokens": ["Er\u00b7he\u00b7bend", "so", "zur", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Natur des Mitgef\u00fchls den rohen, baaren,", "tokens": ["Na\u00b7tur", "des", "Mit\u00b7ge\u00b7f\u00fchls", "den", "ro\u00b7hen", ",", "baa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Unfrommen Schmerz, wo seiner Liebe Walten", "tokens": ["Un\u00b7from\u00b7men", "Schmerz", ",", "wo", "sei\u00b7ner", "Lie\u00b7be", "Wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Alsbald uns tr\u00f6sten mag und neugestalten.", "tokens": ["Als\u00b7bald", "uns", "tr\u00f6s\u00b7ten", "mag", "und", "neu\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So geht ein Mann gebeugt von schwerem Kummer", "tokens": ["So", "geht", "ein", "Mann", "ge\u00b7beugt", "von", "schwe\u00b7rem", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob seiner Kinder frevlem Thun. Ihn peinigt,", "tokens": ["Ob", "sei\u00b7ner", "Kin\u00b7der", "frev\u00b7lem", "Thun", ".", "Ihn", "pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn, der da rein ist, foltert das Gewissen", "tokens": ["Ihn", ",", "der", "da", "rein", "ist", ",", "fol\u00b7tert", "das", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Schuldigen mehr als sie selbst. Es steinigt", "tokens": ["Der", "Schul\u00b7di\u00b7gen", "mehr", "als", "sie", "selbst", ".", "Es", "stei\u00b7nigt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "KOUS", "PPER", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihn auf der Stra\u00dfe und ihn flieht der Schlummer", "tokens": ["Ihn", "auf", "der", "Stra\u00b7\u00dfe", "und", "ihn", "flieht", "der", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Nacht. Krank, arm durch ihre Schuld, beflissen", "tokens": ["Der", "Nacht", ".", "Krank", ",", "arm", "durch", "ih\u00b7re", "Schuld", ",", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$.", "NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur ihren Finsternissen", "tokens": ["Nur", "ih\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Licht zu sein, ach! schleppt er noch sein Leben.", "tokens": ["Ein", "Licht", "zu", "sein", ",", "ach", "!", "schleppt", "er", "noch", "sein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VAINF", "$,", "ITJ", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Jetzt mit dem greisen Vater f\u00fchlt Erbarmen", "tokens": ["Jetzt", "mit", "dem", "grei\u00b7sen", "Va\u00b7ter", "f\u00fchlt", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sein j\u00fcngstes Kind. Umarmen", "tokens": ["Sein", "j\u00fcngs\u00b7tes", "Kind", ".", "Um\u00b7ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Darf er das weinend und zu sich erheben.", "tokens": ["Darf", "er", "das", "wei\u00b7nend", "und", "zu", "sich", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "VVPP", "KON", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da stralt sein Blick: \u201ela\u00dft mich von hinnen fliehen!", "tokens": ["Da", "stralt", "sein", "Blick", ":", "\u201e", "la\u00dft", "mich", "von", "hin\u00b7nen", "flie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$.", "$(", "VVIMP", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es wird mein Tod sie alle zu mir ziehen!\u201c", "tokens": ["Es", "wird", "mein", "Tod", "sie", "al\u00b7le", "zu", "mir", "zie\u00b7hen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "PIS", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O wunderbar Geheimni\u00df du der Liebe,", "tokens": ["O", "wun\u00b7der\u00b7bar", "Ge\u00b7heim\u00b7ni\u00df", "du", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dennoch allen k\u00fcndlich die da lieben,", "tokens": ["Und", "den\u00b7noch", "al\u00b7len", "k\u00fcnd\u00b7lich", "die", "da", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "ART", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie die Gemeinschaft, welche sie begr\u00fcndet,", "tokens": ["Wie", "die", "Ge\u00b7mein\u00b7schaft", ",", "wel\u00b7che", "sie", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So Schuld wie Unschuld theilt, des S\u00fcnders Trieben", "tokens": ["So", "Schuld", "wie", "Un\u00b7schuld", "theilt", ",", "des", "S\u00fcn\u00b7ders", "Trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "KOKOM", "NN", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Reinen Reinheit eignet, und im Siebe", "tokens": ["Des", "Rei\u00b7nen", "Rein\u00b7heit", "eig\u00b7net", ",", "und", "im", "Sie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Schuld den Edeln umwirft, da\u00df verb\u00fcndet", "tokens": ["Der", "Schuld", "den", "E\u00b7deln", "um\u00b7wirft", ",", "da\u00df", "ver\u00b7b\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sich Beider Herz entz\u00fcndet", "tokens": ["Sich", "Bei\u00b7der", "Herz", "ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu neuen doppelt s\u00fc\u00dfen Himmelsflammen!", "tokens": ["Zu", "neu\u00b7en", "dop\u00b7pelt", "s\u00fc\u00b7\u00dfen", "Him\u00b7mels\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Anbetungsw\u00fcrdiges Gesetz der Liebe", "tokens": ["An\u00b7be\u00b7tungs\u00b7w\u00fcr\u00b7di\u00b7ges", "Ge\u00b7setz", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Das alle Todeshiebe", "tokens": ["Das", "al\u00b7le", "To\u00b7des\u00b7hie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ausheilet und das Weltall h\u00e4lt zusammen!", "tokens": ["Aus\u00b7hei\u00b7let", "und", "das", "Wel\u00b7tall", "h\u00e4lt", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zwar frommer Wehmut magst du Ursach werden,", "tokens": ["Zwar", "from\u00b7mer", "Weh\u00b7mut", "magst", "du", "Ur\u00b7sach", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Doch die hat niemand noch gereut auf Erden.", "tokens": ["Doch", "die", "hat", "nie\u00b7mand", "noch", "ge\u00b7reut", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mit dem gekreuzigten Erl\u00f6ser b\u00fc\u00dfen,", "tokens": ["Mit", "dem", "ge\u00b7kreu\u00b7zig\u00b7ten", "Er\u00b7l\u00f6\u00b7ser", "b\u00fc\