], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weil um den Tod zu werben,", "tokens": ["Weil", "um", "den", "Tod", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In dem die Welt liegt, Mitleid ihn getrieben.", "tokens": ["In", "dem", "die", "Welt", "liegt", ",", "Mit\u00b7leid", "ihn", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So stirbt und lebt der Heiligen Gemeine", "tokens": ["So", "stirbt", "und", "lebt", "der", "Hei\u00b7li\u00b7gen", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In l\u00e4uterndem Vereine", "tokens": ["In", "l\u00e4u\u00b7tern\u00b7dem", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit dem der sie vermittelt durch sein Lieben.", "tokens": ["Mit", "dem", "der", "sie", "ver\u00b7mit\u00b7telt", "durch", "sein", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So ist ", "tokens": ["So", "ist"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Nicht kann es f\u00fcr der Deutung falsche Bahnen.", "tokens": ["Nicht", "kann", "es", "f\u00fcr", "der", "Deu\u00b7tung", "fal\u00b7sche", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Doch euch will noch was l\u00e4ngst der Geist geehret", "tokens": ["Doch", "euch", "will", "noch", "was", "l\u00e4ngst", "der", "Geist", "ge\u00b7eh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PWS", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Aergerni\u00df und eine Thorheit d\u00e4uchten.", "tokens": ["Ein", "A\u00b7er\u00b7ger\u00b7ni\u00df", "und", "ei\u00b7ne", "Thor\u00b7heit", "d\u00e4uch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Kreuze wollt ihr \u201eaus der Erde rei\u00dfen!\u201c", "tokens": ["Die", "Kreu\u00b7ze", "wollt", "ihr", "\u201e", "aus", "der", "Er\u00b7de", "rei\u00b7\u00dfen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "$(", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Euch blendet des Jahrhunderts Wetterleuchten", "tokens": ["Euch", "blen\u00b7det", "des", "Jahr\u00b7hun\u00b7derts", "Wet\u00b7ter\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.p