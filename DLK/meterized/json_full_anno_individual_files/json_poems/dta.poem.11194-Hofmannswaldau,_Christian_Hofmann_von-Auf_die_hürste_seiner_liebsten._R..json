{"dta.poem.11194": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die h\u00fcrste seiner liebsten.  \n  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das haar das trug zuvor ein schwein auf seinem r\u00fccken;", "tokens": ["Das", "haar", "das", "trug", "zu\u00b7vor", "ein", "schwein", "auf", "sei\u00b7nem", "r\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "VVFIN", "ADV", "ART", "ADJD", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jtzt gilt es mehr, es soll der liebsten haare schm\u00fccken.", "tokens": ["Jtzt", "gilt", "es", "mehr", ",", "es", "soll", "der", "liebs\u00b7ten", "haa\u00b7re", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}