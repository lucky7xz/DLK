punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die alten Formen st\u00fcrzen ein;", "tokens": ["Und", "die", "al\u00b7ten", "For\u00b7men", "st\u00fcr\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht das Weltmeer hemmt des Krieges Toben,", "tokens": ["Nicht", "das", "Welt\u00b7meer", "hemmt", "des", "Krie\u00b7ges", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht der Nilgott und der alte Rhein.", "tokens": ["Nicht", "der", "Nil\u00b7gott", "und", "der", "al\u00b7te", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "KON", "ART", "ADJA", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Zwo gewaltge Nationen ringen", "tokens": ["Zwo", "ge\u00b7walt\u00b7ge", "Na\u00b7ti\u00b7o\u00b7nen", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um der Welt alleinigen Besitz,", "tokens": ["Um", "der", "Welt", "al\u00b7lei\u00b7ni\u00b7gen", "Be\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aller L\u00e4nder Freiheit zu verschlingen,", "tokens": ["Al\u00b7ler", "L\u00e4n\u00b7der", "Frei\u00b7heit", "zu", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwingen sie den Dreizack und den Blitz.", "tokens": ["Schwin\u00b7gen", "sie", "den", "Drei\u00b7zack", "und", "den", "Blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Gold mu\u00df ihnen jede Landschaft w\u00e4gen,", "tokens": ["Gold", "mu\u00df", "ih\u00b7nen", "je\u00b7de", "Land\u00b7schaft", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und wie ", "tokens": ["Und", "wie"], "token_info": ["word", "word"], "pos": ["KON", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Legt der Franke seinen ehrnen Degen", "tokens": ["Legt", "der", "Fran\u00b7ke", "sei\u00b7nen", "ehr\u00b7nen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In die Waage der Gerechtigkeit.", "tokens": ["In", "die", "Waa\u00b7ge", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Seine Handelsflotten streckt der Brite", "tokens": ["Sei\u00b7ne", "Han\u00b7dels\u00b7flot\u00b7ten", "streckt", "der", "Bri\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gierig wie Polypenarme aus,", "tokens": ["Gie\u00b7rig", "wie", "Po\u00b7ly\u00b7pen\u00b7ar\u00b7me", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Reich der freien Amphitrite", "tokens": ["Und", "das", "Reich", "der", "frei\u00b7en", "Am\u00b7phit\u00b7ri\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Will er schlie\u00dfen wie sein eignes Haus.", "tokens": ["Will", "er", "schlie\u00b7\u00dfen", "wie", "sein", "eig\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Zu des S\u00fcdpols nie erblickten Sternen", "tokens": ["Zu", "des", "S\u00fcd\u00b7pols", "nie", "er\u00b7blick\u00b7ten", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dringt sein rastlos ungehemmter Lauf,", "tokens": ["Dringt", "sein", "rast\u00b7los", "un\u00b7ge\u00b7hemm\u00b7ter", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alle Inseln sp\u00fcrt er, alle fernen", "tokens": ["Al\u00b7le", "In\u00b7seln", "sp\u00fcrt", "er", ",", "al\u00b7le", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "PIAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00fcsten \u2013 nur das Paradies nicht auf.", "tokens": ["K\u00fcs\u00b7ten", "\u2013", "nur", "das", "Pa\u00b7ra\u00b7dies", "nicht", "auf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Ach umsonst auf allen L\u00e4nderkarten", "tokens": ["Ach", "um\u00b7sonst", "auf", "al\u00b7len", "L\u00e4n\u00b7der\u00b7kar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sp\u00e4hst du nach dem seligen Gebiet,", "tokens": ["Sp\u00e4hst", "du", "nach", "dem", "se\u00b7li\u00b7gen", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der Freiheit ewig gr\u00fcner Garten,", "tokens": ["Wo", "der", "Frei\u00b7heit", "e\u00b7wig", "gr\u00fc\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo der Menschheit sch\u00f6ne Jugend bl\u00fcht.", "tokens": ["Wo", "der", "Menschheit", "sch\u00f6\u00b7ne", "Ju\u00b7gend", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Endlos liegt die Welt vor deinen Blicken,", "tokens": ["End\u00b7los", "liegt", "die", "Welt", "vor", "dei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Schiffahrt selbst ermi\u00dft sie kaum,", "tokens": ["Und", "die", "Schif\u00b7fahrt", "selbst", "er\u00b7mi\u00dft", "sie", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch auf ihrem unerme\u00dfnen R\u00fccken", "tokens": ["Doch", "auf", "ih\u00b7rem", "un\u00b7er\u00b7me\u00df\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist f\u00fcr zehen Gl\u00fcckliche nicht Raum.", "tokens": ["Ist", "f\u00fcr", "ze\u00b7hen", "Gl\u00fcck\u00b7li\u00b7che", "nicht", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "CARD", "NN", "PTKNEG", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "In des Herzens heilig stille R\u00e4ume", "tokens": ["In", "des", "Her\u00b7zens", "hei\u00b7lig", "stil\u00b7le", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00dft du fliehen aus des Lebens Drang,", "tokens": ["Mu\u00dft", "du", "flie\u00b7hen", "aus", "des", "Le\u00b7bens", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Freiheit ist nur in dem Reich der Tr\u00e4ume,", "tokens": ["Frei\u00b7heit", "ist", "nur", "in", "dem", "Reich", "der", "Tr\u00e4u\u00b7me", ","], "token_info": ["