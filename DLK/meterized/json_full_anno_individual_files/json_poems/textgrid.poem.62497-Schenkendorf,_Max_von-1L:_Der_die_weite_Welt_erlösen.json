{"textgrid.poem.62497": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der die weite Welt erl\u00f6sen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der die weite Welt erl\u00f6sen", "tokens": ["Der", "die", "wei\u00b7te", "Welt", "er\u00b7l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der S\u00fcnde soll, vom B\u00f6sen,", "tokens": ["Von", "der", "S\u00fcn\u00b7de", "soll", ",", "vom", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von des ew'gen Todes Pfeil,", "tokens": ["Von", "des", "ew'\u00b7gen", "To\u00b7des", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Welchen Gott uns zugeschworen \u2013", "tokens": ["Wel\u00b7chen", "Gott", "uns", "zu\u00b7ge\u00b7schwo\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dreifach wird der Herr geboren,", "tokens": ["Drei\u00b7fach", "wird", "der", "Herr", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dreifach nahet unser Heil.", "tokens": ["Drei\u00b7fach", "na\u00b7het", "un\u00b7ser", "Heil", "."], "token_info": ["word", "word", "wo