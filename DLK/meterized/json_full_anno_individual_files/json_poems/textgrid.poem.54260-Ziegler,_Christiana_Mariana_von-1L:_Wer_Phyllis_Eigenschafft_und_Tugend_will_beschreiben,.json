{"textgrid.poem.54260": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer Phyllis Eigenschafft und Tugend will beschreiben,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Phyllis Eigenschafft und Tugend will beschreiben,", "tokens": ["Wer", "Phyl\u00b7lis", "Ei\u00b7gen\u00b7schafft", "und", "Tu\u00b7gend", "will", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wird besch\u00e4mt und auch vor Wunder stehen bleiben,", "tokens": ["Der", "wird", "be\u00b7sch\u00e4mt", "und", "auch", "vor", "Wun\u00b7der", "ste\u00b7hen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KON", "ADV", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Mahler und Poet trifft ihre Seltenheit,", "tokens": ["Kein", "Mah\u00b7ler", "und", "Po\u00b7et", "trifft", "ih\u00b7re", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der auch die Tadelsucht ein Lob-Lied billig weyht.", "tokens": ["Der", "auch", "die", "Ta\u00b7del\u00b7sucht", "ein", "Lob\u00b7Lied", "bil\u00b7lig", "weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie wei\u00df was Adel ist, da\u00df nicht die h\u00e4uffgen Ahnen", "tokens": ["Sie", "wei\u00df", "was", "A\u00b7del", "ist", ",", "da\u00df", "nicht", "die", "h\u00e4uff\u00b7gen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "NN", "VAFIN", "$,", "KOUS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur blo\u00df allein den Weg zu Ruhm und Ansehn bahnen,", "tokens": ["Nur", "blo\u00df", "al\u00b7lein", "den", "Weg", "zu", "Ruhm", "und", "An\u00b7sehn", "bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df Geld und grosses Gut vor unsrer Welt zwar Pracht,", "tokens": ["Da\u00df", "Geld", "und", "gros\u00b7ses", "Gut", "vor", "uns\u00b7rer", "Welt", "zwar", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch nicht, wie manche denckt, den rechten Adel macht.", "tokens": ["Doch", "nicht", ",", "wie", "man\u00b7che", "denckt", ",", "den", "rech\u00b7ten", "A\u00b7del", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihr edler Geist l\u00e4st uns ein recht vortrefflich Wesen,", "tokens": ["Ihr", "ed\u00b7ler", "Geist", "l\u00e4st", "uns", "ein", "recht", "vor\u00b7treff\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und himmlischen Verstand aus Aug und Minen lesen,", "tokens": ["Und", "himm\u00b7li\u00b7schen", "Ver\u00b7stand", "aus", "Aug", "und", "Mi\u00b7nen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und spricht ihr artger Mund, so pflichtet jedes bey,", "tokens": ["Und", "spricht", "ihr", "art\u00b7ger", "Mund", ",", "so", "pflich\u00b7tet", "je\u00b7des", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df ihres gleichen fast nicht leicht zu finden sey.", "tokens": ["Da\u00df", "ih\u00b7res", "glei\u00b7chen", "fast", "nicht", "leicht", "zu", "fin\u00b7den", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr holdes L\u00e4cheln kan uns Seel und Sinne r\u00fchren,", "tokens": ["Ihr", "hol\u00b7des", "L\u00e4\u00b7cheln", "kan", "uns", "Seel", "und", "Sin\u00b7ne", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Ha\u00df und Neid mu\u00df selbst aus ihren Reden sp\u00fcren,", "tokens": ["Der", "Ha\u00df", "und", "Neid", "mu\u00df", "selbst", "aus", "ih\u00b7ren", "Re\u00b7den", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df sich ein jederman nach ihren Umgang rei\u00dft,", "tokens": ["Da\u00df", "sich", "ein", "je\u00b7der\u00b7man", "nach", "ih\u00b7ren", "Um\u00b7gang", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der mehr als angenehm und unentbehrlich hei\u00dft.", "tokens": ["Der", "mehr", "als", "an\u00b7ge\u00b7nehm", "und", "un\u00b7ent\u00b7behr\u00b7lich", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ihr Damen! last sie euch statt einer Vorschrifft dienen,", "tokens": ["Ihr", "Da\u00b7men", "!", "last", "sie", "euch", "statt", "ei\u00b7ner", "Vor\u00b7schrifft", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bespiegelt euch an ihr, gebt Acht auf ihre Minen,", "tokens": ["Be\u00b7spie\u00b7gelt", "euch", "an", "ihr", ",", "gebt", "Acht", "auf", "ih\u00b7re", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "VVFIN", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.19": {"text": "Wie sittsam selbge seynd, was, wenn ihr Mund sich regt,", "tokens": ["Wie", "sitt\u00b7sam", "selb\u00b7ge", "seynd", ",", "was", ",", "wenn", "ihr", "Mund", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "VAFIN", "$,", "PRELS", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein jedes Wort f\u00fcr Krafft, Geist, Glut und Nachdruck hegt.", "tokens": ["Ein", "je\u00b7des", "Wort", "f\u00fcr", "Krafft", ",", "Geist", ",", "Glut", "und", "Nach\u00b7druck", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kein eitler Wahn kan sie, kein toller Hochmuth st\u00f6hren,", "tokens": ["Kein", "eit\u00b7ler", "Wahn", "kan", "sie", ",", "kein", "tol\u00b7ler", "Hoch\u00b7muth", "st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "$,", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sie wei\u00df ein jegliches, bescheiden anzuh\u00f6ren,", "tokens": ["Sie", "wei\u00df", "ein", "jeg\u00b7li\u00b7ches", ",", "be\u00b7schei\u00b7den", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wohl wissend, da\u00df ihr nichts von ihren Werth entf\u00e4llt,", "tokens": ["Wohl", "wis\u00b7send", ",", "da\u00df", "ihr", "nichts", "von", "ih\u00b7ren", "Werth", "ent\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "PPER", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So h\u00f6flich und geneigt sie sich auch immer stellt.", "tokens": ["So", "h\u00f6f\u00b7lich", "und", "ge\u00b7neigt", "sie", "sich", "auch", "im\u00b7mer", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ihr Auge l\u00e4st uns nichts von Neid und Mi\u00dfgunst blicken,", "tokens": ["Ihr", "Au\u00b7ge", "l\u00e4st", "uns", "nichts", "von", "Neid", "und", "Mi\u00df\u00b7gunst", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIS", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie pflegt nicht ihren Mund nach Momus Art zu r\u00fccken,", "tokens": ["Sie", "pflegt", "nicht", "ih\u00b7ren", "Mund", "nach", "Mo\u00b7mus", "Art", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es klebt kein L\u00e4ster-Wort auf ihrer Lippen-Pracht", "tokens": ["Es", "klebt", "kein", "L\u00e4s\u00b7ter\u00b7Wort", "auf", "ih\u00b7rer", "Lip\u00b7pen\u00b7Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das doch die heutge Welt zur gr\u00f6sten Mode macht.", "tokens": ["Das", "doch", "die", "heut\u00b7ge", "Welt", "zur", "gr\u00f6s\u00b7ten", "Mo\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die sp\u00e4te Welt wird noch vor sie viel Ehrfurcht hegen,", "tokens": ["Die", "sp\u00e4\u00b7te", "Welt", "wird", "noch", "vor", "sie", "viel", "Ehr\u00b7furcht", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+---", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Und sich di\u00df Tugend-Bild in Aug und Hertze pr\u00e4gen,", "tokens": ["Und", "sich", "di\u00df", "Tu\u00b7gen\u00b7dBild", "in", "Aug", "und", "Hert\u00b7ze", "pr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PDS", "NN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zumahl wenn man dereinst di\u00df auf ihr Grabmal schreibt:", "tokens": ["Zu\u00b7mahl", "wenn", "man", "de\u00b7reinst", "di\u00df", "auf", "ihr", "Grab\u00b7mal", "schreibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df Phyllis in der That des Adels Zierde bleibt.", "tokens": ["Da\u00df", "Phyl\u00b7lis", "in", "der", "That", "des", "A\u00b7dels", "Zier\u00b7de", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer Phyllis Eigenschafft und Tugend will beschreiben,", "tokens": ["Wer", "Phyl\u00b7lis", "Ei\u00b7gen\u00b7schafft", "und", "Tu\u00b7gend", "will", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wird besch\u00e4mt und auch vor Wunder stehen bleiben,", "tokens": ["Der", "wird", "be\u00b7sch\u00e4mt", "und", "auch", "vor", "Wun\u00b7der", "ste\u00b7hen", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "KON", "ADV", "APPR", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Mahler und Poet trifft ihre Seltenheit,", "tokens": ["Kein", "Mah\u00b7ler", "und", "Po\u00b7et", "trifft", "ih\u00b7re", "Sel\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "met