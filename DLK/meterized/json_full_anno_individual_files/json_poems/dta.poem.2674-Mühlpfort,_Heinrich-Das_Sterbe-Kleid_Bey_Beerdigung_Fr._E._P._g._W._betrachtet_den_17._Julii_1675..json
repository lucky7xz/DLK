{"dta.poem.2674": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Sterbe-Kleid/  \n  Bey Beerdigung Fr. E. P. g. W. betrachtet/  \n den 17. Julii 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst di\u00df der letzte Schmuck/ du wertheste der Frauen/", "tokens": ["Ist", "di\u00df", "der", "letz\u00b7te", "Schmuck", "/", "du", "wert\u00b7hes\u00b7te", "der", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der deinen kalten Leib nunmehr bekleiden soll?", "tokens": ["Der", "dei\u00b7nen", "kal\u00b7ten", "Leib", "nun\u00b7mehr", "be\u00b7klei\u00b7den", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich weiter ", "tokens": ["Ich", "wei\u00b7ter"], "token_info": ["word", "word"], "pos": ["PPER", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Gef\u00e4llt ein Leinen Tuch f\u00fcr andern dir so wol?", "tokens": ["Ge\u00b7f\u00e4llt", "ein", "Lei\u00b7nen", "Tuch", "f\u00fcr", "an\u00b7dern", "dir", "so", "wol", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und w\u00fcnscht du eintzig nur di\u00df Kleid bald anzulegen/", "tokens": ["Und", "w\u00fcnscht", "du", "eint\u00b7zig", "nur", "di\u00df", "Kleid", "bald", "an\u00b7zu\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "PDS", "NN", "ADV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hingegen Fleisch und Blut die H\u00fclle wegzuthun?", "tokens": ["Hin\u00b7ge\u00b7gen", "Fleisch", "und", "Blut", "die", "H\u00fcl\u00b7le", "weg\u00b7zu\u00b7thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "So sag ich diese Tracht bringt dir mehr Nutz zu wegen/", "tokens": ["So", "sag", "ich", "die\u00b7se", "Tracht", "bringt", "dir", "mehr", "Nutz", "zu", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKZU", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als wenn du sonst auf Sammt und Purpur w\u00fcrdest ruhn.", "tokens": ["Als", "wenn", "du", "sonst", "auf", "Sammt", "und", "Pur\u00b7pur", "w\u00fcr\u00b7dest", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Di\u00df ist nicht ungemein da\u00df bey verstrichnen Zeiten", "tokens": ["Di\u00df", "ist", "nicht", "un\u00b7ge\u00b7mein", "da\u00df", "bey", "ver\u00b7strich\u00b7nen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sich schon die alte Welt zum Sterben hat geschickt/", "tokens": ["Sich", "schon", "die", "al\u00b7te", "Welt", "zum", "Ster\u00b7ben", "hat", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df sie ihr Grufft und Sarg lie\u00df herrlich zubereiten/", "tokens": ["Da\u00df", "sie", "ihr", "Grufft", "und", "Sarg", "lie\u00df", "herr\u00b7lich", "zu\u00b7be\u00b7rei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ja hat gleich einer Braut zur Heimfarth sich geschm\u00fcckt.", "tokens": ["Ja", "hat", "gleich", "ei\u00b7ner", "Braut", "zur", "Heimf\u00b7arth", "sich", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die/ so gemeines Heil mit Sorgen unterst\u00fctzet/", "tokens": ["Die", "/", "so", "ge\u00b7mei\u00b7nes", "Heil", "mit", "Sor\u00b7gen", "un\u00b7ter\u00b7st\u00fct\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "ADJA", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verlangten in dem Rock der Ehren zu vergehn.", "tokens": ["Ver\u00b7lang\u00b7ten", "in", "dem", "Rock", "der", "Eh\u00b7ren", "zu", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die/ so ihr Helden-Blut f\u00fcrs Vaterland verspritzet/", "tokens": ["Die", "/", "so", "ihr", "Hel\u00b7den\u00b7Blut", "f\u00fcrs", "Va\u00b7ter\u00b7land", "ver\u00b7sprit\u00b7zet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Begehrten in der Pracht der Helden gleich zu stehn.", "tokens": ["Be\u00b7gehr\u00b7ten", "in", "der", "Pracht", "der", "Hel\u00b7den", "gleich", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und M\u00e4nner nicht allein hat dieser Wahn besessen/", "tokens": ["Und", "M\u00e4n\u00b7ner", "nicht", "al\u00b7lein", "hat", "die\u00b7ser", "Wahn", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "VAFIN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df Zier und Herrligkeit den Tod vers\u00fcssen kan/", "tokens": ["Da\u00df", "Zier", "und", "Herr\u00b7lig\u00b7keit", "den", "Tod", "ver\u00b7s\u00fcs\u00b7sen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nein! kluger Frauen-Witz hat gleiches Ziel ermessen/", "tokens": ["Nein", "!", "klu\u00b7ger", "Frau\u00b7en\u00b7Witz", "hat", "glei\u00b7ches", "Ziel", "er\u00b7mes\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und legt die Sterbenden mit sch\u00f6nen Kleidern an.", "tokens": ["Und", "legt", "die", "Ster\u00b7ben\u00b7den", "mit", "sch\u00f6\u00b7nen", "Klei\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So lieff Olympias den Feinden in die H\u00e4nde/", "tokens": ["So", "lieff", "O\u00b7lym\u00b7pias", "den", "Fein\u00b7den", "in", "die", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Alceste gieng geschm\u00fcckt au\u00df Lieb und Treu in Tod.", "tokens": ["Al\u00b7ces\u00b7te", "gieng", "ge\u00b7schm\u00fcckt", "au\u00df", "Lieb", "und", "Treu", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Man wei\u00df Cleopatrens ihr wurder-pr\u00e4chtig Ende/", "tokens": ["Man", "wei\u00df", "Cleo\u00b7pa\u00b7trens", "ihr", "wur\u00b7der\u00b7pr\u00e4ch\u00b7tig", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "PPER", "ADJD", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Wenn sie die Schlangen f\u00fchlt/ doch nicht der R\u00f6mer Noth.", "tokens": ["Wenn", "sie", "die", "Schlan\u00b7gen", "f\u00fchlt", "/", "doch", "nicht", "der", "R\u00f6\u00b7mer", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Allein ein falscher Traum von th\u00f6richten Gedancken", "tokens": ["Al\u00b7lein", "ein", "fal\u00b7scher", "Traum", "von", "th\u00f6\u00b7rich\u00b7ten", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Hat nur ein Sterbe-Kleid dem blinden Volck gewebt/", "tokens": ["Hat", "nur", "ein", "Ster\u00b7be\u00b7Kleid", "dem", "blin\u00b7den", "Volck", "ge\u00b7webt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als da\u00df niemals erkand der Eitelkeiten Schrancken/", "tokens": ["Als", "da\u00df", "nie\u00b7mals", "er\u00b7kand", "der", "Ei\u00b7tel\u00b7kei\u00b7ten", "Schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.28": {"text": "Und an der Erden nur mit Hertz und Sinn geklebt.", "tokens": ["Und", "an", "der", "Er\u00b7den", "nur", "mit", "Hertz", "und", "Sinn", "ge\u00b7klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach so ein Laster-Rock/ er sey von edlen Steinen", "tokens": ["Ach", "so", "ein", "Las\u00b7ter\u00b7Rock", "/", "er", "sey", "von", "ed\u00b7len", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ART", "NN", "$(", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als Sternen angef\u00fcllt/ von Gold und Silber schwer/", "tokens": ["Als", "Ster\u00b7nen", "an\u00b7ge\u00b7f\u00fcllt", "/", "von", "Gold", "und", "Sil\u00b7ber", "schwer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$(", "APPR", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wird f\u00fcr dem Richter-Stuhl de\u00df H\u00f6chsten gar nicht scheinen", "tokens": ["Wird", "f\u00fcr", "dem", "Rich\u00b7ter\u00b7Stuhl", "de\u00df", "H\u00f6chs\u00b7ten", "gar", "nicht", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn die entbl\u00f6ste Seel von aller Tugend leer", "tokens": ["Wenn", "die", "ent\u00b7bl\u00f6s\u00b7te", "Seel", "von", "al\u00b7ler", "Tu\u00b7gend", "leer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "