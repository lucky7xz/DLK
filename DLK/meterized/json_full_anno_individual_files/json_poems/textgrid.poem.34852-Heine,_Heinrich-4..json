{"textgrid.poem.34852": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmeine Schwiegermutter Ceres!", "tokens": ["\u00bb", "mei\u00b7ne", "Schwie\u00b7ger\u00b7mut\u00b7ter", "Ce\u00b7res", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Klagen, la\u00df die Bitten!", "tokens": ["La\u00df", "die", "Kla\u00b7gen", ",", "la\u00df", "die", "Bit\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein Verlangen, ich gew\u00e4hr es \u2013", "tokens": ["Dein", "Ver\u00b7lan\u00b7gen", ",", "ich", "ge\u00b7w\u00e4hr", "es", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "ADJD", "PPER", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Habe selbst soviel gelitten!", "tokens": ["Ha\u00b7be", "selbst", "so\u00b7viel", "ge\u00b7lit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tr\u00f6ste dich, wir wollen ehrlich", "tokens": ["Tr\u00f6s\u00b7te", "dich", ",", "wir", "wol\u00b7len", "ehr\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Besitz der Tochter teilen,", "tokens": ["Den", "Be\u00b7sitz", "der", "Toch\u00b7ter", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sechs Monden soll sie j\u00e4hrlich", "tokens": ["Und", "sechs", "Mon\u00b7den", "soll", "sie", "j\u00e4hr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VMFIN", "PPER", "ADJD"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Auf der Oberwelt verweilen.", "tokens": ["Auf", "der", "O\u00b7ber\u00b7welt", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hilft dir dort an Sommertagen", "tokens": ["Hilft", "dir", "dort", "an", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den Ackerbaugesch\u00e4ften;", "tokens": ["Bei", "den", "A\u00b7cker\u00b7bau\u00b7ge\u00b7sch\u00e4f\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Strohhut wird sie tragen,", "tokens": ["Ei\u00b7nen", "Stroh\u00b7hut", "wird", "sie", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird auch Blumen daran heften.", "tokens": ["Wird", "auch", "Blu\u00b7men", "da\u00b7ran", "hef\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schw\u00e4rmen wird sie, wenn den Himmel", "tokens": ["Schw\u00e4r\u00b7men", "wird", "sie", ",", "wenn", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberzieht die Abendr\u00f6te,", "tokens": ["\u00dc\u00b7berz\u00b7ieht", "die", "A\u00b7bend\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und am Bach ein Bauerl\u00fcmmel", "tokens": ["Und", "am", "Bach", "ein", "Bau\u00b7er\u00b7l\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00e4rtlich bl\u00e4st die Hirtenfl\u00f6te.", "tokens": ["Z\u00e4rt\u00b7lich", "bl\u00e4st", "die", "Hir\u00b7ten\u00b7fl\u00f6\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wird sich freun mit Gret' und H\u00e4nschen", "tokens": ["Wird", "sich", "freun", "mit", "Gret'", "und", "H\u00e4n\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei des Erntefestes Reigen;", "tokens": ["Bei", "des", "Ern\u00b7te\u00b7fes\u00b7tes", "Rei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Sch\u00f6psen, unter G\u00e4nschen,", "tokens": ["Un\u00b7ter", "Sch\u00f6p\u00b7sen", ",", "un\u00b7ter", "G\u00e4n\u00b7schen", ","], "to