{"textgrid.poem.32741": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "An Lauren im Kloster", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, die, mir entrissen", "tokens": ["O", "du", ",", "die", ",", "mir", "ent\u00b7ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Wahn und Grausamkeit,", "tokens": ["Durch", "Wahn", "und", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In \u00f6den Finsternissen", "tokens": ["In", "\u00f6\u00b7den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich nun dem Tode weiht!", "tokens": ["Sich", "nun", "dem", "To\u00b7de", "weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hier an der Klosterschwelle", "tokens": ["Hier", "an", "der", "Klos\u00b7ter\u00b7schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bewein' ich, Laura, dich,", "tokens": ["Be\u00b7wein'", "ich", ",", "Lau\u00b7ra", ",", "dich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NE", "$,", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und irr' um deine Zelle,", "tokens": ["Und", "irr'", "um", "dei\u00b7ne", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und niemand h\u00f6ret mich.", "tokens": ["Und", "nie\u00b7mand", "h\u00f6\u00b7ret", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wenn noch zu mir Armen", "tokens": ["O", "wenn", "noch", "zu", "mir", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "APPR", "PPER", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sich Engel Gottes nahn;", "tokens": ["Sich", "En\u00b7gel", "Got\u00b7tes", "nahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So zeiget aus Erbarmen", "tokens": ["So", "zei\u00b7get", "aus", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr meinen Jammer an!", "tokens": ["Ihr", "mei\u00b7nen", "Jam\u00b7mer", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df ihrer Andacht Feuer", "tokens": ["Da\u00df", "ih\u00b7rer", "An\u00b7dacht", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir Linderung erfleh',", "tokens": ["Mir", "Lin\u00b7de\u00b7rung", "er\u00b7fleh'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und meine Seele freier", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "frei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durchs Thal der Leiden geh'!", "tokens": ["Durchs", "Thal", "der", "Lei\u00b7den", "geh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von schw\u00e4rzrer Nacht umgeben,", "tokens": ["Von", "schw\u00e4rz\u00b7rer", "Nacht", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als diese Mitternacht,", "tokens": ["Als", "die\u00b7se", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchirr' ich dieses Leben,", "tokens": ["Durch\u00b7irr'", "ich", "die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das du einst hell gemacht.", "tokens": ["Das", "du", "einst", "hell", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Hain, wo liebetrunken", "tokens": ["Im", "Hain", ",", "wo", "lie\u00b7be\u00b7trun\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein Mund mir K\u00fcsse gab,", "tokens": ["Dein", "Mund", "mir", "K\u00fcs\u00b7se", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wank' ich, in Harm versunken,", "tokens": ["Wank'", "ich", ",", "in", "Harm", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und suche stumm mein Grab.", "tokens": ["Und", "su\u00b7che", "stumm", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn in des Chores Halle", "tokens": ["Wenn", "in", "des", "Cho\u00b7res", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich oft Verzweiflung f\u00fchrt,", "tokens": ["Mich", "oft", "Ver\u00b7zwei\u00b7flung", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und durch die Stimmen alle", "tokens": ["Und", "durch", "die", "Stim\u00b7men", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich deine Stimme r\u00fchrt;", "tokens": ["Mich", "dei\u00b7ne", "Stim\u00b7me", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann deucht mir's, da\u00df vom Himmel,", "tokens": ["Dann", "deucht", "mir's", ",", "da\u00df", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Freude dich umwallt,", "tokens": ["Wo", "Freu\u00b7de", "dich", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dein Lied mir ins Get\u00fcmmel", "tokens": ["Dein", "Lied", "mir", "ins", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verworfner Geister hallt.", "tokens": ["Ver\u00b7worf\u00b7ner", "Geis\u00b7ter", "hallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Oft tr\u00e4um' ich, wie der Riegel", "tokens": ["Oft", "tr\u00e4um'", "ich", ",", "wie", "der", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Zelle schnell zerspringt,", "tokens": ["Der", "Zel\u00b7le", "schnell", "zer\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auf der Liebe Fl\u00fcgel", "tokens": ["Und", "auf", "der", "Lie\u00b7be", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich mir ein Engel bringt.", "tokens": ["Dich", "mir", "ein", "En\u00b7gel", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Dein Bildnis wallt hernieder;", "tokens": ["Dein", "Bild\u00b7nis", "wallt", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch ich umarm' es kaum,", "tokens": ["Doch", "ich", "um\u00b7arm'", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So wach' und wein' ich wieder,", "tokens": ["So", "wach'", "und", "wein'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und fluche meinem Traum.", "tokens": ["Und", "flu\u00b7che", "mei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Leben ohne Lauren,", "tokens": ["O", "Le\u00b7ben", "oh\u00b7ne", "Lau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Grimm mir zugedacht!", "tokens": ["Im", "Grimm", "mir", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange wirst du dauren,", "tokens": ["Wie", "lan\u00b7ge", "wirst", "du", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bange Fiebernacht!", "tokens": ["Du", "ban\u00b7ge", "Fie\u00b7ber\u00b7nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erweich ihn du, o Reine,", "tokens": ["Er\u00b7weich", "ihn", "du", ",", "o", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Richter, da\u00df einmal", "tokens": ["Den", "Rich\u00b7ter", ",", "da\u00df", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch Lieb' er uns vereine,", "tokens": ["Durch", "Lieb'", "er", "uns", "ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die er uns selbst befahl!", "tokens": ["Die", "er", "uns", "selbst", "be\u00b7fahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O du, die, mir entrissen", "tokens": ["O", "du", ",", "die", ",", "mir", "ent\u00b7ris\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Wahn und Grausamkeit,", "tokens": ["Durch", "Wahn", "und", "Grau\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In