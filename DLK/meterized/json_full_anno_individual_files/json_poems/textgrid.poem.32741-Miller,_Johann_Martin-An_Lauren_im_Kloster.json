, "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und fluche meinem Traum.", "tokens": ["Und", "flu\u00b7che", "mei\u00b7nem", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O Leben ohne Lauren,", "tokens": ["O", "Le\u00b7ben", "oh\u00b7ne", "Lau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Grimm mir zugedacht!", "tokens": ["Im", "Grimm", "mir", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange wirst du dauren,", "tokens": ["Wie", "lan\u00b7ge", "wirst", "du", "dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bange Fiebernacht!", "tokens": ["Du", "ban\u00b7ge", "Fie\u00b7ber\u00b7nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erweich ihn du, o Reine,", "tokens": ["Er\u00b7weich", "ihn", "du", ",", "o", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Richter, da\u00df einmal", "tokens": ["Den", "Rich\u00b7ter", ",", "da\u00df", "ein\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch Lieb' er uns