{"textgrid.poem.48494": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. Da\u00df er habe unbillig getan, indem er ihm vorgesetzt, sie in zweien Tagen nicht zu besuchen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte, la\u00df doch sehn, was denn die Liebe kan,", "tokens": ["Ich", "dach\u00b7te", ",", "la\u00df", "doch", "sehn", ",", "was", "denn", "die", "Lie\u00b7be", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "ADV", "VVINF", "$,", "PRELS", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wiewol sie sich noch nie mir recht hat kund getan!", "tokens": ["wie\u00b7wol", "sie", "sich", "noch", "nie", "mir", "recht", "hat", "kund", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "PPER", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dictyuna soll ihr H\u00e4upt zu zweien Malen zeigen,", "tokens": ["Dic\u00b7ty\u00b7u\u00b7na", "soll", "ihr", "H\u00e4upt", "zu", "zwei\u00b7en", "Ma\u00b7len", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "und zweimal soll von Ost Apollo wieder steigen,", "tokens": ["und", "zwei\u00b7mal", "soll", "von", "Ost", "A\u00b7pol\u00b7lo", "wie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "APPR", "NE", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "eh' sie mich soll ersehn. Cupido war nicht weit.", "tokens": ["eh'", "sie", "mich", "soll", "er\u00b7sehn", ".", "Cu\u00b7pi\u00b7do", "war", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$.", "NE", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was aber, sprach er, ist denn di\u00df vor ein Bescheid?", "tokens": ["Was", "a\u00b7ber", ",", "sprach", "er", ",", "ist", "denn", "di\u00df", "vor", "ein", "Be\u00b7scheid", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit diesem truckt er los. Ach Lieb, ich bin getroffen!", "tokens": ["Mit", "die\u00b7sem", "truckt", "er", "los", ".", "Ach", "Lieb", ",", "ich", "bin", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df hier, di\u00df war der Pfeil. Die Wunde steht weit offen.", "tokens": ["Di\u00df", "hier", ",", "di\u00df", "war", "der", "Pfeil", ".", "Die", "Wun\u00b7de", "steht", "weit", "of\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PDS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Giebst du mir einen Ku\u00df, so hat es keine Not,", "tokens": ["Giebst", "du", "mir", "ei\u00b7nen", "Ku\u00df", ",", "so", "hat", "es", "kei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "siehst du mich sauer an, so bin ich pl\u00f6tzlich tot.", "tokens": ["siehst", "du", "mich", "sau\u00b7er", "an", ",", "so", "bin", "ich", "pl\u00f6tz\u00b7lich", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich dachte, la\u00df doch sehn, was denn die Liebe kan,", "tokens": ["Ich", "dach\u00b7te", ",", "la\u00df", "doch", "sehn", ",", "was", "denn", "die", "Lie\u00b7be", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "ADV", "VVINF", "$,", "PRELS", "ADV", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wiewol sie sich noch nie mir recht hat kund getan!", "tokens": ["wie\u00b7wol", "sie", "sich", "noch", "nie", "mir", "recht", "hat", "kund", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "PPER", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.i