{"dta.poem.11510": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das 3. capitel des buches Hiob.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durchaus-verlohrner tag! da ich gebohren bin!", "tokens": ["Durch\u00b7aus\u00b7verl\u00b7ohr\u00b7ner", "tag", "!", "da", "ich", "ge\u00b7boh\u00b7ren", "bin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchaus-verlohrner tag! da mich ein weib empfangen", "tokens": ["Durch\u00b7aus\u00b7verl\u00b7ohr\u00b7ner", "tag", "!", "da", "mich", "ein", "weib", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tag! der verworffen bleibt, den mensch und sonne fliehn:", "tokens": ["Tag", "!", "der", "ver\u00b7worf\u00b7fen", "bleibt", ",", "den", "mensch", "und", "son\u00b7ne", "fliehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Den himmel und natur nicht mehr zu sehn verlangen!", "tokens": ["Den", "him\u00b7mel", "und", "na\u00b7tur", "nicht", "mehr", "zu", "sehn", "ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein glantz verwandle sich in grause dunckelheit!", "tokens": ["Dein", "glantz", "ver\u00b7wand\u00b7le", "sich", "in", "grau\u00b7se", "dun\u00b7ckel\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man m\u00fcsse dich nicht mehr in dem calender lesen!", "tokens": ["Man", "m\u00fcs\u00b7se", "dich", "nicht", "mehr", "in", "dem", "ca\u00b7len\u00b7der", "le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und du verdammte nacht! verla\u00df das buch der zeit,", "tokens": ["Und", "du", "ver\u00b7damm\u00b7te", "nacht", "!", "ver\u00b7la\u00df", "das", "buch", "der", "zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$.", "VVIMP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und bleib so tieff verdeckt, als w\u00e4rst du nie gewesen!", "tokens": ["Und", "bleib", "so", "tieff", "ver\u00b7deckt", ",", "als", "w\u00e4rst", "du", "nie", "ge\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "VVPP", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die furcht der einsamkeit schlie\u00df alles jauchtzen aus!", "tokens": ["Die", "furcht", "der", "ein\u00b7sam\u00b7keit", "schlie\u00df", "al\u00b7les", "jaucht\u00b7zen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und man verfluche dich in allen zauber-versen;", "tokens": ["Und", "man", "ver\u00b7flu\u00b7che", "dich", "in", "al\u00b7len", "zau\u00b7ber\u00b7ver\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein himmel aber sey ein schwartzes wolcken-haus,", "tokens": ["Dein", "him\u00b7mel", "a\u00b7ber", "sey", "ein", "schwart\u00b7zes", "wol\u00b7cken\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In welchem statt der storn angst-volle nebel herrschen!", "tokens": ["In", "wel\u00b7chem", "statt", "der", "storn", "angst\u00b7vol\u00b7le", "ne\u00b7bel", "herr\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "APPR", "ART", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Die hoffnung, die du hast, zerflatter\u2019 in die lufft!", "tokens": ["Die", "hoff\u00b7nung", ",", "die", "du", "hast", ",", "zer\u00b7flat\u00b7ter'", "in", "die", "lufft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aurora m\u00fcsse dir kein s\u00fcsses aug ertheilen!", "tokens": ["Au\u00b7ro\u00b7ra", "m\u00fcs\u00b7se", "dir", "kein", "s\u00fcs\u00b7ses", "aug", "er\u00b7thei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der sch\u00f6pffer stosse dich in eine bange klufft,", "tokens": ["Der", "sch\u00f6pf\u00b7fer", "stos\u00b7se", "dich", "in", "ei\u00b7ne", "ban\u00b7ge", "klufft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "APPR", "ART", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo blasser jammer wohnt, und w\u00fcste v\u00f6gel heulen!", "tokens": ["Wo", "blas\u00b7ser", "jam\u00b7mer", "wohnt", ",", "und", "w\u00fcs\u00b7te", "v\u00f6\u00b7gel", "heu\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Warum versperrtest du dem Hiob nicht die welt?", "tokens": ["Wa\u00b7rum", "ver\u00b7sperr\u00b7test", "du", "dem", "Hiob", "nicht", "die", "welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NE", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Warum verbargst du nicht die qual vor meinen augen?", "tokens": ["Wa\u00b7rum", "ver\u00b7bargst", "du", "nicht", "die", "qual", "vor", "mei\u00b7nen", "au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So w\u00fcrde mir die ruh nicht, wie itzund, verg\u00e4llt,", "tokens": ["So", "w\u00fcr\u00b7de", "mir", "die", "ruh", "nicht", ",", "wie", "it\u00b7zund", ",", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "PWAV", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+++-+", "measure": "unknown.measure.septa"}, "line.20": {"text": "Da meine lippen nichts als herbe thr\u00e4nen saugen.", "tokens": ["Da", "mei\u00b7ne", "lip\u00b7pen", "nichts", "als", "her\u00b7be", "thr\u00e4\u00b7nen", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach! warum starb ich nicht, da ich gebohren war?", "tokens": ["Ach", "!", "wa\u00b7rum", "starb", "ich", "nicht", ",", "da", "ich", "ge\u00b7boh\u00b7ren", "war", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Warum vergieng ich nicht, eh mich das licht ber\u00fchret?", "tokens": ["Wa\u00b7rum", "ver\u00b7gieng", "ich", "nicht", ",", "eh", "mich", "das", "licht", "be\u00b7r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn unser mutter schos ist doch die beste bahr,", "tokens": ["Denn", "un\u00b7ser", "mut\u00b7ter", "schos", "ist", "doch", "die", "bes\u00b7te", "bahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Indem sie alle pein mit uns zu grabe f\u00fchret.", "tokens": ["In\u00b7dem", "sie", "al\u00b7le", "pein", "mit", "uns", "zu", "gra\u00b7be", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach! warum schlo\u00df der tod nicht mund und augen zu,", "tokens": ["Ach", "!", "wa\u00b7rum", "schlo\u00df", "der", "tod", "nicht", "mund", "und", "au\u00b7gen", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "ART", "NN", "PTKNEG", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Eh\u2019 ich die s\u00fcsse krafft der mutter-milch genossen?", "tokens": ["Eh'", "ich", "die", "s\u00fcs\u00b7se", "krafft", "der", "mut\u00b7ter\u00b7milch", "ge\u00b7nos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So schlief ich ohne schmertz, und l\u00e4g in stiller ruh:", "tokens": ["So", "schlief", "ich", "oh\u00b7ne", "schmertz", ",", "und", "l\u00e4g", "in", "stil\u00b7ler", "ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Denn diese bleibet doch nur in der grufft verschlossen.", "tokens": ["Denn", "die\u00b7se", "blei\u00b7bet", "doch", "nur", "in", "der", "grufft", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In tieffen gr\u00e4bern hat kein f\u00fcrste was zuvor:", "tokens": ["In", "tief\u00b7fen", "gr\u00e4\u00b7bern", "hat", "kein", "f\u00fcrs\u00b7te", "was", "zu\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "PRELS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Denn der verstorbnen fried ist gleiche durch gegr\u00fcndet.", "tokens": ["Denn", "der", "ver\u00b7storb\u00b7nen", "fried", "ist", "glei\u00b7che", "durch", "ge\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJA", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wohl diesem! der den tod noch, eh er an das thor", "tokens": ["Wohl", "die\u00b7sem", "!", "der", "den", "tod", "noch", ",", "eh", "er", "an", "das", "thor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "$.", "ART", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Des bangen lebens kommt in seiner mutter findet.", "tokens": ["Des", "ban\u00b7gen", "le\u00b7bens", "kommt", "in", "sei\u00b7ner", "mut\u00b7ter", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Jm grabe h\u00f6ret doch der feinde rasen auf:", "tokens": ["Jm", "gra\u00b7be", "h\u00f6\u00b7ret", "doch", "der", "fein\u00b7de", "ra\u00b7sen", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das maul der l\u00e4sterer w\u00fchlt nur in blosem sande:", "tokens": ["Das", "maul", "der", "l\u00e4s\u00b7te\u00b7rer", "w\u00fchlt", "nur", "in", "blo\u00b7sem", "san\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Des todes starcke faust h\u00e4lt alles elend auf,", "tokens": ["Des", "to\u00b7des", "star\u00b7cke", "faust", "h\u00e4lt", "al\u00b7les", "e\u00b7lend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und wen sie frey gemacht, der f\u00fchlet keine bande.", "tokens": ["Und", "wen", "sie", "frey", "ge\u00b7macht", ",", "der", "f\u00fch\u00b7let", "kei\u00b7ne", "ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "O grab! du sanffter ort! dir gleichet kein pallast.", "tokens": ["O", "grab", "!", "du", "sanff\u00b7ter", "ort", "!", "dir", "glei\u00b7chet", "kein", "pal\u00b7last", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Denn wieviel kummer wohnt auch unter g\u00f6ldnen decken;", "tokens": ["Denn", "wie\u00b7viel", "kum\u00b7mer", "wohnt", "auch", "un\u00b7ter", "g\u00f6ld\u00b7nen", "de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bey dir hat jedermann die angenehmste rast,", "tokens": ["Bey", "dir", "hat", "je\u00b7der\u00b7mann", "die", "an\u00b7ge\u00b7nehms\u00b7te", "rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Von der ihn weder blitz noch donner-keil erwecken.", "tokens": ["Von", "der", "ihn", "we\u00b7der", "blitz", "noch", "don\u00b7ner\u00b7keil", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "KON", "VVIMP", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "In deinem hofe gilt kein ansehn der person:", "tokens": ["In", "dei\u00b7nem", "ho\u00b7fe", "gilt", "kein", "an\u00b7sehn", "der", "per\u00b7son", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Hier ist kein herr zu gros, und auch kein knecht zu wenig:", "tokens": ["Hier", "ist", "kein", "herr", "zu", "gros", ",", "und", "auch", "kein", "knecht", "zu", "we\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKA", "ADJD", "$,", "KON", "ADV", "PIAT", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Hier ist ein schlechter stuhl so kostbar als ein thron:", "tokens": ["Hier", "ist", "ein", "schlech\u00b7ter", "stuhl", "so", "kost\u00b7bar", "als", "ein", "thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und der geringste ruht so sicher als ein k\u00f6nig.", "tokens": ["Und", "der", "ge\u00b7rings\u00b7te", "ruht", "so", "si\u00b7cher", "als", "ein", "k\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wie gl\u00fccklich w\u00e4r ein mensch, dem dieses lebens-licht", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "w\u00e4r", "ein", "mensch", ",", "dem", "die\u00b7ses", "le\u00b7bens\u00b7licht"], "token_info": ["word", "word", "word", "word", 