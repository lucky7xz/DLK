word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Ach, schmertzen, die ihr mich in marck und adern plagt!", "tokens": ["Ach", ",", "schmert\u00b7zen", ",", "die", "ihr", "mich", "in", "marck", "und", "a\u00b7dern", "plagt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "PRELS", "PPER", "PRF", "APPR", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Wenn machet ihr mir einst die burg des todes offen?", "tokens": ["Wenn", "ma\u00b7chet", "ihr", "mir", "einst", "die", "burg", "des", "to\u00b7des", "of\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Denn mein gel\u00fcck ist aus! ich bin nicht, der ich war", "tokens": ["Denn", "mein", "ge\u00b7l\u00fcck", "ist", "aus", "!", "ich", "bin", "nicht", ",", "der", "ich", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$.", "PPER", "VAFIN", "PTKNEG", "$,", "PRELS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Der himmel hat den bau der stillen ruh zerrissen:", "tokens": ["Der", "him\u00b7mel", "hat", "den", "bau", "der", "stil\u00b7len", "ruh", "zer\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Mein leben ist mein tod, das betten meine bahr;", "tokens": ["Mein", "le\u00b7ben", "ist", "mein", "tod", ",", "das", "bet\u00b7ten", "mei\u00b7ne", "bahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Ich aber bin ein brunn, aus welchem thr\u00e4nen fliessen.", "tokens": ["Ich", "a\u00b7ber", "bin", "ein", "brunn", ",", "aus", "wel\u00b7chem", "thr\u00e4\u00b7nen", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "$,", "APPR", "PWAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}