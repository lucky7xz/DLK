{"textgrid.poem.48782": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Zum Abschiede, als einer von der Liebsten verreisete. Auch nach demselbigen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade! Du hartes Wort! Mit Furchten mu\u00df ich scheiden,", "tokens": ["A\u00b7de", "!", "Du", "har\u00b7tes", "Wort", "!", "Mit", "Furch\u00b7ten", "mu\u00df", "ich", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$.", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "mit Sorgen mu\u00df ich weg. Denn wie man sonsten spricht,", "tokens": ["mit", "Sor\u00b7gen", "mu\u00df", "ich", "weg", ".", "Denn", "wie", "man", "sons\u00b7ten", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKVZ", "$.", "KON", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so pflegt Abwesenheit zu l\u00f6schen aus das Licht,", "tokens": ["so", "pflegt", "Ab\u00b7we\u00b7sen\u00b7heit", "zu", "l\u00f6\u00b7schen", "aus", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das angestecket ist im Herzen unsrer Freuden.", "tokens": ["das", "an\u00b7ge\u00b7ste\u00b7cket", "ist", "im", "Her\u00b7zen", "uns\u00b7rer", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum weil mir diese Furcht erregt ein solches Leiden,", "tokens": ["Drum", "weil", "mir", "die\u00b7se", "Furcht", "er\u00b7regt", "ein", "sol\u00b7ches", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das mich vorher st\u00f6\u00dft an, so tu ich unrecht nicht,", "tokens": ["das", "mich", "vor\u00b7her", "st\u00f6\u00dft", "an", ",", "so", "tu", "ich", "un\u00b7recht", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ich ein Tr\u00e4nenbad gie\u00df auf mein Angesicht,", "tokens": ["da\u00df", "ich", "ein", "Tr\u00e4\u00b7nen\u00b7bad", "gie\u00df", "auf", "mein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ob nicht ein Fremder sich f\u00fcr mich an euch mag weiden.", "tokens": ["ob", "nicht", "ein", "Frem\u00b7der", "sich", "f\u00fcr", "mich", "an", "euch", "mag", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PRF", "APPR", "PPER", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch scheid' ich anders nicht, als auf des Vogels Art,", "tokens": ["Doch", "scheid'", "ich", "an\u00b7ders", "nicht", ",", "als", "auf", "des", "Vo\u00b7gels", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der, weil er durch das Garn zuvor gefangen ward,", "tokens": ["der", ",", "weil", "er", "durch", "das", "Garn", "zu\u00b7vor", "ge\u00b7fan\u00b7gen", "ward", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so oft zur\u00fccke mu\u00df, so oft er auf begehret.", "tokens": ["so", "oft", "zu\u00b7r\u00fc\u00b7cke", "mu\u00df", ",", "so", "oft", "er", "auf", "be\u00b7ge\u00b7hret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VMFIN", "$,", "ADV", "ADV", "PPER", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Lieb, euer Angesicht ist mir an Bandes Stat,", "tokens": ["Lieb", ",", "eu\u00b7er", "An\u00b7ge\u00b7sicht", "ist", "mir", "an", "Ban\u00b7des", "Stat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "da mein gefangner Geist sich ein verwirret hat,", "tokens": ["da", "mein", "ge\u00b7fang\u00b7ner", "Geist", "sich", "ein", "ver\u00b7wir\u00b7ret", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PRF", "ART", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df, wo ich auch nur bin, er wieder zu euch kehret.", "tokens": ["da\u00df", ",", "wo", "ich", "auch", "nur", "bin", ",", "er", "wie\u00b7der", "zu", "euch", "keh\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ade! Du hartes Wort! Mit Furchten mu\u00df ich scheiden,", "tokens": ["A\u00b7de", "!", "Du", "har\u00b7tes", "Wort", "!", "Mit", "Furch\u00b7ten", "mu\u00df", "ich", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$.", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "mit Sorgen mu\u00df ich weg. Denn wie man sonsten spricht,", "tokens": ["mit", "Sor\u00b7gen", "mu\u00df", "ich", "weg", ".", "Denn", "wie", "man", "sons\u00b7ten", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKVZ", "$.", "KON", "PWAV", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so pflegt Abwesenheit zu l\u00f6schen aus das Licht,", "tokens": ["so", "pflegt", "Ab\u00b7we\u00b7sen\u00b7heit", "zu", "l\u00f6\u00b7schen", "aus", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct