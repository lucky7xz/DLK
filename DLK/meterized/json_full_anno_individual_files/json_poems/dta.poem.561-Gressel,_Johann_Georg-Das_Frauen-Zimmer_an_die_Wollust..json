{"dta.poem.561": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Frauen-Zimmer an die Wollust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wir bleiben dir getreu/ du Mutter aller L\u00fcste", "tokens": ["Wir", "blei\u00b7ben", "dir", "ge\u00b7treu", "/", "du", "Mut\u00b7ter", "al\u00b7ler", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "PPER", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleib uns in Gnaden auch nur ferner zugethan/", "tokens": ["Bleib", "uns", "in", "Gna\u00b7den", "auch", "nur", "fer\u00b7ner", "zu\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir ehren deine Pracht/ wir zeigen unser Br\u00fcste", "tokens": ["Wir", "eh\u00b7ren", "dei\u00b7ne", "Pracht", "/", "wir", "zei\u00b7gen", "un\u00b7ser", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und stimmen zu dem Sieg ein frohes ", "tokens": ["Und", "stim\u00b7men", "zu", "dem", "Sieg", "ein", "fro\u00b7hes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}