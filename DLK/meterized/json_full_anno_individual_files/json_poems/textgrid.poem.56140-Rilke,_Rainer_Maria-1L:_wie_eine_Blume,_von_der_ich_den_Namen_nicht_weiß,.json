{"textgrid.poem.56140": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: wie eine Blume, von der ich den Namen nicht wei\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "wie eine Blume, von der ich den Namen nicht wei\u00df,", "tokens": ["wie", "ei\u00b7ne", "Blu\u00b7me", ",", "von", "der", "ich", "den", "Na\u00b7men", "nicht", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "noch ", "tokens": ["noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "sch\u00f6ne Gespielin des un\u00fcberwindlichen Schrei's.", "tokens": ["sch\u00f6\u00b7ne", "Ge\u00b7spie\u00b7lin", "des", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7li\u00b7chen", "Schrei'", "s."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "NE"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}, "stanza.2": {"line.1": {"text": "T\u00e4nzerin erst, die pl\u00f6tzlich, den K\u00f6rper voll Z\u00f6gern,", "tokens": ["T\u00e4n\u00b7ze\u00b7rin", "erst", ",", "die", "pl\u00f6tz\u00b7lich", ",", "den", "K\u00f6r\u00b7per", "voll", "Z\u00f6\u00b7gern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ADJD", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "anhielt, als g\u00f6\u00df man ihr Jungsein in Erz;", "tokens": ["an\u00b7hielt", ",", "als", "g\u00f6\u00df", "man", "ihr", "