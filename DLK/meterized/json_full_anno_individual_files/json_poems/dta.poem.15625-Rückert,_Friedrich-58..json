{"dta.poem.15625": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Menschheit k\u00f6nntest du als einen Kreis wol denken,", "tokens": ["Die", "Menschheit", "k\u00f6nn\u00b7test", "du", "als", "ei\u00b7nen", "Kreis", "wol", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Worein die Einzelnen nothwendig sich verschrenken.", "tokens": ["Wo\u00b7rein", "die", "Ein\u00b7zel\u00b7nen", "noth\u00b7wen\u00b7dig", "sich", "ver\u00b7schren\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Als Kreisabschnitte dann, die frei im Ganzen haften", "tokens": ["Als", "Kreis\u00b7ab\u00b7schnit\u00b7te", "dann", ",", "die", "frei", "im", "Gan\u00b7zen", "haf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eigne Ganze, kannst du denken V\u00f6lkerschaften.", "tokens": ["Als", "eig\u00b7ne", "Gan\u00b7ze", ",", "kannst", "du", "den\u00b7ken", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein das Ganze selbst tritt niemals ganz hervor,", "tokens": ["Al\u00b7lein", "das", "Gan\u00b7ze", "selbst", "tritt", "nie\u00b7mals", "ganz", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und andre Menschheit lebt stets als gelebt zuvor.", "tokens": ["Und", "and\u00b7re", "Menschheit", "lebt", "stets", "als", "ge\u00b7lebt", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "KOKOM", "VVPP", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Vorstellen magst du denn, alsob ein Wasser w\u00e4re", "tokens": ["Vor\u00b7stel\u00b7len", "magst", "du", "denn", ",", "al\u00b7sob", "ein", "Was\u00b7ser", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,", "KOUS", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Ewigkeit, wo Kreis aus Kreis die Zeit geb\u00e4re.", "tokens": ["Die", "E\u00b7wig\u00b7keit", ",", "wo", "Kreis", "aus", "Kreis", "die", "Zeit", "ge\u00b7b\u00e4\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wo ist der Kreis, der war? zum weitern aufgeschlossen;", "tokens": ["Wo", "ist", "der", "Kreis", ",", "der", "war", "?", "zum", "wei\u00b7tern", "auf\u00b7ge\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "VAFIN", "$.", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo der weitere? zu weiterm noch ergossen.", "tokens": ["Und", "wo", "der", "wei\u00b7te\u00b7re", "?", "zu", "wei\u00b7term", "noch", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "$.", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Was sind die Einzelnen? sie sind die wirklich seinden;", "tokens": ["Was", "sind", "die", "Ein\u00b7zel\u00b7nen", "?", "sie", "sind", "die", "wirk\u00b7lich", "sein\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Gedankenkreise nur Menschheit und Volksgemeinden.", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7krei\u00b7se", "nur", "Menschheit", "und", "Volks\u00b7ge\u00b7mein\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Sie sind die bleibenden, wenn Kreis in Kreis zerronnen,", "tokens": ["Sie", "sind", "die", "blei\u00b7ben\u00b7den", ",", "wenn", "Kreis", "in", "Kreis", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "KOUS", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wassertropfen, die Gott z\u00e4hlt, im Sch\u00f6pfungsbronnen.", "tokens": ["Die", "Was\u00b7ser\u00b7trop\u00b7fen", ",", "die", "Gott", "z\u00e4hlt", ",", "im", "Sch\u00f6p\u00b7fungs\u00b7bron\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Drum danke Gott, und f\u00fchls, da\u00df du ein Einzler seist,", "tokens": ["Drum", "dan\u00b7ke", "Gott", ",", "und", "f\u00fchls", ",", "da\u00df", "du", "ein", "Einz\u00b7ler", "seist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht die Erscheinung nur von allgemeinem Geist.", "tokens": ["Nicht", "die", "Er\u00b7schei\u00b7nung", "nur", "von", "all\u00b7ge\u00b7mei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}