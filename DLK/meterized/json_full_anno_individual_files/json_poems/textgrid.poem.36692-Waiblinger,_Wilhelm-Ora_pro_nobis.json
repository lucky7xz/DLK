{"textgrid.poem.36692": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ora pro nobis", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil'ge D\u00e4mm'rung waltet durch der Rotunda", "tokens": ["Heil'\u00b7ge", "D\u00e4m\u00b7m'\u00b7rung", "wal\u00b7tet", "durch", "der", "Ro\u00b7tun\u00b7da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Tausendj\u00e4hr'ge W\u00f6lbung, der Geist des Abends", "tokens": ["Tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "W\u00f6l\u00b7bung", ",", "der", "Geist", "des", "A\u00b7bends"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Mahnt zum Beten, mahnet zur letzten Andacht,", "tokens": ["Mahnt", "zum", "Be\u00b7ten", ",", "mah\u00b7net", "zur", "letz\u00b7ten", "An\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf den Knieen umher in des Tempels hoher", "tokens": ["Auf", "den", "Kni\u00b7e\u00b7en", "um\u00b7her", "in", "des", "Tem\u00b7pels", "ho\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPR", "ART", "NN", "ADJA"], "meter": "+-+---+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rundung liegt das gl\u00e4ubige Volk, und Alles", "tokens": ["Run\u00b7dung", "liegt", "das", "gl\u00e4u\u00b7bi\u00b7ge", "Volk", ",", "und", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PIS"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "T\u00f6nt einstimmig, Jungfrau, dein Lob und flehet:", "tokens": ["T\u00f6nt", "ein\u00b7stim\u00b7mig", ",", "Jung\u00b7frau", ",", "dein", "Lob", "und", "fle\u00b7het", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "NN", "$,", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und die Schatten decken auch mich; der Vielen", "tokens": ["Und", "die", "Schat\u00b7ten", "de\u00b7cken", "auch", "mich", ";", "der", "Vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PPER", "$.", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sieht mich keiner, wunderbar dr\u00e4ngt's von Innen,", "tokens": ["Sieht", "mich", "kei\u00b7ner", ",", "wun\u00b7der\u00b7bar", "dr\u00e4ngt's", "von", "In\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Widerst\u00e4nd' ich? \u2013 Zaubrische Macht, ich kniee,", "tokens": ["Wi\u00b7der\u00b7st\u00e4nd'", "ich", "?", "\u2013", "Zaub\u00b7ri\u00b7sche", "Macht", ",", "ich", "kni\u00b7ee", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "$(", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+---", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Immer wiederkehrt der Gesang, der Vorwelt", "tokens": ["Im\u00b7mer", "wie\u00b7der\u00b7kehrt", "der", "Ge\u00b7sang", ",", "der", "Vor\u00b7welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Schauer kehren wieder mit ihm \u2013 o Menschheit,", "tokens": ["Schau\u00b7er", "keh\u00b7ren", "wie\u00b7der", "mit", "ihm", "\u2013", "o", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPER", "$(", "FM", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sieh' mich nicht, ich bin \u2013 ich bin dein und flehe:", "tokens": ["Sieh'", "mich", "nicht", ",", "ich", "bin", "\u2013", "ich", "bin", "dein", "und", "fle\u00b7he", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "$(", "PPER", "VAFIN", "PPOSAT", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Doch was f\u00fchl' ich! Holde Erinn'rung, bist du's,", "tokens": ["Doch", "was", "f\u00fchl'", "ich", "!", "Hol\u00b7de", "Er\u00b7inn'\u00b7rung", ",", "bist", "du's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,", "VAFIN", "PIS", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mich tief anwandelt, o bitter bist du,", "tokens": ["Die", "mich", "tief", "an\u00b7wan\u00b7delt", ",", "o", "bit\u00b7ter", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "FM", "ADJD", "VAFIN", "PPER