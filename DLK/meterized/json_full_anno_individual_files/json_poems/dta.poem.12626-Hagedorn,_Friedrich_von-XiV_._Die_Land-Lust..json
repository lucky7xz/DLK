{"dta.poem.12626": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiV .  \n  Die Land-Lust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gesch\u00e4fte, Zwang und Grillen,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7te", ",", "Zwang", "und", "Gril\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entweiht nicht diese Trift:", "tokens": ["Ent\u00b7weiht", "nicht", "die\u00b7se", "Trift", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich finde hier im Stillen", "tokens": ["Ich", "fin\u00b7de", "hier", "im", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Unmuths Gegengift.", "tokens": ["Des", "Un\u00b7muths", "Ge\u00b7gen\u00b7gift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr Schw\u00e4tzer, die ich meide,", "tokens": ["Ihr", "Schw\u00e4t\u00b7zer", ",", "die", "ich", "mei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verge\u00dft mir nachzuziehn:", "tokens": ["Ver\u00b7ge\u00dft", "mir", "nach\u00b7zu\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Verfehlt den Sitz der Freude,", "tokens": ["Ver\u00b7fehlt", "den", "Sitz", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verfehlt der Felder Gr\u00fcn.", "tokens": ["Ver\u00b7fehlt", "der", "Fel\u00b7der", "Gr\u00fcn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es webet, wallt und spielet", "tokens": ["Es", "we\u00b7bet", ",", "wallt", "und", "spie\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Laub um jeden Strauch,", "tokens": ["Das", "Laub", "um", "je\u00b7den", "Strauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Staude f\u00fchlet", "tokens": ["Und", "je\u00b7de", "Stau\u00b7de", "f\u00fch\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des lauen Zephyrs Hauch.", "tokens": ["Des", "lau\u00b7en", "Ze\u00b7phyrs", "Hauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was mir vor Augen schwebet,", "tokens": ["Was", "mir", "vor", "Au\u00b7gen", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gef\u00e4llt und h\u00fcpft und singt;", "tokens": ["Ge\u00b7f\u00e4llt", "und", "h\u00fcpft", "und", "singt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und alles, alles lebet,", "tokens": ["Und", "al\u00b7les", ",", "al\u00b7les", "le\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und alles scheint verj\u00fcngt.", "tokens": ["Und", "al\u00b7les", "scheint", "ver\u00b7j\u00fcngt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr Th\u00e4ler und ihr H\u00f6hen,", "tokens": ["Ihr", "Th\u00e4\u00b7ler", "und", "ihr", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lust und Sommer schm\u00fcckt!", "tokens": ["Die", "Lust", "und", "Som\u00b7mer", "schm\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Euch, ungest\u00f6rt, zu sehen", "tokens": ["Euch", ",", "un\u00b7ge\u00b7st\u00f6rt", ",", "zu", "se\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJD", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist was mein Herz erquickt.", "tokens": ["Ist", "was", "mein", "Herz", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Reitzung freyer Felder", "tokens": ["Die", "Reit\u00b7zung", "frey\u00b7er", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Besch\u00e4mt der G\u00e4rten Pracht,", "tokens": ["Be\u00b7sch\u00e4mt", "der", "G\u00e4r\u00b7ten", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in die offnen W\u00e4lder", "tokens": ["Und", "in", "die", "off\u00b7nen", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird ohne Zwang gelacht.", "tokens": ["Wird", "oh\u00b7ne", "Zwang", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Saat ist aufgeschossen", "tokens": ["Die", "Saat", "ist", "auf\u00b7ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und reitzt der Schnitter Hand.", "tokens": ["Und", "reitzt", "der", "Schnit\u00b7ter", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bl\u00e4ttervollen Sprossen", "tokens": ["Die", "bl\u00e4t\u00b7ter\u00b7vol\u00b7len", "Spros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschatten Berg und Land.", "tokens": ["Be\u00b7schat\u00b7ten", "Berg", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die V\u00f6gel, die wir h\u00f6ren,", "tokens": ["Die", "V\u00f6\u00b7gel", ",", "die", "wir", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geniessen ihrer Zeit:", "tokens": ["Ge\u00b7nies\u00b7sen", "ih\u00b7rer", "Zeit", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts t\u00f6nt in ihren Ch\u00f6ren,", "tokens": ["Nichts", "t\u00f6nt", "in", "ih\u00b7ren", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Scherz und Z\u00e4rtlichkeit.", "tokens": ["Als", "Scherz", "und", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie thront auf Mo\u00df und Rasen", "tokens": ["Wie", "thront", "auf", "Mo\u00df", "und", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Hirt in sanfter Ruh!", "tokens": ["Der", "Hirt", "in", "sanf\u00b7ter", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sieht die Herde grasen", "tokens": ["Er", "sieht", "die", "Her\u00b7de", "gra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spielt ein Lied dazu.", "tokens": ["Und", "spielt", "ein", "Lied", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein muntres Lied ergetzet", "tokens": ["Sein", "mun\u00b7tres", "Lied", "er\u00b7get\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und scheut die Kenner nicht;", "tokens": ["Und", "scheut", "die", "Ken\u00b7ner", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gef\u00e4lligkeit ersetzet", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "er\u00b7set\u00b7zet"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was ihm an Kunst gebricht.", "tokens": ["Was", "ihm", "an", "Kunst", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Aus Dorf und B\u00fcschen dringet", "tokens": ["Aus", "Dorf", "und", "B\u00fc\u00b7schen", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Jugend Kern hervor,", "tokens": ["Der", "Ju\u00b7gend", "Kern", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tanzt und stimmt und singet", "tokens": ["Und", "tanzt", "und", "stimmt", "und", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach seinem Haber-Roh.", "tokens": ["Nach", "sei\u00b7nem", "Ha\u00b7ber\u00b7Roh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Reihen-Tanz vollenden", "tokens": ["Den", "Rei\u00b7hen\u00b7Tanz", "voll\u00b7en\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Hirten auf der Hut,", "tokens": ["Die", "Hir\u00b7ten", "auf", "der", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit treu-vereinten H\u00e4nden,", "tokens": ["Mit", "treu\u00b7ver\u00b7ein\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Spr\u00fcngen voller Muth.", "tokens": ["Mit", "Spr\u00fcn\u00b7gen", "vol\u00b7ler", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie manche frische Dirne", "tokens": ["Wie", "man\u00b7che", "fri\u00b7sche", "Dir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schminkt sich aus jenem Bach", "tokens": ["Schminkt", "sich", "aus", "je\u00b7nem", "Bach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gibt an Brust und Stirne", "tokens": ["Und", "gibt", "an", "Brust", "und", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch nicht den Sch\u00f6nsten nach.", "tokens": ["Doch", "nicht", "den", "Sch\u00f6ns\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gesundheit und Vergn\u00fcgen", "tokens": ["Ge\u00b7sund\u00b7heit", "und", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Belebt ihr Aug und Herz,", "tokens": ["Be\u00b7lebt", "ihr", "Aug", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und reitzt in ihren Z\u00fcgen,", "tokens": ["Und", "reitzt", "in", "ih\u00b7ren", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und lacht in ihrem Scherz.", "tokens": ["Und", "lacht", "in", "ih\u00b7rem", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}