{"textgrid.poem.24071": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: A la bonne heure! Strategischen Blick", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "A la bonne heure! Strategischen Blick", "tokens": ["A", "la", "bon\u00b7ne", "heu\u00b7re", "!", "Stra\u00b7te\u00b7gi\u00b7schen", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Hatten die R\u00f6mer und viel Geschick,", "tokens": ["Hat\u00b7ten", "die", "R\u00f6\u00b7mer", "und", "viel", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mu\u00df ich sagen, im Schanzenbauen.", "tokens": ["Mu\u00df", "ich", "sa\u00b7gen", ",", "im", "Schan\u00b7zen\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hoch steh ich oben in eifrigem Schauen", "tokens": ["Hoch", "steh", "ich", "o\u00b7ben", "in", "eif\u00b7ri\u00b7gem", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Durch den sch\u00f6nen Septembertag,", "tokens": ["Durch", "den", "sch\u00f6\u00b7nen", "Sep\u00b7tem\u00b7ber\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ob sie nicht endlich kommen mag.", "tokens": ["Ob", "sie", "nicht", "end\u00b7lich", "kom\u00b7men", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Unten der See liegt unbewegt,", "tokens": ["Un\u00b7ten", "der", "See", "liegt", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Oben im Walde kein Wipfel sich regt,", "tokens": ["O\u00b7ben", "im", "Wal\u00b7de", "kein", "Wip\u00b7fel", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Ringsum auf Feldern mit Sensen und Sicheln", "tokens": ["Ring\u00b7sum", "auf", "Fel\u00b7dern", "mit", "Sen\u00b7sen", "und", "Si\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Wimmelts von Hansen und Franzen und Micheln;", "tokens": ["Wim\u00b7melts", "von", "Han\u00b7sen", "und", "Fran\u00b7zen", "und", "Mi\u00b7cheln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Feierlich brummt es vom Klosterturm sechse,", "tokens": ["Fei\u00b7er\u00b7lich", "brummt", "es", "vom", "Klos\u00b7ter\u00b7turm", "sech\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.12": {"text": "Hurra, da kommt meine braune Hexe!", "tokens": ["Hur\u00b7ra", ",", "da", "kommt", "mei\u00b7ne", "brau\u00b7ne", "He\u00b7xe", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.13": {"text": "\u00bbschneller, schneller, ich warte dein!\u00ab", "tokens": ["\u00bb", "schnel\u00b7ler", ",", "schnel\u00b7ler", ",", "ich", "war\u00b7te", "dein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "$,", "PPER", "VVFIN", "PPOSAT", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Holla, da rennt sie querfeldein,", "tokens": ["Hol\u00b7la", ",", "da", "rennt", "sie", "quer\u00b7fel\u00b7dein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Fliegt an die Brust mir mit einem Sprunge,", "tokens": ["Fliegt", "an", "die", "Brust", "mir", "mit", "ei\u00b7nem", "Sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "St\u00fcrmisch heb ich sie hoch im Schwunge,", "tokens": ["St\u00fcr\u00b7misch", "heb", "ich", "sie", "hoch", "im", "Schwun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Ku\u00df und Umarmung, eins, zwei drei,", "tokens": ["Ku\u00df", "und", "Um\u00b7ar\u00b7mung", ",", "eins", ",", "zwei", "drei", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PIS", "$,", "CARD", "CARD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Und im Grase liegen wir zwei,", "tokens": ["Und", "im", "Gra\u00b7se", "lie\u00b7gen", "wir", "zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "CARD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Rollen die B\u00f6schung hinunter weich,", "tokens": ["Rol\u00b7len", "die", "B\u00f6\u00b7schung", "hin\u00b7un\u00b7ter", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Rollen direkt ins Himmelreich.", "tokens": ["Rol\u00b7len", "di\u00b7rekt", "ins", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Keiner st\u00f6rt uns. Schanzenumsch\u00fctzt", "tokens": ["Kei\u00b7ner", "st\u00f6rt", "uns", ".", "Schan\u00b7ze\u00b7num\u00b7sch\u00fctzt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Haben wir r\u00f6mische Kriegskunst gen\u00fctzt.", "tokens": ["Ha\u00b7ben", "wir", "r\u00f6\u00b7mi\u00b7sche", "Kriegs\u00b7kunst", "ge\u00b7n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Was vor vielen hundert Jahren", "tokens": ["Was", "vor", "vie\u00b7len", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Schutz gewesen den Legionaren", "tokens": ["Schutz", "ge\u00b7we\u00b7sen", "den", "Le\u00b7gi\u00b7o\u00b7na\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAPP", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Gegen Attaque und Ueberfall", "tokens": ["Ge\u00b7gen", "At\u00b7ta\u00b7que", "und", "Ue\u00b7ber\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Ward uns zum bergenden Liebeswall.", "tokens": ["Ward", "uns", "zum", "ber\u00b7gen\u00b7den", "Lie\u00b7bes\u00b7wall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Lieb ich auch sonst nicht die harte Stadt,", "tokens": ["Lieb", "ich", "auch", "sonst", "nicht", "die", "har\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.28": {"text": "Die eine W\u00f6lfin im Wappen hat,", "tokens": ["Die", "ei\u00b7ne", "W\u00f6l\u00b7fin", "im", "Wap\u00b7pen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Heute sing ich ihr Preis und Lob,", "tokens": ["Heu\u00b7te", "sing", "ich", "ihr", "Preis", "und", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Da\u00df sie die sch\u00fctzende Schanze uns hob,", "tokens": ["Da\u00df", "sie", "die", "sch\u00fct\u00b7zen\u00b7de", "Schan\u00b7ze", "uns", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Die uns ein Liebesbette bot", "tokens": ["Die", "uns", "ein", "Lie\u00b7bes\u00b7bet\u00b7te", "bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Bis ins erl\u00f6schende Abendrot.", "tokens": ["Bis", "ins", "er\u00b7l\u00f6\u00b7schen\u00b7de", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "A la bonne heure! Strategischen Blick", "tokens": ["A", "la", "bon\u00b7ne", "heu\u00b7re", "!", "Stra\u00b7te\u00b7gi\u00b7schen", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Hatten die R\u00f6mer und viel Geschick,", "tokens": ["Hat\u00b7ten", "die", "R\u00f6\u00b7mer", "und", "viel", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "PIAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mu\u00df ich sagen, im Schanzenbauen.", "tokens": ["Mu\u00df", "ich", "sa\u00b7gen", ",", "im", "Schan\u00b7zen\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "wor