.invert"}, "line.21": {"text": "Keiner st\u00f6rt uns. Schanzenumsch\u00fctzt", "tokens": ["Kei\u00b7ner", "st\u00f6rt", "uns", ".", "Schan\u00b7ze\u00b7num\u00b7sch\u00fctzt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Haben wir r\u00f6mische Kriegskunst gen\u00fctzt.", "tokens": ["Ha\u00b7ben", "wir", "r\u00f6\u00b7mi\u00b7sche", "Kriegs\u00b7kunst", "ge\u00b7n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Was vor vielen hundert Jahren", "tokens": ["Was", "vor", "vie\u00b7len", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Schutz gewesen den Legionaren", "tokens": ["Schutz", "ge\u00b7we\u00b7sen", "den", "Le\u00b7gi\u00b7o\u00b7na\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAPP", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Gegen Attaque und Ueberfall", "tokens": ["Ge\u00b7gen", "At\u00b7ta\u00b7que", "und", "Ue\u00b7ber\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Ward uns zum bergenden Liebeswall.", "tokens": ["Ward", "uns", "zum", "ber\u00b7gen\u00b7den", "Lie\u00b7bes\u00b7wall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Lieb ich auch sonst nicht die harte Stadt,", "tokens": ["Lieb", "ich", "auch", "sonst", "nicht", "die", "har\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.28": {"text": "Die eine W\u00f6lfin im Wappen hat,", "tokens": ["Die", "ei\u00b7ne", "W\u00f6l\u00b7fin", "im", "Wap\u00b7pen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Heute sing ich ihr Preis und Lob,", "tokens": ["Heu\u00b7te", "sing", "ich", "ihr", "Preis", "und", "Lob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Da\u00df sie die sch\u00fctzende Schanze uns hob,", "tokens": ["Da\u00df", "sie", "die", "sch\u00fct\u00b7zen\u00b7de", "Schan\u00b7ze", "uns", "hob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Die uns ein Liebesbette bot", "tokens": ["Die", "uns", "ein", "Lie\u00b7bes\u00b7bet\u00b7te", "bot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Bis ins erl\u00f6schende Abendrot.", "tokens": ["Bis", "ins", "er\u00b7l\u00f6\u00b7schen\u00b7de", "A\u00b7ben\u00b7drot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}