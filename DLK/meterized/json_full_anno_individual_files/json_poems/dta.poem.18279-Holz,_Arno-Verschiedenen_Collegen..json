{"dta.poem.18279": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Verschiedenen Collegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "In jedem Lenz noch rhythmisch angeschw\u00e4rmt,", "tokens": ["In", "je\u00b7dem", "Lenz", "noch", "rhyth\u00b7misch", "an\u00b7ge\u00b7schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O, wenn ihr w\u00fc\u00dftet, wie sich meine Seele", "tokens": ["O", ",", "wenn", "ihr", "w\u00fc\u00df\u00b7tet", ",", "wie", "sich", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um ihre gottverlassnen Schwestern h\u00e4rmt!", "tokens": ["Um", "ih\u00b7re", "gott\u00b7ver\u00b7lass\u00b7nen", "Schwes\u00b7tern", "h\u00e4rmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dreht ihr auch noch so ernsthaft eure Phrase,", "tokens": ["Dreht", "ihr", "auch", "noch", "so", "ernst\u00b7haft", "eu\u00b7re", "Phra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Teufel setzt sie lustig in Musik,", "tokens": ["Der", "Teu\u00b7fel", "setzt", "sie", "lus\u00b7tig", "in", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn eine ungeheuer lange Nase", "tokens": ["Denn", "ei\u00b7ne", "un\u00b7ge\u00b7heu\u00b7er", "lan\u00b7ge", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat seine Gro\u00dfmama, die Frau Kritik.", "tokens": ["Hat", "sei\u00b7ne", "Gro\u00df\u00b7ma\u00b7ma", ",", "die", "Frau", "Kri\u00b7tik", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}