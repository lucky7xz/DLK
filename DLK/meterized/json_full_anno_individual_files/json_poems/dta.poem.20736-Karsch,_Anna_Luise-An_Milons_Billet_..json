{"dta.poem.20736": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Milons Billet .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was seh ich! all ihr Thatenrichter!               ", "tokens": ["Was", "seh", "ich", "!", "all", "ihr", "Tha\u00b7ten\u00b7rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr G\u00f6tter! \u2014 Was erblick ich hier!", "tokens": ["Ihr", "G\u00f6t\u00b7ter", "!", "Was", "er\u00b7blick", "ich", "hier", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "$(", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ha, mein Geliebter spricht mit mir,", "tokens": ["Ha", ",", "mein", "Ge\u00b7lieb\u00b7ter", "spricht", "mit", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er selbst, er ist der feine Dichter,", "tokens": ["Er", "selbst", ",", "er", "ist", "der", "fei\u00b7ne", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der diese goldne Worte schrieb \u2014", "tokens": ["Der", "die\u00b7se", "gold\u00b7ne", "Wor\u00b7te", "schrieb"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun wirds in meiner Seele lichter,", "tokens": ["Nun", "wirds", "in", "mei\u00b7ner", "See\u00b7le", "lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nun hab\u2019 ichs Leben wieder lieb \u2014", "tokens": ["Nun", "hab'", "ichs", "Le\u00b7ben", "wie\u00b7der", "lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O theures Blatt, wo willst du bleiben;", "tokens": ["O", "theu\u00b7res", "Blatt", ",", "wo", "willst", "du", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Herz verlangt dich Schlag auf Schlag", "tokens": ["Mein", "Herz", "ver\u00b7langt", "dich", "Schlag", "auf", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit hei\u00dfen Foderungsbetreiben.", "tokens": ["Mit", "hei\u00b7\u00dfen", "Fo\u00b7de\u00b7rungs\u00b7be\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es will du sollst dich Nacht und Tag", "tokens": ["Es", "will", "du", "sollst", "dich", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VMFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "An seine linke Seite schmiegen. \u2014", "tokens": ["An", "sei\u00b7ne", "lin\u00b7ke", "Sei\u00b7te", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ach allzus\u00fc\u00dfes, sch\u00f6nes Pfand", "tokens": ["Ach", "all\u00b7zu\u00b7s\u00fc\u00b7\u00dfes", ",", "sch\u00f6\u00b7nes", "Pfand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ITJ", "PIS", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier k\u00f6nntest du nicht lange liegen,", "tokens": ["Hier", "k\u00f6nn\u00b7test", "du", "nicht", "lan\u00b7ge", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So h\u00e4tte dich die Gluth verbrannt,", "tokens": ["So", "h\u00e4t\u00b7te", "dich", "die", "Gluth", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die stets in diesem Herzen lodert;", "tokens": ["Die", "stets", "in", "die\u00b7sem", "Her\u00b7zen", "lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch w\u00fcrdest du nur gar zu oft", "tokens": ["Auch", "w\u00fcr\u00b7dest", "du", "nur", "gar", "zu", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PTKA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von diesem Munde hier gefodert,", "tokens": ["Von", "die\u00b7sem", "Mun\u00b7de", "hier", "ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der dich noch dann zu k\u00fcssen hofft,", "tokens": ["Der", "dich", "noch", "dann", "zu", "k\u00fcs\u00b7sen", "hofft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn er auf ewig sich soll schlie\u00dfen.", "tokens": ["Wenn", "er", "auf", "e\u00b7wig", "sich", "soll", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Nein, nein, du must nicht untergehn,", "tokens": ["Nein", ",", "nein", ",", "du", "must", "nicht", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ich will dich sehen und genie\u00dfen,", "tokens": ["Ich", "will", "dich", "se\u00b7hen", "und", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "So lange noch ein Wunsch im Herzen kann entstehn.", "tokens": ["So", "lan\u00b7ge", "noch", "ein", "Wunsch", "im", "Her\u00b7zen", "kann", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drey Tage soll dies Herz dich haben,", "tokens": ["Drey", "Ta\u00b7ge", "soll", "dies", "Herz", "dich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VMFIN", "PDS", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und nach drey Tagen liegst du dort,", "tokens": ["Und", "nach", "drey", "Ta\u00b7gen", "liegst", "du", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wo Milons Rose ward begraben:", "tokens": ["Wo", "Mi\u00b7lons", "Ro\u00b7se", "ward", "be\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Da soll mein allererstes Wort", "tokens": ["Da", "soll", "mein", "al\u00b7le\u00b7rers\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Des Morgens fr\u00fch beim Sonnengru\u00dfe", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", "beim", "Son\u00b7nen\u00b7gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die Frage seyn: wo ist mein Schatz?", "tokens": ["Die", "Fra\u00b7ge", "seyn", ":", "wo", "ist", "mein", "Schatz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$.", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und Abends sp\u00e4t nehm ich nach siebenfachem Kusse", "tokens": ["Und", "A\u00b7bends", "sp\u00e4t", "nehm", "ich", "nach", "sie\u00b7ben\u00b7fa\u00b7chem", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Von dir auf meinem Lager Platz. \u2014", "tokens": ["Von", "dir", "auf", "mei\u00b7nem", "La\u00b7ger", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}