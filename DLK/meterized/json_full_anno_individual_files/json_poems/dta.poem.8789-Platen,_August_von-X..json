{"dta.poem.8789": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast sie nicht verdammt in meinem Busen,", "tokens": ["Du", "hast", "sie", "nicht", "ver\u00b7dammt", "in", "mei\u00b7nem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weichlich ruh'n, zum Lobe dir, Ges\u00e4nge,", "tokens": ["Und", "weich\u00b7lich", "ruh'n", ",", "zum", "Lo\u00b7be", "dir", ",", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "APPRART", "NN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Kronen auf dem Sammt, in meinem Busen;", "tokens": ["Wie", "Kro\u00b7nen", "auf", "dem", "Sammt", ",", "in", "mei\u00b7nem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Dichtung Lanzen fass' ich mit einander,", "tokens": ["Der", "Dich\u00b7tung", "Lan\u00b7zen", "fass'", "ich", "mit", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und berge sie gesammt in meinem Busen;", "tokens": ["Und", "ber\u00b7ge", "sie", "ge\u00b7sammt", "in", "mei\u00b7nem", "Bu\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja, wie ein Fl\u00e4mmchen, flackert eine Rose,", "tokens": ["Ja", ",", "wie", "ein", "Fl\u00e4mm\u00b7chen", ",", "fla\u00b7ckert", "ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die noch aus Eden stammt, in meinem Busen.", "tokens": ["Die", "noch", "aus", "E\u00b7den", "stammt", ",", "in", "mei\u00b7nem", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}