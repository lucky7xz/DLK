{"dta.poem.10388": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf meiner Herzliebsten Aeugelein", "tokens": ["Auf", "mei\u00b7ner", "Herz\u00b7liebs\u00b7ten", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Mach' ich die sch\u00f6nsten Canzonen.", "tokens": ["Mach'", "ich", "die", "sch\u00f6ns\u00b7ten", "Can\u00b7zo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf meiner Herzliebsten M\u00fcndchen klein", "tokens": ["Auf", "mei\u00b7ner", "Herz\u00b7liebs\u00b7ten", "M\u00fcnd\u00b7chen", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE", "ADJD"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Mach' ich die besten Terzinen.", "tokens": ["Mach'", "ich", "die", "bes\u00b7ten", "Ter\u00b7zi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf meiner Herzliebsten W\u00e4ngelein", "tokens": ["Auf", "mei\u00b7ner", "Herz\u00b7liebs\u00b7ten", "W\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mach' ich die herrlichsten Stanzen.", "tokens": ["Mach'", "ich", "die", "herr\u00b7lichs\u00b7ten", "Stan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und wenn meine Liebste ein Herzchen h\u00e4tt',", "tokens": ["Und", "wenn", "mei\u00b7ne", "Liebs\u00b7te", "ein", "Herz\u00b7chen", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So wollt' ich drauf machen ein h\u00fcbsches Sonett.", "tokens": ["So", "wollt'", "ich", "drauf", "ma\u00b7chen", "ein", "h\u00fcb\u00b7sches", "So\u00b7nett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}