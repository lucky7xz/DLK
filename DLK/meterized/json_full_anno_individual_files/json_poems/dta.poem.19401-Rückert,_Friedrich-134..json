{"dta.poem.19401": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "134.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird gesagt, es sei des b\u00f6sen Herzens Zeichen,", "tokens": ["Es", "wird", "ge\u00b7sagt", ",", "es", "sei", "des", "b\u00f6\u00b7sen", "Her\u00b7zens", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die Menschen liebst mit Thieren zu vergleichen.", "tokens": ["Wenn", "du", "die", "Men\u00b7schen", "liebst", "mit", "Thie\u00b7ren", "zu", "ver\u00b7glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch ist es so, wenn du mit Flei\u00df herunterziehst", "tokens": ["Auch", "ist", "es", "so", ",", "wenn", "du", "mit", "Flei\u00df", "her\u00b7un\u00b7ter\u00b7ziehst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen, und in ihm des Thieres Z\u00fcge siehst.", "tokens": ["Den", "Men\u00b7schen", ",", "und", "in", "ihm", "des", "Thie\u00b7res", "Z\u00fc\u00b7ge", "siehst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch anders ist es, ja es ist kein andrer Rath,", "tokens": ["Doch", "an\u00b7ders", "ist", "es", ",", "ja", "es", "ist", "kein", "an\u00b7drer", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "ADV", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo wirklich in Gestalt des Thiers der Mensch auftrat,", "tokens": ["Wo", "wirk\u00b7lich", "in", "Ge\u00b7stalt", "des", "Thiers", "der", "Mensch", "auf\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es sei der Einzelne, es sei ein ganz Geschlecht,", "tokens": ["Es", "sei", "der", "Ein\u00b7zel\u00b7ne", ",", "es", "sei", "ein", "ganz", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "PPER", "VAFIN", "ART", "ADV", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "An dem verloren scheint der Menschheit ew'ges Recht;", "tokens": ["An", "dem", "ver\u00b7lo\u00b7ren", "scheint", "der", "Menschheit", "ew'\u00b7ges", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVPP", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Da ist kein andrer Rath, als, liebes Herz, zu sagen:", "tokens": ["Da", "ist", "kein", "an\u00b7drer", "Rath", ",", "als", ",", "lie\u00b7bes", "Herz", ",", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "$,", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier will der Menschengeist einmal Thiermaske tragen.", "tokens": ["Hier", "will", "der", "Men\u00b7schen\u00b7geist", "ein\u00b7mal", "Thier\u00b7mas\u00b7ke", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Unw\u00fcrdig ist das Spiel, da\u00df er die Maske nahm;", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "ist", "das", "Spiel", ",", "da\u00df", "er", "die", "Mas\u00b7ke", "nahm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn er sich besinnt, legt er sie ab mit Scham.", "tokens": ["Und", "wenn", "er", "sich", "be\u00b7sinnt", ",", "legt", "er", "sie", "ab", "mit", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Der Maske Anblick schon ist zur Gen\u00fcge h\u00e4\u00dflich;", "tokens": ["Der", "Mas\u00b7ke", "An\u00b7blick", "schon", "ist", "zur", "Ge\u00b7n\u00fc\u00b7ge", "h\u00e4\u00df\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als wirkliches Gesicht betrachtet, w\u00e4r' es gr\u00e4\u00dflich.", "tokens": ["Als", "wirk\u00b7li\u00b7ches", "Ge\u00b7sicht", "be\u00b7trach\u00b7tet", ",", "w\u00e4r'", "es", "gr\u00e4\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVPP", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}