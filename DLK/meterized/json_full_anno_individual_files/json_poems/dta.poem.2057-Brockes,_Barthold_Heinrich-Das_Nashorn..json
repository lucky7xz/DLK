{"dta.poem.2057": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Nashorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df er der erlegten K\u00f6rper Haut und Fleisch bis auf", "tokens": ["Da\u00df", "er", "der", "er\u00b7leg\u00b7ten", "K\u00f6r\u00b7per", "Haut", "und", "Fleisch", "bis", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "KON", "NN", "APPR", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "das Bein", "tokens": ["das", "Bein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Abzulecken f\u00e4hig ist. Sonsten sollen ihn allein", "tokens": ["Ab\u00b7zu\u00b7le\u00b7cken", "f\u00e4\u00b7hig", "ist", ".", "Sons\u00b7ten", "sol\u00b7len", "ihn", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VAFIN", "$.", "NN", "VMFIN", "PPER", "ADV"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Scharf und harte Kr\u00e4uter n\u00e4hren. Sonst soll es vertr\u00e4g-", "tokens": ["Scharf", "und", "har\u00b7te", "Kr\u00e4u\u00b7ter", "n\u00e4h\u00b7ren", ".", "Sonst", "soll", "es", "ver\u00b7tr\u00e4g"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJA", "NN", "VVINF", "$.", "ADV", "VMFIN", "PPER", "TRUNC"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "lich, g\u00fctig,", "tokens": ["lich", ",", "g\u00fc\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Wo man es nicht reizet, seyn: nur, verletzet, wird es", "tokens": ["Wo", "man", "es", "nicht", "rei\u00b7zet", ",", "seyn", ":", "nur", ",", "ver\u00b7let\u00b7zet", ",", "wird", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "PTKNEG", "VVFIN", "$,", "VAINF", "$.", "ADV", "$,", "VVFIN", "$,", "VAFIN", "PPER"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "w\u00fctig,", "tokens": ["w\u00fc\u00b7tig", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Da es denn mit seinem Zorn, Grimm und Wut so weit", "tokens": ["Da", "es", "denn", "mit", "sei\u00b7nem", "Zorn", ",", "Grimm", "und", "Wut", "so", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "NE", "KON", "NN", "ADV", "ADJD"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.9": {"text": "soll gehn,", "tokens": ["soll", "gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Da\u00df im Wege stehnde B\u00e4ume seinem Grimm nicht wi-", "tokens": ["Da\u00df", "im", "We\u00b7ge", "stehn\u00b7de", "B\u00e4u\u00b7me", "sei\u00b7nem", "Grimm", "nicht", "wi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN", "PPOSAT", "NE", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "derstehn.", "tokens": ["der\u00b7stehn", "."], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Da der Mauren Volk sein Fleisch, wir das Horn, ge-", "tokens": ["Da", "der", "Mau\u00b7ren", "Volk", "sein", "Fleisch", ",", "wir", "das", "Horn", ",", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "NN", "PPOSAT", "NN", "$,", "PPER", "ART", "NN", "$,", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "brauchen k\u00f6nnen,", "tokens": ["brau\u00b7chen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VMINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Werden wir auch dieses Thier nicht mit Recht unn\u00fctzlich", "tokens": ["Wer\u00b7den", "wir", "auch", "die\u00b7ses", "Thier", "nicht", "mit", "Recht", "un\u00b7n\u00fctz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PDAT", "NN", "PTKNEG", "APPR", "NN", "ADJD"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "nennen.", "tokens": ["nen\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.16": {"text": "Aus der Glieder Ebenmaa\u00dfe stralet denn bey diesem", "tokens": ["Aus", "der", "Glie\u00b7der", "E\u00b7ben\u00b7maa\u00b7\u00dfe", "stra\u00b7let", "denn", "bey", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "ADV", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Thier,", "tokens": ["Thier", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Nebst dem Nutzen, auch des Sch\u00f6pfers Weisheit, Lieb", "tokens": ["Nebst", "dem", "Nut\u00b7zen", ",", "auch", "des", "Sch\u00f6p\u00b7fers", "Weis\u00b7heit", ",", "Lieb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "und Macht herf\u00fcr.", "tokens": ["und", "Macht", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}