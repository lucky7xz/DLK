{"textgrid.poem.32524": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "V. Die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Stern, durch den es bei uns tagt -", "tokens": ["Der", "Stern", ",", "durch", "den", "es", "bei", "uns", "tagt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach! Dichter, lern', wie unser einer sprechen!", "tokens": ["\u00bb", "ach", "!", "Dich\u00b7ter", ",", "lern'", ",", "wie", "un\u00b7ser", "ei\u00b7ner", "spre\u00b7chen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "NN", "$,", "VVFIN", "$,", "PWAV", "PPOSAT", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df man, wenn du erz\u00e4hlst,", "tokens": ["Mu\u00df", "man", ",", "wenn", "du", "er\u00b7z\u00e4hlst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und uns mit albern Fabeln qu\u00e4lst,", "tokens": ["Und", "uns", "mit", "al\u00b7bern", "Fa\u00b7beln", "qu\u00e4lst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich denkend noch den Kopf zerbrechen?\u00ab", "tokens": ["Sich", "den\u00b7kend", "noch", "den", "Kopf", "zer\u00b7bre\u00b7chen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ADJD", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nun gut! die Sonne ward gefragt:", "tokens": ["Nun", "gut", "!", "die", "Son\u00b7ne", "ward", "ge\u00b7fragt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob sie es nicht verdr\u00f6sse,", "tokens": ["Ob", "sie", "es", "nicht", "ver\u00b7dr\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ihre unerme\u00dfne Gr\u00f6\u00dfe", "tokens": ["Da\u00df", "ih\u00b7re", "un\u00b7er\u00b7me\u00df\u00b7ne", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die durch den Schein betrogne Welt", "tokens": ["Die", "durch", "den", "Schein", "be\u00b7trog\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im Durchschnitt' gr\u00f6\u00dfer kaum, als eine Spanne, h\u00e4lt?", "tokens": ["Im", "Durch\u00b7schnitt'", "gr\u00f6\u00b7\u00dfer", "kaum", ",", "als", "ei\u00b7ne", "Span\u00b7ne", ",", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mich, spricht sie, sollte dieses kr\u00e4nken?", "tokens": ["Mich", ",", "spricht", "sie", "