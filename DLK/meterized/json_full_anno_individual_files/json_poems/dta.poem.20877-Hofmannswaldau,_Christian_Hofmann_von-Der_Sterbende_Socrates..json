{"dta.poem.20877": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist es da\u00df du nicht kanst fassen/", "tokens": ["Was", "ist", "es", "da\u00df", "du", "nicht", "kanst", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was dein ungeschickter Geist", "tokens": ["Und", "was", "dein", "un\u00b7ge\u00b7schick\u00b7ter", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Unrecht unverst\u00e4ndlich heist/", "tokens": ["Mit", "Un\u00b7recht", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "heist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht kan ungetadelt lassen.", "tokens": ["Und", "nicht", "kan", "un\u00b7ge\u00b7ta\u00b7delt", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}