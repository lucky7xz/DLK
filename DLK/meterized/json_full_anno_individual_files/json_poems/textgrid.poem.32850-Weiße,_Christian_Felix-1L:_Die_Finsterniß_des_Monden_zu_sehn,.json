{"textgrid.poem.32850": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Finsterni\u00df des Monden zu sehn,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Finsterni\u00df des Monden zu sehn,", "tokens": ["Die", "Fins\u00b7ter\u00b7ni\u00df", "des", "Mon\u00b7den", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ladt ich j\u00fcngst Freund und Br\u00fcder ein:", "tokens": ["Ladt", "ich", "j\u00fcngst", "Freund", "und", "Br\u00fc\u00b7der", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df kann, sprach ich vortrefflich geschehn", "tokens": ["Die\u00df", "kann", ",", "sprach", "ich", "vor\u00b7treff\u00b7lich", "ge\u00b7schehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "$,", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In einem Glase blanken Wein.", "tokens": ["In", "ei\u00b7nem", "Gla\u00b7se", "blan\u00b7ken", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie kamen und ich schenkte frisch ein:", "tokens": ["Sie", "ka\u00b7men", "und", "ich", "schenk\u00b7te", "frisch", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "ADJD", "PTKVZ",