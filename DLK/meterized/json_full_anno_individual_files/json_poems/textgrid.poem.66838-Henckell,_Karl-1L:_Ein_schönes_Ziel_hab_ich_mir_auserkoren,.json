{"textgrid.poem.66838": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein sch\u00f6nes Ziel hab ich mir auserkoren,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sch\u00f6nes Ziel hab ich mir auserkoren,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Ziel", "hab", "ich", "mir", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm bleibt mein letzter Hauch von Kraft geweiht.", "tokens": ["Ihm", "bleibt", "mein", "letz\u00b7ter", "Hauch", "von", "Kraft", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es harrt das Volk und lauscht mit durstigen Ohren", "tokens": ["Es", "harrt", "das", "Volk", "und", "lauscht", "mit", "durs\u00b7ti\u00b7gen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Auf Lieder, drin die neue Frucht gedeiht.", "tokens": ["Auf", "Lie\u00b7der", ",", "drin", "die", "neu\u00b7e", "Frucht", "ge\u00b7deiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Spielmann sein will ich an jenen Toren,", "tokens": ["Ein", "Spiel\u00b7mann", "sein", "will", "ich", "an", "je\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "VMFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die donnernd schleudern ihre Fl\u00fcgel weit,", "tokens": ["Die", "don\u00b7nernd", "schleu\u00b7dern", "ih\u00b7re", "Fl\u00fc\u00b7gel", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Land der Sehnsucht aufzutun den Augen,", "tokens": ["Das", "Land", "der", "Sehn\u00b7sucht", "auf\u00b7zu\u00b7tun", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die sich der Sonne fromm entgegensaugen.", "tokens": ["Die", "sich", "der", "Son\u00b7ne", "fromm", "ent\u00b7ge\u00b7gen\u00b7sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O meine Br\u00fcder, durch des Wahnsinns W\u00fcsten", "tokens": ["O", "mei\u00b7ne", "Br\u00fc\u00b7der", ",", "durch", "des", "Wahn\u00b7sinns", "W\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Bin ich gewandert mit versengtem Haupt,", "tokens": ["Bin", "ich", "ge\u00b7wan\u00b7dert", "mit", "ver\u00b7seng\u00b7tem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo die Gerippe der Verzweiflung gr\u00fc\u00dften", "tokens": ["Wo", "die", "Ge\u00b7rip\u00b7pe", "der", "Ver\u00b7zwei\u00b7flung", "gr\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Gru\u00df des Grausens, wo der Pestwind schnaubt.", "tokens": ["Den", "Gru\u00df", "des", "Grau\u00b7sens", ",", "wo", "der", "Pest\u00b7wind", "schnaubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo trostlos schleppend meine Schritte b\u00fc\u00dften", "tokens": ["Wo", "trost\u00b7los", "schlep\u00b7pend", "mei\u00b7ne", "Schrit\u00b7te", "b\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dies Qualjahrhundert, tief mit Schmach bestaubt,", "tokens": ["Dies", "Qual\u00b7jahr\u00b7hun\u00b7dert", ",", "tief", "mit", "Schmach", "be\u00b7staubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo mich die giftigen Insekten stachen,", "tokens": ["Wo", "mich", "die", "gif\u00b7ti\u00b7gen", "In\u00b7sek\u00b7ten", "sta\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis meine Glieder zuckend niederbrachen.", "tokens": ["Bis", "mei\u00b7ne", "Glie\u00b7der", "zu\u00b7ckend", "nie\u00b7der\u00b7bra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vorw\u00e4rts empor! Und kann ich hoch mich richten,", "tokens": ["Vor\u00b7w\u00e4rts", "em\u00b7por", "!", "Und", "kann", "ich", "hoch", "mich", "rich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "KON", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Genossen meines Elends, euch allein", "tokens": ["Ge\u00b7nos\u00b7sen", "mei\u00b7nes", "E\u00b7lends", ",", "euch", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll all mein F\u00fchlen, Wollen, Denken, Dichten,", "tokens": ["Soll", "all", "mein", "F\u00fch\u00b7len", ",", "Wol\u00b7len", ",", "Den\u00b7ken", ",", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIAT", "PPOSAT", "NN", "$,", "VVFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein letztes K\u00f6nnen froh versch\u00fcttet sein.", "tokens": ["Mein", "letz\u00b7tes", "K\u00f6n\u00b7nen", "froh", "ver\u00b7sch\u00fct\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was durch die Seele schwillt von freudelichten", "tokens": ["Was", "durch", "die", "See\u00b7le", "schwillt", "von", "freu\u00b7de\u00b7lich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebilden aus der Zukunft Bl\u00fctenhain,", "tokens": ["Ge\u00b7bil\u00b7den", "aus", "der", "Zu\u00b7kunft", "Bl\u00fc\u00b7ten\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was mild mein Wehe lindert, soll zu Liedern", "tokens": ["Was", "mild", "mein", "We\u00b7he", "lin\u00b7dert", ",", "soll", "zu", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verkl\u00e4rten Trostes schluchzend sich befiedern.", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "Tros\u00b7tes", "schluch\u00b7zend", "sich", "be\u00b7fie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn in der Tiefe z\u00fcndet meine Flamme,", "tokens": ["Denn", "in", "der", "Tie\u00b7fe", "z\u00fcn\u00b7det", "mei\u00b7ne", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die purpurrote Flamme weitumher.", "tokens": ["Die", "pur\u00b7pur\u00b7ro\u00b7te", "Flam\u00b7me", "wei\u00b7tum\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin ein Reis am neuen Menschheitsstamme,", "tokens": ["Ich", "bin", "ein", "Reis", "am", "neu\u00b7en", "Menschheits\u00b7stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Wellenschlag im neuen Menschheitsmeer.", "tokens": ["Ein", "Wel\u00b7len\u00b7schlag", "im", "neu\u00b7en", "Menschheits\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zukunft w\u00e4lzt auf blutigem Wogenkamme", "tokens": ["Die", "Zu\u00b7kunft", "w\u00e4lzt", "auf", "blu\u00b7ti\u00b7gem", "Wo\u00b7gen\u00b7kam\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Des Elends Todesnachen trauerschwer,", "tokens": ["Des", "E\u00b7lends", "To\u00b7des\u00b7na\u00b7chen", "trau\u00b7er\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Durch Grauen und Jammer h\u00f6r' ich Stimmen rufen", "tokens": ["Durch", "Grau\u00b7en", "und", "Jam\u00b7mer", "h\u00f6r'", "ich", "Stim\u00b7men", "ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Zum Fels des Heils, hoch \u00fcber Feuerstufen.", "tokens": ["Zum", "Fels", "des", "Heils", ",", "hoch", "\u00fc\u00b7ber", "Feu\u00b7er\u00b7stu\u00b7fen", "."], "token_info": ["word", "wo