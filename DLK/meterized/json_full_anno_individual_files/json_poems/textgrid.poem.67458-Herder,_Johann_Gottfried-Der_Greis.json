{"textgrid.poem.67458": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Greis", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbach der Jugend! sie folgt so selten dem weiseren Alter!\u00ab", "tokens": ["\u00bb", "ach", "der", "Ju\u00b7gend", "!", "sie", "folgt", "so", "sel\u00b7ten", "dem", "wei\u00b7se\u00b7ren", "Al\u00b7ter", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Also klaget der Greis, seufzet und warnet umsonst.", "tokens": ["Al\u00b7so", "kla\u00b7get", "der", "Greis", ",", "seuf\u00b7zet", "und", "war\u00b7net", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "J\u00fcnglinge halten ihn, er h\u00e4lt die J\u00fcnglinge Kinder;", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "hal\u00b7ten", "ihn", ",", "er", "h\u00e4lt", "die", "J\u00fcng\u00b7lin\u00b7ge", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Selten macht er sie klug, oft er mit ihnen ein Kind.", "tokens": ["Sel\u00b7ten", "macht", "er", "sie", "klug", ",", "oft", "er", "mit", "ih\u00b7nen", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word",