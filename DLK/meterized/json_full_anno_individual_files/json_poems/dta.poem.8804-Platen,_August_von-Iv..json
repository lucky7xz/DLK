{"dta.poem.8804": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Doch Jugend, leider! bl\u00fcht nicht allzulange!", "tokens": ["Doch", "Ju\u00b7gend", ",", "lei\u00b7der", "!", "bl\u00fcht", "nicht", "all\u00b7zu\u00b7lan\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "$.", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein wolkenfreyes Angesicht verkl\u00e4ret", "tokens": ["Dein", "wol\u00b7ken\u00b7fre\u00b7yes", "An\u00b7ge\u00b7sicht", "ver\u00b7kl\u00e4\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein leichter Sinn, an dem ich z\u00e4rtlich hange;", "tokens": ["Ein", "leich\u00b7ter", "Sinn", ",", "an", "dem", "ich", "z\u00e4rt\u00b7lich", "han\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie freundlich lacht das Aug' aus blonder Wimper,", "tokens": ["Wie", "freund\u00b7lich", "lacht", "das", "Aug'", "aus", "blon\u00b7der", "Wim\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie schm\u00fcckt der Bart so sch\u00f6n die Tulpenwange!", "tokens": ["Wie", "schm\u00fcckt", "der", "Bart", "so", "sch\u00f6n", "die", "Tul\u00b7pen\u00b7wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Becher f\u00fclle mir! Der Wein beschwichtigt", "tokens": ["Den", "Be\u00b7cher", "f\u00fcl\u00b7le", "mir", "!", "Der", "Wein", "be\u00b7schwich\u00b7tigt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die kranke Brust mit ihrem wilden Drange:", "tokens": ["Die", "kran\u00b7ke", "Brust", "mit", "ih\u00b7rem", "wil\u00b7den", "Dran\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du zwingst zu lieben dich die Welt, wie ", "tokens": ["Du", "zwingst", "zu", "lie\u00b7ben", "dich", "die", "Welt", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "ADV", "PTKZU", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Euch Beyde d'rum verk\u00fcnd' ich im Gesange.", "tokens": ["Euch", "Bey\u00b7de", "d'\u00b7rum", "ver\u00b7k\u00fcnd'", "ich", "im", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PAV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}