{"textgrid.poem.44086": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00f6re, gro\u00dfer Gott, den Donner deiner Stimme;", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6re, gro\u00dfer Gott, den Donner deiner Stimme;", "tokens": ["Ich", "h\u00f6\u00b7re", ",", "gro\u00b7\u00dfer", "Gott", ",", "den", "Don\u00b7ner", "dei\u00b7ner", "Stim\u00b7me", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00f6rest auch nicht mehr, ich soll von deinem Grimme", "tokens": ["Du", "h\u00f6\u00b7rest", "auch", "nicht", "mehr", ",", "ich", "soll", "von", "dei\u00b7nem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Gr\u00f6\u00dfe meiner Schuld ein ewig Opfer seyn,", "tokens": ["Aus", "Gr\u00f6\u00b7\u00dfe", "mei\u00b7ner", "Schuld", "ein", "e\u00b7wig", "Op\u00b7fer", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "ART", "ADJD", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich soll, ich mu\u00df, ich will und gebe mich darein.", "tokens": ["Ich", "soll", ",", "ich", "mu\u00df", ",", "ich", "will", "und", "ge\u00b7be", "mich", "da\u00b7rein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "KON", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich troze deinen Zorn, ich fleh nicht mehr um Gnade,", "tokens": ["Ich", "tro\u00b7ze", "dei\u00b7nen", "Zorn", ",", "ich", "fleh", "nicht", "mehr", "um", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich will nicht, da\u00df dein Herz mich dieser Straf entlade.", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "dein", "Herz", "mich", "die\u00b7ser", "Straf", "ent\u00b7la\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du bist kein Vater mehr, als Richter bitt ich dich:", "tokens": ["Du", "bist", "kein", "Va\u00b7ter", "mehr", ",", "als", "Rich\u00b7ter", "bitt", "ich", "dich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,", "KOUS", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vergi\u00df vorher dein Kind, hernach versto\u00dfe mich.", "tokens": ["Ver\u00b7gi\u00df", "vor\u00b7her", "dein", "Kind", ",", "her\u00b7nach", "ver\u00b7sto\u00b7\u00dfe", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "NN", "$,", "