{"textgrid.poem.35152": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Das Waldes Seele", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im Wald. Die B\u00e4ume alle schliefen;", "tokens": ["Es", "war", "im", "Wald", ".", "Die", "B\u00e4u\u00b7me", "al\u00b7le", "schlie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mond belauschte l\u00e4chelnd ihren Traum.", "tokens": ["Der", "Mond", "be\u00b7lauschte", "l\u00e4\u00b7chelnd", "ih\u00b7ren", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Schatten lagen ruhig in den Tiefen;", "tokens": ["Die", "Schat\u00b7ten", "la\u00b7gen", "ru\u00b7hig", "in", "den", "Tie\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Welle k\u00fc\u00dfte still des Weihers Saum.", "tokens": ["Die", "Wel\u00b7le", "k\u00fc\u00df\u00b7te", "still", "des", "Wei\u00b7hers", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da kam ein linder, milder Hauch gezogen,", "tokens": ["Da", "kam", "ein", "lin\u00b7der", ",", "mil\u00b7der", "Hauch", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Tr\u00e4umenden gew\u00fcrzger Athemzug,", "tokens": ["Des", "Tr\u00e4u\u00b7men\u00b7den", "ge\u00b7w\u00fcrz\u00b7ger", "A\u00b7them\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der in des Maienduftes zarten Wogen", "tokens": ["Der", "in", "des", "Mai\u00b7en\u00b7duf\u00b7tes", "zar\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Waldes Seele auf zum Himmel trug.", "tokens": ["Des", "Wal\u00b7des", "See\u00b7le", "auf", "zum", "Him\u00b7mel", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort schwebte sie zur ewgen Gnadenquelle,", "tokens": ["Dort", "schweb\u00b7te", "sie", "zur", "ew\u00b7gen", "Gna\u00b7den\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor der die Bitte um das Leben kniet,", "tokens": ["Vor", "der", "die", "Bit\u00b7te", "um", "das", "Le\u00b7ben", "kniet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie vom V\u00f6glein an der Waldkapelle", "tokens": ["Und", "wie", "vom", "V\u00f6\u00b7glein", "an", "der", "Wald\u00b7ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erklang ihr sanftes, frommes Klagelied:", "tokens": ["Er\u00b7klang", "ihr", "sanf\u00b7tes", ",", "from\u00b7mes", "Kla\u00b7ge\u00b7lied", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbes preisen dich des Firmamentes Heere,", "tokens": ["\u00bb", "es", "prei\u00b7sen", "dich", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "Hee\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deren Licht dein Ruhm herniederschallt.", "tokens": ["Auf", "de\u00b7ren", "Licht", "dein", "Ruhm", "her\u00b7nie\u00b7der\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ihm erf\u00fcllt sind alle Weltenmeere;", "tokens": ["Von", "ihm", "er\u00b7f\u00fcllt", "sind", "al\u00b7le", "Wel\u00b7ten\u00b7mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Thau und Regen trinkt ihn auch der Wald.", "tokens": ["Im", "Thau", "und", "Re\u00b7gen", "trinkt", "ihn", "auch", "der", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Von da soll er aus tausend Quellen flie\u00dfen,", "tokens": ["Von", "da", "soll", "er", "aus", "tau\u00b7send", "Quel\u00b7len", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "APPR", "CARD", "NN", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dem Erdenland zum Heil und Segen sein,", "tokens": ["Dem", "Er\u00b7den\u00b7land", "zum", "Heil", "und", "Se\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In alle Fl\u00fcsse, Str\u00f6me sich ergie\u00dfen", "tokens": ["In", "al\u00b7le", "Fl\u00fcs\u00b7se", ",", "Str\u00f6\u00b7me", "sich", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dich verk\u00fcnden, Vater, dich allein.", "tokens": ["Und", "dich", "ver\u00b7k\u00fcn\u00b7den", ",", "Va\u00b7ter", ",", "dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "$,", "NN", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch schau hinab! Die Menschen, die du segnest,", "tokens": ["Doch", "schau", "hin\u00b7ab", "!", "Die", "Men\u00b7schen", ",", "die", "du", "seg\u00b7nest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begreifen deine Gottesweisheit nicht.", "tokens": ["Be\u00b7grei\u00b7fen", "dei\u00b7ne", "Got\u00b7tes\u00b7weis\u00b7heit", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe, die du ihnen niederregnest,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "du", "ih\u00b7nen", "nie\u00b7der\u00b7reg\u00b7nest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird ihrem Unverstand zum Strafgericht.", "tokens": ["Wird", "ih\u00b7rem", "Un\u00b7ver\u00b7stand", "zum", "Straf\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sie haben weder dich, o Herr, verstanden,", "tokens": ["Sie", "ha\u00b7ben", "we\u00b7der", "dich", ",", "o", "Herr", ",", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach deines freundlichsten Gesetzes Sinn;", "tokens": ["Nach", "dei\u00b7nes", "freund\u00b7lichs\u00b7ten", "Ge\u00b7set\u00b7zes", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum handeln sie, als sei ich nicht vorhanden,", "tokens": ["Drum", "han\u00b7deln", "sie", ",", "als", "sei", "ich", "nicht", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Obgleich ich ihnen unentbehrlich bin.", "tokens": ["Ob\u00b7gleich", "ich", "ih\u00b7nen", "un\u00b7ent\u00b7behr\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "La\u00df mich nicht sterben, la\u00df mich nicht verschmachten,", "tokens": ["La\u00df", "mich", "nicht", "ster\u00b7ben", ",", "la\u00df", "mich", "nicht", "ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sonst ists auch um ihr eignes Heil geschehn.", "tokens": ["Sonst", "ists", "auch", "um", "ihr", "eig\u00b7nes", "Heil", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lehr sie, den Wald mit Liebe zu betrachten,", "tokens": ["Lehr", "sie", ",", "den", "Wald", "mit", "Lie\u00b7be", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Damit sie endlich seine Seele sehn!\u00ab", "tokens": ["Da\u00b7mit", "sie", "end\u00b7lich", "sei\u00b7ne", "See\u00b7le", "sehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie schwieg und senkte wartend ihren Schleier;", "tokens": ["Sie", "schwieg", "und", "senk\u00b7te", "war\u00b7tend", "ih\u00b7ren", "Schlei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Traum entfloh; es war die Nacht vorbei.", "tokens": ["Der", "Traum", "ent\u00b7floh", ";", "es", "war", "die", "Nacht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Erde lag in stiller Morgenfeier;", "tokens": ["Die", "Er\u00b7de", "lag", "in", "stil\u00b7ler", "Mor\u00b7gen\u00b7fei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Gl\u00f6cklein k\u00fcndete, da\u00df Sabbath sei.", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "k\u00fcn\u00b7de\u00b7te", ",", "da\u00df", "Sab\u00b7ba\u00b7th", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Der Wald erwachte, und der V\u00f6glein Lieder", "tokens": ["Der", "Wald", "er\u00b7wach\u00b7te", ",", "und", "der", "V\u00f6\u00b7glein", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erklangen jubelnd \u00fcber Berg und Thal.", "tokens": ["Er\u00b7klan\u00b7gen", "ju\u00b7belnd", "\u00fc\u00b7ber", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Seele kehrte aus dem Himmel wieder,", "tokens": ["Die", "See\u00b7le", "kehr\u00b7te", "aus", "dem", "Him\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Getragen von dem ersten Sonnenstrahl.", "tokens": ["Ge\u00b7tra\u00b7gen", "von", "dem", "ers\u00b7ten", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sie tauchte in des Weihers klare Welle", "tokens": ["Sie", "tauch\u00b7te", "in", "des", "Wei\u00b7hers", "kla\u00b7re", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stieg sodann ans thauesfrische Land,", "tokens": ["Und", "stieg", "so\u00b7dann", "ans", "thau\u00b7es\u00b7fri\u00b7sche", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Empfangen von dem Kehlchen der Kapelle,", "tokens": ["Emp\u00b7fan\u00b7gen", "von", "dem", "Kehl\u00b7chen", "der", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei dem sie nun des Vaters Antwort fand:", "tokens": ["Bei", "dem", "sie", "nun", "des", "Va\u00b7ters", "Ant\u00b7wort", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "\u00bbich lie\u00df f\u00fcr dich das Sabathgl\u00f6cklein l\u00e4uten:", "tokens": ["\u00bb", "ich", "lie\u00df", "f\u00fcr", "dich", "das", "Sa\u00b7ba\u00b7th\u00b7gl\u00f6c\u00b7klein", "l\u00e4u\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es l\u00e4utete den Waldesfrieden ein.", "tokens": ["Es", "l\u00e4u\u00b7te\u00b7te", "den", "Wal\u00b7des\u00b7frie\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das hat f\u00fcr dich Erh\u00f6rung zu bedeuten;", "tokens": ["Das", "hat", "f\u00fcr", "dich", "Er\u00b7h\u00f6\u00b7rung", "zu", "be\u00b7deu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sollst fortan dem Menschen heilig sein.", "tokens": ["Du", "sollst", "for\u00b7tan", "dem", "Men\u00b7schen", "hei\u00b7lig", "sein", "."], "toke