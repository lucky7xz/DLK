{"dta.poem.8130": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gerichtsdonner, ach zu furchtbar t\u00f6nest du", "tokens": ["Ge\u00b7richts\u00b7don\u00b7ner", ",", "ach", "zu", "furcht\u00b7bar", "t\u00f6\u00b7nest", "du"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PTKA", "ADJD", "VVFIN", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In die Grabmale! L\u00e4ngrer, ewiger Schlaf", "tokens": ["In", "die", "Grab\u00b7ma\u00b7le", "!", "L\u00e4ng\u00b7rer", ",", "e\u00b7wi\u00b7ger", "Schlaf"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "$,", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ist ihr Flehn; aber sie kommen aus der Nacht", "tokens": ["Ist", "ihr", "Flehn", ";", "a\u00b7ber", "sie", "kom\u00b7men", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF", "$.", "KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und wehklagen: O falle, Gebirg, deck\u2019 uns!", "tokens": ["Und", "weh\u00b7kla\u00b7gen", ":", "O", "fal\u00b7le", ",", "Ge\u00b7birg", ",", "deck'", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NE", "VVFIN", "$,", "NN", "$,", "PRELS", "PPER", "$."], "meter": "-++--+--+-+", "measure": "iambic.penta.relaxed"}}}}}