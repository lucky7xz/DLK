{"textgrid.poem.56308": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettler und sein Hund", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Taler erlegen f\u00fcr meinen Hund!", "tokens": ["Drei", "Ta\u00b7ler", "er\u00b7le\u00b7gen", "f\u00fcr", "mei\u00b7nen", "Hund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schlage das Wetter mich gleich in den Grund!", "tokens": ["So", "schla\u00b7ge", "das", "Wet\u00b7ter", "mich", "gleich", "in", "den", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was denken die Herrn von der Polizei?", "tokens": ["Was", "den\u00b7ken", "die", "Herrn", "von", "der", "Po\u00b7li\u00b7zei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was soll nun wieder die Schinderei?", "tokens": ["Was", "soll", "nun", "wie\u00b7der", "die", "Schin\u00b7de\u00b7rei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich bin ein alter, ein kranker Mann,", "tokens": ["Ich", "bin", "ein", "al\u00b7ter", ",", "ein", "kran\u00b7ker", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der keinen Groschen verdienen kann;", "tokens": ["Der", "kei\u00b7nen", "Gro\u00b7schen", "ver\u00b7die\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich habe nicht Geld, ich habe nicht Brot,", "tokens": ["Ich", "ha\u00b7be", "nicht", "Geld", ",", "ich", "ha\u00b7be", "nicht", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich lebe ja nur von Hunger und Not.", "tokens": ["Ich", "le\u00b7be", "ja", "nur", "von", "Hun\u00b7ger", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wann ich erkrankt, und wann ich verarmt,", "tokens": ["Und", "wann", "ich", "er\u00b7krankt", ",", "und", "wann", "ich", "ver\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$,", "KON", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer hat sich da noch meiner erbarmt?", "tokens": ["Wer", "hat", "sich", "da", "noch", "mei\u00b7ner", "er\u00b7barmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wer hat, wann ich auf Gottes Welt", "tokens": ["Wer", "hat", ",", "wann", "ich", "auf", "Got\u00b7tes", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein mich fand, zu mir sich gesellt?", "tokens": ["Al\u00b7lein", "mich", "fand", ",", "zu", "mir", "sich", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer hat mich geliebt, wann ich mich geh\u00e4rmt?", "tokens": ["Wer", "hat", "mich", "ge\u00b7liebt", ",", "wann", "ich", "mich", "ge\u00b7h\u00e4rmt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wer, wann ich fror, hat mich gew\u00e4rmt?", "tokens": ["Wer", ",", "wann", "ich", "fror", ",", "hat", "mich", "ge\u00b7w\u00e4rmt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat mit mir, wann ich hungrig gemurrt,", "tokens": ["Wer", "hat", "mit", "mir", ",", "wann", "ich", "hung\u00b7rig", "ge\u00b7murrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Getrost gehungert und nicht geknurrt?", "tokens": ["Ge\u00b7trost", "ge\u00b7hun\u00b7gert", "und", "nicht", "ge\u00b7knurrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es geht zur Neige mit uns zwein,", "tokens": ["Es", "geht", "zur", "Nei\u00b7ge", "mit", "uns", "zwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mu\u00df, mein Tier, geschieden sein;", "tokens": ["Es", "mu\u00df", ",", "mein", "Tier", ",", "ge\u00b7schie\u00b7den", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist, wie ich, nun alt und krank,", "tokens": ["Du", "bist", ",", "wie", "ich", ",", "nun", "alt", "und", "krank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich soll dich ers\u00e4ufen, das ist der Dank!", "tokens": ["Ich", "soll", "dich", "er\u00b7s\u00e4u\u00b7fen", ",", "das", "ist", "der", "Dank", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Das ist der Dank, das ist der Lohn!", "tokens": ["Das", "ist", "der", "Dank", ",", "das", "ist", "der", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir geht's, wie manchem Erdensohn.", "tokens": ["Dir", "geht's", ",", "wie", "man\u00b7chem", "Er\u00b7den\u00b7sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Teufel! ich war bei mancher Schlacht,", "tokens": ["Zum", "Teu\u00b7fel", "!", "ich", "war", "bei", "man\u00b7cher", "Schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Henker hab ich noch nicht gemacht.", "tokens": ["Den", "Hen\u00b7ker", "hab", "ich", "noch", "nicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Das ist der Strick, das ist der Stein,", "tokens": ["Das", "ist", "der", "Strick", ",", "das", "ist", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist das Wasser, \u2013 es mu\u00df ja sein.", "tokens": ["Das", "ist", "das", "Was\u00b7ser", ",", "\u2013", "es", "mu\u00df", "ja", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "$(", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Komm her, du K\u00f6ter, und sieh mich nicht an,", "tokens": ["Komm", "her", ",", "du", "K\u00f6\u00b7ter", ",", "und", "sieh", "mich", "nicht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,", "KON", "VVIMP", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch nur ein Fu\u00dfsto\u00df, so ist es getan.", "tokens": ["Noch", "nur", "ein", "Fu\u00df\u00b7sto\u00df", ",", "so", "ist", "es", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Wie er in die Schlinge den Hals ihm gesteckt,", "tokens": ["Wie", "er", "in", "die", "Schlin\u00b7ge", "den", "Hals", "ihm", "ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat wedelnd der Hund die Hand ihm geleckt,", "tokens": ["Hat", "we\u00b7delnd", "der", "Hund", "die", "Hand", "ihm", "ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da zog er die Schlinge sogleich zur\u00fcck,", "tokens": ["Da", "zog", "er", "die", "Schlin\u00b7ge", "sog\u00b7leich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und warf sie schnell um sein eigen Genick.", "tokens": ["Und", "warf", "sie", "schnell", "um", "sein", "ei\u00b7gen", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Und tat einen Fluch, gar schauderhaft,", "tokens": ["Und", "tat", "ei\u00b7nen", "Fluch", ",", "gar", "schau\u00b7der\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und raffte zusammen die letzte Kraft,", "tokens": ["Und", "raff\u00b7te", "zu\u00b7sam\u00b7men", "die", "letz\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und st\u00fcrzt' in die Flut sich, die t\u00f6nend stieg,", "tokens": ["Und", "st\u00fcrzt'", "in", "die", "Flut", "sich", ",", "die", "t\u00f6\u00b7nend", "stieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PRF", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In Kreise sich zog und \u00fcber ihm schwieg.", "tokens": ["In", "Krei\u00b7se", "sich", "zog", "und", "\u00fc\u00b7ber", "ihm", "schwieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wohl sprang der Hund zur Rettung hinzu,", "tokens": ["Wohl", "sprang", "der", "Hund", "zur", "Ret\u00b7tung", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wohl heult' er die Schiffer aus ihrer Ruh,", "tokens": ["Wohl", "heult'", "er", "die", "Schif\u00b7fer", "aus", "ih\u00b7rer", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wohl zog er sie winselnd und zerrend her, \u2013", "tokens": ["Wohl", "zog", "er", "sie", "win\u00b7selnd", "und", "zer\u00b7rend", "her", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "KON", "VVPP", "PTKVZ", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie sie ihn fanden, da war er nicht mehr.", "tokens": ["Wie", "sie", "ihn", "fan\u00b7den", ",", "da", "war", "er", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Er ward verscharret in stiller Stund,", "tokens": ["Er", "ward", "ver\u00b7schar\u00b7ret", "in", "stil\u00b7ler", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es folgt' ihm winselnd nur der Hund,", "tokens": ["Es", "folgt'", "ihm", "win\u00b7selnd", "nur", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat, wo den Leib die Erde deckt,", "tokens": ["Der", "hat", ",", "wo", "den", "Leib", "die", "Er\u00b7de", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich hingestreckt und ist da verreckt.", "tokens": ["Sich", "hin\u00b7ge\u00b7streckt", "und", "ist", "da", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.12": {"line.1": {"text": "Drei Taler erlegen f\u00fcr meinen Hund!", "tokens": ["Drei", "Ta\u00b7ler", "er\u00b7le\u00b7gen", "f\u00fcr", "mei\u00b7nen", "Hund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schlage das Wetter mich gleich in den Grund!", "tokens": ["So", "schla\u00b7ge", "das", "Wet\u00b7ter", "mich", "gleich", "in", "den", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Was denken die Herrn von der Polizei?", "tokens": ["Was", "den\u00b7ken", "die", "Herrn", "von", "der", "Po\u00b7li\u00b7zei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Was soll nun wieder die Schinderei?", "tokens": ["Was", "soll", "nun", "wie\u00b7der", "die", "Schin\u00b7de\u00b7rei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Ich bin ein alter, ein kranker Mann,", "tokens": ["Ich", "bin", "ein", "al\u00b7ter", ",", "ein", "kran\u00b7ker", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der keinen Groschen verdienen kann;", "tokens": ["Der", "kei\u00b7nen", "Gro\u00b7schen", "ver\u00b7die\u00b7nen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich habe nicht Geld, ich habe nicht Brot,", "tokens": ["Ich", "ha\u00b7be", "nicht", "Geld", ",", "ich", "ha\u00b7be", "nicht", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich lebe ja nur von Hunger und Not.", "tokens": ["Ich", "le\u00b7be", "ja", "nur", "von", "Hun\u00b7ger", "und", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Und wann ich erkrankt, und wann ich verarmt,", "tokens": ["Und", "wann", "ich", "er\u00b7krankt", ",", "und", "wann", "ich", "ver\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$,", "KON", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wer hat sich da noch meiner erbarmt?", "tokens": ["Wer", "hat", "sich", "da", "noch", "mei\u00b7ner", "er\u00b7barmt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADV", "ADV", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wer hat, wann ich auf Gottes Welt", "tokens": ["Wer", "hat", ",", "wann", "ich", "auf", "Got\u00b7tes", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allein mich fand, zu mir sich gesellt?", "tokens": ["Al\u00b7lein", "mich", "fand", ",", "zu", "mir", "sich", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Wer hat mich geliebt, wann ich mich geh\u00e4rmt?", "tokens": ["Wer", "hat", "mich", "ge\u00b7liebt", ",", "wann", "ich", "mich", "ge\u00b7h\u00e4rmt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "PPER", "PRF", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wer, wann ich fror, hat mich gew\u00e4rmt?", "tokens": ["Wer", ",", "wann", "ich", "fror", ",", "hat", "mich", "ge\u00b7w\u00e4rmt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat mit mir, wann ich hungrig gemurrt,", "tokens": ["Wer", "hat", "mit", "mir", ",", "wann", "ich", "hung\u00b7rig", "ge\u00b7murrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "$,", "PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Getrost gehungert und nicht geknurrt?", "tokens": ["Ge\u00b7trost", "ge\u00b7hun\u00b7gert", "und", "nicht", "ge\u00b7knurrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Es geht zur Neige mit uns zwein,", "tokens": ["Es", "geht", "zur", "Nei\u00b7ge", "mit", "uns", "zwein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mu\u00df, mein Tier, geschieden sein;", "tokens": ["Es", "mu\u00df", ",", "mein", "Tier", ",", "ge\u00b7schie\u00b7den", "sein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist, wie ich, nun alt und krank,", "tokens": ["Du", "bist", ",", "wie", "ich", ",", "nun", "alt", "und", "krank", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich soll dich ers\u00e4ufen, das ist der Dank!", "tokens": ["Ich", "soll", "dich", "er\u00b7s\u00e4u\u00b7fen", ",", "das", "ist", "der", "Dank", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.17": {"line.1": {"text": "Das ist der Dank, das ist der Lohn!", "tokens": ["Das", "ist", "der", "Dank", ",", "das", "ist", "der", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir geht's, wie manchem Erdensohn.", "tokens": ["Dir", "geht's", ",", "wie", "man\u00b7chem", "Er\u00b7den\u00b7sohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Teufel! ich war bei mancher Schlacht,", "tokens": ["Zum", "Teu\u00b7fel", "!", "ich", "war", "bei", "man\u00b7cher", "Schlacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Henker hab ich noch nicht gemacht.", "tokens": ["Den", "Hen\u00b7ker", "hab", "ich", "noch", "nicht", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Das ist der Strick, das ist der Stein,", "tokens": ["Das", "ist", "der", "Strick", ",", "das", "ist", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist das Wasser, \u2013 es mu\u00df ja sein.", "tokens": ["Das", "ist", "das", "Was\u00b7ser", ",", "\u2013", "es", "mu\u00df", "ja", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "$(", "PPER", "VMFIN", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Komm her, du K\u00f6ter, und sieh mich nicht an,", "tokens": ["Komm", "her", ",", "du", "K\u00f6\u00b7ter", ",", "und", "sieh", "mich", "nicht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,", "KON", "VVIMP", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch nur ein Fu\u00dfsto\u00df, so ist es getan.", "tokens": ["Noch", "nur", "ein", "Fu\u00df\u00b7sto\u00df", ",", "so", "ist", "es", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Wie er in die Schlinge den Hals ihm gesteckt,", "tokens": ["Wie", "er", "in", "die", "Schlin\u00b7ge", "den", "Hals", "ihm", "ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat wedelnd der Hund die Hand ihm geleckt,", "tokens": ["Hat", "we\u00b7delnd", "der", "Hund", "die", "Hand", "ihm", "ge\u00b7leckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da zog er die Schlinge sogleich zur\u00fcck,", "tokens": ["Da", "zog", "er", "die", "Schlin\u00b7ge", "sog\u00b7leich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und warf sie schnell um sein eigen Genick.", "tokens": ["Und", "warf", "sie", "schnell", "um", "sein", "ei\u00b7gen", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.20": {"line.1": {"text": "Und tat einen Fluch, gar schauderhaft,", "tokens": ["Und", "tat", "ei\u00b7nen", "Fluch", ",", "gar", "schau\u00b7der\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und raffte zusammen die letzte Kraft,", "tokens": ["Und", "raff\u00b7te", "zu\u00b7sam\u00b7men", "die", "letz\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und st\u00fcrzt' 