hn fanden, da war er nicht mehr.", "tokens": ["Wie", "sie", "ihn", "fan\u00b7den", ",", "da", "war", "er", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Er ward verscharret in stiller Stund,", "tokens": ["Er", "ward", "ver\u00b7schar\u00b7ret", "in", "stil\u00b7ler", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es folgt' ihm winselnd nur der Hund,", "tokens": ["Es", "folgt'", "ihm", "win\u00b7selnd", "nur", "der", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der hat, wo den Leib die Erde deckt,", "tokens": ["Der", "hat", ",", "wo", "den", "Leib", "die", "Er\u00b7de", "deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich hingestreckt und ist da verreckt.", "tokens": ["Sich", "hin\u00b7ge\u00b7streckt", "und", "ist", "da", "ver\u00b7reckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}