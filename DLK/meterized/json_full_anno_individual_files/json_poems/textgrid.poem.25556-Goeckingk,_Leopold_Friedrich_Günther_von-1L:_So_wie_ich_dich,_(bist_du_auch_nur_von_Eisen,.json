{"textgrid.poem.25556": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wie ich dich, (bist du auch nur von Eisen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie ich dich, (bist du auch nur von Eisen,", "tokens": ["So", "wie", "ich", "dich", ",", "(", "bist", "du", "auch", "nur", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "$,", "$(", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und gibst du gleich nicht F\u00fcrsten-Sold, nicht Rang,)", "tokens": ["Und", "gibst", "du", "gleich", "nicht", "F\u00fcrs\u00b7ten\u00b7Sold", ",", "nicht", "Rang", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf jenen Nacht verh\u00fcllten Reisen", "tokens": ["Auf", "je\u00b7nen", "Nacht", "ver\u00b7h\u00fcll\u00b7ten", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meinen K\u00fcssen fast verschlang:", "tokens": ["Mit", "mei\u00b7nen", "K\u00fcs\u00b7sen", "fast", "ver\u00b7schlang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So starrte nie der Ehrsucht lodernd Feuer", "tokens": ["So", "starr\u00b7te", "nie", "der", "Ehr\u00b7sucht", "lo\u00b7dernd", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Den seine sittenlose Leyer", "tokens": ["Den", "sei\u00b7ne", "sit\u00b7ten\u00b7lo\u00b7se", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Mit eines K\u00f6nigs Gunst gewann.", "tokens": ["Mit", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Gunst", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Antichambre goldne Fl\u00fcgelth\u00fcren,", "tokens": ["Der", "An\u00b7ti\u00b7chamb\u00b7re", "gold\u00b7ne", "Fl\u00fc\u00b7gelt\u00b7h\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Er\u00f6ffneten vor seinem Schl\u00fcssel sich,", "tokens": ["Er\u00b7\u00f6ff\u00b7ne\u00b7ten", "vor", "sei\u00b7nem", "Schl\u00fcs\u00b7sel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du konntest mich zu ", "tokens": ["Du", "konn\u00b7test", "mich", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKZU"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Wer hatte mehr? Er, oder ich?", "tokens": ["Wer", "hat\u00b7te", "mehr", "?", "Er", ",", "o\u00b7der", "ich", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$.", "PPER", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich hatte dich! wie spielten um mein Leben", "tokens": ["Ich", "hat\u00b7te", "dich", "!", "wie", "spiel\u00b7ten", "um", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$.", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Freuden da gleich einem Bienenschwarm'!", "tokens": ["Die", "Freu\u00b7den", "da", "gleich", "ei\u00b7nem", "Bie\u00b7nen\u00b7schwarm'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nur halb so viel als du zu geben,", "tokens": ["Nur", "halb", "so", "viel", "als", "du", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "KOUS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "War selbst der K\u00f6nig viel zu arm!", "tokens": ["War", "selbst", "der", "K\u00f6\u00b7nig", "viel", "zu", "arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wer dich besa\u00df, (", "tokens": ["Wer", "dich", "be\u00b7sa\u00df", ",", "("], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Den Sonnenglanz bei diesem aufgestellt!)", "tokens": ["Den", "Son\u00b7nen\u00b7glanz", "bei", "die\u00b7sem", "auf\u00b7ge\u00b7stellt", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Besa\u00df das Herz, ja selbst die Ehre,", "tokens": ["Be\u00b7sa\u00df", "das", "Herz", ",", "ja", "selbst", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Des ersten M\u00e4dchens auf der Welt.", "tokens": ["Des", "ers\u00b7ten", "M\u00e4d\u00b7chens", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "O! h\u00e4tt' ich nichts als einen Thron verloren,", "tokens": ["O", "!", "h\u00e4tt'", "ich", "nichts", "als", "ei\u00b7nen", "Thron", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "PIS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Dann w\u00fcrd' ich stolz wie ", "tokens": ["Dann", "w\u00fcrd'", "ich", "stolz", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM"], "meter": "-+-+-", "measure": "iambic.di"}, "line.23": {"text": "Und z\u00f6ge heiter zu den Thoren", "tokens": ["Und", "z\u00f6\u00b7ge", "hei\u00b7ter", "zu", "den", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der Ruhe und der Weisheit ein.", "tokens": ["Der", "Ru\u00b7he", "und", "der", "Weis\u00b7heit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "So aber will die Weisheit mich nicht kennen!", "tokens": ["So", "a\u00b7ber", "will", "die", "Weis\u00b7heit", "mich", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die Ruhe schl\u00e4gt vor mir die Th\u00fcren zu!", "tokens": ["Die", "Ru\u00b7he", "schl\u00e4gt", "vor", "mir", "die", "Th\u00fc\u00b7ren", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Will nichts mir eine Freistadt g\u00f6nnen?", "tokens": ["Will", "nichts", "mir", "ei\u00b7ne", "Frei\u00b7stadt", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "O Tod! so g\u00f6nne mir sie du!", "tokens": ["O", "Tod", "!", "so", "g\u00f6n\u00b7ne", "mir", "sie", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Begleite mich, du den ich lieber habe,", "tokens": ["Be\u00b7glei\u00b7te", "mich", ",", "du", "den", "ich", "lie\u00b7ber", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ART", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Als alles, was zur\u00fcck hier bleiben mu\u00df,", "tokens": ["Als", "al\u00b7les", ",", "was", "zu\u00b7r\u00fcck", "hier", "blei\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Begleite mich zu meinem Grabe,", "tokens": ["Be\u00b7glei\u00b7te", "mich", "zu", "mei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Nimm zum voraus den Abschiedsku\u00df!", "tokens": ["Nimm", "zum", "vo\u00b7raus", "den", "Ab\u00b7schieds\u00b7ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und unter meines Hauptes K\u00fcssen, zehre", "tokens": ["Und", "un\u00b7ter", "mei\u00b7nes", "Haup\u00b7tes", "K\u00fcs\u00b7sen", ",", "zeh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Der Rost dich auf von meinem Thr\u00e4nenbach'!", "tokens": ["Der", "Rost", "dich", "auf", "von", "mei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7bach'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Da la\u00df uns ruhn! nicht Nantens Ehre,", "tokens": ["Da", "la\u00df", "uns", "ruhn", "!", "nicht", "Nan\u00b7tens", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$.", "PTKNEG", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Nur ihre Reue folgt uns nach!", "tokens": ["Nur", "ih\u00b7re", "Reu\u00b7e", "folgt", "uns", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So wie ich dich, (bist du auch nur von Eisen,", "tokens": ["So", "wie", "ich", "dich", ",", "(", "bist", "du", "auch", "nur", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "$,", "$(", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und gibst du gleich nicht F\u00fcrsten-Sold, nicht Rang,)", "tokens": ["Und", "gibst", "du", "gleich", "nicht", "F\u00fcrs\u00b7ten\u00b7Sold", ",", "nicht", "Rang", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf jenen Nacht verh\u00fcllten Reisen", "tokens": ["Auf", "je\u00b7nen", "Nacht", "ver\u00b7h\u00fcll\u00b7ten", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meinen K\u00fcssen fast verschlang:", "tokens": ["Mit", "mei\u00b7nen", "K\u00fcs\u00b7sen", "fast", "ver\u00b7schlang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN