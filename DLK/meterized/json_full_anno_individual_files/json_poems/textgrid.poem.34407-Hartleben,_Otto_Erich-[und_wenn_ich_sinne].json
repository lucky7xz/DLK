{"textgrid.poem.34407": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[und wenn ich sinne]", "genre": "verse", "period": "N.A.", "pub_year": 1885, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u2013 Und wenn ich sinne, denk ich wohl der Mutter.", "tokens": ["\u2013", "Und", "wenn", "ich", "sin\u00b7ne", ",", "denk", "ich", "wohl", "der", "Mut\u00b7ter", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedankenk\u00fchn in aller Leidensdemuth,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7k\u00fchn", "in", "al\u00b7ler", "Lei\u00b7dens\u00b7de\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "den Blick geheftet auf das Christusbild,", "tokens": ["den", "Blick", "ge\u00b7hef\u00b7tet", "auf", "das", "Chris\u00b7tus\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "zu F\u00fcssen deines Bettes an der Wand \u2013", "tokens": ["zu", "F\u00fcs\u00b7sen", "dei\u00b7nes", "Bet\u00b7tes", "an", "der", "Wand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "so lagst du vor mir. Und die Worte wieder", "tokens": ["so", "lagst", "du", "vor", "mir", ".", "Und", "die", "Wor\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "KON", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "h\u00f6r ich aus deinem sterbensmatten Munde:", "tokens": ["h\u00f6r", "ich", "aus", "dei\u00b7nem", "ster\u00b7bens\u00b7mat\u00b7ten", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht meinetwegen bangt mir vor dem Tode.", "tokens": ["Nicht", "mei\u00b7net\u00b7we\u00b7gen", "bangt", "mir", "vor", "dem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Euch lass ich nun allein ... In tiefes Sinnen", "tokens": ["Euch", "lass", "ich", "nun", "al\u00b7lein", "...", "In", "tie\u00b7fes", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "verlorst du dich. Und deine Augen hingen", "tokens": ["ver\u00b7lorst", "du", "dich", ".", "Und", "dei\u00b7ne", "Au\u00b7gen", "hin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$.", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "wie zukunftsuchend an dem Dorngekr\u00f6nten,", "tokens": ["wie", "zu\u00b7kunft\u00b7su\u00b7chend", "an", "dem", "Dorn\u00b7ge\u00b7kr\u00f6n\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "an den du glaubtest.", "tokens": ["an", "den", "du", "glaub\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich war ein Kind und ahnte kaum die Stunde", "tokens": ["Ich", "war", "ein", "Kind", "und", "ahn\u00b7te", "kaum", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und ihre unheilschwere Leidensf\u00fclle,", "tokens": ["und", "ih\u00b7re", "un\u00b7heil\u00b7schwe\u00b7re", "Lei\u00b7dens\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "doch oft seitdem will es mich leis gemahnen,", "tokens": ["doch", "oft", "seit\u00b7dem", "will", "es", "mich", "leis", "ge\u00b7mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "VMFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "als h\u00e4tte damals lastend schon die Hand", "tokens": ["als", "h\u00e4t\u00b7te", "da\u00b7mals", "las\u00b7tend", "schon", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "VAFIN", "ADV", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "des Schicksals sich gelegt auf meinen Nacken,", "tokens": ["des", "Schick\u00b7sals", "sich", "ge\u00b7legt", "auf", "mei\u00b7nen", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "als st\u00fcnd ich fremd in einem reichen Hause ...", "tokens": ["als", "st\u00fcnd", "ich", "fremd", "in", "ei\u00b7nem", "rei\u00b7chen", "Hau\u00b7se", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch seis darum. Was frommt es, insgeheim", "tokens": ["Doch", "seis", "da\u00b7rum", ".", "Was", "frommt", "es", ",", "ins\u00b7ge\u00b7heim"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "PAV", "$.", "PWS", "VVFIN", "PPER", "$,", "APPRART"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "knirschend zu rechten mit dem ewgen Unrecht.", "tokens": ["knir\u00b7schend", "zu", "rech\u00b7ten", "mit", "dem", "ew\u00b7gen", "Un\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich bin durch dich, und in mir rollt dein Blut,", "tokens": ["Ich", "bin", "durch", "dich", ",", "und", "in", "mir", "rollt", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und keinen Tropfen liess ich um das Gl\u00fcck", "tokens": ["und", "kei\u00b7nen", "Trop\u00b7fen", "liess", "ich", "um", "das", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["