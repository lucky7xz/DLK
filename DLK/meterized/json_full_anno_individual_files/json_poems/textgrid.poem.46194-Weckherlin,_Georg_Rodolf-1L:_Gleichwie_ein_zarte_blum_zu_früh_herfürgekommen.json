{"textgrid.poem.46194": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleichwie ein zarte blum zu fr\u00fch herf\u00fcrgekommen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie ein zarte blum zu fr\u00fch herf\u00fcrgekommen", "tokens": ["Gleich\u00b7wie", "ein", "zar\u00b7te", "blum", "zu", "fr\u00fch", "her\u00b7f\u00fcr\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NE", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wird durch des morgens frost schnell wider weggenommen,", "tokens": ["wird", "durch", "des", "mor\u00b7gens", "frost", "schnell", "wi\u00b7der", "weg\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADV", "ADJD", "ADJD", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und gleichwie eine frucht zart an gestalt und saft,", "tokens": ["und", "gleich\u00b7wie", "ei\u00b7ne", "frucht", "zart", "an", "ge\u00b7stalt", "und", "saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "ART", "NN", "ADJD", "APPR", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wan sie reif vor der zeit, nicht bleibet lang wehrhaft:", "tokens": ["wan", "sie", "reif", "vor", "der", "zeit", ",", "nicht", "blei\u00b7bet", "lang", "wehr\u00b7haft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "PTKNEG", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "also die bl\u00fcmelein der weisheit, zucht und tugend,", "tokens": ["al\u00b7so", "die", "bl\u00fc\u00b7me\u00b7lein", "der", "weis\u00b7heit", ",", "zucht", "und", "tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "die man frisch bl\u00fchen sah in deiner seelen jugend,", "tokens": ["die", "man", "frisch", "bl\u00fc\u00b7hen", "sah", "in", "dei\u00b7ner", "see\u00b7len", "ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVINF", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "o fr\u00e4ulein, also sch\u00f6n, als h\u00f6flich, klug und from,", "tokens": ["o", "fr\u00e4u\u00b7lein", ",", "al\u00b7so", "sch\u00f6n", ",", "als", "h\u00f6f\u00b7lich", ",", "klug", "und", "from", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "ADV", "ADJD", "$,", "KOUS", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "bezeugten mehr dan gnug, da\u00df deiner jahren trum", "tokens": ["be\u00b7zeug\u00b7ten", "mehr", "dan", "gnug", ",", "da\u00df", "dei\u00b7ner", "jah\u00b7ren", "trum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "kont nicht langw\u00fcrig sein, und da\u00df dein kurzes leben", "tokens": ["kont", "nicht", "lang\u00b7w\u00fc\u00b7rig", "sein", ",", "und", "da\u00df", "dein", "kur\u00b7zes", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ADJD", "VAINF", "$,", "KON", "KOUS", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "uns solt ein schnelle lehr der wunder gottes geben,", "tokens": ["uns", "solt", "ein", "schnel\u00b7le", "lehr", "der", "wun\u00b7der", "got\u00b7tes", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und deines leibs gestalt an sch\u00f6nheit und an zucht", "tokens": ["und", "dei\u00b7nes", "leibs", "ge\u00b7stalt", "an", "sch\u00f6n\u00b7heit", "und", "an", "zucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ein himmelische blum, solt mit so werter frucht", "tokens": ["ein", "him\u00b7me\u00b7li\u00b7sche", "blum", ",", "solt", "mit", "so", "wer\u00b7ter", "frucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "$,", "VMFIN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "nicht lang das teutsche reich, noch dein geschlecht bereichen,", "tokens": ["nicht", "lang", "das", "teut\u00b7sche", "reich", ",", "noch", "dein", "ge\u00b7schlecht", "be\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "ADJA", "ADJD", "$,", "ADV", "PPOSAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sondern bald in den saal der ewigkeit entweichen.", "tokens": ["son\u00b7dern", "bald", "in", "den", "saal", "der", "e\u00b7wig\u00b7keit", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "dan die vollkommenheit, die alhie keinen platz,", "tokens": ["dan", "die", "voll\u00b7kom\u00b7men\u00b7heit", ",", "die", "al\u00b7hie", "kei\u00b7nen", "platz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "ist nicht f\u00fcr dieser welt, sondern f\u00fcr gottes schatz.", "tokens": ["ist", "nicht", "f\u00fcr", "die\u00b7ser", "welt", ",", "son\u00b7dern", "f\u00fcr", "got\u00b7tes", "schatz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "ach! wie vil sch\u00f6nheit, ehr und engelische sitten", "tokens": ["ach", "!", "wie", "vil", "sch\u00f6n\u00b7heit", ",", "ehr", "und", "en\u00b7ge\u00b7li\u00b7sche", "sit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "PIAT", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.18": {"text": "(des Teutschlands trost und zier) hat die Park abgeschnitten", "tokens": ["(", "des", "Teutschlands", "trost", "und", "zier", ")", "hat", "die", "Park", "ab\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "VVFIN", "KON", "NN", "$(", "VAFIN", "ART", "NN", "VVPP"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "mit dieser frischen blum! blum, deren lieblichkeit", "tokens": ["mit", "die\u00b7ser", "fri\u00b7schen", "blum", "!", "blum", ",", "de\u00b7ren", "lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NE", "$.", "NE", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "jetzund gepflanzet ist in die unsterblichkeit.", "tokens": ["je\u00b7tzund", "ge\u00b7pflan\u00b7zet", "ist", "in", "die", "uns\u00b7terb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Gnadlose Atropos! ach, kontest du nicht sparen", "tokens": ["Gnad\u00b7lo\u00b7se", "A\u00b7tro\u00b7pos", "!", "ach", ",", "kon\u00b7test", "du", "nicht", "spa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "XY", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.22": {"text": "bis gegen ihrem herbst den fr\u00fcling ihrer jahren!", "tokens": ["bis", "ge\u00b7gen", "ih\u00b7rem", "herbst", "den", "fr\u00fc\u00b7ling", "ih\u00b7rer", "jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "nein! dan du, sehend an ihr herz, seel und verstand,", "tokens": ["nein", "!", "dan", "du", ",", "se\u00b7hend", "an", "ihr", "herz", ",", "seel", "und", "ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PPER", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "an welchen gar nichts jung, hast mit zu frecher hand", "tokens": ["an", "wel\u00b7chen", "gar", "nichts", "jung", ",", "hast", "mit", "zu", "fre\u00b7cher", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADV", "PIS", "ADJD", "$,", "VAFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "f\u00fcrzeitig diese frucht, noch gar gr\u00fcn, abgezwicket", "tokens": ["f\u00fcr\u00b7zei\u00b7tig", "die\u00b7se", "frucht", ",", "noch", "gar", "gr\u00fcn", ",", "ab\u00b7ge\u00b7zwi\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "PDAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und, uns entgl\u00fcckend ganz, das firmament begl\u00fccket.", "tokens": ["und", ",", "uns", "ent\u00b7gl\u00fc\u00b7ckend", "ganz", ",", "das", "fir\u00b7ma\u00b7ment", "be\u00b7gl\u00fc\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVPP", "ADV", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wol. Das war gottes will, der mu\u00df gehalten sein.", "tokens": ["Wol", ".", "Das", "war", "got\u00b7tes", "will", ",", "der", "mu\u00df", "ge\u00b7hal\u00b7ten", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PDS", "VAFIN", "ADJA", "VMFIN", "$,", "ART", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.28": {"text": "so nim nu hin von uns, du wertes seelelein,", "tokens": ["so", "nim", "nu", "hin", "von", "uns", ",", "du", "wer\u00b7tes", "see\u00b7lel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "APPR", "PPER", "$,", "PPER", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "mit unserm zeherflu\u00df, dan wir ja mehr nicht haben,", "tokens": ["mit", "un\u00b7serm", "ze\u00b7her\u00b7flu\u00df", ",", "dan", "wir", "ja", "mehr", "nicht", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "PPER", "ADV", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "f\u00fcr dein grab diese schrift in unser herz gegraben:", "tokens": ["f\u00fcr", "dein", "grab", "die\u00b7se", "schrift", "in", "un\u00b7ser", "herz", "ge\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Indem des himmels macht und des erdreichs frechheit,", "tokens": ["In\u00b7dem", "des", "him\u00b7mels", "macht", "und", "des", "er\u00b7dreichs", "frech\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "o fr\u00e4ulein, deine seel und leib f\u00fcr sich erkoren,", "tokens": ["o", "fr\u00e4u\u00b7lein", ",", "dei\u00b7ne", "seel", "und", "leib", "f\u00fcr", "sich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "PPOSAT", "NN", "KON", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "da hat auch die natur, die tugend, welt, sch\u00f6nheit", "tokens": ["da", "hat", "auch", "die", "na\u00b7tur", ",", "die", "tu\u00b7gend", ",", "welt", ",", "sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "gleich ihren besten ruhm, schatz, trost und freud verloren.", "tokens": ["gleich", "ih\u00b7ren", "bes\u00b7ten", "ruhm", ",", "schatz", ",", "trost", "und", "freud", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gleichwie ein zarte blum zu fr\u00fch herf\u00fcrgekommen", "tokens": ["Gleich\u00b7wie", "ein", "zar\u00b7te", "blum", "zu", "fr\u00fch", "her\u00b7f\u00fcr\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NE", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text":