{"textgrid.poem.47297": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist von mir als wie der Lenz geschieden,", "tokens": ["Du", "bist", "von", "mir", "als", "wie", "der", "Lenz", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie war dein Abschiedl\u00e4cheln zaubervoll!", "tokens": ["Wie", "war", "dein", "Ab\u00b7schied\u00b7l\u00e4\u00b7cheln", "zau\u00b7ber\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Thr\u00e4nchen Tau an deinen Augenliden,", "tokens": ["Ein", "Thr\u00e4n\u00b7chen", "Tau", "an", "dei\u00b7nen", "Au\u00b7gen\u00b7li\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Seufzerchen, das auf der Lippe schwoll!", "tokens": ["Ein", "Seuf\u00b7zer\u00b7chen", ",", "das", "auf", "der", "Lip\u00b7pe", "schwoll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schiedest du, der Wehmut stillen Frieden", "tokens": ["So", "schie\u00b7dest", "du", ",", "der", "Weh\u00b7mut", "stil\u00b7len", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In meinem Busen lassend, nicht den Groll.", "tokens": ["In", "mei\u00b7nem", "Bu\u00b7sen", "las\u00b7send", ",", "nicht", "den", "Groll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So sieht Erinn'rung ewig dich hienieden,", "tokens": ["So", "sieht", "Er\u00b7inn'\u00b7rung", "e\u00b7wig", "dich", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis ich dich droben ewig sehen soll.", "tokens": ["Bis", "ich", "dich", "dro\u00b7ben", "e\u00b7wig", "se\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist von mir als wie der Lenz geschieden,", "tokens": ["Du", "bist", "von", "mir", "als", "wie", "der", "Lenz", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie war dein Abschiedl\u00e4cheln zaubervoll!", "tokens": ["Wie", "war", "dein", "Ab\u00b7schied\u00b7l\u00e4\u00b7cheln", "zau\u00b7ber\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}