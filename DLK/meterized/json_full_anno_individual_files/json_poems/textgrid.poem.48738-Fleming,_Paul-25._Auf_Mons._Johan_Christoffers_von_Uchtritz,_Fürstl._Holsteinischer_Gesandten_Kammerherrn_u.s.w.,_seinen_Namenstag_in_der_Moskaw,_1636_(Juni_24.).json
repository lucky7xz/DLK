{"textgrid.poem.48738": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "25. Auf Mons. Johan Christoffers von Uchtritz, F\u00fcrstl. Holsteinischer Gesandten Kammerherrn u.s.w., seinen Namenstag in der Moskaw, 1636 (Juni 24.)", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und gleichwol hab ich dich noch dennoch einmal feste,", "tokens": ["Und", "gleich\u00b7wol", "hab", "ich", "dich", "noch", "den\u00b7noch", "ein\u00b7mal", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du edler Mei\u00dfner du! Ob zweie sind vorbei,", "tokens": ["du", "ed\u00b7ler", "Mei\u00df\u00b7ner", "du", "!", "Ob", "zwei\u00b7e", "sind", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$.", "KOUS", "PPER", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so l\u00e4\u00dft das dritte Jahr dich aber drum nicht frei.", "tokens": ["so", "l\u00e4\u00dft", "das", "drit\u00b7te", "Jahr", "dich", "a\u00b7ber", "drum", "nicht", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir m\u00fcssen dennoch sein auf heute deine G\u00e4ste.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "den\u00b7noch", "sein", "auf", "heu\u00b7te", "dei\u00b7ne", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", 