{"textgrid.poem.24113": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Schusterperspektive", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Held und Herr hatte Stiefel not,", "tokens": ["Ein", "Held", "und", "Herr", "hat\u00b7te", "Stie\u00b7fel", "not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und einen Schuster zu sich entbot,", "tokens": ["Und", "ei\u00b7nen", "Schus\u00b7ter", "zu", "sich", "ent\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie anzumessen.", "tokens": ["Sie", "an\u00b7zu\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der rannte begl\u00fcckt von Trinken und Essen", "tokens": ["Der", "rann\u00b7te", "be\u00b7gl\u00fcckt", "von", "Trin\u00b7ken", "und", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und kam.", "tokens": ["Und", "kam", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Zunfts\u00e4uberlich das Fu\u00dfma\u00df nahm.", "tokens": ["Zunf\u00b7ts\u00e4u\u00b7ber\u00b7lich", "das", "Fu\u00df\u00b7ma\u00df", "nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "A