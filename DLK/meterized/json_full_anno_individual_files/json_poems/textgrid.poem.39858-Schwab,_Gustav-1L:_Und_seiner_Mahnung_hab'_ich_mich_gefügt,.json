{"textgrid.poem.39858": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und seiner Mahnung hab' ich mich gef\u00fcgt,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und seiner Mahnung hab' ich mich gef\u00fcgt,", "tokens": ["Und", "sei\u00b7ner", "Mah\u00b7nung", "hab'", "ich", "mich", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier bin ich, wo die M\u00fc\u00dfigg\u00e4nger thronen,", "tokens": ["Hier", "bin", "ich", ",", "wo", "die", "M\u00fc\u00b7\u00dfig\u00b7g\u00e4n\u00b7ger", "thro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sanften Ruhe soll ich hier gewohnen,", "tokens": ["Der", "sanf\u00b7ten", "Ru\u00b7he", "soll", "ich", "hier", "ge\u00b7woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Denken mich nicht mehr um Sein betr\u00fcgt.", "tokens": ["Wo", "Den\u00b7ken", "mich", "nicht", "mehr", "um", "Sein", "be\u00b7tr\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und l\u00e4ugnen kann ich's nicht: ich bin vergn\u00fcgt,", "tokens": ["Und", "l\u00e4ug\u00b7nen", "kann", "ich's", "nicht", ":", "ich", "bin", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "PTKNEG", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Leib erstarkt, mein Geist l\u00e4\u00dft gern sich schonen,", "tokens": ["Mein", "Leib", "er\u00b7starkt", ",", "mein", "Geist", "l\u00e4\u00dft", "gern", "sich", "scho\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn beide d\u00e4mmern unter Bl\u00e4tterkronen,", "tokens": ["Wenn", "bei\u00b7de", "d\u00e4m\u00b7mern", "un\u00b7ter", "Bl\u00e4t\u00b7ter\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wenn der Becher beiden Jugend l\u00fcgt.", "tokens": ["Und", "wenn", "der", "Be\u00b7cher", "bei\u00b7den", "Ju\u00b7gend", "l\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wann denn verwandert ist der frische Morgen,", "tokens": ["Wann", "denn", "ver\u00b7wan\u00b7dert", "ist", "der", "fri\u00b7sche", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlummert ist der schw\u00fcle Nachmittag,", "tokens": ["Ver\u00b7schlum\u00b7mert", "