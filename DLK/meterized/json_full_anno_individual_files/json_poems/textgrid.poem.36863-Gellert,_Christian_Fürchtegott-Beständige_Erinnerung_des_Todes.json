{"textgrid.poem.36863": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Best\u00e4ndige Erinnerung des Todes", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sorgst du \u00e4ngstlich f\u00fcr dein Leben?", "tokens": ["Was", "sorgst", "du", "\u00e4ngst\u00b7lich", "f\u00fcr", "dein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es Gott gelassen \u00fcbergeben,", "tokens": ["Es", "Gott", "ge\u00b7las\u00b7sen", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wahre Ruh und deine Pflicht.", "tokens": ["Ist", "wah\u00b7re", "Ruh", "und", "dei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du sollst es lieben, weislich n\u00fctzen,", "tokens": ["Du", "sollst", "es", "lie\u00b7ben", ",", "weis\u00b7lich", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es dankbar, als ein Gl\u00fcck, besitzen,", "tokens": ["Es", "dank\u00b7bar", ",", "als", "ein", "Gl\u00fcck", ",", "be\u00b7sit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verlieren, als verl\u00f6rst du's nicht.", "tokens": ["Ver\u00b7lie\u00b7ren", ",", "als", "ver\u00b7l\u00f6rst", "du's", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tod soll dich nicht traurig schrecken:", "tokens": ["Der", "Tod", "soll", "dich", "nicht", "trau\u00b7rig", "schre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch dich zur Weisheit zu erwecken,", "tokens": ["Doch", "dich", "zur", "Weis\u00b7heit", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Soll er dir stets vor Augen sein.", "tokens": ["Soll", "er", "dir", "stets", "vor", "Au\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er soll den Wunsch zu leben mindern,", "tokens": ["Er", "soll", "den", "Wunsch", "zu", "le\u00b7ben", "min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch dich in deiner Pflicht nicht hindern,", "tokens": ["Doch", "dich", "in", "dei\u00b7ner", "Pflicht", "nicht", "hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vielmehr dir Kraft dazu verleihn.", "tokens": ["Viel\u00b7mehr", "dir", "Kraft", "da\u00b7zu", "ver\u00b7leihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ermattest du in deinen Pflichten:", "tokens": ["Er\u00b7mat\u00b7test", "du", "in", "dei\u00b7nen", "Pflich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df den Tod dich unterrichten,", "tokens": ["So", "la\u00df", "den", "Tod", "dich", "un\u00b7ter\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wenig deiner Tage sind.", "tokens": ["Wie", "we\u00b7nig", "dei\u00b7ner", "Ta\u00b7ge", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich: Sollt ich Gutes wohl verschieben?", "tokens": ["Sprich", ":", "Sollt", "ich", "Gu\u00b7tes", "wohl", "ver\u00b7schie\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nein, meine Zeit, es auszu\u00fcben,", "tokens": ["Nein", ",", "mei\u00b7ne", "Zeit", ",", "es", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist kurz, und sie verfliegt geschwind.", "tokens": ["Ist", "kurz", ",", "und", "sie", "ver\u00b7fliegt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denk an den Tod, wenn b\u00f6se Triebe,", "tokens": ["Denk", "an", "den", "Tod", ",", "wenn", "b\u00f6\u00b7se", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Lust der Welt und ihre Liebe", "tokens": ["Wenn", "Lust", "der", "Welt", "und", "ih\u00b7re", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich reizen; und ersticke sie.", "tokens": ["Dich", "rei\u00b7zen", ";", "und", "er\u00b7sti\u00b7cke", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich: Kann ich nicht noch heute sterben?", "tokens": ["Sprich", ":", "Kann", "ich", "nicht", "noch", "heu\u00b7te", "ster\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und k\u00f6nnt ich auch die Welt erwerben,", "tokens": ["Und", "k\u00f6nnt", "ich", "auch", "die", "Welt", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Beging ich doch solch \u00dcbel nie.", "tokens": ["Be\u00b7ging", "ich", "doch", "solch", "\u00dc\u00b7bel", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denk an den Tod, wenn Ruhm und Ehren,", "tokens": ["Denk", "an", "den", "Tod", ",", "wenn", "Ruhm", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn deine Sch\u00e4tze sich vermehren,", "tokens": ["Wenn", "dei\u00b7ne", "Sch\u00e4t\u00b7ze", "sich", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du sie nicht zu heftig liebst.", "tokens": ["Da\u00df", "du", "sie", "nicht", "zu", "hef\u00b7tig", "liebst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denk an die Eitelkeit der Erden,", "tokens": ["Denk", "an", "die", "Ei\u00b7tel\u00b7keit", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wenn sie dir entrissen werden,", "tokens": ["Da\u00df", ",", "wenn", "sie", "dir", "ent\u00b7ris\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du dann dich nicht zu sehr betr\u00fcbst.", "tokens": ["Du", "dann", "dich", "nicht", "zu", "sehr", "be\u00b7tr\u00fcbst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPER", "PTKNEG", "PTKA", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denk an den Tod bei frohen Tagen.", "tokens": ["Denk", "an", "den", "Tod", "bei", "fro\u00b7hen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann deine Lust sein Bild vertragen:", "tokens": ["Kann", "dei\u00b7ne", "Lust", "sein", "Bild", "ver\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist sie gut und unschuldsvoll.", "tokens": ["So", "ist", "sie", "gut", "und", "un\u00b7schulds\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich, dein Vergn\u00fcgen zu vers\u00fc\u00dfen:", "tokens": ["Sprich", ",", "dein", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Welch Gl\u00fcck werd ich erst dort genie\u00dfen,", "tokens": ["Welch", "Gl\u00fcck", "werd", "ich", "erst", "dort", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich unendlich leben soll!", "tokens": ["Wo", "ich", "un\u00b7end\u00b7lich", "le\u00b7ben", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denk an den Tod, wenn deinem Leben", "tokens": ["Denk", "an", "den", "Tod", ",", "wenn", "dei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das fehlt, wonach die Reichen streben;", "tokens": ["Das", "fehlt", ",", "wo\u00b7nach", "die", "Rei\u00b7chen", "stre\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sprich: Bin ich hier, um reich zu sein?", "tokens": ["Sprich", ":", "Bin", "ich", "hier", ",", "um", "reich", "zu", "sein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VAFIN", "PPER", "ADV", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heil mir! wenn ich in Christo sterbe,", "tokens": ["Heil", "mir", "!", "wenn", "ich", "in", "Chris\u00b7to", "ster\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann ist ein unbeflecktes Erbe,", "tokens": ["Dann", "ist", "ein", "un\u00b7be\u00b7fleck\u00b7tes", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dann ist der Himmel Reichtum mein.", "tokens": ["Dann", "ist", "der", "Him\u00b7mel", "Reich\u00b7tum", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Denk an den Tod, wenn Leiden kommen;", "tokens": ["Denk", "an", "den", "Tod", ",", "wenn", "Lei\u00b7den", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprich: Alle Tr\u00fcbsal eines Frommen", "tokens": ["Sprich", ":", "Al\u00b7le", "Tr\u00fcb\u00b7sal", "ei\u00b7nes", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PIAT", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ist zeitlich, und im Glauben leicht.", "tokens": ["Ist", "zeit\u00b7lich", ",", "und", "im", "Glau\u00b7ben", "leicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich leide; doch von allem B\u00f6sen", "tokens": ["Ich", "lei\u00b7de", ";", "doch", "von", "al\u00b7lem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wird mich der Tod bald, bald erl\u00f6sen;", "tokens": ["Wird", "mich", "der", "Tod", "bald", ",", "bald", "er\u00b7l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er ist's, der mir die Krone reicht.", "tokens": ["Er", "ist's", ",", "der", "mir", "die", "Kro\u00b7ne", "reicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Denk an den Tod, wenn freche Rotten", "tokens": ["Denk", "an", "den", "Tod", ",", "wenn", "fre\u00b7che", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Glaubens und der Tugend spotten,", "tokens": ["Des", "Glau\u00b7bens", "und", "der", "Tu\u00b7gend", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Laster stolz ihr Haupt erh\u00f6hn.", "tokens": ["Und", "Las\u00b7ter", "stolz", "ihr", "Haupt", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich bei dir selbst: Gott tr\u00e4gt