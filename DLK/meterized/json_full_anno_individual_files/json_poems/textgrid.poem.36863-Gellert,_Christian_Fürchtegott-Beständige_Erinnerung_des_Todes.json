en F\u00e4llen", "tokens": ["So", "su\u00b7che", "dir", "in", "al\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tod oft, lebhaft, vorzustellen;", "tokens": ["Den", "Tod", "oft", ",", "leb\u00b7haft", ",", "vor\u00b7zu\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wirst du ihn nicht zitternd scheun;", "tokens": ["So", "wirst", "du", "ihn", "nicht", "zit\u00b7ternd", "scheun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird er dir ein Trost in Klagen,", "tokens": ["So", "wird", "er", "dir", "ein", "Trost", "in", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein weiser Freund in guten Tagen,", "tokens": ["Ein", "wei\u00b7ser", "Freund", "in", "gu\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schild in der Versuchung sein.", "tokens": ["Ein", "Schild", "in", "der", "Ver\u00b7su\u00b7chung", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}