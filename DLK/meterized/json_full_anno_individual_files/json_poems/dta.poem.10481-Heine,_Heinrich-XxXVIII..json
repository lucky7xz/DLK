{"dta.poem.10481": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind, wir waren Kinder,", "tokens": ["Mein", "Kind", ",", "wir", "wa\u00b7ren", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Kinder, klein und froh;", "tokens": ["Zwei", "Kin\u00b7der", ",", "klein", "und", "froh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir krochen in's H\u00fchnerh\u00e4uschen", "tokens": ["Wir", "kro\u00b7chen", "in's", "H\u00fch\u00b7ner\u00b7h\u00e4usc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und steckten uns unter das Stroh.", "tokens": ["Und", "steck\u00b7ten", "uns", "un\u00b7ter", "das", "Stroh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Wir kr\u00e4hten wie die H\u00e4hne,", "tokens": ["Wir", "kr\u00e4h\u00b7ten", "wie", "die", "H\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kamen Leute vorbei \u2014", "tokens": ["Und", "ka\u00b7men", "Leu\u00b7te", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Kikerek\u00fch! sie glaubten,", "tokens": ["Ki\u00b7ke\u00b7re\u00b7k\u00fch", "!", "sie", "glaub\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Es w\u00e4re Hahnengeschrei.", "tokens": ["Es", "w\u00e4\u00b7re", "Hah\u00b7nen\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Die Kisten auf unserem Hofe,", "tokens": ["Die", "Kis\u00b7ten", "auf", "un\u00b7se\u00b7rem", "Ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die tapezirten wir aus,", "tokens": ["Die", "ta\u00b7pe\u00b7zir\u00b7ten", "wir", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wohnten drin beisammen,", "tokens": ["Und", "wohn\u00b7ten", "drin", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und machten ein vornehmes Haus.", "tokens": ["Und", "mach\u00b7ten", "ein", "vor\u00b7neh\u00b7mes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des Nachbars alte Katze", "tokens": ["Des", "Nach\u00b7bars", "al\u00b7te", "Kat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kam \u00f6fters zum Besuch;", "tokens": ["Kam", "\u00f6f\u00b7ters", "zum", "Be\u00b7such", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir machten ihr B\u00fcckling' und Knixe,", "tokens": ["Wir", "mach\u00b7ten", "ihr", "B\u00fc\u00b7ck\u00b7ling'", "und", "Kni\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Complimente genug.", "tokens": ["Und", "Com\u00b7pli\u00b7men\u00b7te", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Wir haben nach ihrem Befinden", "tokens": ["Wir", "ha\u00b7ben", "nach", "ih\u00b7rem", "Be\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Besorglich und freundlich gefragt;", "tokens": ["Be\u00b7sorg\u00b7lich", "und", "freund\u00b7lich", "ge\u00b7fragt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir haben seitdem dasselbe", "tokens": ["Wir", "ha\u00b7ben", "seit\u00b7dem", "das\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "PDAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mancher alten Katze gesagt.", "tokens": ["Man\u00b7cher", "al\u00b7ten", "Kat\u00b7ze", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Wir sa\u00dfen auch oft und sprachen", "tokens": ["Wir", "sa\u00b7\u00dfen", "auch", "oft", "und", "spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vern\u00fcnftig, wie alte Leut',", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", ",", "wie", "al\u00b7te", "Leut'", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und klagten, wie Alles besser", "tokens": ["Und", "klag\u00b7ten", ",", "wie", "Al\u00b7les", "bes\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gewesen zu unserer Zeit;", "tokens": ["Ge\u00b7we\u00b7sen", "zu", "un\u00b7se\u00b7rer", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Wie Lieb' und Treu' und Glauben", "tokens": ["Wie", "Lieb'", "und", "Treu'", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verschwunden aus der Welt,", "tokens": ["Ver\u00b7schwun\u00b7den", "aus", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie so theuer der Kaffee,", "tokens": ["Und", "wie", "so", "theu\u00b7er", "der", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wie so rar das Geld! \u2014 \u2014 \u2014", "tokens": ["Und", "wie", "so", "rar", "das", "Geld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "ART", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vorbei sind die Kinderspiele", "tokens": ["Vor\u00b7bei", "sind", "die", "Kin\u00b7der\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Alles rollt vorbei, \u2014", "tokens": ["Und", "Al\u00b7les", "rollt", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Geld und die Welt und die Zeiten,", "tokens": ["Das", "Geld", "und", "die", "Welt", "und", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Glauben und Lieb' und Treu'.", "tokens": ["Und", "Glau\u00b7ben", "und", "Lieb'", "und", "Treu'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}