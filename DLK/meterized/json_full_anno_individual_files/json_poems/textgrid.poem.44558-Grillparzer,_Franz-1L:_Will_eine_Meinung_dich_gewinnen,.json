{"textgrid.poem.44558": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Will eine Meinung dich gewinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1848, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will eine Meinung dich gewinnen,", "tokens": ["Will", "ei\u00b7ne", "Mei\u00b7nung", "dich", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00e4llt die Wahl, wie \u00f6fter, schwer,", "tokens": ["Und", "f\u00e4llt", "die", "Wahl", ",", "wie", "\u00f6f\u00b7ter", ",", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So frag, willst du dich recht besinnen,", "tokens": ["So", "frag", ",", "willst", "du", "dich", "recht", "be\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur nach dem Was, dem Wie, dem Wer.", "tokens": ["Nur", "nach", "dem", "Was", ",", "dem", "Wie", ",", "dem", "Wer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "PWAV", "$,", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Das Was? es g\u00e4lte wohl das meiste,", "tokens": ["Das", "Was", "?", "es", "g\u00e4l\u00b7te", "wohl", "das", "meis\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch rein zu l\u00f6sen ist es nie,", "tokens": ["Doch", "rein", "zu", "l\u00f6\u00b7sen", "ist", "es", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zumal bei aufgeregtem Geiste,", "tokens": ["Zu\u00b7mal", "bei", "auf\u00b7ge\u00b7reg\u00b7tem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dann geh du weiter auf das Wie?", "tokens": ["Dann", "geh", "du", "wei\u00b7ter", "auf", "das", "Wie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch welche Mittel sich behaupte", "tokens": ["Durch", "wel\u00b7che", "Mit\u00b7tel", "sich", "be\u00b7haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meinung auf dem Weg zum Ziel?", "tokens": ["Die", "Mei\u00b7nung", "auf", "dem", "Weg", "zum", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sind es schlechte, unerlaubte,", "tokens": ["Und", "sind", "es", "schlech\u00b7te", ",", "un\u00b7er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "p