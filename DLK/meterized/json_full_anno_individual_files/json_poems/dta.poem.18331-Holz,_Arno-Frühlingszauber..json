{"dta.poem.18331": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingszauber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchl's an meines Herzens Schlag,", "tokens": ["Ich", "f\u00fchl's", "an", "mei\u00b7nes", "Her\u00b7zens", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00f6ner wird's an allen Enden", "tokens": ["Und", "sch\u00f6\u00b7ner", "wird's", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und lieblicher mit jedem Tag.", "tokens": ["Und", "lieb\u00b7li\u00b7cher", "mit", "je\u00b7dem", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Liebe schn\u00fcrt ihr rothes Mieder,", "tokens": ["Die", "Lie\u00b7be", "schn\u00fcrt", "ihr", "ro\u00b7thes", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Armuth schmeckt ihr trocknes Brod", "tokens": ["Der", "Ar\u00b7muth", "schmeckt", "ihr", "trock\u00b7nes", "Brod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00fc\u00df klingt's n\u00e4chtlich aus dem Flieder:", "tokens": ["Und", "s\u00fc\u00df", "klingt's", "n\u00e4cht\u00b7lich", "aus", "dem", "Flie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Fr\u00fchling l\u00e4chelt selbst der Tod!", "tokens": ["Im", "Fr\u00fch\u00b7ling", "l\u00e4\u00b7chelt", "selbst", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}