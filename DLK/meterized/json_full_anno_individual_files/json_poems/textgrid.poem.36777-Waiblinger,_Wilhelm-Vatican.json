{"textgrid.poem.36777": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vatican", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's denn wirklich so gro\u00df, das vatican'sche Museum,", "tokens": ["Ist's", "denn", "wirk\u00b7lich", "so", "gro\u00df", ",", "das", "va\u00b7ti\u00b7can'\u00b7sche", "Mu\u00b7se\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie viel h\u00e4tte man denn n\u00f6thig, es ganz zu durchgehn?", "tokens": ["Wie", "viel", "h\u00e4t\u00b7te", "man", "denn", "n\u00f6\u00b7thig", ",", "es", "ganz", "zu", "durch\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PIS", "ADV", "ADJD", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbwohl drei Stunden, mein Herr, doch die Kunst \u2013\u00ab Ich bin ein ber\u00fchmter", "tokens": ["\u00bb", "wohl", "drei", "Stun\u00b7den", ",", "mein", "Herr", ",", "doch", "die", "Kunst", "\u2013", "\u00ab", "Ich", "bin", "ein", "be\u00b7r\u00fchm\u00b7ter"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "CARD", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "$(", "$(", "PPER", "VAFIN", "ART", "ADJA"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "L\u00e4ufer, basta, und so komm' ich in anderthalb durch.", "tokens": ["L\u00e4u\u00b7fer", ",", "bas\u0