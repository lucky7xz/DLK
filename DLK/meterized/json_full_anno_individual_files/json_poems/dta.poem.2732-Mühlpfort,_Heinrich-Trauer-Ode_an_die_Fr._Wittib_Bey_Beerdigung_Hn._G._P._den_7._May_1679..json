{"dta.poem.2732": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trauer-Ode an die Fr. Wittib/  \n Bey Beerdigung Hn. G. P. den 7. May  \n 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jetzt gehst du aus des Kerckers Nacht", "tokens": ["Jetzt", "gehst", "du", "aus", "des", "Ker\u00b7ckers", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bist gantz frey/ die Fessel springen/", "tokens": ["Und", "bist", "gantz", "frey", "/", "die", "Fes\u00b7sel", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Band und Ketten lo\u00df gemacht/", "tokens": ["Von", "Band", "und", "Ket\u00b7ten", "lo\u00df", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kanst nun ein Triumf-Lied singen.", "tokens": ["Und", "kanst", "nun", "ein", "Tri\u00b7um\u00b7f\u00b7Lied", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du schickst ja nur den Leib zu Grabe", "tokens": ["Du", "schickst", "ja", "nur", "den", "Leib", "zu", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Leib/ der faulen mu\u00df ein Raub der Mott\u2019 und Schabe.", "tokens": ["Den", "Leib", "/", "der", "fau\u00b7len", "mu\u00df", "ein", "Raub", "der", "Mott'", "und", "Scha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVINF", "VMFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Wohnhau\u00df/ das so manche Noth", "tokens": ["Dein", "Wohn\u00b7hau\u00df", "/", "das", "so", "man\u00b7che", "Noth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PDS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und hei\u00df entbranter Schmertz bestritten/", "tokens": ["Und", "hei\u00df", "ent\u00b7bran\u00b7ter", "Schmertz", "be\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Darinnen du nicht einen Tod", "tokens": ["Da\u00b7rin\u00b7nen", "du", "nicht", "ei\u00b7nen", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ja wol viel tausend hast erlitten/", "tokens": ["Ja", "wol", "viel", "tau\u00b7send", "hast", "er\u00b7lit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "VVPP", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Den Nothstall l\u00e4ssest du jetzt stehen", "tokens": ["Den", "Noth\u00b7stall", "l\u00e4s\u00b7sest", "du", "jetzt", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wilst in ein Gemach wol aus zuruhen gehen.", "tokens": ["Und", "wilst", "in", "ein", "Ge\u00b7mach", "wol", "aus", "zu\u00b7ru\u00b7hen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach freylich suchst du nichts als Ruh/", "tokens": ["Ach", "frey\u00b7lich", "suchst", "du", "nichts", "als", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PIS", "KOKOM", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Wenn dir nicht einen Schlaff lie\u00df zu", "tokens": ["Wenn", "dir", "nicht", "ei\u00b7nen", "Schlaff", "lie\u00df", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die Gicht/ so peinlich dich gequ\u00e4let/", "tokens": ["Die", "Gicht", "/", "so", "pein\u00b7lich", "dich", "ge\u00b7qu\u00e4\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und bey Verlauff so vieler Jahre", "tokens": ["Und", "bey", "Ver\u00b7lauff", "so", "vie\u00b7ler", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dein Bette dir verkehrt in eine Todten-Baare.", "tokens": ["Dein", "Bet\u00b7te", "dir", "ver\u00b7kehrt", "in", "ei\u00b7ne", "Tod\u00b7ten\u00b7Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Japoneser Folter-Banck/", "tokens": ["Der", "Ja\u00b7po\u00b7ne\u00b7ser", "Fol\u00b7ter\u00b7Banck", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Und was von Martern nur zu finden/", "tokens": ["Und", "was", "von", "Mar\u00b7tern", "nur", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Jhr Singkod oder H\u00f6llen-Tranck", "tokens": ["Ihr", "Sing\u00b7kod", "o\u00b7der", "H\u00f6l\u00b7len\u00b7Tranck"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der durch sein Brennen pflag zu schinden/", "tokens": ["Der", "durch", "sein", "Bren\u00b7nen", "pflag", "zu", "schin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Sind Julep f\u00fcr den Angst-Geschirren", "tokens": ["Sind", "Ju\u00b7lep", "f\u00fcr", "den", "Angst\u00b7Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die t\u00e4glich dich getr\u00e4nckt mit mehr als Gall und Myrch\u1ebd.", "tokens": ["Die", "t\u00e4g\u00b7lich", "dich", "ge\u00b7tr\u00e4nckt", "mit", "mehr", "als", "Gall", "und", "My\u00b7rch\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "APPR", "PIAT", "KOKOM", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie nenn ich deine Lagerstadt", "tokens": ["Wie", "nenn", "ich", "dei\u00b7ne", "La\u00b7ger\u00b7stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die zugericht von Dorn und Hecken?", "tokens": ["Die", "zu\u00b7ge\u00b7richt", "von", "Dorn", "und", "He\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "War es Jxions Marter-Rad?", "tokens": ["War", "es", "Jxions", "Mar\u00b7ter\u00b7Rad", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.28": {"text": "Der Fel\u00df wo Titius sich strecken", "tokens": ["Der", "Fel\u00df", "wo", "Ti\u00b7tius", "sich", "stre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PWAV", "NE", "PRF", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Den Geyern mu\u00df zu neuen Plagen/", "tokens": ["Den", "Ge\u00b7yern", "mu\u00df", "zu", "neu\u00b7en", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Der t\u00e4glich wird verzehrt und doch nicht weggetragen.", "tokens": ["Der", "t\u00e4g\u00b7lich", "wird", "ver\u00b7zehrt", "und", "doch", "nicht", "weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "KON", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was Nero hat an Grausamkeit/", "tokens": ["Was", "Ne\u00b7ro", "hat", "an", "Grau\u00b7sam\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und Menschen Pein zu Rom begangen/", "tokens": ["Und", "Men\u00b7schen", "Pein", "zu", "Rom", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Caligula nur zubereit", "tokens": ["Ca\u00b7li\u00b7gu\u00b7la", "nur", "zu\u00b7be\u00b7reit"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Dem langsam T\u00f6dten ein Verlangen/", "tokens": ["Dem", "lang\u00b7sam", "T\u00f6d\u00b7ten", "ein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Und Hertzens-Lust sehn in dem Sterben", "tokens": ["Und", "Hert\u00b7zens\u00b7Lust", "sehn", "in", "dem", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wie sich der arme Mensch in letzter Angst mu\u00df f\u00e4rben.", "tokens": ["Wie", "sich", "der", "ar\u00b7me", "Mensch", "in", "letz\u00b7ter", "Angst", "mu\u00df", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das gliech doch deinen Schmertzen nicht", "tokens": ["Das", "gliech", "doch", "dei\u00b7nen", "Schmert\u00b7zen", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Die schnelle Zeit macht offt ein Ende.", "tokens": ["Die", "schnel\u00b7le", "Zeit", "macht", "offt", "ein", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Hier aber schn\u00fcrte dir die Gicht", "tokens": ["Hier", "a\u00b7ber", "schn\u00fcr\u00b7te", "dir", "die", "Gicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "So unauffh\u00f6rlich F\u00fc\u00df\u2019 und H\u00e4nde/", "tokens": ["So", "un\u00b7auff\u00b7h\u00f6r\u00b7lich", "F\u00fc\u00df'", "und", "H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Da\u00df auch den unbelebten Steinen", "tokens": ["Da\u00df", "auch", "den", "un\u00b7be\u00b7leb\u00b7ten", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", 