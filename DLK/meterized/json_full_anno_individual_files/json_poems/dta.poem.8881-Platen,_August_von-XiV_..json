{"dta.poem.8881": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiV .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist die Rose nur, von dir gebrochen,", "tokens": ["Sch\u00f6n", "ist", "die", "Ro\u00b7se", "nur", ",", "von", "dir", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ein Gedicht nur sch\u00f6n, von dir gesprochen:", "tokens": ["Und", "ein", "Ge\u00b7dicht", "nur", "sch\u00f6n", ",", "von", "dir", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Todt ist die Welt, du bist allein am Leben.", "tokens": ["Todt", "ist", "die", "Welt", ",", "du", "bist", "al\u00b7lein", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "In diesen Lauben, die sich hold verweben,", "tokens": ["In", "die\u00b7sen", "Lau\u00b7ben", ",", "die", "sich", "hold", "ver\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird ohne dich mir jeder Tag zu Wochen,", "tokens": ["Wird", "oh\u00b7ne", "dich", "mir", "je\u00b7der", "Tag", "zu", "Wo\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PPER", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieser Wein, den warme Sonnen kochen,", "tokens": ["Und", "die\u00b7ser", "Wein", ",", "den", "war\u00b7me", "Son\u00b7nen", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kann nur aus deiner Hand ein Herz beleben.", "tokens": ["Kann", "nur", "aus", "dei\u00b7ner", "Hand", "ein", "Herz", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von dir geschieden, trenn' ich mich vom Gl\u00fccke,", "tokens": ["Von", "dir", "ge\u00b7schie\u00b7den", ",", "trenn'", "ich", "mich", "vom", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Sch\u00f6nste dient mir nur, mich zu zerstreuen,", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "dient", "mir", "nur", ",", "mich", "zu", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Gr\u00f6\u00dfte f\u00fcllt mir kaum des Innern L\u00fccke.", "tokens": ["Das", "Gr\u00f6\u00df\u00b7te", "f\u00fcllt", "mir", "kaum", "des", "In\u00b7nern", "L\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch dr\u00fcckst du mich an deine Brust, den Treuen,", "tokens": ["Doch", "dr\u00fcckst", "du", "mich", "an", "dei\u00b7ne", "Brust", ",", "den", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann kehrt die Welt in meine Brust zur\u00fccke,", "tokens": ["Dann", "kehrt", "die", "Welt", "in", "mei\u00b7ne", "Brust", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und am Geringsten kann ich mich erfreuen.", "tokens": ["Und", "am", "Ge\u00b7rings\u00b7ten", "kann", "ich", "mich", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}