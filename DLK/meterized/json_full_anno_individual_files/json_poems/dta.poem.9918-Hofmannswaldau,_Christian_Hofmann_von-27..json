{"dta.poem.9918": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein jahr zu ende laufft/ haben wir auch unser ende/", "tokens": ["Wenn", "ein", "jahr", "zu", "en\u00b7de", "laufft", "/", "ha\u00b7ben", "wir", "auch", "un\u00b7ser", "en\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Narren kommen wir so wohl/ als den kl\u00fcgsten/ in die h\u00e4nde:", "tokens": ["Nar\u00b7ren", "kom\u00b7men", "wir", "so", "wohl", "/", "als", "den", "kl\u00fcgs\u00b7ten", "/", "in", "die", "h\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "ART", "ADJA", "$(", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Man beschuldigt uns zwar offt/ da\u00df wir unwahr reden sollen/", "tokens": ["Man", "be\u00b7schul\u00b7digt", "uns", "zwar", "offt", "/", "da\u00df", "wir", "un\u00b7wahr", "re\u00b7den", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da von uns nichts wird gesagt/ als was unsre meister wollen.", "tokens": ["Da", "von", "uns", "nichts", "wird", "ge\u00b7sagt", "/", "als", "was", "uns\u00b7re", "meis\u00b7ter", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PIS", "VAFIN", "VVPP", "$(", "KOUS", "PIS", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}