{"dta.poem.7544": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Milden sprach ein Freund: Du mu\u00dft die Mild' ablegen,", "tokens": ["Zum", "Mil\u00b7den", "sprach", "ein", "Freund", ":", "Du", "mu\u00dft", "die", "Mild'", "ab\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.2": {"text": "Die dich verarmen macht. Der Milde sprach dagegen:", "tokens": ["Die", "dich", "ver\u00b7ar\u00b7men", "macht", ".", "Der", "Mil\u00b7de", "sprach", "da\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "$.", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zur Milde hab' ich mich gew\u00f6hnt nach Gottes Bilde,", "tokens": ["Zur", "Mil\u00b7de", "hab'", "ich", "mich", "ge\u00b7w\u00f6hnt", "nach", "Got\u00b7tes", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Mild' hat sich gew\u00f6hnt an meine Milde.", "tokens": ["Und", "sei\u00b7ne", "Mild'", "hat", "sich", "ge\u00b7w\u00f6hnt", "an", "mei\u00b7ne", "Mil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", 