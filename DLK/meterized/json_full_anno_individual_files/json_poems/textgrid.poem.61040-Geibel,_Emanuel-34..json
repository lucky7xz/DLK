{"textgrid.poem.61040": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist das Gl\u00fcck ein fl\u00fcchtig Ding", "tokens": ["Es", "ist", "das", "Gl\u00fcck", "ein", "fl\u00fcch\u00b7tig", "Ding"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und war's zu allen Tagen;", "tokens": ["Und", "wa\u00b7r's", "zu", "al\u00b7len", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und jagtest du um der Erde Ring,", "tokens": ["Und", "jag\u00b7test", "du", "um", "der", "Er\u00b7de", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du m\u00f6chtest es nicht erjagen.", "tokens": ["Du", "m\u00f6ch\u00b7test", "es", "nicht", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Leg' dich lieber ins Gras voll Duft", "tokens": ["Leg'", "dich", "lie\u00b7ber", "ins", "Gras", "voll", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "ADJD", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und singe deine Lieder;", "tokens": ["Und", "sin\u00b7ge", "dei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Pl\u00f6tzlich vielleicht aus blauer Luft", "tokens": ["Pl\u00f6tz\u00b7lich", "viel\u00b7leicht", "aus", "blau\u00b7er", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00e4llt es auf dich hernieder.", "tokens": ["F\u00e4llt", "es", "auf", "dich", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aber dann pack' es und halt es fest", "tokens": ["A\u00b7ber", "dann", "pack'", "es", "und", "halt", "es", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und plaudre nicht viel dazwischen;", "tokens": ["Und", "plaud\u00b7re", "nicht", "viel", "da\u00b7zwi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PAV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn du zu lang es warten l\u00e4\u00dft,", "tokens": ["Wenn", "du", "zu", "lang", "es", "war\u00b7ten", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "M\u00f6cht' es dir wieder entwischen.", "tokens": ["M\u00f6cht'", "es", "dir", "wie\u00b7der", "ent\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "pu