{"textgrid.poem.39672": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Die Bande der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! mein Geliebter ist tod! er wandelt im Lande der Schatten", "tokens": ["Ach", "!", "mein", "Ge\u00b7lieb\u00b7ter", "ist", "tod", "!", "er", "wan\u00b7delt", "im", "Lan\u00b7de", "der", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sterne leuchten ihm nicht, ihm ergl\u00e4nzet kein Tag", "tokens": ["Ster\u00b7ne", "leuch\u00b7ten", "ihm", "nicht", ",", "ihm", "er\u00b7gl\u00e4n\u00b7zet", "kein", "Tag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und ihm schweigt die Geschichte; das Schicksal der Zeiten", "tokens": ["Und", "ihm", "schweigt", "die", "Ge\u00b7schich\u00b7te", ";", "das", "Schick\u00b7sal", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Gehet den m\u00e4chtigen Gang, doch ihn erwecket es nicht;", "tokens": ["Ge\u00b7het", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Gang", ",", "doch", "ihn", "er\u00b7we\u00b7cket", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Alles starb ihm mit ihm, mir ist er doch nicht gestorben", "tokens": ["Al\u00b7les", "starb", "ihm", "mit", "ihm", ",", "mir", "ist", "er", "doch", "nicht", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP"], "meter": "+-+-+-+---+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Denn ein ewiges Band eint mir noch immer den Freund.", "tokens": ["Denn", "ein", "e\u00b7wi\u00b7ges", "Band", "eint", "mir", "noch", "im\u00b7mer", "den", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Liebe hei\u00dfet dies Band, das an den Tag mir gekn\u00fcpft", "tokens": ["Lie\u00b7be", "hei\u00b7\u00dfet", "dies", "Band", ",", "das", "an", "den", "Tag", "mir", "ge\u00b7kn\u00fcpft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PDS", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVPP"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Hat die erebische Nacht, Tod mit dem Leben vereint.", "tokens": ["Hat", "die", "e\u00b7re\u00b7bi\u00b7sche", "Nacht", ",", "Tod", "mit", "dem", "Le\u00b7ben", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ja ich kenne ein Land, wo Todte zu Lebenden reden,", "tokens": ["Ja", "ich", "ken\u00b7ne", "ein", "Land", ",", "wo", "Tod\u00b7te", "zu", "Le\u00b7ben\u00b7den", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wo sie, dem Orkus entflohn, wieder sich freuen des Lichts,", "tokens": ["Wo", "sie", ",", "dem", "Or\u00b7kus", "ent\u00b7flohn", ",", "wie\u00b7der", "sich", "freu\u00b7en", "des", "Lichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ART", "NN", "PTKVZ", "$,", "ADV", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wo von Erinn'rung erweckt, sie auferstehn von den Todten", "tokens": ["Wo", "von", "Er\u00b7inn'\u00b7rung", "er\u00b7weckt", ",", "sie", "auf\u00b7er\u00b7stehn", "von", "den", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wo ein irdisches Licht gl\u00fchet im Leichengewand.", "tokens": ["Wo", "ein", "ir\u00b7di\u00b7sches", "Licht", "gl\u00fc\u00b7het", "im", "Lei\u00b7chen\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Seliges Land der Tr\u00e4ume! wo, mit Lebendigen, Todte", "tokens": ["Se\u00b7li\u00b7ges", "Land", "der", "Tr\u00e4u\u00b7me", "!", "wo", ",", "mit", "Le\u00b7ben\u00b7di\u00b7gen", ",", "Tod\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$.", "PWAV", "$,", "APPR", "NN", "$,", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.14": {"text": "Wandeln, im D\u00e4mmerschein, freuen des Daseyns sich noch.", "tokens": ["Wan\u00b7deln", ",", "im", "D\u00e4m\u00b7mer\u00b7schein", ",", "freu\u00b7en", "des", "Da\u00b7seyns", "sich", "noch", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Dort, in dem gl\u00fccklichen Land, begegnet mir wieder der Theure,", "tokens": ["Dort", ",", "in", "dem", "gl\u00fcck\u00b7li\u00b7chen", "Land", ",", "be\u00b7geg\u00b7net", "mir", "wie\u00b7der", "der", "Theu\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Freuet, der Liebe, sich meiner Umarmungen noch;", "tokens": ["Freu\u00b7et", ",", "der", "Lie\u00b7be", ",", "sich", "mei\u00b7ner", "Um\u00b7ar\u00b7mun\u00b7gen", "noch", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "$,", "PRF", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Und ich hauche die Kraft der Jugend dann in den Schatten,", "tokens": ["Und", "ich", "hau\u00b7che", "die", "Kraft", "der", "Ju\u00b7gend", "dann", "in", "den", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Da\u00df ein lebendig Roth wieder die Wange ihm f\u00e4rbt,", "tokens": ["Da\u00df", "ein", "le\u00b7ben\u00b7dig", "Roth", "wie\u00b7der", "die", "Wan\u00b7ge", "ihm", "f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Da\u00df die erstarreten Pulse vom warmen Hauche sich regen,", "tokens": ["Da\u00df", "die", "er\u00b7star\u00b7re\u00b7ten", "Pul\u00b7se", "vom", "war\u00b7men", "Hau\u00b7che", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Und der Liebe Gef\u00fchl wieder den Busen ihm hebt.", "tokens": ["Und", "der", "Lie\u00b7be", "Ge\u00b7f\u00fchl", "wie\u00b7der", "den", "Bu\u00b7sen", "ihm", "hebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Darum fraget nicht, Gespielen! was ich so bebe?", "tokens": ["Da\u00b7rum", "fra\u00b7get", "nicht", ",", "Ge\u00b7spie\u00b7len", "!", "was", "ich", "so", "be\u00b7be", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "NN", "$.", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Warum das rosigte Roth l\u00f6scht ein ert\u00f6dtendes Bla\u00df?", "tokens": ["Wa\u00b7rum", "das", "ro\u00b7sig\u00b7te", "Roth", "l\u00f6scht", "ein", "er\u00b7t\u00f6d\u00b7ten\u00b7des", "Bla\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Theil ich mein Leben doch mit unterirdischen Schatten,", "tokens": ["Theil", "ich", "mein", "Le\u00b7ben", "doch", "mit", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Meiner Jugend Kraft schl\u00fcrfen sie gierig mir aus.", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Kraft", "schl\u00fcr\u00b7fen", "sie", "gie\u00b7rig", "mir", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ach! mein Geliebter ist tod! er wandelt im Lande der Schatten", "tokens": ["Ach", "!", "mein", "Ge\u00b7lieb\u00b7ter", "ist", "tod", "!", "er", "wan\u00b7delt", "im", "Lan\u00b7de", "der", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VAFIN", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sterne leuchten ihm nicht, ihm ergl\u00e4nzet kein Tag", "tokens"