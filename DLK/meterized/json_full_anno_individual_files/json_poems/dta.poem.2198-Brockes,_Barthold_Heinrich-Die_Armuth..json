{"dta.poem.2198": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Armuth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nur von sich allen", "tokens": ["Wer", "nur", "von", "sich", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PRF", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verlangt geehrt zu seyn;", "tokens": ["Ver\u00b7langt", "ge\u00b7ehrt", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4\u00dft noch den gr\u00f6\u00dften Hochmuth sehn,", "tokens": ["L\u00e4\u00dft", "noch", "den", "gr\u00f6\u00df\u00b7ten", "Hoch\u00b7muth", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indem, da er sich selber zu erh\u00f6hn", "tokens": ["In\u00b7dem", ",", "da", "er", "sich", "sel\u00b7ber", "zu", "er\u00b7h\u00f6hn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit Ausschlu\u00df aller andern trachtet,", "tokens": ["Mit", "Aus\u00b7schlu\u00df", "al\u00b7ler", "an\u00b7dern", "trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Er alle andere verachtet.", "tokens": ["Er", "al\u00b7le", "an\u00b7de\u00b7re", "ver\u00b7ach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}