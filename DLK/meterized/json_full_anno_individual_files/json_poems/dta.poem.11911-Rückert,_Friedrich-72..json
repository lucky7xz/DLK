{"dta.poem.11911": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur wer daheim ist, mag wol einen Gast empfangen,", "tokens": ["Nur", "wer", "da\u00b7heim", "ist", ",", "mag", "wol", "ei\u00b7nen", "Gast", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "VAFIN", "$,", "VMFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wer auf Reisen selbst ist in die Welt gegangen.", "tokens": ["Nicht", "wer", "auf", "Rei\u00b7sen", "selbst", "ist", "in", "die", "Welt", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "APPR", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur wer allein ist, mag empfangen wohl den Gast,", "tokens": ["Nur", "wer", "al\u00b7lein", "ist", ",", "mag", "emp\u00b7fan\u00b7gen", "wohl", "den", "Gast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADV", "VAFIN", "$,", "VMFIN", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wenn das ganze Haus du schon voll G\u00e4ste hast.", "tokens": ["Nicht", "wenn", "das", "gan\u00b7ze", "Haus", "du", "schon", "voll", "G\u00e4s\u00b7te", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei immer nur daheim, allein und unbeklommen;", "tokens": ["Sei", "im\u00b7mer", "nur", "da\u00b7heim", ",", "al\u00b7lein", "und", "un\u00b7be\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "$,", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir wird der Himmelsgast, den du erwartest, kommen.", "tokens": ["Dir", "wird", "der", "Him\u00b7mels\u00b7gast", ",", "den", "du", "er\u00b7war\u00b7test", ",", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}