": "Stahl und Felsen harte Hertzen/", "tokens": ["Stahl", "und", "Fel\u00b7sen", "har\u00b7te", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht eur holder Blick gleich weich/", "tokens": ["Macht", "eur", "hol\u00b7der", "Blick", "gleich", "weich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch empfindt man keine Schmertzen/", "tokens": ["Doch", "emp\u00b7findt", "man", "kei\u00b7ne", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den er ist dem Blitze gleich.", "tokens": ["Den", "er", "ist", "dem", "Blit\u00b7ze", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn man nur von ferue schauet/", "tokens": ["Wenn", "man", "nur", "von", "fe\u00b7rue", "schau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eurer Augen Wunder-Licht/", "tokens": ["Eu\u00b7rer", "Au\u00b7gen", "Wun\u00b7der\u00b7Licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird ein Tempel ihm gebauet/", "tokens": ["Wird", "ein", "Tem\u00b7pel", "ihm", "ge\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFI