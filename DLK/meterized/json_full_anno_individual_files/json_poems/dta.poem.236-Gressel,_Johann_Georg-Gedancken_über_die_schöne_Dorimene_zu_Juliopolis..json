{"dta.poem.236": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken \u00fcber die sch\u00f6ne  Dorimene  \n zu  Juliopolis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dorimene eures gleichen/", "tokens": ["Do\u00b7ri\u00b7me\u00b7ne", "eu\u00b7res", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Findet man gar keine hier/", "tokens": ["Fin\u00b7det", "man", "gar", "kei\u00b7ne", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PIAT", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Sterne m\u00fcssen weichen", "tokens": ["Wie", "die", "Ster\u00b7ne", "m\u00fcs\u00b7sen", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die Sonne steigt herf\u00fcr/", "tokens": ["Wenn", "die", "Son\u00b7ne", "steigt", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So stirbt auch der andern Schimmer/", "tokens": ["So", "stirbt", "auch", "der", "an\u00b7dern", "Schim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die noch etwas sch\u00f6ne sind", "tokens": ["Die", "noch", "et\u00b7was", "sch\u00f6\u00b7ne", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "ADJA", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn bey diesen Frauen-Zimmer", "tokens": ["Wenn", "bey", "die\u00b7sen", "Frau\u00b7en\u00b7Zim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Man euch ", "tokens": ["Man", "euch"], "token_info": ["word", "word"], "pos": ["PIS", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Wer euch einen Menschen nennet/", "tokens": ["Wer", "euch", "ei\u00b7nen", "Men\u00b7schen", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen Witz im Haupte f\u00fchrt/", "tokens": ["Kei\u00b7nen", "Witz", "im", "Haup\u00b7te", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er keinen Engel kennet/", "tokens": ["Weil", "er", "kei\u00b7nen", "En\u00b7gel", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den des Himmels-Schmuck beziert/", "tokens": ["Den", "des", "Him\u00b7mels\u00b7Schmuck", "be\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was die G\u00f6tter an sich haben/", "tokens": ["Was", "die", "G\u00f6t\u00b7ter", "an", "sich", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was \u00fcber irrdisch hei\u00dft/", "tokens": ["Und", "was", "\u00fc\u00b7ber", "irr\u00b7disch", "hei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Findet man nebst tausend Gaben/", "tokens": ["Fin\u00b7det", "man", "nebst", "tau\u00b7send", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "CARD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An dem tugendhafftem Geist.", "tokens": ["An", "dem", "tu\u00b7gend\u00b7haff\u00b7tem", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie die g\u00fcldne Sonne blendet/", "tokens": ["Wie", "die", "g\u00fcld\u00b7ne", "Son\u00b7ne", "blen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man starre in sie sieht/", "tokens": ["Wenn", "man", "star\u00b7re", "in", "sie", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So eur Glantz die Freyheit endet/", "tokens": ["So", "eur", "Glantz", "die", "Frey\u00b7heit", "en\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Hertzen nach sich zieht/", "tokens": ["Und", "die", "Hert\u00b7zen", "nach", "sich", "zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df einjeder mu\u00df gestehen/", "tokens": ["Da\u00df", "ein\u00b7je\u00b7der", "mu\u00df", "ge\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ihr unvergleichlich seyd/", "tokens": ["Da\u00df", "ihr", "un\u00b7ver\u00b7gleich\u00b7lich", "seyd", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wer euch nur kriegt zusehen/", "tokens": ["Und", "wer", "euch", "nur", "kriegt", "zu\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "R\u00fchmt dis Wunder dieser Zeit.", "tokens": ["R\u00fchmt", "dis", "Wun\u00b7der", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie die hohen ", "tokens": ["Wie", "die", "ho\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Jhr begr\u00fcntes Haupt empor/", "tokens": ["Ihr", "be\u00b7gr\u00fcn\u00b7tes", "Haupt", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "