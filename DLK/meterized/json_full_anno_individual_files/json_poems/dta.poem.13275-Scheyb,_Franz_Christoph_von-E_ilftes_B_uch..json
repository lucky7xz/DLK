{"dta.poem.13275": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehilfft seine Stimme nichts, so zeiget sie doch oft,", "tokens": ["\u201e", "hilfft", "sei\u00b7ne", "Stim\u00b7me", "nichts", ",", "so", "zei\u00b7get", "sie", "doch", "oft", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "PIS", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eda\u00df er den Zufall sieht, den er zuvor gehofft.", "tokens": ["\u201e", "da\u00df", "er", "den", "Zu\u00b7fall", "sieht", ",", "den", "er", "zu\u00b7vor", "ge\u00b7hofft", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewahr ists: sein Dencken geht nicht nach erleucht\u2019ten Schl\u00fcssen;", "tokens": ["\u201e", "wahr", "ists", ":", "sein", "Den\u00b7cken", "geht", "nicht", "nach", "er\u00b7leucht'\u00b7ten", "Schl\u00fcs\u00b7sen", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$.", "PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edoch hat man oft von ihm die Wahrheit lernen m\u00fcssen.", "tokens": ["\u201e", "doch", "hat", "man", "oft", "von", "ihm", "die", "Wahr\u00b7heit", "ler\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIS", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eoft schwingt sich sein Geschrey bi\u00df in den K\u00f6nigs-Saal", "tokens": ["\u201e", "oft", "schwingt", "sich", "sein", "Ge\u00b7schrey", "bi\u00df", "in", "den", "K\u00f6\u00b7nigs\u00b7Saal"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "570\u201dUnd \u00e4ndert, was man dort nach reiffem Rath befahl.", "tokens": ["\"", "Und", "\u00e4n\u00b7dert", ",", "was", "man", "dort", "nach", "reif\u00b7fem", "Rath", "be\u00b7fahl", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "PRELS", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eso mu\u00df man solchen Ruff in Werth und Unwerth lassen,", "tokens": ["\u201e", "so", "mu\u00df", "man", "sol\u00b7chen", "Ruff", "in", "Werth", "und", "Un\u00b7werth", "las\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "PIAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund sich auf jenen Fall, von dem er schreyet, fassen.", "tokens": ["\u201e", "und", "sich", "auf", "je\u00b7nen", "Fall", ",", "von", "dem", "er", "schre\u00b7yet", ",", "fas\u00b7sen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "PRF", "APPR", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}