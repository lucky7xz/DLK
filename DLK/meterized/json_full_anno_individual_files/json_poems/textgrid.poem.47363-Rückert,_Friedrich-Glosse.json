{"textgrid.poem.47363": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Glosse", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie hat nicht Lust, mich freizulassen,", "tokens": ["Sie", "hat", "nicht", "Lust", ",", "mich", "frei\u00b7zu\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch Lust, auch mich ans Herz zu fassen.", "tokens": ["Noch", "Lust", ",", "auch", "mich", "ans", "Herz", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Vogel gleich im Vogelbauer,", "tokens": ["Dem", "Vo\u00b7gel", "gleich", "im", "Vo\u00b7gel\u00b7bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag und Nacht von Liebe singt,", "tokens": ["Der", "Tag", "und", "Nacht", "von", "Lie\u00b7be", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, ob's ihr nicht zu Herzen dringt,", "tokens": ["Der", ",", "ob's", "ihr", "nicht", "zu", "Her\u00b7zen", "dringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+