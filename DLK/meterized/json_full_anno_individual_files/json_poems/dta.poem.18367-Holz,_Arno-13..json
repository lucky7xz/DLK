{"dta.poem.18367": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Sein silbergraues Zwielicht spann,", "tokens": ["Sein", "sil\u00b7ber\u00b7grau\u00b7es", "Zwie\u00b7licht", "spann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war der arme, bleiche Sch\u00e4cher", "tokens": ["Da", "war", "der", "ar\u00b7me", ",", "blei\u00b7che", "Sch\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein stummer und ein stiller Mann.", "tokens": ["Ein", "stum\u00b7mer", "und", "ein", "stil\u00b7ler", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seines Mantels grauen Falten,", "tokens": ["In", "sei\u00b7nes", "Man\u00b7tels", "grau\u00b7en", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So lag er da, kalt und entstellt \u2014", "tokens": ["So", "lag", "er", "da", ",", "kalt", "und", "ent\u00b7stellt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ADJD", "KON", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "F\u00fcrwahr, er hatte Recht behalten,", "tokens": ["F\u00fcr\u00b7wahr", ",", "er", "hat\u00b7te", "Recht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Reich war nicht von dieser Welt!", "tokens": ["Sein", "Reich", "war", "nicht", "von", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein goldnes Sonnenst\u00e4ubchen tippte", "tokens": ["Ein", "gold\u00b7nes", "Son\u00b7nen\u00b7st\u00e4ub\u00b7chen", "tipp\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm auf die Stirn von ungef\u00e4hr", "tokens": ["Ihm", "auf", "die", "Stirn", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine lieben Manuscripte", "tokens": ["Und", "sei\u00b7ne", "lie\u00b7ben", "Ma\u00b7nus\u00b7crip\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verschlo\u00df der Armencommiss\u00e4r.", "tokens": ["Ver\u00b7schlo\u00df", "der", "Ar\u00b7men\u00b7com\u00b7mis\u00b7s\u00e4r", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Freund, der Doctor, aber zierte", "tokens": ["Sein", "Freund", ",", "der", "Doc\u00b7tor", ",", "a\u00b7ber", "zier\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Brutal sich durch das K\u00e4mmerlein", "tokens": ["Bru\u00b7tal", "sich", "durch", "das", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schneuzte sich und constatirte", "tokens": ["Und", "schneuz\u00b7te", "sich", "und", "cons\u00b7ta\u00b7tir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u201everhungert!\u201c auf dem Todtenschein.", "tokens": ["\u201e", "ver\u00b7hun\u00b7gert", "!", "\u201c", "auf", "dem", "Tod\u00b7ten\u00b7schein", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drei Fr\u00fchlingstage sp\u00e4ter karrten", "tokens": ["Drei", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge", "sp\u00e4\u00b7ter", "karr\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn Armenklepper vor das Thor!", "tokens": ["Ihn", "Ar\u00b7men\u00b7klep\u00b7per", "vor", "das", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah's noch, wie sie ihn verscharrten \u2014", "tokens": ["Ich", "sah's", "noch", ",", "wie", "sie", "ihn", "ver\u00b7scharr\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sonne lachte, doch mich fror!", "tokens": ["Die", "Son\u00b7ne", "lach\u00b7te", ",", "doch", "mich", "fror", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich fror und meine H\u00e4nde suchten", "tokens": ["Mich", "fror", "und", "mei\u00b7ne", "H\u00e4n\u00b7de", "such\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umsonst zu w\u00fcrgen meinen Schmerz", "tokens": ["Um\u00b7sonst", "zu", "w\u00fcr\u00b7gen", "mei\u00b7nen", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und meine bleichen Lippen fluchten ...", "tokens": ["Und", "mei\u00b7ne", "blei\u00b7chen", "Lip\u00b7pen", "fluch\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Gott, mein Herz! mein armes Herz!", "tokens": ["O", "Gott", ",", "mein", "Herz", "!", "mein", "ar\u00b7mes", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So stand ich und vermaledeite", "tokens": ["So", "stand", "ich", "und", "ver\u00b7ma\u00b7le\u00b7dei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt bis in ihr Nichts hinab;", "tokens": ["Die", "Welt", "bis", "in", "ihr", "Nichts", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der goldne Fr\u00fchling aber schneite", "tokens": ["Der", "gold\u00b7ne", "Fr\u00fch\u00b7ling", "a\u00b7ber", "schnei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm l\u00e4chelnd Rosen \u00fcbers Grab.", "tokens": ["Ihm", "l\u00e4\u00b7chelnd", "Ro\u00b7sen", "\u00fc\u00b7bers", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon nahten unsichtbaren Zuges", "tokens": ["Schon", "nah\u00b7ten", "un\u00b7sicht\u00b7ba\u00b7ren", "Zu\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gro\u00dfen Geister alter Zeit,", "tokens": ["Die", "gro\u00b7\u00dfen", "Geis\u00b7ter", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dr\u00fcber schwebte leisen Fluges", "tokens": ["Und", "dr\u00fc\u00b7ber", "schweb\u00b7te", "lei\u00b7sen", "Flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Genius der Unsterblichkeit!", "tokens": ["Der", "Ge\u00b7nius", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}