aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Will im Schnee mit mir ziehen zu Wald.", "tokens": ["Will", "im", "Schnee", "mit", "mir", "zie\u00b7hen", "zu", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "APPR", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Willkommen im traulichen J\u00e4gerhaus,", "tokens": ["Will\u00b7kom\u00b7men", "im", "trau\u00b7li\u00b7chen", "J\u00e4\u00b7ger\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So ruf' ich ihr bald, ja bald!", "tokens": ["So", "ruf'", "ich", "ihr", "bald", ",", "ja", "bald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hellauf, mein Gewehr! Einen Freudenschu\u00df", "tokens": ["Hel\u00b7lauf", ",", "mein", "Ge\u00b7wehr", "!", "Ei\u00b7nen", "Freu\u00b7den\u00b7schu\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Durch den Fr\u00fchduft \u00fcber das Thal!", "tokens": ["Durch", "den", "Fr\u00fch\u00b7duft", "\u00fc\u00b7ber", "das", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Liebste kennt meinen Morgengru\u00df,", "tokens": ["Die", "Liebs\u00b7te", "kennt", "mei\u00b7nen", "Mor\u00b7gen\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und verstand ihn vielhundertmal.", "tokens": ["Und", "ver\u00b7stand", "ihn", "viel\u00b7hun\u00b7dert\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und schlummert sie noch, so weck' er sie laut,", "tokens": ["Und", "schlum\u00b7mert", "sie", "noch", ",", "so", "weck'", "er", "sie", "laut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ruf' ihr mit Freuden zu:", "tokens": ["Und", "ruf'", "ihr", "mit", "Freu\u00b7den", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hellauf, du mein Schatz! bist des J\u00e4gers Braut,", "tokens": ["Hel\u00b7lauf", ",", "du", "mein", "Schatz", "!", "bist", "des", "J\u00e4\u00b7gers", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "$.", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Der st\u00f6rt dir noch manche Ruh!", "tokens": ["Der", "st\u00f6rt", "dir", "noch", "man\u00b7che", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "