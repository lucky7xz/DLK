{"textgrid.poem.33191": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2140.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein armes herze sch\u00e4met sich der segen, die sich im chor der ledgen br\u00fcder regen.", "tokens": ["Mein", "ar\u00b7mes", "her\u00b7ze", "sch\u00e4\u00b7met", "sich", "der", "se\u00b7gen", ",", "die", "sich", "im", "chor", "der", "led\u00b7gen", "br\u00fc\u00b7der", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ART", "PPOSS", "$,", "PRELS", "PRF", "APPRART", "NN", "ART", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie manchen hast du seit der zeit erwehlet, und deiner Creuz-Gemeine beygezehlet.", "tokens": ["Wie", "man\u00b7chen", "hast", "du", "seit", "der", "zeit", "er\u00b7weh\u00b7let", ",", "und", "dei\u00b7ner", "Creuz\u00b7Ge\u00b7mei\u00b7ne", "bey\u00b7ge\u00b7zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Man sieht itzt hundert, wo vor wenig jahren kaum neun und zehn zu finden m\u00f6glich waren.", "tokens": ["Man", "sieht", "itzt", "hun\u00b7dert", ",", "wo", "vor", "we\u00b7nig", "jah\u00b7ren", "kaum", "neun", "und", "zehn", "zu", "fin\u00b7den", "m\u00f6g\u00b7lich", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "CARD", "$,", "PWAV", "APPR", "PIAT", "NN", "ADV", "CARD", "KON", "CARD", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Theils hatte schon die welt dahin genommen, theils zehlten sich zu den entschlaffnen frommen.", "tokens": ["Theils", "hat\u00b7te", "schon", "die", "welt", "da\u00b7hin", "ge\u00b7nom\u00b7men", ",", "theils", "zehl\u00b7ten", "sich", "zu", "den", "ent\u00b7schlaff\u00b7nen", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "PAV", "VVPP", "$,", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun lernen sie zugleich die wunden kennen, und ihre herzen fangen an zu brennen.", "tokens": ["Nun", "ler\u00b7nen", "sie", "zu\u00b7gleich", "die", "wun\u00b7den", "ken\u00b7nen", ",", "und", "ih\u00b7re", "her\u00b7zen", "fan\u00b7gen", "an", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Zu ihrer desto bessern seelen-pflege, hast du's gerichtet in die selge wege,", "tokens": ["Zu", "ih\u00b7rer", "des\u00b7to", "bes\u00b7sern", "see\u00b7len\u00b7pfle\u00b7ge", ",", "hast", "du's", "ge\u00b7rich\u00b7tet", "in", "die", "sel\u00b7ge", "we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,", "VAFIN", "PIS", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df wir aparte wohnungen erbauen, da wir das theure chor beysammen schauen.", "tokens": ["Da\u00df", "wir", "a\u00b7par\u00b7te", "woh\u00b7nun\u00b7gen", "er\u00b7bau\u00b7en", ",", "da", "wir", "das", "theu\u00b7re", "chor", "bey\u00b7sam\u00b7men", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Man sieht in diesen neuen Gottes-h\u00e4usern, da\u00df sich auch neue gnaden-regung \u00e4ussern.", "tokens": ["Man", "sieht", "in", "die\u00b7sen", "neu\u00b7en", "Got\u00b7tes\u00b7h\u00e4u\u00b7sern", ",", "da\u00df", "sich", "auch", "neu\u00b7e", "gna\u00b7den\u00b7re\u00b7gung", "\u00e4us\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "PRF", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich sch\u00e4me michs, wenn ichs will \u00fcberlegen, ich kan nichts thun, als danken f\u00fcr die segen;", "tokens": ["Ich", "sch\u00e4\u00b7me", "michs", ",", "wenn", "ichs", "will", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "ich", "kan", "nichts", "thun", ",", "als", "dan\u00b7ken", "f\u00fcr", "die", "se\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PIS", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "NN", "APPR", "ART", "PPOSS", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "Und f\u00fcr die gnade in den ledgen ch\u00f6ren die durchgebohrten h\u00e4nd und f\u00fcsse ehren.", "tokens": ["Und", "f\u00fcr", "die", "gna\u00b7de", "in", "den", "led\u00b7gen", "ch\u00f6\u00b7ren", "die", "durch\u00b7ge\u00b7bohr\u00b7ten", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Gib uns den sinn, der die Maria zierte, die sich nicht ohne deinen willen r\u00fchrte.", "tokens": ["Gib", "uns", "den", "sinn", ",", "der", "die", "Ma\u00b7ria", "zier\u00b7te", ",", "die", "sich", "nicht", "oh\u00b7ne", "dei\u00b7nen", "wil\u00b7len", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,", "PRELS", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Und sich in allen dir so \u00fcberlassen, da\u00df sie dir k\u00f6nt' in deine absicht passen;", "tokens": ["Und", "sich", "in", "al\u00b7len", "dir", "so", "\u00fc\u00b7ber\u00b7las\u00b7sen", ",", "da\u00df", "sie", "dir", "k\u00f6nt'", "in", "dei\u00b7ne", "ab\u00b7sicht", "pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "So solt du alle unsre br\u00fcder haben; du solt dich noch an unsrer einfalt laben.", "tokens": ["So", "solt", "du", "al\u00b7le", "uns\u00b7re", "br\u00fc\u00b7der", "ha\u00b7ben", ";", "du", "solt", "dich", "noch", "an", "uns\u00b7rer", "ein\u00b7falt", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "Wir dienen, wenn du wilt, zu land und seen, und wenn du wilt, so gehn wir in die ehen.", "tokens": ["Wir", "die\u00b7nen", ",", "wenn", "du", "wilt", ",", "zu", "land", "und", "seen", ",", "und", "wenn", "du", "wilt", ",", "so", "gehn", "wir", "in", "die", "e\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "NN", "KON", "VVFIN", "$,", "KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.15": {"line.1": {"text": "Wir wollen uns dir v\u00f6llig \u00fcbergeben; wir wolln als kinder der Gemeine leben.", "tokens": ["Wir", "wol\u00b7len", "uns", "dir", "v\u00f6l\u00b7lig", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ";", "wir", "wolln", "als", "kin\u00b7der", "der", "Ge\u00b7mei\u00b7ne", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADJD", "VVPP", "$.", "PPER", "VMFIN", "KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.16": {"line.1": {"text": "Und was uns bringt aus diesen gnaden-schranken, der vorwitz und vorl\u00e4uffige gedanken,", "tokens": ["Und", "was", "uns", "bringt", "aus", "die\u00b7sen", "gna\u00b7den\u00b7schran\u00b7ken", ",", "der", "vor\u00b7witz", "und", "vor\u00b7l\u00e4uf\u00b7fi\u00b7ge", "ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}}, "stanza.17": {"line.1": {"text": "Das tilge heute noch aus unsrer mitte, und heilge unsern geist und seel und h\u00fctte.", "tokens": ["Das", "til\u00b7ge", "heu\u00b7te", "noch", "aus", "uns\u00b7rer", "mit\u00b7te", ",", "und", "heil\u00b7ge", "un\u00b7sern", "geist", "und", "seel", "und", "h\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.18": {"line.1": {"text": "La\u00df leuchten in dem dir geweihten saale der h\u00e4nd und f\u00fcsse ihre n\u00e4gel-maale.", "tokens": ["La\u00df", "leuch\u00b7ten", "in", "dem", "dir", "ge\u00b7weih\u00b7ten", "saa\u00b7le", "der", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "ih\u00b7re", "n\u00e4\u00b7gel\u00b7maa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "APPR", "PRELS", "PPER", "ADJA", "NN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.19": {"line.1": {"text": "In stunden, classen und in viertelstunden da funkle deine seit und alle wunden.", "tokens": ["In", "stun\u00b7den", ",", "clas\u00b7sen", "und", "in", "vier\u00b7tel\u00b7stun\u00b7den", "da", "funk\u00b7le", "dei\u00b7ne", "seit", "und", "al\u00b7le", "wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "VVINF", "KON", "APPR", "PIS", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.20": {"line.1": {"text": "Zerschmelze aller ledgen br\u00fcder herzen, wenn sie hier vor dir sind, durch deine schmerzen;", "tokens": ["Zer\u00b7schmel\u00b7ze", "al\u00b7ler", "led\u00b7gen", "br\u00fc\u00b7der", "her\u00b7zen", ",", "wenn", "sie", "hier", "vor", "dir", "sind", ",", "durch", "dei\u00b7ne", "schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.21": {"line.1": {"text": "So da\u00df wir nie zusammen kommen m\u00f6gen, da\u00df sich nicht solt ein neuer segen regen.", "tokens": ["So", "da\u00df", "wir", "nie", "zu\u00b7sam\u00b7men", "kom\u00b7men", "m\u00f6\u00b7gen", ",", "da\u00df", "sich", "nicht", "solt", "ein", "neu\u00b7er", "se\u00b7gen", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PRF", "PTKNEG", "VMFIN", "ART", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.22": {"line.1": {"text": "Schik uns auch deine engel auf die s\u00e4le, wo wir itzt schlafen nach der leibes-h\u00f6le:", "tokens": ["Schik", "uns", "auch", "dei\u00b7ne", "en\u00b7gel", "auf", "die", "s\u00e4\u00b7le", ",", "wo", "wir", "itzt", "schla\u00b7fen", "nach", "der", "lei\u00b7bes\u00b7h\u00f6le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.23": {"line.1": {"text": "An statt da\u00df wir was anders phantasiren, la\u00df sie uns mit dem blik der wunden r\u00fchren.", "tokens": ["An", "statt", "da\u00df", "wir", "was", "an\u00b7ders", "phan\u00b7ta\u00b7si\u00b7ren", ",", "la\u00df", "sie", "uns", "mit", "dem", "blik", "der", "wun\u00b7den", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "KOUS", "PPER", "PIS", "ADV", "VVINF", "$,", "VVIMP", "PPER", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.24": {"line.1": {"text": "Um unsrer br\u00fcder ihre keusche betten ziehn deine engel unsichtbare ketten;", "tokens": ["Um", "uns\u00b7rer", "br\u00fc\u00b7der", "ih\u00b7re", "keu\u00b7sche", "bet\u00b7ten", "ziehn", "dei\u00b7ne", "en\u00b7gel", "un\u00b7sicht\u00b7ba\u00b7re", "ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.25": {"line.1": {"text": "Da\u00df wir das bett f\u00fcr so ein pl\u00e4tzgen halten, als unsrer seele sind die wunden-spalten.", "tokens": ["Da\u00df", "wir", "das", "bett", "f\u00fcr", "so", "ein", "pl\u00e4tz\u00b7gen", "hal\u00b7ten", ",", "als", "uns\u00b7rer", "see\u00b7le", "sind", "die", "wun\u00b7den\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "ART", "NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.26": {"line.1": {"text": "Ach heilge unsern leib und auch die seele durch eine f\u00fcllung mit dem priester-\u00f6le.", "tokens": ["Ach", "heil\u00b7ge", "un\u00b7sern", "leib", "und", "auch", "die", "see\u00b7le", "durch", "ei\u00b7ne", "f\u00fcl\u00b7lung", "mit", "dem", "pries\u00b7ter\u00b7\u00f6le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.27": {"line.1": {"text": "Und weil du weist, da\u00df mancher schon verf\u00fchret, ders noch in seiner \u00e4ussern h\u00fctte sp\u00fcret;", "tokens": ["Und", "weil", "du", "weist", ",", "da\u00df", "man\u00b7cher", "schon", "ver\u00b7f\u00fch\u00b7ret", ",", "ders", "noch", "in", "sei\u00b7ner", "\u00e4us\u00b7sern", "h\u00fct\u00b7te", "sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.28": {"line.1": {"text": "Ihm ist sein einfalts-wesen weggenommen, was ", "tokens": ["Ihm", "ist", "sein", "ein\u00b7falts\u00b7we\u00b7sen", "weg\u00b7ge\u00b7nom\u00b7men", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.29": {"line.1": {"text": "So wird ein ieglicher sich als von neuen in seinem stand zum tempel Gottes weihen,", "tokens": ["So", "wird", "ein", "ieg\u00b7li\u00b7cher", "sich", "als", "von", "neu\u00b7en", "in", "sei\u00b7nem", "stand", "zum", "tem\u00b7pel", "Got\u00b7tes", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "PRF", "KOKOM", "APPR", "ADJA", "APPR", "PPOSAT", "VVFIN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.30": {"line.1": {"text": "Und seine h\u00fctte in den k\u00fcnftgen jahren untadelich und heiliglich bewahren.", "tokens": ["Und", "sei\u00b7ne", "h\u00fct\u00b7te", "in", "den", "k\u00fcnft\u00b7gen", "jah\u00b7ren", "un\u00b7ta\u00b7de\u00b7lich", "und", "hei\u00b7lig\u00b7lich", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.31": {"line.1": {"text": "Mein Lamm! du bist auch so wie wir gewesen, durch dich ist manch verf\u00fchrtes herz genesen.", "tokens": ["Mein", "Lamm", "!", "du", "bist", "auch", "so", "wie", "wir", "ge\u00b7we\u00b7sen", ",", "durch", "dich", "ist", "manch", "ver\u00b7f\u00fchr\u00b7tes", "herz", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "VAPP", "$,", "APPR", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.32": {"line.1": {"text": "Du hast empfunden alle menschen-plagen; du hast die glieder alle auch getragen,", "tokens": ["Du", "hast", "emp\u00b7fun\u00b7den", "al\u00b7le", "men\u00b7schen\u00b7pla\u00b7gen", ";", "du", "hast", "die", "glie\u00b7der", "al\u00b7le", "auch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.33": {"line.1": {"text": "So wie wir sie an unserm leibe finden, und du bewahretest sie ohne s\u00fcnden.", "tokens": ["So", "wie", "wir", "sie", "an", "un\u00b7serm", "lei\u00b7be", "fin\u00b7den", ",", "und", "du", "be\u00b7wah\u00b7re\u00b7test", "sie", "oh\u00b7ne", "s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.34": {"line.1": {"text": "Drum sollen auch zu ehren deiner glieder, die glieder heilig werden unsrer br\u00fcder.", "tokens": ["Drum", "sol\u00b7len", "auch", "zu", "eh\u00b7ren", "dei\u00b7ner", "glie\u00b7der", ",", "die", "glie\u00b7der", "hei\u00b7lig", "wer\u00b7den", "uns\u00b7rer", "br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.35": {"line.1": {"text": "Besonders ehre Gott die erste wunde, die du krigtst im gefolg von Abrams bunde.", "tokens": ["Be\u00b7son\u00b7ders", "eh\u00b7re", "Gott", "die", "ers\u00b7te", "wun\u00b7de", ",", "die", "du", "krigtst", "im", "ge\u00b7folg", "von", "Ab\u00b7rams", "bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NE", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.36": {"line.1": {"text": "Wenn er das thut, so schw\u00f6r ich seinem namen, da\u00df seins Sohns leute keusch seyn sollen. Amen!", "tokens": ["Wenn", "er", "das", "thut", ",", "so", "schw\u00f6r", "ich", "sei\u00b7nem", "na\u00b7men", ",", "da\u00df", "seins", "Sohns", "leu\u00b7te", "keusch", "seyn", "sol\u00b7len", ".", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "ADJD", "VAINF", "VMFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.37": {"line.1": {"text": "Mein armes herze sch\u00e4met sich der segen, die sich im chor der ledgen br\u00fcder regen.", "tokens": ["Mein", "ar\u00b7mes", "her\u00b7ze", "sch\u00e4\u00b7met", "sich", "der", "se\u00b7gen", ",", "die", "sich", "im", "chor", "der", "led\u00b7gen", "br\u00fc\u00b7der", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ART", "PPOSS", "$,", "PRELS", "PRF", "APPRART", "NN", "ART", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.38": {"line.1": {"text": "Wie manchen hast du seit der zeit erwehlet, und deiner Creuz-Gemeine beygezehlet.", "tokens": ["Wie", "man\u00b7chen", "hast", "du", "seit", "der", "zeit", "er\u00b7weh\u00b7let", ",", "und", "dei\u00b7ner", "Creuz\u00b7Ge\u00b7mei\u00b7ne", "bey\u00b7ge\u00b7zeh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.39": {"line.1": {"text": "Man sieht itzt hundert, wo vor wenig jahren kaum neun und zehn zu finden m\u00f6glich waren.", "tokens": ["Man", "sieht", "itzt", "hun\u00b7dert", ",", "wo", "vor", "we\u00b7nig", "jah\u00b7ren", "kaum", "neun", "und", "zehn", "zu", "fin\u00b7den", "m\u00f6g\u00b7lich", "wa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "CARD", "$,", "PWAV", "APPR", "PIAT", "NN", "ADV", "CARD", "KON", "CARD", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.40": {"line.1": {"text": "Theils hatte schon die welt dahin genommen, theils zehlten sich zu den entschlaffnen frommen.", "tokens": ["Theils", "hat\u00b7te", "schon", "die", "welt", "da\u00b7hin", "ge\u00b7nom\u00b7men", ",", "theils", "zehl\u00b7ten", "sich", "zu", "den", "ent\u00b7schlaff\u00b7nen", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "PAV", "VVPP", "$,", "ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.41": {"line.1": {"text": "Nun lernen sie zugleich die wunden kennen, und ihre herzen fangen an zu brennen.", "tokens": ["Nun", "ler\u00b7nen", "sie", "zu\u00b7gleich", "die", "wun\u00b7den", "ken\u00b7nen", ",", "und", "ih\u00b7re", "her\u00b7zen", "fan\u00b7gen", "an", "zu", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.42": {"line.1": {"text": "Zu ihrer desto bessern seelen-pflege, hast du's gerichtet in die selge wege,", "tokens": ["Zu", "ih\u00b7rer", "des\u00b7to", "bes\u00b7sern", "see\u00b7len\u00b7pfle\u00b7ge", ",", "hast", "du's", "ge\u00b7rich\u00b7tet", "in", "die", "sel\u00b7ge", "we\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$,", "VAFIN", "PIS", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.43": {"line.1": {"text": "Da\u00df wir aparte wohnungen erbauen, da wir das theure chor beysammen schauen.", "tokens": ["Da\u00df", "wir", "a\u00b7par\u00b7te", "woh\u00b7nun\u00b7gen", "er\u00b7bau\u00b7en", ",", "da", "wir", "das", "theu\u00b7re", "chor", "bey\u00b7sam\u00b7men", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.44": {"line.1": {"text": "Man sieht in diesen neuen Gottes-h\u00e4usern, da\u00df sich auch neue gnaden-regung \u00e4ussern.", "tokens": ["Man", "sieht", "in", "die\u00b7sen", "neu\u00b7en", "Got\u00b7tes\u00b7h\u00e4u\u00b7sern", ",", "da\u00df", "sich", "auch", "neu\u00b7e", "gna\u00b7den\u00b7re\u00b7gung", "\u00e4us\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$,", "KOUS", "PRF", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.45": {"line.1": {"text": "Ich sch\u00e4me michs, wenn ichs will \u00fcberlegen, ich kan nichts thun, als danken f\u00fcr die segen;", "tokens": ["Ich", "sch\u00e4\u00b7me", "michs", ",", "wenn", "ichs", "will", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "ich", "kan", "nichts", "thun", ",", "als", "dan\u00b7ken", "f\u00fcr", "die", "se\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PIS", "VMFIN", "VVINF", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "NN", "APPR", "ART", "PPOSS", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.46": {"line.1": {"text": "Und f\u00fcr die gnade in den ledgen ch\u00f6ren die durchgebohrten h\u00e4nd und f\u00fcsse ehren.", "tokens": ["Und", "f\u00fcr", "die", "gna\u00b7de", "in", "den", "led\u00b7gen", "ch\u00f6\u00b7ren", "die", "durch\u00b7ge\u00b7bohr\u00b7ten", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.47": {"line.1": {"text": "Gib uns den sinn, der die Maria zierte, die sich nicht ohne deinen willen r\u00fchrte.", "tokens": ["Gib", "uns", "den", "sinn", ",", "der", "die", "Ma\u00b7ria", "zier\u00b7te", ",", "die", "sich", "nicht", "oh\u00b7ne", "dei\u00b7nen", "wil\u00b7len", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NE", "VVFIN", "$,", "PRELS", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.48": {"line.1": {"text": "Und sich in allen dir so \u00fcberlassen, da\u00df sie dir k\u00f6nt' in deine absicht passen;", "tokens": ["Und", "sich", "in", "al\u00b7len", "dir", "so", "\u00fc\u00b7ber\u00b7las\u00b7sen", ",", "da\u00df", "sie", "dir", "k\u00f6nt'", "in", "dei\u00b7ne", "ab\u00b7sicht", "pas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.49": {"line.1": {"text": "So solt du alle unsre br\u00fcder haben; du solt dich noch an unsrer einfalt laben.", "tokens": ["So", "solt", "du", "al\u00b7le", "uns\u00b7re", "br\u00fc\u00b7der", "ha\u00b7ben", ";", "du", "solt", "dich", "noch", "an", "uns\u00b7rer", "ein\u00b7falt", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "VAFIN", "$.", "PPER", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.50": {"line.1": {"text": "Wir dienen, wenn du wilt, zu land und seen, und wenn du wilt, so gehn wir in die ehen.", "tokens": ["Wir", "die\u00b7nen", ",", "wenn", "du", "wilt", ",", "zu", "land", "und", "seen", ",", "und", "wenn", "du", "wilt", ",", "so", "gehn", "wir", "in", "die", "e\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "NN", "KON", "VVFIN", "$,", "KON", "KOUS", "PPER", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.51": {"line.1": {"text": "Wir wollen uns dir v\u00f6llig \u00fcbergeben; wir wolln als kinder der Gemeine leben.", "tokens": ["Wir", "wol\u00b7len", "uns", "dir", "v\u00f6l\u00b7lig", "\u00fc\u00b7ber\u00b7ge\u00b7ben", ";", "wir", "wolln", "als", "kin\u00b7der", "der", "Ge\u00b7mei\u00b7ne", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADJD", "VVPP", "$.", "PPER", "VMFIN", "KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.52": {"line.1": {"text": "Und was uns bringt aus diesen gnaden-schranken, der vorwitz und vorl\u00e4uffige gedanken,", "tokens": ["Und", "was", "uns", "bringt", "aus", "die\u00b7sen", "gna\u00b7den\u00b7schran\u00b7ken", ",", "der", "vor\u00b7witz", "und", "vor\u00b7l\u00e4uf\u00b7fi\u00b7ge", "ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus"}}, "stanza.53": {"line.1": {"text": "Das tilge heute noch aus unsrer mitte, und heilge unsern geist und seel und h\u00fctte.", "tokens": ["Das", "til\u00b7ge", "heu\u00b7te", "noch", "aus", "uns\u00b7rer", "mit\u00b7te", ",", "und", "heil\u00b7ge", "un\u00b7sern", "geist", "und", "seel", "und", "h\u00fct\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.54": {"line.1": {"text": "La\u00df leuchten in dem dir geweihten saale der h\u00e4nd und f\u00fcsse ihre n\u00e4gel-maale.", "tokens": ["La\u00df", "leuch\u00b7ten", "in", "dem", "dir", "ge\u00b7weih\u00b7ten", "saa\u00b7le", "der", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "ih\u00b7re", "n\u00e4\u00b7gel\u00b7maa\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "APPR", "PRELS", "PPER", "ADJA", "NN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.55": {"line.1": {"text": "In stunden, classen und in viertelstunden da funkle deine seit und alle wunden.", "tokens": ["In", "stun\u00b7den", ",", "clas\u00b7sen", "und", "in", "vier\u00b7tel\u00b7stun\u00b7den", "da", "funk\u00b7le", "dei\u00b7ne", "seit", "und", "al\u00b7le", "wun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "VVINF", "KON", "APPR", "PIS", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.56": {"line.1": {"text": "Zerschmelze aller ledgen br\u00fcder herzen, wenn sie hier vor dir sind, durch deine schmerzen;", "tokens": ["Zer\u00b7schmel\u00b7ze", "al\u00b7ler", "led\u00b7gen", "br\u00fc\u00b7der", "her\u00b7zen", ",", "wenn", "sie", "hier", "vor", "dir", "sind", ",", "durch", "dei\u00b7ne", "schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "VVINF", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "VAFIN", "$,", "APPR", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.57": {"line.1": {"text": "So da\u00df wir nie zusammen kommen m\u00f6gen, da\u00df sich nicht solt ein neuer segen regen.", "tokens": ["So", "da\u00df", "wir", "nie", "zu\u00b7sam\u00b7men", "kom\u00b7men", "m\u00f6\u00b7gen", ",", "da\u00df", "sich", "nicht", "solt", "ein", "neu\u00b7er", "se\u00b7gen", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PRF", "PTKNEG", "VMFIN", "ART", "ADJA", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.58": {"line.1": {"text": "Schik uns auch deine engel auf die s\u00e4le, wo wir itzt schlafen nach der leibes-h\u00f6le:", "tokens": ["Schik", "uns", "auch", "dei\u00b7ne", "en\u00b7gel", "auf", "die", "s\u00e4\u00b7le", ",", "wo", "wir", "itzt", "schla\u00b7fen", "nach", "der", "lei\u00b7bes\u00b7h\u00f6le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.59": {"line.1": {"text": "An statt da\u00df wir was anders phantasiren, la\u00df sie uns mit dem blik der wunden r\u00fchren.", "tokens": ["An", "statt", "da\u00df", "wir", "was", "an\u00b7ders", "phan\u00b7ta\u00b7si\u00b7ren", ",", "la\u00df", "sie", "uns", "mit", "dem", "blik", "der", "wun\u00b7den", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "KOUS", "PPER", "PIS", "ADV", "VVINF", "$,", "VVIMP", "PPER", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.60": {"line.1": {"text": "Um unsrer br\u00fcder ihre keusche betten ziehn deine engel unsichtbare ketten;", "tokens": ["Um", "uns\u00b7rer", "br\u00fc\u00b7der", "ih\u00b7re", "keu\u00b7sche", "bet\u00b7ten", "ziehn", "dei\u00b7ne", "en\u00b7gel", "un\u00b7sicht\u00b7ba\u00b7re", "ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.61": {"line.1": {"text": "Da\u00df wir das bett f\u00fcr so ein pl\u00e4tzgen halten, als unsrer seele sind die wunden-spalten.", "tokens": ["Da\u00df", "wir", "das", "bett", "f\u00fcr", "so", "ein", "pl\u00e4tz\u00b7gen", "hal\u00b7ten", ",", "als", "uns\u00b7rer", "see\u00b7le", "sind", "die", "wun\u00b7den\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "ART", "NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.62": {"line.1": {"text": "Ach heilge unsern leib und auch die seele durch eine f\u00fcllung mit dem priester-\u00f6le.", "tokens": ["Ach", "heil\u00b7ge", "un\u00b7sern", "leib", "und", "auch", "die", "see\u00b7le", "durch", "ei\u00b7ne", "f\u00fcl\u00b7lung", "mit", "dem", "pries\u00b7ter\u00b7\u00f6le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.63": {"line.1": {"text": "Und weil du weist, da\u00df mancher schon verf\u00fchret, ders noch in seiner \u00e4ussern h\u00fctte sp\u00fcret;", "tokens": ["Und", "weil", "du", "weist", ",", "da\u00df", "man\u00b7cher", "schon", "ver\u00b7f\u00fch\u00b7ret", ",", "ders", "noch", "in", "sei\u00b7ner", "\u00e4us\u00b7sern", "h\u00fct\u00b7te", "sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.64": {"line.1": {"text": "Ihm ist sein einfalts-wesen weggenommen, was ", "tokens": ["Ihm", "ist", "sein", "ein\u00b7falts\u00b7we\u00b7sen", "weg\u00b7ge\u00b7nom\u00b7men", ",", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "PWS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.65": {"line.1": {"text": "So wird ein ieglicher sich als von neuen in seinem stand zum tempel Gottes weihen,", "tokens": ["So", "wird", "ein", "ieg\u00b7li\u00b7cher", "sich", "als", "von", "neu\u00b7en", "in", "sei\u00b7nem", "stand", "zum", "tem\u00b7pel", "Got\u00b7tes", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "PRF", "KOKOM", "APPR", "ADJA", "APPR", "PPOSAT", "VVFIN", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.66": {"line.1": {"text": "Und seine h\u00fctte in den k\u00fcnftgen jahren untadelich und heiliglich bewahren.", "tokens": ["Und", "sei\u00b7ne", "h\u00fct\u00b7te", "in", "den", "k\u00fcnft\u00b7gen", "jah\u00b7ren", "un\u00b7ta\u00b7de\u00b7lich", "und", "hei\u00b7lig\u00b7lich", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.67": {"line.1": {"text": "Mein Lamm! du bist auch so wie wir gewesen, durch dich ist manch verf\u00fchrtes herz genesen.", "tokens": ["Mein", "Lamm", "!", "du", "bist", "auch", "so", "wie", "wir", "ge\u00b7we\u00b7sen", ",", "durch", "dich", "ist", "manch", "ver\u00b7f\u00fchr\u00b7tes", "herz", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "ADV", "KOKOM", "PPER", "VAPP", "$,", "APPR", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+---+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.68": {"line.1": {"text": "Du hast empfunden alle menschen-plagen; du hast die glieder alle auch getragen,", "tokens": ["Du", "hast", "emp\u00b7fun\u00b7den", "al\u00b7le", "men\u00b7schen\u00b7pla\u00b7gen", ";", "du", "hast", "die", "glie\u00b7der", "al\u00b7le", "auch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.69": {"line.1": {"text": "So wie wir sie an unserm leibe finden, und du bewahretest sie ohne s\u00fcnden.", "tokens": ["So", "wie", "wir", "sie", "an", "un\u00b7serm", "lei\u00b7be", "fin\u00b7den", ",", "und", "du", "be\u00b7wah\u00b7re\u00b7test", "sie", "oh\u00b7ne", "s\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.70": {"line.1": {"text": "Drum sollen auch zu ehren deiner glieder, die glieder heilig werden unsrer br\u00fcder.", "tokens": ["Drum", "sol\u00b7len", "auch", "zu", "eh\u00b7ren", "dei\u00b7ner", "glie\u00b7der", ",", "die", "glie\u00b7der", "hei\u00b7lig", "wer\u00b7den", "uns\u00b7rer", "br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.71": {"line.1": {"text": "Besonders ehre Gott die erste wunde, die du krigtst im gefolg von Abrams bunde.", "tokens": ["Be\u00b7son\u00b7ders", "eh\u00b7re", "Gott", "die", "ers\u00b7te", "wun\u00b7de", ",", "die", "du", "krigtst", "im", "ge\u00b7folg", "von", "Ab\u00b7rams", "bun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NE", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.72": {"line.1": {"text": "Wenn er das thut, so schw\u00f6r ich seinem namen, da\u00df seins Sohns leute keusch seyn sollen. Amen!", "tokens": ["Wenn", "er", "das", "thut", ",", "so", "schw\u00f6r", "ich", "sei\u00b7nem", "na\u00b7men", ",", "da\u00df", "seins", "Sohns", "leu\u00b7te", "keusch", "seyn", "sol\u00b7len", ".", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "ADJD", "VAINF", "VMFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}}}}