{"dta.poem.7334": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Nach f\u00fcnfzehn Jahren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie hab' ich doch so manche Sommernacht,", "tokens": ["Wie", "hab'", "ich", "doch", "so", "man\u00b7che", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du d\u00fcstrer Saal, in deinem Raum verwacht!", "tokens": ["Du", "d\u00fcst\u00b7rer", "Saal", ",", "in", "dei\u00b7nem", "Raum", "ver\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und du, Balkon, auf dich bin ich getreten,", "tokens": ["Und", "du", ",", "Bal\u00b7kon", ",", "auf", "dich", "bin", "ich", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "$,", "APPR", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um leise f\u00fcr ein theures Haupt zu beten,", "tokens": ["Um", "lei\u00b7se", "f\u00fcr", "ein", "theu\u00b7res", "Haupt", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn hinter mir aus des Gemaches Tiefen", "tokens": ["Wenn", "hin\u00b7ter", "mir", "aus", "des", "Ge\u00b7ma\u00b7ches", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie H\u00fclfewimmern bange Seufzer riefen,", "tokens": ["Wie", "H\u00fcl\u00b7fe\u00b7wim\u00b7mern", "ban\u00b7ge", "Seuf\u00b7zer", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die Odemz\u00fcge aus geliebtem Mund;", "tokens": ["Die", "O\u00b7dem\u00b7z\u00fc\u00b7ge", "aus", "ge\u00b7lieb\u00b7tem", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja, bitter weint' ich \u2014 o Erinnerung! \u2014", "tokens": ["Ja", ",", "bit\u00b7ter", "weint'", "ich", "o", "E\u00b7rin\u00b7ne\u00b7rung", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "PPER", "$(", "FM", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Doch trug ich muthig es, denn ich war jung,", "tokens": ["Doch", "trug", "ich", "mut\u00b7hig", "es", ",", "denn", "ich", "war", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PPER", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "War jung noch und gesund.", "tokens": ["War", "jung", "noch", "und", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du Bett mit seidnem Franzenhang geziert,", "tokens": ["Du", "Bett", "mit", "seid\u00b7nem", "Fran\u00b7zen\u00b7hang", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie hab' ich deine Falten oft ber\u00fchrt,", "tokens": ["Wie", "hab'", "ich", "dei\u00b7ne", "Fal\u00b7ten", "oft", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit leiser leiser Hand gehemmt ihr Rauschen,", "tokens": ["Mit", "lei\u00b7ser", "lei\u00b7ser", "Hand", "ge\u00b7hemmt", "ihr", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich mich beugte durch den Spalt zu lauschen,", "tokens": ["Wenn", "ich", "mich", "beug\u00b7te", "durch", "den", "Spalt", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein Haupt so m\u00fcde da\u00df es schwamm wie trunken,", "tokens": ["Mein", "Haupt", "so", "m\u00fc\u00b7de", "da\u00df", "es", "schwamm", "wie", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KOUS", "PPER", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So matt mein Knie da\u00df es zum Grund gesunken!", "tokens": ["So", "matt", "mein", "Knie", "da\u00df", "es", "zum", "Grund", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mechanisch l\u00f6ste ich der Z\u00f6pfe Bund", "tokens": ["Me\u00b7cha\u00b7nisch", "l\u00f6s\u00b7te", "ich", "der", "Z\u00f6p\u00b7fe", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und sucht im frischen Trunk Erleichterung;", "tokens": ["Und", "sucht", "im", "fri\u00b7schen", "Trunk", "Er\u00b7leich\u00b7te\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ach, Alles tr\u00e4gt man leicht, ist man nur jung,", "tokens": ["Ach", ",", "Al\u00b7les", "tr\u00e4gt", "man", "leicht", ",", "ist", "man", "nur", "jung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PIS", "ADJD", "$,", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nur jung noch und gesund!", "tokens": ["Nur", "jung", "noch", "und", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und als die Rose, die am Stock erblich,", "tokens": ["Und", "als", "die", "Ro\u00b7se", ",", "die", "am", "Stock", "er\u00b7blich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich wieder auf die kranke Wange schlich,", "tokens": ["Sich", "wie\u00b7der", "auf", "die", "kran\u00b7ke", "Wan\u00b7ge", "schlich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie hab' ich an dem Pfeilertische dr\u00fcben", "tokens": ["Wie", "hab'", "ich", "an", "dem", "Pfei\u00b7ler\u00b7ti\u00b7sche", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem T\u00f6chterchen geringelt seine lieben", "tokens": ["Dem", "T\u00f6ch\u00b7ter\u00b7chen", "ge\u00b7rin\u00b7gelt", "sei\u00b7ne", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Goldbraunen L\u00f6ckchen! wie ich mich beflissen,", "tokens": ["Gold\u00b7brau\u00b7nen", "L\u00f6ck\u00b7chen", "!", "wie", "ich", "mich", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eh ich es f\u00fchrte an der Mutter Kissen!", "tokens": ["Eh", "ich", "es", "f\u00fchr\u00b7te", "an", "der", "Mut\u00b7ter", "Kis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und gute Sitte fl\u00fcstert' ich ihm ein,", "tokens": ["Und", "gu\u00b7te", "Sit\u00b7te", "fl\u00fcs\u00b7tert'", "ich", "ihm", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gelobte ihm die Fabel von dem Schaf", "tokens": ["Ge\u00b7lob\u00b7te", "ihm", "die", "Fa\u00b7bel", "von", "dem", "Schaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und sieben Zicklein, wenn es wolle brav,", "tokens": ["Und", "sie\u00b7ben", "Zic\u00b7klein", ",", "wenn", "es", "wol\u00b7le", "brav", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "KOUS", "PPER", "VMFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Recht brav und sittig seyn.", "tokens": ["Recht", "brav", "und", "sit\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und dort die H\u00fctte in der Tannenschlucht,", "tokens": ["Und", "dort", "die", "H\u00fct\u00b7te", "in", "der", "Tan\u00b7nen\u00b7schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da naschten sie und ich der Rebe Frucht,", "tokens": ["Da", "naschten", "sie", "und", "ich", "der", "Re\u00b7be", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da f\u00fchlten wir das Blut so keimend treiben,", "tokens": ["Da", "f\u00fchl\u00b7ten", "wir", "das", "Blut", "so", "kei\u00b7mend", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als m\u00fcss' es immer frisch und sch\u00e4umend bleiben;", "tokens": ["Als", "m\u00fcss'", "es", "im\u00b7mer", "frisch", "und", "sch\u00e4u\u00b7mend", "blei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Ueberstandnen lachten wir im Hafen:", "tokens": ["Des", "Ue\u00b7ber\u00b7stand\u00b7nen", "lach\u00b7ten", "wir", "im", "Ha\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie ich geschwankt, wie stehend ich geschlafen;", "tokens": ["Wie", "ich", "ge\u00b7schwankt", ",", "wie", "ste\u00b7hend", "ich", "ge\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "PWAV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wandelten am Rasenstreifen fort,", "tokens": ["Und", "wan\u00b7del\u00b7ten", "am", "Ra\u00b7sen\u00b7strei\u00b7fen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und musterten der St\u00e4mmchen schlanke Reihn,", "tokens": ["Und", "mus\u00b7ter\u00b7ten", "der", "St\u00e4mm\u00b7chen", "schlan\u00b7ke", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und schw\u00e4rmten, wie es m\u00fcsse reizend seyn", "tokens": ["Und", "schw\u00e4rm\u00b7ten", ",", "wie", "es", "m\u00fcs\u00b7se", "rei\u00b7zend", "seyn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "VVPP", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nach f\u00fcnfzehn Jahren dort!", "tokens": ["Nach", "f\u00fcnf\u00b7zehn", "Jah\u00b7ren", "dort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O f\u00fcnfzehn Jahre, lange \u00f6de Zeit!", "tokens": ["O", "f\u00fcnf\u00b7zehn", "Jah\u00b7re", ",", "lan\u00b7ge", "\u00f6\u00b7de", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "CARD", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sind die B\u00e4ume jetzt so starr und breit!", "tokens": ["Wie", "sind", "die", "B\u00e4u\u00b7me", "jetzt", "so", "starr", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der H\u00fctte Th\u00fcr vermocht ich kaum zu regen,", "tokens": ["Der", "H\u00fct\u00b7te", "Th\u00fcr", "ver\u00b7mocht", "ich", "kaum", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da scho\u00df mir Staub und w\u00fcst Ger\u00fcll entgegen,", "tokens": ["Da", "scho\u00df", "mir", "Staub", "und", "w\u00fcst", "Ge\u00b7r\u00fcll", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und an dem blanken Gartensaale dr\u00fcben", "tokens": ["Und", "an", "dem", "blan\u00b7ken", "Gar\u00b7ten\u00b7saa\u00b7le", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da steht 'ne schlanke Maid mit ihrem Lieben,", "tokens": ["Da", "steht", "'ne", "schlan\u00b7ke", "Maid", "mit", "ih\u00b7rem", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die schaun sich l\u00e4chelnd in der Seele Grund,", "tokens": ["Die", "schaun", "sich", "l\u00e4\u00b7chelnd", "in", "der", "See\u00b7le", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In ihren braunen Locken rollt der Wind;", "tokens": ["In", "ih\u00b7ren", "brau\u00b7nen", "Lo\u00b7cken", "rollt", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "w