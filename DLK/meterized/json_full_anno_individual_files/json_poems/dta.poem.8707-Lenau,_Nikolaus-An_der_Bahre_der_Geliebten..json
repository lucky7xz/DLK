{"dta.poem.8707": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An der Bahre der Geliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Bla\u00df und auf immer stumm, auf immer! liegst du", "tokens": ["Bla\u00df", "und", "auf", "im\u00b7mer", "stumm", ",", "auf", "im\u00b7mer", "!", "liegst", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "APPR", "ADV", "ADJD", "$,", "APPR", "ADV", "$.", "VVFIN", "PPER"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hingestreckt, o Geliebte, auf der Bahre!", "tokens": ["Hin\u00b7ge\u00b7streckt", ",", "o", "Ge\u00b7lieb\u00b7te", ",", "auf", "der", "Bah\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deine Reize lockten den Tod, er kam, er", "tokens": ["Dei\u00b7ne", "Rei\u00b7ze", "lock\u00b7ten", "den", "Tod", ",", "er", "kam", ",", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "$,", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "H\u00e4lt dich umarmet!", "tokens": ["H\u00e4lt", "dich", "um\u00b7ar\u00b7met", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Einst in der K\u00fchlung leiser Abendwinde", "tokens": ["Einst", "in", "der", "K\u00fch\u00b7lung", "lei\u00b7ser", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sa\u00dfen wir am Gemurmel eines Baches,", "tokens": ["Sa\u00b7\u00dfen", "wir", "am", "Ge\u00b7mur\u00b7mel", "ei\u00b7nes", "Ba\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und ich sprach aus zitternder Seele dir: \u201eich", "tokens": ["Und", "ich", "sprach", "aus", "zit\u00b7tern\u00b7der", "See\u00b7le", "dir", ":", "\u201e", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER", "$.", "$(", "PPER"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201eliebe dich ewig!\u201c", "tokens": ["\u201e", "lie\u00b7be", "dich", "e\u00b7wig", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber du neigtest sinnend nach den Wellen,", "tokens": ["A\u00b7ber", "du", "neig\u00b7test", "sin\u00b7nend", "nach", "den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach den fl\u00fcchtigen, tief dein sch\u00f6nes Antlitz,", "tokens": ["Nach", "den", "fl\u00fcch\u00b7ti\u00b7gen", ",", "tief", "dein", "sch\u00f6\u00b7nes", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie ergriffen von dem Gefl\u00fcster dunkler", "tokens": ["Wie", "er\u00b7grif\u00b7fen", "von", "dem", "Ge\u00b7fl\u00fcs\u00b7ter", "dunk\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Stimmen der Zukunft.", "tokens": ["Stim\u00b7men", "der", "Zu\u00b7kunft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Schmerzlich ber\u00fchrt von deinem Schweigen, frug ich,", "tokens": ["Schmerz\u00b7lich", "be\u00b7r\u00fchrt", "von", "dei\u00b7nem", "Schwei\u00b7gen", ",", "frug", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ob vernommen das Wort du meiner Seele,", "tokens": ["Ob", "ver\u00b7nom\u00b7men", "das", "Wort", "du", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und du nicktest hold; doch es d\u00fcnkte mir dein", "tokens": ["Und", "du", "nick\u00b7test", "hold", ";", "doch", "es", "d\u00fcnk\u00b7te", "mir", "dein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "$.", "KON", "PPER", "VVFIN", "PPER", "PPOSAT"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicken zu wenig. \u2014", "tokens": ["Ni\u00b7cken", "zu", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PIS", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Gl\u00fchende Thr\u00e4nen st\u00fcrzen mir vom Auge,", "tokens": ["Gl\u00fc\u00b7hen\u00b7de", "Thr\u00e4\u00b7nen", "st\u00fcr\u00b7zen", "mir", "vom", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sie pochen an deine kalte Stirne,", "tokens": ["Und", "sie", "po\u00b7chen", "an", "dei\u00b7ne", "kal\u00b7te", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ach, von der geflohen dahin das stille", "tokens": ["Ach", ",", "von", "der", "ge\u00b7flo\u00b7hen", "da\u00b7hin", "das", "stil\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ART", "ADJA", "PAV", "ART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Sinnen der Liebe.", "tokens": ["Sin\u00b7nen", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Meine gebrochne Stimme ruft dir bange.", "tokens": ["Mei\u00b7ne", "ge\u00b7broch\u00b7ne", "Stim\u00b7me", "ruft", "dir", "ban\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nach: \u201eich liebe dich ewig!\u201c o wie selig", "tokens": ["Nach", ":", "\u201e", "ich", "lie\u00b7be", "dich", "e\u00b7wig", "!", "\u201c", "o", "wie", "se\u00b7lig"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "$(", "PPER", "VVFIN", "PPER", "ADJD", "$.", "$(", "FM", "KOKOM", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "W\u00e4r' ich nun, antwortete meinem Schmerz dein", "tokens": ["W\u00e4r'", "ich", "nun", ",", "ant\u00b7wor\u00b7te\u00b7te", "mei\u00b7nem", "Schmerz", "dein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leisestes Nicken!", "tokens": ["Lei\u00b7ses\u00b7tes", "Ni\u00b7cken", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}