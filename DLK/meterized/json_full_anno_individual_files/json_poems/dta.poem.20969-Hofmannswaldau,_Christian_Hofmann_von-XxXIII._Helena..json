{"dta.poem.20969": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.  \n Helena.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gifftloch hat zu Rom viel tausend auffgerie-", "tokens": ["Das", "Gifft\u00b7loch", "hat", "zu", "Rom", "viel", "tau\u00b7send", "auff\u00b7ge\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NE", "ADV", "CARD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Durch dich/ O Helena/ sind ihrer mehr geblieben.", "tokens": ["Durch", "dich", "/", "O", "He\u00b7le\u00b7na", "/", "sind", "ih\u00b7rer", "mehr", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "NE", "NE", "$(", "VAFIN", "PPOSAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwar jenes hat ein Mann mit Pferd und Schwerd", "tokens": ["Zwar", "je\u00b7nes", "hat", "ein", "Mann", "mit", "Pferd", "und", "Schwerd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "gef\u00fcllet/", "tokens": ["ge\u00b7f\u00fcl\u00b7let", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Doch hie hat keiner recht die geile Lust gestillet.", "tokens": ["Doch", "hie", "hat", "kei\u00b7ner", "recht", "die", "gei\u00b7le", "Lust", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}