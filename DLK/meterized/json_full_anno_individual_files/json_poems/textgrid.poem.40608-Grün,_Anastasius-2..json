 "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein duft'ger Zauberring.", "tokens": ["Ein", "duft'\u00b7ger", "Zau\u00b7ber\u00b7ring", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie trotzt dem rauhen Wetter", "tokens": ["Sie", "trotzt", "dem", "rau\u00b7hen", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00fctet, lenzgeweiht,", "tokens": ["Und", "h\u00fc\u00b7tet", ",", "lenz\u00b7ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Rahmen weicher Bl\u00e4tter", "tokens": ["Im", "Rah\u00b7men", "wei\u00b7cher", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Rosenzeit.", "tokens": ["Die", "gan\u00b7ze", "Ro\u00b7sen\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vergessen an der Hecke", "tokens": ["Ver\u00b7ges\u00b7sen", "an", "der", "He\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch eine Traube hing,", "tokens": ["Noch", "ei\u00b7ne", "Trau\u00b7be", "hing", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die in dem Blattversteck