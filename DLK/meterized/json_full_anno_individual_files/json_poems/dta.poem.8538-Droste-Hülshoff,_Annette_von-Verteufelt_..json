{"dta.poem.8538": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Verteufelt .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Naht, o naht dem Gewande nicht", "tokens": ["Naht", ",", "o", "naht", "dem", "Ge\u00b7wan\u00b7de", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Des todten Hundes, des Giauren,", "tokens": ["Des", "tod\u00b7ten", "Hun\u00b7des", ",", "des", "Gi\u00b7au\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der erschlagen den Muselmann", "tokens": ["Der", "er\u00b7schla\u00b7gen", "den", "Mu\u00b7sel\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An Mekka\u2019s heiliger Pforte!", "tokens": ["An", "Mek\u00b7ka's", "hei\u00b7li\u00b7ger", "Pfor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nehmt auch die kleinen Kinder fort,", "tokens": ["Nehmt", "auch", "die", "klei\u00b7nen", "Kin\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie es nimmer erschauen;", "tokens": ["Da\u00df", "sie", "es", "nim\u00b7mer", "er\u00b7schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Denn die Dschinnen hauchten\u2019s an,", "tokens": ["Denn", "die", "Dschin\u00b7nen", "hauch\u00b7ten's", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und Iblis, der dreimal verruchte.", "tokens": ["Und", "Ib\u00b7lis", ",", "der", "drei\u00b7mal", "ver\u00b7ruch\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}