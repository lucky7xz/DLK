{"textgrid.poem.59276": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Leben, nicht so wild die Locken wehen", "tokens": ["La\u00df", ",", "Le\u00b7ben", ",", "nicht", "so", "wild", "die", "Lo\u00b7cken", "we\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "NN", "$,", "PTKNEG", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es will so rascher Ritt mir nicht mehr gl\u00fccken,", "tokens": ["Es", "will", "so", "ra\u00b7scher", "Ritt", "mir", "nicht", "mehr", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch \u00fcberm Land von diamantnen Br\u00fccken:", "tokens": ["Hoch", "\u00fc\u00b7berm", "Land", "von", "di\u00b7a\u00b7mant\u00b7nen", "Br\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mir schwindelt, in den Glanz hinabzusehen.", "tokens": ["Mir", "schwin\u00b7delt", ",", "in", "den", "Glanz", "hin\u00b7ab\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbvom Rosse spielend meine Blicke gehen", "tokens": ["\u00bb", "vom", "Ros\u00b7se", "spie\u00b7lend", "mei\u00b7ne", "Bli\u00b7cke", "ge\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach j\u00fcngern Augen, die mein Herz ber\u00fccken,", "tokens": ["Nach", "j\u00fcn\u00b7gern", "Au\u00b7gen", ",", "die", "mein", "Herz", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Horch, wie der Fr\u00fchling aufjauchzt vor Entz\u00fccken,", "tokens": ["Horch", ",", "wie", "der", "Fr\u00fch\u00b7ling", "auf\u00b7jauchzt", "vor", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kannst du nicht mit hinab, la\u00df ich dich stehen.\u00ab", "tokens": ["Kannst", "du", "nicht", "mit", "hin\u00b7ab", ",", "la\u00df", "ich", "dich", "ste\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PTKVZ", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kaum noch herzinnig mein, wendst du dich wieder,", "tokens": ["Kaum", "noch", "her\u00b7zin\u00b7nig", "mein", ",", "wendst", "du", "dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPOSAT", "$,", "KOUS", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": 