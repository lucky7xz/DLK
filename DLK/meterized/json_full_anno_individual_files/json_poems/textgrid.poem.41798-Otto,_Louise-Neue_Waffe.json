{"textgrid.poem.41798": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Neue Waffe", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob eine Welt sich wider mich verschworen,", "tokens": ["Ob", "ei\u00b7ne", "Welt", "sich", "wi\u00b7der", "mich", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob kampfger\u00fcstet meine Feinde stehn,", "tokens": ["Ob", "kampf\u00b7ge\u00b7r\u00fcs\u00b7tet", "mei\u00b7ne", "Fein\u00b7de", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob jeder Tag ein neues Leid geboren,", "tokens": ["Ob", "je\u00b7der", "Tag", "ein", "neu\u00b7es", "Leid", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und keine H\u00fclfe ringsum noch zu sehn:", "tokens": ["Und", "kei\u00b7ne", "H\u00fcl\u00b7fe", "ring\u00b7sum", "noch", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob mich bedrohen, t\u00fcckische D\u00e4monen,", "tokens": ["Ob", "mich", "be\u00b7dro\u00b7hen", ",", "t\u00fc\u00b7cki\u00b7sche", "D\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dienstbare Geister einer finstern Macht,", "tokens": ["Dienst\u00b7ba\u00b7re", "Geis\u00b7ter", "ei\u00b7ner", "fins\u00b7tern", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die tagesscheu nur mag im Dunkel thronen", "tokens": ["Die", "ta\u00b7ges\u00b7scheu", "nur", "mag", "im", "Dun\u00b7kel", "thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Reich des B\u00f6sen und der d\u00fcstern Nacht \u2013", "tokens": ["Im", "Reich", "des", "B\u00f6\u00b7sen", "und", "der", "d\u00fcs\u00b7tern", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich zittre nicht ich bin getrost und heiter,", "tokens": ["Ich", "zitt\u00b7re", "nicht", "ich", "bin", "ge\u00b7trost", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "VAFIN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und furchtlos wandle ich die stille Bahn", "tokens": ["Und", "furcht\u00b7los", "wand\u00b7le", "ich", "die", "stil\u00b7le", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu meinem Ziel mit sichrem Schritte weiter,", "tokens": ["Zu", "mei\u00b7nem", "Ziel", "mit", "sich\u00b7rem", "Schrit\u00b7te", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mitleid nur f\u00fchl ich bei der Feinde Wahn.", "tokens": ["Mit\u00b7leid", "nur", "f\u00fchl", "ich", "bei", "der", "Fein\u00b7de", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die alte Fahne wu\u00dft ich festzuhalten,", "tokens": ["Die", "al\u00b7te", "Fah\u00b7ne", "wu\u00dft", "ich", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie ist noch nicht verschossen und zerschlitzt!", "tokens": ["Sie", "ist", "noch", "nicht", "ver\u00b7schos\u00b7sen", "und", "zer\u00b7schlitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich darf sie rein, ich darf sie stolz entfalten,", "tokens": ["Ich", "darf", "sie", "rein", ",", "ich", "darf", "sie", "stolz", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df hell und gl\u00e4nzend ihre Farbe blitzt.", "tokens": ["Da\u00df", "hell", "und", "gl\u00e4n\u00b7zend", "ih\u00b7re", "Far\u00b7be", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Schwert und Helm, die kriegerischen Zeichen,", "tokens": ["Doch", "Schwert", "und", "Helm", ",", "die", "krie\u00b7ge\u00b7ri\u00b7schen", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hing sie auf am heiligsten Altar,", "tokens": ["Ich", "hing", "sie", "auf", "am", "hei\u00b7ligs\u00b7ten", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und werde darum nicht vom Kampfplatz weichen,", "tokens": ["Und", "wer\u00b7de", "da\u00b7rum", "nicht", "vom", "Kampf\u00b7platz", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich der Waffen auch, des Schutzes bar.", "tokens": ["Bin", "ich", "der", "Waf\u00b7fen", "auch", ",", "des", "Schut\u00b7zes", "bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das Schwert vertauscht, ich mit dem Saitenspiele,", "tokens": ["Das", "Schwert", "ver\u00b7tauscht", ",", "ich", "mit", "dem", "Sai\u00b7ten\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Helm mit einem immergr\u00fcnen Kranz,", "tokens": ["Den", "Helm", "mit", "ei\u00b7nem", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch immer wandle ich zum selben Ziele,", "tokens": ["Doch", "im\u00b7mer", "wand\u00b7le", "ich", "zum", "sel\u00b7ben", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob jetzt auch wehrlos, ohne Waffenglanz.", "tokens": ["Ob", "jetzt", "auch", "wehr\u00b7los", ",", "oh\u00b7ne", "Waf\u00b7fen\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "$,", "KOUI", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich hab ein holdes Zauberwort vernommen", "tokens": ["Ich", "hab", "ein", "hol\u00b7des", "Zau\u00b7ber\u00b7wort", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unverwundbar hat es mich gemacht,", "tokens": ["Und", "un\u00b7ver\u00b7wund\u00b7bar", "hat", "es", "mich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Himmel ist's, ein Sonnenstrahl, gekommen", "tokens": ["Vom", "Him\u00b7mel", "ist's", ",", "ein", "Son\u00b7nen\u00b7strahl", ",", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VAFIN", "$,", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hat die Lieb' im Herzen angefacht.", "tokens": ["Und", "hat", "die", "Lieb'", "im", "Her\u00b7zen", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und wenn ich sonst im Ha\u00df nur Mut gefunden,", "tokens": ["Und", "wenn", "ich", "sonst", "im", "Ha\u00df", "nur", "Mut", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im kalten Trotz hielt in der Hand das Schwert:", "tokens": ["Im", "kal\u00b7ten", "Trotz", "hielt", "in", "der", "Hand", "das", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt hat die Lieb' der Freiheit mich verbunden,", "tokens": ["Jetzt", "hat", "die", "Lieb'", "der", "Frei\u00b7heit", "mich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt k\u00e4mpfe ich von ihrer Macht verkl\u00e4rt.", "tokens": ["Jetzt", "k\u00e4mp\u00b7fe", "ich", "von", "ih\u00b7rer", "Macht", "ver\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie ist mein Schild, mit dem ich siegen werde,", "tokens": ["Sie", "ist", "mein", "Schild", ",", "mit", "dem", "ich", "sie\u00b7gen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Feind erlahmt, wenn er es nur erschaut \u2013", "tokens": ["Der", "Feind", "er\u00b7lahmt", ",", "wenn", "er", "es", "nur", "er\u00b7schaut", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was braucht es Waffen noch von dieser Erde,", "tokens": ["Was", "braucht", "es", "Waf\u00b7fen", "noch", "von", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn solche Wehr der Himmel mir vertraut?", "tokens": ["Wenn", "sol\u00b7che", "Wehr", "der", "Him\u00b7mel", "mir", "ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Ob eine Welt sich wider mich verschworen,", "tokens": ["Ob"