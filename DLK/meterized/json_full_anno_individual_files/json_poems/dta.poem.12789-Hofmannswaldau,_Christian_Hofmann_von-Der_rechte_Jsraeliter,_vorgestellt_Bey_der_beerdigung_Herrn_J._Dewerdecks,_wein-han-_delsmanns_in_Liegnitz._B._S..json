{"dta.poem.12789": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der rechte Jsraeliter,  \n vorgestellt  \n  Bey der beerdigung  \n Herrn J. Dewerdecks, wein-han-  \n delsmanns in Liegnitz.  \n B. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jtzt, da man schwartz und wei\u00df mit einer feder schreibt,", "tokens": ["Jtzt", ",", "da", "man", "schwartz", "und", "wei\u00df", "mit", "ei\u00b7ner", "fe\u00b7der", "schreibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da gall und honigseim aus einem munde qvellen,", "tokens": ["Da", "gall", "und", "ho\u00b7ni\u00b7gseim", "aus", "ei\u00b7nem", "mun\u00b7de", "qvel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenn die augen sich wie tauben-blicke stellen,", "tokens": ["Und", "wenn", "die", "au\u00b7gen", "sich", "wie", "tau\u00b7ben\u00b7bli\u00b7cke", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das hertze doch ein nest vergiffter schlangen bleibt:", "tokens": ["Das", "hert\u00b7ze", "doch", "ein", "nest", "ver\u00b7giff\u00b7ter", "schlan\u00b7gen", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jtzt, sag ich, ist die zeit, da redliche gem\u00fcther", "tokens": ["Jtzt", ",", "sag", "ich", ",", "ist", "die", "zeit", ",", "da", "red\u00b7li\u00b7che", "ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kaum mit der aloe in funffzig jahren bl\u00fchn:", "tokens": ["Kaum", "mit", "der", "a\u00b7loe", "in", "funff\u00b7zig", "jah\u00b7ren", "bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Es w\u00e4chst die wahre treu wie seltnes winter-gr\u00fcn,", "tokens": ["Es", "w\u00e4chst", "die", "wah\u00b7re", "treu", "wie", "selt\u00b7nes", "win\u00b7ter\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist so angenehm, als die verlegnen g\u00fcther:", "tokens": ["Und", "ist", "so", "an\u00b7ge\u00b7nehm", ",", "als", "die", "ver\u00b7leg\u00b7nen", "g\u00fc\u00b7ther", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der zeiten finsterni\u00df umnebelt dieses licht,", "tokens": ["Der", "zei\u00b7ten", "fins\u00b7ter\u00b7ni\u00df", "um\u00b7ne\u00b7belt", "die\u00b7ses", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df seine funcken kaum noch in der asche glimmen,", "tokens": ["Da\u00df", "sei\u00b7ne", "fun\u00b7cken", "kaum", "noch", "in", "der", "asc\u00b7he", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil hertz, mund und hand so schlecht zusammen stimmen,", "tokens": ["Und", "weil", "hertz", ",", "mund", "und", "hand", "so", "schlecht", "zu\u00b7sam\u00b7men", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "$,", "NN", "KON", "NN", "ADV", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bleibt die losung stets: Vertraue keinem nicht.", "tokens": ["So", "bleibt", "die", "lo\u00b7sung", "stets", ":", "Ver\u00b7trau\u00b7e", "kei\u00b7nem", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$.", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seit dem politisch-seyn zur mode morden ist,", "tokens": ["Seit", "dem", "po\u00b7li\u00b7tischseyn", "zur", "mo\u00b7de", "mor\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "H\u00f6rt man die redligkeit der einfalt titul f\u00fchren:", "tokens": ["H\u00f6rt", "man", "die", "red\u00b7lig\u00b7keit", "der", "ein\u00b7falt", "ti\u00b7tul", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der wahrheit edles gold mu\u00df glantz und werth verlieren,", "tokens": ["Der", "wahr\u00b7heit", "ed\u00b7les", "gold", "mu\u00df", "glantz", "und", "werth", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil lauter schlacken-werck der menschen thun umschliest.", "tokens": ["Weil", "lau\u00b7ter", "schla\u00b7cken\u00b7\u00b7werck", "der", "men\u00b7schen", "thun", "um\u00b7schliest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die unart dieser welt giebt ei\u00df vor crystallinen,", "tokens": ["Die", "un\u00b7art", "die\u00b7ser", "welt", "giebt", "ei\u00df", "vor", "crys\u00b7tal\u00b7li\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PDAT", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00fcllt die schwartze brust in schwanen-federn ein.", "tokens": ["Und", "h\u00fcllt", "die", "schwart\u00b7ze", "brust", "in", "schwa\u00b7nen\u00b7fe\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es mu\u00df die falschheit kunst, die t\u00fccke tugend seyn,", "tokens": ["Es", "mu\u00df", "die", "falschheit", "kunst", ",", "die", "t\u00fc\u00b7cke", "tu\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und worte voller pracht zu lauter fesseln dienen.", "tokens": ["Und", "wor\u00b7te", "vol\u00b7ler", "pracht", "zu", "lau\u00b7ter", "fes\u00b7seln", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der menschen falsches hertz kommt einer uhren bey,", "tokens": ["Der", "men\u00b7schen", "fal\u00b7sches", "hertz", "kommt", "ei\u00b7ner", "uh\u00b7ren", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die anders schl\u00e4gt und zeigt; ja die erfahrung lehret,", "tokens": ["Die", "an\u00b7ders", "schl\u00e4gt", "und", "zeigt", ";", "ja", "die", "er\u00b7fah\u00b7rung", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$.", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df offt der gr\u00f6ste freund, den man am meisten ehret,", "tokens": ["Da\u00df", "offt", "der", "gr\u00f6s\u00b7te", "freund", ",", "den", "man", "am", "meis\u00b7ten", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PIS", "APPRART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm munde Seneca, im hertzen Nero sey.", "tokens": ["Jm", "mun\u00b7de", "Se\u00b7ne\u00b7ca", ",", "im", "hert\u00b7zen", "Ne\u00b7ro", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$,", "APPRART", "NN", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Jhr! die ihr noch sehr tieff in solcher larve steckt,", "tokens": ["Ihr", "!", "die", "ihr", "noch", "sehr", "tieff", "in", "sol\u00b7cher", "lar\u00b7ve", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PRELS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entbl\u00f6set das gesicht bey diesen todten-gr\u00fcfften,", "tokens": ["Ent\u00b7bl\u00f6\u00b7set", "das", "ge\u00b7sicht", "bey", "die\u00b7sen", "tod\u00b7ten\u00b7gr\u00fcff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo wir der redligkeit das letzte denckmahl stifften.", "tokens": ["Wo", "wir", "der", "red\u00b7lig\u00b7keit", "das", "letz\u00b7te", "denck\u00b7mahl", "stiff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier ruht ein ehren-mann, den zwar die erde deckt,", "tokens": ["Hier", "ruht", "ein", "eh\u00b7ren\u00b7mann", ",", "den", "zwar", "die", "er\u00b7de", "deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch dessen nachruhm kan auch in dem tode zeigen,", "tokens": ["Doch", "des\u00b7sen", "nach\u00b7ruhm", "kan", "auch", "in", "dem", "to\u00b7de", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df weder grufft noch nacht die tugend decken kan.", "tokens": ["Da\u00df", "we\u00b7der", "grufft", "noch", "nacht", "die", "tu\u00b7gend", "de\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KON", "VVFIN", "ADV", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schaut den erbla\u00dften mund als einen lehrer an,", "tokens": ["Schaut", "den", "er\u00b7bla\u00df\u00b7ten", "mund", "als", "ei\u00b7nen", "leh\u00b7rer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der euch gesetze giebt im reden und im schweigen:", "tokens": ["Der", "euch", "ge\u00b7set\u00b7ze", "giebt", "im", "re\u00b7den", "und", "im", "schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "VVFIN", "APPRART", "VVINF", "KON", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie man vor GOtt und welt sich ohne falsch bezeigt.", "tokens": ["Wie", "man", "vor", "Gott", "und", "welt", "sich", "oh\u00b7ne", "falsch", "be\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "KON", "VVFIN", "PRF", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lernt euren wandel hier auf treu und wahrheit gr\u00fcnden,", "tokens": ["Lernt", "eu\u00b7ren", "wan\u00b7del", "hier", "auf", "treu", "und", "wahr\u00b7heit", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lernt, wie man wort und werck durch liebe mu\u00df verbinden,", "tokens": ["Lernt", ",", "wie", "man", "wort", "und", "werck", "durch", "lie\u00b7be", "mu\u00df", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "NN", "KON", "NN", "APPR", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit ein guter ruch aus eurem grabe steigt.", "tokens": ["Da\u00b7mit", "ein", "gu\u00b7ter", "ruch", "aus", "eu\u00b7rem", "gra\u00b7be", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Verzeihe, Seeligster! wenn ich dich loben will,", "tokens": ["Ver\u00b7zei\u00b7he", ",", "See\u00b7ligs\u00b7ter", "!", "wenn", "ich", "dich", "lo\u00b7ben", "will", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist bey lebens-zeit dem ruhme feind gewesen;", "tokens": ["Du", "bist", "bey", "le\u00b7bens\u00b7zeit", "dem", "ruh\u00b7me", "feind", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch was die wahrheit schreibt, das mag ein ieder lesen:", "tokens": ["Doch", "was", "die", "wahr\u00b7heit", "schreibt", ",", "das", "mag", "ein", "ie\u00b7der", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VMFIN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es war die redligkeit dein auserlesnes ziel.", "tokens": ["Es", "war", "die", "red\u00b7lig\u00b7keit", "dein", "au\u00b7ser\u00b7les\u00b7nes", "ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-