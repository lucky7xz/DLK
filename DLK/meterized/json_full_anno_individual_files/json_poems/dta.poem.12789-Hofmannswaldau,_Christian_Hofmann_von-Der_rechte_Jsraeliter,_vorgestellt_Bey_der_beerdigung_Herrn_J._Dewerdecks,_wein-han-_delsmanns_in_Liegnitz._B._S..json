 ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem, der dir seine noth und seinen kummer klagte;", "tokens": ["Dem", ",", "der", "dir", "sei\u00b7ne", "noth", "und", "sei\u00b7nen", "kum\u00b7mer", "klag\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein wunder, wenn man nun bey deinem tode sagte:", "tokens": ["Kein", "wun\u00b7der", ",", "wenn", "man", "nun", "bey", "dei\u00b7nem", "to\u00b7de", "sag\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die leute deiner art sind, leyder! gar zu rar.", "tokens": ["Die", "leu\u00b7te", "dei\u00b7ner", "art", "sind", ",", "ley\u00b7der", "!", "gar", "zu", "rar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "$.", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Wie unverf\u00e4lscht war doch dein guter lebens-lauf?", "tokens": ["Wie", "un\u00b7ver\u00b7f\u00e4lscht", "war", "doch", "dein", "gu\u00b7ter", "le\u00b7bens\u00b7lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es heuchelt ja die welt mit ihrem Christenthume:", "tokens": ["Es", "heu\u00b7chelt", "ja", "die", "welt", "mit", "ih\u00b7rem", "Chris\u00b7ten\u00b7thu\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie sucht nur euserlich die fr\u00f6mmigkeit zum ruhme;", "tokens": ["Sie", "sucht", "nur", "eu\u00b7ser\u00b7lich", "die", "fr\u00f6m\u00b7mig\u00b7keit", "zum", "ruh\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir aber gieng das licht in deinem hertzen auf.", "tokens": ["Dir", "a\u00b7ber", "gieng", "das", "licht", "in", "dei\u00b7nem", "hert\u00b7zen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Dein eifriges gebet, dein stetes bibel-lesen,", "tokens": ["Dein", "eif\u00b7ri\u00b7ges", "ge\u00b7bet", ",", "dein", "ste\u00b7tes", "bi\u00b7bel\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein flei\u00dfig kirchen-gehn, die k\u00f6nnen zeuge seyn,", "tokens": ["Dein", "flei\u00b7\u00dfig", "kir\u00b7chen\u00b7gehn", ",", "die", "k\u00f6n\u00b7nen", "zeu\u00b7ge", "seyn", ","], "token_info": ["word", "word", "word", "punct", 