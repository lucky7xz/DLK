 "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem wird die tugend selbst sein lob in marmor graben.", "tokens": ["Dem", "wird", "die", "tu\u00b7gend", "selbst", "sein", "lob", "in", "mar\u00b7mor", "gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.27": {"line.1": {"text": "Ach ja! wir sehen schon, wie sehr ihr ihn geklagt,", "tokens": ["Ach", "ja", "!", "wir", "se\u00b7hen", "schon", ",", "wie", "sehr", "ihr", "ihn", "ge\u00b7klagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ADV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihm die grabschrifft setzt: Mein leser! wilst du wissen,", "tokens": ["Und", "ihm", "die", "grab\u00b7schrifft", "setzt", ":", "Mein", "le\u00b7ser", "!", "wilst", "du", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer hier sein mattes haupt zur ruhe legen m\u00fcssen?", "tokens": ["Wer", "hier", "sein", "mat\u00b7tes", "haupt", "zur", "ru\u00b7he", "le\u00b7gen", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er lebte schlecht und recht. Ich habe gnug gesagt.", "tokens": ["Er", "leb\u00b7te", "schlecht", "und", "recht", ".", "Ich", "ha\u00b7be", "gnug", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}