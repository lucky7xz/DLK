{"textgrid.poem.42192": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcbe Stunden schleichen sachte", "tokens": ["Tr\u00fc\u00b7be", "Stun\u00b7den", "schlei\u00b7chen", "sach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die stille Seele mir;", "tokens": ["Durch", "die", "stil\u00b7le", "See\u00b7le", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fcck, das ich zu haschen dachte,", "tokens": ["Gl\u00fcck", ",", "das", "ich", "zu", "ha\u00b7schen", "dach\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so ferne bin ich dir!", "tokens": ["Wie", "so", "fer\u00b7ne", "bin", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00fchsam schleppt sich meine Feder", "tokens": ["M\u00fch\u00b7sam", "schleppt", "sich", "mei\u00b7ne", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber ein zerknicktes Blatt,", "tokens": ["\u00dc\u00b7ber", "ein", "zer\u00b7knick\u00b7tes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leis bewimmernd, was ein jeder", "tokens": ["Leis", "be\u00b7wim\u00b7mernd", ",", "was", "ein", "je\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "PRELS", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal zu verschmerzen hat.", "tokens": ["Ein\u00b7mal", "zu", "ver\u00b7schmer\u00b7zen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn den alten Mut ich f\u00e4nde,", "tokens": ["Wenn", "den", "al\u00b7ten", "Mut", "ich", "f\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4nd ich auch die alte Kraft \u2013", "tokens": ["F\u00e4nd", "ich", "auch", "die", "al\u00b7te", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, die wundgestraften H\u00e4nde", "tokens": ["Ach", ",", "die", "wund\u00b7ges\u00b7traf\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind auf lange Zeit erschlafft.", "tokens": ["Sind", "auf", "lan\u00b7ge", "Zeit", "er\u00b7schlafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tr\u00fcbe Stunden schleichen sa