{"textgrid.poem.32815": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt zu! ich will die Weisheit singen:", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt zu! ich will die Weisheit singen:", "tokens": ["H\u00f6rt", "zu", "!", "ich", "will", "die", "Weis\u00b7heit", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst, sich selbsten zu bezwingen,", "tokens": ["Die", "Kunst", ",", "sich", "selbs\u00b7ten", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kenn ich, ich kenne sie allein.", "tokens": ["Kenn", "ich", ",", "ich", "ken\u00b7ne", "sie", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es lehrt kein Dockter und Professer", "tokens": ["Es", "lehrt", "kein", "Dock\u00b7ter", "und", "Pro\u00b7fes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie leichter, gr\u00fcndlicher und besser:", "tokens": ["Sie", "leich\u00b7ter", ",", "gr\u00fcnd\u00b7li\u00b7cher", "und", "bes\u00b7ser", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trinkt Wein!", "tokens": ["Trinkt", "Wein", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Die\u00df lernt euch weise seyn.", "tokens": ["Die\u00df", "lernt", "euch", "wei\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00fc\u00dft ihr euch vor Markolphen beugen,", "tokens": ["M\u00fc\u00dft", "ihr", "euch", "vor", "Mar\u00b7kol\u00b7phen", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Seht ihr ihn t\u00e4glich h\u00f6her steigen,", "tokens": ["Seht", "ihr", "ihn", "t\u00e4g\u00b7lich", "h\u00f6\u00b7her", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man weist euch ab, l\u00e4\u00dft Narren ein:", "tokens": ["Man", "weist", "euch", "ab", ",", "l\u00e4\u00dft", "Nar\u00b7ren", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O la\u00dft die Narren beyeinander,", "tokens": ["O", "la\u00dft", "die", "Nar\u00b7ren", "be\u00b7yein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und schleichet aus der