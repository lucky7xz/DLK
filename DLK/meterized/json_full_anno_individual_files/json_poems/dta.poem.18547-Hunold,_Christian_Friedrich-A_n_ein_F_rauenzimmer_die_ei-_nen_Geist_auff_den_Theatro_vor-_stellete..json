{"dta.poem.18547": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n ein  F rauenzimmer/ die ei-  \n nen Geist auff den Theatro vor-  \n stellete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt es/ art'ger Geist! da\u00df mich dein Wesen blendet/", "tokens": ["Wie", "kommt", "es", "/", "art'\u00b7ger", "Geist", "!", "da\u00df", "mich", "dein", "We\u00b7sen", "blen\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "ADJA", "NN", "$.", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Wesen/ da\u00df man sonst bey keinen Geiste sieht?", "tokens": ["Dein", "We\u00b7sen", "/", "da\u00df", "man", "sonst", "bey", "kei\u00b7nen", "Geis\u00b7te", "sieht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PIS", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie da\u00df dir meine Brust so manchen Seuffzer sendet/", "tokens": ["Wie", "da\u00df", "dir", "mei\u00b7ne", "Brust", "so", "man\u00b7chen", "Seuff\u00b7zer", "sen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich vielleicht umsonst um deine Gunst bem\u00fcth?", "tokens": ["Der", "sich", "viel\u00b7leicht", "um\u00b7sonst", "um", "dei\u00b7ne", "Gunst", "be\u00b7m\u00fcth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn ein wahrhaffter Geist kan nicht warhafftig lieben/", "tokens": ["Denn", "ein", "wahr\u00b7haff\u00b7ter", "Geist", "kan", "nicht", "war\u00b7haff\u00b7tig", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dieweil er weder Gluth noch heisse Regung kennt:", "tokens": ["Die\u00b7weil", "er", "we\u00b7der", "Gluth", "noch", "heis\u00b7se", "Re\u00b7gung", "kennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dieser wird ins Buch der Thoren eingeschrieben/", "tokens": ["Und", "die\u00b7ser", "wird", "ins", "Buch", "der", "Tho\u00b7ren", "ein\u00b7ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer sonder Hoffnung liebt/ und ohne Feuer brennt.", "tokens": ["Wer", "son\u00b7der", "Hoff\u00b7nung", "liebt", "/", "und", "oh\u00b7ne", "Feu\u00b7er", "brennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$(", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ich/ ich Th\u00f6richter/ mu\u00df selbst mein Urtheil f\u00e4llen:", "tokens": ["Und", "ich", "/", "ich", "Th\u00f6\u00b7rich\u00b7ter", "/", "mu\u00df", "selbst", "mein", "Ur\u00b7theil", "f\u00e4l\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "NN", "$(", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Mein Hertz zerfliesset fast in einer stummen Gluth/", "tokens": ["Mein", "Hertz", "zer\u00b7flies\u00b7set", "fast", "in", "ei\u00b7ner", "stum\u00b7men", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und dennoch soll vor ihn ein k\u00fchler Julep quellen/", "tokens": ["Und", "den\u00b7noch", "soll", "vor", "ihn", "ein", "k\u00fch\u00b7ler", "Ju\u00b7lep", "quel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo Venus stilles Meer nicht heget Ebb' und Fluth.", "tokens": ["Wo", "Ve\u00b7nus", "stil\u00b7les", "Meer", "nicht", "he\u00b7get", "Ebb'", "und", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADJA", "NN", "PTKNEG", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So speiset mich das Gl\u00fcck mit Damascener Fr\u00fcchten/", "tokens": ["So", "spei\u00b7set", "mich", "das", "Gl\u00fcck", "mit", "Da\u00b7mas\u00b7ce\u00b7ner", "Fr\u00fcch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da ihre Sch\u00f6nheit nichts als Aschen in sich h\u00e4lt.", "tokens": ["Da", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "nichts", "als", "A\u00b7schen", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wird mein Hertze sich nur lauter Marter dichten.", "tokens": ["So", "wird", "mein", "Hert\u00b7ze", "sich", "nur", "lau\u00b7ter", "Mar\u00b7ter", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "PRF", "ADV", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil es dem sch\u00f6nen Geist umsonst zu Fusse f\u00e4llt.", "tokens": ["Weil", "es", "dem", "sch\u00f6\u00b7nen", "Geist", "um\u00b7sonst", "zu", "Fus\u00b7se", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ist beliebter Geist/ dein ungemeines Wesen", "tokens": ["Und", "ist", "be\u00b7lieb\u00b7ter", "Geist", "/", "dein", "un\u00b7ge\u00b7mei\u00b7nes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Gantz ohne Fleisch und Blut und Unempfindlichkeit/", "tokens": ["Gantz", "oh\u00b7ne", "Fleisch", "und", "Blut", "und", "Un\u00b7emp\u00b7find\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So hat der Himmel dich zu unsrer Straff erlesen/", "tokens": ["So", "hat", "der", "Him\u00b7mel", "dich", "zu", "uns\u00b7rer", "Straff", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil jeder halb entseelt vergebens Weyrauch streut.", "tokens": ["Weil", "je\u00b7der", "halb", "ent\u00b7seelt", "ver\u00b7ge\u00b7bens", "Wey\u00b7rauch", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein da die Gestalt gleichwohl vollkommen blitzet/", "tokens": ["Al\u00b7lein", "da", "die", "Ge\u00b7stalt", "gleich\u00b7wohl", "voll\u00b7kom\u00b7men", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da mich die Zauberey zu deinen Sclaven macht/", "tokens": ["Da", "mich", "die", "Zau\u00b7be\u00b7rey", "zu", "dei\u00b7nen", "Scla\u00b7ven", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da deiner Augen Gluth der Sonnen gleich erhitzet;", "tokens": ["Da", "dei\u00b7ner", "Au\u00b7gen", "Gluth", "der", "Son\u00b7nen", "gleich", "er\u00b7hit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So sey doch/ wie du kanst/ auff K\u00fchlung auch bedacht:", "tokens": ["So", "sey", "doch", "/", "wie", "du", "kanst", "/", "auff", "K\u00fch\u00b7lung", "auch", "be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$(", "PWAV", "PPER", "VMFIN", "$(", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du wirst sie sch\u00f6ner Geist/ am aller besten wissen/", "tokens": ["Du", "wirst", "sie", "sch\u00f6\u00b7ner", "Geist", "/", "am", "al\u00b7ler", "bes\u00b7ten", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "$(", "PTKA", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du kanst ja sonder M\u00fch' als wie ein Engel gehn-", "tokens": ["Du", "kanst", "ja", "son\u00b7der", "M\u00fch'", "als", "wie", "ein", "En\u00b7gel", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "KOUS", "KOKOM", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Darff ich in der Gestalt dich denn nur einmahl k\u00fcssen/", "tokens": ["Darff", "ich", "in", "der", "Ge\u00b7stalt", "dich", "denn", "nur", "ein\u00b7mahl", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.28": {"text": "So kan ich mich vergn\u00fcgt in meiner Liebe sehn.", "tokens": ["So", "kan", "ich", "mich", "ver\u00b7gn\u00fcgt", "in", "mei\u00b7ner", "Lie\u00b7be", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Allein mu\u00df deinen Mund nur sch\u00f6nes Blendwerck banen/", "tokens": ["Al\u00b7lein", "mu\u00df", "dei\u00b7nen", "Mund", "nur", "sch\u00f6\u00b7nes", "Blend\u00b7werck", "ba\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und ist der Schein zwar gut/ doch die Empfindung nicht;", "tokens": ["Und", "ist", "der", "Schein", "zwar", "gut", "/", "doch", "die", "Emp\u00b7fin\u00b7dung", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$(", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So la\u00df mich auch den Schein der andern Glieder schauen/", "tokens": ["So", "la\u00df", "mich", "auch", "den", "Schein", "der", "an\u00b7dern", "Glie\u00b7der", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Vielleicht sind sie so sch\u00f6n/ wie aussen dein Gesicht.", "tokens": ["Viel\u00b7leicht", "sind", "sie", "so", "sch\u00f6n", "/", "wie", "aus\u00b7sen", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "PWAV", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein Blick vergn\u00fcget auch/ da\u00df wissen tausend Augen/", "tokens": ["Ein", "Blick", "ver\u00b7gn\u00fc\u00b7get", "auch", "/", "da\u00df", "wis\u00b7sen", "tau\u00b7send", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "KOUS", "VVFIN", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die durch Crystallen Gla\u00df die Liebste nackend sehn:", "tokens": ["Die", "durch", "Crys\u00b7tal\u00b7len", "Gla\u00df", "die", "Liebs\u00b7te", "na\u00b7ckend", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wozu soll nun bey dir die Pracht der Kleider taugen?", "tokens": ["Wo\u00b7zu", "soll", "nun", "bey", "dir", "die", "Pracht", "der", "Klei\u00b7der", "tau\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.36": {"text": "Ach glaube/ blo\u00df mu\u00df dir doch alles sch\u00f6ner stehn.", "tokens": ["Ach", "glau\u00b7be", "/", "blo\u00df", "mu\u00df", "dir", "doch", "al\u00b7les", "sch\u00f6\u00b7ner", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "ADV", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun Engel kommen ja zur Menschen Lust auff Erden/", "tokens": ["Nun", "En\u00b7gel", "kom\u00b7men", "ja", "zur", "Men\u00b7schen", "Lust", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADV", "APPRART", "NN", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Drum stelle dich zum Schutz bey meinen Lager ein;", "tokens": ["Drum", "stel\u00b7le", "dich", "zum", "Schutz", "bey", "mei\u00b7nen", "La\u00b7ger", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich denck' es solte wohl was menschliches draus werden/", "tokens": ["Ich", "denck", "es", "sol\u00b7te", "wohl", "was", "menschli\u00b7ches", "draus", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VMFIN", "ADV", "PWS", "PIS", "PAV", "VAINF", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Kan nur mein sch\u00f6ner Geist in meinen Armen seyn.", "tokens": ["Kan", "nur", "mein", "sch\u00f6\u00b7ner", "Geist", "in", "mei\u00b7nen", "Ar\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}