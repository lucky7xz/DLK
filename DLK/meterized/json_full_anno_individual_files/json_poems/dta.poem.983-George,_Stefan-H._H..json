{"dta.poem.983": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "H. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.85", "da:0.14"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Erfinder rollenden gesangs und spr&#252;hend                 ", "tokens": ["Er\u00b7fin\u00b7der", "rol\u00b7len\u00b7den", "ge\u00b7sangs", "und", "spr", "&#252;", "hend"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["NN", "VVFIN", "ADV", "KON", "VVFIN", "$(", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewandter zwiegespr&#228;che: frist und trennung                 ", "tokens": ["Ge\u00b7wand\u00b7ter", "zwie\u00b7ge\u00b7spr", "&#228;", "che", ":", "frist", "und", "tren\u00b7nung"], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erlaubt dass ich auf meine d&#228;chtnisstafel                 ", "tokens": ["Er\u00b7laubt", "dass", "ich", "auf", "mei\u00b7ne", "d", "&#228;", "cht\u00b7nis\u00b7sta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["VVFIN", "KOUS", "PPER", "APPR", "PPOSAT", "XY", "XY", "XY"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den fr&#252;hern gegner grabe &#8212; thu desgleichen                 ", "tokens": ["Den", "fr", "&#252;", "hern", "geg\u00b7ner", "gra\u00b7be", "&#8212;", "thu", "des\u00b7glei\u00b7chen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn auf des rausches und der regung leiter                 ", "tokens": ["Denn", "auf", "des", "rau\u00b7sches", "und", "der", "re\u00b7gung", "lei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "KON", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind beide wir im sinken &#183; nie mehr werden                 ", "tokens": ["Sind", "bei\u00b7de", "wir", "im", "sin\u00b7ken", "&#183;", "nie", "mehr", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PPER", "APPRART", "VVINF", "$(", "ADV", "ADV", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der knaben preis und jubel so mir schmeicheln                 ", "tokens": ["Der", "kna\u00b7ben", "preis", "und", "ju\u00b7bel", "so", "mir", "schmei\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "KON", "ADJD", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nie wieder strofen so im ohr dir donnern.               ", "tokens": ["Nie", "wie\u00b7der", "stro\u00b7fen", "so", "im", "ohr", "dir", "don\u00b7nern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}