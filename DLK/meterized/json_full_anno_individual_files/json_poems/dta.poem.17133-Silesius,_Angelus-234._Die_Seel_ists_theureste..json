{"dta.poem.17133": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "234.  Die Seel ists theureste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich halte meine Seel f\u00fcrs theurest\u2019 auf der Erden:", "tokens": ["Ich", "hal\u00b7te", "mei\u00b7ne", "Seel", "f\u00fcrs", "theu\u00b7rest'", "auf", "der", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie mit Gottesblutt erkaufft hat m\u00fcssen werden.", "tokens": ["Weil", "sie", "mit", "Got\u00b7tes\u00b7blutt", "er\u00b7kaufft", "hat", "m\u00fcs\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}