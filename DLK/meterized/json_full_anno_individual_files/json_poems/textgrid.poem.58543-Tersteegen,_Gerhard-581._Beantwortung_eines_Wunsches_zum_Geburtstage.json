{"textgrid.poem.58543": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "581. Beantwortung eines Wunsches zum Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lobst beim neuen Jahr Gott meiner Jahre wegen", "tokens": ["Du", "lobst", "beim", "neu\u00b7en", "Jahr", "Gott", "mei\u00b7ner", "Jah\u00b7re", "we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "NN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00fcnschst zu k\u00fcnftigen mir neue Kraft und Segen;", "tokens": ["Und", "w\u00fcnschst", "zu", "k\u00fcnf\u00b7ti\u00b7gen", "mir", "neu\u00b7e", "Kraft", "und", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKZU", "VVFIN", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sei's! Mir w\u00e4r' f\u00fcr mich ein Jahr, ein Tag zu lang,", "tokens": ["So", "sei's", "!", "Mir", "w\u00e4r'", "f\u00fcr", "mich", "ein", "Jahr", ",", "ein", "Tag", "zu", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$.", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die l\u00e4ngste Zeit zu kurz zu Gottes Lob und Dank.", "tokens": ["Die", "l\u00e4ngs\u00b7te", "Zeit", "zu", "kurz", "zu", "Got\u00b7tes", "Lob", "und", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word",