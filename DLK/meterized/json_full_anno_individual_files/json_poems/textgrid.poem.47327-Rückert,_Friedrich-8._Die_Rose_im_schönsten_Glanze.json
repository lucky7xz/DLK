{"textgrid.poem.47327": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "8. Die Rose im sch\u00f6nsten Glanze", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der hat in ihrem sch\u00f6nsten Glanz die Rose nicht gesehen,", "tokens": ["Der", "hat", "in", "ih\u00b7rem", "sch\u00f6ns\u00b7ten", "Glanz", "die", "Ro\u00b7se", "nicht", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Wer nie die Perle des Gef\u00fchls ihr sah im Auge stehen.", "tokens": ["Wer", "nie", "die", "Per\u00b7le", "des", "Ge\u00b7f\u00fchls", "ihr", "sah", "im", "Au\u00b7ge", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "O Liebe! wunderbare Macht, da\u00df deine h\u00f6chste Wonne", "tokens": ["O", "Lie\u00b7be", "!", "wun\u00b7der\u00b7ba\u00b7re", "Macht", ",", "da\u00df", "dei\u00b7ne", "h\u00f6chs\u00b7te", "Won\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "In Menschenbrust den Ausdruck mu\u00df borgen von Schmerz und Wehen.", "tokens": ["In", "Men\u00b7schen\u00b7brust", "den", "Aus\u00b7druck", "mu\u00df", "bor\u00b7gen", "von", "Schmerz", "und", "We\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Rose l\u00e4chelte mich an, und von den s\u00fc\u00dfen Strahlen", "tokens": ["Die", "Ro\u00b7se", "l\u00e4\u00b7chel\u00b7te", "mich", "an", ",", "und", "von", "den", "s\u00fc\u00b7\u00dfen", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Ging mir im stillen Herzen auf ein Drang zu s\u00fc\u00dfem Flehen.", "tokens": ["Ging", "mir", "im", "stil\u00b7len", "Her\u00b7zen", "auf", "ein", "Drang", "zu", "s\u00fc\u00b7\u00dfem", "Fle\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Ich klagte wie die Nachtigall, bis meine Rose weinte;", "tokens": ["Ich", "klag\u00b7te", "wie", "die", "Nach\u00b7ti\u00b7gall", ",", "bis", "mei\u00b7ne", "Ro\u00b7se", "wein\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und wie ich's sah, verklagt' ich mich, da\u00df es durch mich geschehen.", "tokens": ["Und", "wie", "ich's", "sah", ",", "ver\u00b7klagt'", "ich", "mich", ",", "da\u00df", "es", "durch", "mich", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "wor