{"dta.poem.10853": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das j\u00fcngste Gericht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der H\u00f6chste ist gerecht, er mu\u00df den Men-\nschen geben,", "tokens": ["Der", "H\u00f6chs\u00b7te", "ist", "ge\u00b7recht", ",", "er", "mu\u00df", "den", "Men", "schen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "ART", "TRUNC", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie allhie verdient. Wie viele", "tokens": ["Was", "sie", "all\u00b7hie", "ver\u00b7dient", ".", "Wie", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$.", "PWAV", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleiben ungestraft in dieser eitlen", "tokens": ["Und", "blei\u00b7ben", "un\u00b7ge\u00b7straft", "in", "die\u00b7ser", "eit\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Draus folgt es werde dort ein Tag noch ange-", "tokens": ["Draus", "folgt", "es", "wer\u00b7de", "dort", "ein", "Tag", "noch", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da die Gerechtigkeit, nach Thaten und Verbre-", "tokens": ["Da", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ",", "nach", "Tha\u00b7ten", "und", "Ver\u00b7bre"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird ein unwandelbahr und richtig Urtheil spre-", "tokens": ["Wird", "ein", "un\u00b7wan\u00b7del\u00b7bahr", "und", "rich\u00b7tig", "Ur\u00b7theil", "spre"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "KON", "ADJD", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr B\u00f6sen! Die ihr hie, so frei und sicher geht,", "tokens": ["Ihr", "B\u00f6\u00b7sen", "!", "Die", "ihr", "hie", ",", "so", "frei", "und", "si\u00b7cher", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "PPER", "ADV", "$,", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie ein Lorbeerbaum in sch\u00f6nsten Flore steht,", "tokens": ["Und", "wie", "ein", "Lor\u00b7beer\u00b7baum", "in", "sch\u00f6ns\u00b7ten", "Flo\u00b7re", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gedenkt an diesen Tag; da kommt was hie gespon-", "tokens": ["Ge\u00b7denkt", "an", "die\u00b7sen", "Tag", ";", "da", "kommt", "was", "hie", "ge\u00b7spon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "$.", "ADV", "VVFIN", "PWS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In Dunkelheit verdekt, ans helle Licht der Son-", "tokens": ["In", "Dun\u00b7kel\u00b7heit", "ver\u00b7dekt", ",", "ans", "hel\u00b7le", "Licht", "der", "Son"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr meint es w\u00fcrden euch die Laster frei ausgehn,", "tokens": ["Ihr", "meint", "es", "w\u00fcr\u00b7den", "euch", "die", "Las\u00b7ter", "frei", "aus\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein ihr irret sehr. Jhr m\u00fcsset dreinsten stehn,", "tokens": ["Al\u00b7lein", "ihr", "ir\u00b7ret", "sehr", ".", "Ihr", "m\u00fcs\u00b7set", "dreins\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vor einem Richterstuhl, da alles wird entdek-", "tokens": ["Vor", "ei\u00b7nem", "Rich\u00b7ter\u00b7stuhl", ",", "da", "al\u00b7les", "wird", "ent\u00b7dek"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PIS", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was in der Welt geschehn, und heimlich hie ver-", "tokens": ["Was", "in", "der", "Welt", "ge\u00b7schehn", ",", "und", "heim\u00b7lich", "hie", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "$,", "KON", "ADJD", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Ich bitte dich vorher: verdamm mich S\u00fcnder nicht.", "tokens": ["Ich", "bit\u00b7te", "dich", "vor\u00b7her", ":", "ver\u00b7damm", "mich", "S\u00fcn\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "VVFIN", "PPER", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier ist mein Advocat, mein JEsus der erw\u00fcrget,", "tokens": ["Hier", "ist", "mein", "Ad\u00b7vo\u00b7cat", ",", "mein", "Je\u00b7sus", "der", "er\u00b7w\u00fcr\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der hat sein Leben auch f\u00fcr meine Seel verb\u00fcr-", "tokens": ["Der", "hat", "sein", "Le\u00b7ben", "auch", "f\u00fcr", "mei\u00b7ne", "Seel", "ver\u00b7b\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die S\u00fcnde klagt mich an, und die Gerechtigkeit,", "tokens": ["Die", "S\u00fcn\u00b7de", "klagt", "mich", "an", ",", "und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die allen Schuldenern ein schwer Gerichte dreut", "tokens": ["Die", "al\u00b7len", "Schul\u00b7de\u00b7nern", "ein", "schwer", "Ge\u00b7rich\u00b7te", "dreut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Erschrekt mich; weil ihr Bliz aus hellen Augen", "tokens": ["Er\u00b7schrekt", "mich", ";", "weil", "ihr", "Bliz", "aus", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Jedoch mein JEsus spricht: Ich hab die Schuld", "tokens": ["Je\u00b7doch", "mein", "Je\u00b7sus", "spricht", ":", "Ich", "hab", "die", "Schuld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NE", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "O! Heiland! dein Verdienst, das nehm ich gl\u00e4u-", "tokens": ["O", "!", "Hei\u00b7land", "!", "dein", "Ver\u00b7dienst", ",", "das", "nehm", "ich", "gl\u00e4u"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "$.", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ich wei\u00df, da\u00df ich damit vor ", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ich", "da\u00b7mit", "vor"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PAV", "PTKVZ"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}