{"dta.poem.7193": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gedancken auf den Schnupff-  \n Toback.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann ich offtermals mit andern selber Schnupff-To-", "tokens": ["Wann", "ich", "off\u00b7ter\u00b7mals", "mit", "an\u00b7dern", "sel\u00b7ber", "Schnupf\u00b7fTo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PIS", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "back genommen,", "tokens": ["back", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Bin ich je zuweilen wol auf die Gedancken kommen:", "tokens": ["Bin", "ich", "je", "zu\u00b7wei\u00b7len", "wol", "auf", "die", "Ge\u00b7dan\u00b7cken", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Was f\u00fcr Ursach uns doch treibe, zum Vergn\u00fcgen, Staub", "tokens": ["Was", "f\u00fcr", "Ur\u00b7sach", "uns", "doch", "trei\u00b7be", ",", "zum", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "NN", "PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "zu wehlen?", "tokens": ["zu", "weh\u00b7len", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Man erkaufft ihn, tr\u00e4gt ihn bey sich, braucht ihn \u00f6ffters,", "tokens": ["Man", "er\u00b7kaufft", "ihn", ",", "tr\u00e4gt", "ihn", "bey", "sich", ",", "braucht", "ihn", "\u00f6ff\u00b7ters", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "PRF", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "reicht ihn gar,", "tokens": ["reicht", "ihn", "gar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Aus gantz seltner H\u00f6flichkeit, ungefodert andern dar.", "tokens": ["Aus", "gantz", "selt\u00b7ner", "H\u00f6f\u00b7lich\u00b7keit", ",", "un\u00b7ge\u00b7fo\u00b7dert", "an\u00b7dern", "dar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "ADJD", "ADJA", "PTKVZ", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}