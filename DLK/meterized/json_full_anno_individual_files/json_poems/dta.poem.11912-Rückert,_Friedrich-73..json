{"dta.poem.11912": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor allem lerne nur, dich selber zu belehren;", "tokens": ["Vor", "al\u00b7lem", "ler\u00b7ne", "nur", ",", "dich", "sel\u00b7ber", "zu", "be\u00b7leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werden andre dich als ihren Lehrer ehren.", "tokens": ["So", "wer\u00b7den", "and\u00b7re", "dich", "als", "ih\u00b7ren", "Leh\u00b7rer", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PRF", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor allem bilde nur, dich selber zu erfreun;", "tokens": ["Vor", "al\u00b7lem", "bil\u00b7de", "nur", ",", "dich", "sel\u00b7ber", "zu", "er\u00b7freun", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird sich Lust der Welt an deinem Bild erneun.", "tokens": ["So", "wird", "sich", "Lust", "der", "Welt", "an", "dei\u00b7nem", "Bild", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor allem bleibe dir der Friede nur beschieden", "tokens": ["Vor", "al\u00b7lem", "blei\u00b7be", "dir", "der", "Frie\u00b7de", "nur", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wirst du rings um dich verbreiten Gottes Frieden.", "tokens": ["So", "wirst", "du", "rings", "um", "dich", "ver\u00b7brei\u00b7ten", "Got\u00b7tes", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}