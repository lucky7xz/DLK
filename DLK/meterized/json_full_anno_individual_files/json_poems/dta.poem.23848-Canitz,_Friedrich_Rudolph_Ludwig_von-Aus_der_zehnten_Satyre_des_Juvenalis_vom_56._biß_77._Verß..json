{"dta.poem.23848": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Aus der zehnten Satyre des Juvenalis  \n vom 56. bi\u00df 77. Ver\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Wje mancher/ den das Gl\u00fcck mit Ehr und Macht\ngekr\u00f6nt/", "tokens": ["Wie", "man\u00b7cher", "/", "den", "das", "Gl\u00fcck", "mit", "Ehr", "und", "Macht", "ge\u00b7kr\u00f6nt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "$(", "ART", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird endlich von dem Neid zertreten und verh\u00f6nt?", "tokens": ["Wird", "end\u00b7lich", "von", "dem", "Neid", "zer\u00b7tre\u00b7ten", "und", "ver\u00b7h\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie mancher/ den die Kunst in blanckes Ertz gegossen/", "tokens": ["Wie", "man\u00b7cher", "/", "den", "die", "Kunst", "in", "blan\u00b7ckes", "Ertz", "ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$(", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als f\u00fchr er im Triumph mit seinen muntern Rossen", "tokens": ["Als", "f\u00fchr", "er", "im", "Tri\u00b7umph", "mit", "sei\u00b7nen", "mun\u00b7tern", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach Romuls hohen Burg/ verf\u00e4llt im Augenblick/", "tokens": ["Nach", "Ro\u00b7muls", "ho\u00b7hen", "Burg", "/", "ver\u00b7f\u00e4llt", "im", "Au\u00b7gen\u00b7blick", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$(", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man das stoltze Bild mit ausgedehntem Strick", "tokens": ["Wenn", "man", "das", "stolt\u00b7ze", "Bild", "mit", "aus\u00b7ge\u00b7dehn\u00b7tem", "Strick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von seinen Pfeilern hohlt. Schau wie Gespann und", "tokens": ["Von", "sei\u00b7nen", "Pfei\u00b7lern", "hohlt", ".", "Schau", "wie", "Ge\u00b7spann", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "NN", "KOKOM", "NN", "KON"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Das gleichwohl nichts gethan/ in st\u00fccken wird geschla-", "tokens": ["Das", "gleich\u00b7wohl", "nichts", "ge\u00b7than", "/", "in", "st\u00fc\u00b7cken", "wird", "ge\u00b7schla"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIS", "VVPP", "$(", "APPR", "VVINF", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Betrachte wie Sejan im Ofen schmeltzen mu\u00df/", "tokens": ["Be\u00b7trach\u00b7te", "wie", "Se\u00b7jan", "im", "O\u00b7fen", "schmelt\u00b7zen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wie/ (o Unbestand) durch einen neuen Gu\u00df", "tokens": ["Und", "wie", "/", "(", "o", "Un\u00b7be\u00b7stand", ")", "durch", "ei\u00b7nen", "neu\u00b7en", "Gu\u00df"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$(", "$(", "FM", "NN", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des K\u00e4ysers liebster Freund/ den alle Welt geehret/", "tokens": ["Des", "K\u00e4y\u00b7sers", "liebs\u00b7ter", "Freund", "/", "den", "al\u00b7le", "Welt", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich in ein schlecht Geschirr/ und Nacht-Gef\u00e4\u00df verkeh-", "tokens": ["Sich", "in", "ein", "schlecht", "Ge\u00b7schirr", "/", "und", "Nacht\u00b7Ge\u00b7f\u00e4\u00df", "ver\u00b7keh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJD", "NN", "$(", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch das erhitzte Volck sucht mehr als di\u00df Metall;", "tokens": ["Doch", "das", "er\u00b7hitz\u00b7te", "Volck", "sucht", "mehr", "als", "di\u00df", "Me\u00b7tall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PIS", "KOKOM", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sejan wird selbst gest\u00fcrtzt;man rufft mit frohem Schall:", "tokens": ["Se\u00b7jan", "wird", "selbst", "ge\u00b7st\u00fcrtzt", ";", "man", "rufft", "mit", "fro\u00b7hem", "Schall", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "$.", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.15": {"text": "Auf! la\u00dft uns den Pallast mit Lorbeer-Aesten zieren/", "tokens": ["Auf", "!", "la\u00dft", "uns", "den", "Pal\u00b7last", "mit", "Lor\u00b7beer\u00b7Aes\u00b7ten", "zie\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.16": {"text": "Und auf das Capitol ein Stier zum Opfer f\u00fchren;", "tokens": ["Und", "auf", "das", "Ca\u00b7pi\u00b7tol", "ein", "Stier", "zum", "Op\u00b7fer", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Weil nun die Rache kommt/ und den verfluchten Mann", "tokens": ["Weil", "nun", "die", "Ra\u00b7che", "kommt", "/", "und", "den", "ver\u00b7fluch\u00b7ten", "Mann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Zu seiner Straffe schleppt! sieh doch/ f\u00e4ngt einer an/", "tokens": ["Zu", "sei\u00b7ner", "Straf\u00b7fe", "schleppt", "!", "sieh", "doch", "/", "f\u00e4ngt", "ei\u00b7ner", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "VVIMP", "ADV", "$(", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sein t\u00fccksches Angesicht. Steht nicht/ was er betrieben/", "tokens": ["Sein", "t\u00fcck\u00b7sches", "An\u00b7ge\u00b7sicht", ".", "Steht", "nicht", "/", "was", "er", "be\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VVFIN", "PTKNEG", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zusamt der Todes-Art/ an seiner Stirn geschrieben?", "tokens": ["Zu\u00b7samt", "der", "To\u00b7des\u00b7Art", "/", "an", "sei\u00b7ner", "Stirn", "ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ja/ spricht der andre drauf: ich wil es nur gestehn/", "tokens": ["Ja", "/", "spricht", "der", "and\u00b7re", "drauf", ":", "ich", "wil", "es", "nur", "ge\u00b7stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "VVFIN", "ART", "PIS", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ich ihn allemahl mit Abscheu angesehn.", "tokens": ["Da\u00df", "ich", "ihn", "al\u00b7le\u00b7mahl", "mit", "Ab\u00b7scheu", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Darf ich mit allem dem nach sein Verbrechen fragen?", "tokens": ["Darf", "ich", "mit", "al\u00b7lem", "dem", "nach", "sein", "Ver\u00b7bre\u00b7chen", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIS", "ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was hat er wider das/ was seine Kl\u00e4ger sagen/", "tokens": ["Was", "hat", "er", "wi\u00b7der", "das", "/", "was", "sei\u00b7ne", "Kl\u00e4\u00b7ger", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "$(", "PWS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und die ihn \u00fcberzeugt/ zur Ausflucht vorgewandt?", "tokens": ["Und", "die", "ihn", "\u00fc\u00b7berz\u00b7eugt", "/", "zur", "Aus\u00b7flucht", "vor\u00b7ge\u00b7wandt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVPP", "$(", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ein mehres h\u00f6rt man nicht/ als da\u00df mit eigner Hand", "tokens": ["Ein", "meh\u00b7res", "h\u00f6rt", "man", "nicht", "/", "als", "da\u00df", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PIS", "PTKNEG", "$(", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der K\u00e4yser an den Raht/ vom Eyland der Capreen/", "tokens": ["Der", "K\u00e4y\u00b7ser", "an", "den", "Raht", "/", "vom", "Ey\u00b7land", "der", "Cap\u00b7re\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Von vielen Sachen schrieb/ aus welchen zu verstehen/", "tokens": ["Von", "vie\u00b7len", "Sa\u00b7chen", "schrieb", "/", "aus", "wel\u00b7chen", "zu", "ver\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$(", "APPR", "PWAT", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df der/ so alles war/ nun seines Herren Huld/", "tokens": ["Da\u00df", "der", "/", "so", "al\u00b7les", "war", "/", "nun", "sei\u00b7nes", "Her\u00b7ren", "Huld", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ADV", "PIS", "VAFIN", "$(", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich wei\u00df nicht wie/ verschertzt. Wolan so hat er", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "/", "ver\u00b7schertzt", ".", "Wo\u00b7lan", "so", "hat", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "$(", "VVPP", "$.", "ADV", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Das ist mir schon genug. So l\u00e4\u00dft zu allen Zeiten/", "tokens": ["Das", "ist", "mir", "schon", "ge\u00b7nug", ".", "So", "l\u00e4\u00dft", "zu", "al\u00b7len", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das blinde R\u00f6mer-Volck sich von dem Gl\u00fccke leiten!", "tokens": ["Das", "blin\u00b7de", "R\u00f6\u00b7mer\u00b7Volck", "sich", "von", "dem", "Gl\u00fc\u00b7cke", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wer das verlohren hat/ ist auch bey ihm verha\u00dft/", "tokens": ["Wer", "das", "ver\u00b7loh\u00b7ren", "hat", "/", "ist", "auch", "bey", "ihm", "ver\u00b7ha\u00dft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVPP", "VAFIN", "$(", "VAFIN", "ADV", "APPR", "PPER", "ADJD", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Denn h\u00e4tte nur Sejan den Vortheil abgepa\u00dft/", "tokens": ["Denn", "h\u00e4t\u00b7te", "nur", "Se\u00b7jan", "den", "Vor\u00b7theil", "ab\u00b7ge\u00b7pa\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NE", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und eh durch k\u00fchnen Mord den K\u00e4yser weggeschoben/", "tokens": ["Und", "eh", "durch", "k\u00fch\u00b7nen", "Mord", "den", "K\u00e4y\u00b7ser", "weg\u00b7ge\u00b7scho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So h\u00e4tte ihn dieses Volck an seine statt erhoben.", "tokens": ["So", "h\u00e4t\u00b7te", "ihn", "die\u00b7ses", "Volck", "an", "sei\u00b7ne", "statt", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}