{"dta.poem.9618": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warumb betr\u00fcbstu mich/ der dich so hertzlich liebet/", "tokens": ["Wa\u00b7rumb", "be\u00b7tr\u00fcbs\u00b7tu", "mich", "/", "der", "dich", "so", "hertz\u00b7lich", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so viel seufftzer dir zum treuen opffer giebet/", "tokens": ["Und", "so", "viel", "seufft\u00b7zer", "dir", "zum", "treu\u00b7en", "opf\u00b7fer", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich dacht/ es zeigte licht in deinen augen sich/", "tokens": ["Ich", "dacht", "/", "es", "zeig\u00b7te", "licht", "in", "dei\u00b7nen", "au\u00b7gen", "sich", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jtzt find\u2019 ich flecken drein/ warum betr\u00fcbstu mich?", "tokens": ["Jtzt", "find'", "ich", "fle\u00b7cken", "drein", "/", "wa\u00b7rum", "be\u00b7tr\u00fcbs\u00b7tu", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$(", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die vormahls treue hand/ die ich so offt gedr\u00fccket/", "tokens": ["Die", "vor\u00b7mahls", "treu\u00b7e", "hand", "/", "die", "ich", "so", "offt", "ge\u00b7dr\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$(", "PRELS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Befind ich nicht mehr treu/ sie hat mich nur ber\u00fccket;", "tokens": ["Be\u00b7find", "ich", "nicht", "mehr", "treu", "/", "sie", "hat", "mich", "nur", "be\u00b7r\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "ADJD", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auch frembden fingern ist ihr k\u00fctzel schon bekandt/", "tokens": ["Auch", "fremb\u00b7den", "fin\u00b7gern", "ist", "ihr", "k\u00fct\u00b7zel", "schon", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVINF", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie dr\u00fcckt mich nicht allein die vormahls treue hand.", "tokens": ["Sie", "dr\u00fcckt", "mich", "nicht", "al\u00b7lein", "die", "vor\u00b7mahls", "treu\u00b7e", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6ne liljen-brust voll lieblicher narcissen/", "tokens": ["Die", "sch\u00f6\u00b7ne", "lil\u00b7jen\u00b7brust", "voll", "lieb\u00b7li\u00b7cher", "nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit rosen auffgespitzt/ die ich nur pflag zu k\u00fcssen/", "tokens": ["Mit", "ro\u00b7sen", "auff\u00b7ge\u00b7spitzt", "/", "die", "ich", "nur", "pflag", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "$(", "PRELS", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hegt frembden lippen nun auch blumen s\u00fcsser lust/", "tokens": ["Hegt", "fremb\u00b7den", "lip\u00b7pen", "nun", "auch", "blu\u00b7men", "s\u00fcs\u00b7ser", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ADV", "ADV", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum ist sie so falsch die sch\u00f6ne liljen-brust?", "tokens": ["Wa\u00b7rum", "ist", "sie", "so", "falsch", "die", "sch\u00f6\u00b7ne", "lil\u00b7jen\u00b7brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die quelle meiner lust/ ob sie noch rein verschlossen/", "tokens": ["Die", "quel\u00b7le", "mei\u00b7ner", "lust", "/", "ob", "sie", "noch", "rein", "ver\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht auch frembde saat mit ihrem thau begossen?", "tokens": ["Und", "nicht", "auch", "fremb\u00b7de", "saat", "mit", "ih\u00b7rem", "thau", "be\u00b7gos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O nein! verstreustu schon die liljen deiner brust/", "tokens": ["O", "nein", "!", "ver\u00b7streus\u00b7tu", "schon", "die", "lil\u00b7jen", "dei\u00b7ner", "brust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibt auch die schoo\u00df nicht rein/ die quelle meiner lust.", "tokens": ["Bleibt", "auch", "die", "schoo\u00df", "nicht", "rein", "/", "die", "quel\u00b7le", "mei\u00b7ner", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "VVFIN", "PTKNEG", "ADJD", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der mund ist etwas kreu; doch wills nur also scheinen/", "tokens": ["Der", "mund", "ist", "et\u00b7was", "kreu", ";", "doch", "wills", "nur", "al\u00b7so", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "ADV", "VMFIN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem er kr\u00e4fftig denckt die fehler zu verneinen;", "tokens": ["In\u00b7dem", "er", "kr\u00e4ff\u00b7tig", "denckt", "die", "feh\u00b7ler", "zu", "ver\u00b7nei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nein/ ich glaub es nicht/ bring mir nicht unschuld bey/", "tokens": ["Doch", "nein", "/", "ich", "glaub", "es", "nicht", "/", "bring", "mir", "nicht", "un\u00b7schuld", "bey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aug/ hand/ brust/ schoo\u00df sind falsch/ der mund ist etwas treu.", "tokens": ["Aug", "/", "hand", "/", "brust", "/", "schoo\u00df", "sind", "falsch", "/", "der", "mund", "ist", "et\u00b7was", "treu", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "VVFIN", "$(", "VVFIN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Mein hertz bezwinge dich/ dasselbe zu verlassen/", "tokens": ["Mein", "hertz", "be\u00b7zwin\u00b7ge", "dich", "/", "das\u00b7sel\u00b7be", "zu", "ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$(", "PDAT", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was du so hertzlich liebst/ die untreu mu\u00df man hassen.", "tokens": ["Was", "du", "so", "hertz\u00b7lich", "liebst", "/", "die", "un\u00b7treu", "mu\u00df", "man", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$(", "ART", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kr\u00f6nt dich/ Melinde/ nur dergleichen treu/ wie mich/", "tokens": ["Kr\u00f6nt", "dich", "/", "Me\u00b7lin\u00b7de", "/", "nur", "derg\u00b7lei\u00b7chen", "treu", "/", "wie", "mich", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "NE", "$(", "ADV", "PIS", "ADJD", "$(", "PWAV", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich lie\u00df dich nimmermehr; mein hertz bezwinge dich.", "tokens": ["Ich", "lie\u00df", "dich", "nim\u00b7mer\u00b7mehr", ";", "mein", "hertz", "be\u00b7zwin\u00b7ge", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}