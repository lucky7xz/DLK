{"textgrid.poem.48689": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Ich bin die Auferstehung u.s.w", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich aber bin der Tod und ganze Niederlage,", "tokens": ["Ich", "a\u00b7ber", "bin", "der", "Tod", "und", "gan\u00b7ze", "Nie\u00b7der\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "vermag nicht so viel Kraft, um mich zu richten auf.", "tokens": ["ver\u00b7mag", "nicht", "so", "viel", "Kraft", ",", "um", "mich", "zu", "rich\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich f\u00e4lle mich selbselbst durch meinen eignen Lauf,", "tokens": ["Ich", "f\u00e4l\u00b7le", "mich", "selb\u00b7selbst", "durch", "mei\u00b7nen", "eig\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "matt, kraftlos, ohne Macht. Wer ist hier, dem ichs klage?", "tokens": ["matt", ",", "kraft\u00b7los", ",", "oh\u00b7ne", "Macht", ".", "Wer", "ist", "hier", ",", "dem", "ichs", "kla\u00b7ge", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "KOUI", "NN", "$.", "PWS", "VAFIN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach! da\u00df ein Retter k\u00e4m' und h\u00fclfe meiner Plage!", "tokens": ["Ach", "!", "da\u00df", "ein", "Ret\u00b7ter", "k\u00e4m'", "und", "h\u00fcl\u00b7fe", "mei\u00b7ner", "Pla\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An wen doch steif ich mich? wer giebet Achtung drauf,", "tokens": ["An", "wen", "doch", "steif", "ich", "mich", "?", "wer", "gie\u00b7bet", "Ach\u00b7tung", "drauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "ADV", "VVFIN", "PPER", "PRF", "$.", "PWS", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie \u00e4ngstlich mir geschicht? Es h\u00e4uft sich Hauf auf Hauf',", "tokens": ["wie", "\u00e4ngst\u00b7lich", "mir", "ge\u00b7schicht", "?", "Es", "h\u00e4uft", "sich", "Hauf", "auf", "Hauf'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PRF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "an Not, an Angst, an Qual, in welcher ich verzage.", "tokens": ["an", "Not", ",", "an", "Angst", ",", "an", "Qual", ",", "in", "wel\u00b7cher", "ich", "ver\u00b7za\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So lieg' ich Schwacher, denn in tausent herben Schmerzen,", "tokens": ["So", "lieg'", "ich", "Schwa\u00b7cher", ",", "denn", "in", "tau\u00b7sent", "her\u00b7ben", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "KON", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so sterb' ich Toter vor, ehs iemand nimt zu Herzen.", "tokens": ["so", "sterb'", "ich", "To\u00b7ter", "vor", ",", "ehs", "ie\u00b7mand", "nimt", "zu", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und leg' und st\u00fcrb' ich mir, so h\u00e4tt' es keine Not.", "tokens": ["Und", "leg'", "und", "st\u00fcrb'", "ich", "mir", ",", "so", "h\u00e4tt'", "es", "kei\u00b7ne", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Komm, Auferstehung, komm, komm Leben, komm geschwinde,", "tokens": ["Komm", ",", "Auf\u00b7er\u00b7ste\u00b7hung", ",", "komm", ",", "komm", "Le\u00b7ben", ",", "komm", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "NN", "$,", "VV