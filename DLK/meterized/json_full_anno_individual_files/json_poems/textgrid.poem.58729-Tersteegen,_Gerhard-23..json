{"textgrid.poem.58729": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir das Herz verwundet, meine Schwester, liebe Braut. Hoheslied 4, 9", "tokens": ["Du", "hast", "mir", "das", "Herz", "ver\u00b7wun\u00b7det", ",", "mei\u00b7ne", "Schwes\u00b7ter", ",", "lie\u00b7be", "Braut", ".", "Ho\u00b7hes\u00b7lied", "4", ",", "9"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Du, wenn du ", "tokens": ["Du", ",", "wenn", "du"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "La\u00df deine Weisheit mich mit g\u00f6ttlich wahrem Munde", "tokens": ["La\u00df", "dei\u00b7ne", "Weis\u00b7heit", "mich", "mit", "g\u00f6tt\u00b7lich", "wah\u00b7rem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PRF", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einst lehren, was im Sinne steckt!", "tokens": ["Einst", "leh\u00b7ren", ",", "was", "im", "Sin\u00b7ne", "steckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, ja, ich fasse schon dies hoh' Geheimnis: eben", "tokens": ["Ja", ",", "ja", ",", "ich", "fas\u00b7se", "schon", "dies", "hoh'", "Ge\u00b7heim\u00b7nis", ":", "e\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "PDS", "ADJA", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Derselbe Pfeil auch dich verwund't,", "tokens": ["Der\u00b7sel\u00b7be", "Pfeil", "auch", "dich", "ver\u00b7wund't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Womit du deiner Braut die Wunde hast gegeben;", "tokens": ["Wo\u00b7mit", "du", "dei\u00b7ner", "Braut", "die", "Wun\u00b7de", "hast", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dies ist der tiefe Sinn und Grund.", "tokens": ["Dies", "ist", "der", "tie\u00b7fe", "Sinn", "und", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Tauscht lieber Herz um Herz, gib du der Braut das deine!", "tokens": ["Tauscht", "lie\u00b7ber", "Herz", "um", "Herz", ",", "gib", "du", "der", "Braut", "das", "dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts kann Liebreizenders geschehn \u2013", "tokens": ["Nichts", "kann", "Lieb\u00b7rei\u00b7zen\u00b7ders", "ge\u00b7schehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "VVPP", "$("], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat ihr Herz nicht mehr, sie will hinfort alleine", "tokens": ["Sie", "hat", "ihr", "Herz", "nicht", "mehr", ",", "sie", "will", "hin\u00b7fort", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,", "PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Br\u00e4ut'gams Herz nur in sich sehn.", "tokens": ["Des", "Br\u00e4ut'\u00b7gams", "Herz", "nur", "in", "sich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie hat auch \u00fcbrig's Recht, die Braut in ihrem Triebe,", "tokens": ["Sie", "hat", "auch", "\u00fcb\u00b7rig's", "Recht", ",", "die", "Braut", "in", "ih\u00b7rem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrem Herzenstausch mit dir,", "tokens": ["In", "ih\u00b7rem", "Her\u00b7zen\u00b7stausch", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dadurch stiftet sie auf ewig fest die Liebe,", "tokens": ["Denn", "da\u00b7durch", "stif\u