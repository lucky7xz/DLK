{"textgrid.poem.44099": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Geist, bereite dir im Stillen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist, bereite dir im Stillen", "tokens": ["Mein", "Geist", ",", "be\u00b7rei\u00b7te", "dir", "im", "Stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Kummer Trost, aus Wermuth Wein!", "tokens": ["Aus", "Kum\u00b7mer", "Trost", ",", "aus", "Wer\u00b7muth", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedenck an Gottes Rath und Willen:", "tokens": ["Ge\u00b7denck", "an", "Got\u00b7tes", "Rath", "und", "Wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du must durch Angst gel\u00e4utert sein.", "tokens": ["Du", "must", "durch", "Angst", "ge\u00b7l\u00e4u\u00b7tert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Golde zeigt die Glut den Werth,", "tokens": ["Am", "Gol\u00b7de", "zeigt", "die", "Glut", "den", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Leid ist der Christen Pr\u00fcfungsherd.", "tokens": ["Leid", "ist", "der", "Chris\u00b7ten", "Pr\u00fc\u00b7fungs\u00b7herd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es giebt sonst vielerley Vergn\u00fcgen,", "tokens": ["Es", "giebt", "sonst", "vie\u00b7ler\u00b7ley", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts aber ist, was mehr entz\u00fcckt,", "tokens": ["Nichts", "a\u00b7ber", "ist", ",", "was", "mehr", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wenn man in des H\u00f6chsten F\u00fcgen", "tokens": ["Als", "wenn", "man", "in", "des", "H\u00f6chs\u00b7ten", "F\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mit gela\u00dfner Demuth schickt.", "tokens": ["Sich", "mit", "ge\u00b7la\u00df\u00b7ner", "De\u00b7muth", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Pre\u00dft Noth und Jammer noch so sehr,", "tokens": ["Pre\u00dft", "Noth", "und", "Jam\u00b7mer", "noch", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An Gott gedacht, es pre\u00dft nicht mehr.", "tokens": ["An", "Gott", "ge\u00b7dacht", ",", "es", "pre\u00dft", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Rizt oft der Creuzdorn das Gem\u00fcthe,", "tokens": ["Rizt", "oft", "der", "Creuz\u00b7dorn", "das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rei\u00dft Neid und Spott ins Ehrenkleid,", "tokens": ["Rei\u00dft", "Neid", "und", "Spott", "ins", "Eh\u00b7ren\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schweige, weil der Vorsicht G\u00fcte", "tokens": ["Ich", "schwei\u00b7ge", ",", "weil", "der", "Vor\u00b7sicht", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Hagel Thau und Rosen streut.", "tokens": ["Nach", "Ha\u00b7gel", "Thau", "und", "Ro\u00b7sen", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott f\u00fchrt hinein, Gott f\u00fchrt heraus,", "tokens": ["Gott", "f\u00fchrt", "hin\u00b7ein", ",", "Gott", "f\u00fchrt", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Er s\u00e4umt und bleibt gleichwohl nicht aus.", "tokens": ["Er", "s\u00e4umt", "und", "bleibt", "gleich\u00b7wohl", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bedenckt doch dies, ihr schwachen Sinnen,", "tokens": ["Be\u00b7denckt", "doch", "dies", ",", "ihr", "schwa\u00b7chen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob alles um euch bricht und f\u00e4llt,", "tokens": ["Ob", "al\u00b7les", "um", "euch", "bricht", "und", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ist's Verlust, dort heist's Gewinnen;", "tokens": ["Hier", "ist's", "Ver\u00b7lust", ",", "dort", "heist's", "Ge\u00b7win\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Regiert den Gram und lacht der Welt!", "tokens": ["Re\u00b7giert", "den", "Gram", "und", "lacht", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nachdem ihr willig tragt und fast,", "tokens": ["Nach\u00b7dem", "ihr", "wil\u00b7lig", "tragt", "und", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erleichtert ihr euch selbst die Last.", "tokens": ["Er\u00b7leich\u00b7tert", "ihr", "euch", "selbst", "die", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit Hofnung und Gebeth und Glauben", "tokens": ["Mit", "Hof\u00b7nung", "und", "Ge\u00b7beth", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwarth ich st\u00fcndlich be\u00dfre Zeit;", "tokens": ["Er\u00b7warth", "ich", "st\u00fcnd\u00b7lich", "be\u00df\u00b7re", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts kan mir Gottes Liebe rauben.", "tokens": ["Nichts", "kan", "mir", "Got\u00b7tes", "Lie\u00b7be", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tod, Satan, H\u00f6lle, Welt und Neid", "tokens": ["Tod", ",", "Sa\u00b7tan", ",", "H\u00f6l\u00b7le", ",", "Welt", "und", "Neid"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ziehn mich umsonst in Angst und Weh,", "tokens": ["Ziehn", "mich", "um\u00b7sonst", "in", "Angst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es wacht mein Schuzherr in der H\u00f6h.", "tokens": ["Es", "wacht", "mein", "Schuz\u00b7herr", "in", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Last endlich auch die Freunde wancken!", "tokens": ["Last", "end\u00b7lich", "auch", "die", "Freun\u00b7de", "wan\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh sie mit Verachtung an,", "tokens": ["Ich", "seh", "sie", "mit", "Ver\u00b7ach\u00b7tung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachdem ich selbst mir in Gedancken", "tokens": ["Nach\u00b7dem", "ich", "selbst", "mir", "in", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den besten Zuspruch geben kan:", "tokens": ["Den", "bes\u00b7ten", "Zu\u00b7spruch", "ge\u00b7ben", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mein Himmel kommt schon auf der Welt,", "tokens": ["Mein", "Him\u00b7mel", "kommt", "schon", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Weil, was Gott will, mir auch gef\u00e4llt.", "tokens": ["Weil", ",", "was", "Gott", "will", ",", "mir", "auch", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "NN", "VMFIN", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Geist, bereite dir im Stillen", "tokens": ["Mein", "Geist", ",", "be\u00b7rei\u00b7te", "dir", "im", "Stil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Kummer Trost, aus Wermuth Wein!", "tokens": ["Aus", "Kum\u00b7mer", "Trost", ",", "aus", "Wer\u00b7muth", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gedenck an Gottes Rath und Willen:", "tokens": ["Ge\u00b7denck", "an", "Got\u00b7tes", "Rath", "und", "Wil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du must durch Angst gel\u00e4utert sein.", "tokens": ["Du", "must", "durch", "Angst", "ge\u00b7l\u00e4u\u00b7tert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Golde zeigt die Glut den Werth,", "tokens": ["Am", "Gol\u00b7de", "zeigt", "die", "Glut", "den", "Werth", ",