{"textgrid.poem.24172": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Eigentum", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast gekauft und du erworben,", "tokens": ["Du", "hast", "ge\u00b7kauft", "und", "du", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast geerbt, wie dein Vater gestorben:", "tokens": ["Du", "hast", "ge\u00b7erbt", ",", "wie", "dein", "Va\u00b7ter", "ge\u00b7stor\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr sitzt im Recht.", "tokens": ["Ihr", "sitzt", "im", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Aber der Erste, der euer Land besessen,", "tokens": ["A\u00b7ber", "der", "Ers\u00b7te", ",", "der", "eu\u00b7er", "Land", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Hat sichs mit Keulen zugemessen,", "tokens": ["Hat", "sichs", "mit", "Keu\u00b7len", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sichs erfrecht.", "tokens": ["Hat", "sichs", "er\u00b7frecht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wahrlich, ich sage euch: Fragt nicht zur\u00fcck!", "tokens": ["Wahr\u00b7lich", ",", "ich", "sa\u00b7ge", "euch", ":", "Fragt", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Auf dem Grunde des Grundbuchs steht: Recht ist Gl\u00fcck.", "tokens": ["Auf", "dem", "Grun\u00b7de", "des", "Grund\u00b7buchs", "steht", ":", "Recht", "ist", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$.", "NN", "VAFIN", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Machts wie jener, der sich erfrechte:", "tokens": ["Machts", "wie", "je\u00b7ner", ",", "der", "sich", "er\u00b7frech\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "PDS", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Behauptet mit Keulen euch in euerm Rechte", "tokens": ["Be\u00b7haup\u00b7tet", "mit", "Keu\u00b7len", "euch", "in", "eu\u00b7erm", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u2013 Wenn aber nun wiederum Einer k\u00e4me", "tokens": ["\u2013", "Wenn", "a\u00b7ber", "nun", "wie\u00b7de\u00b7rum", "Ei\u00b7ner", "k\u00e4\u00b7me"], "token_info": ["p