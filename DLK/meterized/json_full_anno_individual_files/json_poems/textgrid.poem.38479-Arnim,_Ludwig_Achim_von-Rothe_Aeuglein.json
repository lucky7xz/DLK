{"textgrid.poem.38479": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Rothe Aeuglein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnst du meine Aeuglein sehen,", "tokens": ["K\u00f6nnst", "du", "mei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie sie sind vom Weinen roth,", "tokens": ["Wie", "sie", "sind", "vom", "Wei\u00b7nen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich soll in das Kloster gehen,", "tokens": ["Ich", "soll", "in", "das", "Klos\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und allein seyn bis in Tod.", "tokens": ["Und", "al\u00b7lein", "seyn", "bis", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAINF", "KON", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es sitzen auch zwey Turtelt\u00e4ublein", "tokens": ["Es", "sit\u00b7zen", "auch", "zwey", "Tur\u00b7tel\u00b7t\u00e4u\u00b7blein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00fcben auf dem gr\u00fcnen Ast,", "tokens": ["Dr\u00fc\u00b7ben", "auf", "dem", "gr\u00fc\u00b7nen", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die von einander scheiden,", "tokens": ["Wenn", "die", "von", "ein\u00b7an\u00b7der", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So vergehen Laub und Gras.", "tokens": ["So", "ver\u00b7ge\u00b7hen", "Laub", "und", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "K\u00f6nnst du meine Aeuglein sehen,", "tokens": ["K\u00f6nnst", "du", "mei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie sie sind vom Weinen roth,", "tokens": ["Wie", "sie", "sind", "vom", "Wei\u00b7nen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich soll in das Kloster gehen,", "tokens": ["Ich", "soll", "in", "das", "Klos\u00b7ter", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und allein seyn bis in Tod.", "tokens": ["Und", "al\u00b7lein", "seyn", "bis", "in", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAINF", "KON", "APPR", "NN", "$."], "