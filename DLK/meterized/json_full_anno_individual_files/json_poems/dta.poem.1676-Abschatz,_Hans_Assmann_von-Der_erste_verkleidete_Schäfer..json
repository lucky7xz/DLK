{"dta.poem.1676": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der erste verkleidete Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat mich meine Braut geziert mit mancherley:", "tokens": ["Es", "hat", "mich", "mei\u00b7ne", "Braut", "ge\u00b7ziert", "mit", "man\u00b7cher\u00b7ley", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der rothen M\u00fctze Schmuck deckt mein Gold-gelbes", "tokens": ["Der", "ro\u00b7then", "M\u00fct\u00b7ze", "Schmuck", "deckt", "mein", "Gold\u00b7gel\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Haar/", "tokens": ["Haar", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Das nechst bey Sonnenschein vor roth verd\u00e4chtig war.", "tokens": ["Das", "nechst", "bey", "Son\u00b7nen\u00b7schein", "vor", "roth", "ver\u00b7d\u00e4ch\u00b7tig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der steiffe Kragen steht geziert mit sch\u00f6nem Bande/", "tokens": ["Der", "steif\u00b7fe", "Kra\u00b7gen", "steht", "ge\u00b7ziert", "mit", "sch\u00f6\u00b7nem", "Ban\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mein gef\u00f6rmelt Peltz ist wohl der sch\u00f6nst im Lande.", "tokens": ["Und", "mein", "ge\u00b7f\u00f6r\u00b7melt", "Peltz", "ist", "wohl", "der", "sch\u00f6nst", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVPP", "NN", "VAFIN", "ADV", "ART", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Leder h\u00e4lt den Stich/ von dem ich Hosen trage/", "tokens": ["Das", "Le\u00b7der", "h\u00e4lt", "den", "Stich", "/", "von", "dem", "ich", "Ho\u00b7sen", "tra\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was meynt ihr das ich von den schmucken Stiefeln sage?", "tokens": ["Was", "meynt", "ihr", "das", "ich", "von", "den", "schmu\u00b7cken", "Stie\u00b7feln", "sa\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Kober ist mir gutt/ den K\u00e4\u00df hinein zu stecken;", "tokens": ["Der", "Ko\u00b7ber", "ist", "mir", "gutt", "/", "den", "K\u00e4\u00df", "hin\u00b7ein", "zu", "ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$(", "ART", "NN", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df/ ihr esst mit mir auff meinen Wolle-S\u00e4cken.", "tokens": ["Ich", "wei\u00df", "/", "ihr", "esst", "mit", "mir", "auff", "mei\u00b7nen", "Wol\u00b7le\u00b7S\u00e4\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Keule sch\u00fczt mich auch f\u00fcr Unthier und f\u00fcr Dieben.", "tokens": ["Die", "Keu\u00b7le", "sch\u00fczt", "mich", "auch", "f\u00fcr", "Un\u00b7thier", "und", "f\u00fcr", "Die\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jedoch mit alle dem ist was vergessen blieben/", "tokens": ["Je\u00b7doch", "mit", "al\u00b7le", "dem", "ist", "was", "ver\u00b7ges\u00b7sen", "blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "PDS", "VAFIN", "PIS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das/ leider/ mir noch fehlt: Rath\u2019t alle/ was es sey?", "tokens": ["Das", "/", "lei\u00b7der", "/", "mir", "noch", "fehlt", ":", "Ra\u00b7th't", "al\u00b7le", "/", "was", "es", "sey", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "$(", "PPER", "ADV", "VVFIN", "$.", "VVFIN", "PIS", "$(", "PWS", "PPER", "VAFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}