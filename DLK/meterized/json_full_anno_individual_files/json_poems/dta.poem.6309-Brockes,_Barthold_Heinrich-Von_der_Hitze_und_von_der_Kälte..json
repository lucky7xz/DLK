{"dta.poem.6309": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um nun des Brennens Eigenschafft,", "tokens": ["Um", "nun", "des", "Bren\u00b7nens", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die so gewaltige geschwinde Krafft,", "tokens": ["Die", "so", "ge\u00b7wal\u00b7ti\u00b7ge", "ge\u00b7schwin\u00b7de", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die so verschiedne W\u00fcrckung thut,", "tokens": ["Die", "so", "ver\u00b7schied\u00b7ne", "W\u00fcr\u00b7ckung", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht gr\u00fcndlich einzusehn; so k\u00f6nnen wir die Gluht", "tokens": ["Recht", "gr\u00fcnd\u00b7lich", "ein\u00b7zu\u00b7sehn", ";", "so", "k\u00f6n\u00b7nen", "wir", "die", "Gluht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVINF", "$.", "ADV", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als etwas fl\u00fcssiges beschreiben,", "tokens": ["Als", "et\u00b7was", "fl\u00fcs\u00b7si\u00b7ges", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Worinn die kleinen C\u00f6rperlein,", "tokens": ["Wo\u00b7rinn", "die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die hart und sehr durchdringend seyn,", "tokens": ["Die", "hart", "und", "sehr", "durch\u00b7drin\u00b7gend", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit einer schnellen Fluth gedrehet sind und treiben.", "tokens": ["Mit", "ei\u00b7ner", "schnel\u00b7len", "Fluth", "ge\u00b7dre\u00b7het", "sind", "und", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie schwimmen, fortger\u00fcckt vom ersten Element,", "tokens": ["Sie", "schwim\u00b7men", ",", "fort\u00b7ge\u00b7r\u00fcckt", "vom", "ers\u00b7ten", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie nehmen die Natur von dessen schnellen Regen", "tokens": ["Sie", "neh\u00b7men", "die", "Na\u00b7tur", "von", "des\u00b7sen", "schnel\u00b7len", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und folglich von desselbigen Bewegen", "tokens": ["Und", "folg\u00b7lich", "von", "des\u00b7sel\u00b7bi\u00b7gen", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die schnellste Fertigkeit. Dasjenige, was brennt,", "tokens": ["Die", "schnells\u00b7te", "Fer\u00b7tig\u00b7keit", ".", "Das\u00b7je\u00b7ni\u00b7ge", ",", "was", "brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PDS", "$,", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sind brausende, beweglich-leichte Wellen,", "tokens": ["Sind", "brau\u00b7sen\u00b7de", ",", "be\u00b7weg\u00b7lich\u00b7leich\u00b7te", "Wel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die, da sie durch die L\u00fcffte dringen,", "tokens": ["Die", ",", "da", "sie", "durch", "die", "L\u00fcff\u00b7te", "drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Stets umgetrieben, knastern, springen,", "tokens": ["Stets", "um\u00b7ge\u00b7trie\u00b7ben", ",", "knas\u00b7tern", ",", "sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und weit vom Mittelpunet getrennt:", "tokens": ["Und", "weit", "vom", "Mit\u00b7tel\u00b7pu\u00b7net", "ge\u00b7trennt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Doch von der Lufft, durch welche sie gedr\u00fcckt,", "tokens": ["Doch", "von", "der", "Lufft", ",", "durch", "wel\u00b7che", "sie", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "In ihrem Kreis zur\u00fcck gehalten und geschickt,", "tokens": ["In", "ih\u00b7rem", "Kreis", "zu\u00b7r\u00fcck", "ge\u00b7hal\u00b7ten", "und", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf jeden nahen Vorwurff fallen.", "tokens": ["Auf", "je\u00b7den", "na\u00b7hen", "Vor\u00b7wurff", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie suchen sich auf allen Seiten", "tokens": ["Sie", "su\u00b7chen", "sich", "auf", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "In schnellster W\u00fcrckung auszubreiten.", "tokens": ["In", "schnells\u00b7ter", "W\u00fcr\u00b7ckung", "aus\u00b7zu\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Durch ihre Fl\u00fcchtigkeit, zertheilen sie, durchdringen", "tokens": ["Durch", "ih\u00b7re", "Fl\u00fcch\u00b7tig\u00b7keit", ",", "zer\u00b7thei\u00b7len", "sie", ",", "durch\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die C\u00f6rper, \u00f6ffnen, trennen, schneiden.", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "\u00f6ff\u00b7nen", ",", "tren\u00b7nen", ",", "schnei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}