{"textgrid.poem.47512": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o harte Sterne! Nie vers\u00f6hnte, rauhe Welt!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O harte Sterne! Nie vers\u00f6hnte, rauhe Welt!", "tokens": ["O", "har\u00b7te", "Ster\u00b7ne", "!", "Nie", "ver\u00b7s\u00f6hn\u00b7te", ",", "rau\u00b7he", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kaum rastet einmal seelig in der Liebe Zelt", "tokens": ["Kaum", "ras\u00b7tet", "ein\u00b7mal", "see\u00b7lig", "in", "der", "Lie\u00b7be", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das m\u00fcde Herz, von sehnlicher Begier geschwellt, \u2013", "tokens": ["Das", "m\u00fc\u00b7de", "Herz", ",", "von", "sehn\u00b7li\u00b7cher", "Be\u00b7gier", "ge\u00b7schwellt", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da, horch, der Karawanenglocke Stimme gellt,", "tokens": ["Da", ",", "horch", ",", "der", "Ka\u00b7ra\u00b7wa\u00b7nen\u00b7glo\u00b7cke", "Stim\u00b7me", "gellt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexan