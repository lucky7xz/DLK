{"dta.poem.13802": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df' nie die Schmach mich sehen,", "tokens": ["La\u00df'", "nie", "die", "Schmach", "mich", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df auch Dein Herz, der L\u00fcge", "tokens": ["Da\u00df", "auch", "Dein", "Herz", ",", "der", "L\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des andern Volks zum Raube,", "tokens": ["Des", "an\u00b7dern", "Volks", "zum", "Rau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bereuend feig und hohl,", "tokens": ["Be\u00b7reu\u00b7end", "feig", "und", "hohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An Licht und Schmuck mag zagen!", "tokens": ["An", "Licht", "und", "Schmuck", "mag", "za\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wahr ist, was sie sagen:", "tokens": ["Nicht", "wahr", "ist", ",", "was", "sie", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Lieb' und Lust vergehen,", "tokens": ["Da\u00df", "Lieb'", "und", "Lust", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht wahr, da\u00df uns betr\u00fcge", "tokens": ["Nicht", "wahr", ",", "da\u00df", "uns", "be\u00b7tr\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der sch\u00f6ne, freud'ge Glaube,", "tokens": ["Der", "sch\u00f6\u00b7ne", ",", "freu\u00b7d'\u00b7ge", "Glau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und also lebe wohl!", "tokens": ["Und", "al\u00b7so", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}