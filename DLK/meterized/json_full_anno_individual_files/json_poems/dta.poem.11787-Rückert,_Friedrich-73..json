{"dta.poem.11787": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn eingetroffen ist ein unverhofftes Hoffen;", "tokens": ["Wenn", "ein\u00b7ge\u00b7trof\u00b7fen", "ist", "ein", "un\u00b7ver\u00b7hoff\u00b7tes", "Hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh er begl\u00fcckt sich f\u00fchlt, f\u00fchlt sich der Geist betroffen;", "tokens": ["Eh", "er", "be\u00b7gl\u00fcckt", "sich", "f\u00fchlt", ",", "f\u00fchlt", "sich", "der", "Geist", "be\u00b7trof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$,", "VVFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Wie, wer vom Schlaf erwacht, sich f\u00fchlet erst bet\u00e4ubt,", "tokens": ["Wie", ",", "wer", "vom", "Schlaf", "er\u00b7wacht", ",", "sich", "f\u00fch\u00b7let", "erst", "be\u00b7t\u00e4ubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "APPRART", "NN", "VVPP", "$,", "PRF", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann der Aurikel gleich von frischem Duft best\u00e4ubt;", "tokens": ["Dann", "der", "Au\u00b7ri\u00b7kel", "gleich", "von", "fri\u00b7schem", "Duft", "be\u00b7st\u00e4ubt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und wie die Blume selbst, wann Regen kommt, erschrickt", "tokens": ["Und", "wie", "die", "Blu\u00b7me", "selbst", ",", "wann", "Re\u00b7gen", "kommt", ",", "er\u00b7schrickt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "$,", "PWAV", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor der Erquickung, eh sie still sich f\u00fchlt erquickt.", "tokens": ["Vor", "der", "Er\u00b7quic\u00b7kung", ",", "eh", "sie", "still", "sich", "f\u00fchlt", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "PRF", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}