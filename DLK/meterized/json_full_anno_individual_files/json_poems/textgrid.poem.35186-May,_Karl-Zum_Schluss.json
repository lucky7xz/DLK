{"textgrid.poem.35186": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zum Schluss", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade, ade, ihr wohlgemeinten Worte,", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "ihr", "wohl\u00b7ge\u00b7mein\u00b7ten", "Wor\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gesprochen f\u00fcr der Menschheit Heil und Gl\u00fcck.", "tokens": ["Ge\u00b7spro\u00b7chen", "f\u00fcr", "der", "Menschheit", "Heil", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es bleibt euch offen die vertraute Pforte,", "tokens": ["Es", "bleibt", "euch", "of\u00b7fen", "die", "ver\u00b7trau\u00b7te", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O kehret gern, kehrt als Gebet zur\u00fcck!", "tokens": ["O", "keh\u00b7ret", "gern", ",", "kehrt", "als", "Ge\u00b7bet", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "VVFIN", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihr t\u00f6net nicht von unbekanntem Orte;", "tokens": ["Ihr", "t\u00f6\u00b7net", "nicht", "von", "un\u00b7be\u00b7kann\u00b7tem", "Or\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr seid nicht leerer, wesenloser Schall.", "tokens": ["Ihr", "seid", "nicht", "lee\u00b7rer", ",", "we\u00b7sen\u00b7lo\u00b7ser", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im gro\u00dfen, frommverstandnen Weltaccorde", "tokens": ["Im", "gro\u00b7\u00dfen", ",", "fromm\u00b7ver\u00b7stand\u00b7nen", "Wel\u00b7tac\u00b7cor\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ist heilges Leben jedes Intervall.", "tokens": ["Ist", "heil\u00b7ges", "Le\u00b7ben", "je\u00b7des", "In\u00b7ter\u00b7vall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geht hin, geht hin! Es wird euch stets begleiten", "tokens": ["Geht", "hin", ",", "geht", "hin", "!", "Es", "wird", "euch", "stets", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Glaubensmuth, der laut zu sprechen wagt,", "tokens": ["Der", "Glau\u00b7bens\u00b7muth", ",", "der", "laut", "zu", "spre\u00b7chen", "wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Liebe, nichts als Liebe zu verbreiten,", "tokens": ["Um", "Lie\u00b7be", ",", "nichts", "als", "Lie\u00b7be", "zu", "ver\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "PIS", "KOKOM", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo man euch freundlich ein Willkommen sagt.", "tokens": ["Wo", "man", "euch", "freund\u00b7lich", "ein", "Will\u00b7kom\u00b7men", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wechseln in der Sterblichkeit die Zeiten,", "tokens": ["Es", "wech\u00b7seln", "in", "der", "Sterb\u00b7lich\u00b7keit", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Glaube aber bleibt unwandelbar", "tokens": ["Der", "Glau\u00b7be", "a\u00b7ber", "bleibt", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wird einst siegreich \u00fcber Alles schreiten,", "tokens": ["Und", "wird", "einst", "sieg\u00b7reich", "\u00fc\u00b7ber", "Al\u00b7les", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was ihn verh\u00f6hnte, weil es sterblich war.", "tokens": ["Was", "ihn", "ver\u00b7h\u00f6hn\u00b7te", ",", "weil", "es", "st