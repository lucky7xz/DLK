{"textgrid.poem.61396": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der reiche Mann von K\u00f6ln", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu K\u00f6ln ein reicher Kaufherr sa\u00df,", "tokens": ["Zu", "K\u00f6ln", "ein", "rei\u00b7cher", "Kauf\u00b7herr", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hatt' ein Herz von Eisen;", "tokens": ["Der", "hatt'", "ein", "Herz", "von", "Ei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er lebte dahin in Saus und Braus", "tokens": ["Er", "leb\u00b7te", "da\u00b7hin", "in", "Saus", "und", "Braus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dr\u00fcckte Witwen und Waisen.", "tokens": ["Und", "dr\u00fcck\u00b7te", "Wit\u00b7wen", "und", "Wai\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er z\u00e4hlte sein Silber und wog sein Gold", "tokens": ["Er", "z\u00e4hl\u00b7te", "sein", "Sil\u00b7ber", "und", "wog", "sein", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und lachte dazu im stillen;", "tokens": ["Und", "lach\u00b7te", "da\u00b7zu", "im", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPRART", "VVINF", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der Richter bog um Gunst und Geld", "tokens": ["Der", "Rich\u00b7ter", "bog", "um", "Gunst", "und", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Recht nach seinem Willen.", "tokens": ["Das", "Recht", "nach", "sei\u00b7nem", "Wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da war ein M\u00e4gdlein in der Stadt,", "tokens": ["Da", "war", "ein", "M\u00e4gd\u00b7lein", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Kind von jungen Jahren,", "tokens": ["Ein", "Kind", "von", "jun\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er trieb es fort von Haus und Hof", "tokens": ["Er", "trieb", "es", "fort", "von", "Haus", "und", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit grimmigem Gebaren.", "tokens": ["Mit", "grim\u00b7mi\u00b7gem", "Ge\u00b7ba\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als der Schnee im Winter fiel,", "tokens": ["Und", "als", "der", "Schnee", "im", "Win\u00b7ter", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ging der Rhein mit Eise,", "tokens": ["Und", "ging", "der", "Rhein", "mit", "Ei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihn jammerte nicht des Kindes Not,", "tokens": ["Ihn", "jam\u00b7mer\u00b7te", "nicht", "des", "Kin\u00b7des", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das hatte nicht Kleid noch Speise.", "tokens": ["Das", "hat\u00b7te", "nicht", "Kleid", "noch", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und als der Fr\u00fchling kam ins Land,", "tokens": ["Und", "als", "der", "Fr\u00fch\u00b7ling", "kam", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6glein sangen mit Schalle:", "tokens": ["Die", "V\u00f6\u00b7glein", "san\u00b7gen", "mit", "Schal\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie fanden das M\u00e4gdlein morgens tot", "tokens": ["Sie", "fan\u00b7den", "das", "M\u00e4gd\u00b7lein", "mor\u00b7gens", "tot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf einer Streu im Stalle.", "tokens": ["Auf", "ei\u00b7ner", "Streu", "im", "Stal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie trugen es fort und gruben es ein", "tokens": ["Sie", "tru\u00b7gen", "es", "fort", "und", "gru\u00b7ben", "es", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ART"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Friedhof auf der Wiese;", "tokens": ["Am", "Fried\u00b7hof", "auf", "der", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Seele ging in Sankt Michaels Scho\u00df", "tokens": ["Die", "See\u00b7le", "ging", "in", "Sankt", "Mic\u00b7haels", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "VVFIN", "NE", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinauf zum Paradiese.", "tokens": ["Hin\u00b7auf", "zum", "Pa\u00b7ra\u00b7die\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den Tag darnach der Kaufmann ritt", "tokens": ["Den", "Tag", "dar\u00b7nach", "der", "Kauf\u00b7mann", "ritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl lachend daher im Trabe,", "tokens": ["Wohl", "la\u00b7chend", "da\u00b7her", "im", "Tra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da standen drei Lilien wei\u00df wie Schnee", "tokens": ["Da", "stan\u00b7den", "drei", "Li\u00b7li\u00b7en", "wei\u00df", "wie", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gewachsen auf dem Grabe;", "tokens": ["Ge\u00b7wach\u00b7sen", "auf", "dem", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da standen drei Lilien wei\u00df wie Schnee,", "tokens": ["Da", "stan\u00b7den", "drei", "Li\u00b7li\u00b7en", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Winde die Blumen gingen;", "tokens": ["Im", "Win\u00b7de", "die", "Blu\u00b7men", "gin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein V\u00f6glein schwang vom H\u00fcgel sich auf,", "tokens": ["Ein", "V\u00f6\u00b7glein", "schwang", "vom", "H\u00fc\u00b7gel", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Im Flug hub's an zu singen:", "tokens": ["Im", "Flug", "hub's", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APZR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbherr Marx von K\u00f6ln, Herr Marx von K\u00f6ln,", "tokens": ["\u00bb", "herr", "Marx", "von", "K\u00f6ln", ",", "Herr", "Marx", "von", "K\u00f6ln", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "APPR", "NE", "$,", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bleich ist dein Gesichte!", "tokens": ["Wie", "bleich", "ist", "dein", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ein M\u00f6rder, Herr Marx von K\u00f6ln,", "tokens": ["Du", "bist", "ein", "M\u00f6r\u00b7der", ",", "Herr", "Marx", "von", "K\u00f6ln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "NE", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich lade dich zu Gerichte.\u00ab", "tokens": ["Ich", "la\u00b7de", "dich", "zu", "Ge\u00b7rich\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Dem Kaufherrn wohl das Lachen verging,", "tokens": ["Dem", "Kauf\u00b7herrn", "wohl", "das", "La\u00b7chen", "ver\u00b7ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sein Mut war all verloren;", "tokens": ["Sein", "Mut", "war", "all", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er wandte sein Ro\u00df und jagte nach Haus,", "tokens": ["Er", "wand\u00b7te", "sein", "Ro\u00df", "und", "jag\u00b7te", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Blute troffen die Sporen.", "tokens": ["Vom", "Blu\u00b7te", "trof\u00b7fen", "die", "Spo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Er mochte nicht nehmen Speise noch Trank", "tokens": ["Er", "moch\u00b7te", "nicht", "neh\u00b7men", "Spei\u00b7se", "noch", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVINF", "NN", "ADV", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor \u00e4ngstlichen Gedanken;", "tokens": ["Vor", "\u00e4ngst\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin er schaut' in Saal und Hof,", "tokens": ["Wo\u00b7hin", "er", "schaut'", "in", "Saal", "und", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drei Lilien sah er schwanken;", "tokens": ["Drei", "Li\u00b7li\u00b7en", "sah", "er", "schwan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Und als er nachts auf dem Kissen lag,", "tokens": ["Und", "als", "er", "nachts", "auf", "dem", "Kis\u00b7sen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Keinen Schlaf konnt' er erzwingen;", "tokens": ["Kei\u00b7nen", "Schlaf", "konnt'", "er", "er\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sobald ihm fielen die Augen zu,", "tokens": ["So\u00b7bald", "ihm", "fie\u00b7len", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "H\u00f6rt' er das V\u00f6glein singen.", "tokens": ["H\u00f6rt'", "er", "das", "V\u00f6\u00b7glein", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "\u00bbach helft mir, helft mir, lieber Arzt!", "tokens": ["\u00bb", "ach", "helft", "mir", ",", "helft", "mir", ",", "lie\u00b7ber", "Arzt", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will's Euch neunfach zahlen,", "tokens": ["Ich", "will's", "Euch", "neun\u00b7fach", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir brennt's im Herzen wie h\u00f6llisch Feur;", "tokens": ["Mir", "brennt's", "im", "Her\u00b7zen", "wie", "h\u00f6l\u00b7lisch", "Feur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Helft mir von diesen Qualen!\u00ab", "tokens": ["Helft", "mir", "von", "die\u00b7sen", "Qua\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wohl ging der Arzt, mit Sorg' und Flei\u00df", "tokens": ["Wohl", "ging", "der", "Arzt", ",", "mit", "Sor\u00b7g'", "und", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Manch bittern Trank zu mischen;", "tokens": ["Manch", "bit\u00b7tern", "Trank", "zu", "mi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es tat nicht gut, es tat nicht schlimm,", "tokens": ["Es", "tat", "nicht", "gut", ",", "es", "tat", "nicht", "schlimm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das V\u00f6glein sang dazwischen:", "tokens": ["Das", "V\u00f6\u00b7glein", "sang", "da\u00b7zwi\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bbherr Marx von K\u00f6ln, an deiner S\u00fcnd'", "tokens": ["\u00bb", "herr", "Marx", "von", "K\u00f6ln", ",", "an", "dei\u00b7ner", "S\u00fcnd'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "NE", "APPR", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird alle Kunst zunichte!", "tokens": ["Wird", "al\u00b7le", "Kunst", "zu\u00b7nich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ein M\u00f6rder, Herr Marx von K\u00f6ln!", "tokens": ["Du", "bist", "ein", "M\u00f6r\u00b7der", ",", "Herr", "Marx", "von", "K\u00f6ln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "NE", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich lade dich zu Gerichte.\u00ab", "tokens": ["Ich", "la\u00b7de", "dich", "zu", "Ge\u00b7rich\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$.", "$("], "m