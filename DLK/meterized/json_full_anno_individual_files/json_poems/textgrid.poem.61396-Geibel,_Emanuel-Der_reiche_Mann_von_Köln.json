], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das V\u00f6glein sang dazwischen:", "tokens": ["Das", "V\u00f6\u00b7glein", "sang", "da\u00b7zwi\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.33": {"line.1": {"text": "\u00bbherr Marx von K\u00f6ln, an deiner S\u00fcnd'", "tokens": ["\u00bb", "herr", "Marx", "von", "K\u00f6ln", ",", "an", "dei\u00b7ner", "S\u00fcnd'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "NE", "APPR", "NE", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird alle Kunst zunichte!", "tokens": ["Wird", "al\u00b7le", "Kunst", "zu\u00b7nich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist ein M\u00f6rder, Herr Marx von K\u00f6ln!", "tokens": ["Du", "bist", "ein", "M\u00f6r\u00b7der", ",", "Herr", "Marx", "von", "K\u00f6ln", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NN", "NE", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich lade dich zu Gerichte.\u00ab", "tokens": ["Ich", "la\u00b7de", "dich", "zu", "Ge\u00b7rich\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.34": {"line.1": {"text": "Und um die dritte Mitternacht", "tokens": ["Und", "um", "die", "drit\u00b7te", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ging an der T\u00fcr ein Klopfen;", "tokens": ["Ging", "an", "der", "T\u00fcr", "ein", "Klop\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Kranken trieb's vom Lager auf,", "tokens": ["Den", "Kran\u00b7ken", "trie\u00b7b's", "vom", "La\u00b7ger", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihm flo\u00df die Stirn von Tropfen.", "tokens": ["Ihm", "flo\u00df", "die", "Stirn", "von", "Trop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.35": {"line.1": {"text": "Und als seine Hand den Riegel schob,", "tokens": ["Und", "als", "sei\u00b7ne", "Hand", "den", "Rie\u00b7gel", "schob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sie flog vor Angst und Schmerze;", "tokens": ["Sie", "flog", "vor", "Angst", "und", "Schmer\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als die T\u00fcr in den Angeln ging,", "tokens": ["Und", "als", "die", "T\u00fcr", "in", "den", "An\u00b7geln", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Zug blies aus die Kerze.", "tokens": ["Ein", "Zug", "blies", "aus", "die", "Ker\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.36": {"line.1": {"text": "Der drau\u00dfen stand, das war der Tod;", "tokens": ["Der", "drau\u00b7\u00dfen", "stand", ",", "das", "war", "der", "Tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er nahm Herrn Marx von K\u00f6llen,", "tokens": ["Er", "nahm", "Herrn", "Marx", "von", "K\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er setzt' ihn auf sein aschfarb Ro\u00df", "tokens": ["Er", "setzt'", "ihn", "auf", "sein", "aschfarb", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und fuhr mit ihm zur H\u00f6llen.", "tokens": ["Und", "fuhr", "mit", "ihm", "zur", "H\u00f6l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}