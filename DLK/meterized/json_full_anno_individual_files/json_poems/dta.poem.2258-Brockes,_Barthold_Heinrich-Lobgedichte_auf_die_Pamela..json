{"dta.poem.2258": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lobgedichte  \n auf die Pamela.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch Wolf gleich sagen m\u00f6chte:", "tokens": ["Wenn", "auch", "Wolf", "gleich", "sa\u00b7gen", "m\u00f6ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wisse menschliches Geschlechte,", "tokens": ["Wis\u00b7se", "menschli\u00b7ches", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da\u00df nur ich dein Lehrer bin;", "tokens": ["Da\u00df", "nur", "ich", "dein", "Leh\u00b7rer", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spricht das menschliche Geschlechte", "tokens": ["Spricht", "das", "menschli\u00b7che", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Doch zur Pamela mit Rechte:", "tokens": ["Doch", "zur", "Pa\u00b7me\u00b7la", "mit", "Rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NE", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Du bist unsre Lehrerinn!", "tokens": ["Du", "bist", "uns\u00b7re", "Leh\u00b7re\u00b7rinn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}}}}