{"dta.poem.16175": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "229.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Andacht stehn wir fest, o Erd', auf dir, und preisen", "tokens": ["In", "An\u00b7dacht", "stehn", "wir", "fest", ",", "o", "Erd'", ",", "auf", "dir", ",", "und", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PPER", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Elemente, die in dir und um dich kreisen;", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", ",", "die", "in", "dir", "und", "um", "dich", "krei\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "KON", "APPR", "PRF", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Die Flut, die dich umschlie\u00dft, die Glut, die dich durchflie\u00dft,", "tokens": ["Die", "Flut", ",", "die", "dich", "um\u00b7schlie\u00dft", ",", "die", "Glut", ",", "die", "dich", "durch\u00b7flie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft, die um dich weit sich wie ein Mantel gie\u00dft.", "tokens": ["Die", "Luft", ",", "die", "um", "dich", "weit", "sich", "wie", "ein", "Man\u00b7tel", "gie\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADJD", "PRF", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So \u00fcberschw\u00e4nglich sind die drei und wunderbar,", "tokens": ["So", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "sind", "die", "drei", "und", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "CARD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sich jedwedes stellt als ein Weltanfang dar;", "tokens": ["Da\u00df", "sich", "jed\u00b7we\u00b7des", "stellt", "als", "ein", "Welt\u00b7an\u00b7fang", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Soda\u00df die Weisen, die zuerst Weltursprung dachten,", "tokens": ["So\u00b7da\u00df", "die", "Wei\u00b7sen", ",", "die", "zu\u00b7erst", "Wel\u00b7tur\u00b7sprung", "dach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Ersten diese dis und jene jenes machten.", "tokens": ["Zum", "Ers\u00b7ten", "die\u00b7se", "dis", "und", "je\u00b7ne", "je\u00b7nes", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "PDS", "KON", "PDS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus Wasser lie\u00dfen die hervor die Sch\u00f6pfung tauchen,", "tokens": ["Aus", "Was\u00b7ser", "lie\u00b7\u00dfen", "die", "her\u00b7vor", "die", "Sch\u00f6p\u00b7fung", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die aus Feuerglanz, und die aus Aetherhauchen.", "tokens": ["Und", "die", "aus", "Feu\u00b7er\u00b7glanz", ",", "und", "die", "aus", "A\u00b7e\u00b7ther\u00b7hau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "$,", "KON", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "In Eintracht fassen wir die streitenden zusammen,", "tokens": ["In", "Ein\u00b7tracht", "fas\u00b7sen", "wir", "die", "strei\u00b7ten\u00b7den", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sehn die Welt erbl\u00fchn aus L\u00fcften, Fluten, Flammen.", "tokens": ["Und", "sehn", "die", "Welt", "er\u00b7bl\u00fchn", "aus", "L\u00fcf\u00b7ten", ",", "Flu\u00b7ten", ",", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wer k\u00f6nnt' am Weltgeweb recht sondern alle F\u00e4den,", "tokens": ["Wer", "k\u00f6nnt'", "am", "Welt\u00b7ge\u00b7web", "recht", "son\u00b7dern", "al\u00b7le", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPRART", "NN", "ADJD", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dreifach zusammen wol geschlungen f\u00e4nd' er jeden.", "tokens": ["Drei\u00b7fach", "zu\u00b7sam\u00b7men", "wol", "ge\u00b7schlun\u00b7gen", "f\u00e4nd'", "er", "je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VVPP", "VVFIN", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch wir zerpfl\u00fccken nicht den Teppich der Natur,", "tokens": ["Doch", "wir", "zer\u00b7pfl\u00fc\u00b7cken", "nicht", "den", "Tep\u00b7pich", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und freun uns der aus Drei gewebten Buntheit nur.", "tokens": ["Und", "freun", "uns", "der", "aus", "Drei", "ge\u00b7web\u00b7ten", "Bunt\u00b7heit", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "CARD", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}