{"textgrid.poem.38582": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Vollendung", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich einst das Ziel errungen habe,", "tokens": ["Wenn", "ich", "einst", "das", "Ziel", "er\u00b7run\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den Lichtgefilden jener Welt,", "tokens": ["In", "den", "Licht\u00b7ge\u00b7fil\u00b7den", "je\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heil, der Thr\u00e4ne dann an meinem Grabe", "tokens": ["Heil", ",", "der", "Thr\u00e4\u00b7ne", "dann", "an", "mei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die auf hingestreute Rosen f\u00e4llt!", "tokens": ["Die", "auf", "hin\u00b7ge\u00b7streu\u00b7te", "Ro\u00b7sen", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Sehnsuchtsvoll, mit hoher Ahndungswonne,", "tokens": ["Sehn\u00b7suchts\u00b7voll", ",", "mit", "ho\u00b7her", "Ahn\u00b7dungs\u00b7won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ruhig, wie der mondbegl\u00e4nzte Hain,", "tokens": ["Ru\u00b7hig", ",", "wie", "der", "mond\u00b7be\u00b7gl\u00e4nz\u00b7te", "Hain", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "L\u00e4chelnd, wie beim Niedergang die Sonne,", "tokens": ["L\u00e4\u00b7chelnd", ",", "wie", "beim", "Nie\u00b7der\u00b7gang", "die", "Son\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Harr' ich, g\u00f6ttliche Vollendung, dein!", "tokens": ["Harr'", "ich", ",", "g\u00f6tt\u00b7li\u00b7che", "Vol\u00b7len\u00b7dung", ",", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPOSAT", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Eil', o eile mich empor zu fl\u00fcgeln", "tokens": ["Eil'", ",", "o", "ei\u00b7le", "mich", "em\u00b7por", "zu", "fl\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo sich unter mir die Welten drehn,", "tokens": ["Wo", "sich", "un\u00b7ter", "mir", "die", "Wel\u00b7ten", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo im Lebensquell sich Palmen spiegeln,", "tokens": ["Wo", "im", "Le\u00b7bens\u00b7quell", "sich", "Pal\u00b7men", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo die Liebenden sich wieder sehn.", "tokens": ["Wo", "die", "Lie\u00b7ben\u00b7den", "sich", "wie\u00b7der", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sklavenketten sind der Erde Leiden,", "tokens": ["Skla\u00b7ven\u00b7ket\u00b7ten", "sind", "der", "Er\u00b7de", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft, ach! \u00f6fters bricht sie nur der Tod!", "tokens": ["Oft", ",", "ach", "!", "\u00f6f\u00b7ters", "bricht", "sie", "nur", "der", "Tod", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blumenkr\u00e4nzen gleichen ihre Freuden,", "tokens": ["Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", "glei\u00b7chen", "ih\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die ein Westhauch zu entbl\u00e4ttern droht!", "tokens": ["Die", "ein", "West\u00b7hauch", "zu", "ent\u00b7bl\u00e4t\u00b7tern", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn ich einst das Ziel errungen