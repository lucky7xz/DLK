{"dta.poem.12001": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tage nach dem Tag, wo du gepflanzt den Baum,", "tokens": ["Die", "Ta\u00b7ge", "nach", "dem", "Tag", ",", "wo", "du", "ge\u00b7pflanzt", "den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem du bl\u00fchen siehst der Zukunft goldnen Traum,", "tokens": ["An", "dem", "du", "bl\u00fc\u00b7hen", "siehst", "der", "Zu\u00b7kunft", "gold\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Tage w\u00fcnschest du, da\u00df sie gefl\u00fcgelt seien,", "tokens": ["Die", "Ta\u00b7ge", "w\u00fcn\u00b7schest", "du", ",", "da\u00df", "sie", "ge\u00b7fl\u00fc\u00b7gelt", "sei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um nur mit einemmal zu sehn des Baums Gedeihen.", "tokens": ["Um", "nur", "mit", "ei\u00b7nem\u00b7mal", "zu", "sehn", "des", "Baums", "Ge\u00b7dei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ADV", "PTKZU", "VVINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch geben kann dein Wunsch den Tagen keine Fl\u00fcgel;", "tokens": ["Doch", "ge\u00b7ben", "kann", "dein", "Wunsch", "den", "Ta\u00b7gen", "kei\u00b7ne", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPOSAT", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die starke Hand der Zeit f\u00fchrt sie am festen Z\u00fcgel.", "tokens": ["Die", "star\u00b7ke", "Hand", "der", "Zeit", "f\u00fchrt", "sie", "am", "fes\u00b7ten", "Z\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und desto langsamer siehst du dahin sie schreiten,", "tokens": ["Und", "des\u00b7to", "lang\u00b7sa\u00b7mer", "siehst", "du", "da\u00b7hin", "sie", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Je ungeduldiger du w\u00fcnschest ihr Entgleiten.", "tokens": ["Je", "un\u00b7ge\u00b7dul\u00b7di\u00b7ger", "du", "w\u00fcn\u00b7schest", "ihr", "Ent\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O w\u00fcnsche nichts vorbei, und w\u00fcnsche nichts zur\u00fcck!", "tokens": ["O", "w\u00fcn\u00b7sche", "nichts", "vor\u00b7bei", ",", "und", "w\u00fcn\u00b7sche", "nichts", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "PTKVZ", "$,", "KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur ruhiges Gef\u00fchl der Gegenwart ist Gl\u00fcck.", "tokens": ["Nur", "ru\u00b7hi\u00b7ges", "Ge\u00b7f\u00fchl", "der", "Ge\u00b7gen\u00b7wart", "ist", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "VAFIN", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Zukunft kommt von selbst, beeile nicht die Fahrt!", "tokens": ["Die", "Zu\u00b7kunft", "kommt", "von", "selbst", ",", "be\u00b7ei\u00b7le", "nicht", "die", "Fahrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sogleich Vergangenheit ist jede Gegenwart.", "tokens": ["Sog\u00b7leich", "Ver\u00b7gan\u00b7gen\u00b7heit", "ist", "je\u00b7de", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du aber pflanz' ein Kraut an jedem Tag im Garten,", "tokens": ["Du", "a\u00b7ber", "pflanz'", "ein", "Kraut", "an", "je\u00b7dem", "Tag", "im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kannst du jeden Tag auch eine Bl\u00fct' erwarten.", "tokens": ["So", "kannst", "du", "je\u00b7den", "Tag", "auch", "ei\u00b7ne", "Bl\u00fct'", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}