{"dta.poem.1267": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoffnung/ welche sich kan nimmer ruhig wissen/", "tokens": ["Die", "Hoff\u00b7nung", "/", "wel\u00b7che", "sich", "kan", "nim\u00b7mer", "ru\u00b7hig", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "VMFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ists/ die unser Hertz in tausend St\u00fccke theilt.", "tokens": ["Die", "ists", "/", "die", "un\u00b7ser", "Hertz", "in", "tau\u00b7send", "St\u00fc\u00b7cke", "theilt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wunden/ welche sonst Gedult und Zeit verheilt/", "tokens": ["Die", "Wun\u00b7den", "/", "wel\u00b7che", "sonst", "Ge\u00b7dult", "und", "Zeit", "ver\u00b7heilt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat eitles Hoffen mehr als erstlich auffgerissen.", "tokens": ["Hat", "eit\u00b7les", "Hof\u00b7fen", "mehr", "als", "erst\u00b7lich", "auff\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PIAT", "KOKOM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im fall nicht Tantalus im Wasser m\u00fcste stehen/", "tokens": ["Im", "fall", "nicht", "Tan\u00b7ta\u00b7lus", "im", "Was\u00b7ser", "m\u00fcs\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "NE", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im fall die Aepffel ihm nicht reichten an den Mund/", "tokens": ["Im", "fall", "die", "A\u00b7e\u00b7pf\u00b7fel", "ihm", "nicht", "reich\u00b7ten", "an", "den", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Da ihm doch Spei\u00df und Tranck zu brauchen nicht ver-", "tokens": ["Da", "ihm", "doch", "Spei\u00df", "und", "Tranck", "zu", "brau\u00b7chen", "nicht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "KON", "NN", "PTKZU", "VVFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00fcrde seiner Qual ein grosser Theil entgehen.", "tokens": ["So", "w\u00fcr\u00b7de", "sei\u00b7ner", "Qual", "ein", "gros\u00b7ser", "Theil", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ihr/ die ihr Ruhe sucht in schwerer Angst und Leyden/", "tokens": ["Ihr", "/", "die", "ihr", "Ru\u00b7he", "sucht", "in", "schwe\u00b7rer", "Angst", "und", "Ley\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sehr euch auch beschwert die aufferlegte Pein/", "tokens": ["Wie", "sehr", "euch", "auch", "be\u00b7schwert", "die", "auf\u00b7fer\u00b7leg\u00b7te", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im fall ihr mit der Zeit derselben lo\u00df wolt seyn/", "tokens": ["Im", "fall", "ihr", "mit", "der", "Zeit", "der\u00b7sel\u00b7ben", "lo\u00df", "wolt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "ART", "NN", "PDAT", "ADV", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So m\u00fcsset ihr die Last der eitlen Hoffnung meiden.", "tokens": ["So", "m\u00fcs\u00b7set", "ihr", "die", "Last", "der", "eit\u00b7len", "Hoff\u00b7nung", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Hoffnung/ fremdes Gutt und Ehre zu erlangen/", "tokens": ["Die", "Hoff\u00b7nung", "/", "frem\u00b7des", "Gutt", "und", "Eh\u00b7re", "zu", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schickt ein verwegnes Hertz auffs fichtne Wasser-", "tokens": ["Schickt", "ein", "ver\u00b7weg\u00b7nes", "Hertz", "auffs", "ficht\u00b7ne", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcllt die erz\u00fcrnte See mit todten Leichen au\u00df.", "tokens": ["F\u00fcllt", "die", "er\u00b7z\u00fcrn\u00b7te", "See", "mit", "tod\u00b7ten", "Lei\u00b7chen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Hoffnung macht das Garn mit reichem Raube pran-", "tokens": ["Die", "Hoff\u00b7nung", "macht", "das", "Garn", "mit", "rei\u00b7chem", "Rau\u00b7be", "pran"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Hoffnung pfleget sich Bellona zu bedienen/", "tokens": ["Der", "Hoff\u00b7nung", "pfle\u00b7get", "sich", "Bel\u00b7lo\u00b7na", "zu", "be\u00b7die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "NE", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn sie das blancke Feld mit Menschen-Blutte nezt:", "tokens": ["Wenn", "sie", "das", "blan\u00b7cke", "Feld", "mit", "Men\u00b7schen\u00b7Blut\u00b7te", "nezt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im fall die Hoffnung ihr ein langes Ziel gesezt/", "tokens": ["Im", "fall", "die", "Hoff\u00b7nung", "ihr", "ein", "lan\u00b7ges", "Ziel", "ge\u00b7sezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll unbeweget stehn der Bau der Himmel-B\u00fch-", "tokens": ["Soll", "un\u00b7be\u00b7we\u00b7get", "stehn", "der", "Bau", "der", "Him\u00b7mel\u00b7B\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Soll Wind und Wetter sich zu ihren Willen schicken/", "tokens": ["Soll", "Wind", "und", "Wet\u00b7ter", "sich", "zu", "ih\u00b7ren", "Wil\u00b7len", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum hoffe wahren Trost nur von der Hoffnung nicht:", "tokens": ["Drum", "hof\u00b7fe", "wah\u00b7ren", "Trost", "nur", "von", "der", "Hoff\u00b7nung", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr du diese n\u00e4hrst/ ie mehr die Last wird dr\u00fc-", "tokens": ["Je", "mehr", "du", "die\u00b7se", "n\u00e4hrst", "/", "ie", "mehr", "die", "Last", "wird", "dr\u00fc"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PDS", "VVFIN", "$(", "ADV", "ADV", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}