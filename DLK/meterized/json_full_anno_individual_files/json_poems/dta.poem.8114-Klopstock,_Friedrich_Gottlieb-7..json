{"dta.poem.8114": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Donnr\u2019 es, o Gesang, in der Nacht", "tokens": ["Donn\u00b7r'", "es", ",", "o", "Ge\u00b7sang", ",", "in", "der", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Schrecken hinab, zu Gehenna\u2019s Emp\u00f6rer hin:", "tokens": ["Schre\u00b7cken", "hin\u00b7ab", ",", "zu", "Ge\u00b7hen\u00b7na's", "Em\u00b7p\u00f6\u00b7rer", "hin", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die am Staub\u2019 einst Elend, und der Tod traf,", "tokens": ["Die", "am", "Staub'", "einst", "E\u00b7lend", ",", "und", "der", "Tod", "traf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie erwachen zu dem Schaun!", "tokens": ["Sie", "er\u00b7wa\u00b7chen", "zu", "dem", "Schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}