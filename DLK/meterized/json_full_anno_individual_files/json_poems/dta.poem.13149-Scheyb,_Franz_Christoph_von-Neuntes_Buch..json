{"dta.poem.13149": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Krei\u00df befremdte sich: mir schien es wunderbar,", "tokens": ["Der", "Krei\u00df", "be\u00b7fremd\u00b7te", "sich", ":", "mir", "schien", "es", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df meine F\u00fchrerinn zum Rath beschieden war.", "tokens": ["Da\u00df", "mei\u00b7ne", "F\u00fch\u00b7re\u00b7rinn", "zum", "Rath", "be\u00b7schie\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "235Ich bildete mir ein, da\u00df etwan sie vernommen,", "tokens": ["bil\u00b7de\u00b7te", "mir", "ein", ",", "da\u00df", "et\u00b7wan", "sie", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Es werde zum Beschlu\u00df auf sie die Frage kommen.", "tokens": ["Es", "wer\u00b7de", "zum", "Be\u00b7schlu\u00df", "auf", "sie", "die", "Fra\u00b7ge", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch ich sahe nicht, da\u00df man ihr was gesagt,", "tokens": ["Je\u00b7doch", "ich", "sa\u00b7he", "nicht", ",", "da\u00df", "man", "ihr", "was", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie, oder da\u00df man sie vorher um was gefragt.", "tokens": ["Wie", ",", "o\u00b7der", "da\u00df", "man", "sie", "vor\u00b7her", "um", "was", "ge\u00b7fragt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "KOUS", "PIS", "PPER", "ADV", "APPR", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich fande keine Zeit die Sache zu erkunden,", "tokens": ["Ich", "fan\u00b7de", "kei\u00b7ne", "Zeit", "die", "Sa\u00b7che", "zu", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "240Weil alle schon bereit, sie zu vernehmen, stunden.", "tokens": ["al\u00b7le", "schon", "be\u00b7reit", ",", "sie", "zu", "ver\u00b7neh\u00b7men", ",", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,", "ADJA", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}